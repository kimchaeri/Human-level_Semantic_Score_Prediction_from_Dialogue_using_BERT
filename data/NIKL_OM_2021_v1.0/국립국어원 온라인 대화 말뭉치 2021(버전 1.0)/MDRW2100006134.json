      "utterance": [
        {
          "id": "MDRW2100006134.1.1",
          "form": "헐 그렇군요",
          "original_form": "헐 그렇군요",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006134.1.2",
          "form": "전 아직 카카페는 도전을 못해봤어요",
          "original_form": "전 아직 카카페는 도전을 못해봤어요",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006134.1.3",
          "form": "그럼 어디 보시나요?",
          "original_form": "그럼 어디 보시나요?",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100006134.1.4",
          "form": "저는 시리즈로만 봐요",
          "original_form": "저는 시리즈로만 봐요",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100006134.1.5",
          "form": "네이버 시리즈!",
          "original_form": "네이버 시리즈!",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100006134.1.6",
          "form": "아하 시리즈",
          "original_form": "아하 시리즈",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006134.1.7",
          "form": "로판?",
          "original_form": "로판?",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006134.1.8",
          "form": "로맨스?",
          "original_form": "로맨스?",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006134.1.9",
          "form": "뭐좋아하세요?",
          "original_form": "뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006134.1.10",
          "form": "둘 다 좋은데",
          "original_form": "둘 다 좋은데",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006134.1.11",
          "form": "로판을 아무래도 더 많이 읽는거 같아요!",
          "original_form": "로판을 아무래도 더 많이 읽는거 같아요!",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006134.1.12",
          "form": "어떤 거 좋아하세요?",
          "original_form": "어떤 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006134.1.13",
          "form": "그럼 카카페죠",
          "original_form": "그럼 카카페죠",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006134.1.14",
          "form": "뭐하세요",
          "original_form": "뭐하세요",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006134.1.15",
          "form": "당장까세요",
          "original_form": "당장까세요",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006134.1.16",
          "form": "시리즈는 로맨스라구요",
          "original_form": "시리즈는 로맨스라구요",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100006134.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006134.1.18",
          "form": "아하 이런..!",
          "original_form": "아하 이런..!",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006134.1.19",
          "form": "시리즈는 로맨스군요?",
          "original_form": "시리즈는 로맨스군요?",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006134.1.20",
          "form": "전 시리즈는 로맨스보고",
          "original_form": "전 시리즈는 로맨스보고",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006134.1.21",
          "form": "카카페 로판봐요",
          "original_form": "카카페 로판봐요",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006134.1.22",
          "form": "아니 name1 웹소설잘알이시군요!",
          "original_form": "아니 &name1& 웹소설잘알이시군요!",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006134.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006134.1.24",
          "form": "진짜 로판운 카카페!!",
          "original_form": "진짜 로판운 카카페!!",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006134.1.25",
          "form": "시리즈에서 뭐 보셨나요?",
          "original_form": "시리즈에서 뭐 보셨나요?",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100006134.1.26",
          "form": "저는 그 내남편을 뺏어줘?",
          "original_form": "저는 그 내남편을 뺏어줘?",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100006134.1.27",
          "form": "인가?",
          "original_form": "인가?",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100006134.1.28",
          "form": "아 알거같아욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 알거같아욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100006134.1.29",
          "form": "으잉?",
          "original_form": "으잉?",
          "speaker_id": "1",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100006134.1.30",
          "form": "검색안나와요",
          "original_form": "검색안나와요",
          "speaker_id": "1",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100006134.1.31",
          "form": "앗 잠시만요",
          "original_form": "앗 잠시만요",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100006134.1.32",
          "form": "내남편과결혼해줘",
          "original_form": "내남편과결혼해줘",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100006134.1.33",
          "form": "였어요!!",
          "original_form": "였어요!!",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100006134.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100006134.1.35",
          "form": "맞아요 그거 유명해요 ㅋㅋㅋ",
          "original_form": "맞아요 그거 유명해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100006134.1.36",
          "form": "근데 로맨스인데요?",
          "original_form": "근데 로맨스인데요?",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100006134.1.37",
          "form": "아하! 재밌더라구요!",
          "original_form": "아하! 재밌더라구요!",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100006134.1.38",
          "form": "로판은요??",
          "original_form": "로판은요??",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100006134.1.39",
          "form": "로판은 그 어느날 공주가 되어버렸다?",
          "original_form": "로판은 그 어느날 공주가 되어버렸다?",
          "speaker_id": "2",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100006134.1.40",
          "form": "없어요...",
          "original_form": "없어요...",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006134.1.41",
          "form": "오노우",
          "original_form": "오노우",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006134.1.42",
          "form": "어느 날 공주가 되어버렸다",
          "original_form": "어느 날 공주가 되어버렸다",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006134.1.43",
          "form": "이거요!!",
          "original_form": "이거요!!",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006134.1.44",
          "form": "아 이건 첨봐요 ㅎㅎ",
          "original_form": "아 이건 첨봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006134.1.45",
          "form": "표지도 첨보네요",
          "original_form": "표지도 첨보네요",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006134.1.46",
          "form": "오 이거 넘 재밌엉ㅎ..!!",
          "original_form": "오 이거 넘 재밌엉ㅎ..!!",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006134.1.47",
