          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017878.1.8",
          "form": "3일은 아프단 말야ㅠ",
          "original_form": "3일은 아프단 말야ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017878.1.9",
          "form": "주말에도 일해?",
          "original_form": "주말에도 일해?",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017878.1.10",
          "form": "지금 괜찮은데",
          "original_form": "지금 괜찮은데",
          "speaker_id": "2",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017878.1.11",
          "form": "몸 좀 어때?",
          "original_form": "몸 좀 어때?",
