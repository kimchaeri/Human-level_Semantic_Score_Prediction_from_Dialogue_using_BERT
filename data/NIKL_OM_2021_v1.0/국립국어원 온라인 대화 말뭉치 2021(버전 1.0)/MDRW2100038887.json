        {
          "id": "MDRW2100038887.1.1",
          "form": "name2님 여행 좋아하샤요?",
          "original_form": "&name2&님 여행 좋아하샤요?",
          "speaker_id": "1",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100038887.1.2",
          "form": "저 여행 좋아해요~~~",
          "original_form": "저 여행 좋아해요~~~",
          "speaker_id": "2",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100038887.1.3",
          "form": "커로나 되고나서 뭔가 가고싶어서",
          "original_form": "커로나 되고나서 뭔가 가고싶어서",
          "speaker_id": "2",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100038887.1.4",
          "form": "좋아졌어요..ㅎㅎ",
          "original_form": "좋아졌어요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100038887.1.5",
          "form": "저도요 ㅎ.ㅎ ! 어디를 제일 가고 싶으셔요?",
          "original_form": "저도요 ㅎ.ㅎ ! 어디를 제일 가고 싶으셔요?",
          "speaker_id": "1",
          "time": "20210902 22:43"
        },
        {
          "id": "MDRW2100038887.1.6",
          "form": "저 여수 가고싶어요",
          "original_form": "저 여수 가고싶어요",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100038887.1.7",
          "form": "여스~~",
          "original_form": "여스~~",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100038887.1.8",
          "form": "아니면 제주도요!",
          "original_form": "아니면 제주도요!",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100038887.1.9",
          "form": "가본적있으새요?",
          "original_form": "가본적있으새요?",
          "speaker_id": "2",
          "time": "20210902 23:08"
        },
        {
          "id": "MDRW2100038887.1.10",
          "form": "여수는 안 가봤는데 제주도는 가봣어요!",
          "original_form": "여수는 안 가봤는데 제주도는 가봣어요!",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100038887.1.11",
          "form": "저 제주도 완전 좋아하거든요",
          "original_form": "저 제주도 완전 좋아하거든요",
          "speaker_id": "1",
          "time": "20210902 23:12"
        },
        {
          "id": "MDRW2100038887.1.12",
          "form": "저도 제주도 오ㅓㄴ전 좋어요",
          "original_form": "저도 제주도 오ㅓㄴ전 좋어요",
          "speaker_id": "2",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100038887.1.13",
          "form": "못간지 한 2년 돤거 같아요",
          "original_form": "못간지 한 2년 돤거 같아요",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100038887.1.14",
          "form": "헐헐 제주도에사 어딜 제일 좋아하셔요?",
          "original_form": "헐헐 제주도에사 어딜 제일 좋아하셔요?",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100038887.1.15",
          "form": "뭔가 엄청 여기가 딱 좋다!",
          "original_form": "뭔가 엄청 여기가 딱 좋다!",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100038887.1.16",
          "form": "하는데는 없는데",
          "original_form": "하는데는 없는데",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100038887.1.17",
          "form": "고등어회가 먹고싶네요오을은",
          "original_form": "고등어회가 먹고싶네요오을은",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100038887.1.18",
          "form": "오늘은",
          "original_form": "오늘은",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100038887.1.19",
          "form": "허얼!!!!!!! 고등어회 못 먹어보고 돌아왔난데",
          "original_form": "허얼!!!!!!! 고등어회 못 먹어보고 돌아왔난데",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100038887.1.20",
          "form": "맛있나요!??!?!",
          "original_form": "맛있나요!??!?!",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100038887.1.21",
          "form": "약간 기름진거 좋아하묜",
          "original_form": "약간 기름진거 좋아하묜",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100038887.1.22",
          "form": "좋아하실거같아요",
          "original_form": "좋아하실거같아요",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100038887.1.23",
          "form": "전 기름진걸 좋아하나봐요",
          "original_form": "전 기름진걸 좋아하나봐요",
          "speaker_id": "2",
          "time": "20210902 23:40"
        },
        {
          "id": "MDRW2100038887.1.24",
          "form": "헉 그렇구나!",
          "original_form": "헉 그렇구나!",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100038887.1.25",
          "form": "제주도 유명한 푸딩도 드샤보샸어요?",
          "original_form": "제주도 유명한 푸딩도 드샤보샸어요?",
          "speaker_id": "1",
          "time": "20210902 23:41"
        },
        {
          "id": "MDRW2100038887.1.26",
          "form": "앗 아뇨!! 유명한 푸딩이 있너교 ?",
          "original_form": "앗 아뇨!! 유명한 푸딩이 있너교 ?",
          "speaker_id": "2",
