      "utterance": [
        {
          "id": "MDRW2100045162.1.1",
          "form": "아니 너무 불쌍해",
          "original_form": "아니 너무 불쌍해",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045162.1.2",
          "form": "그걸 팬들이 좋아하는것도아님",
          "original_form": "그걸 팬들이 좋아하는것도아님",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045162.1.3",
          "form": "나라도",
          "original_form": "나라도",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045162.1.4",
          "form": "일본아이돌 못팔듯",
          "original_form": "일본아이돌 못팔듯",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045162.1.5",
          "form": "ㄹㅇ...",
          "original_form": "ㄹㅇ...",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045162.1.6",
          "form": "아무리",
          "original_form": "아무리",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045162.1.7",
          "form": "일본에서도 한국 유출보고",
          "original_form": "일본에서도 한국 유출보고",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045162.1.8",
          "form": "엔시티 덕질이 빡세도",
          "original_form": "엔시티 덕질이 빡세도",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045162.1.9",
          "form": "당연한거라고",
          "original_form": "당연한거라고",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045162.1.10",
          "form": "일본돌 덕질보단",
          "original_form": "일본돌 덕질보단",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045162.1.11",
          "form": "백배낫지",
          "original_form": "백배낫지",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045162.1.12",
          "form": "팬싸에서",
          "original_form": "팬싸에서",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045162.1.13",
          "form": "여자들이랑손잡고싶지",
          "original_form": "여자들이랑손잡고싶지",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045162.1.14",
          "form": "아저씨들이랑 손잡고싶냐고",
          "original_form": "아저씨들이랑 손잡고싶냐고",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045162.1.15",
          "form": "팬섭이라는것도 수위 장난아니던데",
          "original_form": "팬섭이라는것도 수위 장난아니던데",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045162.1.16",
          "form": "또이거하니 성수가 생각나네",
          "original_form": "또이거하니 성수가 생각나네",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045162.1.17",
          "form": "성수야 간담회좀 열어주라",
          "original_form": "성수야 간담회좀 열어주라",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045162.1.18",
          "form": "몇 년이 지나도",
          "original_form": "몇 년이 지나도",
          "speaker_id": "1",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045162.1.19",
          "form": "화가가라앉지가않나7ㅣ",
          "original_form": "화가가라앉지가않나7ㅣ",
          "speaker_id": "1",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045162.1.20",
          "form": "한성수 잘해라...",
          "original_form": "한성수 잘해라...",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045162.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045162.1.22",
          "form": "일본여돌들",
          "original_form": "일본여돌들",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045162.1.23",
          "form": "악수회 하다가",
          "original_form": "악수회 하다가",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045162.1.24",
          "form": "진짜 테러당한 적도 있고..",
          "original_form": "진짜 테러당한 적도 있고..",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045162.1.25",
          "form": "탈주하고싶어하는 ㅇ들 많다던데",
          "original_form": "탈주하고싶어하는 ㅇ들 많다던데",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045162.1.26",
          "form": "걔들 서로가 그런대잖아",
          "original_form": "걔들 서로가 그런대잖아",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045162.1.27",
          "form": "한국가서 데뷔하면 대접이다른데",
          "original_form": "한국가서 데뷔하면 대접이다른데",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045162.1.28",
          "form": "톱이었나 무슨 흉기 가져갔었는데",
          "original_form": "톱이었나 무슨 흉기 가져갔었는데",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045162.1.29",
          "form": "그래서 에케비안에서도 말많던데",
          "original_form": "그래서 에케비안에서도 말많던데",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045162.1.30",
          "form": "시상에",
          "original_form": "시상에",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045162.1.31",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045162.1.32",
          "form": "물론 한국에서도",
          "original_form": "물론 한국에서도",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045162.1.33",
          "form": "그래서 다침???",
          "original_form": "그래서 다침???",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045162.1.34",
          "form": "어느정도 떠야.. 하긴 하겠지만",
          "original_form": "어느정도 떠야.. 하긴 하겠지만",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045162.1.35",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045162.1.36",
          "form": "심하게 다치고",
          "original_form": "심하게 다치고",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045162.1.37",
          "form": "트라우마로",
          "original_form": "트라우마로",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045162.1.38",
          "form": "졸업이라는 이름의 은퇴..",
          "original_form": "졸업이라는 이름의 은퇴..",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045162.1.39",
          "form": "안뜨면 우리나라도 심각하지만..ㅂ",
          "original_form": "안뜨면 우리나라도 심각하지만..ㅂ",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045162.1.40",
          "form": "아니세상에",
          "original_form": "아니세상에",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045162.1.41",
          "form": "갑자기 또 생각나는",
          "original_form": "갑자기 또 생각나는",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045162.1.42",
          "form": "그 가해자 피해자 난리났던 습격사건생각나네",
          "original_form": "그 가해자 피해자 난리났던 습격사건생각나네",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045162.1.43",
          "form": "에케비",
          "original_form": "에케비",
          "speaker_id": "1",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045162.1.44",
          "form": "굳이 여돌 데뷔하느니 차라리 유튜버를 할듯",
          "original_form": "굳이 여돌 데뷔하느니 차라리 유튜버를 할듯",
