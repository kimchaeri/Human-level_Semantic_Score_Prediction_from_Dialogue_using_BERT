      "utterance": [
        {
          "id": "MDRW2100030859.1.1",
          "form": "장수 했군요",
          "original_form": "장수 했군요",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030859.1.2",
          "form": "네 무척 건강 했어요",
          "original_form": "네 무척 건강 했어요",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030859.1.3",
          "form": "요즘 고양이들은 캣타워도 오르락내리락하고",
          "original_form": "요즘 고양이들은 캣타워도 오르락내리락하고",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030859.1.4",
          "form": "장병도 없었지요",
          "original_form": "장병도 없었지요",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030859.1.5",
          "form": "집도 몇 개 있어서 이집 저집 돌아다니며",
          "original_form": "집도 몇 개 있어서 이집 저집 돌아다니며",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030859.1.6",
          "form": "요즘에는 돈이 너무 많이 들어요",
          "original_form": "요즘에는 돈이 너무 많이 들어요",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030859.1.7",
          "form": "펜션 5가 듯이 잘 다니더라구요",
          "original_form": "펜션 5가 듯이 잘 다니더라구요",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030859.1.8",
          "form": "그때는 밥을 먹여 없거든요",
          "original_form": "그때는 밥을 먹여 없거든요",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030859.1.9",
          "form": "간식 같은 건 없었죠",
          "original_form": "간식 같은 건 없었죠",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030859.1.10",
          "form": "맞아요 사료 보다는 밥 이었죠 잡식성",
          "original_form": "맞아요 사료 보다는 밥 이었죠 잡식성",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030859.1.11",
          "form": "강아지 샴푸도 없었어요",
          "original_form": "강아지 샴푸도 없었어요",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030859.1.12",
          "form": "요즘 고양이들은 추도 먹고",
          "original_form": "요즘 고양이들은 추도 먹고",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030859.1.13",
          "form": "산책을 안 시켜 준게 미안하네요",
          "original_form": "산책을 안 시켜 준게 미안하네요",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030859.1.14",
          "form": "건 사료 말고 생 자료도 먹었죠",
          "original_form": "건 사료 말고 생 자료도 먹었죠",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030859.1.15",
          "form": "요새는 스케일링도 한대요",
          "original_form": "요새는 스케일링도 한대요",
          "speaker_id": "1",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100030859.1.16",
          "form": "저희 집에는 혼자서 나가서 놀다 왔어요",
          "original_form": "저희 집에는 혼자서 나가서 놀다 왔어요",
          "speaker_id": "2",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100030859.1.17",
          "form": "병원비도 많이 나가죠",
          "original_form": "병원비도 많이 나가죠",
          "speaker_id": "1",
          "time": "20210904 13:36"
        }
      ]
