      "utterance": [
        {
          "id": "MDRW2100029637.1.1",
          "form": "점심! 뭐 드실건가요",
          "original_form": "점심! 뭐 드실건가요",
          "speaker_id": "2",
          "time": "20210903 11:10"
        },
        {
          "id": "MDRW2100029637.1.2",
          "form": "헉.. 뭐먹죠",
          "original_form": "헉.. 뭐먹죠",
          "speaker_id": "1",
          "time": "20210903 11:10"
        },
        {
          "id": "MDRW2100029637.1.3",
          "form": "매일 점심이 고민이에요.. 음 김치찌개와 순대국과 중국집정도가 있습니다",
          "original_form": "매일 점심이 고민이에요.. 음 김치찌개와 순대국과 중국집정도가 있습니다",
          "speaker_id": "1",
          "time": "20210903 11:10"
        },
        {
          "id": "MDRW2100029637.1.4",
          "form": "짜장면 어떠세요",
          "original_form": "짜장면 어떠세요",
          "speaker_id": "2",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100029637.1.5",
          "form": "제가 원래 먹으려고 했다가",
          "original_form": "제가 원래 먹으려고 했다가",
          "speaker_id": "2",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100029637.1.6",
          "form": "일이 있어서 못먹게 됐거든요...ㅠ",
          "original_form": "일이 있어서 못먹게 됐거든요...ㅠ",
          "speaker_id": "2",
          "time": "20210903 11:11"
        },
        {
          "id": "MDRW2100029637.1.7",
          "form": "좋아요!! 받은 추천대로 오늘은 중국집으로ㅋㅋ",
          "original_form": "좋아요!! 받은 추천대로 오늘은 중국집으로ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029637.1.8",
          "form": "어쩌다 계획이 틀어지셨대요ㅠ",
          "original_form": "어쩌다 계획이 틀어지셨대요ㅠ",
          "speaker_id": "1",
          "time": "20210903 11:12"
        },
        {
          "id": "MDRW2100029637.1.9",
          "form": "할머니가 김치 받아가라고 부르셨어요...",
          "original_form": "할머니가 김치 받아가라고 부르셨어요...",
          "speaker_id": "2",
          "time": "20210903 11:16"
        },
        {
          "id": "MDRW2100029637.1.10",
          "form": "외출이 좀 길어질 예정입니다",
          "original_form": "외출이 좀 길어질 예정입니다",
          "speaker_id": "2",
          "time": "20210903 11:17"
        },
        {
          "id": "MDRW2100029637.1.11",
          "form": "앗 할무니손맛 김치는 어쩔수 없죠",
          "original_form": "앗 할무니손맛 김치는 어쩔수 없죠",
          "speaker_id": "1",
          "time": "20210903 11:18"
        },
        {
          "id": "MDRW2100029637.1.12",
          "form": "그건 맞죠ㅎㅎ",
          "original_form": "그건 맞죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 11:52"
        },
        {
          "id": "MDRW2100029637.1.13",
          "form": "점심먹으러 왔어요!",
          "original_form": "점심먹으러 왔어요!",
          "speaker_id": "1",
          "time": "20210903 11:58"
        },
        {
          "id": "MDRW2100029637.1.14",
          "form": "간짜장을 먹을겁니다",
          "original_form": "간짜장을 먹을겁니다",
          "speaker_id": "1",
          "time": "20210903 11:58"
        },
        {
          "id": "MDRW2100029637.1.15",
          "form": "ㅎㅎ 좋아요",
          "original_form": "ㅎㅎ 좋아요",
          "speaker_id": "2",
          "time": "20210903 12:05"
        },
        {
          "id": "MDRW2100029637.1.16",
          "form": "전 지금 막 도착했어요",
          "original_form": "전 지금 막 도착했어요",
          "speaker_id": "2",
          "time": "20210903 12:06"
        },
        {
          "id": "MDRW2100029637.1.17",
          "form": "ㅋㅋㅋㅋ맛점심 하세요! 저는 후식 아아메를 숑숑",
          "original_form": "ㅋㅋㅋㅋ맛점심 하세요! 저는 후식 아아메를 숑숑",
          "speaker_id": "1",
          "time": "20210903 12:25"
        },
        {
          "id": "MDRW2100029637.1.18",
          "form": "가아니라 김치시랬지",
          "original_form": "가아니라 김치시랬지",
          "speaker_id": "1",
          "time": "20210903 12:26"
        },
        {
          "id": "MDRW2100029637.1.19",
          "form": "할무니댁 점심 얻어드시려나",
          "original_form": "할무니댁 점심 얻어드시려나",
          "speaker_id": "1",
          "time": "20210903 12:26"
        },
        {
          "id": "MDRW2100029637.1.20",
          "form": "얻어먹을줄 알았는데 짜장면 시켜 먹었어요ㅋㅋ",
          "original_form": "얻어먹을줄 알았는데 짜장면 시켜 먹었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100029637.1.21",
          "form": "오! 역시 먹고싶은거 먹는게 짱이에요ㅋㅋ",
          "original_form": "오! 역시 먹고싶은거 먹는게 짱이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 14:18"
        },
