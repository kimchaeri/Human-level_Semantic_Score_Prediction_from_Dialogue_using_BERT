      "utterance": [
        {
          "id": "MDRW2100039738.1.1",
          "form": "꼭 도전해볼게요!!",
          "original_form": "꼭 도전해볼게요!!",
          "speaker_id": "1",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100039738.1.2",
          "form": "좋아요~~!",
          "original_form": "좋아요~~!",
          "speaker_id": "2",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100039738.1.3",
          "form": "앗 2연속 식음료ㅋㅋㅋㅋㅋ",
          "original_form": "앗 2연속 식음료ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:06"
        },
        {
          "id": "MDRW2100039738.1.4",
          "form": "배달 자주 시키시나요?-?",
          "original_form": "배달 자주 시키시나요?-?",
          "speaker_id": "2",
          "time": "20210903 21:07"
        },
        {
          "id": "MDRW2100039738.1.5",
          "form": "앗ㅋㅋㅋㅋ다른 주제두 좋아요!!! name2님이 골라주세용",
          "original_form": "앗ㅋㅋㅋㅋ다른 주제두 좋아요!!! &name2&님이 골라주세용",
          "speaker_id": "1",
          "time": "20210903 21:07"
        },
        {
          "id": "MDRW2100039738.1.6",
          "form": "앗 저는 자주는 안 시켜용...",
          "original_form": "앗 저는 자주는 안 시켜용...",
          "speaker_id": "1",
          "time": "20210903 21:07"
        },
        {
          "id": "MDRW2100039738.1.7",
          "form": "음식 얘기는 다양하니깐 좋아오!",
          "original_form": "음식 얘기는 다양하니깐 좋아오!",
          "speaker_id": "2",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039738.1.8",
          "form": "헉 글쿠나",
          "original_form": "헉 글쿠나",
          "speaker_id": "2",
          "time": "20210903 22:29"
        },
        {
          "id": "MDRW2100039738.1.9",
          "form": "음 저두 자주 안시키는데 한달에 3~5번 정도일까요",
          "original_form": "음 저두 자주 안시키는데 한달에 3~5번 정도일까요",
          "speaker_id": "2",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100039738.1.10",
          "form": "시킬때 주로 어떤종류로 시켜요??",
          "original_form": "시킬때 주로 어떤종류로 시켜요??",
          "speaker_id": "2",
          "time": "20210903 22:30"
        },
        {
          "id": "MDRW2100039738.1.11",
          "form": "저는 주로 치킨이나 떡볶이만 먹는 것 같아요ㅋㅋㅋ!!",
          "original_form": "저는 주로 치킨이나 떡볶이만 먹는 것 같아요ㅋㅋㅋ!!",
          "speaker_id": "1",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100039738.1.12",
          "form": "냐낭님은요?",
          "original_form": "냐낭님은요?",
          "speaker_id": "1",
          "time": "20210903 22:32"
        },
        {
          "id": "MDRW2100039738.1.13",
          "form": "헉 저두.,,,!",
          "original_form": "헉 저두.,,,!",
          "speaker_id": "2",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100039738.1.14",
          "form": "우왕 무슨 떡볶이 젤 좋아하세요?",
          "original_form": "우왕 무슨 떡볶이 젤 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 22:36"
        },
        {
          "id": "MDRW2100039738.1.15",
          "form": "떡볶이 젤 많이 시켜먹었을거에요ㅎㅎ 픽업하거나",
          "original_form": "떡볶이 젤 많이 시켜먹었을거에요ㅎㅎ 픽업하거나",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100039738.1.16",
          "form": "하 다 좋아하는데",
          "original_form": "하 다 좋아하는데",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100039738.1.17",
          "form": "신전 사랑합니다...",
          "original_form": "신전 사랑합니다...",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100039738.1.18",
          "form": "아 헐 저 어제 신전 로제 먹었는데!!!!!",
          "original_form": "아 헐 저 어제 신전 로제 먹었는데!!!!!",
          "speaker_id": "1",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039738.1.19",
          "form": "저 매운거 진짜 못 먹는데 신전 로제는 무조건 2단계로 먹어요....",
          "original_form": "저 매운거 진짜 못 먹는데 신전 로제는 무조건 2단계로 먹어요....",
          "speaker_id": "1",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039738.1.20",
          "form": "로제는 신전이 짱인듯요",
          "original_form": "로제는 신전이 짱인듯요",
          "speaker_id": "1",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039738.1.21",
          "form": "헐 대박",
          "original_form": "헐 대박",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100039738.1.22",
          "form": "어제 제가 신전을 먹었어야했네요(이마 탁",
          "original_form": "어제 제가 신전을 먹었어야했네요(이마 탁",
          "speaker_id": "2",
          "time": "20210903 23:20"
        },
        {
          "id": "MDRW2100039738.1.23",
          "form": "신전 로제도 괜찮았던거같아요!",
          "original_form": "신전 로제도 괜찮았던거같아요!",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100039738.1.24",
          "form": "로제 추가해서 5천원이라 쵸큼 비싼값이 있지만...",
          "original_form": "로제 추가해서 5천원이라 쵸큼 비싼값이 있지만...",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100039738.1.25",
          "form": "하 담에 신전 로제에 감자면 추가해서 먹을래요",
          "original_form": "하 담에 신전 로제에 감자면 추가해서 먹을래요",
          "speaker_id": "2",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100039738.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ마자요... 감자면 맛있나요??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ마자요... 감자면 맛있나요??",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100039738.1.27",
          "form": "저 아직 시도해본 적 없어용",
          "original_form": "저 아직 시도해본 적 없어용",
          "speaker_id": "1",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100039738.1.28",
          "form": "맛있있어요!! 면도 꽤 있었구",
          "original_form": "맛있있어요!! 면도 꽤 있었구",
          "speaker_id": "2",
          "time": "20210903 23:25"
        },
        {
          "id": "MDRW2100039738.1.29",
          "form": "중국당면 좋아하시면 한 번 추가해서 드셔두 나쁘지 않을거 같아여ㅎㅎ",
          "original_form": "중국당면 좋아하시면 한 번 추가해서 드셔두 나쁘지 않을거 같아여ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:26"
        },
        {
          "id": "MDRW2100039738.1.30",
          "form": "헐 넵 저 중당 없어서 못 먹는데 담에 꼭 도전해볼게요",
          "original_form": "헐 넵 저 중당 없어서 못 먹는데 담에 꼭 도전해볼게요",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100039738.1.31",
          "form": "벌써 설레네요...",
          "original_form": "벌써 설레네요...",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100039738.1.32",
          "form": "좋아요,,,!!! name1님의 입맛에도 맞기를~~!",
          "original_form": "좋아요,,,!!! &name1&님의 입맛에도 맞기를~~!",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100039738.1.33",
          "form": "오뎅튀김도 필수필수!",
          "original_form": "오뎅튀김도 필수필수!",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100039738.1.34",
          "form": "헤헷 조아요 오뎅튀김은 당연히 필수죠!!",
          "original_form": "헤헷 조아요 오뎅튀김은 당연히 필수죠!!",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100039738.1.35",
          "form": "전 단호박 튀김 갠적으로 너무 좋아해요ㅋㅋㅋ!!",
          "original_form": "전 단호박 튀김 갠적으로 너무 좋아해요ㅋㅋㅋ!!",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100039738.1.36",
          "form": "헛 신전 단호박튀김은 안먹어봤는데",
          "original_form": "헛 신전 단호박튀김은 안먹어봤는데",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100039738.1.37",
          "form": "단호박 튀김 맛있죵ㅠㅠ",
          "original_form": "단호박 튀김 맛있죵ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100039738.1.38",
          "form": "야채튀김 김말이...다 좋아해요",
          "original_form": "야채튀김 김말이...다 좋아해요",
