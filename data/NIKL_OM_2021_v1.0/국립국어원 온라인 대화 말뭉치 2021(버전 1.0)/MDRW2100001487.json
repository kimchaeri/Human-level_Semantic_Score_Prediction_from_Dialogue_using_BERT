      "utterance": [
        {
          "id": "MDRW2100001487.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211015 13:56"
        },
        {
          "id": "MDRW2100001487.1.2",
          "form": "안녕하세요^^~",
          "original_form": "안녕하세요^^~",
          "speaker_id": "2",
          "time": "20211015 13:57"
        },
        {
          "id": "MDRW2100001487.1.3",
          "form": "혹시 최근에 사신 물건이 있으신가요?",
          "original_form": "혹시 최근에 사신 물건이 있으신가요?",
          "speaker_id": "2",
          "time": "20211015 13:57"
        },
        {
          "id": "MDRW2100001487.1.4",
          "form": "네 최근에 골프채 구매했어요",
          "original_form": "네 최근에 골프채 구매했어요",
          "speaker_id": "1",
          "time": "20211015 13:57"
        },
        {
          "id": "MDRW2100001487.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20211015 13:57"
        },
        {
          "id": "MDRW2100001487.1.6",
          "form": "오 골프채라니",
          "original_form": "오 골프채라니",
          "speaker_id": "2",
          "time": "20211015 13:57"
        },
        {
          "id": "MDRW2100001487.1.7",
          "form": "골프 치시는구나",
          "original_form": "골프 치시는구나",
          "speaker_id": "2",
          "time": "20211015 13:57"
        },
        {
          "id": "MDRW2100001487.1.8",
          "form": "골린이애요",
          "original_form": "골린이애요",
          "speaker_id": "1",
          "time": "20211015 13:57"
        },
        {
          "id": "MDRW2100001487.1.9",
          "form": "저는 가을옷 샀어요 ㅎㅎ",
          "original_form": "저는 가을옷 샀어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 13:57"
        },
        {
          "id": "MDRW2100001487.1.10",
          "form": "저는 아예 채를 잡아본적도 없는",
          "original_form": "저는 아예 채를 잡아본적도 없는",
          "speaker_id": "2",
          "time": "20211015 13:57"
        },
        {
          "id": "MDRW2100001487.1.11",
          "form": "골린이 조차도 못되는..",
          "original_form": "골린이 조차도 못되는..",
          "speaker_id": "2",
          "time": "20211015 13:58"
        },
        {
          "id": "MDRW2100001487.1.12",
          "form": "요새 다들 골프가 유행이라서요",
          "original_form": "요새 다들 골프가 유행이라서요",
          "speaker_id": "1",
          "time": "20211015 13:58"
        },
        {
          "id": "MDRW2100001487.1.13",
          "form": "그렇긴한가봐요",
          "original_form": "그렇긴한가봐요",
          "speaker_id": "2",
          "time": "20211015 13:58"
        },
        {
          "id": "MDRW2100001487.1.14",
          "form": "코로나 이후로 부쩍 인기가 많아진 느낌!",
          "original_form": "코로나 이후로 부쩍 인기가 많아진 느낌!",
          "speaker_id": "2",
          "time": "20211015 13:58"
        },
        {
          "id": "MDRW2100001487.1.15",
          "form": "레슨을 받아서 한번해보세요",
          "original_form": "레슨을 받아서 한번해보세요",
          "speaker_id": "1",
          "time": "20211015 13:58"
        },
        {
          "id": "MDRW2100001487.1.16",
          "form": "네 한 번 배워볼 생각은 있어요 ㅎㅎ",
          "original_form": "네 한 번 배워볼 생각은 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 13:58"
        },
        {
          "id": "MDRW2100001487.1.17",
          "form": "빨리할수록 좋은듯해요",
          "original_form": "빨리할수록 좋은듯해요",
          "speaker_id": "1",
          "time": "20211015 13:58"
        },
        {
          "id": "MDRW2100001487.1.18",
          "form": "가을옷이요? 겨울옷이요?",
          "original_form": "가을옷이요? 겨울옷이요?",
          "speaker_id": "1",
          "time": "20211015 13:58"
        },
        {
          "id": "MDRW2100001487.1.19",
          "form": "잠바요?",
          "original_form": "잠바요?",
          "speaker_id": "1",
          "time": "20211015 13:59"
        },
        {
          "id": "MDRW2100001487.1.20",
          "form": "니트를 샀습니당ㅎㅎ",
          "original_form": "니트를 샀습니당ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 14:00"
        },
        {
          "id": "MDRW2100001487.1.21",
          "form": "근데.. 벌써 겨울이네욬ㅋㅋㅋㅋ ㅋ날씨가",
          "original_form": "근데.. 벌써 겨울이네욬ㅋㅋㅋㅋ ㅋ날씨가",
          "speaker_id": "2",
          "time": "20211015 14:00"
        },
        {
          "id": "MDRW2100001487.1.22",
          "form": "겨울옷 사러 또 가야될거같아요...",
          "original_form": "겨울옷 사러 또 가야될거같아요...",
          "speaker_id": "2",
          "time": "20211015 14:00"
        },
        {
          "id": "MDRW2100001487.1.23",
          "form": "벌써 겨울이네요",
          "original_form": "벌써 겨울이네요",
          "speaker_id": "1",
          "time": "20211015 14:00"
        },
        {
          "id": "MDRW2100001487.1.24",
          "form": "가을이 짧아져가네요",
          "original_form": "가을이 짧아져가네요",
          "speaker_id": "1",
          "time": "20211015 14:00"
        },
        {
          "id": "MDRW2100001487.1.25",
          "form": "옷은 주로 어디서 구매하나요?",
          "original_form": "옷은 주로 어디서 구매하나요?",
          "speaker_id": "1",
          "time": "20211015 14:01"
        },
        {
          "id": "MDRW2100001487.1.26",
          "form": "다음주 제 생일이라서요",
          "original_form": "다음주 제 생일이라서요",
          "speaker_id": "1",
          "time": "20211015 14:01"
        },
        {
          "id": "MDRW2100001487.1.27",
          "form": "저는 주로 SPA 브랜드에서 많이 사요!",
          "original_form": "저는 주로 SPA 브랜드에서 많이 사요!",
          "speaker_id": "2",
          "time": "20211015 14:03"
        },
        {
          "id": "MDRW2100001487.1.28",
          "form": "오 생일시구나 ㅎㅎ",
          "original_form": "오 생일시구나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 14:03"
        },
        {
          "id": "MDRW2100001487.1.29",
          "form": "미리 축하드려요!",
          "original_form": "미리 축하드려요!",
          "speaker_id": "2",
          "time": "20211015 14:03"
        },
        {
          "id": "MDRW2100001487.1.30",
          "form": "생일 기념 옷 선물 많이 받으시길 ㅎㅎ\r\n",
          "original_form": "생일 기념 옷 선물 많이 받으시길 ㅎㅎ\r\n",
          "speaker_id": "2",
          "time": "20211015 14:03"
        }
      ]
