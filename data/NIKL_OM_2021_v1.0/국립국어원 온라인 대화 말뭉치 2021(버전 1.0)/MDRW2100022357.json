      "utterance": [
        {
          "id": "MDRW2100022357.1.1",
          "form": "빵으로 식사하면 얼마나 걸려요?",
          "original_form": "빵으로 식사하면 얼마나 걸려요?",
          "speaker_id": "2",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100022357.1.2",
          "form": "금방 먹긴해요",
          "original_form": "금방 먹긴해요",
          "speaker_id": "1",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100022357.1.3",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100022357.1.4",
          "form": "밥 다먹으면 뭐할거에요?",
          "original_form": "밥 다먹으면 뭐할거에요?",
          "speaker_id": "2",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100022357.1.5",
          "form": "저 식사시간 조절하게요",
          "original_form": "저 식사시간 조절하게요",
          "speaker_id": "2",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100022357.1.6",
          "form": "어.. 쉬지않을까요? 천천히 드세요",
          "original_form": "어.. 쉬지않을까요? 천천히 드세요",
          "speaker_id": "1",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100022357.1.7",
          "form": "ㅋㅋㅋ왤케 급해요",
          "original_form": "ㅋㅋㅋ왤케 급해요",
          "speaker_id": "1",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100022357.1.8",
          "form": "ㅋㅋㅋ그럼 밥 먹고 휴식 같이 가질래요?",
          "original_form": "ㅋㅋㅋ그럼 밥 먹고 휴식 같이 가질래요?",
          "speaker_id": "2",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100022357.1.9",
          "form": "체하세요",
          "original_form": "체하세요",
          "speaker_id": "1",
          "time": "20210829 13:02"
        },
        {
          "id": "MDRW2100022357.1.10",
          "form": "저 님이랑 같이 이야기 하고 싶어요",
          "original_form": "저 님이랑 같이 이야기 하고 싶어요",
          "speaker_id": "2",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022357.1.11",
          "form": "뽈찜은 뭐랑 드실거에요",
          "original_form": "뽈찜은 뭐랑 드실거에요",
          "speaker_id": "1",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022357.1.12",
          "form": "밥이랑 뽈찜만 먹나",
          "original_form": "밥이랑 뽈찜만 먹나",
          "speaker_id": "1",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022357.1.13",
          "form": "뽈찜만 먹으려구요 다른건 너무 귀찮아요",
          "original_form": "뽈찜만 먹으려구요 다른건 너무 귀찮아요",
          "speaker_id": "2",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022357.1.14",
          "form": "뭐에 어울리는 지조차 잘 모르겠더라구요",
          "original_form": "뭐에 어울리는 지조차 잘 모르겠더라구요",
          "speaker_id": "2",
          "time": "20210829 13:03"
        },
        {
          "id": "MDRW2100022357.1.15",
          "form": "뽈찜을 안먹어봐서",
          "original_form": "뽈찜을 안먹어봐서",
          "speaker_id": "1",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022357.1.16",
          "form": "음 뭐랑 어울릴지",
          "original_form": "음 뭐랑 어울릴지",
          "speaker_id": "1",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022357.1.17",
          "form": "일단 점심 때 먹어보고 저녁때 다른거랑 같이 먹게요",
          "original_form": "일단 점심 때 먹어보고 저녁때 다른거랑 같이 먹게요",
          "speaker_id": "2",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022357.1.18",
          "form": "생각해둔건 삶은계란 정도..?",
          "original_form": "생각해둔건 삶은계란 정도..?",
          "speaker_id": "2",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022357.1.19",
          "form": "아 여러개사셨구나",
          "original_form": "아 여러개사셨구나",
          "speaker_id": "1",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022357.1.20",
          "form": "저 하나만 샀는데",
          "original_form": "저 하나만 샀는데",
          "speaker_id": "2",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022357.1.21",
          "form": "계란? 계란이 어울리나",
          "original_form": "계란? 계란이 어울리나",
          "speaker_id": "1",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022357.1.22",
          "form": "양이 많아요 610g이더라구요",
          "original_form": "양이 많아요 610g이더라구요",
          "speaker_id": "2",
          "time": "20210829 13:04"
        },
        {
          "id": "MDRW2100022357.1.23",
          "form": "아 넉넉하구나",
          "original_form": "아 넉넉하구나",
