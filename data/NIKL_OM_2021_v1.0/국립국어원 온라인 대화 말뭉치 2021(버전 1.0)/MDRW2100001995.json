      "utterance": [
        {
          "id": "MDRW2100001995.1.1",
          "form": "좋아하는 프로 있어요?",
          "original_form": "좋아하는 프로 있어요?",
          "speaker_id": "1",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001995.1.2",
          "form": "저 예능프로 좋아해요!",
          "original_form": "저 예능프로 좋아해요!",
          "speaker_id": "2",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100001995.1.3",
          "form": "놀라운 토요일이랑 대탈출 잘 봐요 요새는",
          "original_form": "놀라운 토요일이랑 대탈출 잘 봐요 요새는",
          "speaker_id": "2",
          "time": "20210819 23:05"
        },
        {
          "id": "MDRW2100001995.1.4",
          "form": "전 런닝맨",
          "original_form": "전 런닝맨",
          "speaker_id": "1",
          "time": "20210819 23:06"
        },
        {
          "id": "MDRW2100001995.1.5",
          "form": "오 런닝맨 재밌죠",
          "original_form": "오 런닝맨 재밌죠",
          "speaker_id": "2",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100001995.1.6",
          "form": "저 1화부터 열심히 봤어요!!",
          "original_form": "저 1화부터 열심히 봤어요!!",
          "speaker_id": "2",
          "time": "20210819 23:07"
        },
        {
          "id": "MDRW2100001995.1.7",
          "form": "광수 빠져서 걱정했는데 그래도 재미있더라구요",
          "original_form": "광수 빠져서 걱정했는데 그래도 재미있더라구요",
          "speaker_id": "1",
          "time": "20210819 23:10"
        },
        {
          "id": "MDRW2100001995.1.8",
          "form": "그쵸 나가기 전엔 엄청 걱정했는데",
          "original_form": "그쵸 나가기 전엔 엄청 걱정했는데",
          "speaker_id": "2",
          "time": "20210819 23:11"
        },
        {
          "id": "MDRW2100001995.1.9",
          "form": "요새 재밌더라구요",
          "original_form": "요새 재밌더라구요",
          "speaker_id": "2",
          "time": "20210819 23:11"
        },
        {
          "id": "MDRW2100001995.1.10",
          "form": "네 케미가 오오",
          "original_form": "네 케미가 오오",
          "speaker_id": "1",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100001995.1.11",
          "form": "그 피디도 너무 웃겨요ㅋㅋㅋ",
          "original_form": "그 피디도 너무 웃겨요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100001995.1.12",
          "form": "멤버들이랑 잘어울리는 거 같아요",
          "original_form": "멤버들이랑 잘어울리는 거 같아요",
          "speaker_id": "2",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100001995.1.13",
          "form": "서로를 이제는 잘아는 듯",
          "original_form": "서로를 이제는 잘아는 듯",
          "speaker_id": "1",
          "time": "20210819 23:16"
        },
        {
          "id": "MDRW2100001995.1.14",
          "form": "그쵸 거기서 나오는 재미가 큰 거 같아요",
          "original_form": "그쵸 거기서 나오는 재미가 큰 거 같아요",
          "speaker_id": "2",
          "time": "20210819 23:22"
        },
        {
          "id": "MDRW2100001995.1.15",
          "form": "서로 너무 친하니까",
          "original_form": "서로 너무 친하니까",
          "speaker_id": "2",
          "time": "20210819 23:22"
        },
        {
          "id": "MDRW2100001995.1.16",
          "form": "맞아요 재미있어여",
          "original_form": "맞아요 재미있어여",
          "speaker_id": "1",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100001995.1.17",
          "form": "지석진까지",
          "original_form": "지석진까지",
          "speaker_id": "1",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100001995.1.18",
          "form": "요새 지석진 진짜 웃기지 않아요?",
          "original_form": "요새 지석진 진짜 웃기지 않아요?",
          "speaker_id": "2",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100001995.1.19",
          "form": "네 완전요",
          "original_form": "네 완전요",
          "speaker_id": "1",
          "time": "20210819 23:28"
        }
      ]
