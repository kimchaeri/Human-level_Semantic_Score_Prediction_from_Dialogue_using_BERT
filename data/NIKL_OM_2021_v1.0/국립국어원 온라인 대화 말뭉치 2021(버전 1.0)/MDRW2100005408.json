      "utterance": [
        {
          "id": "MDRW2100005408.1.1",
          "form": "이제 곧 가을이네요",
          "original_form": "이제 곧 가을이네요",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005408.1.2",
          "form": "오늘부터 가을장마인거 알고 계셨어요?",
          "original_form": "오늘부터 가을장마인거 알고 계셨어요?",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005408.1.3",
          "form": "내일부터 가을장마라는데 들으셨어요?",
          "original_form": "내일부터 가을장마라는데 들으셨어요?",
          "speaker_id": "1",
          "time": "20210821 22:15"
        },
        {
          "id": "MDRW2100005408.1.4",
          "form": "아까 일하고 있었을때 갑자기 비가와서 당황했어요",
          "original_form": "아까 일하고 있었을때 갑자기 비가와서 당황했어요",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005408.1.5",
          "form": "가을장마 처음들어보는데요? 여름이 끝나가는건가요?",
          "original_form": "가을장마 처음들어보는데요? 여름이 끝나가는건가요?",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005408.1.6",
          "form": "네 일본쪽에서 가을장마 옮겨왔다는데",
          "original_form": "네 일본쪽에서 가을장마 옮겨왔다는데",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005408.1.7",
          "form": "이제 가을이라는 얘기같아요",
          "original_form": "이제 가을이라는 얘기같아요",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005408.1.8",
          "form": "가을 좋아하세요?",
          "original_form": "가을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005408.1.9",
          "form": "저는 4계절중에 가을을 좋아해요 이제 곧 가을이 오니 기분이 좋네요",
          "original_form": "저는 4계절중에 가을을 좋아해요 이제 곧 가을이 오니 기분이 좋네요",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005408.1.10",
          "form": "어떤 계절을 좋아하세요?",
          "original_form": "어떤 계절을 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005408.1.11",
          "form": "가을이 왜 좋은거같아요?",
          "original_form": "가을이 왜 좋은거같아요?",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005408.1.12",
          "form": "아 저는 봄좋아해요",
          "original_form": "아 저는 봄좋아해요",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005408.1.13",
          "form": "향이 되게 마음에드네요",
          "original_form": "향이 되게 마음에드네요",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005408.1.14",
          "form": "덥지도 않고 춥지도 않는 계절같아요",
          "original_form": "덥지도 않고 춥지도 않는 계절같아요",
          "speaker_id": "2",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005408.1.15",
          "form": "하계올림픽 이제 끝났는데",
          "original_form": "하계올림픽 이제 끝났는데",
          "speaker_id": "1",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005408.1.16",
          "form": "올해 겨울에 또 열리겠네요",
          "original_form": "올해 겨울에 또 열리겠네요",
          "speaker_id": "1",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005408.1.17",
          "form": "기대되는 중목이 있어요",
          "original_form": "기대되는 중목이 있어요",
          "speaker_id": "2",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005408.1.18",
          "form": "올림픽에 관심있어요?",
          "original_form": "올림픽에 관심있어요?",
          "speaker_id": "2",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005408.1.19",
          "form": "네 개막식이나 페막식보는게 되게 재밌어서 좋아해요",
          "original_form": "네 개막식이나 페막식보는게 되게 재밌어서 좋아해요",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005408.1.20",
          "form": "저희쪽은 눈 잘 안내리는데",
          "original_form": "저희쪽은 눈 잘 안내리는데",
          "speaker_id": "1",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005408.1.21",
          "form": "그쪽에는 눈 자주내리나요?",
          "original_form": "그쪽에는 눈 자주내리나요?",
          "speaker_id": "1",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005408.1.22",
          "form": "이쪽은 눈사람 만들 정도로 내려요 겨울은 춥잖아요 겨울에 생각나는거 있어요?",
          "original_form": "이쪽은 눈사람 만들 정도로 내려요 겨울은 춥잖아요 겨울에 생각나는거 있어요?",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005408.1.23",
          "form": "저는 크리스마스 캐롤들리면 그 분위기가 좋은거같아요",
          "original_form": "저는 크리스마스 캐롤들리면 그 분위기가 좋은거같아요",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005408.1.24",
          "form": "캐롤 좋아하세요?",
          "original_form": "캐롤 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005408.1.25",
          "form": "겨울되면 캐롤만 듣는거같아요 이번 크리스마스도 눈이 내릴까요?",
          "original_form": "겨울되면 캐롤만 듣는거같아요 이번 크리스마스도 눈이 내릴까요?",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005408.1.26",
          "form": "강원도나 그런쪽엔 내릴거같은데 저희쪽에는 원래 눈 자주 안와서 저는 크리스마스때 눈 못볼거같아요",
          "original_form": "강원도나 그런쪽엔 내릴거같은데 저희쪽에는 원래 눈 자주 안와서 저는 크리스마스때 눈 못볼거같아요",
          "speaker_id": "1",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005408.1.27",
          "form": "그럼 눈이 안내리면 겨울같지도 않잖아요 이번 크리스마스 기대안되나요?",
          "original_form": "그럼 눈이 안내리면 겨울같지도 않잖아요 이번 크리스마스 기대안되나요?",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005408.1.28",
          "form": "아뇨 정말 기대되네요",
          "original_form": "아뇨 정말 기대되네요",
          "speaker_id": "1",
          "time": "20210821 22:24"
        }
      ]
