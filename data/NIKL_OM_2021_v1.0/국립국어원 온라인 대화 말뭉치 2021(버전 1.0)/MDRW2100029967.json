      "utterance": [
        {
          "id": "MDRW2100029967.1.1",
          "form": "너 공연이나 전시회",
          "original_form": "너 공연이나 전시회",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100029967.1.2",
          "form": "본 적 잇어?",
          "original_form": "본 적 잇어?",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100029967.1.3",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029967.1.4",
          "form": "본 적 있어",
          "original_form": "본 적 있어",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029967.1.5",
          "form": "헐뭐 봤어?",
          "original_form": "헐뭐 봤어?",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029967.1.6",
          "form": "박효신 콘서트도 가본 적 있고",
          "original_form": "박효신 콘서트도 가본 적 있고",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029967.1.7",
          "form": "뮤지컬 팬텀도 본 적 있고",
          "original_form": "뮤지컬 팬텀도 본 적 있고",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029967.1.8",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029967.1.9",
          "form": "오페라도 한 번 봤어",
          "original_form": "오페라도 한 번 봤어",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029967.1.10",
          "form": "어땠어?",
          "original_form": "어땠어?",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029967.1.11",
          "form": "유명한 작품은 아니지만",
          "original_form": "유명한 작품은 아니지만",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029967.1.12",
          "form": "엄청 좋았어",
          "original_form": "엄청 좋았어",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029967.1.13",
          "form": "콘서트 얘기좀 해줘",
          "original_form": "콘서트 얘기좀 해줘",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029967.1.14",
          "form": "특히 콘서트는 진짜 좋았어",
          "original_form": "특히 콘서트는 진짜 좋았어",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029967.1.15",
          "form": "알겠어",
          "original_form": "알겠어",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029967.1.16",
          "form": "콘서트는 사운드가 진짜 대박이야",
          "original_form": "콘서트는 사운드가 진짜 대박이야",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029967.1.17",
          "form": "진짜로 소리가 진짜 커서",
          "original_form": "진짜로 소리가 진짜 커서",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029967.1.18",
          "form": "몸이 떨렸어",
          "original_form": "몸이 떨렸어",
          "speaker_id": "1",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029967.1.19",
          "form": "와 콘서트 스피거 커서",
          "original_form": "와 콘서트 스피거 커서",
          "speaker_id": "2",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029967.1.20",
          "form": "심장박동이 느껴짐",
          "original_form": "심장박동이 느껴짐",
          "speaker_id": "1",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029967.1.21",
          "form": "소리 진짜 좋앗겠다",
          "original_form": "소리 진짜 좋앗겠다",
          "speaker_id": "2",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029967.1.22",
          "form": "맞아맞아 진짜 좋았어",
          "original_form": "맞아맞아 진짜 좋았어",
          "speaker_id": "1",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029967.1.23",
          "form": "너랑 나중에 같이 가고싶다",
          "original_form": "너랑 나중에 같이 가고싶다",
          "speaker_id": "1",
          "time": "20210903 17:28"
        }
      ]
