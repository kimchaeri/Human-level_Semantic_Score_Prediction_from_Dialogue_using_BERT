      "utterance": [
        {
          "id": "MDRW2100018532.1.1",
          "form": "저도 망고 좋아해요",
          "original_form": "저도 망고 좋아해요",
          "speaker_id": "1",
          "time": "20210827 07:50"
        },
        {
          "id": "MDRW2100018532.1.2",
          "form": "생과 너무맛있더라구요",
          "original_form": "생과 너무맛있더라구요",
          "speaker_id": "1",
          "time": "20210827 07:50"
        },
        {
          "id": "MDRW2100018532.1.3",
          "form": "망고는 얼린 망고도 맛있고",
          "original_form": "망고는 얼린 망고도 맛있고",
          "speaker_id": "2",
          "time": "20210827 07:50"
        },
        {
          "id": "MDRW2100018532.1.4",
          "form": "말린것도 맛있어요",
          "original_form": "말린것도 맛있어요",
          "speaker_id": "2",
          "time": "20210827 07:50"
        },
        {
          "id": "MDRW2100018532.1.5",
          "form": "말린건 진짜 달더라구요",
          "original_form": "말린건 진짜 달더라구요",
          "speaker_id": "1",
          "time": "20210827 07:50"
        },
        {
          "id": "MDRW2100018532.1.6",
          "form": "이래도 되나싶을정도로 달고 쫀득하고",
          "original_form": "이래도 되나싶을정도로 달고 쫀득하고",
          "speaker_id": "1",
          "time": "20210827 07:51"
        },
        {
          "id": "MDRW2100018532.1.7",
          "form": "애플망고도 좋아하세요?",
          "original_form": "애플망고도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 07:51"
        },
        {
          "id": "MDRW2100018532.1.8",
          "form": "애플망고도 좋아하는데 그냥 망고도 좋아해요!",
          "original_form": "애플망고도 좋아하는데 그냥 망고도 좋아해요!",
          "speaker_id": "2",
          "time": "20210827 07:51"
        },
        {
          "id": "MDRW2100018532.1.9",
          "form": "가장 좋아하는 음식 있으세요?",
          "original_form": "가장 좋아하는 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210827 07:51"
        },
        {
          "id": "MDRW2100018532.1.10",
          "form": "음.. ,저는 육류 좋아해요",
          "original_form": "음.. ,저는 육류 좋아해요",
          "speaker_id": "1",
          "time": "20210827 07:51"
        },
        {
          "id": "MDRW2100018532.1.11",
          "form": "삼겹살?",
          "original_form": "삼겹살?",
          "speaker_id": "1",
          "time": "20210827 07:51"
        },
        {
          "id": "MDRW2100018532.1.12",
          "form": "아무래도 가장 접근도 쉽고 맛있어서ㅎ",
          "original_form": "아무래도 가장 접근도 쉽고 맛있어서ㅎ",
          "speaker_id": "1",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100018532.1.13",
          "form": "오호 저는 꽃게탕 좋아해요",
          "original_form": "오호 저는 꽃게탕 좋아해요",
          "speaker_id": "2",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100018532.1.14",
          "form": "아 피자도!",
          "original_form": "아 피자도!",
          "speaker_id": "2",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100018532.1.15",
          "form": "저도 피자좋아해요",
          "original_form": "저도 피자좋아해요",
          "speaker_id": "1",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100018532.1.16",
          "form": "그럼 질문",
          "original_form": "그럼 질문",
          "speaker_id": "1",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100018532.1.17",
          "form": "하와이안 피자 호 불호",
          "original_form": "하와이안 피자 호 불호",
          "speaker_id": "1",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100018532.1.18",
          "form": "호에요ㅋㅋㅋ",
          "original_form": "호에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100018532.1.19",
          "form": "저도 호에요",
          "original_form": "저도 호에요",
          "speaker_id": "1",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100018532.1.20",
          "form": "저는 호불호 갈리는 음식 대부분이 호에요ㅋㅋ",
          "original_form": "저는 호불호 갈리는 음식 대부분이 호에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100018532.1.21",
          "form": "피자는 다 맛있거든요ㅆ",
          "original_form": "피자는 다 맛있거든요ㅆ",
          "speaker_id": "1",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100018532.1.22",
          "form": "민초 좋아하세요?",
          "original_form": "민초 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100018532.1.23",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210827 07:52"
        },
        {
          "id": "MDRW2100018532.1.24",
          "form": "우리잠시 멀어져요",
          "original_form": "우리잠시 멀어져요",
          "speaker_id": "1",
          "time": "20210827 07:53"
        },
        {
          "id": "MDRW2100018532.1.25",
          "form": "민초 못먹습니다 ㅎ",
          "original_form": "민초 못먹습니다 ㅎ",
          "speaker_id": "1",
          "time": "20210827 07:53"
        },
        {
          "id": "MDRW2100018532.1.26",
          "form": "ㅋㅋㅋ 반민초단이시군요!",
