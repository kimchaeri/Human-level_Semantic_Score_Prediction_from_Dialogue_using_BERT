      "utterance": [
        {
          "id": "MDRW2100034235.1.1",
          "form": "저는 딱 올초에 첨으로 불면증을 겪었어요",
          "original_form": "저는 딱 올초에 첨으로 불면증을 겪었어요",
          "speaker_id": "2",
          "time": "20210827 21:41"
        },
        {
          "id": "MDRW2100034235.1.2",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100034235.1.3",
          "form": "원래 엄청 잘 잤거든요.. 그래서 암튼 올초에 이것저것 알아봤죠",
          "original_form": "원래 엄청 잘 잤거든요.. 그래서 암튼 올초에 이것저것 알아봤죠",
          "speaker_id": "2",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100034235.1.4",
          "form": "코로나 불루였나봐요..",
          "original_form": "코로나 불루였나봐요..",
          "speaker_id": "2",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100034235.1.5",
          "form": "전갱년기 두통이 있어서",
          "original_form": "전갱년기 두통이 있어서",
          "speaker_id": "1",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100034235.1.6",
          "form": "작년 일년간 스트레스 나도모르게 받았던게 터졌나봐요",
          "original_form": "작년 일년간 스트레스 나도모르게 받았던게 터졌나봐요",
          "speaker_id": "2",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100034235.1.7",
          "form": "불면증도 같이와요",
          "original_form": "불면증도 같이와요",
          "speaker_id": "1",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100034235.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100034235.1.9",
          "form": "코로나가 진찌",
          "original_form": "코로나가 진찌",
          "speaker_id": "1",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100034235.1.10",
          "form": "사람을 피폐하게 해요",
          "original_form": "사람을 피폐하게 해요",
          "speaker_id": "1",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100034235.1.11",
          "form": "그래서 약도 처음 먹어봤고",
          "original_form": "그래서 약도 처음 먹어봤고",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100034235.1.12",
          "form": "스트레스가 진짜 장난 아니예요",
          "original_form": "스트레스가 진짜 장난 아니예요",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100034235.1.13",
          "form": "근데 약 말고 자는데 도움 되는 걸 찾다보니까.. 마그네슘 먹으라고 하고",
          "original_form": "근데 약 말고 자는데 도움 되는 걸 찾다보니까.. 마그네슘 먹으라고 하고",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100034235.1.14",
          "form": "상추나 국화차 도움된다고 하고",
          "original_form": "상추나 국화차 도움된다고 하고",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100034235.1.15",
          "form": "저도 먹어요",
          "original_form": "저도 먹어요",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100034235.1.16",
          "form": "그래서 그런거 챙기게 됐어요",
          "original_form": "그래서 그런거 챙기게 됐어요",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100034235.1.17",
          "form": "영양제도 한트럭 이예요 ㅋㅋ",
          "original_form": "영양제도 한트럭 이예요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100034235.1.18",
          "form": "잘하셨어요",
          "original_form": "잘하셨어요",
          "speaker_id": "1",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100034235.1.19",
          "form": "근데 아마 이번 코로나로 불명증 환자 엄청 늘었을거에요",
          "original_form": "근데 아마 이번 코로나로 불명증 환자 엄청 늘었을거에요",
          "speaker_id": "2",
          "time": "20210827 21:43"
        },
        {
          "id": "MDRW2100034235.1.20",
          "form": "이젠 치료제 나오기 까진",
          "original_form": "이젠 치료제 나오기 까진",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100034235.1.21",
          "form": "알게 모르게 다들 스트레스 만땅일듯",
          "original_form": "알게 모르게 다들 스트레스 만땅일듯",
          "speaker_id": "2",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100034235.1.22",
          "form": "계속 갈것 같아요 코로나",
          "original_form": "계속 갈것 같아요 코로나",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100034235.1.23",
          "form": "그럼요 저도 우울 하더라구요",
          "original_form": "그럼요 저도 우울 하더라구요",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100034235.1.24",
          "form": "그니까요.. 매년 백신 맞고 살아야 할 거 같아요.. 백신 무서운데",
          "original_form": "그니까요.. 매년 백신 맞고 살아야 할 거 같아요.. 백신 무서운데",
          "speaker_id": "2",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100034235.1.25",
          "form": "전 접종도일찍했러요",
          "original_form": "전 접종도일찍했러요",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100034235.1.26",
          "form": "넘 일찍 한듯해요 ㅋㅋ",
          "original_form": "넘 일찍 한듯해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100034235.1.27",
          "form": "전 아직 안맞았어요",
          "original_form": "전 아직 안맞았어요",
          "speaker_id": "2",
          "time": "20210827 21:45"
        },
        {
          "id": "MDRW2100034235.1.28",
          "form": "백신 담달에 맞아요",
          "original_form": "백신 담달에 맞아요",
          "speaker_id": "2",
          "time": "20210827 21:45"
        }
      ]
    }
