      "utterance": [
        {
          "id": "MDRW2100017273.1.1",
          "form": "학교 가기 싫어",
          "original_form": "학교 가기 싫어",
          "speaker_id": "1",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017273.1.2",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017273.1.3",
          "form": "아악",
          "original_form": "아악",
          "speaker_id": "2",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017273.1.4",
          "form": "말이 돼?",
          "original_form": "말이 돼?",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017273.1.5",
          "form": "벌써 2달 반이 지났다고?",
          "original_form": "벌써 2달 반이 지났다고?",
          "speaker_id": "1",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017273.1.6",
          "form": "왜?!",
          "original_form": "왜?!",
          "speaker_id": "2",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017273.1.7",
          "form": "대체 왜에에ㅔ",
          "original_form": "대체 왜에에ㅔ",
          "speaker_id": "2",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017273.1.8",
          "form": "아 정말 믿을 수 없어",
          "original_form": "아 정말 믿을 수 없어",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017273.1.9",
          "form": "너희도 개강 9월 1일이야?",
          "original_form": "너희도 개강 9월 1일이야?",
          "speaker_id": "1",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017273.1.10",
          "form": "ㄴㄴ 30",
          "original_form": "ㄴㄴ 30",
          "speaker_id": "2",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017273.1.11",
          "form": "...ㅎ",
          "original_form": "...ㅎ",
          "speaker_id": "2",
          "time": "20210826 18:31"
        },
        {
          "id": "MDRW2100017273.1.12",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017273.1.13",
          "form": "8월 30일?",
          "original_form": "8월 30일?",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017273.1.14",
          "form": "뭐야 그 애매한 날은",
          "original_form": "뭐야 그 애매한 날은",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017273.1.15",
          "form": "월요일이잖아...",
          "original_form": "월요일이잖아...",
          "speaker_id": "2",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100017273.1.16",
          "form": "너무하다 정말",
          "original_form": "너무하다 정말",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017273.1.17",
          "form": "저번에도 월요일에 개강했어?",
          "original_form": "저번에도 월요일에 개강했어?",
          "speaker_id": "1",
          "time": "20210826 18:34"
        },
        {
          "id": "MDRW2100017273.1.18",
          "form": "아니 화요일...",
          "original_form": "아니 화요일...",
          "speaker_id": "2",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100017273.1.19",
          "form": "그럼 그냥 1일에 하지",
          "original_form": "그럼 그냥 1일에 하지",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017273.1.20",
          "form": "떼잉...",
          "original_form": "떼잉...",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017273.1.21",
          "form": "에휴..",
          "original_form": "에휴..",
          "speaker_id": "2",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017273.1.22",
          "form": "일주일도 안 남았어",
          "original_form": "일주일도 안 남았어",
          "speaker_id": "1",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017273.1.23",
          "form": "끔찍해",
          "original_form": "끔찍해",
          "speaker_id": "1",
          "time": "20210826 18:37"
        },
        {
