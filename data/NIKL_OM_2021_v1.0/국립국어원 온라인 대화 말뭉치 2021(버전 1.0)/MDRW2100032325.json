      "utterance": [
        {
          "id": "MDRW2100032325.1.1",
          "form": "주말인데 가족이랑 뭐해여",
          "original_form": "주말인데 가족이랑 뭐해여",
          "speaker_id": "1",
          "time": "20210905 15:34"
        },
        {
          "id": "MDRW2100032325.1.2",
          "form": "엄마는.외가에 고추따러 갔구 아빠는 친가에 벌초하러 갔어여 ㅋㅋㅋㅋ",
          "original_form": "엄마는.외가에 고추따러 갔구 아빠는 친가에 벌초하러 갔어여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:59"
        },
        {
          "id": "MDRW2100032325.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:59"
        },
        {
          "id": "MDRW2100032325.1.4",
          "form": "전 누워있음",
          "original_form": "전 누워있음",
          "speaker_id": "2",
          "time": "20210905 15:59"
        },
        {
          "id": "MDRW2100032325.1.5",
          "form": "아 다들 바쁘시네요",
          "original_form": "아 다들 바쁘시네요",
          "speaker_id": "1",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100032325.1.6",
          "form": "저도 누워서 핸펀만하고 있어여",
          "original_form": "저도 누워서 핸펀만하고 있어여",
          "speaker_id": "1",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100032325.1.7",
          "form": "동생은 알바하고 집옴",
          "original_form": "동생은 알바하고 집옴",
          "speaker_id": "1",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100032325.1.8",
          "form": "오 주말알바",
          "original_form": "오 주말알바",
          "speaker_id": "2",
          "time": "20210905 16:08"
        },
        {
          "id": "MDRW2100032325.1.9",
          "form": "저희동생도 겜만하는데",
          "original_form": "저희동생도 겜만하는데",
          "speaker_id": "2",
          "time": "20210905 16:08"
        },
        {
          "id": "MDRW2100032325.1.10",
          "form": "커피숍알바해여",
          "original_form": "커피숍알바해여",
          "speaker_id": "1",
          "time": "20210905 16:27"
        },
        {
          "id": "MDRW2100032325.1.11",
          "form": "겜도 부지런해야하는뎅",
          "original_form": "겜도 부지런해야하는뎅",
          "speaker_id": "1",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100032325.1.12",
          "form": "주말에는 집안일 최소한으로만ㅋㅋ원래도안하지만 암것도안하고 티비보며 힐링",
          "original_form": "주말에는 집안일 최소한으로만ㅋㅋ원래도안하지만 암것도안하고 티비보며 힐링",
          "speaker_id": "1",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100032325.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 마자여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아 마자여",
          "speaker_id": "2",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100032325.1.14",
          "form": "전 오늘 하루종일 뜨개질 했어요",
          "original_form": "전 오늘 하루종일 뜨개질 했어요",
          "speaker_id": "2",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100032325.1.15",
          "form": "팔아픔 ㅜ",
          "original_form": "팔아픔 ㅜ",
          "speaker_id": "2",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100032325.1.16",
          "form": "와우 뭐뜨고있어여? 이젠 눈감고할듯",
          "original_form": "와우 뭐뜨고있어여? 이젠 눈감고할듯",
          "speaker_id": "1",
          "time": "20210905 21:48"
        },
        {
          "id": "MDRW2100032325.1.17",
          "form": "친구네 애기 담요요",
          "original_form": "친구네 애기 담요요",
          "speaker_id": "2",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100032325.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100032325.1.19",
          "form": "7월부터 뜨던거 아직도 떠요ㅠ",
          "original_form": "7월부터 뜨던거 아직도 떠요ㅠ",
          "speaker_id": "2",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100032325.1.20",
          "form": "찐친이네요.  친구는 육아하느냐고 정신없겠어요",
          "original_form": "찐친이네요.  친구는 육아하느냐고 정신없겠어요",
          "speaker_id": "1",
          "time": "20210906 07:40"
        },
        {
          "id": "MDRW2100032325.1.21",
          "form": "네 아들만 둘이라...",
          "original_form": "네 아들만 둘이라...",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100032325.1.22",
          "form": "최고죠 ㅋㅋㅋㅋ 육아 난이도 상상상",
          "original_form": "최고죠 ㅋㅋㅋㅋ 육아 난이도 상상상",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100032325.1.23",
          "form": "주임님은 집안일 뭐해여",
          "original_form": "주임님은 집안일 뭐해여",
          "speaker_id": "1",
          "time": "20210907 07:20"
        },
        {
          "id": "MDRW2100032325.1.24",
          "form": "설거지 하구 빨래...??",
          "original_form": "설거지 하구 빨래...??",
          "speaker_id": "2",
          "time": "20210907 07:27"
        },
        {
          "id": "MDRW2100032325.1.25",
          "form": "별로 안 해요 ㅋㅋㅋ",
          "original_form": "별로 안 해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 07:27"
        },
        {
          "id": "MDRW2100032325.1.26",
          "form": "빨라는 세탁기가해주고ㅋㅋㅋ저도 딱 그정도",
          "original_form": "빨라는 세탁기가해주고ㅋㅋㅋ저도 딱 그정도",
          "speaker_id": "1",
          "time": "20210907 07:41"
        },
        {
          "id": "MDRW2100032325.1.27",
          "form": "설거지 빨래돌리기 개기",
          "original_form": "설거지 빨래돌리기 개기",
          "speaker_id": "1",
          "time": "20210907 07:42"
        },
        {
          "id": "MDRW2100032325.1.28",
