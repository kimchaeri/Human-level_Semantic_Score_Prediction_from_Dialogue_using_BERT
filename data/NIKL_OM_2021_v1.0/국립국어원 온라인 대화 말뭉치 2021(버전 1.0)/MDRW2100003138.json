        {
          "id": "MDRW2100003138.1.1",
          "form": "오늘은 운동 못했네",
          "original_form": "오늘은 운동 못했네",
          "speaker_id": "2",
          "time": "20210820 19:17"
        },
        {
          "id": "MDRW2100003138.1.2",
          "form": "ㅎㅎ 내가 집에서 한 잔 하느라 못했네",
          "original_form": "ㅎㅎ 내가 집에서 한 잔 하느라 못했네",
          "speaker_id": "1",
          "time": "20210820 19:17"
        },
        {
          "id": "MDRW2100003138.1.3",
          "form": "낮에라도 나갈 걸 그랬나?",
          "original_form": "낮에라도 나갈 걸 그랬나?",
          "speaker_id": "2",
          "time": "20210820 19:17"
        },
        {
          "id": "MDRW2100003138.1.4",
          "form": "나 한 잔 하는 날은 애들이랑 낮에 미리 나가요.",
          "original_form": "나 한 잔 하는 날은 애들이랑 낮에 미리 나가요.",
          "speaker_id": "1",
          "time": "20210820 19:18"
        },
        {
          "id": "MDRW2100003138.1.5",
          "form": "하긴 이젠 낮에도 많이 안더우니",
          "original_form": "하긴 이젠 낮에도 많이 안더우니",
          "speaker_id": "2",
          "time": "20210820 19:18"
        },
        {
          "id": "MDRW2100003138.1.6",
          "form": "ㅇㅇ 조금씩이라도 운동해",
          "original_form": "ㅇㅇ 조금씩이라도 운동해",
          "speaker_id": "1",
          "time": "20210820 19:18"
        },
        {
          "id": "MDRW2100003138.1.7",
          "form": "나 없어도 ㅎㅎ",
          "original_form": "나 없어도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 19:18"
        },
        {
          "id": "MDRW2100003138.1.8",
          "form": "ㅎㅎ 신랑이랑 운동하는 게 좋은데",
          "original_form": "ㅎㅎ 신랑이랑 운동하는 게 좋은데",
          "speaker_id": "2",
          "time": "20210820 19:18"
        },
        {
          "id": "MDRW2100003138.1.9",
          "form": "ㅎㅎ 평소에는 거의 나랑 운동하니깐",
          "original_form": "ㅎㅎ 평소에는 거의 나랑 운동하니깐",
          "speaker_id": "1",
          "time": "20210820 19:19"
        },
        {
          "id": "MDRW2100003138.1.10",
          "form": "하루정도 푹 쉬어",
          "original_form": "하루정도 푹 쉬어",
          "speaker_id": "1",
          "time": "20210820 19:19"
        },
        {
          "id": "MDRW2100003138.1.11",
          "form": "웅 ㅎㅎ",
          "original_form": "웅 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 19:19"
        },
        {
          "id": "MDRW2100003138.1.12",
          "form": "운동도 매일 하는 것보다 중간에 하루 정도 쉬어주는 게 좋대",
          "original_form": "운동도 매일 하는 것보다 중간에 하루 정도 쉬어주는 게 좋대",
          "speaker_id": "1",
          "time": "20210820 19:19"
        },
        {
          "id": "MDRW2100003138.1.13",
          "form": "웅 내일은 같이 해 ㅎㅎ",
          "original_form": "웅 내일은 같이 해 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 19:19"
        },
        {
          "id": "MDRW2100003138.1.14",
          "form": "알았옹 예약 ㅎㅎ",
          "original_form": "알았옹 예약 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 19:19"
        },
        {
          "id": "MDRW2100003138.1.15",
          "form": "낼은 애들 데리고 동네 산책하고 줄넘기도 해야겠다",
