      "utterance": [
        {
          "id": "MDRW2100044285.1.1",
          "form": "오 학교 다니시고있나요?",
          "original_form": "오 학교 다니시고있나요?",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044285.1.2",
          "form": "학교는 졸업했습니다ㅋㅋ",
          "original_form": "학교는 졸업했습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044285.1.3",
          "form": "출근지가 학교긴해요",
          "original_form": "출근지가 학교긴해요",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044285.1.4",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100044285.1.5",
          "form": "얼마전 개학해서",
          "original_form": "얼마전 개학해서",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044285.1.6",
          "form": "바쁘시겠어요",
          "original_form": "바쁘시겠어요",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100044285.1.7",
          "form": "아 넵 저희지역은 계속 대면수업이라서 그냥 평소처럼 하고있답니다",
          "original_form": "아 넵 저희지역은 계속 대면수업이라서 그냥 평소처럼 하고있답니다",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044285.1.8",
          "form": "아 그럼 다 등교하나요?",
          "original_form": "아 그럼 다 등교하나요?",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044285.1.9",
          "form": "돌아가면서 등교한다고 들었어요",
          "original_form": "돌아가면서 등교한다고 들었어요",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044285.1.10",
          "form": "네 작년2학기부터 계속등교했어요",
          "original_form": "네 작년2학기부터 계속등교했어요",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044285.1.11",
          "form": "등교하는게 좀덜바쁘네요ㅋㅋ",
          "original_form": "등교하는게 좀덜바쁘네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044285.1.12",
          "form": "줌보다 나으신가요ㅋㅋ",
          "original_form": "줌보다 나으신가요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044285.1.13",
          "form": "처음 온라인수업때는 혼돈 그 자체였던거같은데",
          "original_form": "처음 온라인수업때는 혼돈 그 자체였던거같은데",
          "speaker_id": "1",
          "time": "20210907 22:27"
        },
        {
          "id": "MDRW2100044285.1.14",
          "form": "줌은 괜찮은데 첨에 녹화수업할땐",
          "original_form": "줌은 괜찮은데 첨에 녹화수업할땐",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044285.1.15",
          "form": "당황하셨겠어요",
          "original_form": "당황하셨겠어요",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044285.1.16",
          "form": "5분짜리하나만드는데 몇시간걸려서 ㅋㅋ",
          "original_form": "5분짜리하나만드는데 몇시간걸려서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044285.1.17",
          "form": "으악ㅋㅋㅋ",
          "original_form": "으악ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044285.1.18",
          "form": "대면이면 금방 끝나는걸 몇시간이나ㅋ큐",
          "original_form": "대면이면 금방 끝나는걸 몇시간이나ㅋ큐",
          "speaker_id": "1",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044285.1.19",
          "form": "등교가 부모님들도 만족도 높고좋네요",
          "original_form": "등교가 부모님들도 만족도 높고좋네요",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044285.1.20",
          "form": "맞아요 그리고 대면이면 애들 푸는시간 포함해서 40분인데",
          "original_form": "맞아요 그리고 대면이면 애들 푸는시간 포함해서 40분인데",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044285.1.21",
          "form": "온전한 설명 강의로 시간을 채우기란 힘들더라구요",
          "original_form": "온전한 설명 강의로 시간을 채우기란 힘들더라구요",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044285.1.22",
          "form": "그져 눈보고 하는 수업이 생각보다 중요한거 같아요",
          "original_form": "그져 눈보고 하는 수업이 생각보다 중요한거 같아요",
          "speaker_id": "1",
          "time": "20210907 22:29"
        }
      ]
