      "utterance": [
        {
          "id": "MDRW2100024035.1.1",
          "form": "요즘 자주듣는 곡 있나요?",
          "original_form": "요즘 자주듣는 곡 있나요?",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100024035.1.2",
          "form": "그냥 멜론 차트 정주행해요 ㅋㅋㅋ",
          "original_form": "그냥 멜론 차트 정주행해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100024035.1.3",
          "form": "오~ 요즘 차트가 예전같지 않아서 차트별로 듣는게 어렵더라구요",
          "original_form": "오~ 요즘 차트가 예전같지 않아서 차트별로 듣는게 어렵더라구요",
          "speaker_id": "1",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100024035.1.4",
          "form": "근데 나이들어서 그런지 예전노래가 좋더라구여 ㅋㅋ",
          "original_form": "근데 나이들어서 그런지 예전노래가 좋더라구여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100024035.1.5",
          "form": "저두 간혹 추억에 잠기고 싶을때 그시절 노래 찾아듣곤해요",
          "original_form": "저두 간혹 추억에 잠기고 싶을때 그시절 노래 찾아듣곤해요",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100024035.1.6",
          "form": "심지어 리메이크도 많이해서",
          "original_form": "심지어 리메이크도 많이해서",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100024035.1.7",
          "form": "예전엔 팝송도 자주 들었는데",
          "original_form": "예전엔 팝송도 자주 들었는데",
          "speaker_id": "2",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100024035.1.8",
          "form": "요즘은 잘 안듣게되네요",
          "original_form": "요즘은 잘 안듣게되네요",
          "speaker_id": "2",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100024035.1.9",
          "form": "앗 저두요",
          "original_form": "앗 저두요",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100024035.1.10",
          "form": "웨라 팬이었는데",
          "original_form": "웨라 팬이었는데",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100024035.1.11",
          "form": "그때 한참 bsb 웨라 팬으로 나눴었는데",
          "original_form": "그때 한참 bsb 웨라 팬으로 나눴었는데",
          "speaker_id": "1",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100024035.1.12",
          "form": "크",
          "original_form": "크",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024035.1.13",
          "form": "추억이네요",
          "original_form": "추억이네요",
          "speaker_id": "2",
          "time": "20210830 21:37"
        },
        {
          "id": "MDRW2100024035.1.14",
          "form": "진짜 많이들었죠",
          "original_form": "진짜 많이들었죠",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100024035.1.15",
          "form": "전 둘다 좋아했다는",
          "original_form": "전 둘다 좋아했다는",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100024035.1.16",
          "form": "참 귀셨네요",
          "original_form": "참 귀셨네요",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100024035.1.17",
          "form": "다 띵곡들이라서",
          "original_form": "다 띵곡들이라서",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100024035.1.18",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100024035.1.19",
          "form": "요즘은 그런그룹이 없네요",
          "original_form": "요즘은 그런그룹이 없네요",
          "speaker_id": "2",
          "time": "20210830 21:40"
        },
        {
          "id": "MDRW2100024035.1.20",
          "form": "그쵸 그나마 듣는게 혼네 정도네요",
          "original_form": "그쵸 그나마 듣는게 혼네 정도네요",
          "speaker_id": "1",
          "time": "20210830 21:43"
        },
        {
          "id": "MDRW2100024035.1.21",
          "form": "bts꺼좀 듣고",
          "original_form": "bts꺼좀 듣고",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100024035.1.22",
          "form": "요즘 노래 금방 질려서",
          "original_form": "요즘 노래 금방 질려서",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100024035.1.23",
          "form": "요즘 악뮤 새로나온 앨범 좋아서 자주들어요",
          "original_form": "요즘 악뮤 새로나온 앨범 좋아서 자주들어요",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024035.1.24",
          "form": "악뮤 앨범 나왔군여?",
          "original_form": "악뮤 앨범 나왔군여?",
          "speaker_id": "2",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024035.1.25",
          "form": "네 전곡 콜라보로 이뤄진거요",
          "original_form": "네 전곡 콜라보로 이뤄진거요",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024035.1.26",
          "form": "lp도 나온다고 해서 예약 걸어뒀어요",
          "original_form": "lp도 나온다고 해서 예약 걸어뒀어요",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100024035.1.27",
          "form": "저도 악뮤 좋아하는데",
          "original_form": "저도 악뮤 좋아하는데",
          "speaker_id": "2",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100024035.1.28",
          "form": "엘피 들을수 있는 장치 가지고 계세요?",
          "original_form": "엘피 들을수 있는 장치 가지고 계세요?",
          "speaker_id": "2",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100024035.1.29",
          "form": "넹 턴테이블 선물로 받아서",
          "original_form": "넹 턴테이블 선물로 받아서",
          "speaker_id": "1",
          "time": "20210830 21:53"
        },
        {
          "id": "MDRW2100024035.1.30",
          "form": "요즘 lp한두장씩 사모으고 있어요",
          "original_form": "요즘 lp한두장씩 사모으고 있어요",
          "speaker_id": "1",
          "time": "20210830 21:53"
        },
        {
          "id": "MDRW2100024035.1.31",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100024035.1.32",
          "form": "엘피로 들으면 옛날감성이라 좋을거 같아요",
          "original_form": "엘피로 들으면 옛날감성이라 좋을거 같아요",
          "speaker_id": "2",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100024035.1.33",
          "form": "특히 중고 엘피판 사면 그 특유의 지지직 오징어굽는 소리가 조터라구요",
          "original_form": "특히 중고 엘피판 사면 그 특유의 지지직 오징어굽는 소리가 조터라구요",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100024035.1.34",
          "form": "그거 좋져",
          "original_form": "그거 좋져",
          "speaker_id": "2",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100024035.1.35",
          "form": "그맛에 엘피 듣는거져",
          "original_form": "그맛에 엘피 듣는거져",
          "speaker_id": "2",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100024035.1.36",
          "form": "네! 근데 간혹잘못사면 한 부분만 무한 튕기기도 해요",
          "original_form": "네! 근데 간혹잘못사면 한 부분만 무한 튕기기도 해요",
          "speaker_id": "1",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100024035.1.37",
          "form": "아 그래요?불량인가요?",
          "original_form": "아 그래요?불량인가요?",
          "speaker_id": "2",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100024035.1.38",
          "form": "중고는 오래됐으니 그러려니 하는데 신품이 불량난경우도 간혹있어요",
          "original_form": "중고는 오래됐으니 그러려니 하는데 신품이 불량난경우도 간혹있어요",
          "speaker_id": "1",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100024035.1.39",
          "form": "엘피는 안사봐서 몰랐네요",
          "original_form": "엘피는 안사봐서 몰랐네요",
          "speaker_id": "2",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100024035.1.40",
          "form": "제친구도 턴테이블 사서 엘피 사모으더라구요",
          "original_form": "제친구도 턴테이블 사서 엘피 사모으더라구요",
          "speaker_id": "2",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100024035.1.41",
          "form": "그게 은근 욕심나더라구요",
          "original_form": "그게 은근 욕심나더라구요",
          "speaker_id": "1",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100024035.1.42",
          "form": "맨날한정판이구",
          "original_form": "맨날한정판이구",
          "speaker_id": "1",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100024035.1.43",
          "form": "새로운 취미가 생기셨군여",
          "original_form": "새로운 취미가 생기셨군여",
          "speaker_id": "2",
          "time": "20210830 22:14"
        },
        {
          "id": "MDRW2100024035.1.44",
          "form": "퇴근길에 배철수 라디오를 듣는데",
          "original_form": "퇴근길에 배철수 라디오를 듣는데",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024035.1.45",
          "form": "엘피 틀어주는거 같더라구여",
          "original_form": "엘피 틀어주는거 같더라구여",
          "speaker_id": "2",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100024035.1.46",
          "form": "라디오는 요즘 잘안들었는데 들어봐야겠어요",
          "original_form": "라디오는 요즘 잘안들었는데 들어봐야겠어요",
          "speaker_id": "1",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100024035.1.47",
          "form": "라디오 들어보세요 전 요즘 라디오 자주들어요",
          "original_form": "라디오 들어보세요 전 요즘 라디오 자주들어요",
