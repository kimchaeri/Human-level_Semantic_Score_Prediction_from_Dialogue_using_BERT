      "utterance": [
        {
          "id": "MDRW2100018903.1.1",
          "form": "ㅋㅋㅋ 그래 그런거라도 타면 재밌잖아!",
          "original_form": "ㅋㅋㅋ 그래 그런거라도 타면 재밌잖아!",
          "speaker_id": "2",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018903.1.2",
          "form": "너네가 그런거만 타게 해주면 갈겤ㅋㅋㅋ",
          "original_form": "너네가 그런거만 타게 해주면 갈겤ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:30"
        },
        {
          "id": "MDRW2100018903.1.3",
          "form": "ㅋㅋㅋ 아니 그거뭐야 패러글라이딩이 더 무섭지않냐고ㅋㅋ",
          "original_form": "ㅋㅋㅋ 아니 그거뭐야 패러글라이딩이 더 무섭지않냐고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018903.1.4",
          "form": "패러글라이딩은 뒤에 사람이 지켜주잖아",
          "original_form": "패러글라이딩은 뒤에 사람이 지켜주잖아",
          "speaker_id": "1",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018903.1.5",
          "form": "나 못믿어?!",
          "original_form": "나 못믿어?!",
          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018903.1.6",
          "form": "내가 옆에서 지켜줄게",
          "original_form": "내가 옆에서 지켜줄게",
          "speaker_id": "2",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018903.1.7",
          "form": "그리고 뜨면 아주 안정적이라구",
          "original_form": "그리고 뜨면 아주 안정적이라구",
          "speaker_id": "1",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018903.1.8",
          "form": "아니야 옆은 안돼",
          "original_form": "아니야 옆은 안돼",
          "speaker_id": "1",
          "time": "20210827 13:34"
        },
        {
          "id": "MDRW2100018903.1.9",
          "form": "왜? 옆에서 손잡아줄게",
          "original_form": "왜? 옆에서 손잡아줄게",
          "speaker_id": "2",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018903.1.10",
          "form": "아냐 손 잡는다고 지켜줄 순 없엌ㅋㅋ",
          "original_form": "아냐 손 잡는다고 지켜줄 순 없엌ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018903.1.11",
          "form": "ㅋㅋㅋ아니 그 아저씨랑 뭐가 달라",
          "original_form": "ㅋㅋㅋ아니 그 아저씨랑 뭐가 달라",
          "speaker_id": "2",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018903.1.12",
          "form": "좌석이 쏙 들어가서 안정감이 느껴져야한다구",
          "original_form": "좌석이 쏙 들어가서 안정감이 느껴져야한다구",
          "speaker_id": "1",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018903.1.13",
          "form": "사람보다 기계가 더 안전하지",
          "original_form": "사람보다 기계가 더 안전하지",
          "speaker_id": "2",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018903.1.14",
          "form": "빠른기계는 안전하지 않아...",
          "original_form": "빠른기계는 안전하지 않아...",
          "speaker_id": "1",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018903.1.15",
          "form": "에이 놀이기구도 좌석 다 있자너",
          "original_form": "에이 놀이기구도 좌석 다 있자너",
          "speaker_id": "2",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018903.1.16",
          "form": "벨트도있고!",
          "original_form": "벨트도있고!",
          "speaker_id": "2",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018903.1.17",
          "form": "그 좌석은 딱딱하고 벨트는 헐겁잖아!",
          "original_form": "그 좌석은 딱딱하고 벨트는 헐겁잖아!",
          "speaker_id": "1",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018903.1.18",
          "form": "내 몸을 꽉 잡아줘야한다구",
          "original_form": "내 몸을 꽉 잡아줘야한다구",
          "speaker_id": "1",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018903.1.19",
          "form": "ㅋㅋㅋ 그래도 한번도 안가본데라 재밌을가같지않아?",
          "original_form": "ㅋㅋㅋ 그래도 한번도 안가본데라 재밌을가같지않아?",
          "speaker_id": "2",
          "time": "20210827 13:39"
        }
      ]
