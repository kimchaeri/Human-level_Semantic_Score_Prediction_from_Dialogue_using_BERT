      "utterance": [
        {
          "id": "MDRW2100015656.1.1",
          "form": "저 교육분야 입니다!",
          "original_form": "저 교육분야 입니다!",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015656.1.2",
          "form": "헐 이과일거같았는데 ㅋㅋ",
          "original_form": "헐 이과일거같았는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015656.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015656.1.4",
          "form": "아 뭐 특목고라 이과 문과 의미 없긴 해요",
          "original_form": "아 뭐 특목고라 이과 문과 의미 없긴 해요",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015656.1.5",
          "form": "저는 그냥 인사관리...ㅋㅋㅋㅋ",
          "original_form": "저는 그냥 인사관리...ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015656.1.6",
          "form": "오 인사과 좀 무서워요 ㅋㅋㅋㅋ",
          "original_form": "오 인사과 좀 무서워요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015656.1.7",
          "form": "아 그래요? 대학교도요?",
          "original_form": "아 그래요? 대학교도요?",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015656.1.8",
          "form": "네 대학교도 그렇죠",
          "original_form": "네 대학교도 그렇죠",
          "speaker_id": "2",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015656.1.9",
          "form": "원래 아니었는데 소규모라",
          "original_form": "원래 아니었는데 소규모라",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015656.1.10",
          "form": "인사도 맡겨버리더라구요 저한테 ㅋㅋ",
          "original_form": "인사도 맡겨버리더라구요 저한테 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015656.1.11",
          "form": "이번달부터 ㅋㅋ",
          "original_form": "이번달부터 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015656.1.12",
          "form": "그럼 면접관도 하시고 직접 채용하셔요?",
          "original_form": "그럼 면접관도 하시고 직접 채용하셔요?",
          "speaker_id": "2",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015656.1.13",
          "form": "아니요 그런건 안해요 그건부장님이 ㅋㅋㅋ",
          "original_form": "아니요 그런건 안해요 그건부장님이 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015656.1.14",
          "form": "그럼 어떤거 하세요?",
          "original_form": "그럼 어떤거 하세요?",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015656.1.15",
          "form": "지원 서류 정리랑 1차 거르기 이런건가..",
          "original_form": "지원 서류 정리랑 1차 거르기 이런건가..",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015656.1.16",
          "form": "새로운 신입들 교육하고 관리하고 ㅋㅋㅋ",
          "original_form": "새로운 신입들 교육하고 관리하고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015656.1.17",
          "form": "아~ 그런 거였군요 !",
          "original_form": "아~ 그런 거였군요 !",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015656.1.18",
          "form": "그거 보통 일이 아닌데 대단한 거 하고 계시네요 ㅎㅎ",
          "original_form": "그거 보통 일이 아닌데 대단한 거 하고 계시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015656.1.19",
          "form": "네 ㅋㅋ 월급 관리..도 ㅎ",
          "original_form": "네 ㅋㅋ 월급 관리..도 ㅎ",
          "speaker_id": "1",
          "time": "20210826 00:04"
        },
        {
          "id": "MDRW2100015656.1.20",
          "form": "이제부터 주위 월급 다알아요 이제 ㅋㅋㅋㅋㅋ",
          "original_form": "이제부터 주위 월급 다알아요 이제 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015656.1.21",
          "form": "ㅋㅋㅋ엑셀 잘하시겠어요",
          "original_form": "ㅋㅋㅋ엑셀 잘하시겠어요",
          "speaker_id": "2",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015656.1.22",
          "form": "능력 비슷한데 월급 차이 많이 나기도 하고 그런가요?",
          "original_form": "능력 비슷한데 월급 차이 많이 나기도 하고 그런가요?",
          "speaker_id": "2",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015656.1.23",
          "form": "음 그냥 여긴 비슷한거같아요",
          "original_form": "음 그냥 여긴 비슷한거같아요",
          "speaker_id": "1",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015656.1.24",
          "form": "부장님이 안그래도",
          "original_form": "부장님이 안그래도",
          "speaker_id": "1",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015656.1.25",
          "form": "함부로 떠들고다니지말라고",
          "original_form": "함부로 떠들고다니지말라고",
          "speaker_id": "1",
          "time": "20210826 00:05"
        },
        {
          "id": "MDRW2100015656.1.26",
          "form": "제가 말할데도 없다고 ㅋㅋㅋㅋ",
          "original_form": "제가 말할데도 없다고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015656.1.27",
          "form": "ㅋㅋㅋㅋ 그러다 직원들끼리 싸움나고 사표써요 ㅜ",
          "original_form": "ㅋㅋㅋㅋ 그러다 직원들끼리 싸움나고 사표써요 ㅜ",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015656.1.28",
          "form": "그쵸 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015656.1.29",
          "form": "왜 쟤가 나보다 더 많이 받냐면서 삿대질하고 ㅜㅜ",
          "original_form": "왜 쟤가 나보다 더 많이 받냐면서 삿대질하고 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015656.1.30",
          "form": "그니까요 모든 싸움의 원인 ㅎ",
          "original_form": "그니까요 모든 싸움의 원인 ㅎ",
          "speaker_id": "1",
          "time": "20210826 00:06"
        },
        {
          "id": "MDRW2100015656.1.31",
          "form": "직장 스트레스는 있으신가요??",
          "original_form": "직장 스트레스는 있으신가요??",
          "speaker_id": "1",
          "time": "20210826 00:06"
        }
      ]
