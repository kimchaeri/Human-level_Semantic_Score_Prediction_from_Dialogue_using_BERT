      "utterance": [
        {
          "id": "MDRW2100004415.1.1",
          "form": "학생이신가요",
          "original_form": "학생이신가요",
          "speaker_id": "2",
          "time": "20210821 16:23"
        },
        {
          "id": "MDRW2100004415.1.2",
          "form": "네 학생이에요!",
          "original_form": "네 학생이에요!",
          "speaker_id": "1",
          "time": "20210821 16:23"
        },
        {
          "id": "MDRW2100004415.1.3",
          "form": "지금 방학이네요",
          "original_form": "지금 방학이네요",
          "speaker_id": "1",
          "time": "20210821 16:25"
        },
        {
          "id": "MDRW2100004415.1.4",
          "form": "우와 부러워요",
          "original_form": "우와 부러워요",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004415.1.5",
          "form": "요즘에 등교하시나요",
          "original_form": "요즘에 등교하시나요",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004415.1.6",
          "form": "코로나때문에 원격수업이 대부분이라고 들었었는데",
          "original_form": "코로나때문에 원격수업이 대부분이라고 들었었는데",
          "speaker_id": "2",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004415.1.7",
          "form": "맞아요 저도 원격수업으로 수업해요",
          "original_form": "맞아요 저도 원격수업으로 수업해요",
          "speaker_id": "1",
          "time": "20210821 16:27"
        },
        {
          "id": "MDRW2100004415.1.8",
          "form": "우와 학교안가면 좋을 거 같기도 하고 심심할 거 같기도해요 혹시 고등학생이세요?",
          "original_form": "우와 학교안가면 좋을 거 같기도 하고 심심할 거 같기도해요 혹시 고등학생이세요?",
          "speaker_id": "2",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004415.1.9",
          "form": "아뇨 대학생입니다!",
          "original_form": "아뇨 대학생입니다!",
          "speaker_id": "1",
          "time": "20210821 16:29"
        },
        {
          "id": "MDRW2100004415.1.10",
          "form": "와 그럼 아쉬운 게 많으실 거 같아요ㅠㅠ",
          "original_form": "와 그럼 아쉬운 게 많으실 거 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:30"
        },
        {
          "id": "MDRW2100004415.1.11",
          "form": "맞아요ㅠㅠ 축제도 온라인으로 하고.. 대학느낌이 안나네요",
          "original_form": "맞아요ㅠㅠ 축제도 온라인으로 하고.. 대학느낌이 안나네요",
          "speaker_id": "1",
          "time": "20210821 16:31"
        },
        {
          "id": "MDRW2100004415.1.12",
          "form": "신기하네요 축제를 온라인으로ㅠㅠㅠㅠ",
          "original_form": "신기하네요 축제를 온라인으로ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004415.1.13",
          "form": "캠퍼스 돌아다니실 일도 별로 없겠어요",
          "original_form": "캠퍼스 돌아다니실 일도 별로 없겠어요",
          "speaker_id": "2",
          "time": "20210821 16:32"
        },
        {
          "id": "MDRW2100004415.1.14",
          "form": "근데 한편으론 직접 안가도 돼서 편할 거 같기도해요",
          "original_form": "근데 한편으론 직접 안가도 돼서 편할 거 같기도해요",
          "speaker_id": "2",
          "time": "20210821 16:33"
        },
        {
          "id": "MDRW2100004415.1.15",
          "form": "맞아요! 통학시간이 긴 편이라 이건 좋더라구요",
          "original_form": "맞아요! 통학시간이 긴 편이라 이건 좋더라구요",
          "speaker_id": "1",
          "time": "20210821 16:35"
        },
        {
          "id": "MDRW2100004415.1.16",
          "form": "하지만 등록금이 조금 아까워지는 느낌은 있네요",
          "original_form": "하지만 등록금이 조금 아까워지는 느낌은 있네요",
          "speaker_id": "1",
          "time": "20210821 16:35"
        },
        {
          "id": "MDRW2100004415.1.17",
          "form": "통학시간이 길면 괴롭죠 등록금은 진짜 아까울 거 같아요",
          "original_form": "통학시간이 길면 괴롭죠 등록금은 진짜 아까울 거 같아요",
          "speaker_id": "2",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004415.1.18",
          "form": "한학기 거의 500만원인데ㅠㅠ",
          "original_form": "한학기 거의 500만원인데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004415.1.19",
          "form": "맞아요 세상에 이렇게 비싼 인강이 어디있어요",
          "original_form": "맞아요 세상에 이렇게 비싼 인강이 어디있어요",
          "speaker_id": "1",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004415.1.20",
          "form": "와진짜 그렇게 생각하니까 너무 비싸요",
          "original_form": "와진짜 그렇게 생각하니까 너무 비싸요",
          "speaker_id": "2",
          "time": "20210821 16:45"
        },
        {
          "id": "MDRW2100004415.1.21",
          "form": "대학교 거의 날강도아닌가요ㅜㅜㅜㅜㅋㅋㅋㅋ",
          "original_form": "대학교 거의 날강도아닌가요ㅜㅜㅜㅜㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:45"
        },
        {
          "id": "MDRW2100004415.1.22",
          "form": "그러니까요! 그래서 뭐 반환하니 뭐니 이러던데 좀 지나니까 다 모른척.. 이게 뭐하는 짓인지",
          "original_form": "그러니까요! 그래서 뭐 반환하니 뭐니 이러던데 좀 지나니까 다 모른척.. 이게 뭐하는 짓인지",
          "speaker_id": "1",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004415.1.23",
          "form": "안그래도 비싼 등록금인데 인강이라뇨",
          "original_form": "안그래도 비싼 등록금인데 인강이라뇨",
          "speaker_id": "2",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004415.1.24",
          "form": "반값이라도 아까울 거 같아요",
          "original_form": "반값이라도 아까울 거 같아요",
          "speaker_id": "2",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004415.1.25",
          "form": "학교에서 지원해주는 거도 없을텐데요ㅠㅠ",
          "original_form": "학교에서 지원해주는 거도 없을텐데요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004415.1.26",
          "form": "진짜 맞아요..",
          "original_form": "진짜 맞아요..",
