      "utterance": [
        {
          "id": "MDRW2100044470.1.1",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210907 22:49"
        },
        {
          "id": "MDRW2100044470.1.2",
          "form": "지금 머리가 너무 길어서요ㅠ",
          "original_form": "지금 머리가 너무 길어서요ㅠ",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044470.1.3",
          "form": "지금 머리가 어디까지 오세요?",
          "original_form": "지금 머리가 어디까지 오세요?",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044470.1.4",
          "form": "허리까지요",
          "original_form": "허리까지요",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044470.1.5",
          "form": "헉 엄청 기시네요!",
          "original_form": "헉 엄청 기시네요!",
          "speaker_id": "2",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044470.1.6",
          "form": "네 그래서 머리감기가 귀찮아요",
          "original_form": "네 그래서 머리감기가 귀찮아요",
          "speaker_id": "1",
          "time": "20210907 22:50"
        },
        {
          "id": "MDRW2100044470.1.7",
          "form": "머리감고 말리는것도 일이겠어요",
          "original_form": "머리감고 말리는것도 일이겠어요",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044470.1.8",
          "form": "네 사실 감는거보다 말리는게ㅠㅠ",
          "original_form": "네 사실 감는거보다 말리는게ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044470.1.9",
          "form": "머리숱도 많으세요?",
          "original_form": "머리숱도 많으세요?",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044470.1.10",
          "form": "네 엄청요",
          "original_form": "네 엄청요",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044470.1.11",
          "form": "그래도 복받으신거에요 ㅎㅎ",
          "original_form": "그래도 복받으신거에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044470.1.12",
          "form": "없는거보단 낫겠지만 머리말릴때 너무 오래걸려요",
          "original_form": "없는거보단 낫겠지만 머리말릴때 너무 오래걸려요",
          "speaker_id": "1",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044470.1.13",
          "form": "조금만 자르시면 안되나요?",
          "original_form": "조금만 자르시면 안되나요?",
          "speaker_id": "2",
          "time": "20210907 22:51"
        },
        {
          "id": "MDRW2100044470.1.14",
          "form": "단발하고 파마할까봐요",
          "original_form": "단발하고 파마할까봐요",
          "speaker_id": "1",
          "time": "20210907 22:51"
