      "utterance": [
        {
          "id": "MDRW2100014084.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014084.1.2",
          "form": "혹시 어떤 운동하시나요?",
          "original_form": "혹시 어떤 운동하시나요?",
          "speaker_id": "1",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100014084.1.3",
          "form": "안녕하세요~~원래는 헬스장다니는걸좋아했어요",
          "original_form": "안녕하세요~~원래는 헬스장다니는걸좋아했어요",
          "speaker_id": "2",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014084.1.4",
          "form": "요즘은 헬스장 가기가 힘들죠?",
          "original_form": "요즘은 헬스장 가기가 힘들죠?",
          "speaker_id": "1",
          "time": "20210825 17:29"
        },
        {
          "id": "MDRW2100014084.1.5",
          "form": "맞아요 ㅠㅠ요즘은 그래서 유튜브보고 집에서 운동해요",
          "original_form": "맞아요 ㅠㅠ요즘은 그래서 유튜브보고 집에서 운동해요",
          "speaker_id": "2",
          "time": "20210825 17:30"
        },
        {
          "id": "MDRW2100014084.1.6",
          "form": "맞아요.  홈트가 유행이죠",
          "original_form": "맞아요.  홈트가 유행이죠",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100014084.1.7",
          "form": "운동좋아하시나요?",
          "original_form": "운동좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014084.1.8",
          "form": "저도 홈트한다구 실내자전거랑 철봉샀어요",
          "original_form": "저도 홈트한다구 실내자전거랑 철봉샀어요",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014084.1.9",
          "form": "트램폴린도샀어요",
          "original_form": "트램폴린도샀어요",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014084.1.10",
          "form": "우와!트램폴린!! 어때요?",
          "original_form": "우와!트램폴린!! 어때요?",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014084.1.11",
          "form": "수영좋아해요!",
          "original_form": "수영좋아해요!",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014084.1.12",
          "form": "와 만능 스포츠맨이시네요",
          "original_form": "와 만능 스포츠맨이시네요",
          "speaker_id": "2",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014084.1.13",
          "form": "티비앞에두고 오며가며 뛰어요",
          "original_form": "티비앞에두고 오며가며 뛰어요",
          "speaker_id": "1",
          "time": "20210825 17:32"
        },
        {
          "id": "MDRW2100014084.1.14",
          "form": "주로옆구리운동으로 몸통을 비틀어가면서뛰어요",
          "original_form": "주로옆구리운동으로 몸통을 비틀어가면서뛰어요",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014084.1.15",
          "form": "우와 ㅎㅎㅎㅎ재밌을거같아요  저도 헬스장에서 한번 타봤는데 재밌더라구요",
          "original_form": "우와 ㅎㅎㅎㅎ재밌을거같아요  저도 헬스장에서 한번 타봤는데 재밌더라구요",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014084.1.16",
          "form": "운동신경 장난아니시넹ㄴ",
          "original_form": "운동신경 장난아니시넹ㄴ",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014084.1.17",
          "form": "움직이는거 좋아해요",
          "original_form": "움직이는거 좋아해요",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100014084.1.18",
          "form": "활동량이 적어지구 나이가 드니. . .",
          "original_form": "활동량이 적어지구 나이가 드니. . .",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014084.1.19",
          "form": "운동을 해야겠다는  생각이 들어요",
          "original_form": "운동을 해야겠다는  생각이 들어요",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100014084.1.20",
          "form": "우와 그래도 이렇게 하시는게 진짜대단하시네요  저도 더 자극되요",
