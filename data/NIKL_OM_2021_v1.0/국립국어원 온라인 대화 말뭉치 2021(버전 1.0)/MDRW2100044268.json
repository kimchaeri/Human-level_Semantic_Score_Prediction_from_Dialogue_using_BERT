      "utterance": [
        {
          "id": "MDRW2100044268.1.1",
          "form": "오늘 축구해서 그런지",
          "original_form": "오늘 축구해서 그런지",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044268.1.2",
          "form": "배달음식중에 치킨이 유독 많았오~",
          "original_form": "배달음식중에 치킨이 유독 많았오~",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044268.1.3",
          "form": "오오 확실히 그렇구만!",
          "original_form": "오오 확실히 그렇구만!",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044268.1.4",
          "form": "웅웅~~ 치킨냄새 넘나 향긋했어~",
          "original_form": "웅웅~~ 치킨냄새 넘나 향긋했어~",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044268.1.5",
          "form": "배민커넥트 넘 잼나 ㅋ",
          "original_form": "배민커넥트 넘 잼나 ㅋ",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044268.1.6",
          "form": "크 향긋한 치킨냄새 넘좋아",
          "original_form": "크 향긋한 치킨냄새 넘좋아",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044268.1.7",
          "form": "ㅎㅎㅎ쏠쏠한 재미~",
          "original_form": "ㅎㅎㅎ쏠쏠한 재미~",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044268.1.8",
          "form": "진짜 치킨들 많이 시켜먹는거 같아~",
          "original_form": "진짜 치킨들 많이 시켜먹는거 같아~",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044268.1.9",
          "form": "웅웅 ㅋㅋ 보람차고",
          "original_form": "웅웅 ㅋㅋ 보람차고",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044268.1.10",
          "form": "ㅋㅋㅋ 치킨이 젤 자주 땡기는듯",
          "original_form": "ㅋㅋㅋ 치킨이 젤 자주 땡기는듯",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044268.1.11",
          "form": "응웅~ 어딜가나 치킨집이 제일 많고~",
          "original_form": "응웅~ 어딜가나 치킨집이 제일 많고~",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044268.1.12",
          "form": "마자마자 ㅋㅋ",
          "original_form": "마자마자 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044268.1.13",
          "form": "치킨박스 가방에 들어갔엉?",
          "original_form": "치킨박스 가방에 들어갔엉?",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044268.1.14",
          "form": "아!! 징짜!!",
          "original_form": "아!! 징짜!!",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044268.1.15",
          "form": "3개중에 2개는 처갓집이라 잘 들어갔믄데",
          "original_form": "3개중에 2개는 처갓집이라 잘 들어갔믄데",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044268.1.16",
          "form": "헉!",
          "original_form": "헉!",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044268.1.17",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044268.1.18",
          "form": "1개 네네치킨은",
          "original_form": "1개 네네치킨은",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044268.1.19",
          "form": "박스가 피자박스만해가지고",
          "original_form": "박스가 피자박스만해가지고",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044268.1.20",
          "form": "들고 뜀..ㅋㅋㅋㅋ",
          "original_form": "들고 뜀..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044268.1.21",
          "form": "큰 박스는 안들어갈것같다고 생각하긴했어",
          "original_form": "큰 박스는 안들어갈것같다고 생각하긴했어",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044268.1.22",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044268.1.23",
          "form": "그래도 배달지가 5분거리여서",
          "original_form": "그래도 배달지가 5분거리여서",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044268.1.24",
          "form": "식기전에 도착했어서 다행니였어~~",
          "original_form": "식기전에 도착했어서 다행니였어~~",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044268.1.25",
          "form": "이런ㅋㅋㅋ 다행다행!",
          "original_form": "이런ㅋㅋㅋ 다행다행!",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044268.1.26",
          "form": "마자 그 피자박스처럼 넓적한 치킨박스 ㅠㅠ조심해야겠군",
          "original_form": "마자 그 피자박스처럼 넓적한 치킨박스 ㅠㅠ조심해야겠군",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044268.1.27",
          "form": "담부터 네네는 패쓰~",
          "original_form": "담부터 네네는 패쓰~",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044268.1.28",
          "form": "웅우~~//",
          "original_form": "웅우~~//",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044268.1.29",
          "form": "나두그래야겠당",
          "original_form": "나두그래야겠당",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044268.1.30",
          "form": "아니면 아주 가까운거면 고고 ㅋㅋ",
          "original_form": "아니면 아주 가까운거면 고고 ㅋㅋ",
          "speaker_id": "1",
