      "utterance": [
        {
          "id": "MDRW2100007071.1.1",
          "form": "안녕하세요 스포츠 좋아하시는거 있으세요?",
          "original_form": "안녕하세요 스포츠 좋아하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007071.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210822 10:44"
        },
        {
          "id": "MDRW2100007071.1.3",
          "form": "잘 모르는데 좋아하시나요?",
          "original_form": "잘 모르는데 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 10:44"
        },
        {
          "id": "MDRW2100007071.1.4",
          "form": "얼마전에 올림픽할때 배구 경기본게 다네요 ㅎ",
          "original_form": "얼마전에 올림픽할때 배구 경기본게 다네요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 10:45"
        },
        {
          "id": "MDRW2100007071.1.5",
          "form": "저도 잘 몰라요 ㅋㅋㅋ 배구경기 트위터로 접하고 티비로 봤네요 ㅎ",
          "original_form": "저도 잘 몰라요 ㅋㅋㅋ 배구경기 트위터로 접하고 티비로 봤네요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 10:47"
        },
        {
          "id": "MDRW2100007071.1.6",
          "form": "아 경기 보셨어요?",
          "original_form": "아 경기 보셨어요?",
          "speaker_id": "2",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007071.1.7",
          "form": "엄청 멋지지 않았나요 ㅠㅠ",
          "original_form": "엄청 멋지지 않았나요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007071.1.8",
          "form": "저 심자 터지는 줄 알았잖아요",
          "original_form": "저 심자 터지는 줄 알았잖아요",
          "speaker_id": "2",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007071.1.9",
          "form": "맞아요 특히 터키전할때 너무 심장떨렸어요ㅠ 경기결과 알고봤는데도",
          "original_form": "맞아요 특히 터키전할때 너무 심장떨렸어요ㅠ 경기결과 알고봤는데도",
          "speaker_id": "1",
          "time": "20210822 10:48"
        },
        {
          "id": "MDRW2100007071.1.10",
          "form": "원래 이런거 잘 안챙겨보는 사람이라",
          "original_form": "원래 이런거 잘 안챙겨보는 사람이라",
          "speaker_id": "2",
          "time": "20210822 10:49"
        },
        {
          "id": "MDRW2100007071.1.11",
          "form": "대충 보고 넘기려했는데",
          "original_form": "대충 보고 넘기려했는데",
          "speaker_id": "2",
          "time": "20210822 10:49"
        },
        {
          "id": "MDRW2100007071.1.12",
          "form": "하.. 일이 손에 안잡히더라고요 ㅋㅋㅋㅋ",
          "original_form": "하.. 일이 손에 안잡히더라고요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:49"
        },
        {
          "id": "MDRW2100007071.1.13",
          "form": "결국 끝까지 다봤는데 이겼을때 전율이 막 ㅠㅠ",
          "original_form": "결국 끝까지 다봤는데 이겼을때 전율이 막 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 10:50"
        },
        {
          "id": "MDRW2100007071.1.14",
          "form": "이제는 배구 챙겨볼것같아요",
          "original_form": "이제는 배구 챙겨볼것같아요",
          "speaker_id": "2",
          "time": "20210822 10:50"
        },
        {
          "id": "MDRW2100007071.1.15",
          "form": "네 배구보니까 배구 막 배우고 싶고 경기도 보러고가고싶더라고요 ㅠ",
          "original_form": "네 배구보니까 배구 막 배우고 싶고 경기도 보러고가고싶더라고요 ㅠ",
          "speaker_id": "1",
          "time": "20210822 10:50"
        },
        {
          "id": "MDRW2100007071.1.16",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007071.1.17",
          "form": "배구 저도 배우고싶은데",
          "original_form": "배구 저도 배우고싶은데",
          "speaker_id": "2",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007071.1.18",
          "form": "이제는 나이가 차서 기회가 없네요 ㅎㅎ",
          "original_form": "이제는 나이가 차서 기회가 없네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007071.1.19",
          "form": "학창시절에는 참 재밌게 했는데",
          "original_form": "학창시절에는 참 재밌게 했는데",
          "speaker_id": "2",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007071.1.20",
          "form": "아유 늦은 나이는 없죠 ㅎㅎ",
          "original_form": "아유 늦은 나이는 없죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007071.1.21",
          "form": "저는 배구할만한 사람모으기가 안돼서 못하고 있거든요",
          "original_form": "저는 배구할만한 사람모으기가 안돼서 못하고 있거든요",
          "speaker_id": "1",
          "time": "20210822 10:53"
        },
        {
          "id": "MDRW2100007071.1.22",
          "form": "그것도 그렇네요 ㅎㅎ",
          "original_form": "그것도 그렇네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 10:54"
        },
        {
          "id": "MDRW2100007071.1.23",
          "form": "시간날때 한번 원데이 클래스나 그런거라도 해봐야겠네요",
          "original_form": "시간날때 한번 원데이 클래스나 그런거라도 해봐야겠네요",
          "speaker_id": "2",
          "time": "20210822 10:55"
        },
        {
          "id": "MDRW2100007071.1.24",
          "form": "그쵸 배구는 혼자 못하니까",
          "original_form": "그쵸 배구는 혼자 못하니까",
          "speaker_id": "2",
          "time": "20210822 10:55"
        },
        {
          "id": "MDRW2100007071.1.25",
          "form": "동아리같은건 어려울까요?",
          "original_form": "동아리같은건 어려울까요?",
          "speaker_id": "2",
          "time": "20210822 10:55"
        },
        {
          "id": "MDRW2100007071.1.26",
          "form": "운동 동아리 하면 멋질 것 같긴 하네요 !",
          "original_form": "운동 동아리 하면 멋질 것 같긴 하네요 !",
          "speaker_id": "1",
          "time": "20210822 10:56"
        },
        {
          "id": "MDRW2100007071.1.27",
          "form": "지금당장은 티비로 보는 걸로 만족하려고요 ㅎ",
          "original_form": "지금당장은 티비로 보는 걸로 만족하려고요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 10:56"
        },
        {
          "id": "MDRW2100007071.1.28",
          "form": "코시국이라 힘든일이 너무 많네요 ㅎㅎ",
          "original_form": "코시국이라 힘든일이 너무 많네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 11:01"
        },
        {
          "id": "MDRW2100007071.1.29",
          "form": "스포츠경기는 가서보면 더 멋진데",
          "original_form": "스포츠경기는 가서보면 더 멋진데",
          "speaker_id": "2",
          "time": "20210822 11:01"
        },
        {
          "id": "MDRW2100007071.1.30",
          "form": "제가 중학교때 딱 한번 농구경기 관람해본적 있거든요",
          "original_form": "제가 중학교때 딱 한번 농구경기 관람해본적 있거든요",
          "speaker_id": "2",
          "time": "20210822 11:02"
        },
        {
          "id": "MDRW2100007071.1.31",
          "form": "맞아요 저도 축구 한번 직접 가서 봤는데 티비랑은 다르더라고요",
          "original_form": "맞아요 저도 축구 한번 직접 가서 봤는데 티비랑은 다르더라고요",
          "speaker_id": "1",
          "time": "20210822 11:02"
        },
        {
          "id": "MDRW2100007071.1.32",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210822 11:02"
        },
        {
          "id": "MDRW2100007071.1.33",
          "form": "박진감이 장난이 아니더라고요",
          "original_form": "박진감이 장난이 아니더라고요",
          "speaker_id": "2",
          "time": "20210822 11:02"
        },
        {
          "id": "MDRW2100007071.1.34",
          "form": "룰을 진짜 전혀몰랐는데 ㅋㅋㅋ",
          "original_form": "룰을 진짜 전혀몰랐는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:02"
        },
        {
          "id": "MDRW2100007071.1.35",
          "form": "룰 몰라도 점수판만 봐도 떨려요 ㅋㅋㅋㅋ",
          "original_form": "룰 몰라도 점수판만 봐도 떨려요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 11:03"
        }
      ]
