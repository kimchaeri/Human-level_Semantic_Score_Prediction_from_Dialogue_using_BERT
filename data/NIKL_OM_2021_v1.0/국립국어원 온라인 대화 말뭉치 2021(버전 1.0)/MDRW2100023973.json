      "utterance": [
        {
          "id": "MDRW2100023973.1.1",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210830 20:50"
        },
        {
          "id": "MDRW2100023973.1.2",
          "form": "유튭이 안질리더라구요",
          "original_form": "유튭이 안질리더라구요",
          "speaker_id": "2",
          "time": "20210830 20:50"
        },
        {
          "id": "MDRW2100023973.1.3",
          "form": "유툽엔 넘나많은 콘텐츠가 있어요",
          "original_form": "유툽엔 넘나많은 콘텐츠가 있어요",
          "speaker_id": "1",
          "time": "20210830 20:50"
        },
        {
          "id": "MDRW2100023973.1.4",
          "form": "저는 사랑의불시착도 유툽클립으로 봤어요ㅎㅎ",
          "original_form": "저는 사랑의불시착도 유툽클립으로 봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100023973.1.5",
          "form": "아 전 그거 안봤어요ㅜ",
          "original_form": "아 전 그거 안봤어요ㅜ",
          "speaker_id": "2",
          "time": "20210830 20:51"
        },
        {
          "id": "MDRW2100023973.1.6",
          "form": "영화클립도 시간덜들이고 영화도 보고 꿀이에요",
          "original_form": "영화클립도 시간덜들이고 영화도 보고 꿀이에요",
          "speaker_id": "1",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023973.1.7",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "2",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023973.1.8",
          "form": "유튭알고리즘 좋지요ㅋ",
          "original_form": "유튭알고리즘 좋지요ㅋ",
          "speaker_id": "2",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023973.1.9",
          "form": "자동으로 땋",
          "original_form": "자동으로 땋",
          "speaker_id": "1",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100023973.1.10",
          "form": "재밌는거 띄워줘요 ㅎ",
          "original_form": "재밌는거 띄워줘요 ㅎ",
          "speaker_id": "1",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100023973.1.11",
          "form": "매탈남 보고 왔어요",
          "original_form": "매탈남 보고 왔어요",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100023973.1.12",
          "form": "저는 이제 큼터켰는데",
          "original_form": "저는 이제 큼터켰는데",
          "speaker_id": "1",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100023973.1.13",
          "form": "저는 멸치에 고추장찍어먹고있었어요",
          "original_form": "저는 멸치에 고추장찍어먹고있었어요",
          "speaker_id": "1",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100023973.1.14",
          "form": "고양이세요?",
          "original_form": "고양이세요?",
          "speaker_id": "2",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100023973.1.15",
          "form": "고양이가없거든요",
          "original_form": "고양이가없거든요",
          "speaker_id": "1",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100023973.1.16",
          "form": "음식 취향은 아저씨네요",
          "original_form": "음식 취향은 아저씨네요",
          "speaker_id": "2",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100023973.1.17",
          "form": "고양이도 주면 머거등요",
          "original_form": "고양이도 주면 머거등요",
          "speaker_id": "1",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100023973.1.18",
          "form": "헐!",
          "original_form": "헐!",
          "speaker_id": "2",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100023973.1.19",
          "form": "고양이를 먹는다고요?",
          "original_form": "고양이를 먹는다고요?",
          "speaker_id": "2",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100023973.1.20",
          "form": "아뇨 멸치를 고양이가요",
          "original_form": "아뇨 멸치를 고양이가요",
          "speaker_id": "1",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100023973.1.21",
          "form": "ㅡ.ㅡ",
          "original_form": "ㅡ.ㅡ",
          "speaker_id": "1",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100023973.1.22",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
