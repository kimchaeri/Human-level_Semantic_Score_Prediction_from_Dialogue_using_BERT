      "utterance": [
        {
          "id": "MDRW2100005227.1.1",
          "form": "종교있어요?",
          "original_form": "종교있어요?",
          "speaker_id": "1",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005227.1.2",
          "form": "아뇨 무교에요",
          "original_form": "아뇨 무교에요",
          "speaker_id": "2",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005227.1.3",
          "form": "믿는 종교가 있나요?",
          "original_form": "믿는 종교가 있나요?",
          "speaker_id": "2",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005227.1.4",
          "form": "저도 없어서",
          "original_form": "저도 없어서",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005227.1.5",
          "form": "종교는 그냥 역사 배울때나",
          "original_form": "종교는 그냥 역사 배울때나",
          "speaker_id": "2",
          "time": "20210821 21:26"
        },
        {
          "id": "MDRW2100005227.1.6",
          "form": "조금 듣게 되는 것 말고는",
          "original_form": "조금 듣게 되는 것 말고는",
          "speaker_id": "2",
          "time": "20210821 21:26"
        },
        {
          "id": "MDRW2100005227.1.7",
          "form": "잘 모르겠네요",
          "original_form": "잘 모르겠네요",
          "speaker_id": "2",
          "time": "20210821 21:26"
        },
        {
          "id": "MDRW2100005227.1.8",
          "form": "부모님은 불교요",
          "original_form": "부모님은 불교요",
          "speaker_id": "1",
          "time": "20210821 21:28"
        },
        {
          "id": "MDRW2100005227.1.9",
          "form": "부모님 세대는 불교가 많긴 하죠",
          "original_form": "부모님 세대는 불교가 많긴 하죠",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005227.1.10",
          "form": "절에 가본 적 있나요?",
          "original_form": "절에 가본 적 있나요?",
          "speaker_id": "2",
          "time": "20210821 21:35"
        },
        {
          "id": "MDRW2100005227.1.11",
          "form": "네 따라 가서 공양했어요",
          "original_form": "네 따라 가서 공양했어요",
          "speaker_id": "1",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005227.1.12",
          "form": "저는 어렸을 때 할머니따라 가본 것 말고는 없네요",
          "original_form": "저는 어렸을 때 할머니따라 가본 것 말고는 없네요",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005227.1.13",
          "form": "주변에 다른 종교 믿는 사람 있나요?",
          "original_form": "주변에 다른 종교 믿는 사람 있나요?",
          "speaker_id": "2",
          "time": "20210821 21:56"
        },
        {
          "id": "MDRW2100005227.1.14",
          "form": "없어요",
          "original_form": "없어요",
          "speaker_id": "1",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005227.1.15",
          "form": "무교 주변엔 대부분 무교가 많긴 하죠",
          "original_form": "무교 주변엔 대부분 무교가 많긴 하죠",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005227.1.16",
          "form": "네 이제 교회이미지가",
          "original_form": "네 이제 교회이미지가",
          "speaker_id": "1",
          "time": "20210821 22:04"
        },
        {
          "id": "MDRW2100005227.1.17",
          "form": "아 신천지도 있고 여러모로 난리였죠",
          "original_form": "아 신천지도 있고 여러모로 난리였죠",
          "speaker_id": "2",
          "time": "20210821 22:08"
        },
        {
          "id": "MDRW2100005227.1.18",
