      "utterance": [
        {
          "id": "MDRW2100022964.1.1",
          "form": "으헹헹 요즘 난 쇼핑홀릭이지",
          "original_form": "으헹헹 요즘 난 쇼핑홀릭이지",
          "speaker_id": "1",
          "time": "20210829 19:41"
        },
        {
          "id": "MDRW2100022964.1.2",
          "form": "넌 주로 뭘 사니",
          "original_form": "넌 주로 뭘 사니",
          "speaker_id": "2",
          "time": "20210829 19:41"
        },
        {
          "id": "MDRW2100022964.1.3",
          "form": "화장품 먹을거?",
          "original_form": "화장품 먹을거?",
          "speaker_id": "1",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100022964.1.4",
          "form": "옷? 가방?",
          "original_form": "옷? 가방?",
          "speaker_id": "1",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100022964.1.5",
          "form": "나는 책같은 걸 사",
          "original_form": "나는 책같은 걸 사",
          "speaker_id": "2",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100022964.1.6",
          "form": "오 난 책은 안사 ㅋㅋㅋㅋㅋ 난 보통 소모품을 많이 사는 느낌인데",
          "original_form": "오 난 책은 안사 ㅋㅋㅋㅋㅋ 난 보통 소모품을 많이 사는 느낌인데",
          "speaker_id": "1",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100022964.1.7",
          "form": "먹을거나...소모품같은거...",
          "original_form": "먹을거나...소모품같은거...",
          "speaker_id": "1",
          "time": "20210829 19:42"
        },
        {
          "id": "MDRW2100022964.1.8",
          "form": "지난 주에는 먹을걸 넘 많이 사서 냉장고 터져나감",
          "original_form": "지난 주에는 먹을걸 넘 많이 사서 냉장고 터져나감",
          "speaker_id": "1",
          "time": "20210829 19:43"
        },
        {
          "id": "MDRW2100022964.1.9",
          "form": "어떤 걸 샀는데?",
          "original_form": "어떤 걸 샀는데?",
          "speaker_id": "2",
          "time": "20210829 19:43"
        },
        {
          "id": "MDRW2100022964.1.10",
          "form": "음..지금 냉장고에 있는게",
          "original_form": "음..지금 냉장고에 있는게",
          "speaker_id": "1",
          "time": "20210829 19:43"
        },
        {
          "id": "MDRW2100022964.1.11",
          "form": "치즈 버팔로스틱 주먹밥 우유...",
          "original_form": "치즈 버팔로스틱 주먹밥 우유...",
          "speaker_id": "1",
          "time": "20210829 19:43"
        },
        {
          "id": "MDRW2100022964.1.12",
          "form": "스파게티소스도 샀다",
          "original_form": "스파게티소스도 샀다",
          "speaker_id": "1",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100022964.1.13",
          "form": "쇼핑한 것 중에 먹어 본 거 있어?",
          "original_form": "쇼핑한 것 중에 먹어 본 거 있어?",
          "speaker_id": "2",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100022964.1.14",
          "form": "ㅇㅇ버팔로스틱이랑 주먹밥 우유 먹어봤어",
          "original_form": "ㅇㅇ버팔로스틱이랑 주먹밥 우유 먹어봤어",
          "speaker_id": "1",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100022964.1.15",
          "form": "버팔로스틱은 별로였고 주먹밥은 맛있음",
          "original_form": "버팔로스틱은 별로였고 주먹밥은 맛있음",
          "speaker_id": "1",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100022964.1.16",
          "form": "어디 주먹밥이야?ㅋㅋ 나도 좀 사볼까",
          "original_form": "어디 주먹밥이야?ㅋㅋ 나도 좀 사볼까",
          "speaker_id": "2",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100022964.1.17",
          "form": "비비고꺼 싸게 팔길래 ㅋㅋ",
          "original_form": "비비고꺼 싸게 팔길래 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022964.1.18",
          "form": "불고기랑 김치치즈 치즈닭갈비샀엉",
          "original_form": "불고기랑 김치치즈 치즈닭갈비샀엉",
          "speaker_id": "1",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022964.1.19",
          "form": "셋 다 맛있어보이는데 어떤 맛이 제일 맛있었니",
          "original_form": "셋 다 맛있어보이는데 어떤 맛이 제일 맛있었니",
          "speaker_id": "2",
          "time": "20210829 19:46"
        },
        {
          "id": "MDRW2100022964.1.20",
          "form": "나 불고기는 안먹어봤고 김치치즈랑 치즈닭갈비중엔 치즈닭갈비가 나았엉",
          "original_form": "나 불고기는 안먹어봤고 김치치즈랑 치즈닭갈비중엔 치즈닭갈비가 나았엉",
          "speaker_id": "1",
          "time": "20210829 19:46"
        },
        {
          "id": "MDRW2100022964.1.21",
          "form": "비비고꺼 나 꼭 산다..",
          "original_form": "비비고꺼 나 꼭 산다..",
          "speaker_id": "2",
          "time": "20210829 19:46"
        },
        {
          "id": "MDRW2100022964.1.22",
          "form": "담에 다먹으면 또 살거같아 ㅋㅋ",
          "original_form": "담에 다먹으면 또 살거같아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:46"
        }
      ]
