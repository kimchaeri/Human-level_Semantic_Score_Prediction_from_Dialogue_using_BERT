      "utterance": [
        {
          "id": "MDRW2100034090.1.1",
          "form": "오늘 천도 복숭아요",
          "original_form": "오늘 천도 복숭아요",
          "speaker_id": "2",
          "time": "20210827 15:37"
        },
        {
          "id": "MDRW2100034090.1.2",
          "form": "ㅋㅋㅋㅋ전 간만에 포도 먹고 있으니 좋네요 올해 처음 먹는 포도!",
          "original_form": "ㅋㅋㅋㅋ전 간만에 포도 먹고 있으니 좋네요 올해 처음 먹는 포도!",
          "speaker_id": "1",
          "time": "20210827 15:38"
        },
        {
          "id": "MDRW2100034090.1.3",
          "form": "아직 못먹었어요",
          "original_form": "아직 못먹었어요",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100034090.1.4",
          "form": "내일 나가서 사와야 겠러요",
          "original_form": "내일 나가서 사와야 겠러요",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100034090.1.5",
          "form": "무슨포도요?",
          "original_form": "무슨포도요?",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100034090.1.6",
          "form": "까만 캠벨요?",
          "original_form": "까만 캠벨요?",
          "speaker_id": "2",
          "time": "20210827 15:41"
        },
        {
          "id": "MDRW2100034090.1.7",
          "form": "움 네네 일반포도! ㄴㅋㅋ",
          "original_form": "움 네네 일반포도! ㄴㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100034090.1.8",
          "form": "샤인머스캣은 가격 대비 만족도가 떨어져서욬ㅋㅋㅋ",
          "original_form": "샤인머스캣은 가격 대비 만족도가 떨어져서욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100034090.1.9",
          "form": "샤인 넘 사악해요 ㅋㅋ",
          "original_form": "샤인 넘 사악해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100034090.1.10",
          "form": "일반 최고입니다",
          "original_form": "일반 최고입니다",
          "speaker_id": "2",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100034090.1.11",
          "form": "그죠?",
          "original_form": "그죠?",
          "speaker_id": "1",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100034090.1.12",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100034090.1.13",
          "form": "자도 워낙 샤인샤인해서",
          "original_form": "자도 워낙 샤인샤인해서",
          "speaker_id": "1",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100034090.1.14",
          "form": "한번 먹어봤는데ㅋㅋㅋ저는 샤인마토에 약간 실망 너무 달아서....",
          "original_form": "한번 먹어봤는데ㅋㅋㅋ저는 샤인마토에 약간 실망 너무 달아서....",
          "speaker_id": "1",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100034090.1.15",
          "form": "달긴한데 그렇다고 그가격은 좀 그래요",
          "original_form": "달긴한데 그렇다고 그가격은 좀 그래요",
          "speaker_id": "2",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100034090.1.16",
          "form": "맞아요 달달",
          "original_form": "맞아요 달달",
          "speaker_id": "2",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100034090.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ동의합니다 한송이에 만몇천원이 무슨말이야",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ동의합니다 한송이에 만몇천원이 무슨말이야",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100034090.1.18",
          "form": "적당히 시고 적당히 달아야죠",
          "original_form": "적당히 시고 적당히 달아야죠",
          "speaker_id": "2",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100034090.1.19",
          "form": "맞아요 ㅡㅡ",
          "original_form": "맞아요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100034090.1.20",
          "form": "샤인마토는 한번 좋은 기억있어서 가족들 같이 먹자고 사봤는데",
          "original_form": "샤인마토는 한번 좋은 기억있어서 가족들 같이 먹자고 사봤는데",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100034090.1.21",
          "form": "다들 어우 하나 이상 못먹겠다고",
          "original_form": "다들 어우 하나 이상 못먹겠다고",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100034090.1.22",
          "form": "그래서 의도치않게 아껴먹었네요ㅋㅋㅋ",
          "original_form": "그래서 의도치않게 아껴먹었네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100034090.1.23",
          "form": "ㅋㅋ그럴때 다들 아껴먹죠",
          "original_form": "ㅋㅋ그럴때 다들 아껴먹죠",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100034090.1.24",
          "form": "갑자기 집에 귀중한녀석 ㅋㅋ",
          "original_form": "갑자기 집에 귀중한녀석 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100034090.1.25",
          "form": "됩니다 안먹어서",
          "original_form": "됩니다 안먹어서",
          "speaker_id": "2",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100034090.1.26",
          "form": "수박도 참 잘먹었는데 이제 가을이라 끝이네요 ㅎㅎ",
          "original_form": "수박도 참 잘먹었는데 이제 가을이라 끝이네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:49"
        }
      ]
    }
