        },
        {
          "id": "MDRW2100035396.1.2",
          "form": "와인 구매하시게여?",
          "original_form": "와인 구매하시게여?",
          "speaker_id": "1",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100035396.1.3",
          "form": "나만의 냉장고에있는 와인24에서 사려고요",
          "original_form": "나만의 냉장고에있는 와인24에서 사려고요",
          "speaker_id": "2",
          "time": "20210829 16:15"
        },
        {
          "id": "MDRW2100035396.1.4",
          "form": "언제요?",
          "original_form": "언제요?",
          "speaker_id": "1",
          "time": "20210829 16:17"
        },
        {
          "id": "MDRW2100035396.1.5",
          "form": "지금 사려고요",
          "original_form": "지금 사려고요",
          "speaker_id": "2",
