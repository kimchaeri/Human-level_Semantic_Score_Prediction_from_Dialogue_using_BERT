      "utterance": [
        {
          "id": "MDRW2100034706.1.1",
          "form": "다이어트 콜라를 그냥 콜ㄹ랑 비슷하게 조항하시면",
          "original_form": "다이어트 콜라를 그냥 콜ㄹ랑 비슷하게 조항하시면",
          "speaker_id": "2",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100034706.1.2",
          "form": "저칼로리 케챱도 맛있으실거에요",
          "original_form": "저칼로리 케챱도 맛있으실거에요",
          "speaker_id": "2",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100034706.1.3",
          "form": "오 그럼 괜찮을거같아요 ㅋㅋㅋ",
          "original_form": "오 그럼 괜찮을거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100034706.1.4",
          "form": "콜라는 제로콜라먹거든요",
          "original_form": "콜라는 제로콜라먹거든요",
          "speaker_id": "1",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100034706.1.5",
          "form": "잘됐네요!",
          "original_form": "잘됐네요!",
          "speaker_id": "2",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100034706.1.6",
          "form": "토마토 맛에 가려서 그렇게 아주 큰 차인없어요",
          "original_form": "토마토 맛에 가려서 그렇게 아주 큰 차인없어요",
          "speaker_id": "2",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100034706.1.7",
          "form": "오 다행이네요",
          "original_form": "오 다행이네요",
          "speaker_id": "1",
          "time": "20210828 17:57"
        },
        {
          "id": "MDRW2100034706.1.8",
          "form": "제로음료 좋아하세요?",
          "original_form": "제로음료 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100034706.1.9",
          "form": "있음 먹는데 아주 좋아하진 않아요 햄버거먹을떈 먹어요",
          "original_form": "있음 먹는데 아주 좋아하진 않아요 햄버거먹을떈 먹어요",
          "speaker_id": "2",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100034706.1.10",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100034706.1.11",
          "form": "저는 다이어터라",
          "original_form": "저는 다이어터라",
          "speaker_id": "1",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100034706.1.12",
          "form": "죄책감 줄이기용으로 피자랑 제로콜라를 먹는답니다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "죄책감 줄이기용으로 피자랑 제로콜라를 먹는답니다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:59"
        },
        {
          "id": "MDRW2100034706.1.13",
          "form": "아하! 그건 확실히 도움이 돼죠",
          "original_form": "아하! 그건 확실히 도움이 돼죠",
          "speaker_id": "2",
          "time": "20210828 17:59"
        },
        {
          "id": "MDRW2100034706.1.14",
          "form": "저도 그럴때 먹어요 ㅋㅋㅋㅋ",
          "original_form": "저도 그럴때 먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:59"
        },
        {
          "id": "MDRW2100034706.1.15",
          "form": "저만 그런게 아니군요ㅋㅋㅋ",
          "original_form": "저만 그런게 아니군요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:59"
        },
        {
          "id": "MDRW2100034706.1.16",
          "form": "제로사이다도 드셔보셨어요?",
          "original_form": "제로사이다도 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210828 17:59"
        },
        {
          "id": "MDRW2100034706.1.17",
          "form": "아뇨 패스트푸드점에서 안팔아서 안먹어봤어요",
          "original_form": "아뇨 패스트푸드점에서 안팔아서 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210828 18:00"
        },
        {
          "id": "MDRW2100034706.1.18",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "2",
          "time": "20210828 18:00"
        },
        {
          "id": "MDRW2100034706.1.19",
          "form": "놀랐어요",
          "original_form": "놀랐어요",
          "speaker_id": "1",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100034706.1.20",
          "form": "칠성건데 정말 똑같아요",
          "original_form": "칠성건데 정말 똑같아요",
          "speaker_id": "1",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100034706.1.21",
          "form": "먹어봐야겠어요",
          "original_form": "먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100034706.1.22",
          "form": "알려줘서 고마워요",
          "original_form": "알려줘서 고마워요",
          "speaker_id": "2",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100034706.1.23",
          "form": "근데 끝맛이 아주살짝 써요!",
          "original_form": "근데 끝맛이 아주살짝 써요!",
          "speaker_id": "1",
          "time": "20210828 18:01"
        },
        {
          "id": "MDRW2100034706.1.24",
          "form": "말 안하면 모르더라구요 친구들은",
          "original_form": "말 안하면 모르더라구요 친구들은",
          "speaker_id": "1",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100034706.1.25",
          "form": "그래도 칼로리가 없다면 전 좋습니다 ㅋㅋ",
          "original_form": "그래도 칼로리가 없다면 전 좋습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:04"
        },
        {
          "id": "MDRW2100034706.1.26",
          "form": "그리고 사이다나 콜라는 음식이랑 같이 먹잖아요",
          "original_form": "그리고 사이다나 콜라는 음식이랑 같이 먹잖아요",
          "speaker_id": "2",
          "time": "20210828 18:04"
        }
      ]
