      "utterance": [
        {
          "id": "MDRW2100018535.1.1",
          "form": "민초를 좋아하시는군요",
          "original_form": "민초를 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210827 07:53"
        },
        {
          "id": "MDRW2100018535.1.2",
          "form": "쌀국수에 고수는요?",
          "original_form": "쌀국수에 고수는요?",
          "speaker_id": "1",
          "time": "20210827 07:53"
        },
        {
          "id": "MDRW2100018535.1.3",
          "form": "저는 호!",
          "original_form": "저는 호!",
          "speaker_id": "1",
          "time": "20210827 07:53"
        },
        {
          "id": "MDRW2100018535.1.4",
          "form": "어우.. 죄송해요 저 고수못먹어요ㅋㅋㅋ",
          "original_form": "어우.. 죄송해요 저 고수못먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100018535.1.5",
          "form": "먹고 뱉었어요",
          "original_form": "먹고 뱉었어요",
          "speaker_id": "2",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100018535.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ 그럴수도 있죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그럴수도 있죠",
          "speaker_id": "1",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100018535.1.7",
          "form": "고수 호면 어떤맛으로 느껴지시나요",
          "original_form": "고수 호면 어떤맛으로 느껴지시나요",
          "speaker_id": "2",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100018535.1.8",
          "form": "취향 존중합니다",
          "original_form": "취향 존중합니다",
          "speaker_id": "1",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100018535.1.9",
          "form": "음 음식이 좀",
          "original_form": "음 음식이 좀",
          "speaker_id": "1",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100018535.1.10",
          "form": "느끼할수있는걸 잡아주는 역할을 한다고 할까요",
          "original_form": "느끼할수있는걸 잡아주는 역할을 한다고 할까요",
          "speaker_id": "1",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100018535.1.11",
          "form": "음 배라의 요거트같은 느낌이군",
          "original_form": "음 배라의 요거트같은 느낌이군",
          "speaker_id": "2",
          "time": "20210827 07:54"
        },
        {
          "id": "MDRW2100018535.1.12",
          "form": "치킨의 무 피자의 피클",
          "original_form": "치킨의 무 피자의 피클",
          "speaker_id": "2",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018535.1.13",
          "form": "그런느낌인거쇼",
          "original_form": "그런느낌인거쇼",
          "speaker_id": "2",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018535.1.14",
          "form": "오 맞아요ㅋㅋㅋ",
          "original_form": "오 맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018535.1.15",
          "form": "데자와 아세요?",
          "original_form": "데자와 아세요?",
          "speaker_id": "1",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018535.1.16",
          "form": "당연하죠",
          "original_form": "당연하죠",
          "speaker_id": "2",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100018535.1.17",
          "form": "미리말씀드리자면 저는 불호입니다ㅎ",
          "original_form": "미리말씀드리자면 저는 불호입니다ㅎ",
          "speaker_id": "2",
          "time": "20210827 07:56"
        },
        {
          "id": "MDRW2100018535.1.18",
          "form": "이것도 혹시 싫어하시나요?ㅠㅠ",
          "original_form": "이것도 혹시 싫어하시나요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 07:56"
        },
        {
          "id": "MDRW2100018535.1.19",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210827 07:56"
        },
        {
          "id": "MDRW2100018535.1.20",
          "form": "어제 누가 데저와 솔의눈 고르래서",
          "original_form": "어제 누가 데저와 솔의눈 고르래서",
          "speaker_id": "2",
          "time": "20210827 07:56"
        },
        {
          "id": "MDRW2100018535.1.21",
          "form": "솔의눈을 골랐어요ㅎ",
          "original_form": "솔의눈을 골랐어요ㅎ",
          "speaker_id": "2",
          "time": "20210827 07:56"
        },
        {
          "id": "MDRW2100018535.1.22",
          "form": "ㅋㅋㅋㅋ 전 데자와 넘 좋아하는데",
          "original_form": "ㅋㅋㅋㅋ 전 데자와 넘 좋아하는데",
          "speaker_id": "1",
          "time": "20210827 07:56"
        },
        {
          "id": "MDRW2100018535.1.23",
          "form": "없어서 못먹어요",
          "original_form": "없어서 못먹어요",
          "speaker_id": "1",
          "time": "20210827 07:56"
        },
        {
          "id": "MDRW2100018535.1.24",
          "form": "어떤 점이 좋던가요",
          "original_form": "어떤 점이 좋던가요",
          "speaker_id": "2",
          "time": "20210827 07:56"
        },
        {
          "id": "MDRW2100018535.1.25",
          "form": "(제가 모든걸 불호라 외쳐 죄송합니다)",
          "original_form": "(제가 모든걸 불호라 외쳐 죄송합니다)",
          "speaker_id": "2",
          "time": "20210827 07:56"
        },
        {
          "id": "MDRW2100018535.1.26",
          "form": "홍차를 좋아하다보니 자연스럽게",
          "original_form": "홍차를 좋아하다보니 자연스럽게",
          "speaker_id": "1",
          "time": "20210827 07:57"
        },
        {
          "id": "MDRW2100018535.1.27",
          "form": "좋아하는것같아요ㅋㅋㅋ",
          "original_form": "좋아하는것같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 07:57"
        }
      ]
    }
  ]
