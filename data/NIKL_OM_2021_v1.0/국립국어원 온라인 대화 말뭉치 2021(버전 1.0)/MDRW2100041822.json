      "utterance": [
        {
          "id": "MDRW2100041822.1.1",
          "form": "바른 자세 하고 있니",
          "original_form": "바른 자세 하고 있니",
          "speaker_id": "2",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100041822.1.2",
          "form": "저야 맨날 바른 자세 하고 다니죠 ㅎㅎ",
          "original_form": "저야 맨날 바른 자세 하고 다니죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041822.1.3",
          "form": "너의 굽은 허리 안에 숨은 2센치 중요하지",
          "original_form": "너의 굽은 허리 안에 숨은 2센치 중요하지",
          "speaker_id": "2",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041822.1.4",
          "form": "요즘은 스트레칭도 한다구요 ㅋㅋㅋ",
          "original_form": "요즘은 스트레칭도 한다구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100041822.1.5",
          "form": "무슨 스트레칭 해",
          "original_form": "무슨 스트레칭 해",
          "speaker_id": "2",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041822.1.6",
          "form": "허리랑 목 거북목 이런거 신경쓰이더라구요",
          "original_form": "허리랑 목 거북목 이런거 신경쓰이더라구요",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041822.1.7",
          "form": "맨날 게임만 하니가 그렇지",
          "original_form": "맨날 게임만 하니가 그렇지",
          "speaker_id": "2",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041822.1.8",
          "form": "아니에요 ㅠ 저 공부도 하는데 힝입니다",
          "original_form": "아니에요 ㅠ 저 공부도 하는데 힝입니다",
          "speaker_id": "1",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100041822.1.9",
          "form": "엄마 딸 믿는다",
          "original_form": "엄마 딸 믿는다",
          "speaker_id": "2",
          "time": "20210906 16:43"
        },
        {
          "id": "MDRW2100041822.1.10",
          "form": "네 ㅎㅎ 엄마도 운동 하시나요",
          "original_form": "네 ㅎㅎ 엄마도 운동 하시나요",
          "speaker_id": "1",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100041822.1.11",
          "form": "엄마 요즘 너무 더워서 운동이 안된다",
          "original_form": "엄마 요즘 너무 더워서 운동이 안된다",
          "speaker_id": "2",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100041822.1.12",
          "form": "더워도 운동 해야하는대.. 걱정이에요",
          "original_form": "더워도 운동 해야하는대.. 걱정이에요",
          "speaker_id": "1",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100041822.1.13",
          "form": "니 걱정이나 해",
          "original_form": "니 걱정이나 해",
          "speaker_id": "2",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100041822.1.14",
          "form": "몸도 안좋은것이",
          "original_form": "몸도 안좋은것이",
          "speaker_id": "2",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100041822.1.15",
          "form": "저 그래도 요즘엔 목 별로 안아파요",
          "original_form": "저 그래도 요즘엔 목 별로 안아파요",
          "speaker_id": "1",
          "time": "20210906 16:44"
        },
        {
          "id": "MDRW2100041822.1.16",
          "form": "엄마가 준 머플러 하고 있어서 그렇지?",
          "original_form": "엄마가 준 머플러 하고 있어서 그렇지?",
          "speaker_id": "2",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100041822.1.17",
          "form": "땀은 엄청 나는데 ㅎㅎ 엄청 줄었어요",
          "original_form": "땀은 엄청 나는데 ㅎㅎ 엄청 줄었어요",
          "speaker_id": "1",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100041822.1.18",
          "form": "그거 진짜 좋긴 한거같아요",
          "original_form": "그거 진짜 좋긴 한거같아요",
          "speaker_id": "1",
          "time": "20210906 16:46"
        }
