      "utterance": [
        {
          "id": "MDRW2100028499.1.1",
          "form": "넌 왜 교회 안가니",
          "original_form": "넌 왜 교회 안가니",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028499.1.2",
          "form": "저 일요일마다 일어나는게 힘들어요...",
          "original_form": "저 일요일마다 일어나는게 힘들어요...",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028499.1.3",
          "form": "부지런해져야지",
          "original_form": "부지런해져야지",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028499.1.4",
          "form": "그래야 하나님도 좋아하신다",
          "original_form": "그래야 하나님도 좋아하신다",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028499.1.5",
          "form": "잠이 더 소중하다구요 ㅠㅠ 요즘 예배 안나가시죠?",
          "original_form": "잠이 더 소중하다구요 ㅠㅠ 요즘 예배 안나가시죠?",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028499.1.6",
          "form": "코로나때문에 교회 가기도 힘들거같아요",
          "original_form": "코로나때문에 교회 가기도 힘들거같아요",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028499.1.7",
          "form": "소년부는 기도 녹음해서 제출해야 한대",
          "original_form": "소년부는 기도 녹음해서 제출해야 한대",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028499.1.8",
          "form": "오늘도 사진 찍었어",
          "original_form": "오늘도 사진 찍었어",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028499.1.9",
          "form": "아빠요??",
          "original_form": "아빠요??",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028499.1.10",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028499.1.11",
          "form": "전도사님한테 사진이랑 녹음해서 보냈다",
          "original_form": "전도사님한테 사진이랑 녹음해서 보냈다",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028499.1.12",
          "form": "그냥 녹화하면 되는거 아니에요?",
          "original_form": "그냥 녹화하면 되는거 아니에요?",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028499.1.13",
          "form": "엄만 잘 모르니깐...",
          "original_form": "엄만 잘 모르니깐...",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028499.1.14",
          "form": "마스크 잘 쓰고 다니시면 됐어요",
          "original_form": "마스크 잘 쓰고 다니시면 됐어요",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028499.1.15",
          "form": "건강하셔야죠",
          "original_form": "건강하셔야죠",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028499.1.16",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028499.1.17",
          "form": "너도 다음에 교회 나와라",
          "original_form": "너도 다음에 교회 나와라",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028499.1.18",
          "form": "선생님 딸이 안다니면 어떡해",
          "original_form": "선생님 딸이 안다니면 어떡해",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028499.1.19",
          "form": "아빠는 아빠고 저는 저니까 ㅠ",
          "original_form": "아빠는 아빠고 저는 저니까 ㅠ",
          "speaker_id": "2",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028499.1.20",
          "form": "그래도 사람 만나면 좋아",
          "original_form": "그래도 사람 만나면 좋아",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028499.1.21",
          "form": "목사님 말씀 잘하셔서 재밌어",
          "original_form": "목사님 말씀 잘하셔서 재밌어",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028499.1.22",
          "form": "전 근데 목사님 말씀 들으면 잠와요 ㅋㅋㅋㅋ 약간 교수님톤",
          "original_form": "전 근데 목사님 말씀 들으면 잠와요 ㅋㅋㅋㅋ 약간 교수님톤",
          "speaker_id": "2",
          "time": "20210902 20:30"
        }
      ]
