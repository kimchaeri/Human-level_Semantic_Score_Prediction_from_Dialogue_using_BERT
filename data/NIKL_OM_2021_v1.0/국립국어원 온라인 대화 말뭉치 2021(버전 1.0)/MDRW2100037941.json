      "utterance": [
        {
          "id": "MDRW2100037941.1.1",
          "form": "등이 채팅 많이 아파서 눈아프다",
          "original_form": "등이 채팅 많이 아파서 눈아프다",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037941.1.2",
          "form": "루테인 먹어야 할 듯",
          "original_form": "루테인 먹어야 할 듯",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100037941.1.3",
          "form": "블루스크린 어플 설치하셨나요?",
          "original_form": "블루스크린 어플 설치하셨나요?",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037941.1.4",
          "form": "아니용",
          "original_form": "아니용",
          "speaker_id": "1",
          "time": "20210901 22:09"
        },
        {
          "id": "MDRW2100037941.1.5",
          "form": "배달이 민족",
          "original_form": "배달이 민족",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037941.1.6",
          "form": "요기요만 써봄",
          "original_form": "요기요만 써봄",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037941.1.7",
          "form": "음 어플사용처가 넘 분명하시군요",
          "original_form": "음 어플사용처가 넘 분명하시군요",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100037941.1.8",
          "form": "배달음식 주젠인데여",
          "original_form": "배달음식 주젠인데여",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037941.1.9",
          "form": "저는 배달을 잘 안시켜묵거든요",
          "original_form": "저는 배달을 잘 안시켜묵거든요",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100037941.1.10",
          "form": "그럼 유명 카페는요?",
          "original_form": "그럼 유명 카페는요?",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100037941.1.11",
          "form": "저 오늘 마늘장아찌랑 누룽지랑 쥐포샀는데",
          "original_form": "저 오늘 마늘장아찌랑 누룽지랑 쥐포샀는데",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100037941.1.12",
          "form": "마늘장아찌만 남았어요 name3이편에 인도보냄",
          "original_form": "마늘장아찌만 남았어요 &name3&이편에 인도보냄",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037941.1.13",
          "form": "으 쥐포사서 고추장으로 뒤적하면 맛있지",
          "original_form": "으 쥐포사서 고추장으로 뒤적하면 맛있지",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100037941.1.14",
          "form": "ㅇ 근데 인도보냄",
          "original_form": "ㅇ 근데 인도보냄",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100037941.1.15",
          "form": "다시사야겠어 농협가서",
          "original_form": "다시사야겠어 농협가서",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100037941.1.16",
          "form": "오늘 밥 뭐랑 먹음?",
          "original_form": "오늘 밥 뭐랑 먹음?",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100037941.1.17",
          "form": "김치볶음밥",
          "original_form": "김치볶음밥",
          "speaker_id": "2",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100037941.1.18",
          "form": "나는 햇반이랑 반찬 3팩 조금씩",
          "original_form": "나는 햇반이랑 반찬 3팩 조금씩",
          "speaker_id": "1",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100037941.1.19",
          "form": "마트에서 사온 거",
          "original_form": "마트에서 사온 거",
          "speaker_id": "1",
          "time": "20210901 22:17"
        }
      ]
