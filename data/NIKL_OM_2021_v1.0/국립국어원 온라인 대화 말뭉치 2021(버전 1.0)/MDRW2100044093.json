      "utterance": [
        {
          "id": "MDRW2100044093.1.1",
          "form": "맞아요 ㅎ",
          "original_form": "맞아요 ㅎ",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044093.1.2",
          "form": "근데 개취지만 전 그 조합 별로에요ㅠ",
          "original_form": "근데 개취지만 전 그 조합 별로에요ㅠ",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044093.1.3",
          "form": "앗 왜죠..?ㅠㅠ",
          "original_form": "앗 왜죠..?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044093.1.4",
          "form": "그릭요거트는 약간 새콤한게 어울린다고 생각이 들어서요 ㅎ",
          "original_form": "그릭요거트는 약간 새콤한게 어울린다고 생각이 들어서요 ㅎ",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044093.1.5",
          "form": "복숭아랑 먹어봫른데",
          "original_form": "복숭아랑 먹어봫른데",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044093.1.6",
          "form": "별로였어요",
          "original_form": "별로였어요",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044093.1.7",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044093.1.8",
          "form": "먹어본 사람들이",
          "original_form": "먹어본 사람들이",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044093.1.9",
          "form": "따로먹으면 맛있는데 같이 먹는 조합은",
          "original_form": "따로먹으면 맛있는데 같이 먹는 조합은",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044093.1.10",
          "form": "그닥이라고 한 것 같아요",
          "original_form": "그닥이라고 한 것 같아요",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044093.1.11",
          "form": "맞아요ㅋㅋ딱 그거에요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋ딱 그거에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044093.1.12",
          "form": "진짜 그닥이에요",
          "original_form": "진짜 그닥이에요",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044093.1.13",
          "form": "ㅋㅋ요샌 무화과랑 먹더라고요 또",
          "original_form": "ㅋㅋ요샌 무화과랑 먹더라고요 또",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044093.1.14",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044093.1.15",
          "form": "그건 안먹어봤지만",
          "original_form": "그건 안먹어봤지만",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044093.1.16",
          "form": "일단 별로일듯요ㅋㅋㅋㅋㅋ무화과..흠",
          "original_form": "일단 별로일듯요ㅋㅋㅋㅋㅋ무화과..흠",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044093.1.17",
          "form": "ㅋㅋㅋㅋ왜여ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ왜여ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044093.1.18",
          "form": "무화과도 맛있지않나요",
          "original_form": "무화과도 맛있지않나요",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044093.1.19",
          "form": "따로는 맛있을텐데",
          "original_form": "따로는 맛있을텐데",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044093.1.20",
          "form": "그릭요거트랑은...",
          "original_form": "그릭요거트랑은...",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044093.1.21",
          "form": "저는 딸기가 제일 맛있어요",
          "original_form": "저는 딸기가 제일 맛있어요",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044093.1.22",
          "form": "그담에 나머지 베리류",
          "original_form": "그담에 나머지 베리류",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044093.1.23",
          "form": "그레놀라는여?",
          "original_form": "그레놀라는여?",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044093.1.24",
          "form": "귤도 괜찮고요 청포도도 맛있어요",
          "original_form": "귤도 괜찮고요 청포도도 맛있어요",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044093.1.25",
          "form": "오 그건",
          "original_form": "오 그건",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044093.1.26",
          "form": "존.맛",
          "original_form": "존.맛",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044093.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044093.1.28",
          "form": "저는 만약 먹게된다면",
          "original_form": "저는 만약 먹게된다면",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044093.1.29",
          "form": "블루베리 그래놀라 꿀 조합으로 먹고싶어요",
          "original_form": "블루베리 그래놀라 꿀 조합으로 먹고싶어요",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044093.1.30",
          "form": "와 그건",
          "original_form": "와 그건",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044093.1.31",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044093.1.32",
