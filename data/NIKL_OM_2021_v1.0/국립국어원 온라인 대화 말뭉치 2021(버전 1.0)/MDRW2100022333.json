      "utterance": [
        {
          "id": "MDRW2100022333.1.1",
          "form": "예전에는 오징어튀김이랑 순살치킨도 갖다놓으셨는데",
          "original_form": "예전에는 오징어튀김이랑 순살치킨도 갖다놓으셨는데",
          "speaker_id": "2",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022333.1.2",
          "form": "피자몰 이제 한물 간거 같기도 하고 ㅠㅠ",
          "original_form": "피자몰 이제 한물 간거 같기도 하고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022333.1.3",
          "form": "사이드가 점점 사라지는구나ㅠ",
          "original_form": "사이드가 점점 사라지는구나ㅠ",
          "speaker_id": "1",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022333.1.4",
          "form": "애슌리도 좋아하세요",
          "original_form": "애슌리도 좋아하세요",
          "speaker_id": "1",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022333.1.5",
          "form": "네 가짓수가 점점 줄어들더라구요 ...",
          "original_form": "네 가짓수가 점점 줄어들더라구요 ...",
          "speaker_id": "2",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022333.1.6",
          "form": "애슐리..",
          "original_form": "애슐리..",
          "speaker_id": "1",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022333.1.7",
          "form": "애슐리는 개인적으로 별로 안좋아하는 편이에요",
          "original_form": "애슐리는 개인적으로 별로 안좋아하는 편이에요",
          "speaker_id": "2",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022333.1.8",
          "form": "근데 좋아하는 사람 많더라구요",
          "original_form": "근데 좋아하는 사람 많더라구요",
          "speaker_id": "2",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022333.1.9",
          "form": "앗 그러시구나",
          "original_form": "앗 그러시구나",
          "speaker_id": "1",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022333.1.10",
          "form": "네ㅠㅠ제 기준엔 애슐리 퀸즈 가도 먹을 게ㅇ 없구",
          "original_form": "네ㅠㅠ제 기준엔 애슐리 퀸즈 가도 먹을 게ㅇ 없구",
          "speaker_id": "2",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022333.1.11",
          "form": "그건그래요",
          "original_form": "그건그래요",
          "speaker_id": "1",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022333.1.12",
          "form": "음식도 거의 식어있고 그렇더라구요",
          "original_form": "음식도 거의 식어있고 그렇더라구요",
          "speaker_id": "2",
          "time": "20210829 12:44"
        },
        {
          "id": "MDRW2100022333.1.13",
          "form": "약간 무슨 축제하면",
          "original_form": "약간 무슨 축제하면",
          "speaker_id": "1",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022333.1.14",
          "form": "광고만 그럴듯하고",
          "original_form": "광고만 그럴듯하고",
          "speaker_id": "1",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022333.1.15",
          "form": "맞아요 막상가보면 먹을게 없어요",
          "original_form": "맞아요 막상가보면 먹을게 없어요",
          "speaker_id": "2",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022333.1.16",
          "form": "먹을거없긴하더라구요",
          "original_form": "먹을거없긴하더라구요",
          "speaker_id": "1",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022333.1.17",
          "form": "광고에서 본 메뉴 어딨어? 설마 이거야?!!!",
          "original_form": "광고에서 본 메뉴 어딨어? 설마 이거야?!!!",
          "speaker_id": "2",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022333.1.18",
          "form": "바이럴을 너무잘하는듯..",
          "original_form": "바이럴을 너무잘하는듯..",
          "speaker_id": "1",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022333.1.19",
          "form": "근데 그에 비해 빕스가 많이 발전했어요",
          "original_form": "근데 그에 비해 빕스가 많이 발전했어요",
          "speaker_id": "2",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022333.1.20",
          "form": "퐁듀같은거.. 겁나 조잡",
          "original_form": "퐁듀같은거.. 겁나 조잡",
          "speaker_id": "1",
          "time": "20210829 12:45"
        },
        {
          "id": "MDRW2100022333.1.21",
          "form": "얼마전에 빕스 갔는데 너무 최고였어요...",
          "original_form": "얼마전에 빕스 갔는데 너무 최고였어요...",
          "speaker_id": "2",
          "time": "20210829 12:45"
        }
      ]
