      "utterance": [
        {
          "id": "MDRW2100024559.1.1",
          "form": "저 태평양데뷔하려구 스레기줄이기운동하자나용",
          "original_form": "저 태평양데뷔하려구 스레기줄이기운동하자나용",
          "speaker_id": "2",
          "time": "20210831 14:10"
        },
        {
          "id": "MDRW2100024559.1.2",
          "form": "그게 뭔가오?",
          "original_form": "그게 뭔가오?",
          "speaker_id": "1",
          "time": "20210831 14:10"
        },
        {
          "id": "MDRW2100024559.1.3",
          "form": "거북이친구들하구 친구하려구요",
          "original_form": "거북이친구들하구 친구하려구요",
          "speaker_id": "2",
          "time": "20210831 14:11"
        },
        {
          "id": "MDRW2100024559.1.4",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210831 14:11"
        },
        {
          "id": "MDRW2100024559.1.5",
          "form": "저는 재택하고싶어요",
          "original_form": "저는 재택하고싶어요",
          "speaker_id": "2",
          "time": "20210831 14:11"
        },
        {
          "id": "MDRW2100024559.1.6",
          "form": "거북이가 좋아하겠네요",
          "original_form": "거북이가 좋아하겠네요",
          "speaker_id": "1",
          "time": "20210831 14:12"
        },
        {
          "id": "MDRW2100024559.1.7",
          "form": "출근시간에 목운동 할수있어요",
          "original_form": "출근시간에 목운동 할수있어요",
          "speaker_id": "2",
          "time": "20210831 14:12"
        },
        {
          "id": "MDRW2100024559.1.8",
          "form": "하긴 하시나요?",
          "original_form": "하긴 하시나요?",
          "speaker_id": "1",
          "time": "20210831 14:12"
        },
        {
          "id": "MDRW2100024559.1.9",
          "form": "아프면 할수밖에없어요 살려궁",
          "original_form": "아프면 할수밖에없어요 살려궁",
          "speaker_id": "2",
          "time": "20210831 14:13"
        },
        {
          "id": "MDRW2100024559.1.10",
          "form": "오 하시는군요",
          "original_form": "오 하시는군요",
          "speaker_id": "1",
          "time": "20210831 14:19"
        },
        {
          "id": "MDRW2100024559.1.11",
          "form": "폼플러로 목 풀면 짱인데",
          "original_form": "폼플러로 목 풀면 짱인데",
          "speaker_id": "1",
          "time": "20210831 14:19"
        },
        {
          "id": "MDRW2100024559.1.12",
          "form": "폼플러 없으면 좀 두꺼운병으로 하시면 좋아요",
          "original_form": "폼플러 없으면 좀 두꺼운병으로 하시면 좋아요",
          "speaker_id": "1",
          "time": "20210831 14:19"
        },
        {
          "id": "MDRW2100024559.1.13",
          "form": "오 와서좀 펴주세요 ㅋㅋ",
          "original_form": "오 와서좀 펴주세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:44"
        },
        {
          "id": "MDRW2100024559.1.14",
          "form": "저는 수건을써요",
          "original_form": "저는 수건을써요",
          "speaker_id": "2",
          "time": "20210831 14:44"
        },
        {
          "id": "MDRW2100024559.1.15",
          "form": "제가 펴면 갈비뼈도 펴줄꺼같아서.",
          "original_form": "제가 펴면 갈비뼈도 펴줄꺼같아서.",
          "speaker_id": "1",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100024559.1.16",
          "form": "거절할께요",
          "original_form": "거절할께요",
          "speaker_id": "2",
          "time": "20210831 15:05"
        },
        {
          "id": "MDRW2100024559.1.17",
          "form": "저 컴터오래써서 마우스 버티컬로 바꿨어요",
          "original_form": "저 컴터오래써서 마우스 버티컬로 바꿨어요",
          "speaker_id": "2",
          "time": "20210831 15:05"
        },
        {
          "id": "MDRW2100024559.1.18",
          "form": "버티컬은 뭔가요?",
          "original_form": "버티컬은 뭔가요?",
          "speaker_id": "1",
          "time": "20210831 15:10"
        },
        {
          "id": "MDRW2100024559.1.19",
          "form": "버티컬마우스 세로로 버튼이 나열되어있어서 터널증후군을 예방해줘요 재택이든 삼실이든 컴터많이쓰는분들 필수",
          "original_form": "버티컬마우스 세로로 버튼이 나열되어있어서 터널증후군을 예방해줘요 재택이든 삼실이든 컴터많이쓰는분들 필수",
          "speaker_id": "2",
          "time": "20210831 15:16"
        },
        {
          "id": "MDRW2100024559.1.20",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100024559.1.21",
          "form": "모르겠군요",
          "original_form": "모르겠군요",
          "speaker_id": "1",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100024559.1.22",
          "form": "컴안쓰는 일이면 필요없쥬",
          "original_form": "컴안쓰는 일이면 필요없쥬",
          "speaker_id": "2",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100024559.1.23",
          "form": "근데 노트북은 갖고 싶어요",
          "original_form": "근데 노트북은 갖고 싶어요",
          "speaker_id": "1",
          "time": "20210831 17:58"
        }
      ]
