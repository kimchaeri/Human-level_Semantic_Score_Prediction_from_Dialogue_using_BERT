      "utterance": [
        {
          "id": "MDRW2100002972.1.1",
          "form": "안녕하세요 반가워요",
          "original_form": "안녕하세요 반가워요",
          "speaker_id": "1",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002972.1.2",
          "form": "저랑 대화해보실래요?",
          "original_form": "저랑 대화해보실래요?",
          "speaker_id": "1",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002972.1.3",
          "form": "안녕하세요! 요즘 어떤걸 많이 보시나요?",
          "original_form": "안녕하세요! 요즘 어떤걸 많이 보시나요?",
          "speaker_id": "2",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002972.1.4",
          "form": "유튜브로 동물 영상을 많이 봐요",
          "original_form": "유튜브로 동물 영상을 많이 봐요",
          "speaker_id": "1",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002972.1.5",
          "form": "name2 님은 무엇을 보시나요",
          "original_form": "&name2& 님은 무엇을 보시나요",
          "speaker_id": "1",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002972.1.6",
          "form": "저는 뮤직비디오를 많이 봐요~",
          "original_form": "저는 뮤직비디오를 많이 봐요~",
          "speaker_id": "2",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002972.1.7",
          "form": "아이돌 음악을 좋아해서!",
          "original_form": "아이돌 음악을 좋아해서!",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002972.1.8",
          "form": "제일 좋아하는 아이돌이 누구인가요",
          "original_form": "제일 좋아하는 아이돌이 누구인가요",
          "speaker_id": "1",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002972.1.9",
          "form": "저는 연예인을 좋아하지 않아요",
          "original_form": "저는 연예인을 좋아하지 않아요",
          "speaker_id": "1",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002972.1.10",
          "form": "저는 에스엠, 와이지 아이돌들 좋아해요",
          "original_form": "저는 에스엠, 와이지 아이돌들 좋아해요",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002972.1.11",
          "form": "연예인 안좋아하는 이유가 있으신가용?",
          "original_form": "연예인 안좋아하는 이유가 있으신가용?",
          "speaker_id": "2",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002972.1.12",
          "form": "중학교때 좋아했는데 잘생긴 연예인을 보러 강남역에서 6시간을 기다렸어요",
          "original_form": "중학교때 좋아했는데 잘생긴 연예인을 보러 강남역에서 6시간을 기다렸어요",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002972.1.13",
          "form": "그리고 1분정도 보고나니 내가 왜 이렇게까지 했나 싶었어요",
          "original_form": "그리고 1분정도 보고나니 내가 왜 이렇게까지 했나 싶었어요",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002972.1.14",
          "form": "그후로 연예인을 좋아하지 않았어요",
          "original_form": "그후로 연예인을 좋아하지 않았어요",
          "speaker_id": "1",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002972.1.15",
          "form": "6시간이면 엄청나네요..",
          "original_form": "6시간이면 엄청나네요..",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002972.1.16",
          "form": "저도 연예인을 엄청 좋아하고 그런것 까진 아니고",
          "original_form": "저도 연예인을 엄청 좋아하고 그런것 까진 아니고",
          "speaker_id": "2",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002972.1.17",
          "form": "음악취향이 아이돌음악 취향이에요@",
          "original_form": "음악취향이 아이돌음악 취향이에요@",
          "speaker_id": "2",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002972.1.18",
          "form": "아이돌음악은 정말 신나요",
          "original_form": "아이돌음악은 정말 신나요",
          "speaker_id": "1",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002972.1.19",
          "form": "어떤 아이돌이 가장 춤을 잘 추나요",
          "original_form": "어떤 아이돌이 가장 춤을 잘 추나요",
          "speaker_id": "1",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002972.1.20",
          "form": "음 유노윤호,,?",
          "original_form": "음 유노윤호,,?",
          "speaker_id": "2",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002972.1.21",
          "form": "윤호도 안좋은 일이 있어서..",
          "original_form": "윤호도 안좋은 일이 있어서..",
          "speaker_id": "2",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002972.1.22",
          "form": "신동도 잘추고요",
          "original_form": "신동도 잘추고요",
          "speaker_id": "2",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002972.1.23",
          "form": "옛날 사람들이네요",
          "original_form": "옛날 사람들이네요",
          "speaker_id": "1",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002972.1.24",
          "form": "여자아이돌도 좋아하나요?",
          "original_form": "여자아이돌도 좋아하나요?",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002972.1.25",
          "form": "네 여자아이돌도 노래가 좋으면 좋아해요",
          "original_form": "네 여자아이돌도 노래가 좋으면 좋아해요",
          "speaker_id": "2",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002972.1.26",
          "form": "에스파 요즘 푹빠졌어요",
          "original_form": "에스파 요즘 푹빠졌어요",
          "speaker_id": "2",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002972.1.27",
          "form": "요즘 라디오에 많이 나오더군요",
          "original_form": "요즘 라디오에 많이 나오더군요",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002972.1.28",
          "form": "저도 라디오를 통해서 들었어요",
          "original_form": "저도 라디오를 통해서 들었어요",
          "speaker_id": "1",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002972.1.29",
          "form": "넥스트 레벨보다 블랙맘바가 더 좋은것 같아요  둘다 아는 노래신가요?",
          "original_form": "넥스트 레벨보다 블랙맘바가 더 좋은것 같아요  둘다 아는 노래신가요?",
          "speaker_id": "2",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002972.1.30",
          "form": "예 넥스트레벨이 인기가 많으니 블랙맘바도 자꾸 나오더라구요",
          "original_form": "예 넥스트레벨이 인기가 많으니 블랙맘바도 자꾸 나오더라구요",
          "speaker_id": "1",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002972.1.31",
          "form": "헬스장에서 노래를 들었어요",
          "original_form": "헬스장에서 노래를 들었어요",
          "speaker_id": "1",
          "time": "20210820 17:23"
        },
        {
          "id": "MDRW2100002972.1.32",
          "form": "에스파 흥했으면~",
          "original_form": "에스파 흥했으면~",
          "speaker_id": "2",
          "time": "20210820 17:24"
        }
      ]
