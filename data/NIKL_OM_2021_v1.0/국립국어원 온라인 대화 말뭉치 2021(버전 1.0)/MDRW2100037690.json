      "utterance": [
        {
          "id": "MDRW2100037690.1.1",
          "form": "쫄면이랑 만두 먹었습니다!",
          "original_form": "쫄면이랑 만두 먹었습니다!",
          "speaker_id": "2",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100037690.1.2",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210901 16:13"
        },
        {
          "id": "MDRW2100037690.1.3",
          "form": "전 햄버거요 !!",
          "original_form": "전 햄버거요 !!",
          "speaker_id": "1",
          "time": "20210901 16:32"
        },
        {
          "id": "MDRW2100037690.1.4",
          "form": "맥날이엤나요!",
          "original_form": "맥날이엤나요!",
          "speaker_id": "2",
          "time": "20210901 16:33"
        },
        {
          "id": "MDRW2100037690.1.5",
          "form": "헐 어떻게 아셨죠 !",
          "original_form": "헐 어떻게 아셨죠 !",
          "speaker_id": "1",
          "time": "20210901 16:33"
        },
        {
          "id": "MDRW2100037690.1.6",
          "form": "제가 지금 맥날이 먹고싶어서요 ㅎㅎ",
          "original_form": "제가 지금 맥날이 먹고싶어서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100037690.1.7",
          "form": "무슨 버거 드셨나요?",
          "original_form": "무슨 버거 드셨나요?",
          "speaker_id": "2",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100037690.1.8",
          "form": "저 1955 먹었어요 !!",
          "original_form": "저 1955 먹었어요 !!",
          "speaker_id": "1",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100037690.1.9",
          "form": "1955 밀크 쉐이크랑 먹으면 맛있는데 드셔보셨나요ㅠㅠ!",
          "original_form": "1955 밀크 쉐이크랑 먹으면 맛있는데 드셔보셨나요ㅠㅠ!",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037690.1.10",
          "form": "헐 진짜여????!",
          "original_form": "헐 진짜여????!",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037690.1.11",
          "form": "항상 밀크쉐이크는 감튀랑만 먹었는데",
          "original_form": "항상 밀크쉐이크는 감튀랑만 먹었는데",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037690.1.12",
          "form": "버거랑도 맛있나요 !!!",
          "original_form": "버거랑도 맛있나요 !!!",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037690.1.13",
          "form": "그럼요! 경험상 불고기패티 들어간 버거랑 잘 어울렸어요!",
          "original_form": "그럼요! 경험상 불고기패티 들어간 버거랑 잘 어울렸어요!",
          "speaker_id": "2",
          "time": "20210901 17:28"
        },
        {
          "id": "MDRW2100037690.1.14",
          "form": "오오 !! 저 에그불고기도 좋아하는데",
          "original_form": "오오 !! 저 에그불고기도 좋아하는데",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100037690.1.15",
          "form": "꼭 먹어볼께요 !!!",
          "original_form": "꼭 먹어볼께요 !!!",
          "speaker_id": "1",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100037690.1.16",
          "form": "딸기, 초코 안되고 꼭 바닐라쉐이크로 추천드립니다 ㅎㅎ",
          "original_form": "딸기, 초코 안되고 꼭 바닐라쉐이크로 추천드립니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100037690.1.17",
          "form": "원래 쉐이크는 바닐라죠 ㅎㅎㅎ !!!",
          "original_form": "원래 쉐이크는 바닐라죠 ㅎㅎㅎ !!!",
          "speaker_id": "1",
          "time": "20210901 18:15"
        },
        {
          "id": "MDRW2100037690.1.18",
          "form": "쉑쉑꺼도 좋아해요 ㅎㅎ",
          "original_form": "쉑쉑꺼도 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:15"
        },
        {
          "id": "MDRW2100037690.1.19",
          "form": "쉑쉑 쉐이크 진하고 맛있는 것 같아요!",
          "original_form": "쉑쉑 쉐이크 진하고 맛있는 것 같아요!",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100037690.1.20",
          "form": "마자여 !! 근데 가격이 너무해서",
          "original_form": "마자여 !! 근데 가격이 너무해서",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100037690.1.21",
          "form": "맥날꺼 먹어요 ㅎㅎ",
          "original_form": "맥날꺼 먹어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:19"
