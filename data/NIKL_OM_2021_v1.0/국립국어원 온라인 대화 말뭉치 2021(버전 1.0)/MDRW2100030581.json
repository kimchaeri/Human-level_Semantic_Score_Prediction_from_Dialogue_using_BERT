      "utterance": [
        {
          "id": "MDRW2100030581.1.1",
          "form": "저 작년에 했어요 ㅎㅎ",
          "original_form": "저 작년에 했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 09:31"
        },
        {
          "id": "MDRW2100030581.1.2",
          "form": "다행히 1단계때 ㅎㅎ",
          "original_form": "다행히 1단계때 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 09:31"
        },
        {
          "id": "MDRW2100030581.1.3",
          "form": "우와 작년!! 늦었지만 축하드려요 ㅋㅋㅋ",
          "original_form": "우와 작년!! 늦었지만 축하드려요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 09:31"
        },
        {
          "id": "MDRW2100030581.1.4",
          "form": "감사합니다 ㅋㅋ 운이 좋았어요",
          "original_form": "감사합니다 ㅋㅋ 운이 좋았어요",
          "speaker_id": "2",
          "time": "20210904 09:40"
        },
        {
          "id": "MDRW2100030581.1.5",
          "form": "요새 보니까 지옥이던데ㅋㅋ",
          "original_form": "요새 보니까 지옥이던데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 09:41"
        },
        {
          "id": "MDRW2100030581.1.6",
          "form": "맞아요ㅠ 다음주에 친한 후배 결혼식가는데 힘들어보이더라구요",
          "original_form": "맞아요ㅠ 다음주에 친한 후배 결혼식가는데 힘들어보이더라구요",
          "speaker_id": "1",
          "time": "20210904 09:46"
        },
        {
          "id": "MDRW2100030581.1.7",
          "form": "99인 식사 없이가 뭡니까아아",
          "original_form": "99인 식사 없이가 뭡니까아아",
          "speaker_id": "2",
          "time": "20210904 10:04"
        },
        {
          "id": "MDRW2100030581.1.8",
          "form": "그니까요ㅠ 근데 어르신들까지 식사없이 할수가없으니까..",
          "original_form": "그니까요ㅠ 근데 어르신들까지 식사없이 할수가없으니까..",
          "speaker_id": "1",
          "time": "20210904 10:06"
        },
        {
          "id": "MDRW2100030581.1.9",
          "form": "후배는 어쩔수없이 49인 하기로했나보더라구요ㅠ",
          "original_form": "후배는 어쩔수없이 49인 하기로했나보더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210904 10:07"
        },
        {
          "id": "MDRW2100030581.1.10",
          "form": "근까요 49인 식사하고",
          "original_form": "근까요 49인 식사하고",
          "speaker_id": "2",
          "time": "20210904 10:30"
        },
        {
          "id": "MDRW2100030581.1.11",
          "form": "나머지는 좀 풀어주지",
          "original_form": "나머지는 좀 풀어주지",
          "speaker_id": "2",
          "time": "20210904 10:30"
        },
        {
          "id": "MDRW2100030581.1.12",
          "form": "마스크 끼고 있으면 되지 않나ㅠㅠ",
          "original_form": "마스크 끼고 있으면 되지 않나ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 10:30"
        },
        {
          "id": "MDRW2100030581.1.13",
          "form": "그니까요ㅠ그리구 99인이 뭐 식도 못보고? 이런것도 있나보던데 ㅠㅠㅠ",
          "original_form": "그니까요ㅠ그리구 99인이 뭐 식도 못보고? 이런것도 있나보던데 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 10:31"
        },
        {
          "id": "MDRW2100030581.1.14",
          "form": "요즘 결혼하는 예비부부들 힘들 것 같아요 ㅠㅠ",
          "original_form": "요즘 결혼하는 예비부부들 힘들 것 같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 10:32"
        },
        {
          "id": "MDRW2100030581.1.15",
          "form": "이제 식보는거는 99까지 되긴할텐데",
          "original_form": "이제 식보는거는 99까지 되긴할텐데",
          "speaker_id": "2",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030581.1.16",
          "form": "의미 없죠ㅠ",
          "original_form": "의미 없죠ㅠ",
          "speaker_id": "2",
          "time": "20210904 13:58"
        },
        {
          "id": "MDRW2100030581.1.17",
          "form": "작년에 신혼여행은 제주 다녀오셨어툐?",
          "original_form": "작년에 신혼여행은 제주 다녀오셨어툐?",
          "speaker_id": "1",
          "time": "20210904 13:58"
        },
        {
          "id": "MDRW2100030581.1.18",
          "form": "어요?",
          "original_form": "어요?",
          "speaker_id": "1",
          "time": "20210904 13:58"
        },
        {
          "id": "MDRW2100030581.1.19",
          "form": "음 아니오",
          "original_form": "음 아니오",
          "speaker_id": "2",
          "time": "20210904 14:04"
        },
        {
          "id": "MDRW2100030581.1.20",
          "form": "올해 울릉&독도!",
          "original_form": "올해 울릉&독도!",
          "speaker_id": "2",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100030581.1.21",
          "form": "헐!!!! 대박 ㅋㅋㅋ 성공적으로 다녀오셨나요?",
          "original_form": "헐!!!! 대박 ㅋㅋㅋ 성공적으로 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100030581.1.22",
          "form": "3대가",
          "original_form": "3대가",
          "speaker_id": "2",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030581.1.23",
          "form": "덕을 쌓아야",
          "original_form": "덕을 쌓아야",
          "speaker_id": "2",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030581.1.24",
          "form": "독도 입도할 수 있다고..",
          "original_form": "독도 입도할 수 있다고..",
          "speaker_id": "2",
          "time": "20210904 14:08"
        }
      ]
    }
