      "utterance": [
        {
          "id": "MDRW2100042569.1.1",
          "form": "보고싶은 콘서트나 공연 있나요?",
          "original_form": "보고싶은 콘서트나 공연 있나요?",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042569.1.2",
          "form": "이문세 콘서트를 꼭 복 싶어요",
          "original_form": "이문세 콘서트를 꼭 복 싶어요",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042569.1.3",
          "form": "헐 저도요!",
          "original_form": "헐 저도요!",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042569.1.4",
          "form": "이문세 콘서트나 박효신 콘서트 너무 가고시퍼ㅛㅜㅜ",
          "original_form": "이문세 콘서트나 박효신 콘서트 너무 가고시퍼ㅛㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042569.1.5",
          "form": "맞아요 현장에사 꼭 듣고 싶네요",
          "original_form": "맞아요 현장에사 꼭 듣고 싶네요",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042569.1.6",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042569.1.7",
          "form": "또 다른 가수는요?",
          "original_form": "또 다른 가수는요?",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042569.1.8",
          "form": "이문세 노래 엄청 좋아하거든료\n아세요 가을남은",
          "original_form": "이문세 노래 엄청 좋아하거든료\n아세요 가을남은",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042569.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ네! 저는 소녀라는 곡이 좋더라고요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ네! 저는 소녀라는 곡이 좋더라고요!",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042569.1.10",
          "form": "소녀 광화문연가 ...",
          "original_form": "소녀 광화문연가 ...",
          "speaker_id": "2",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100042569.1.11",
          "form": "ㅋㅋㅋㅋ맞아요~~~!",
          "original_form": "ㅋㅋㅋㅋ맞아요~~~!",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042569.1.12",
          "form": "광화문연가라는 곡보니 규현의 광화문에서라는 곡이 생각나네용!",
          "original_form": "광화문연가라는 곡보니 규현의 광화문에서라는 곡이 생각나네용!",
          "speaker_id": "1",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042569.1.13",
          "form": "음 아이유 노래도 좋아해요",
          "original_form": "음 아이유 노래도 좋아해요",
          "speaker_id": "2",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042569.1.14",
          "form": "아이유 노래 좋죠!!!",
          "original_form": "아이유 노래 좋죠!!!",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042569.1.15",
          "form": "네 규현근도 목소리가 극보급이죠",
          "original_form": "네 규현근도 목소리가 극보급이죠",
          "speaker_id": "2",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042569.1.16",
          "form": "어떤노래를 제을 좋아하세요?",
          "original_form": "어떤노래를 제을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 23:03"
        },
        {
          "id": "MDRW2100042569.1.17",
          "form": "넝의의미요",
          "original_form": "넝의의미요",
          "speaker_id": "2",
          "time": "20210906 23:04"
        },
        {
          "id": "MDRW2100042569.1.18",
          "form": "너의의미 좋죠!",
          "original_form": "너의의미 좋죠!",
          "speaker_id": "1",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042569.1.19",
          "form": "혹시 아이유가 부른 날아라병아리라는 곡 아세요?",
          "original_form": "혹시 아이유가 부른 날아라병아리라는 곡 아세요?",
          "speaker_id": "1",
          "time": "20210906 23:05"
        },
        {
          "id": "MDRW2100042569.1.20",
          "form": "원래는 신해철가수의 곡인데요 ㅎㅎ",
          "original_form": "원래는 신해철가수의 곡인데요 ㅎㅎ",
          "speaker_id": "1",
