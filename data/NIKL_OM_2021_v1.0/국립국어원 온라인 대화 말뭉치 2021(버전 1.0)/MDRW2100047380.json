      "utterance": [
        {
          "id": "MDRW2100047380.1.1",
          "form": "일 하고 계시나요?",
          "original_form": "일 하고 계시나요?",
          "speaker_id": "2",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100047380.1.2",
          "form": "네 회사에 있어요",
          "original_form": "네 회사에 있어요",
          "speaker_id": "1",
          "time": "20210826 14:17"
        },
        {
          "id": "MDRW2100047380.1.3",
          "form": "저도요....",
          "original_form": "저도요....",
          "speaker_id": "2",
          "time": "20210826 14:21"
        },
        {
          "id": "MDRW2100047380.1.4",
          "form": "직장에 있는 것만으로도 기빨리는거 같아요",
          "original_form": "직장에 있는 것만으로도 기빨리는거 같아요",
          "speaker_id": "1",
          "time": "20210826 14:23"
        },
        {
          "id": "MDRW2100047380.1.5",
          "form": "ㅋㅋㅋㅋㅋ아 진심요 ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ아 진심요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 14:49"
        },
        {
          "id": "MDRW2100047380.1.6",
          "form": "너무 슬퍼뇨",
          "original_form": "너무 슬퍼뇨",
          "speaker_id": "2",
          "time": "20210826 14:49"
        },
        {
          "id": "MDRW2100047380.1.7",
          "form": "빨리 쉬는 날이 되면 좋겠어요",
          "original_form": "빨리 쉬는 날이 되면 좋겠어요",
          "speaker_id": "1",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100047380.1.8",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100047380.1.9",
          "form": "곧 주말이니까",
          "original_form": "곧 주말이니까",
          "speaker_id": "2",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100047380.1.10",
          "form": "참아봐요",
          "original_form": "참아봐요",
          "speaker_id": "2",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100047380.1.11",
          "form": "오늘이 금요일이면 좋았을텐데요ㅋㅋㅋ",
          "original_form": "오늘이 금요일이면 좋았을텐데요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100047380.1.12",
          "form": "ㅋㅋㅋ 아 진심요",
          "original_form": "ㅋㅋㅋ 아 진심요",
          "speaker_id": "2",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100047380.1.13",
          "form": "근데 주말은",
          "original_form": "근데 주말은",
          "speaker_id": "2",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100047380.1.14",
          "form": "뭐이리 짧은지 ㅡㅡ",
          "original_form": "뭐이리 짧은지 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100047380.1.15",
          "form": "그러니깐요ㅠㅠ",
          "original_form": "그러니깐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100047380.1.16",
          "form": "퇴근시간도 빨라졌음 좋겠어요",
          "original_form": "퇴근시간도 빨라졌음 좋겠어요",
          "speaker_id": "1",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100047380.1.17",
          "form": "ㅋㅋ ㅋㅋㅋㅋㅋㅋ 전 그냥",
          "original_form": "ㅋㅋ ㅋㅋㅋㅋㅋㅋ 전 그냥",
          "speaker_id": "2",
          "time": "20210826 15:03"
        },
        {
          "id": "MDRW2100047380.1.18",
          "form": "출든 안해도 돈주면 좋겠는..",
          "original_form": "출든 안해도 돈주면 좋겠는..",
          "speaker_id": "2",
          "time": "20210826 15:03"
        },
        {
          "id": "MDRW2100047380.1.19",
          "form": "불로소득.. 너무 희망합니다ㅋㅋㅋㅋㅋ",
          "original_form": "불로소득.. 너무 희망합니다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100047380.1.20",
          "form": "ㅋㅋㅋㅋㅋ아주요",
          "original_form": "ㅋㅋㅋㅋㅋ아주요",
          "speaker_id": "2",
          "time": "20210826 15:04"
        },
        {
          "id": "MDRW2100047380.1.21",
          "form": "돈많은 백수 제 꿈입니다",
          "original_form": "돈많은 백수 제 꿈입니다",
          "speaker_id": "2",
          "time": "20210826 15:04"
        }
      ]
