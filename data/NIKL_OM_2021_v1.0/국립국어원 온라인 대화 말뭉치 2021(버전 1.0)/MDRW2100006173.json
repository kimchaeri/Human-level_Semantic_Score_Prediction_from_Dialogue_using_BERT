      "utterance": [
        {
          "id": "MDRW2100006173.1.1",
          "form": "연애중이세요?",
          "original_form": "연애중이세요?",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100006173.1.2",
          "form": "아니요 연애하시나요?",
          "original_form": "아니요 연애하시나요?",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006173.1.3",
          "form": "아니요 기혼이라서 ㅎㅎ",
          "original_form": "아니요 기혼이라서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100006173.1.4",
          "form": "와 이상형을 만나셨군요",
          "original_form": "와 이상형을 만나셨군요",
          "speaker_id": "2",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100006173.1.5",
          "form": "그건 모르겠는데요 ㅎㅎ",
          "original_form": "그건 모르겠는데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100006173.1.6",
          "form": "어떻게 결혼 결심하셨어요?",
          "original_form": "어떻게 결혼 결심하셨어요?",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100006173.1.7",
          "form": "착하기도 하고~ 같은 직장이여서요",
          "original_form": "착하기도 하고~ 같은 직장이여서요",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100006173.1.8",
          "form": "와 ㅈ자연스러운만남이네요",
          "original_form": "와 ㅈ자연스러운만남이네요",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100006173.1.9",
          "form": "누가 먼저호감있었나요?",
          "original_form": "누가 먼저호감있었나요?",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100006173.1.10",
          "form": "저였죠 ㅎㅎ",
          "original_form": "저였죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100006173.1.11",
          "form": "와 어떻게 고백하셨나요",
          "original_form": "와 어떻게 고백하셨나요",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100006173.1.12",
          "form": "좀 배우고싶네요",
          "original_form": "좀 배우고싶네요",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100006173.1.13",
          "form": "ㅋㅋ음 처음엔 제 친구 소개해줬는데요?",
          "original_form": "ㅋㅋ음 처음엔 제 친구 소개해줬는데요?",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100006173.1.14",
          "form": "친구가 별로 맘에 안들어했나요?",
          "original_form": "친구가 별로 맘에 안들어했나요?",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100006173.1.15",
          "form": "네 서로 인연이 아닌듯",
          "original_form": "네 서로 인연이 아닌듯",
          "speaker_id": "1",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100006173.1.16",
          "form": "저도 당시에는 만나던 분이 있어서",
          "original_form": "저도 당시에는 만나던 분이 있어서",
          "speaker_id": "1",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100006173.1.17",
          "form": "아하ㅋㅋ역시 결혼인연은 따로있네요",
          "original_form": "아하ㅋㅋ역시 결혼인연은 따로있네요",
          "speaker_id": "2",
          "time": "20210907 21:36"
        }
      ]
