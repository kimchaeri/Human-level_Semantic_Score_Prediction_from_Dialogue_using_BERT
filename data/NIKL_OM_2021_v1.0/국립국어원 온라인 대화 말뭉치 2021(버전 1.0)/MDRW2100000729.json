      "utterance": [
        {
          "id": "MDRW2100000729.1.1",
          "form": "언니야 데바데에 새로운 캐릭터 또 나온대",
          "original_form": "언니야 데바데에 새로운 캐릭터 또 나온대",
          "speaker_id": "1",
          "time": "20210819 18:47"
        },
        {
          "id": "MDRW2100000729.1.2",
          "form": "학지운말고",
          "original_form": "학지운말고",
          "speaker_id": "1",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000729.1.3",
          "form": "진짜???",
          "original_form": "진짜???",
          "speaker_id": "2",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000729.1.4",
          "form": "재밋겠다",
          "original_form": "재밋겠다",
          "speaker_id": "2",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000729.1.5",
          "form": "나도 얼른 데바데 사야집",
          "original_form": "나도 얼른 데바데 사야집",
          "speaker_id": "2",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000729.1.6",
          "form": "너는 언제살래?",
          "original_form": "너는 언제살래?",
          "speaker_id": "2",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000729.1.7",
          "form": "스팀에서 사야지",
          "original_form": "스팀에서 사야지",
          "speaker_id": "1",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100000729.1.8",
          "form": "할로윈할인때",
          "original_form": "할로윈할인때",
          "speaker_id": "1",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100000729.1.9",
          "form": "살거야",
          "original_form": "살거야",
          "speaker_id": "1",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100000729.1.10",
          "form": "너 스팀에서 다른게임 사고싶은건 없어??",
          "original_form": "너 스팀에서 다른게임 사고싶은건 없어??",
          "speaker_id": "2",
          "time": "20210819 18:49"
        },
        {
          "id": "MDRW2100000729.1.11",
          "form": "없긴 당연히있지",
          "original_form": "없긴 당연히있지",
          "speaker_id": "1",
          "time": "20210819 18:50"
        },
        {
          "id": "MDRW2100000729.1.12",
          "form": "나 러스트레이크도 사고싶고",
          "original_form": "나 러스트레이크도 사고싶고",
          "speaker_id": "1",
          "time": "20210819 18:50"
        },
        {
          "id": "MDRW2100000729.1.13",
          "form": "하우스플리퍼도사고싶어",
          "original_form": "하우스플리퍼도사고싶어",
          "speaker_id": "1",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100000729.1.14",
          "form": "오 대박많다",
          "original_form": "오 대박많다",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100000729.1.15",
          "form": "러스트레이크는 뭐야??",
          "original_form": "러스트레이크는 뭐야??",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100000729.1.16",
          "form": "그거 햄튜브가 이번에 영상올린 게임인데",
          "original_form": "그거 햄튜브가 이번에 영상올린 게임인데",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000729.1.17",
          "form": "재밌어보여서",
          "original_form": "재밌어보여서",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000729.1.18",
          "form": "하고싶어",
          "original_form": "하고싶어",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100000729.1.19",
          "form": "아그래?",
          "original_form": "아그래?",
          "speaker_id": "2",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100000729.1.20",
          "form": "어떤 게임이냐구",
          "original_form": "어떤 게임이냐구",
          "speaker_id": "2",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100000729.1.21",
          "form": "궁금해궁금해~~",
          "original_form": "궁금해궁금해~~",
          "speaker_id": "2",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100000729.1.22",
          "form": "어떤 요상한 사건에 대해서",
          "original_form": "어떤 요상한 사건에 대해서",
          "speaker_id": "1",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000729.1.23",
          "form": "조사를하면서",
          "original_form": "조사를하면서",
          "speaker_id": "1",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000729.1.24",
          "form": "진실을 파헤치는건데",
          "original_form": "진실을 파헤치는건데",
          "speaker_id": "1",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000729.1.25",
          "form": "재밌어보여",
          "original_form": "재밌어보여",
          "speaker_id": "1",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000729.1.26",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000729.1.27",
          "form": "재밌었다",
          "original_form": "재밌었다",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000729.1.28",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000729.1.29",
          "form": "언니얀 뭐 하고싶은 게임없어??",
          "original_form": "언니얀 뭐 하고싶은 게임없어??",
          "speaker_id": "1",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000729.1.30",
          "form": "나는 멀티스타듀밸리!",
          "original_form": "나는 멀티스타듀밸리!",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000729.1.31",
          "form": "혼자 하는건 재미없더라!",
          "original_form": "혼자 하는건 재미없더라!",
          "speaker_id": "2",
          "time": "20210819 18:56"
        },
        {
          "id": "MDRW2100000729.1.32",
          "form": "맞아! 게임은 같이해야재밋지",
          "original_form": "맞아! 게임은 같이해야재밋지",
          "speaker_id": "1",
          "time": "20210819 18:58"
        }
      ]
    }
  ]
