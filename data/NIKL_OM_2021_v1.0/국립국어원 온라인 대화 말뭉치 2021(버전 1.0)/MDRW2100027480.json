      "utterance": [
        {
          "id": "MDRW2100027480.1.1",
          "form": "날다람쥐보시나요",
          "original_form": "날다람쥐보시나요",
          "speaker_id": "2",
          "time": "20210902 13:01"
        },
        {
          "id": "MDRW2100027480.1.2",
          "form": "헐 키우는 건가요",
          "original_form": "헐 키우는 건가요",
          "speaker_id": "1",
          "time": "20210902 13:01"
        },
        {
          "id": "MDRW2100027480.1.3",
          "form": "아니요... ㅋㅋㅋㅋㅋ그냥 유튭만 봐요 ㅋㅋ",
          "original_form": "아니요... ㅋㅋㅋㅋㅋ그냥 유튭만 봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100027480.1.4",
          "form": "그런 채널도 있나봐요 요즘 애완동물 다양해서 보는맛이",
          "original_form": "그런 채널도 있나봐요 요즘 애완동물 다양해서 보는맛이",
          "speaker_id": "1",
          "time": "20210902 13:16"
        },
        {
          "id": "MDRW2100027480.1.5",
          "form": "맞아요 근데 그걸로 돈버는사람도있더라규요 ㅠㅠ",
          "original_form": "맞아요 근데 그걸로 돈버는사람도있더라규요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027480.1.6",
          "form": "너무괴심해요 ㅠㅠ",
          "original_form": "너무괴심해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027480.1.7",
          "form": "진짜 못됐네요",
          "original_form": "진짜 못됐네요",
          "speaker_id": "1",
          "time": "20210902 13:22"
        },
        {
          "id": "MDRW2100027480.1.8",
          "form": "주작이 많데요",
          "original_form": "주작이 많데요",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027480.1.9",
          "form": "너무해",
          "original_form": "너무해",
          "speaker_id": "1",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027480.1.10",
          "form": "맞아요 어떠케 말못하는 동물로 ㅠㅠ",
          "original_form": "맞아요 어떠케 말못하는 동물로 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027480.1.11",
          "form": "여행 유투브는 안보시나요",
          "original_form": "여행 유투브는 안보시나요",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027480.1.12",
          "form": "여행보다",
          "original_form": "여행보다",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027480.1.13",
          "form": "물놀이 보ㅓ요 ㅋㅋㅋ",
          "original_form": "물놀이 보ㅓ요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027480.1.14",
          "form": "수영장 가는거 찍은건가요 ㅋㅋㅋ",
          "original_form": "수영장 가는거 찍은건가요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027480.1.15",
          "form": "맞아요 그런거 만터라구요",
          "original_form": "맞아요 그런거 만터라구요",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027480.1.16",
          "form": "동물들이랑가는것도있구",
          "original_form": "동물들이랑가는것도있구",
          "speaker_id": "2",
          "time": "20210902 13:28"
        },
        {
          "id": "MDRW2100027480.1.17",
          "form": "대박 저는 고양이 키워서 같이못가는데",
          "original_form": "대박 저는 고양이 키워서 같이못가는데",
          "speaker_id": "1",
          "time": "20210902 13:31"
        },
        {
          "id": "MDRW2100027480.1.18",
          "form": "고양이는 물 싫어하죠??",
          "original_form": "고양이는 물 싫어하죠??",
          "speaker_id": "2",
          "time": "20210902 13:39"
