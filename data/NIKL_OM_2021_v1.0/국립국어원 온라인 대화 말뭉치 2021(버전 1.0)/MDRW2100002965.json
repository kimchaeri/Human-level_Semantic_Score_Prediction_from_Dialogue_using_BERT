        },
        {
          "id": "MDRW2100002965.1.2",
          "form": "오늘 회 먹어도 될까?",
          "original_form": "오늘 회 먹어도 될까?",
          "speaker_id": "1",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002965.1.3",
          "form": "ㅎㅎ 되징",
          "original_form": "ㅎㅎ 되징",
          "speaker_id": "2",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002965.1.4",
          "form": "색시가 안좋아하긴 하는데 라면이랑 같이 먹자",
          "original_form": "색시가 안좋아하긴 하는데 라면이랑 같이 먹자",
          "speaker_id": "1",
          "time": "20210820 17:08"
        },
        {
          "id": "MDRW2100002965.1.5",
          "form": "ㅇㅇ 알았옹",
          "original_form": "ㅇㅇ 알았옹",
          "speaker_id": "2",
          "time": "20210820 17:09"
        },
        {
          "id": "MDRW2100002965.1.6",
          "form": "물반 고기반이라고 평이 좋은 곳이 있던데",
          "original_form": "물반 고기반이라고 평이 좋은 곳이 있던데",
          "speaker_id": "1",
          "time": "20210820 17:09"
        },
        {
          "id": "MDRW2100002965.1.7",
          "form": "거기에 시켜?",
          "original_form": "거기에 시켜?",
          "speaker_id": "2",
          "time": "20210820 17:09"
        },
        {
          "id": "MDRW2100002965.1.8",
          "form": "ㅇㅇ 광어랑 우럭 색시도 먹을 수 있게 ㅎㅎ",
          "original_form": "ㅇㅇ 광어랑 우럭 색시도 먹을 수 있게 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:10"
        },
        {
          "id": "MDRW2100002965.1.9",
          "form": "ㅇㅇ 알았어요",
          "original_form": "ㅇㅇ 알았어요",
          "speaker_id": "2",
          "time": "20210820 17:10"
        },
        {
          "id": "MDRW2100002965.1.10",
          "form": "거기에 라면까지 먹으면 딱이겠다 ㅎㅎ",
          "original_form": "거기에 라면까지 먹으면 딱이겠다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 17:10"
        },
        {
          "id": "MDRW2100002965.1.11",
          "form": "고마워요",
          "original_form": "고마워요",
          "speaker_id": "1",
          "time": "20210820 17:10"
        },
        {
          "id": "MDRW2100002965.1.12",
          "form": "뭘유 ㅎㅎ",
          "original_form": "뭘유 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 17:10"
        },
        {
          "id": "MDRW2100002965.1.13",
          "form": "애들도 좋아하겠네",
          "original_form": "애들도 좋아하겠네",
          "speaker_id": "2",
          "time": "20210820 17:11"
        },
        {
          "id": "MDRW2100002965.1.14",
          "form": "그러게 요즘엔 애들이 더 잘 먹더라 ㅋㅋ",
          "original_form": "그러게 요즘엔 애들이 더 잘 먹더라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:11"
        },
        {
          "id": "MDRW2100002965.1.15",
          "form": "나 먹을 회가 모자를듯 ㅎㅎ",
          "original_form": "나 먹을 회가 모자를듯 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002965.1.16",
          "form": "음식 아까 주문했는데 아직 안오네",
