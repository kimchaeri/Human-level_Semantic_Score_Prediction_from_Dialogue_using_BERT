        {
          "id": "MDRW2100003048.1.1",
          "form": "오랜만에 너구리 먹으니까 맛있다 ㅎㅎ",
          "original_form": "오랜만에 너구리 먹으니까 맛있다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 18:48"
        },
        {
          "id": "MDRW2100003048.1.2",
          "form": "순한맛 맛있지?",
          "original_form": "순한맛 맛있지?",
          "speaker_id": "1",
          "time": "20210820 18:48"
        },
        {
          "id": "MDRW2100003048.1.3",
          "form": "ㅇㅇ 면발도 오동통하고 ㅎㅎ 다 먹고 나니 아쉽",
          "original_form": "ㅇㅇ 면발도 오동통하고 ㅎㅎ 다 먹고 나니 아쉽",
          "speaker_id": "2",
          "time": "20210820 18:48"
        },
        {
          "id": "MDRW2100003048.1.4",
          "form": "하나 더 끓여먹어",
          "original_form": "하나 더 끓여먹어",
          "speaker_id": "1",
          "time": "20210820 18:48"
        },
        {
          "id": "MDRW2100003048.1.5",
          "form": "안돼~속 부대껴 ㅋ",
          "original_form": "안돼~속 부대껴 ㅋ",
          "speaker_id": "2",
          "time": "20210820 18:48"
        },
        {
          "id": "MDRW2100003048.1.6",
          "form": "먹고 죽은 귀신 때깔도 곱다 ㅎㄹ",
          "original_form": "먹고 죽은 귀신 때깔도 곱다 ㅎㄹ",
          "speaker_id": "1",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100003048.1.7",
          "form": "헐 ㅋㅋ",
          "original_form": "헐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100003048.1.8",
          "form": "암튼 오랜만에 포식했네",
          "original_form": "암튼 오랜만에 포식했네",
          "speaker_id": "2",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100003048.1.9",
          "form": "가끔 라면 엄청 땡기지",
          "original_form": "가끔 라면 엄청 땡기지",
          "speaker_id": "1",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100003048.1.10",
          "form": "힘든 날이나 스트레스 받는 날은 더 ㅎㅎ",
          "original_form": "힘든 날이나 스트레스 받는 날은 더 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100003048.1.11",
          "form": "ㅎㅎ 그래도 너무 많이 먹진 마",
          "original_form": "ㅎㅎ 그래도 너무 많이 먹진 마",
          "speaker_id": "1",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100003048.1.12",
          "form": "그냥 국물은 남기고 면만 먹응께 ㅋㅋ",
          "original_form": "그냥 국물은 남기고 면만 먹응께 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100003048.1.13",
          "form": "ㅇㅇ 국물 많이 먹으면 몸에 안좋대",
          "original_form": "ㅇㅇ 국물 많이 먹으면 몸에 안좋대",
          "speaker_id": "1",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100003048.1.14",
          "form": "나도 알아 ㅎㅎ 티비에도 많이 나오잖아",
          "original_form": "나도 알아 ㅎㅎ 티비에도 많이 나오잖아",
          "speaker_id": "2",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100003048.1.15",
          "form": "가끔 먹으니 괜찮겠지 뭐 ㅋ",
          "original_form": "가끔 먹으니 괜찮겠지 뭐 ㅋ",
          "speaker_id": "1",
          "time": "20210820 18:51"
        },
