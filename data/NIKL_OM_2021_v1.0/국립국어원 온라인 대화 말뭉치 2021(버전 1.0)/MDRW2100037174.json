      "utterance": [
        {
          "id": "MDRW2100037174.1.1",
          "form": "게임좋아하세요?",
          "original_form": "게임좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 15:57"
        },
        {
          "id": "MDRW2100037174.1.2",
          "form": "별로 안해요 게임 좋아하시나요?",
          "original_form": "별로 안해요 게임 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100037174.1.3",
          "form": "저도 하는건없어요 ㅎ",
          "original_form": "저도 하는건없어요 ㅎ",
          "speaker_id": "2",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100037174.1.4",
          "form": "전에 위베이베이스 게임햇엇는ㄷ0",
          "original_form": "전에 위베이베이스 게임햇엇는ㄷ0",
          "speaker_id": "2",
          "time": "20210901 16:15"
        },
        {
          "id": "MDRW2100037174.1.5",
          "form": "그건 어떤 게임인가요?",
          "original_form": "그건 어떤 게임인가요?",
          "speaker_id": "1",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100037174.1.6",
          "form": "많이들 모르시더라구요ㅠㅠㅋ ㅋㅋ퍼즐게임이라해야하나",
          "original_form": "많이들 모르시더라구요ㅠㅠㅋ ㅋㅋ퍼즐게임이라해야하나",
          "speaker_id": "2",
          "time": "20210901 16:25"
        },
        {
          "id": "MDRW2100037174.1.7",
          "form": "애니팡처럼",
          "original_form": "애니팡처럼",
          "speaker_id": "2",
          "time": "20210901 16:25"
        },
        {
          "id": "MDRW2100037174.1.8",
          "form": "3개 맞추는 게임인가요?",
          "original_form": "3개 맞추는 게임인가요?",
          "speaker_id": "1",
          "time": "20210901 16:27"
        },
        {
          "id": "MDRW2100037174.1.9",
          "form": "저도 요새안한지오래되었는데 계속 업뎃되서 미션도 많고 ㅋㅋ 잼있어요 전 ㅎ",
          "original_form": "저도 요새안한지오래되었는데 계속 업뎃되서 미션도 많고 ㅋㅋ 잼있어요 전 ㅎ",
          "speaker_id": "2",
          "time": "20210901 16:28"
        },
        {
          "id": "MDRW2100037174.1.10",
          "form": "그 게임도 시간제한 있는게임인가요?",
          "original_form": "그 게임도 시간제한 있는게임인가요?",
          "speaker_id": "1",
          "time": "20210901 16:30"
        },
        {
          "id": "MDRW2100037174.1.11",
          "form": "턴수가 제한일꺼에요! ㅋㅋㅋ기억이가물가물이네요",
          "original_form": "턴수가 제한일꺼에요! ㅋㅋㅋ기억이가물가물이네요",
          "speaker_id": "2",
          "time": "20210901 16:32"
        },
        {
          "id": "MDRW2100037174.1.12",
          "form": "그렇군요 어디게임이었나요?",
          "original_form": "그렇군요 어디게임이었나요?",
          "speaker_id": "1",
          "time": "20210901 16:32"
        },
        {
          "id": "MDRW2100037174.1.13",
          "form": "폰게임이에요!!",
          "original_form": "폰게임이에요!!",
          "speaker_id": "2",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100037174.1.14",
          "form": "카카오게임인가요?",
          "original_form": "카카오게임인가요?",
          "speaker_id": "1",
          "time": "20210901 16:37"
        },
        {
          "id": "MDRW2100037174.1.15",
          "form": "아! 그런거 같네요 맞나!? 이게 웹툰도잇는데 ㅎㅎ",
          "original_form": "아! 그런거 같네요 맞나!? 이게 웹툰도잇는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 16:38"
        },
        {
          "id": "MDRW2100037174.1.16",
          "form": "한 번 찾아봐야겠네요",
          "original_form": "한 번 찾아봐야겠네요",
          "speaker_id": "1",
