      "utterance": [
        {
          "id": "MDRW2100034221.1.1",
          "form": "여행 좋아하세요",
          "original_form": "여행 좋아하세요",
          "speaker_id": "1",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034221.1.2",
          "form": "아 저는 집순이라 별로 못가봤어요 ㅜㅜ",
          "original_form": "아 저는 집순이라 별로 못가봤어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100034221.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100034221.1.4",
          "form": "마음만요 ㅋㅋ 경주 자주갔어요 통영이랑",
          "original_form": "마음만요 ㅋㅋ 경주 자주갔어요 통영이랑",
          "speaker_id": "1",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100034221.1.5",
          "form": "경주하면 뭔가 어렸을때",
          "original_form": "경주하면 뭔가 어렸을때",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100034221.1.6",
          "form": "견학간게 생각나네요 ㅋㅋㅋ",
          "original_form": "견학간게 생각나네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100034221.1.7",
          "form": "아정말요 저는 가을에 자전거 경주에서 타는게 정말 좋아요",
          "original_form": "아정말요 저는 가을에 자전거 경주에서 타는게 정말 좋아요",
          "speaker_id": "1",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100034221.1.8",
          "form": "봄가을에 타면 딱 좋겠어요",
          "original_form": "봄가을에 타면 딱 좋겠어요",
          "speaker_id": "2",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100034221.1.9",
          "form": "맞아요 ㅠ 저는 전동 킥보드도 좋라요",
          "original_form": "맞아요 ㅠ 저는 전동 킥보드도 좋라요",
          "speaker_id": "1",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100034221.1.10",
          "form": "전 아직 못 타봤는데 어때요?",
          "original_form": "전 아직 못 타봤는데 어때요?",
          "speaker_id": "2",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100034221.1.11",
          "form": "짱 재밌어요",
          "original_form": "짱 재밌어요",
          "speaker_id": "1",
          "time": "20210827 21:32"
        },
        {
          "id": "MDRW2100034221.1.12",
          "form": "속도가 엄청 빠르더라구요",
          "original_form": "속도가 엄청 빠르더라구요",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100034221.1.13",
          "form": "맞아요 전 무서워서 천천히",
          "original_form": "맞아요 전 무서워서 천천히",
          "speaker_id": "1",
          "time": "20210827 21:35"
        },
        {
          "id": "MDRW2100034221.1.14",
          "form": "통영은 어떠셨어요?",
          "original_form": "통영은 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100034221.1.15",
          "form": "통영 짱 좋아요 루지재믺어요",
          "original_form": "통영 짱 좋아요 루지재믺어요",
          "speaker_id": "1",
          "time": "20210827 21:39"
        }
