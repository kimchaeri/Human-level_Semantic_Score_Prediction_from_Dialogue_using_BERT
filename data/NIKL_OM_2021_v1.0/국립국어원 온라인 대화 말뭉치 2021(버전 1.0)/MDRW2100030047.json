        },
        {
          "id": "MDRW2100030047.1.2",
          "form": "안녕하세요ㅎ",
          "original_form": "안녕하세요ㅎ",
          "speaker_id": "2",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100030047.1.3",
          "form": "술 좋아하시나요?",
          "original_form": "술 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100030047.1.4",
          "form": "술을 좋아하진 않구요ㅋㅋㅋ",
          "original_form": "술을 좋아하진 않구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:11"
        },
        {
          "id": "MDRW2100030047.1.5",
          "form": "곰표? 맥주 아이스크림많더라구요ㅎ",
          "original_form": "곰표? 맥주 아이스크림많더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100030047.1.6",
          "form": "진로두꺼비는 귀여워서 열쇠고리가 있었어요",
          "original_form": "진로두꺼비는 귀여워서 열쇠고리가 있었어요",
          "speaker_id": "1",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100030047.1.7",
          "form": "곰표는 밀가루라",
          "original_form": "곰표는 밀가루라",
          "speaker_id": "1",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100030047.1.8",
          "form": "나초도 맛있더라구요",
          "original_form": "나초도 맛있더라구요",
          "speaker_id": "1",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100030047.1.9",
          "form": "두꺼비도 귀엽죠ㅋ",
          "original_form": "두꺼비도 귀엽죠ㅋ",
          "speaker_id": "2",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100030047.1.10",
          "form": "나초요?",
          "original_form": "나초요?",
          "speaker_id": "2",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100030047.1.11",
          "form": "그건 안본지 오래되서ㅋ",
          "original_form": "그건 안본지 오래되서ㅋ",
          "speaker_id": "2",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100030047.1.12",
          "form": "곰표아이스크림은 여전히 비싸구요 맛있대요",
          "original_form": "곰표아이스크림은 여전히 비싸구요 맛있대요",
          "speaker_id": "1",
          "time": "20210903 20:14"
        },
        {
          "id": "MDRW2100030047.1.13",
          "form": "편의점에서 봤었는데..",
          "original_form": "편의점에서 봤었는데..",
          "speaker_id": "1",
          "time": "20210903 20:15"
        },
        {
          "id": "MDRW2100030047.1.14",
          "form": "곰표 나초는 드셔보셔나요?",
          "original_form": "곰표 나초는 드셔보셔나요?",
          "speaker_id": "1",
          "time": "20210903 20:34"
        },
        {
          "id": "MDRW2100030047.1.15",
          "form": "아진짜요? 아이스크림은 한번 먹어봐야겠네요ㅋ",
          "original_form": "아진짜요? 아이스크림은 한번 먹어봐야겠네요ㅋ",
          "speaker_id": "2",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100030047.1.16",
          "form": "씨유에서 팔아요!!!",
          "original_form": "씨유에서 팔아요!!!",
          "speaker_id": "1",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100030047.1.17",
          "form": "그런거 도전 잘못해서ㅠㅋ",
          "original_form": "그런거 도전 잘못해서ㅠㅋ",
          "speaker_id": "2",
          "time": "20210903 20:44"
        },
        {
          "id": "MDRW2100030047.1.18",
          "form": "그래도 아는맛인데 조금 색다른 맛이니까요 ㅎㅎ",
          "original_form": "그래도 아는맛인데 조금 색다른 맛이니까요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100030047.1.19",
          "form": "ㅋㅋㅋㅋㅋ날씨도 그렇고 피곤하니 특이한 것들 먹을 식욕도 안나요ㅠㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ날씨도 그렇고 피곤하니 특이한 것들 먹을 식욕도 안나요ㅠㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100030047.1.20",
          "form": "모험은 약간의 위험도 있으니까요 ㅎㅎ",
