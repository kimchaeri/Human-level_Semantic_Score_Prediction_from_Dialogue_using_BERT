      "utterance": [
        {
          "id": "MDRW2100035707.1.1",
          "form": "걸으면서",
          "original_form": "걸으면서",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100035707.1.2",
          "form": "더벤티 초코라떼 먹었어요",
          "original_form": "더벤티 초코라떼 먹었어요",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100035707.1.3",
          "form": "휘핑크림잔뜩 ㅋㅋ",
          "original_form": "휘핑크림잔뜩 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100035707.1.4",
          "form": "설마 지금요?",
          "original_form": "설마 지금요?",
          "speaker_id": "1",
          "time": "20210829 23:00"
        },
        {
          "id": "MDRW2100035707.1.5",
          "form": "아녀 피자먹고나오면서요",
          "original_form": "아녀 피자먹고나오면서요",
          "speaker_id": "2",
          "time": "20210829 23:00"
        },
        {
          "id": "MDRW2100035707.1.6",
          "form": "후식배는 따로 ㅋㅋ",
          "original_form": "후식배는 따로 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:00"
        },
        {
          "id": "MDRW2100035707.1.7",
          "form": "ㅎㅎ 하루에 몇칼로리를 드신 건가요ㅎㅎㅎ",
          "original_form": "ㅎㅎ 하루에 몇칼로리를 드신 건가요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 23:00"
        },
        {
          "id": "MDRW2100035707.1.8",
          "form": "이 힘으로 이개월을 나시려는 거군요!",
          "original_form": "이 힘으로 이개월을 나시려는 거군요!",
          "speaker_id": "1",
          "time": "20210829 23:01"
        },
        {
          "id": "MDRW2100035707.1.9",
          "form": "생각해보니 한 삼일치 미리 땡겨먹은거같기도 ㅋㅋㅋ",
          "original_form": "생각해보니 한 삼일치 미리 땡겨먹은거같기도 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:01"
        },
        {
          "id": "MDRW2100035707.1.10",
          "form": "ㅎㅎ  평소 단 음료 좋아하세요?",
          "original_form": "ㅎㅎ  평소 단 음료 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 23:01"
        },
        {
          "id": "MDRW2100035707.1.11",
          "form": "저 한동안 1일1식했어서요",
          "original_form": "저 한동안 1일1식했어서요",
          "speaker_id": "2",
          "time": "20210829 23:01"
        },
        {
          "id": "MDRW2100035707.1.12",
          "form": "단거 ㅜ환장해요",
          "original_form": "단거 ㅜ환장해요",
          "speaker_id": "2",
          "time": "20210829 23:02"
        },
        {
          "id": "MDRW2100035707.1.13",
          "form": "황제같은 1식 다이어트죠?",
          "original_form": "황제같은 1식 다이어트죠?",
          "speaker_id": "1",
          "time": "20210829 23:02"
        },
        {
          "id": "MDRW2100035707.1.14",
          "form": "근데 그이후 진짜 먹고싶은거다먹고 야식머고",
          "original_form": "근데 그이후 진짜 먹고싶은거다먹고 야식머고",
          "speaker_id": "2",
          "time": "20210829 23:02"
        },
        {
          "id": "MDRW2100035707.1.15",
          "form": "그래도 의지가 대단하네요ㄷㄷ",
          "original_form": "그래도 의지가 대단하네요ㄷㄷ",
          "speaker_id": "1",
          "time": "20210829 23:02"
        },
        {
          "id": "MDRW2100035707.1.16",
          "form": "두달만에 15키로넘거쪘어요",
          "original_form": "두달만에 15키로넘거쪘어요",
          "speaker_id": "2",
          "time": "20210829 23:02"
        },
        {
          "id": "MDRW2100035707.1.17",
          "form": "아;; 요요가 왔군요ㅜ",
          "original_form": "아;; 요요가 왔군요ㅜ",
          "speaker_id": "1",
          "time": "20210829 23:02"
        },
        {
          "id": "MDRW2100035707.1.18",
          "form": "아이고 뺐던거 그대로 다시ㅜㅜㅜ;;",
          "original_form": "아이고 뺐던거 그대로 다시ㅜㅜㅜ;;",
          "speaker_id": "1",
          "time": "20210829 23:03"
        },
        {
          "id": "MDRW2100035707.1.19",
          "form": "찌는건 금방이더라고요",
          "original_form": "찌는건 금방이더라고요",
          "speaker_id": "2",
          "time": "20210829 23:03"
        },
        {
          "id": "MDRW2100035707.1.20",
          "form": "그 허무함",
          "original_form": "그 허무함",
          "speaker_id": "2",
          "time": "20210829 23:03"
        },
        {
          "id": "MDRW2100035707.1.21",
          "form": "다시하려는데",
          "original_form": "다시하려는데",
          "speaker_id": "2",
          "time": "20210829 23:03"
        }
      ]
