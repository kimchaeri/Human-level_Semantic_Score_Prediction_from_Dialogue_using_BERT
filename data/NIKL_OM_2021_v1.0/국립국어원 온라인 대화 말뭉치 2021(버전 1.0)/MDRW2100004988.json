      "utterance": [
        {
          "id": "MDRW2100004988.1.1",
          "form": "와 안녕하세용~",
          "original_form": "와 안녕하세용~",
          "speaker_id": "1",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004988.1.2",
          "form": "안녕하세요 학생이세요?",
          "original_form": "안녕하세요 학생이세요?",
          "speaker_id": "2",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004988.1.3",
          "form": "대학생이에요!",
          "original_form": "대학생이에요!",
          "speaker_id": "1",
          "time": "20210821 19:47"
        },
        {
          "id": "MDRW2100004988.1.4",
          "form": "아하 그러시구나 코로나라 학교생활 많이 못하시져ㅜ",
          "original_form": "아하 그러시구나 코로나라 학교생활 많이 못하시져ㅜ",
          "speaker_id": "2",
          "time": "20210821 19:49"
        },
        {
          "id": "MDRW2100004988.1.5",
          "form": "네 맞아요ㅜㅜ 근데 학교 가기 싫어서 나름 좋아요 ㅎ",
          "original_form": "네 맞아요ㅜㅜ 근데 학교 가기 싫어서 나름 좋아요 ㅎ",
          "speaker_id": "1",
          "time": "20210821 19:50"
        },
        {
          "id": "MDRW2100004988.1.6",
          "form": "ㅋㅋㅋ장단점이있네요",
          "original_form": "ㅋㅋㅋ장단점이있네요",
          "speaker_id": "2",
          "time": "20210821 19:53"
        },
        {
          "id": "MDRW2100004988.1.7",
          "form": "대학생이시면 과생활말고도 동아리활동같은것도하셨나여?",
          "original_form": "대학생이시면 과생활말고도 동아리활동같은것도하셨나여?",
          "speaker_id": "2",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100004988.1.8",
          "form": "네네! 저는 21학번이라 1학기가 처음이었는데",
          "original_form": "네네! 저는 21학번이라 1학기가 처음이었는데",
          "speaker_id": "1",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100004988.1.9",
          "form": "대학교 가장 큰 낭만이 동아리였어서",
          "original_form": "대학교 가장 큰 낭만이 동아리였어서",
          "speaker_id": "1",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100004988.1.10",
          "form": "자제한 결과 세개만... 들어갔습니다 ㅎㅋㅋㅋ",
          "original_form": "자제한 결과 세개만... 들어갔습니다 ㅎㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:54"
        },
        {
          "id": "MDRW2100004988.1.11",
          "form": "와 대박 3개여",
          "original_form": "와 대박 3개여",
          "speaker_id": "2",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100004988.1.12",
          "form": "어떤데 들어가샸어요 저는 다닐때 하나만했었거든요",
          "original_form": "어떤데 들어가샸어요 저는 다닐때 하나만했었거든요",
          "speaker_id": "2",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100004988.1.13",
          "form": "포트폴리오용 쌓는 소모임 하나랑",
          "original_form": "포트폴리오용 쌓는 소모임 하나랑",
          "speaker_id": "1",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100004988.1.14",
          "form": "제가 어릴 때부터 피아노를 쳐서 밴드 하나랑",
          "original_form": "제가 어릴 때부터 피아노를 쳐서 밴드 하나랑",
          "speaker_id": "1",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100004988.1.15",
          "form": "활 쏘는 국궁!! 동아리 하나 들어갔어요",
          "original_form": "활 쏘는 국궁!! 동아리 하나 들어갔어요",
          "speaker_id": "1",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100004988.1.16",
          "form": "우와 하나같이 대박이네요",
          "original_form": "우와 하나같이 대박이네요",
          "speaker_id": "2",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100004988.1.17",
          "form": "국궁대박 ㅋㅋㅋ요새 양궁꽂혀서 국궁에도 관심생겼는데",
          "original_form": "국궁대박 ㅋㅋㅋ요새 양궁꽂혀서 국궁에도 관심생겼는데",
          "speaker_id": "2",
          "time": "20210821 20:00"
        },
        {
          "id": "MDRW2100004988.1.18",
          "form": "직접 해보니까 어때요?",
          "original_form": "직접 해보니까 어때요?",
          "speaker_id": "2",
          "time": "20210821 20:00"
        },
        {
          "id": "MDRW2100004988.1.19",
          "form": "국궁은 자세 잡는 게 어렵고",
          "original_form": "국궁은 자세 잡는 게 어렵고",
          "speaker_id": "1",
          "time": "20210821 20:02"
        },
        {
          "id": "MDRW2100004988.1.20",
          "form": "원사 가려면 145m라",
          "original_form": "원사 가려면 145m라",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100004988.1.21",
          "form": "교육을 많이 받아야 되더라고요",
          "original_form": "교육을 많이 받아야 되더라고요",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100004988.1.22",
          "form": "1학기 내내 신사교육 받고",
          "original_form": "1학기 내내 신사교육 받고",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100004988.1.23",
          "form": "이제 2학기에 중급반 갈 텐데",
          "original_form": "이제 2학기에 중급반 갈 텐데",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100004988.1.24",
          "form": "빨리 원사 가고 싶습니다..",
          "original_form": "빨리 원사 가고 싶습니다..",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100004988.1.25",
          "form": "양궁도 올림픽 보고 재밌어보여서",
          "original_form": "양궁도 올림픽 보고 재밌어보여서",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100004988.1.26",
          "form": "양궁카페 갔었는데 재밌더라고요 ㅎㅎ",
          "original_form": "양궁카페 갔었는데 재밌더라고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 20:03"
        },
        {
          "id": "MDRW2100004988.1.27",
          "form": "와 동아리활동 진짜넘재밌을것같아요",
          "original_form": "와 동아리활동 진짜넘재밌을것같아요",
          "speaker_id": "2",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100004988.1.28",
          "form": "저도 양궁카페갈려고 찾아봤더니 코로나땜에 문을 닫았드라구여ㅜ",
          "original_form": "저도 양궁카페갈려고 찾아봤더니 코로나땜에 문을 닫았드라구여ㅜ",
          "speaker_id": "2",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004988.1.29",
          "form": "동아리가 3개들면 겹칠때는 어떻게하나요",
          "original_form": "동아리가 3개들면 겹칠때는 어떻게하나요",
          "speaker_id": "2",
          "time": "20210821 20:14"
        },
        {
          "id": "MDRW2100004988.1.30",
          "form": "그게 너무 힘들어요...",
          "original_form": "그게 너무 힘들어요...",
          "speaker_id": "1",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100004988.1.31",
          "form": "그래도 밴드는 지금",
          "original_form": "그래도 밴드는 지금",
          "speaker_id": "1",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100004988.1.32",
          "form": "합주도 못해서 활동이 거의 없는데",
          "original_form": "합주도 못해서 활동이 거의 없는데",
          "speaker_id": "1",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100004988.1.33",
          "form": "가끔 두개씩 일정이 겹치면",
          "original_form": "가끔 두개씩 일정이 겹치면",
          "speaker_id": "1",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100004988.1.34",
          "form": "번갈아가면서 빠지는 것 같아요",
          "original_form": "번갈아가면서 빠지는 것 같아요",
          "speaker_id": "1",
          "time": "20210821 20:17"
        },
        {
          "id": "MDRW2100004988.1.35",
          "form": "이렇게 세개도 버거운데 또 들어가고 싶은 동아리들이 있더라고요 ..",
          "original_form": "이렇게 세개도 버거운데 또 들어가고 싶은 동아리들이 있더라고요 ..",
          "speaker_id": "1",
          "time": "20210821 20:17"
        },
        {
          "id": "MDRW2100004988.1.36",
          "form": "그렇군여ㅜㅜ학년올라가고 또 그만두고 다른데 가고싶은곳가도되니까여",
          "original_form": "그렇군여ㅜㅜ학년올라가고 또 그만두고 다른데 가고싶은곳가도되니까여",
          "speaker_id": "2",
          "time": "20210821 20:25"
        },
        {
          "id": "MDRW2100004988.1.37",
          "form": "저도 동아리활동은딱1년만했어요 전너무재미없는곳했어서 ㅋㅋㅋ",
          "original_form": "저도 동아리활동은딱1년만했어요 전너무재미없는곳했어서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100004988.1.38",
          "form": "할게많은곳은 부러워요",
          "original_form": "할게많은곳은 부러워요",
          "speaker_id": "2",
          "time": "20210821 20:26"
        }
      ]
    }
