      "utterance": [
        {
          "id": "MDRW2100016545.1.1",
          "form": "결혼하셨나요?",
          "original_form": "결혼하셨나요?",
          "speaker_id": "2",
          "time": "20210903 16:04"
        },
        {
          "id": "MDRW2100016545.1.2",
          "form": "네ㅎㅎ나이가 있다보니",
          "original_form": "네ㅎㅎ나이가 있다보니",
          "speaker_id": "1",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100016545.1.3",
          "form": "결혼하면 행복한가요?",
          "original_form": "결혼하면 행복한가요?",
          "speaker_id": "2",
          "time": "20210903 16:05"
        },
        {
          "id": "MDRW2100016545.1.4",
          "form": "그냥 그래요",
          "original_form": "그냥 그래요",
          "speaker_id": "1",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100016545.1.5",
          "form": "저는 비혼이라 여쭤봤어요",
          "original_form": "저는 비혼이라 여쭤봤어요",
          "speaker_id": "2",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100016545.1.6",
          "form": "비혼이 더 좋을거같아요 요즘같은 세상에",
          "original_form": "비혼이 더 좋을거같아요 요즘같은 세상에",
          "speaker_id": "1",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100016545.1.7",
          "form": "ㅠㅠ맞아요 무서운 세상이죠",
          "original_form": "ㅠㅠ맞아요 무서운 세상이죠",
          "speaker_id": "2",
          "time": "20210903 16:06"
        },
        {
          "id": "MDRW2100016545.1.8",
          "form": "혼자 살아도 재미있게 살면 돼요~",
          "original_form": "혼자 살아도 재미있게 살면 돼요~",
          "speaker_id": "1",
          "time": "20210903 16:07"
        },
        {
          "id": "MDRW2100016545.1.9",
          "form": "네 그래서 저도 취미를 많이 만들고있네용",
          "original_form": "네 그래서 저도 취미를 많이 만들고있네용",
          "speaker_id": "2",
          "time": "20210903 16:07"
        },
        {
          "id": "MDRW2100016545.1.10",
          "form": "좋은 자세네요ㅎㅎ",
          "original_form": "좋은 자세네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 16:07"
        },
        {
          "id": "MDRW2100016545.1.11",
          "form": "그런가요ㅋㅋ자녀도 있으시겠어요",
          "original_form": "그런가요ㅋㅋ자녀도 있으시겠어요",
          "speaker_id": "2",
          "time": "20210903 16:08"
        },
        {
          "id": "MDRW2100016545.1.12",
          "form": "네 아들하나 딸하나 있어요",
          "original_form": "네 아들하나 딸하나 있어요",
          "speaker_id": "1",
          "time": "20210903 16:08"
        },
        {
          "id": "MDRW2100016545.1.13",
