      "utterance": [
        {
          "id": "MDRW2100035112.1.1",
          "form": "그게몬가요",
          "original_form": "그게몬가요",
          "speaker_id": "2",
          "time": "20210829 02:58"
        },
        {
          "id": "MDRW2100035112.1.2",
          "form": "그릭요거트는 알고 복슝아도알지만",
          "original_form": "그릭요거트는 알고 복슝아도알지만",
          "speaker_id": "2",
          "time": "20210829 02:58"
        },
        {
          "id": "MDRW2100035112.1.3",
          "form": "나혼산에서 키가 나와서 한것같은데",
          "original_form": "나혼산에서 키가 나와서 한것같은데",
          "speaker_id": "1",
          "time": "20210829 02:58"
        },
        {
          "id": "MDRW2100035112.1.4",
          "form": "복숭아에 홈 파서 그 안에",
          "original_form": "복숭아에 홈 파서 그 안에",
          "speaker_id": "1",
          "time": "20210829 02:58"
        },
        {
          "id": "MDRW2100035112.1.5",
          "form": "그릭요거트 넣어서 먹는건가봐용",
          "original_form": "그릭요거트 넣어서 먹는건가봐용",
          "speaker_id": "1",
          "time": "20210829 02:59"
        },
        {
          "id": "MDRW2100035112.1.6",
          "form": "아 나혼산을 보다안보다해서 ㅋㅋ",
          "original_form": "아 나혼산을 보다안보다해서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:59"
        },
        {
          "id": "MDRW2100035112.1.7",
          "form": "아항 !",
          "original_form": "아항 !",
          "speaker_id": "2",
          "time": "20210829 02:59"
        },
        {
          "id": "MDRW2100035112.1.8",
          "form": "저도 안보고 클립만 봤어요ㅋㅋㅋ",
          "original_form": "저도 안보고 클립만 봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:59"
        },
        {
          "id": "MDRW2100035112.1.9",
          "form": "그래서 복숭아 자르면",
          "original_form": "그래서 복숭아 자르면",
          "speaker_id": "1",
          "time": "20210829 02:59"
        },
        {
          "id": "MDRW2100035112.1.10",
          "form": "집에 복숭아도있고 요거트도있으니",
          "original_form": "집에 복숭아도있고 요거트도있으니",
          "speaker_id": "2",
          "time": "20210829 02:59"
        },
        {
          "id": "MDRW2100035112.1.11",
          "form": "안에 그릭 요거트가ㅋㅋㅋ",
          "original_form": "안에 그릭 요거트가ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:59"
        },
        {
          "id": "MDRW2100035112.1.12",
          "form": "같이먹어야겟군요 ㅋㅋ",
          "original_form": "같이먹어야겟군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:59"
        },
        {
          "id": "MDRW2100035112.1.13",
          "form": "맛있어보이는데",
          "original_form": "맛있어보이는데",
          "speaker_id": "1",
          "time": "20210829 02:59"
        },
        {
          "id": "MDRW2100035112.1.14",
          "form": "손가는 음식같아서",
          "original_form": "손가는 음식같아서",
          "speaker_id": "1",
          "time": "20210829 02:59"
        },
        {
          "id": "MDRW2100035112.1.15",
          "form": "안그래도 지금 요거트퍼묵을까생각했는데",
          "original_form": "안그래도 지금 요거트퍼묵을까생각했는데",
          "speaker_id": "2",
          "time": "20210829 02:59"
        },
        {
          "id": "MDRW2100035112.1.16",
          "form": "복숭아에 대충 발라먹을까 생각하고있어용",
          "original_form": "복숭아에 대충 발라먹을까 생각하고있어용",
          "speaker_id": "1",
          "time": "20210829 02:59"
        },
        {
          "id": "MDRW2100035112.1.17",
          "form": "앗 좋죠!!",
          "original_form": "앗 좋죠!!",
          "speaker_id": "1",
          "time": "20210829 02:59"
        },
        {
          "id": "MDRW2100035112.1.18",
          "form": "좀전에 백도하나랑 천도하나먹고 ㅋㅋㅋ",
          "original_form": "좀전에 백도하나랑 천도하나먹고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 03:00"
        },
        {
          "id": "MDRW2100035112.1.19",
          "form": "복숭아 알러지 있는분들이 안쓰러워요",
          "original_form": "복숭아 알러지 있는분들이 안쓰러워요",
          "speaker_id": "1",
          "time": "20210829 03:00"
        },
        {
          "id": "MDRW2100035112.1.20",
          "form": "이 맛있는걸 ㅋㅋㅋ",
          "original_form": "이 맛있는걸 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 03:00"
        },
        {
          "id": "MDRW2100035112.1.21",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:00"
        },
        {
          "id": "MDRW2100035112.1.22",
          "form": "그리고 요즘",
          "original_form": "그리고 요즘",
          "speaker_id": "1",
          "time": "20210829 03:01"
        },
        {
          "id": "MDRW2100035112.1.23",
          "form": "라떼에",
          "original_form": "라떼에",
          "speaker_id": "1",
          "time": "20210829 03:01"
        },
        {
          "id": "MDRW2100035112.1.24",
          "form": "맥도날드 콘 넣어서 먹는",
          "original_form": "맥도날드 콘 넣어서 먹는",
          "speaker_id": "1",
          "time": "20210829 03:01"
        },
        {
          "id": "MDRW2100035112.1.25",
          "form": "맥도날드식 폴바셋이",
          "original_form": "맥도날드식 폴바셋이",
          "speaker_id": "1",
          "time": "20210829 03:01"
        },
        {
          "id": "MDRW2100035112.1.26",
          "form": "유행이래요",
          "original_form": "유행이래요",
          "speaker_id": "1",
          "time": "20210829 03:01"
        },
        {
          "id": "MDRW2100035112.1.27",
          "form": "전 참 유행을 몰라요ㅠㅠ",
          "original_form": "전 참 유행을 몰라요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 03:01"
        },
        {
          "id": "MDRW2100035112.1.28",
          "form": "앗ㅋㅋㅋㅋ 저도 이런거 잘 모르는데",
          "original_form": "앗ㅋㅋㅋㅋ 저도 이런거 잘 모르는데",
          "speaker_id": "1",
          "time": "20210829 03:02"
        },
        {
          "id": "MDRW2100035112.1.29",
          "form": "어떤비쥬얼일지는 상상이가네요 ㅋㅋㅋ",
          "original_form": "어떤비쥬얼일지는 상상이가네요 ㅋㅋㅋ",
          "speaker_id": "2",
