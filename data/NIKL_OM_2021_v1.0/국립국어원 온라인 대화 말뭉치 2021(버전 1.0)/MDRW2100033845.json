      "utterance": [
        {
          "id": "MDRW2100033845.1.1",
          "form": "비오나요???",
          "original_form": "비오나요???",
          "speaker_id": "2",
          "time": "20210825 12:15"
        },
        {
          "id": "MDRW2100033845.1.2",
          "form": "여긴 비 안와요!",
          "original_form": "여긴 비 안와요!",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100033845.1.3",
          "form": "강화도는 비 엄청 많이 온다고 강화도 친구가 그러더라구요",
          "original_form": "강화도는 비 엄청 많이 온다고 강화도 친구가 그러더라구요",
          "speaker_id": "1",
          "time": "20210825 12:21"
        },
        {
          "id": "MDRW2100033845.1.4",
          "form": "여기도 이제",
          "original_form": "여기도 이제",
          "speaker_id": "2",
          "time": "20210825 12:24"
        },
        {
          "id": "MDRW2100033845.1.5",
          "form": "해가 떠요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "해가 떠요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:24"
        },
        {
          "id": "MDRW2100033845.1.6",
          "form": "폭풍이 남쪽엔 직격탄이었다던데",
          "original_form": "폭풍이 남쪽엔 직격탄이었다던데",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100033845.1.7",
          "form": "여긴 스쳐지나가네요",
          "original_form": "여긴 스쳐지나가네요",
          "speaker_id": "1",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100033845.1.8",
          "form": "갑자기 비와여",
          "original_form": "갑자기 비와여",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100033845.1.9",
          "form": "엄청와요ㅋㅋㅋㅋㅋ",
          "original_form": "엄청와요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100033845.1.10",
          "form": "헉 그래요? 여기 무지 더워요",
          "original_form": "헉 그래요? 여기 무지 더워요",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100033845.1.11",
          "form": "날씨가 요상하네요",
          "original_form": "날씨가 요상하네요",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100033845.1.12",
          "form": "진짜요ㅋㅋㅋㅋㅋ",
          "original_form": "진짜요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033845.1.13",
          "form": "지구온난화 때뭉에",
          "original_form": "지구온난화 때뭉에",
          "speaker_id": "2",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033845.1.14",
          "form": "정말 날씨 미쳤어요",
          "original_form": "정말 날씨 미쳤어요",
          "speaker_id": "2",
          "time": "20210825 14:45"
        },
        {
          "id": "MDRW2100033845.1.15",
          "form": "매미소리도 잘 안나지 않나요",
          "original_form": "매미소리도 잘 안나지 않나요",
          "speaker_id": "1",
          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100033845.1.16",
          "form": "매미는 죽고",
          "original_form": "매미는 죽고",
          "speaker_id": "2",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100033845.1.17",
          "form": "모기가 부활한 거 같아요",
          "original_form": "모기가 부활한 거 같아요",
          "speaker_id": "2",
          "time": "20210825 15:20"
        },
        {
          "id": "MDRW2100033845.1.18",
          "form": "아 맞아요 이번여름 모기가 진짜 너무 심해요",
          "original_form": "아 맞아요 이번여름 모기가 진짜 너무 심해요",
          "speaker_id": "1",
          "time": "20210825 15:54"
        },
        {
          "id": "MDRW2100033845.1.19",
          "form": "네 안보이는 곳에서",
          "original_form": "네 안보이는 곳에서",
          "speaker_id": "2",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100033845.1.20",
          "form": "저를 물더라구요 ㅎㅎ",
          "original_form": "저를 물더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100033845.1.21",
          "form": "홈매트 맨날 키고잇어요 ㅋㅋㅋ",
          "original_form": "홈매트 맨날 키고잇어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100033845.1.22",
          "form": "ㅋㅋ ㅋㅋㅋㅋ 저도 하나",
          "original_form": "ㅋㅋ ㅋㅋㅋㅋ 저도 하나",
          "speaker_id": "2",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100033845.1.23",
          "form": "장만해야겠어요",
          "original_form": "장만해야겠어요",
          "speaker_id": "2",
          "time": "20210826 12:34"
        }
