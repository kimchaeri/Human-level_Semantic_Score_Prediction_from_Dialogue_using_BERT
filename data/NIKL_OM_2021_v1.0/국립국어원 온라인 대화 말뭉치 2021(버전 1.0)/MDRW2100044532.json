        {
          "id": "MDRW2100044532.1.1",
          "form": "108부작인데 시청률이 최고",
          "original_form": "108부작인데 시청률이 최고",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044532.1.2",
          "form": "얼마나 나왔어요?",
          "original_form": "얼마나 나왔어요?",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044532.1.3",
          "form": "35.9%",
          "original_form": "35.9%",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044532.1.4",
          "form": "와 진짜 많이 나왔네요",
          "original_form": "와 진짜 많이 나왔네요",
          "speaker_id": "1",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044532.1.5",
          "form": "2019년도 방영작인데",
          "original_form": "2019년도 방영작인데",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044532.1.6",
          "form": "얼마 안됐네요. 제목이 들어본든 했어요",
          "original_form": "얼마 안됐네요. 제목이 들어본든 했어요",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044532.1.7",
          "form": "애증의 관계가 돼버린 네 모녀의 이야기래",
          "original_form": "애증의 관계가 돼버린 네 모녀의 이야기래",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044532.1.8",
          "form": "애증의 관계라ㅋ 네모녀가 누구예요?",
          "original_form": "애증의 관계라ㅋ 네모녀가 누구예요?",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044532.1.9",
          "form": "나두 들어보고는 안봄",
          "original_form": "나두 들어보고는 안봄",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044532.1.10",
          "form": "김혜숙 김소연",
          "original_form": "김혜숙 김소연",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044532.1.11",
          "form": "저도 일일은 거의 안봐요",
          "original_form": "저도 일일은 거의 안봐요",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044532.1.12",
          "form": "김소연도 나왔어요?",
          "original_form": "김소연도 나왔어요?",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044532.1.13",
          "form": "엉 유선도 나왔네",
          "original_form": "엉 유선도 나왔네",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044532.1.14",
          "form": "출연진 빵빵하네",
