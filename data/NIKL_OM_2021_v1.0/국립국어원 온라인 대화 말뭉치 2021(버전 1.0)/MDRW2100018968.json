      "utterance": [
        {
          "id": "MDRW2100018968.1.1",
          "form": "요즘 날씨 어떻게 생각해",
          "original_form": "요즘 날씨 어떻게 생각해",
          "speaker_id": "1",
          "time": "20210827 14:08"
        },
        {
          "id": "MDRW2100018968.1.2",
          "form": "날씨는 최고야Vv 비가 내려서 그렇지",
          "original_form": "날씨는 최고야Vv 비가 내려서 그렇지",
          "speaker_id": "2",
          "time": "20210827 14:28"
        },
        {
          "id": "MDRW2100018968.1.3",
          "form": "지금도 비와??",
          "original_form": "지금도 비와??",
          "speaker_id": "1",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100018968.1.4",
          "form": "지금은 안온다!",
          "original_form": "지금은 안온다!",
          "speaker_id": "2",
          "time": "20210901 16:32"
        },
        {
          "id": "MDRW2100018968.1.5",
          "form": "마자 그 대신 쌀쌀해",
          "original_form": "마자 그 대신 쌀쌀해",
          "speaker_id": "1",
          "time": "20210901 19:52"
        },
        {
          "id": "MDRW2100018968.1.6",
          "form": "감기 조심해랑",
          "original_form": "감기 조심해랑",
          "speaker_id": "1",
          "time": "20210901 19:52"
        },
        {
          "id": "MDRW2100018968.1.7",
          "form": "따뜻하게 입어",
          "original_form": "따뜻하게 입어",
          "speaker_id": "1",
          "time": "20210901 19:52"
        },
        {
          "id": "MDRW2100018968.1.8",
          "form": "자꾸 까먹는다...미안 너도 따숩게입어라",
          "original_form": "자꾸 까먹는다...미안 너도 따숩게입어라",
          "speaker_id": "2",
          "time": "20210906 15:53"
        },
        {
          "id": "MDRW2100018968.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:53"
        },
        {
          "id": "MDRW2100018968.1.10",
          "form": "가을옷 뭐사지",
          "original_form": "가을옷 뭐사지",
          "speaker_id": "1",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100018968.1.11",
          "form": "뭐 사고 싶은데?",
          "original_form": "뭐 사고 싶은데?",
          "speaker_id": "2",
          "time": "20210906 15:54"
        },
        {
          "id": "MDRW2100018968.1.12",
          "form": "차콜 슬렉스랑 인디핑크 니트",
          "original_form": "차콜 슬렉스랑 인디핑크 니트",
          "speaker_id": "1",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100018968.1.13",
          "form": "오  좋다 좋다",
          "original_form": "오  좋다 좋다",
          "speaker_id": "2",
          "time": "20210906 15:56"
        },
        {
          "id": "MDRW2100018968.1.14",
          "form": "후후 다 사고싶다",
          "original_form": "후후 다 사고싶다",
          "speaker_id": "1",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100018968.1.15",
          "form": "질러버려 이얍",
          "original_form": "질러버려 이얍",
          "speaker_id": "1",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100018968.1.16",
          "form": "오늘 날씨 어땠어?",
          "original_form": "오늘 날씨 어땠어?",
          "speaker_id": "1",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100018968.1.17",
          "form": "하늘이 흐린게 곧 비가 올거 같네",
          "original_form": "하늘이 흐린게 곧 비가 올거 같네",
          "speaker_id": "2",
          "time": "20210906 17:08"
        },
        {
          "id": "MDRW2100018968.1.18",
          "form": "비가 오는 날엔 나를 찾아와~",
          "original_form": "비가 오는 날엔 나를 찾아와~",
          "speaker_id": "1",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100018968.1.19",
          "form": "비오면 습해져서... 흠",
          "original_form": "비오면 습해져서... 흠",
