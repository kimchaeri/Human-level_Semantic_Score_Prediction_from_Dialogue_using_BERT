      "utterance": [
        {
          "id": "MDRW2100029688.1.1",
          "form": "오늘은 엄마보러 가요 ㅎㅎ",
          "original_form": "오늘은 엄마보러 가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100029688.1.2",
          "form": "본가 가서 집밥 먹게요",
          "original_form": "본가 가서 집밥 먹게요",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100029688.1.3",
          "form": "와이프가 맛있는거 해줘도 엄마밥은 또 다르다 하더라구요 ㅋㅋㅋ",
          "original_form": "와이프가 맛있는거 해줘도 엄마밥은 또 다르다 하더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029688.1.4",
          "form": "와이프가 맛있는거를 안해줌니다",
          "original_form": "와이프가 맛있는거를 안해줌니다",
          "speaker_id": "2",
          "time": "20210903 18:56"
        },
        {
          "id": "MDRW2100029688.1.5",
          "form": "아... 그렇다면 자급자족밖에 없죠...",
          "original_form": "아... 그렇다면 자급자족밖에 없죠...",
          "speaker_id": "1",
          "time": "20210903 20:04"
        },
        {
          "id": "MDRW2100029688.1.6",
          "form": "제가 합니다 하하",
          "original_form": "제가 합니다 하하",
          "speaker_id": "2",
          "time": "20210903 21:04"
        },
        {
          "id": "MDRW2100029688.1.7",
          "form": "요리 잘하시겠어요 ㅋㅋㅋ",
          "original_form": "요리 잘하시겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100029688.1.8",
          "form": "그냥 가열 위주....",
          "original_form": "그냥 가열 위주....",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100029688.1.9",
          "form": "밀키트....",
          "original_form": "밀키트....",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100029688.1.10",
          "form": "입니다ㅋㅋ",
          "original_form": "입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:46"
        },
        {
          "id": "MDRW2100029688.1.11",
          "form": "밀키트 괜찮았던거 뭐있나요!",
          "original_form": "밀키트 괜찮았던거 뭐있나요!",
          "speaker_id": "1",
          "time": "20210903 21:56"
        },
        {
          "id": "MDRW2100029688.1.12",
          "form": "짬뽕순두부가",
          "original_form": "짬뽕순두부가",
          "speaker_id": "2",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100029688.1.13",
          "form": "진짜 음식점 뺨 치는듯요",
          "original_form": "진짜 음식점 뺨 치는듯요",
          "speaker_id": "2",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100029688.1.14",
          "form": "그게밀키트로나오나요?",
          "original_form": "그게밀키트로나오나요?",
          "speaker_id": "1",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100029688.1.15",
          "form": "어디서 팔아요?",
          "original_form": "어디서 팔아요?",
          "speaker_id": "1",
          "time": "20210903 22:31"
        },
        {
          "id": "MDRW2100029688.1.16",
          "form": "네 프레시지? 온라인",
          "original_form": "네 프레시지? 온라인",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100029688.1.17",
          "form": "여기꺼 진짜 맛있어요",
          "original_form": "여기꺼 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210903 22:33"
        },
        {
          "id": "MDRW2100029688.1.18",
          "form": "짬뽕순두부 강추",
          "original_form": "짬뽕순두부 강추",
          "speaker_id": "2",
          "time": "20210903 22:34"
        },
        {
          "id": "MDRW2100029688.1.19",
          "form": "오오 진짜 사먹어봐야겠어요!",
          "original_form": "오오 진짜 사먹어봐야겠어요!",
          "speaker_id": "1",
          "time": "20210903 22:35"
        },
        {
          "id": "MDRW2100029688.1.20",
          "form": "진짜 무료배송 금액 채우려고 샀던게",
          "original_form": "진짜 무료배송 금액 채우려고 샀던게",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100029688.1.21",
          "form": "신의 한수.....",
          "original_form": "신의 한수.....",
          "speaker_id": "2",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100029688.1.22",
          "form": "쿠팡에있네요! 많이 매워요?",
          "original_form": "쿠팡에있네요! 많이 매워요?",
          "speaker_id": "1",
          "time": "20210903 23:08"
        },
        {
          "id": "MDRW2100029688.1.23",
          "form": "음 먹을만한데",
          "original_form": "음 먹을만한데",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100029688.1.24",
          "form": "제가 맵고수...",
          "original_form": "제가 맵고수...",
          "speaker_id": "2",
          "time": "20210903 23:13"
        },
        {
          "id": "MDRW2100029688.1.25",
          "form": "아하. .  신라면과 비교하면?",
          "original_form": "아하. .  신라면과 비교하면?",
          "speaker_id": "1",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100029688.1.26",
          "form": "그정도 비슷할거 같은데요",
          "original_form": "그정도 비슷할거 같은데요",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100029688.1.27",
          "form": "약간 위?",
          "original_form": "약간 위?",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100029688.1.28",
          "form": "오오... 일단시도는 해볼만 하겠군요",
          "original_form": "오오... 일단시도는 해볼만 하겠군요",
          "speaker_id": "1",
          "time": "20210903 23:18"
        }
      ]
    }
  ]
