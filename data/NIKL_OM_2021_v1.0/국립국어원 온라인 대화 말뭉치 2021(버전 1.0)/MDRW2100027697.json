      "utterance": [
        {
          "id": "MDRW2100027697.1.1",
          "form": "예보 보시고 비 안오는 날로 예약하셔요!",
          "original_form": "예보 보시고 비 안오는 날로 예약하셔요!",
          "speaker_id": "2",
          "time": "20210902 14:19"
        },
        {
          "id": "MDRW2100027697.1.2",
          "form": "네네 좋아요",
          "original_form": "네네 좋아요",
          "speaker_id": "1",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027697.1.3",
          "form": "서해가좋은가요?",
          "original_form": "서해가좋은가요?",
          "speaker_id": "1",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027697.1.4",
          "form": "쭈꾸미랑 갑오징어는 서해에서만 잡는걸로 알아요!",
          "original_form": "쭈꾸미랑 갑오징어는 서해에서만 잡는걸로 알아요!",
          "speaker_id": "2",
          "time": "20210902 14:20"
        },
        {
          "id": "MDRW2100027697.1.5",
          "form": "힘들진않나요?ㅠㅠ",
          "original_form": "힘들진않나요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 14:21"
        },
        {
          "id": "MDRW2100027697.1.6",
          "form": "종일배도 있는데 보통 오전배 오후배 나뉘고요",
          "original_form": "종일배도 있는데 보통 오전배 오후배 나뉘고요",
          "speaker_id": "2",
          "time": "20210902 14:22"
        },
        {
          "id": "MDRW2100027697.1.7",
          "form": "3시간 이내로 하는데",
          "original_form": "3시간 이내로 하는데",
          "speaker_id": "2",
          "time": "20210902 14:22"
        },
        {
          "id": "MDRW2100027697.1.8",
          "form": "중간에 주꾸미 넣은 라면도 보통 끓여주시고요ㅎㅎ",
          "original_form": "중간에 주꾸미 넣은 라면도 보통 끓여주시고요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027697.1.9",
          "form": "나들이로 갈만합니다",
          "original_form": "나들이로 갈만합니다",
          "speaker_id": "2",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027697.1.10",
          "form": "오호 좋아요",
          "original_form": "오호 좋아요",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027697.1.11",
          "form": "아들이 좋아할듯요",
          "original_form": "아들이 좋아할듯요",
          "speaker_id": "1",
          "time": "20210902 14:23"
        },
        {
          "id": "MDRW2100027697.1.12",
          "form": "잡으신 주꾸미 가지고 양념집 가시면 바로 샤브샤브도 해드실 수 있어요",
          "original_form": "잡으신 주꾸미 가지고 양념집 가시면 바로 샤브샤브도 해드실 수 있어요",
          "speaker_id": "2",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100027697.1.13",
          "form": "파는거랑은 맛이 다릅니다",
          "original_form": "파는거랑은 맛이 다릅니다",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027697.1.14",
          "form": "제가 주꾸미 머리는 맛이 비리게 느껴져서 못먹었는데 바로 잡은건 맛있게 먹었어요",
          "original_form": "제가 주꾸미 머리는 맛이 비리게 느껴져서 못먹었는데 바로 잡은건 맛있게 먹었어요",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100027697.1.15",
          "form": "오호 한번 해볼께요",
          "original_form": "오호 한번 해볼께요",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027697.1.16",
          "form": "새로운 경험",
          "original_form": "새로운 경험",
          "speaker_id": "1",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100027697.1.17",
          "form": "응원합니다!",
          "original_form": "응원합니다!",
          "speaker_id": "2",
          "time": "20210902 15:05"
        },
        {
          "id": "MDRW2100027697.1.18",
          "form": "네네 낚시해보고싶어서요",
          "original_form": "네네 낚시해보고싶어서요",
          "speaker_id": "1",
          "time": "20210902 15:06"
        },
        {
          "id": "MDRW2100027697.1.19",
          "form": "겨울에 강원도쪽에서",
          "original_form": "겨울에 강원도쪽에서",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027697.1.20",
          "form": "가자미 낚시도 가족단위로 많이 간다고 들었어요",
          "original_form": "가자미 낚시도 가족단위로 많이 간다고 들었어요",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027697.1.21",
          "form": "그것도 재밌겠어요",
          "original_form": "그것도 재밌겠어요",
          "speaker_id": "1",
          "time": "20210902 15:07"
        }
      ]
