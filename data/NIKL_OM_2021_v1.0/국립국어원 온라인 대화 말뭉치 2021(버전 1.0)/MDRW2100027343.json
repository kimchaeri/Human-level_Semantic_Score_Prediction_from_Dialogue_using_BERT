      "utterance": [
        {
          "id": "MDRW2100027343.1.1",
          "form": "개봉예정영화중 재밌는거 있을까요",
          "original_form": "개봉예정영화중 재밌는거 있을까요",
          "speaker_id": "1",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100027343.1.2",
          "form": "개봉예정영화 볼꺼있나요?",
          "original_form": "개봉예정영화 볼꺼있나요?",
          "speaker_id": "1",
          "time": "20210902 11:48"
        },
        {
          "id": "MDRW2100027343.1.3",
          "form": "9월말쯤 007개봉하는것같네요 보고싶은영화에요",
          "original_form": "9월말쯤 007개봉하는것같네요 보고싶은영화에요",
          "speaker_id": "2",
          "time": "20210902 11:50"
        },
        {
          "id": "MDRW2100027343.1.4",
          "form": "오 옛날 그 공공칠?",
          "original_form": "오 옛날 그 공공칠?",
          "speaker_id": "1",
          "time": "20210902 11:51"
        },
        {
          "id": "MDRW2100027343.1.5",
          "form": "네 시리즈에요",
          "original_form": "네 시리즈에요",
          "speaker_id": "2",
          "time": "20210902 12:08"
        },
        {
          "id": "MDRW2100027343.1.6",
          "form": "대박 엄청 옛날꺼 아닌가요",
          "original_form": "대박 엄청 옛날꺼 아닌가요",
          "speaker_id": "1",
          "time": "20210902 12:12"
        },
        {
          "id": "MDRW2100027343.1.7",
          "form": "시작은 오래전에 한 것 같은데 주인공이계속바뀌나봐요",
          "original_form": "시작은 오래전에 한 것 같은데 주인공이계속바뀌나봐요",
          "speaker_id": "2",
          "time": "20210902 12:30"
        },
        {
          "id": "MDRW2100027343.1.8",
          "form": "내용은 안이어지겠죠?",
          "original_form": "내용은 안이어지겠죠?",
          "speaker_id": "1",
          "time": "20210902 12:38"
        },
        {
          "id": "MDRW2100027343.1.9",
          "form": "그럴거에요 액션보는거죠",
          "original_form": "그럴거에요 액션보는거죠",
          "speaker_id": "2",
          "time": "20210902 12:52"
        },
        {
          "id": "MDRW2100027343.1.10",
          "form": "액션 시원한거 보고싶네요",
          "original_form": "액션 시원한거 보고싶네요",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027343.1.11",
          "form": "리암리슨같은",
          "original_form": "리암리슨같은",
          "speaker_id": "1",
          "time": "20210902 12:53"
        },
        {
          "id": "MDRW2100027343.1.12",
          "form": "맞아요 테이큰 진짜 재밌었는데 그뒤로 리암니슨 액션은 꼭 보려고하는것같아요",
          "original_form": "맞아요 테이큰 진짜 재밌었는데 그뒤로 리암니슨 액션은 꼭 보려고하는것같아요",
          "speaker_id": "2",
          "time": "20210902 13:10"
        },
        {
          "id": "MDRW2100027343.1.13",
          "form": "믿고보는분 ㅋㅋ",
          "original_form": "믿고보는분 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100027343.1.14",
          "form": "나이도 있으실텐데",
          "original_form": "나이도 있으실텐데",
          "speaker_id": "1",
          "time": "20210902 13:13"
        },
        {
          "id": "MDRW2100027343.1.15",
          "form": "그러게요 요즘은 과한 액션은 많이 안하시는듯보였어요",
          "original_form": "그러게요 요즘은 과한 액션은 많이 안하시는듯보였어요",
          "speaker_id": "2",
          "time": "20210902 13:23"
        },
        {
          "id": "MDRW2100027343.1.16",
          "form": "스릴러도 보고싶어요",
          "original_form": "스릴러도 보고싶어요",
          "speaker_id": "1",
          "time": "20210902 13:24"
        },
        {
          "id": "MDRW2100027343.1.17",
          "form": "저도요 스릴러 장르도 좋아해요",
          "original_form": "저도요 스릴러 장르도 좋아해요",
          "speaker_id": "2",
          "time": "20210902 13:25"
        },
        {
          "id": "MDRW2100027343.1.18",
          "form": "전 잔인한거 잘본다는",
          "original_form": "전 잔인한거 잘본다는",
          "speaker_id": "1",
          "time": "20210902 13:26"
        },
        {
          "id": "MDRW2100027343.1.19",
          "form": "저도 잔인한거 잘봐요",
          "original_form": "저도 잔인한거 잘봐요",
          "speaker_id": "2",
          "time": "20210902 13:29"
        },
        {
          "id": "MDRW2100027343.1.20",
          "form": "잘 통하네요",
          "original_form": "잘 통하네요",
          "speaker_id": "1",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027343.1.21",
          "form": "랑종보셨나요",
          "original_form": "랑종보셨나요",
          "speaker_id": "1",
          "time": "20210902 13:30"
        },
        {
          "id": "MDRW2100027343.1.22",
          "form": "랑종 어때요",
          "original_form": "랑종 어때요",
          "speaker_id": "1",
          "time": "20210902 13:31"
        }
      ]
