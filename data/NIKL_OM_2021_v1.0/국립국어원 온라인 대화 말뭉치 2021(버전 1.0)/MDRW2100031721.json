      "utterance": [
        {
          "id": "MDRW2100031721.1.1",
          "form": "나이는 상관없는데 4살까지는",
          "original_form": "나이는 상관없는데 4살까지는",
          "speaker_id": "2",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100031721.1.2",
          "form": "직업은 저랑 비슷하거나 더 낫거나 했음 좋겠죠",
          "original_form": "직업은 저랑 비슷하거나 더 낫거나 했음 좋겠죠",
          "speaker_id": "2",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100031721.1.3",
          "form": "종교는 불교였음 좋겠고 아님 천주교",
          "original_form": "종교는 불교였음 좋겠고 아님 천주교",
          "speaker_id": "2",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100031721.1.4",
          "form": "기독교는 아니었음 하네요",
          "original_form": "기독교는 아니었음 하네요",
          "speaker_id": "2",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100031721.1.5",
          "form": "기독교만",
          "original_form": "기독교만",
          "speaker_id": "2",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100031721.1.6",
          "form": "악",
          "original_form": "악",
          "speaker_id": "1",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100031721.1.7",
          "form": "조건이 많네요",
          "original_form": "조건이 많네요",
          "speaker_id": "1",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100031721.1.8",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031721.1.9",
          "form": "제 주변에 있나 했는데",
          "original_form": "제 주변에 있나 했는데",
          "speaker_id": "1",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031721.1.10",
          "form": "누구에요",
          "original_form": "누구에요",
          "speaker_id": "2",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031721.1.11",
          "form": "나은 직업이라면?",
          "original_form": "나은 직업이라면?",
          "speaker_id": "1",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031721.1.12",
          "form": "공무원이나 공기업이나 걍 생활패턴 비슷한 사람으로요",
          "original_form": "공무원이나 공기업이나 걍 생활패턴 비슷한 사람으로요",
          "speaker_id": "2",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031721.1.13",
          "form": "제 주변엔 없네요",
          "original_form": "제 주변엔 없네요",
          "speaker_id": "1",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031721.1.14",
          "form": "그럼 대구오셔야겠어여",
          "original_form": "그럼 대구오셔야겠어여",
          "speaker_id": "1",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031721.1.15",
          "form": "그분은 뭐하시나요",
          "original_form": "그분은 뭐하시나요",
          "speaker_id": "2",
          "time": "20210904 23:51"
        },
        {
          "id": "MDRW2100031721.1.16",
          "form": "농아짓는분도 있고 회사분돛있어요",
          "original_form": "농아짓는분도 있고 회사분돛있어요",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031721.1.17",
          "form": "회사분은 결혼은 바로 가능한데",
          "original_form": "회사분은 결혼은 바로 가능한데",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031721.1.18",
          "form": "모르겠내요 ㅋㅋ",
          "original_form": "모르겠내요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031721.1.19",
          "form": "회사원분은 매력있나요",
          "original_form": "회사원분은 매력있나요",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031721.1.20",
          "form": "외모 성격 궁금",
          "original_form": "외모 성격 궁금",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031721.1.21",
          "form": "ㅋㅋㅋ긍쎄요",
          "original_form": "ㅋㅋㅋ긍쎄요",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031721.1.22",
          "form": "소개못시켜줄듯하내요",
          "original_form": "소개못시켜줄듯하내요",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031721.1.23",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031721.1.24",
          "form": "욕먹을까봐",
          "original_form": "욕먹을까봐",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100031721.1.25",
          "form": "그 오빤 바로 결혼해야 하는데",
          "original_form": "그 오빤 바로 결혼해야 하는데",
          "speaker_id": "1",
          "time": "20210904 23:53"
        },
        {
          "id": "MDRW2100031721.1.26",
          "form": "결혼하고싶어하거든요",
          "original_form": "결혼하고싶어하거든요",
          "speaker_id": "1",
          "time": "20210904 23:53"
        }
      ]
    }
