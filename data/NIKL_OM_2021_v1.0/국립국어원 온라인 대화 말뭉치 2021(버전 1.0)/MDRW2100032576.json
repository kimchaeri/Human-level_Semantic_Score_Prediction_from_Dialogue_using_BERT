      "utterance": [
        {
          "id": "MDRW2100032576.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100032576.1.2",
          "form": "영화 좋아하시나요?",
          "original_form": "영화 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100032576.1.3",
          "form": "안녕하세여 영화 좋아하죠!",
          "original_form": "안녕하세여 영화 좋아하죠!",
          "speaker_id": "2",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032576.1.4",
          "form": "최근에 뭐 보셨어요?",
          "original_form": "최근에 뭐 보셨어요?",
          "speaker_id": "2",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100032576.1.5",
          "form": "최근에 극장에서 본거는 썰이요!",
          "original_form": "최근에 극장에서 본거는 썰이요!",
          "speaker_id": "1",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100032576.1.6",
          "form": "영화관에서 최근에 보신거 있나요?",
          "original_form": "영화관에서 최근에 보신거 있나요?",
          "speaker_id": "1",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100032576.1.7",
          "form": "모가디슈랑 프리가이 그나마 최근에 봤어여ㅋㅋ",
          "original_form": "모가디슈랑 프리가이 그나마 최근에 봤어여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 09:53"
        },
        {
          "id": "MDRW2100032576.1.8",
          "form": "모가디슈 보셨나요?",
          "original_form": "모가디슈 보셨나요?",
          "speaker_id": "2",
          "time": "20210906 09:53"
        },
        {
          "id": "MDRW2100032576.1.9",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "1",
          "time": "20210906 12:27"
        },
        {
          "id": "MDRW2100032576.1.10",
          "form": "그거 재미있나요?",
          "original_form": "그거 재미있나요?",
          "speaker_id": "1",
          "time": "20210906 12:27"
        },
        {
          "id": "MDRW2100032576.1.11",
          "form": "다들 재밌다길래 봤는데 저는 별로였어요ㅜㅜ",
          "original_form": "다들 재밌다길래 봤는데 저는 별로였어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 12:48"
        },
        {
          "id": "MDRW2100032576.1.12",
          "form": "앗....ㅠ",
          "original_form": "앗....ㅠ",
          "speaker_id": "1",
          "time": "20210906 13:23"
        },
        {
          "id": "MDRW2100032576.1.13",
          "form": "프리가이는요?",
          "original_form": "프리가이는요?",
          "speaker_id": "1",
          "time": "20210906 13:23"
        },
        {
          "id": "MDRW2100032576.1.14",
          "form": "프리가이도요ㅋㅋㅋ너무..비급영화 감성이에요",
          "original_form": "프리가이도요ㅋㅋㅋ너무..비급영화 감성이에요",
          "speaker_id": "2",
          "time": "20210906 13:39"
        },
        {
          "id": "MDRW2100032576.1.15",
          "form": "아앗....ㅠ",
          "original_form": "아앗....ㅠ",
          "speaker_id": "1",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100032576.1.16",
          "form": "ㅜㅜㅋㅋㅋ좋아하는 분들은 엄청 좋아하시던데 저랑은 안맞았네요",
          "original_form": "ㅜㅜㅋㅋㅋ좋아하는 분들은 엄청 좋아하시던데 저랑은 안맞았네요",
          "speaker_id": "2",
          "time": "20210906 15:36"
        },
        {
          "id": "MDRW2100032576.1.17",
          "form": "그럴수있죠ㅋㄱㅋ",
          "original_form": "그럴수있죠ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100032576.1.18",
          "form": "제일 재미있게 보신건 어떤건가요?",
          "original_form": "제일 재미있게 보신건 어떤건가요?",
          "speaker_id": "1",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100032576.1.19",
          "form": "좀 된건데 캐시트럭이요ㅋㅋ기대 하나도 안하고 봤는데 진짜 재밌었어요!!!",
          "original_form": "좀 된건데 캐시트럭이요ㅋㅋ기대 하나도 안하고 봤는데 진짜 재밌었어요!!!",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100032576.1.20",
          "form": "무슨 내용인가요?",
          "original_form": "무슨 내용인가요?",
          "speaker_id": "1",
          "time": "20210906 21:23"
        },
        {
          "id": "MDRW2100032576.1.21",
          "form": "액션인데 제이슨스타뎀 아세요?",
          "original_form": "액션인데 제이슨스타뎀 아세요?",
          "speaker_id": "2",
          "time": "20210906 21:26"
        },
        {
          "id": "MDRW2100032576.1.22",
          "form": "앗 아니요!",
          "original_form": "앗 아니요!",
