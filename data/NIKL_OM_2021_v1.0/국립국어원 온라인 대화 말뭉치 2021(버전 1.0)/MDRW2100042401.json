      "utterance": [
        {
          "id": "MDRW2100042401.1.1",
          "form": "붕어빵좋아하세요?",
          "original_form": "붕어빵좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042401.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042401.1.3",
          "form": "팥붕?슈붕?",
          "original_form": "팥붕?슈붕?",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042401.1.4",
          "form": "저는 팥붕어빵이 더 맛있더라고요",
          "original_form": "저는 팥붕어빵이 더 맛있더라고요",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042401.1.5",
          "form": "오 저도요!!",
          "original_form": "오 저도요!!",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042401.1.6",
          "form": "슈크림도 맛있나요?",
          "original_form": "슈크림도 맛있나요?",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042401.1.7",
          "form": "저 슈붕은 안먹어봤어요",
          "original_form": "저 슈붕은 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042401.1.8",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042401.1.9",
          "form": "아뇨 ㅎ 저도 안먹어 봤네요 ㅎ",
          "original_form": "아뇨 ㅎ 저도 안먹어 봤네요 ㅎ",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042401.1.10",
          "form": "ㅋㅋㅋㅋ가격은 슈붕이더 비싸더라고요",
          "original_form": "ㅋㅋㅋㅋ가격은 슈붕이더 비싸더라고요",
          "speaker_id": "1",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042401.1.11",
          "form": "맞아요 우리동네도 슈크림이 조금더비싸요",
          "original_form": "맞아요 우리동네도 슈크림이 조금더비싸요",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042401.1.12",
          "form": "갯수차이가 두갠가",
          "original_form": "갯수차이가 두갠가",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042401.1.13",
          "form": "같은값에",
          "original_form": "같은값에",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042401.1.14",
          "form": "그러더라고요",
          "original_form": "그러더라고요",
          "speaker_id": "1",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042401.1.15",
          "form": "다음에 슈크림도 먹어봐야겠네요",
          "original_form": "다음에 슈크림도 먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210906 22:00"
        },
        {
          "id": "MDRW2100042401.1.16",
          "form": "저도궁금해서 다음엔 먹어보려고요!",
          "original_form": "저도궁금해서 다음엔 먹어보려고요!",
          "speaker_id": "1",
