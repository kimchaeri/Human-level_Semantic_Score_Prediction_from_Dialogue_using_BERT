      "utterance": [
        {
          "id": "MDRW2100034318.1.1",
          "form": "크림바르고나서 화장하면",
          "original_form": "크림바르고나서 화장하면",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100034318.1.2",
          "form": "안밀리나요?",
          "original_form": "안밀리나요?",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100034318.1.3",
          "form": "소량.바르면 전 괜찮더라구요!",
          "original_form": "소량.바르면 전 괜찮더라구요!",
          "speaker_id": "1",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100034318.1.4",
          "form": "제가 꾸덕한 크림을 좋아해서 그런거만 바르는데도 괜찮았어요",
          "original_form": "제가 꾸덕한 크림을 좋아해서 그런거만 바르는데도 괜찮았어요",
          "speaker_id": "1",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100034318.1.5",
          "form": "아 그래요?\n전 양조절을 안해서 밀렸던거였군요ㅋㅋ",
          "original_form": "아 그래요?\n전 양조절을 안해서 밀렸던거였군요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100034318.1.6",
          "form": "혹시 파운데이션 밀면서 바르시나요??",
          "original_form": "혹시 파운데이션 밀면서 바르시나요??",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100034318.1.7",
          "form": "톡톡 바르는데도",
          "original_form": "톡톡 바르는데도",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100034318.1.8",
          "form": "나중에 밀리더라구요ㅠ",
          "original_form": "나중에 밀리더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100034318.1.9",
          "form": "아 ㅜㅜ 크림 뭐쓰세요?",
          "original_form": "아 ㅜㅜ 크림 뭐쓰세요?",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100034318.1.10",
          "form": "저는 지금은 크림 안쓰고",
          "original_form": "저는 지금은 크림 안쓰고",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100034318.1.11",
          "form": "스킨로션만해요",
          "original_form": "스킨로션만해요",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100034318.1.12",
          "form": "흠 근데도 밀리세요??",
          "original_form": "흠 근데도 밀리세요??",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100034318.1.13",
          "form": "근데 썬크림 발라고 밀리더라구요",
          "original_form": "근데 썬크림 발라고 밀리더라구요",
          "speaker_id": "2",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100034318.1.14",
          "form": "아니요 스킨로션만 하면 안밀려요",
          "original_form": "아니요 스킨로션만 하면 안밀려요",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100034318.1.15",
          "form": "흠 왜그럴까요 ㅜㅜ",
          "original_form": "흠 왜그럴까요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100034318.1.16",
          "form": "전 예전에 쿠션말고 크림파데썼을때 브러쉬로 바르면 밀리더라구요",
          "original_form": "전 예전에 쿠션말고 크림파데썼을때 브러쉬로 바르면 밀리더라구요",
          "speaker_id": "1",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100034318.1.17",
          "form": "ㅋㅋ그러게요ㅋ다음엔  양조절을 한번해봐야겠어요ㅋㅋ",
          "original_form": "ㅋㅋ그러게요ㅋ다음엔  양조절을 한번해봐야겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100034318.1.18",
          "form": "밀리면 신경쓰이져 ㅜㅜ",
          "original_form": "밀리면 신경쓰이져 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100034318.1.19",
          "form": "바르는 방법따라서도 밀리는군요",
          "original_form": "바르는 방법따라서도 밀리는군요",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100034318.1.20",
          "form": "마자요 엄청 신경쓰여요ㅠ",
          "original_form": "마자요 엄청 신경쓰여요ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100034318.1.21",
          "form": "앜ㅋㅋ 공감합니다",
          "original_form": "앜ㅋㅋ 공감합니다",
          "speaker_id": "1",
