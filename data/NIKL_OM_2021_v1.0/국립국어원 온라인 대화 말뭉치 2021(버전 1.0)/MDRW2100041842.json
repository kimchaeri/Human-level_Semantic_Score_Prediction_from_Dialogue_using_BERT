      "utterance": [
        {
          "id": "MDRW2100041842.1.1",
          "form": "주로 요리는 가족중에 다른분이 하시나요",
          "original_form": "주로 요리는 가족중에 다른분이 하시나요",
          "speaker_id": "1",
          "time": "20210906 16:33"
        },
        {
          "id": "MDRW2100041842.1.2",
          "form": "네 보통 그런편이죠",
          "original_form": "네 보통 그런편이죠",
          "speaker_id": "2",
          "time": "20210906 16:34"
        },
        {
          "id": "MDRW2100041842.1.3",
          "form": "먹고싶은거 생기면 각자 하던가 사던가 하기도 하고요",
          "original_form": "먹고싶은거 생기면 각자 하던가 사던가 하기도 하고요",
          "speaker_id": "2",
          "time": "20210906 16:35"
        },
        {
          "id": "MDRW2100041842.1.4",
          "form": "아 그거 편하겠네요",
          "original_form": "아 그거 편하겠네요",
          "speaker_id": "1",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100041842.1.5",
          "form": "배달음식도 자주드시나요",
          "original_form": "배달음식도 자주드시나요",
          "speaker_id": "1",
          "time": "20210906 16:48"
        },
        {
          "id": "MDRW2100041842.1.6",
          "form": "자주 먹어요. 주말엔 거의 배달이네요ㅋㅋ",
          "original_form": "자주 먹어요. 주말엔 거의 배달이네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:49"
        },
        {
          "id": "MDRW2100041842.1.7",
          "form": "아 주로 이용하시는 배달어플같은거 있나요",
          "original_form": "아 주로 이용하시는 배달어플같은거 있나요",
          "speaker_id": "1",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100041842.1.8",
          "form": "저 배민이요. 지역앱도 있긴한데 잘 안쓰게 되네요",
          "original_form": "저 배민이요. 지역앱도 있긴한데 잘 안쓰게 되네요",
          "speaker_id": "2",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041842.1.9",
          "form": "배민은 할인같은거 많이하나봐요",
          "original_form": "배민은 할인같은거 많이하나봐요",
          "speaker_id": "1",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041842.1.10",
          "form": "주로 뭐 시켜드세요?",
          "original_form": "주로 뭐 시켜드세요?",
          "speaker_id": "1",
          "time": "20210906 18:28"
        },
        {
          "id": "MDRW2100041842.1.11",
          "form": "저 피자랑 치킨이요",
          "original_form": "저 피자랑 치킨이요",
          "speaker_id": "2",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100041842.1.12",
          "form": "피자는 토핑 다양하니까 안 질리고 먹어요ㅋㅋ",
          "original_form": "피자는 토핑 다양하니까 안 질리고 먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100041842.1.13",
          "form": "어떤 토핑을 주로 선호하시나요?",
          "original_form": "어떤 토핑을 주로 선호하시나요?",
          "speaker_id": "1",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100041842.1.14",
          "form": "전 고구마나 감자요",
          "original_form": "전 고구마나 감자요",
          "speaker_id": "1",
          "time": "20210906 18:30"
        },
        {
          "id": "MDRW2100041842.1.15",
          "form": "저는 불고기나 페퍼로니 좋아해요",
          "original_form": "저는 불고기나 페퍼로니 좋아해요",
          "speaker_id": "2",
          "time": "20210906 19:09"
        },
        {
          "id": "MDRW2100041842.1.16",
          "form": "고기종류 좋아하시는군요",
          "original_form": "고기종류 좋아하시는군요",
          "speaker_id": "1",
          "time": "20210906 19:12"
        },
        {
          "id": "MDRW2100041842.1.17",
          "form": "보통 한번시키면 몇조각드세요?",
          "original_form": "보통 한번시키면 몇조각드세요?",
          "speaker_id": "1",
          "time": "20210906 19:12"
        },
        {
          "id": "MDRW2100041842.1.18",
          "form": "얇으면 서너조각은 먹는거 같아요ㅋㅋ",
          "original_form": "얇으면 서너조각은 먹는거 같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:13"
        }
