      "utterance": [
        {
          "id": "MDRW2100036942.1.1",
          "form": "식사하셨나요??",
          "original_form": "식사하셨나요??",
          "speaker_id": "2",
          "time": "20210831 15:30"
        },
        {
          "id": "MDRW2100036942.1.2",
          "form": "넹! 후식으로 수박도 먹었슴닷",
          "original_form": "넹! 후식으로 수박도 먹었슴닷",
          "speaker_id": "1",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100036942.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100036942.1.4",
          "form": "아하~ 저는 오늘 저녁으로 갈비뜯엇어요 ㅋㅋㅋ",
          "original_form": "아하~ 저는 오늘 저녁으로 갈비뜯엇어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100036942.1.5",
          "form": "수박 요새 넘 비싸지않나요 ㅠㅠ",
          "original_form": "수박 요새 넘 비싸지않나요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100036942.1.6",
          "form": "오우 갈비 최고의 식사셨네요",
          "original_form": "오우 갈비 최고의 식사셨네요",
          "speaker_id": "1",
          "time": "20210831 22:41"
        },
        {
          "id": "MDRW2100036942.1.7",
          "form": "맞아요ㅠ 끝물이라 더 비싸지네요",
          "original_form": "맞아요ㅠ 끝물이라 더 비싸지네요",
          "speaker_id": "1",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100036942.1.8",
          "form": "맞아요 ㅋㅋㅋㅋ 그쵸 끝물인데 아직 달긴한가요??",
          "original_form": "맞아요 ㅋㅋㅋㅋ 그쵸 끝물인데 아직 달긴한가요??",
          "speaker_id": "2",
          "time": "20210831 22:42"
        },
        {
          "id": "MDRW2100036942.1.9",
          "form": "넹ㅋㅋ 꽤 괜찮더라구요!",
          "original_form": "넹ㅋㅋ 꽤 괜찮더라구요!",
          "speaker_id": "1",
          "time": "20210831 22:44"
        },
        {
          "id": "MDRW2100036942.1.10",
          "form": "만족스럽습니당ㅋㅋ",
          "original_form": "만족스럽습니당ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100036942.1.11",
          "form": "그래도 얼른 귤이 나왔으면해요",
          "original_form": "그래도 얼른 귤이 나왔으면해요",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100036942.1.12",
          "form": "저두요! 겨울에 장판누워서 귤까먹는게 제 삶의낙이랍니당ㅋㅋㅋ",
          "original_form": "저두요! 겨울에 장판누워서 귤까먹는게 제 삶의낙이랍니당ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100036942.1.13",
          "form": "꺄ㅋㅋㅋ 겨울에 필수죠~~",
          "original_form": "꺄ㅋㅋㅋ 겨울에 필수죠~~",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100036942.1.14",
          "form": "감도 먹어야하구 먹을게 또 넘치죠",
          "original_form": "감도 먹어야하구 먹을게 또 넘치죠",
          "speaker_id": "1",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100036942.1.15",
          "form": "앜마쟈요ㅋㅋㅋ 저는 감만먹으면 소화가안되서 잘못먹고있어요 ㅠㅠ",
          "original_form": "앜마쟈요ㅋㅋㅋ 저는 감만먹으면 소화가안되서 잘못먹고있어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100036942.1.16",
          "form": "앗ㅜㅠ 감이 그런게 있죠ㅠ",
          "original_form": "앗ㅜㅠ 감이 그런게 있죠ㅠ",
          "speaker_id": "1",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100036942.1.17",
          "form": "저도 조금씩 맛만봐요ㅠ",
          "original_form": "저도 조금씩 맛만봐요ㅠ",
          "speaker_id": "1",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100036942.1.18",
          "form": "아 그렇군여 ㅜㅜ 요새 감말랭이 참 많이사드시더라구요!",
          "original_form": "아 그렇군여 ㅜㅜ 요새 감말랭이 참 많이사드시더라구요!",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100036942.1.19",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100036942.1.20",
          "form": "꽤 맛있는 곳 찾아서 배송시킬까싶더라구요",
          "original_form": "꽤 맛있는 곳 찾아서 배송시킬까싶더라구요",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100036942.1.21",
          "form": "맞아요 이런건 한번사두면 말랭이라 오래두고먹더라고요~",
          "original_form": "맞아요 이런건 한번사두면 말랭이라 오래두고먹더라고요~",
          "speaker_id": "2",
          "time": "20210831 23:07"
        }
      ]
    }
