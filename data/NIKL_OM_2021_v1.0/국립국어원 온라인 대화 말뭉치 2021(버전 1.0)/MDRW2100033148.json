        {
          "id": "MDRW2100033148.1.2",
          "form": "플립 사고 싶음 ㄹㅇ",
          "original_form": "플립 사고 싶음 ㄹㅇ",
          "speaker_id": "2",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100033148.1.3",
          "form": "넘예뽀",
          "original_form": "넘예뽀",
          "speaker_id": "2",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100033148.1.4",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100033148.1.5",
          "form": "갤럭시 요즘 폰 잘뽑더라",
          "original_form": "갤럭시 요즘 폰 잘뽑더라",
          "speaker_id": "1",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100033148.1.6",
          "form": "플립은 특히 예전에 폴더폰쓰던 오타쿠 감성을 지대로 자극하는듯",
          "original_form": "플립은 특히 예전에 폴더폰쓰던 오타쿠 감성을 지대로 자극하는듯",
          "speaker_id": "1",
          "time": "20210906 17:31"
        },
        {
          "id": "MDRW2100033148.1.7",
          "form": "플립 접었다 폈다 100번 하고 싶오",
          "original_form": "플립 접었다 폈다 100번 하고 싶오",
          "speaker_id": "2",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100033148.1.8",
          "form": "플립 사면 얼굴 옆에 들고 대화할거야",
          "original_form": "플립 사면 얼굴 옆에 들고 대화할거야",
          "speaker_id": "2",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100033148.1.9",
          "form": "플꾸(플립꾸미기) 오지게 해서",
          "original_form": "플꾸(플립꾸미기) 오지게 해서",
          "speaker_id": "2",
          "time": "20210906 17:32"
        },
        {
          "id": "MDRW2100033148.1.10",
          "form": "아 개웃기다",
          "original_form": "아 개웃기다",
          "speaker_id": "1",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100033148.1.11",
          "form": "플립근데 액정 가운데 접히는 부분때문에",
          "original_form": "플립근데 액정 가운데 접히는 부분때문에",
          "speaker_id": "1",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100033148.1.12",
          "form": "쩝.. 그게 좀 아쉬운듯",
          "original_form": "쩝.. 그게 좀 아쉬운듯",
          "speaker_id": "1",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100033148.1.13",
          "form": "그리고 그걸로 리겜하면 반 접힌대 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그리고 그걸로 리겜하면 반 접힌대 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100033148.1.14",
          "form": "아마자 폴더랑은 좀 다르지",
          "original_form": "아마자 폴더랑은 좀 다르지",
          "speaker_id": "2",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100033148.1.15",
          "form": "오타쿠적 사고",
          "original_form": "오타쿠적 사고",
          "speaker_id": "2",
          "time": "20210906 17:33"
        },
        {
          "id": "MDRW2100033148.1.16",
          "form": "저.기.요",
          "original_form": "저.기.요",
          "speaker_id": "1",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100033148.1.17",
          "form": "강하게키운다..",
          "original_form": "강하게키운다..",
          "speaker_id": "1",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100033148.1.18",
          "form": "그냥 강의가 듣고 싫고 플립을 사고 싶은 사람이야",
          "original_form": "그냥 강의가 듣고 싫고 플립을 사고 싶은 사람이야",
          "speaker_id": "2",
          "time": "20210906 17:34"
        },
        {
          "id": "MDRW2100033148.1.19",
          "form": "아니근데 플립있자너",
          "original_form": "아니근데 플립있자너",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100033148.1.20",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "2",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100033148.1.21",
