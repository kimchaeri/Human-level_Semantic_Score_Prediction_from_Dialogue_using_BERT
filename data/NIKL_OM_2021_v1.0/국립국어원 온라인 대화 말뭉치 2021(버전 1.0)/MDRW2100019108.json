          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019108.1.3",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019108.1.4",
          "form": "뭐사?",
          "original_form": "뭐사?",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019108.1.5",
          "form": "자격증 교재 새로 사기엔 너무 비싸서",
          "original_form": "자격증 교재 새로 사기엔 너무 비싸서",
          "speaker_id": "1",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019108.1.6",
          "form": "그냥 당근에 올라온거 권당 5천원에 사기로 했어 ㅋㅋㅋ",
          "original_form": "그냥 당근에 올라온거 권당 5천원에 사기로 했어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019108.1.7",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019108.1.8",
          "form": "버즈도 색깔별로 많이 올라와있름",
          "original_form": "버즈도 색깔별로 많이 올라와있름",
          "speaker_id": "1",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019108.1.9",
          "form": "근데 버즈 좀 고가라",
          "original_form": "근데 버즈 좀 고가라",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019108.1.10",
          "form": "중고로 막 지르기 좀 그래",
          "original_form": "중고로 막 지르기 좀 그래",
          "speaker_id": "2",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100019108.1.11",
          "form": "책같은건 보는데에만 이상 없으면",
          "original_form": "책같은건 보는데에만 이상 없으면",
          "speaker_id": "2",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100019108.1.12",
          "form": "걍 좀 흠 있어도 받아보는데",
          "original_form": "걍 좀 흠 있어도 받아보는데",
          "speaker_id": "2",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100019108.1.13",
          "form": "버즈 새거 있어",
          "original_form": "버즈 새거 있어",
          "speaker_id": "1",
          "time": "20210827 15:23"
        },
        {
          "id": "MDRW2100019108.1.14",
          "form": "포장도 안뜯은거 팔더라고",
          "original_form": "포장도 안뜯은거 팔더라고",
          "speaker_id": "1",
          "time": "20210827 15:23"
        },
        {
          "id": "MDRW2100019108.1.15",
          "form": "머 일단은",
          "original_form": "머 일단은",
          "speaker_id": "2",
          "time": "20210827 15:24"
        },
        {
          "id": "MDRW2100019108.1.16",
          "form": "고민중이니까",
          "original_form": "고민중이니까",
          "speaker_id": "2",
          "time": "20210827 15:24"
        },
        {
          "id": "MDRW2100019108.1.17",
          "form": "당장은 아빠꺼 뺏어서 쓸거야",
          "original_form": "당장은 아빠꺼 뺏어서 쓸거야",
          "speaker_id": "2",
          "time": "20210827 15:24"
        },
        {
          "id": "MDRW2100019108.1.18",
          "form": "내가 쓴다는데 어쩔거야",
          "original_form": "내가 쓴다는데 어쩔거야",
          "speaker_id": "2",
          "time": "20210827 15:24"
        },
        {
          "id": "MDRW2100019108.1.19",
          "form": "그래 ㅋㅋㅋㅋㅋ",
          "original_form": "그래 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:24"
        },
        {
          "id": "MDRW2100019108.1.20",
          "form": "들고 출근하시던가",
          "original_form": "들고 출근하시던가",
          "speaker_id": "2",
          "time": "20210827 15:24"
        },
        {
          "id": "MDRW2100019108.1.21",
          "form": "일단 더 써보고",
          "original_form": "일단 더 써보고",
          "speaker_id": "1",
          "time": "20210827 15:24"
        },
        {
          "id": "MDRW2100019108.1.22",
          "form": "없으면 안되겠다 싶으면 사",
          "original_form": "없으면 안되겠다 싶으면 사",
          "speaker_id": "1",
          "time": "20210827 15:24"
        },
        {
          "id": "MDRW2100019108.1.23",
          "form": "근데 이거 좀 귀아파",
          "original_form": "근데 이거 좀 귀아파",
          "speaker_id": "2",
          "time": "20210827 15:24"
        },
        {
