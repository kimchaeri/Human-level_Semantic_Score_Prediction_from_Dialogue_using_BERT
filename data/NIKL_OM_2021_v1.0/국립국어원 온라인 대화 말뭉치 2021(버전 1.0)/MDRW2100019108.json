      "utterance": [
        {
          "id": "MDRW2100019108.1.1",
          "form": "사기당할것같아......",
          "original_form": "사기당할것같아......",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019108.1.2",
          "form": "난 오늘 중고거래 하나 예약 잡아둠",
          "original_form": "난 오늘 중고거래 하나 예약 잡아둠",
