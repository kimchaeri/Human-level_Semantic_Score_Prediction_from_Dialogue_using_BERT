      "utterance": [
        {
          "id": "MDRW2100014303.1.1",
          "form": "싸이월드 사용해보신 적 있으세요?",
          "original_form": "싸이월드 사용해보신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014303.1.2",
          "form": "네 있어요 예전에요 한참전이네요",
          "original_form": "네 있어요 예전에요 한참전이네요",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014303.1.3",
          "form": "저도 10년은 넘은 것 같은데",
          "original_form": "저도 10년은 넘은 것 같은데",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014303.1.4",
          "form": "부활한다고 하니 새롭더라구요",
          "original_form": "부활한다고 하니 새롭더라구요",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014303.1.5",
          "form": "혹시 배경음악은 어떤 노래셨는지 기억하세요?",
          "original_form": "혹시 배경음악은 어떤 노래셨는지 기억하세요?",
          "speaker_id": "1",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014303.1.6",
          "form": "요조 노래였던거같아요",
          "original_form": "요조 노래였던거같아요",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014303.1.7",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014303.1.8",
          "form": "전 해피라는 팝송 했던 것 같아요.",
          "original_form": "전 해피라는 팝송 했던 것 같아요.",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014303.1.9",
          "form": "좀 있어보이려구요",
          "original_form": "좀 있어보이려구요",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014303.1.10",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014303.1.11",
          "form": "아 해피 저 저스트 댄스에서 자주 추던 노래에요",
          "original_form": "아 해피 저 저스트 댄스에서 자주 추던 노래에요",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014303.1.12",
          "form": "일기남기고 친구들 게시팡 글남기던게재밌었는데",
          "original_form": "일기남기고 친구들 게시팡 글남기던게재밌었는데",
          "speaker_id": "2",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014303.1.13",
          "form": "일촌평도 하나 쓰고",
          "original_form": "일촌평도 하나 쓰고",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014303.1.14",
          "form": "감성 있는 프로필 쓰고",
          "original_form": "감성 있는 프로필 쓰고",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014303.1.15",
          "form": "재밌었어요",
          "original_form": "재밌었어요",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014303.1.16",
          "form": "부활한다니 인스타 만큼 뜰까요?",
          "original_form": "부활한다니 인스타 만큼 뜰까요?",
          "speaker_id": "1",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014303.1.17",
          "form": "그럴것같지는않아요ㅠㅠ",
          "original_form": "그럴것같지는않아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014303.1.18",
          "form": "오래된 플랫폼 같아요ㅠㅠ",
          "original_form": "오래된 플랫폼 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014303.1.19",
          "form": "그냥예전에 그대로두지ㅠㅠ없앴다가 부활이라니",
          "original_form": "그냥예전에 그대로두지ㅠㅠ없앴다가 부활이라니",
          "speaker_id": "2",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014303.1.20",
          "form": "저도 그생각 했어요 추억속에 두는 것도 좋은데",
          "original_form": "저도 그생각 했어요 추억속에 두는 것도 좋은데",
          "speaker_id": "1",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014303.1.21",
          "form": "괜히 기억도 흐지부지 될 것 같은 느낌",
          "original_form": "괜히 기억도 흐지부지 될 것 같은 느낌",
          "speaker_id": "1",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014303.1.22",
          "form": "맞습니다ㅠㅠ부활은 한다더니 이미 했는가요?",
          "original_form": "맞습니다ㅠㅠ부활은 한다더니 이미 했는가요?",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014303.1.23",
          "form": "아뇨 아직인 것 같아요",
          "original_form": "아뇨 아직인 것 같아요",
          "speaker_id": "1",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014303.1.24",
          "form": "그래도 왠지 부활하면 도토리를 쓸 것 같네요",
          "original_form": "그래도 왠지 부활하면 도토리를 쓸 것 같네요",
          "speaker_id": "1",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014303.1.25",
          "form": "그러네요 이젠 직장인이라 돈을 쓸거같네요 예전엔 학생이라 안쓰규ㅎㅎ",
          "original_form": "그러네요 이젠 직장인이라 돈을 쓸거같네요 예전엔 학생이라 안쓰규ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014303.1.26",
          "form": "그죠 직장인이 되고나서는 소비가 쉬워졌어요",
          "original_form": "그죠 직장인이 되고나서는 소비가 쉬워졌어요",
