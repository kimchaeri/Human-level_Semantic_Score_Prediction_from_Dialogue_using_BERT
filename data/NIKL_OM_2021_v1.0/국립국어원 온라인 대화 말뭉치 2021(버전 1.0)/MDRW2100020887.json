      "utterance": [
        {
          "id": "MDRW2100020887.1.1",
          "form": "네네 옷 잘입는 사람 좋아해요!",
          "original_form": "네네 옷 잘입는 사람 좋아해요!",
          "speaker_id": "2",
          "time": "20210828 13:54"
        },
        {
          "id": "MDRW2100020887.1.2",
          "form": "막 엄청 잘입진 않아도 깔끔하게만 입으면 좋아요",
          "original_form": "막 엄청 잘입진 않아도 깔끔하게만 입으면 좋아요",
          "speaker_id": "2",
          "time": "20210828 13:54"
        },
        {
          "id": "MDRW2100020887.1.3",
          "form": "오 그럼 약간 슈트취향이세요??",
          "original_form": "오 그럼 약간 슈트취향이세요??",
          "speaker_id": "1",
          "time": "20210828 13:56"
        },
        {
          "id": "MDRW2100020887.1.4",
          "form": "저는 캐쥬얼한거 좋아해여 ㅋㅋㅋㅋ",
          "original_form": "저는 캐쥬얼한거 좋아해여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:57"
        },
        {
          "id": "MDRW2100020887.1.5",
          "form": "오 캐쥬얼 좋죠",
          "original_form": "오 캐쥬얼 좋죠",
          "speaker_id": "1",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100020887.1.6",
          "form": "전 아직 학생이라 너무 딱딱한건 싫고",
          "original_form": "전 아직 학생이라 너무 딱딱한건 싫고",
          "speaker_id": "1",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100020887.1.7",
          "form": "캐쥬얼 정도가 적당하게 괜찮은거같아요",
          "original_form": "캐쥬얼 정도가 적당하게 괜찮은거같아요",
          "speaker_id": "1",
          "time": "20210828 13:58"
        },
        {
          "id": "MDRW2100020887.1.8",
          "form": "연상좋아하세요?",
          "original_form": "연상좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020887.1.9",
          "form": "음 저는 또래가좋아요! 위아래로 큰 차이안나는!",
          "original_form": "음 저는 또래가좋아요! 위아래로 큰 차이안나는!",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020887.1.10",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 13:59"
        },
        {
          "id": "MDRW2100020887.1.11",
          "form": "저는 연상은 좀 불편해서",
          "original_form": "저는 연상은 좀 불편해서",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020887.1.12",
          "form": "동갑이나 연하가 좋을것같아요",
          "original_form": "동갑이나 연하가 좋을것같아요",
          "speaker_id": "1",
          "time": "20210828 14:00"
        },
        {
          "id": "MDRW2100020887.1.13",
          "form": "첫째라 저보다 나이 많은 사람은 약간 불편하더라구요",
          "original_form": "첫째라 저보다 나이 많은 사람은 약간 불편하더라구요",
          "speaker_id": "1",
          "time": "20210828 14:01"
        },
        {
          "id": "MDRW2100020887.1.14",
          "form": "아아 약간 호칭이 부담스럽고 어색하고 그런편인가요?",
          "original_form": "아아 약간 호칭이 부담스럽고 어색하고 그런편인가요?",
          "speaker_id": "2",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100020887.1.15",
          "form": "네네ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100020887.1.16",
          "form": "별로 불러본적이 없어서",
          "original_form": "별로 불러본적이 없어서",
          "speaker_id": "1",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100020887.1.17",
          "form": "어색하더라구요",
          "original_form": "어색하더라구요",
          "speaker_id": "1",
          "time": "20210828 14:02"
        },
        {
          "id": "MDRW2100020887.1.18",
          "form": "name2님은 연상 괜찮으세요?",
          "original_form": "&name2&님은 연상 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100020887.1.19",
          "form": "네 괜찮아요 ㅋㅋㅋㅋ저도 첫째라 처음에는 어색하긴햇는데",
          "original_form": "네 괜찮아요 ㅋㅋㅋㅋ저도 첫째라 처음에는 어색하긴햇는데",
          "speaker_id": "2",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100020887.1.20",
          "form": "또래로 한두살정도는 편해요!",
          "original_form": "또래로 한두살정도는 편해요!",
          "speaker_id": "2",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100020887.1.21",
          "form": "그리고 약간 좀 더 성숙한느낌이라 장점이 잇어요 ㅋㅋㅋ",
          "original_form": "그리고 약간 좀 더 성숙한느낌이라 장점이 잇어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:05"
        },
        {
          "id": "MDRW2100020887.1.22",
          "form": "아 그쵸 연상이면 아무래도 성숙한 느낌은 있겠죠??ㅋㅋㅋ",
          "original_form": "아 그쵸 연상이면 아무래도 성숙한 느낌은 있겠죠??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:08"
        },
        {
          "id": "MDRW2100020887.1.23",
          "form": "연애관은 어떠세요??",
          "original_form": "연애관은 어떠세요??",
          "speaker_id": "1",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020887.1.24",
          "form": "연락 자주 하는거 좋아하세요?",
          "original_form": "연락 자주 하는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 14:09"
        },
        {
          "id": "MDRW2100020887.1.25",
          "form": "네네 자주하면 좋을거같아요!",
          "original_form": "네네 자주하면 좋을거같아요!",
          "speaker_id": "2",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020887.1.26",
          "form": "아 자주하는거 좋아하신는구나",
          "original_form": "아 자주하는거 좋아하신는구나",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020887.1.27",
          "form": "저는 연락하는거 귀찮아해서",
          "original_form": "저는 연락하는거 귀찮아해서",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020887.1.28",
          "form": "타자치는게 귀찮더라구요",
          "original_form": "타자치는게 귀찮더라구요",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020887.1.29",
          "form": "저는 전화 선호해요",
          "original_form": "저는 전화 선호해요",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020887.1.30",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020887.1.31",
          "form": "아아 저는 둘다 좋아요 ㅋㅋㅋㅋ근데 둘중엔 카톡이 더 좋은거같아요",
          "original_form": "아아 저는 둘다 좋아요 ㅋㅋㅋㅋ근데 둘중엔 카톡이 더 좋은거같아요",
