      "utterance": [
        {
          "id": "MDRW2100045039.1.1",
          "form": "서브웨이 좋아하시나요?",
          "original_form": "서브웨이 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045039.1.2",
          "form": "서브웨이 에그마요!!!",
          "original_form": "서브웨이 에그마요!!!",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045039.1.3",
          "form": "넘 맛있죠ㅠ",
          "original_form": "넘 맛있죠ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100045039.1.4",
          "form": "오 맞아여 혹시 베이컨 추가도 하시나요?!",
          "original_form": "오 맞아여 혹시 베이컨 추가도 하시나요?!",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045039.1.5",
          "form": "넣음 맛잇쪄!!",
          "original_form": "넣음 맛잇쪄!!",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045039.1.6",
          "form": "뭐든 넣는건 다 넣고!",
          "original_form": "뭐든 넣는건 다 넣고!",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100045039.1.7",
          "form": "그쵸그쵸 ㅋㅋ 빵은 어떤걸로 드세요?",
          "original_form": "그쵸그쵸 ㅋㅋ 빵은 어떤걸로 드세요?",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045039.1.8",
          "form": "이름 잘 모르는데 납작한것도 좋고",
          "original_form": "이름 잘 모르는데 납작한것도 좋고",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045039.1.9",
          "form": "다 좋던데요",
          "original_form": "다 좋던데요",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100045039.1.10",
          "form": "다 맛있죠 ㅋㅋ 소스는 어떤거 드세요 ?",
          "original_form": "다 맛있죠 ㅋㅋ 소스는 어떤거 드세요 ?",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100045039.1.11",
          "form": "저는 추천소스로.....ㅋㅋㅋ",
          "original_form": "저는 추천소스로.....ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100045039.1.12",
          "form": "아하ㅎㅎ 그러시군요",
          "original_form": "아하ㅎㅎ 그러시군요",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100045039.1.13",
          "form": "저는 렌치 스윗어니언 먹는데 아무메뉴에나 넣어도 맛있더라구요 ㅎㅎ",
          "original_form": "저는 렌치 스윗어니언 먹는데 아무메뉴에나 넣어도 맛있더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045039.1.14",
          "form": "오 기억해둘께여!!",
          "original_form": "오 기억해둘께여!!",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100045039.1.15",
          "form": "네네 담에 꼭 드셔보세요 ㅎㅎ",
          "original_form": "네네 담에 꼭 드셔보세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100045039.1.16",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045039.1.17",
          "form": "조합도 중요하더라구요 ㅎㅎ",
          "original_form": "조합도 중요하더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045039.1.18",
          "form": "맞아요.... 서브웨이는 마음대로 할수있어서 참 좋아요",
          "original_form": "맞아요.... 서브웨이는 마음대로 할수있어서 참 좋아요",
