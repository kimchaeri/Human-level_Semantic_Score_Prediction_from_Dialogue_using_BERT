      "utterance": [
        {
          "id": "MDRW2100029785.1.1",
          "form": "면접은 아니고 입사설명회? 였습니다",
          "original_form": "면접은 아니고 입사설명회? 였습니다",
          "speaker_id": "2",
          "time": "20210903 13:34"
        },
        {
          "id": "MDRW2100029785.1.2",
          "form": "그리고 거긴 갈 생각 없었어",
          "original_form": "그리고 거긴 갈 생각 없었어",
          "speaker_id": "2",
          "time": "20210903 13:34"
        },
        {
          "id": "MDRW2100029785.1.3",
          "form": "알바가 아니고 직장이더라고",
          "original_form": "알바가 아니고 직장이더라고",
          "speaker_id": "2",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100029785.1.4",
          "form": "ㅇ0ㅇ?!",
          "original_form": "ㅇ0ㅇ?!",
          "speaker_id": "1",
          "time": "20210903 13:40"
        },
        {
          "id": "MDRW2100029785.1.5",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "1",
          "time": "20210903 13:40"
        },
        {
          "id": "MDRW2100029785.1.6",
          "form": "직장이면 출근해...?",
          "original_form": "직장이면 출근해...?",
          "speaker_id": "1",
          "time": "20210903 13:40"
        },
        {
          "id": "MDRW2100029785.1.7",
          "form": "ㅇㅇ 나는 공부하면서 병행할 알바 찾는데",
          "original_form": "ㅇㅇ 나는 공부하면서 병행할 알바 찾는데",
          "speaker_id": "2",
          "time": "20210903 13:40"
        },
        {
          "id": "MDRW2100029785.1.8",
          "form": "거긴 평생직장이고 어쩌고 말을 하는데",
          "original_form": "거긴 평생직장이고 어쩌고 말을 하는데",
          "speaker_id": "2",
          "time": "20210903 13:40"
        },
        {
          "id": "MDRW2100029785.1.9",
          "form": "페이는 알바임",
          "original_form": "페이는 알바임",
          "speaker_id": "2",
          "time": "20210903 13:41"
        },
        {
          "id": "MDRW2100029785.1.10",
          "form": "?.....",
          "original_form": "?.....",
          "speaker_id": "2",
          "time": "20210903 13:41"
        },
        {
          "id": "MDRW2100029785.1.11",
          "form": "??무슨 막말이요??",
          "original_form": "??무슨 막말이요??",
          "speaker_id": "1",
          "time": "20210903 13:41"
        },
        {
          "id": "MDRW2100029785.1.12",
          "form": "뭐야ㅋㅋㅋ",
          "original_form": "뭐야ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:41"
        },
        {
          "id": "MDRW2100029785.1.13",
          "form": "어이가 없는 곳이네",
          "original_form": "어이가 없는 곳이네",
          "speaker_id": "1",
          "time": "20210903 13:41"
        },
        {
          "id": "MDRW2100029785.1.14",
          "form": "그러니까요 ㅋㅋㅋㅋ",
          "original_form": "그러니까요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 13:41"
        },
        {
          "id": "MDRW2100029785.1.15",
          "form": "그래서 원래도 생각 없었지만 안가려고",
          "original_form": "그래서 원래도 생각 없었지만 안가려고",
          "speaker_id": "2",
          "time": "20210903 13:42"
        },
        {
          "id": "MDRW2100029785.1.16",
          "form": "다른 곳 가자",
          "original_form": "다른 곳 가자",
          "speaker_id": "1",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100029785.1.17",
          "form": "다른 곳은 넣어본 곳 있어?",
          "original_form": "다른 곳은 넣어본 곳 있어?",
          "speaker_id": "1",
          "time": "20210904 00:30"
        },
        {
          "id": "MDRW2100029785.1.18",
          "form": "서점 지원해야 하는데 깜빡했네",
          "original_form": "서점 지원해야 하는데 깜빡했네",
          "speaker_id": "2",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100029785.1.19",
          "form": "롸",
          "original_form": "롸",
          "speaker_id": "1",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100029785.1.20",
          "form": "지금이라도 넣자",
          "original_form": "지금이라도 넣자",
          "speaker_id": "1",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100029785.1.21",
          "form": "근데 이미 늦었지 않을까....",
          "original_form": "근데 이미 늦었지 않을까....",
          "speaker_id": "2",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100029785.1.22",
          "form": "몰겠고 연달아 3개를 면접보고 오니 좀 지침",
          "original_form": "몰겠고 연달아 3개를 면접보고 오니 좀 지침",
          "speaker_id": "2",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100029785.1.23",
          "form": "허미 3개나 봤나",
          "original_form": "허미 3개나 봤나",
          "speaker_id": "1",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100029785.1.24",
          "form": "나는 하나만 알았네",
          "original_form": "나는 하나만 알았네",
          "speaker_id": "1",
          "time": "20210904 00:45"
        },
        {
          "id": "MDRW2100029785.1.25",
          "form": "늦어도 혹시 모르니까 메일 접수만 해봐",
          "original_form": "늦어도 혹시 모르니까 메일 접수만 해봐",
          "speaker_id": "1",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100029785.1.26",
          "form": "오늘은 하나인데",
          "original_form": "오늘은 하나인데",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100029785.1.27",
          "form": "어제거까지 합치면 3개지",
          "original_form": "어제거까지 합치면 3개지",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100029785.1.28",
          "form": "메일... 내일은 진짜 넣도록 하겠습니다",
          "original_form": "메일... 내일은 진짜 넣도록 하겠습니다",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100029785.1.29",
          "form": "할 수 있습니다",
          "original_form": "할 수 있습니다",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100029785.1.30",
          "form": "화이팅!!",
          "original_form": "화이팅!!",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100029785.1.31",
          "form": "감삼다",
          "original_form": "감삼다",
          "speaker_id": "2",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100029785.1.32",
          "form": "다른 알바는 알아본 곳 있어?",
          "original_form": "다른 알바는 알아본 곳 있어?",
          "speaker_id": "1",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100029785.1.33",
          "form": "맨날 알바몬 들어가서 찾아보고는 있는데",
          "original_form": "맨날 알바몬 들어가서 찾아보고는 있는데",
          "speaker_id": "2",
          "time": "20210904 11:10"
        },
        {
          "id": "MDRW2100029785.1.34",
          "form": "죄다 공장 아니면 풀타임이라서 좀 그렇네",
          "original_form": "죄다 공장 아니면 풀타임이라서 좀 그렇네",
          "speaker_id": "2",
          "time": "20210904 11:11"
        },
        {
          "id": "MDRW2100029785.1.35",
          "form": "파트타임 알아보게?",
          "original_form": "파트타임 알아보게?",
          "speaker_id": "1",
          "time": "20210904 13:03"
        },
        {
          "id": "MDRW2100029785.1.36",
          "form": "그런 거면 빵집도 괜찮지 않나",
          "original_form": "그런 거면 빵집도 괜찮지 않나",
          "speaker_id": "1",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100029785.1.37",
          "form": "안나와.... 공고가 없어......",
          "original_form": "안나와.... 공고가 없어......",
          "speaker_id": "2",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100029785.1.38",
          "form": "앗...",
          "original_form": "앗...",
          "speaker_id": "1",
          "time": "20210905 17:41"
        },
        {
          "id": "MDRW2100029785.1.39",
          "form": "집 근처에 편의점도 많아서",
          "original_form": "집 근처에 편의점도 많아서",
          "speaker_id": "1",
          "time": "20210905 17:41"
        },
        {
          "id": "MDRW2100029785.1.40",
          "form": "한 자리 정도는 나올만 할 텐데",
          "original_form": "한 자리 정도는 나올만 할 텐데",
          "speaker_id": "1",
          "time": "20210905 17:41"
        },
        {
          "id": "MDRW2100029785.1.41",
          "form": "어렵나보네",
          "original_form": "어렵나보네",
          "speaker_id": "1",
          "time": "20210905 17:41"
        },
        {
          "id": "MDRW2100029785.1.42",
          "form": "그러게 우리 동네에 이렇게 편의점이 많은데",
          "original_form": "그러게 우리 동네에 이렇게 편의점이 많은데",
          "speaker_id": "2",
          "time": "20210905 17:51"
        },
        {
          "id": "MDRW2100029785.1.43",
          "form": "나 하나 일할 곳 없단 말이야?",
          "original_form": "나 하나 일할 곳 없단 말이야?",
          "speaker_id": "2",
          "time": "20210905 17:52"
        }
      ]
