      "utterance": [
        {
          "id": "MDRW2100030491.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 02:07"
        },
        {
          "id": "MDRW2100030491.1.2",
          "form": "그럼 진상이 많지않으면",
          "original_form": "그럼 진상이 많지않으면",
          "speaker_id": "2",
          "time": "20210904 02:07"
        },
        {
          "id": "MDRW2100030491.1.3",
          "form": "다른 업무적인 스트레스요인..?같은게 있었나요?",
          "original_form": "다른 업무적인 스트레스요인..?같은게 있었나요?",
          "speaker_id": "2",
          "time": "20210904 02:08"
        },
        {
          "id": "MDRW2100030491.1.4",
          "form": "음.. 저는 비흡연자라",
          "original_form": "음.. 저는 비흡연자라",
          "speaker_id": "1",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030491.1.5",
          "form": "처음에 담배종류 외우는 게 너무 어려웠어요",
          "original_form": "처음에 담배종류 외우는 게 너무 어려웠어요",
          "speaker_id": "1",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030491.1.6",
          "form": "와 확실히ㅜ...",
          "original_form": "와 확실히ㅜ...",
          "speaker_id": "2",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030491.1.7",
          "form": "그리고 담배를 예전 이름이나 다른 이름으로 부르는 손님들도 있었어서 더 헷갈리더라구요 ㅋㅋㅋ",
          "original_form": "그리고 담배를 예전 이름이나 다른 이름으로 부르는 손님들도 있었어서 더 헷갈리더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030491.1.8",
          "form": "담배 브랜드이름은 같은데 이름이 미묘하게다르고",
          "original_form": "담배 브랜드이름은 같은데 이름이 미묘하게다르고",
          "speaker_id": "2",
          "time": "20210904 02:09"
        },
        {
          "id": "MDRW2100030491.1.9",
          "form": "와 예전 이름으로부르는건 정말",
          "original_form": "와 예전 이름으로부르는건 정말",
          "speaker_id": "2",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030491.1.10",
          "form": "헷갈리겠네요",
          "original_form": "헷갈리겠네요",
          "speaker_id": "2",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030491.1.11",
          "form": "맞아요~ 심지어 같은 종류인데 3mm 6mm로 나뉘더라구요",
          "original_form": "맞아요~ 심지어 같은 종류인데 3mm 6mm로 나뉘더라구요",
          "speaker_id": "1",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030491.1.12",
          "form": "헉 그렇구나ㅋㅋㅋ..사실저도 비흡연자라서",
          "original_form": "헉 그렇구나ㅋㅋㅋ..사실저도 비흡연자라서",
          "speaker_id": "2",
          "time": "20210904 02:10"
        },
        {
          "id": "MDRW2100030491.1.13",
          "form": "막 원래 이름은 에쎄 프라임인데 에쎄 라이트라고 많이 부르시고.. 말보로도 라이트 있었고 팔리아멘트도 있던것같아요 ㅋㅋㅋ 알바 그만둔지 꽤 돼서 가물가물하네요",
          "original_form": "막 원래 이름은 에쎄 프라임인데 에쎄 라이트라고 많이 부르시고.. 말보로도 라이트 있었고 팔리아멘트도 있던것같아요 ㅋㅋㅋ 알바 그만둔지 꽤 돼서 가물가물하네요",
          "speaker_id": "1",
          "time": "20210904 02:11"
        },
        {
          "id": "MDRW2100030491.1.14",
          "form": "알바는 왜 그만두게 되셨어요??",
          "original_form": "알바는 왜 그만두게 되셨어요??",
          "speaker_id": "2",
          "time": "20210904 02:12"
        },
        {
          "id": "MDRW2100030491.1.15",
          "form": "음.. 특별한 이유는 없고",
          "original_form": "음.. 특별한 이유는 없고",
          "speaker_id": "1",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030491.1.16",
          "form": "그냥 그만두고 싶었어요 ㅋㅋㅋㅋㅋ",
          "original_form": "그냥 그만두고 싶었어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030491.1.17",
          "form": "6개월밖에 안하긴 했는데 질리더라구요 ㅎ",
          "original_form": "6개월밖에 안하긴 했는데 질리더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210904 02:13"
        },
        {
          "id": "MDRW2100030491.1.18",
          "form": "우와..6개월이면 질릴만하죠",
          "original_form": "우와..6개월이면 질릴만하죠",
          "speaker_id": "2",
          "time": "20210904 02:14"
        },
        {
          "id": "MDRW2100030491.1.19",
          "form": "그럼 구직활동 하고계신거예요?",
          "original_form": "그럼 구직활동 하고계신거예요?",
          "speaker_id": "2",
          "time": "20210904 02:14"
        },
        {
          "id": "MDRW2100030491.1.20",
          "form": "아니면 휴식중이신가용",
          "original_form": "아니면 휴식중이신가용",
          "speaker_id": "2",
          "time": "20210904 02:14"
        },
        {
          "id": "MDRW2100030491.1.21",
          "form": "그냥 놀고있어요 ㅎㅎㅋㅋㅋㅋㅋㅋ",
          "original_form": "그냥 놀고있어요 ㅎㅎㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 02:14"
        },
        {
          "id": "MDRW2100030491.1.22",
          "form": "name2님은 과외 하는 거 어떠신가요? 재미있나요?",
          "original_form": "&name2&님은 과외 하는 거 어떠신가요? 재미있나요?",
          "speaker_id": "1",
          "time": "20210904 02:15"
        },
        {
          "id": "MDRW2100030491.1.23",
          "form": "하긴 요즘에는 알바도 취직도 안구해진다더라구요",
          "original_form": "하긴 요즘에는 알바도 취직도 안구해진다더라구요",
          "speaker_id": "2",
          "time": "20210904 02:15"
        },
        {
          "id": "MDRW2100030491.1.24",
          "form": "저는 재미로하기보다는",
          "original_form": "저는 재미로하기보다는",
          "speaker_id": "2",
          "time": "20210904 02:15"
        },
        {
          "id": "MDRW2100030491.1.25",
          "form": "페이때문에하는거라서요ㅜㅜ",
          "original_form": "페이때문에하는거라서요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 02:15"
        },
        {
          "id": "MDRW2100030491.1.26",
          "form": "알바하는게 다 그렇긴 하죠 근데 주변에 과외 받거나/하는 사람이 없어서 궁금해서 여쭤봤어요 ㅋㅋㅋㅋ",
