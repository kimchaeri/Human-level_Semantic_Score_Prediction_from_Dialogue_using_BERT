      "utterance": [
        {
          "id": "MDRW2100021756.1.1",
          "form": "잉크는 일단 잭 프로스트는 킵인데 딴 건 좀 거 봐여",
          "original_form": "잉크는 일단 잭 프로스트는 킵인데 딴 건 좀 거 봐여",
          "speaker_id": "2",
          "time": "20210828 22:09"
        },
        {
          "id": "MDRW2100021756.1.2",
          "form": "예쓰",
          "original_form": "예쓰",
          "speaker_id": "1",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100021756.1.3",
          "form": "나는 해피 홀리데이스, 윈터 미라클, 인챈티드 오션",
          "original_form": "나는 해피 홀리데이스, 윈터 미라클, 인챈티드 오션",
          "speaker_id": "1",
          "time": "20210828 22:10"
        },
        {
          "id": "MDRW2100021756.1.4",
          "form": "셋 확정이고",
          "original_form": "셋 확정이고",
          "speaker_id": "1",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021756.1.5",
          "form": "딴거 몇개 좀 고민중",
          "original_form": "딴거 몇개 좀 고민중",
          "speaker_id": "1",
          "time": "20210828 22:11"
        },
        {
          "id": "MDRW2100021756.1.6",
          "form": "첨에 준 그 사이트에서 살거지",
          "original_form": "첨에 준 그 사이트에서 살거지",
          "speaker_id": "2",
          "time": "20210828 22:12"
        },
        {
          "id": "MDRW2100021756.1.7",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100021756.1.8",
          "form": "독일한정 거기는 말고",
          "original_form": "독일한정 거기는 말고",
          "speaker_id": "2",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021756.1.9",
          "form": "마음 같아선 거기거 사고 싶은데",
          "original_form": "마음 같아선 거기거 사고 싶은데",
          "speaker_id": "1",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021756.1.10",
          "form": "거기는 배송비가 들어서 니네가 안 좋아할 것 같아서",
          "original_form": "거기는 배송비가 들어서 니네가 안 좋아할 것 같아서",
          "speaker_id": "1",
          "time": "20210828 22:14"
        },
        {
          "id": "MDRW2100021756.1.11",
          "form": "근데 거기는 사이트 안 들어가서 모르는데 독일 한정만 가능한 거야?",
          "original_form": "근데 거기는 사이트 안 들어가서 모르는데 독일 한정만 가능한 거야?",
          "speaker_id": "2",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021756.1.12",
          "form": "ㄴㄴ",
          "original_form": "ㄴㄴ",
          "speaker_id": "1",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021756.1.13",
          "form": "온고잉도 되고",
          "original_form": "온고잉도 되고",
          "speaker_id": "1",
          "time": "20210828 22:15"
        },
        {
          "id": "MDRW2100021756.1.14",
          "form": "독일 사이트라 독일 한정 되고",
          "original_form": "독일 사이트라 독일 한정 되고",
          "speaker_id": "1",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021756.1.15",
          "form": "첫번째 사이트는 영국거라",
          "original_form": "첫번째 사이트는 영국거라",
          "speaker_id": "1",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021756.1.16",
          "form": "영국 한정이 돼",
          "original_form": "영국 한정이 돼",
          "speaker_id": "1",
          "time": "20210828 22:16"
        },
        {
          "id": "MDRW2100021756.1.17",
          "form": "아항 정말 나라 한정...",
          "original_form": "아항 정말 나라 한정...",
          "speaker_id": "2",
          "time": "20210828 22:17"
        },
        {
          "id": "MDRW2100021756.1.18",
          "form": "악랄하군",
          "original_form": "악랄하군",
          "speaker_id": "2",
          "time": "20210828 22:18"
        },
        {
          "id": "MDRW2100021756.1.19",
          "form": "ㅎ...",
          "original_form": "ㅎ...",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021756.1.20",
          "form": "근데 영국한정은 그냥 그런데",
          "original_form": "근데 영국한정은 그냥 그런데",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021756.1.21",
          "form": "독일 한정이 취저",
          "original_form": "독일 한정이 취저",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021756.1.22",
          "form": "영국은 색도 밍숭맹숭하군",
          "original_form": "영국은 색도 밍숭맹숭하군",
          "speaker_id": "2",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021756.1.23",
          "form": "힝구야",
          "original_form": "힝구야",
          "speaker_id": "1",
          "time": "20210828 22:22"
        },
        {
          "id": "MDRW2100021756.1.24",
          "form": "독일 한정은 저렇게 이뿐데..",
          "original_form": "독일 한정은 저렇게 이뿐데..",
          "speaker_id": "1",
          "time": "20210828 22:23"
        },
        {
          "id": "MDRW2100021756.1.25",
          "form": "독일은 간지의 나라인가바 ㅋㅋㅋㅋㅋ",
          "original_form": "독일은 간지의 나라인가바 ㅋㅋㅋㅋㅋ",
