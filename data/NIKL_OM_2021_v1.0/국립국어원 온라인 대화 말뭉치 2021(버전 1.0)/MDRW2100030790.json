      "utterance": [
        {
          "id": "MDRW2100030790.1.1",
          "form": "재밌었어요 ㅋㅋㅋ",
          "original_form": "재밌었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100030790.1.2",
          "form": "저 예전에",
          "original_form": "저 예전에",
          "speaker_id": "2",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100030790.1.3",
          "form": "친구랑 같이 갔는데",
          "original_form": "친구랑 같이 갔는데",
          "speaker_id": "2",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030790.1.4",
          "form": "디즈니 전시회였나",
          "original_form": "디즈니 전시회였나",
          "speaker_id": "1",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030790.1.5",
          "form": "맞아요 그거 그림 많아서",
          "original_form": "맞아요 그거 그림 많아서",
          "speaker_id": "2",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030790.1.6",
          "form": "멋있었어요 신기하기도 하고",
          "original_form": "멋있었어요 신기하기도 하고",
          "speaker_id": "2",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030790.1.7",
          "form": "오래 걸어다니는건 힘들긴 하더라",
          "original_form": "오래 걸어다니는건 힘들긴 하더라",
          "speaker_id": "1",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030790.1.8",
          "form": "중간에 쉼터 있었으면 좋겠어요 ㅠㅠ",
          "original_form": "중간에 쉼터 있었으면 좋겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030790.1.9",
          "form": "빨리 나가라고 그러나",
          "original_form": "빨리 나가라고 그러나",
          "speaker_id": "1",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030790.1.10",
          "form": "설마 싶지만 ㅋㅋㅋ 그럴수도 있겠네요",
          "original_form": "설마 싶지만 ㅋㅋㅋ 그럴수도 있겠네요",
          "speaker_id": "2",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030790.1.11",
          "form": "박물관 회전률을 위한??ㅎㅎ",
          "original_form": "박물관 회전률을 위한??ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030790.1.12",
          "form": "엄마두 미술관은 좋아해",
          "original_form": "엄마두 미술관은 좋아해",
          "speaker_id": "1",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030790.1.13",
          "form": "작가는 잘 모르지만",
          "original_form": "작가는 잘 모르지만",
          "speaker_id": "1",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030790.1.14",
          "form": "몰라도 보는거 재밌지 않아요?",
          "original_form": "몰라도 보는거 재밌지 않아요?",
          "speaker_id": "2",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030790.1.15",
          "form": "마음으로 느끼는거지 예술은",
          "original_form": "마음으로 느끼는거지 예술은",
          "speaker_id": "1",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030790.1.16",
          "form": "ㅋㅋㅋㅋ 멋있는 소리 하시네...",
          "original_form": "ㅋㅋㅋㅋ 멋있는 소리 하시네...",
          "speaker_id": "2",
          "time": "20210904 12:36"
        },
        {
          "id": "MDRW2100030790.1.17",
          "form": "예전에는 다 티켓 끊고 들어갔는데",
          "original_form": "예전에는 다 티켓 끊고 들어갔는데",
          "speaker_id": "2",
          "time": "20210904 12:36"
        },
        {
          "id": "MDRW2100030790.1.18",
          "form": "요즘엔 안그런곳도 많더라구요",
          "original_form": "요즘엔 안그런곳도 많더라구요",
          "speaker_id": "2",
          "time": "20210904 12:36"
        },
        {
          "id": "MDRW2100030790.1.19",
          "form": "공공기관에서 설립해서 그런가봐",
          "original_form": "공공기관에서 설립해서 그런가봐",
          "speaker_id": "1",
          "time": "20210904 12:36"
        },
        {
          "id": "MDRW2100030790.1.20",
          "form": "전남쪽엔 많이 없긴 하지만 ㅎㅎ",
          "original_form": "전남쪽엔 많이 없긴 하지만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 12:36"
