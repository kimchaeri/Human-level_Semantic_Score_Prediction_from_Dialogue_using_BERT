      "utterance": [
        {
          "id": "MDRW2100005966.1.1",
          "form": "전공이?",
          "original_form": "전공이?",
          "speaker_id": "2",
          "time": "20210907 17:48"
        },
        {
          "id": "MDRW2100005966.1.2",
          "form": "영상이여",
          "original_form": "영상이여",
          "speaker_id": "1",
          "time": "20210907 17:48"
        },
        {
          "id": "MDRW2100005966.1.3",
          "form": "방사선 뭐 이런건가요?",
          "original_form": "방사선 뭐 이런건가요?",
          "speaker_id": "2",
          "time": "20210907 17:48"
        },
        {
          "id": "MDRW2100005966.1.4",
          "form": "예?",
          "original_form": "예?",
          "speaker_id": "1",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100005966.1.5",
          "form": "아 영상영화학과?",
          "original_form": "아 영상영화학과?",
          "speaker_id": "2",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100005966.1.6",
          "form": "그쪽이요",
          "original_form": "그쪽이요",
          "speaker_id": "1",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100005966.1.7",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100005966.1.8",
          "form": "지금 2학년인데",
          "original_form": "지금 2학년인데",
          "speaker_id": "2",
          "time": "20210907 17:49"
        },
        {
          "id": "MDRW2100005966.1.9",
          "form": "한참 대학생활 즐길때네요 혹시 연락하는 남자있음?",
          "original_form": "한참 대학생활 즐길때네요 혹시 연락하는 남자있음?",
          "speaker_id": "2",
          "time": "20210907 17:50"
        },
        {
          "id": "MDRW2100005966.1.10",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100005966.1.11",
          "form": "그리고 왜 사회부적응자라고 해요 맘아프게",
          "original_form": "그리고 왜 사회부적응자라고 해요 맘아프게",
          "speaker_id": "2",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100005966.1.12",
          "form": "차차 적응하면되지",
          "original_form": "차차 적응하면되지",
          "speaker_id": "2",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100005966.1.13",
          "form": "나도 아직 미숙함",
          "original_form": "나도 아직 미숙함",
          "speaker_id": "2",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100005966.1.14",
          "form": "되게 따듯하시네요",
          "original_form": "되게 따듯하시네요",
          "speaker_id": "1",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100005966.1.15",
          "form": "세상은 넓고 다양한사람 많으니",
          "original_form": "세상은 넓고 다양한사람 많으니",
          "speaker_id": "2",
          "time": "20210907 17:51"
        },
        {
          "id": "MDRW2100005966.1.16",
          "form": "정답이 없음",
          "original_form": "정답이 없음",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100005966.1.17",
          "form": "남친 사귀고 싶음?",
          "original_form": "남친 사귀고 싶음?",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100005966.1.18",
          "form": "흠...아직 준비가 안된거같긴해요",
          "original_form": "흠...아직 준비가 안된거같긴해요",
          "speaker_id": "1",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100005966.1.19",
          "form": "근데 사실 아직 학교를 가본적이없어여",
          "original_form": "근데 사실 아직 학교를 가본적이없어여",
          "speaker_id": "1",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100005966.1.20",
          "form": "어떤 것 때문에?",
          "original_form": "어떤 것 때문에?",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100005966.1.21",
          "form": "코로나 때문에?",
          "original_form": "코로나 때문에?",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100005966.1.22",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100005966.1.23",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210907 17:52"
        },
        {
          "id": "MDRW2100005966.1.24",
          "form": "진짜 열받네",
          "original_form": "진짜 열받네",
          "speaker_id": "2",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100005966.1.25",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100005966.1.26",
          "form": "코로나",
          "original_form": "코로나",
          "speaker_id": "2",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100005966.1.27",
          "form": "그 첨에 학교가서 썸탈때가 진짜 달달한데",
          "original_form": "그 첨에 학교가서 썸탈때가 진짜 달달한데",
          "speaker_id": "2",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100005966.1.28",
          "form": "여고 나옴?",
          "original_form": "여고 나옴?",
          "speaker_id": "2",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100005966.1.29",
          "form": "어케아셨징?",
          "original_form": "어케아셨징?",
          "speaker_id": "1",
          "time": "20210907 17:53"
        },
        {
          "id": "MDRW2100005966.1.30",
          "form": "저 여중여고",
          "original_form": "저 여중여고",
          "speaker_id": "1",
