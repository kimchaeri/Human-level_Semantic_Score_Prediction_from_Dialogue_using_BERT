      "utterance": [
        {
          "id": "MDRW2100030468.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100030468.1.2",
          "form": "요즘 어떻게 지내세요?",
          "original_form": "요즘 어떻게 지내세요?",
          "speaker_id": "1",
          "time": "20210904 01:34"
        },
        {
          "id": "MDRW2100030468.1.3",
          "form": "학교는 졸업한지",
          "original_form": "학교는 졸업한지",
          "speaker_id": "2",
          "time": "20210904 05:16"
        },
        {
          "id": "MDRW2100030468.1.4",
          "form": "오래입니다 ㅎㅎ",
          "original_form": "오래입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 05:16"
        },
        {
          "id": "MDRW2100030468.1.5",
          "form": "그럼 학교에 대한 추억을 얘기해볼까요?",
          "original_form": "그럼 학교에 대한 추억을 얘기해볼까요?",
          "speaker_id": "1",
          "time": "20210904 15:10"
        },
        {
          "id": "MDRW2100030468.1.6",
          "form": "저는 아직 대학생이긴한데",
          "original_form": "저는 아직 대학생이긴한데",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100030468.1.7",
          "form": "코로나때문에 학교생활을 못하고있거든요",
          "original_form": "코로나때문에 학교생활을 못하고있거든요",
          "speaker_id": "1",
          "time": "20210904 15:11"
        },
        {
          "id": "MDRW2100030468.1.8",
          "form": "몇학년이세요?",
          "original_form": "몇학년이세요?",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100030468.1.9",
          "form": "새내기예요!",
          "original_form": "새내기예요!",
          "speaker_id": "1",
          "time": "20210904 16:00"
        },
        {
          "id": "MDRW2100030468.1.10",
          "form": "아 그럼 완전 하나도 못 누렸네요ㅠㅠ",
          "original_form": "아 그럼 완전 하나도 못 누렸네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100030468.1.11",
          "form": "그래도 고3때도 온라인수업이었어서",
          "original_form": "그래도 고3때도 온라인수업이었어서",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100030468.1.12",
          "form": "수업듣는거자체는 익숙해진것같아요!",
          "original_form": "수업듣는거자체는 익숙해진것같아요!",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100030468.1.13",
          "form": "그래도 새내기때 술 좀 먹고",
          "original_form": "그래도 새내기때 술 좀 먹고",
          "speaker_id": "2",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100030468.1.14",
          "form": "선배들한테 밥 얻어먹고",
          "original_form": "선배들한테 밥 얻어먹고",
          "speaker_id": "2",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100030468.1.15",
          "form": "그런 재미가 있는데요ㅋㅋ",
          "original_form": "그런 재미가 있는데요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100030468.1.16",
          "form": "그러게요ㅠㅠ",
          "original_form": "그러게요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100030468.1.17",
          "form": "벌써 학교에서는 신입생받을준비하더라구요",
          "original_form": "벌써 학교에서는 신입생받을준비하더라구요",
          "speaker_id": "1",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100030468.1.18",
          "form": "그러게요 벌써~~",
          "original_form": "그러게요 벌써~~",
          "speaker_id": "2",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100030468.1.19",
          "form": "대학교 다닐 때 꼭 해봐야하는 놀거리?가뭐가있을까요?",
          "original_form": "대학교 다닐 때 꼭 해봐야하는 놀거리?가뭐가있을까요?",
          "speaker_id": "1",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100030468.1.20",
          "form": "저는 대외활동",
          "original_form": "저는 대외활동",
          "speaker_id": "2",
          "time": "20210904 16:59"
        },
        {
          "id": "MDRW2100030468.1.21",
          "form": "많이 못한게 아쉬워요",
          "original_form": "많이 못한게 아쉬워요",
          "speaker_id": "2",
          "time": "20210904 16:59"
        },
        {
          "id": "MDRW2100030468.1.22",
          "form": "너무 좁은 물 말고 바다로 나가보십셔",
          "original_form": "너무 좁은 물 말고 바다로 나가보십셔",
          "speaker_id": "2",
          "time": "20210904 16:59"
        },
        {
          "id": "MDRW2100030468.1.23",
          "form": "헉 생각지도 못했네요 대외활동..",
          "original_form": "헉 생각지도 못했네요 대외활동..",
          "speaker_id": "1",
          "time": "20210904 17:04"
        }
      ]
