      "utterance": [
        {
          "id": "MDRW2100020149.1.1",
          "form": "다이어트 혹시 하시나요 전 다이어트 하고있어요",
          "original_form": "다이어트 혹시 하시나요 전 다이어트 하고있어요",
          "speaker_id": "1",
          "time": "20210827 23:59"
        },
        {
          "id": "MDRW2100020149.1.2",
          "form": "저는 두달전까지햇엇어요",
          "original_form": "저는 두달전까지햇엇어요",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020149.1.3",
          "form": "얼마나되셧나요",
          "original_form": "얼마나되셧나요",
          "speaker_id": "2",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020149.1.4",
          "form": "전 4개월 됐어요",
          "original_form": "전 4개월 됐어요",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020149.1.5",
          "form": "4개월동안 12키로 감량했고",
          "original_form": "4개월동안 12키로 감량했고",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020149.1.6",
          "form": "지금도 계속 감량중입니다",
          "original_form": "지금도 계속 감량중입니다",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020149.1.7",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020149.1.8",
          "form": "저는 8개월동안 12키로 뺏어요",
          "original_form": "저는 8개월동안 12키로 뺏어요",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020149.1.9",
          "form": "대박 대단하십니다",
          "original_form": "대박 대단하십니다",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020149.1.10",
          "form": "저는 워낙 원래 체중이 많이 나갔던터라",
          "original_form": "저는 워낙 원래 체중이 많이 나갔던터라",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020149.1.11",
          "form": "단기간에도 많은 감량이 생긴것같아요",
          "original_form": "단기간에도 많은 감량이 생긴것같아요",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020149.1.12",
          "form": "앞으로 기간은 어느정도 잡고계시나요??",
          "original_form": "앞으로 기간은 어느정도 잡고계시나요??",
          "speaker_id": "2",
          "time": "20210828 00:09"
        },
        {
          "id": "MDRW2100020149.1.13",
          "form": "대략 내년 2월까지 20키로 더 감량하는게 목표입니다",
          "original_form": "대략 내년 2월까지 20키로 더 감량하는게 목표입니다",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020149.1.14",
          "form": "점점 난이도가 올라가서 쉽지가 않네요ㅠㅠ",
          "original_form": "점점 난이도가 올라가서 쉽지가 않네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020149.1.15",
          "form": "대단하세요",
          "original_form": "대단하세요",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020149.1.16",
          "form": "저는 8개월 걸린거도",
          "original_form": "저는 8개월 걸린거도",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020149.1.17",
          "form": "식단 유지를 못해서 ㅠ",
          "original_form": "식단 유지를 못해서 ㅠ",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020149.1.18",
          "form": "식단이 진짜진짜 힘든거 공감합니다",
          "original_form": "식단이 진짜진짜 힘든거 공감합니다",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020149.1.19",
          "form": "전 탄단지 챙기는것이 제일 힘들어요",
          "original_form": "전 탄단지 챙기는것이 제일 힘들어요",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020149.1.20",
          "form": "맞아요 ㅠㅠ 전 나중엔",
          "original_form": "맞아요 ㅠㅠ 전 나중엔",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020149.1.21",
          "form": "그냥 단백질양이랑 칼로리정도만",
          "original_form": "그냥 단백질양이랑 칼로리정도만",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020149.1.22",
          "form": "지켜먹엇어요",
          "original_form": "지켜먹엇어요",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020149.1.23",
          "form": "단백질 진짜 곤란하지않나요",
          "original_form": "단백질 진짜 곤란하지않나요",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020149.1.24",
          "form": "안먹으면 살쳐지고 머리빠진다니까",
          "original_form": "안먹으면 살쳐지고 머리빠진다니까",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020149.1.25",
          "form": "억지로 먹긴하는데",
          "original_form": "억지로 먹긴하는데",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020149.1.26",
          "form": "이젠 삶은계란만 봐도 질려요",
          "original_form": "이젠 삶은계란만 봐도 질려요",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020149.1.27",
          "form": "닭가.슴살은 안드시나요??",
          "original_form": "닭가.슴살은 안드시나요??",
          "speaker_id": "2",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020149.1.28",
          "form": "닭가슴살도 사놓긴했는데 냉동실에 짱박혀있네요",
          "original_form": "닭가슴살도 사놓긴했는데 냉동실에 짱박혀있네요",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020149.1.29",
          "form": "참...안넘어가요ㅋㅋㅋ",
          "original_form": "참...안넘어가요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:14"
        },
        {
          "id": "MDRW2100020149.1.30",
          "form": "맛있으려면 양념을 해야되는데",
          "original_form": "맛있으려면 양념을 해야되는데",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020149.1.31",
          "form": "그러면 다이어트가 아니게되니",
          "original_form": "그러면 다이어트가 아니게되니",
          "speaker_id": "1",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020149.1.32",
          "form": "양념안된걸 드시는군요!",
          "original_form": "양념안된걸 드시는군요!",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020149.1.33",
          "form": "근데 소금간만해도",
          "original_form": "근데 소금간만해도",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020149.1.34",
          "form": "그나마 먹을만하더라구요",
          "original_form": "그나마 먹을만하더라구요",
          "speaker_id": "2",
          "time": "20210828 00:15"
        },
        {
          "id": "MDRW2100020149.1.35",
          "form": "닭백숙은 맛있는데",
          "original_form": "닭백숙은 맛있는데",
          "speaker_id": "1",
          "time": "20210828 00:16"
        },
        {
          "id": "MDRW2100020149.1.36",
          "form": "왜그럴까요",
          "original_form": "왜그럴까요",
          "speaker_id": "1",
          "time": "20210828 00:17"
        }
      ]
    }
