      "utterance": [
        {
          "id": "MDRW2100034443.1.1",
          "form": "요새 스크릿우먼 파이터라고 댄스 하는거 있드라고",
          "original_form": "요새 스크릿우먼 파이터라고 댄스 하는거 있드라고",
          "speaker_id": "1",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100034443.1.2",
          "form": "유퀴즈에 국가대표2 하네",
          "original_form": "유퀴즈에 국가대표2 하네",
          "speaker_id": "2",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100034443.1.3",
          "form": "아 또 하네",
          "original_form": "아 또 하네",
          "speaker_id": "1",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100034443.1.4",
          "form": "신재환 선수 올림픽 경기할때 그래 손바닥에 침을 많이 뱉던데",
          "original_form": "신재환 선수 올림픽 경기할때 그래 손바닥에 침을 많이 뱉던데",
          "speaker_id": "2",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100034443.1.5",
          "form": "신재환이 어느 종목이고?",
          "original_form": "신재환이 어느 종목이고?",
          "speaker_id": "1",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100034443.1.6",
          "form": "도마",
          "original_form": "도마",
          "speaker_id": "2",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100034443.1.7",
          "form": "아 손에 침좀 뱉드라",
          "original_form": "아 손에 침좀 뱉드라",
          "speaker_id": "1",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100034443.1.8",
          "form": "심하게 ㅋㅋ 다들 그러더라 ㅋㅋ",
          "original_form": "심하게 ㅋㅋ 다들 그러더라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100034443.1.9",
          "form": "이유가 있겠지?",
          "original_form": "이유가 있겠지?",
          "speaker_id": "1",
          "time": "20210828 11:17"
        },
        {
          "id": "MDRW2100034443.1.10",
          "form": "다들 침을 묻히기는 하던데 유독 많이 하니 ㅋㅋ",
          "original_form": "다들 침을 묻히기는 하던데 유독 많이 하니 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100034443.1.11",
          "form": "ㅋㅋ 여자들은 거의 안 뱉던데",
          "original_form": "ㅋㅋ 여자들은 거의 안 뱉던데",
          "speaker_id": "1",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100034443.1.12",
          "form": "그런가",
          "original_form": "그런가",
          "speaker_id": "2",
          "time": "20210828 11:19"
        },
        {
          "id": "MDRW2100034443.1.13",
          "form": "아닌가? 뱉었나 그러니깐 모르겠다ㅋ",
          "original_form": "아닌가? 뱉었나 그러니깐 모르겠다ㅋ",
