      "utterance": [
        {
          "id": "MDRW2100036742.1.1",
          "form": "점심 메뉴 고르셨나요?",
          "original_form": "점심 메뉴 고르셨나요?",
          "speaker_id": "1",
          "time": "20210831 11:16"
        },
        {
          "id": "MDRW2100036742.1.2",
          "form": "아직이요 점심 고르셨어요?",
          "original_form": "아직이요 점심 고르셨어요?",
          "speaker_id": "2",
          "time": "20210831 11:18"
        },
        {
          "id": "MDRW2100036742.1.3",
          "form": "넹 김치전이요~~",
          "original_form": "넹 김치전이요~~",
          "speaker_id": "1",
          "time": "20210831 11:27"
        },
        {
          "id": "MDRW2100036742.1.4",
          "form": "아까까지 꾸리꾸리해서 골랐는데 해뜨네요ㅋㅋ",
          "original_form": "아까까지 꾸리꾸리해서 골랐는데 해뜨네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:27"
        },
        {
          "id": "MDRW2100036742.1.5",
          "form": "해떠도 맛있으니까요!",
          "original_form": "해떠도 맛있으니까요!",
          "speaker_id": "2",
          "time": "20210831 11:30"
        },
        {
          "id": "MDRW2100036742.1.6",
          "form": "저는 전 바삭한게 좋아서 도넛모양으로 구워먹어요",
          "original_form": "저는 전 바삭한게 좋아서 도넛모양으로 구워먹어요",
          "speaker_id": "2",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100036742.1.7",
          "form": "크으 마자용",
          "original_form": "크으 마자용",
          "speaker_id": "1",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100036742.1.8",
          "form": "오? 그거 저 도전했는데 쉽지않던데요ㅠ",
          "original_form": "오? 그거 저 도전했는데 쉽지않던데요ㅠ",
          "speaker_id": "1",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100036742.1.9",
          "form": "헉 그런가요 기름 듬뿍쓰시고 작게만드시면 괜찮으실지도...?",
          "original_form": "헉 그런가요 기름 듬뿍쓰시고 작게만드시면 괜찮으실지도...?",
          "speaker_id": "2",
          "time": "20210831 11:35"
        },
        {
          "id": "MDRW2100036742.1.10",
          "form": "오!!? 그런건가욧 이따 도전해보고 알려드릴게요!!",
          "original_form": "오!!? 그런건가욧 이따 도전해보고 알려드릴게요!!",
          "speaker_id": "1",
          "time": "20210831 11:36"
        },
        {
          "id": "MDRW2100036742.1.11",
          "form": "아니면 작게작게 만들어드셔요!",
          "original_form": "아니면 작게작게 만들어드셔요!",
          "speaker_id": "2",
          "time": "20210831 11:37"
        },
        {
          "id": "MDRW2100036742.1.12",
          "form": "저는 귀찮을때 그렇게 해서먹어요",
          "original_form": "저는 귀찮을때 그렇게 해서먹어요",
          "speaker_id": "2",
          "time": "20210831 11:38"
        },
        {
          "id": "MDRW2100036742.1.13",
          "form": "그것도 좋겠어용!!",
          "original_form": "그것도 좋겠어용!!",
          "speaker_id": "1",
          "time": "20210831 11:38"
        },
        {
          "id": "MDRW2100036742.1.14",
          "form": "늘 크게만 먹었거든요ㅋㅋ",
          "original_form": "늘 크게만 먹었거든요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:39"
        },
        {
          "id": "MDRW2100036742.1.15",
          "form": "부추전하고 김치전중에 김치전을 더 좋아하시나요?",
          "original_form": "부추전하고 김치전중에 김치전을 더 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210831 11:39"
        },
        {
          "id": "MDRW2100036742.1.16",
          "form": "넹! 오징어 넣은 김치전이용",
          "original_form": "넹! 오징어 넣은 김치전이용",
          "speaker_id": "1",
          "time": "20210831 11:40"
        },
        {
          "id": "MDRW2100036742.1.17",
          "form": "name2님은요??",
          "original_form": "&name2&님은요??",
          "speaker_id": "1",
          "time": "20210831 11:40"
        },
        {
          "id": "MDRW2100036742.1.18",
          "form": "저는 부추전이요 오징어랑 홍합 넣은거요!",
          "original_form": "저는 부추전이요 오징어랑 홍합 넣은거요!",
          "speaker_id": "2",
          "time": "20210831 12:01"
        },
        {
          "id": "MDRW2100036742.1.19",
          "form": "오.. 그것도 맛있죠!",
          "original_form": "오.. 그것도 맛있죠!",
          "speaker_id": "1",
          "time": "20210831 12:05"
        },
        {
          "id": "MDRW2100036742.1.20",
          "form": "저 해봤는데 처음은 실패했어요ㅜㅠ",
          "original_form": "저 해봤는데 처음은 실패했어요ㅜㅠ",
