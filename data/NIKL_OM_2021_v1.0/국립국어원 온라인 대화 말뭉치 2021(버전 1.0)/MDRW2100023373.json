      "utterance": [
        {
          "id": "MDRW2100023373.1.1",
          "form": "맞아 나도 그걸 어디에선가 들었어",
          "original_form": "맞아 나도 그걸 어디에선가 들었어",
          "speaker_id": "2",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100023373.1.2",
          "form": "거기서 호랑이 빙글빙글 돌게하면",
          "original_form": "거기서 호랑이 빙글빙글 돌게하면",
          "speaker_id": "1",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100023373.1.3",
          "form": "일본에서 도는 괴담(?) 같은 건가",
          "original_form": "일본에서 도는 괴담(?) 같은 건가",
          "speaker_id": "2",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100023373.1.4",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100023373.1.5",
          "form": "호랑이 버터되서",
          "original_form": "호랑이 버터되서",
          "speaker_id": "1",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100023373.1.6",
          "form": "그걸로 나중에 팬케이크 만들어먹게 해줌ㅋㅋㅋㅋ",
          "original_form": "그걸로 나중에 팬케이크 만들어먹게 해줌ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100023373.1.7",
          "form": "왜지!",
          "original_form": "왜지!",
          "speaker_id": "1",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100023373.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100023373.1.9",
          "form": "ㅋㅋㅋ\nㅋㄱㄱ\nㅋㅋㄱ",
          "original_form": "ㅋㅋㅋ\nㅋㄱㄱ\nㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100023373.1.10",
          "form": "진짜왜지",
          "original_form": "진짜왜지",
          "speaker_id": "2",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100023373.1.11",
          "form": "별 기발한 발상을 다한다는 생각만",
          "original_form": "별 기발한 발상을 다한다는 생각만",
          "speaker_id": "2",
          "time": "20210830 01:05"
        },
        {
          "id": "MDRW2100023373.1.12",
          "form": "ㅋㅋㅋㅋㅋ진짜로",
          "original_form": "ㅋㅋㅋㅋㅋ진짜로",
          "speaker_id": "1",
          "time": "20210830 01:05"
        },
        {
          "id": "MDRW2100023373.1.13",
          "form": "그리고 쌤통이라고 생각한건 그 돈까스 되는놈...",
          "original_form": "그리고 쌤통이라고 생각한건 그 돈까스 되는놈...",
          "speaker_id": "1",
          "time": "20210830 01:05"
        },
        {
          "id": "MDRW2100023373.1.14",
          "form": "맞아..그 새우튀김..",
          "original_form": "맞아..그 새우튀김..",
          "speaker_id": "2",
          "time": "20210830 01:06"
        },
        {
          "id": "MDRW2100023373.1.15",
          "form": "1챕에서 죽어서 진짜 기뻤다",
          "original_form": "1챕에서 죽어서 진짜 기뻤다",
          "speaker_id": "2",
          "time": "20210830 01:06"
        },
        {
          "id": "MDRW2100023373.1.16",
          "form": "싸게싸게가쇼",
          "original_form": "싸게싸게가쇼",
          "speaker_id": "2",
          "time": "20210830 01:06"
        },
        {
          "id": "MDRW2100023373.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ정말로",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ정말로",
          "speaker_id": "1",
          "time": "20210830 01:06"
        },
        {
          "id": "MDRW2100023373.1.18",
          "form": "단간1에서는 그 키모오타쿠 진짜 오래 살아있었잖아",
          "original_form": "단간1에서는 그 키모오타쿠 진짜 오래 살아있었잖아",
          "speaker_id": "1",
          "time": "20210830 01:06"
        },
        {
          "id": "MDRW2100023373.1.19",
          "form": "걘 어떻게 죽었더라?",
          "original_form": "걘 어떻게 죽었더라?",
          "speaker_id": "1",
          "time": "20210830 01:06"
        },
        {
          "id": "MDRW2100023373.1.20",
          "form": "아 그 초고교급동인어저구?",
          "original_form": "아 그 초고교급동인어저구?",
          "speaker_id": "2",
          "time": "20210830 01:06"
        },
        {
          "id": "MDRW2100023373.1.21",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210830 01:06"
        },
        {
          "id": "MDRW2100023373.1.22",
          "form": "걔는.. 흠.. 뭐였지 사실 1이 젤 기억안남 애니로만 보기도 했고",
          "original_form": "걔는.. 흠.. 뭐였지 사실 1이 젤 기억안남 애니로만 보기도 했고",
          "speaker_id": "2",
          "time": "20210830 01:07"
        },
        {
          "id": "MDRW2100023373.1.23",
          "form": "흠..어디 매달려죽었던가",
          "original_form": "흠..어디 매달려죽었던가",
          "speaker_id": "2",
          "time": "20210830 01:07"
        },
        {
          "id": "MDRW2100023373.1.24",
          "form": "플레이 했는데도 기억안남...",
          "original_form": "플레이 했는데도 기억안남...",
          "speaker_id": "1",
          "time": "20210830 01:07"
        },
        {
          "id": "MDRW2100023373.1.25",
          "form": "뭐 걔가 중간에 멍청하게 낚여서 뭐 했던거 같은데",
          "original_form": "뭐 걔가 중간에 멍청하게 낚여서 뭐 했던거 같은데",
          "speaker_id": "1",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023373.1.26",
          "form": "아 맞아맞아 방금 찾아봤는데",
          "original_form": "아 맞아맞아 방금 찾아봤는데",
          "speaker_id": "2",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023373.1.27",
          "form": "첨에 낚여서 죽은척 쑈했다가",
          "original_form": "첨에 낚여서 죽은척 쑈했다가",
          "speaker_id": "2",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023373.1.28",
          "form": "나중에 진짜 살해당한..그런 어저구였음",
          "original_form": "나중에 진짜 살해당한..그런 어저구였음",
          "speaker_id": "2",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023373.1.29",
          "form": "엌ㅋㅋㅋㅋㅋ",
          "original_form": "엌ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023373.1.30",
          "form": "걍 살해당했었군...",
          "original_form": "걍 살해당했었군...",
          "speaker_id": "1",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023373.1.31",
          "form": "녀석 3챕에 죽었네 증맬질기네..",
          "original_form": "녀석 3챕에 죽었네 증맬질기네..",
          "speaker_id": "2",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023373.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023373.1.33",
          "form": "이놈 이름 히후미네",
          "original_form": "이놈 이름 히후미네",
          "speaker_id": "1",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023373.1.34",
          "form": "타장르 친구가 생각나",
          "original_form": "타장르 친구가 생각나",
          "speaker_id": "1",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023373.1.35",
          "form": "빻은 소리 많이 했어? 애니로 봐서 대사를 많이 못본..",
          "original_form": "빻은 소리 많이 했어? 애니로 봐서 대사를 많이 못본..",
          "speaker_id": "2",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023373.1.36",
          "form": "나도.. 그장르 사실 1도 모르는데",
          "original_form": "나도.. 그장르 사실 1도 모르는데",
          "speaker_id": "2",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023373.1.37",
          "form": "이름은 아는 기묘함",
          "original_form": "이름은 아는 기묘함",
          "speaker_id": "2",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100023373.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:11"
        },
        {
          "id": "MDRW2100023373.1.39",
          "form": "많이 했던거 같은데",
          "original_form": "많이 했던거 같은데",
          "speaker_id": "1",
          "time": "20210830 01:12"
        },
        {
          "id": "MDRW2100023373.1.40",
          "form": "약간 이 시리즈 전통으로...헛소리 하는 애들이 하나씩 껴있잖아...",
          "original_form": "약간 이 시리즈 전통으로...헛소리 하는 애들이 하나씩 껴있잖아...",
          "speaker_id": "1",
          "time": "20210830 01:12"
        },
        {
          "id": "MDRW2100023373.1.41",
          "form": "그게 1에서 얘였음..",
          "original_form": "그게 1에서 얘였음..",
          "speaker_id": "1",
          "time": "20210830 01:12"
        },
        {
          "id": "MDRW2100023373.1.42",
          "form": "...정말싫다.... 외관도 좀 많이 싫었는데",
          "original_form": "...정말싫다.... 외관도 좀 많이 싫었는데",
          "speaker_id": "2",
          "time": "20210830 01:12"
        },
        {
          "id": "MDRW2100023373.1.43",
          "form": "나는 단간3에서 그 빻은소리캐가... 여캐가 된게 제법웃겨",
          "original_form": "나는 단간3에서 그 빻은소리캐가... 여캐가 된게 제법웃겨",
          "speaker_id": "2",
          "time": "20210830 01:13"
        }
      ]
