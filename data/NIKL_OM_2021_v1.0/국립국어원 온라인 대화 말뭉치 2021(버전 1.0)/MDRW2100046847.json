      "utterance": [
        {
          "id": "MDRW2100046847.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211107 21:20"
        },
        {
          "id": "MDRW2100046847.1.2",
          "form": "안녕하세요~! 주말이 다 갔네요 ㅠ",
          "original_form": "안녕하세요~! 주말이 다 갔네요 ㅠ",
          "speaker_id": "2",
          "time": "20211107 21:20"
        },
        {
          "id": "MDRW2100046847.1.3",
          "form": "내일이 월요일이라니",
          "original_form": "내일이 월요일이라니",
          "speaker_id": "1",
          "time": "20211107 21:20"
        },
        {
          "id": "MDRW2100046847.1.4",
          "form": "우울합니다",
          "original_form": "우울합니다",
          "speaker_id": "1",
          "time": "20211107 21:20"
        },
        {
          "id": "MDRW2100046847.1.5",
          "form": "정말 믿기지가 않네요",
          "original_form": "정말 믿기지가 않네요",
          "speaker_id": "2",
          "time": "20211107 21:21"
        },
        {
          "id": "MDRW2100046847.1.6",
          "form": "시간이 주말만 2배속 한듯요 ㅋㅋ",
          "original_form": "시간이 주말만 2배속 한듯요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211107 21:21"
        },
        {
          "id": "MDRW2100046847.1.7",
          "form": "주말날씨가 좋았어요",
          "original_form": "주말날씨가 좋았어요",
          "speaker_id": "1",
          "time": "20211107 21:21"
        },
        {
          "id": "MDRW2100046847.1.8",
          "form": "맞아요! 단풍은 좀 보셨나요",
          "original_form": "맞아요! 단풍은 좀 보셨나요",
          "speaker_id": "2",
          "time": "20211107 21:21"
        },
        {
          "id": "MDRW2100046847.1.9",
          "form": "네 산책하고 왔어요",
          "original_form": "네 산책하고 왔어요",
          "speaker_id": "1",
          "time": "20211107 21:21"
        },
        {
          "id": "MDRW2100046847.1.10",
          "form": "단풍이 예쁘더라구요",
          "original_form": "단풍이 예쁘더라구요",
          "speaker_id": "1",
          "time": "20211107 21:22"
        },
        {
          "id": "MDRW2100046847.1.11",
          "form": "저도 집앞에 산책하고 왔는데",
          "original_form": "저도 집앞에 산책하고 왔는데",
          "speaker_id": "2",
          "time": "20211107 21:22"
        },
        {
          "id": "MDRW2100046847.1.12",
          "form": "진짜 가을날씨더라구요",
          "original_form": "진짜 가을날씨더라구요",
          "speaker_id": "2",
          "time": "20211107 21:22"
        },
        {
          "id": "MDRW2100046847.1.13",
          "form": "근데 내일부턴 추워진대요",
          "original_form": "근데 내일부턴 추워진대요",
          "speaker_id": "2",
          "time": "20211107 21:22"
        },
        {
          "id": "MDRW2100046847.1.14",
          "form": "정말요?",
          "original_form": "정말요?",
          "speaker_id": "1",
          "time": "20211107 21:22"
        },
        {
          "id": "MDRW2100046847.1.15",
          "form": "넵 기온이 뚝 떨어진다던데",
          "original_form": "넵 기온이 뚝 떨어진다던데",
          "speaker_id": "2",
          "time": "20211107 21:22"
        },
        {
          "id": "MDRW2100046847.1.16",
          "form": "겨울옷꺼내야겠네요",
          "original_form": "겨울옷꺼내야겠네요",
          "speaker_id": "1",
          "time": "20211107 21:23"
        },
        {
          "id": "MDRW2100046847.1.17",
          "form": "옷 잘 챙겨입으세요",
          "original_form": "옷 잘 챙겨입으세요",
          "speaker_id": "2",
          "time": "20211107 21:23"
        },
        {
          "id": "MDRW2100046847.1.18",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "1",
          "time": "20211107 21:23"
        },
        {
          "id": "MDRW2100046847.1.19",
          "form": "감기조심하세요",
          "original_form": "감기조심하세요",
          "speaker_id": "1",
          "time": "20211107 21:23"
        },
        {
          "id": "MDRW2100046847.1.20",
