      "utterance": [
        {
          "id": "MDRW2100032347.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210905 16:01"
        },
        {
          "id": "MDRW2100032347.1.2",
          "form": "네 자주 가지는 못 하지만 좋아해요",
          "original_form": "네 자주 가지는 못 하지만 좋아해요",
          "speaker_id": "2",
          "time": "20210905 16:06"
        },
        {
          "id": "MDRW2100032347.1.3",
          "form": "그러시군요 코로나 진정되면 어디 가고싶으신가요",
          "original_form": "그러시군요 코로나 진정되면 어디 가고싶으신가요",
          "speaker_id": "1",
          "time": "20210905 16:08"
        },
        {
          "id": "MDRW2100032347.1.4",
          "form": "제주도요",
          "original_form": "제주도요",
          "speaker_id": "2",
          "time": "20210905 16:29"
        },
        {
          "id": "MDRW2100032347.1.5",
          "form": "제주도 좋더라구요",
          "original_form": "제주도 좋더라구요",
          "speaker_id": "1",
          "time": "20210905 16:46"
        },
        {
          "id": "MDRW2100032347.1.6",
          "form": "제주도 여행 가보셨어요?",
          "original_form": "제주도 여행 가보셨어요?",
          "speaker_id": "2",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100032347.1.7",
          "form": "네 올해 5월에 다녀왔어요",
          "original_form": "네 올해 5월에 다녀왔어요",
          "speaker_id": "1",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100032347.1.8",
          "form": "따뜻할 때 다녀오셨네요~\r\n제주도에서 어떤 곳이 가장 기억에 남으셨나요?",
          "original_form": "따뜻할 때 다녀오셨네요~\r\n제주도에서 어떤 곳이 가장 기억에 남으셨나요?",
          "speaker_id": "2",
          "time": "20210905 17:05"
        },
        {
          "id": "MDRW2100032347.1.9",
          "form": "바다.해변이 예쁘더라구요 협재.애월 갔었어요",
          "original_form": "바다.해변이 예쁘더라구요 협재.애월 갔었어요",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032347.1.10",
          "form": "오 제주도 바다 너무 예쁘죠ㅠㅠ\r\n얼른 가보고 싶네요",
          "original_form": "오 제주도 바다 너무 예쁘죠ㅠㅠ\r\n얼른 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210905 17:10"
        },
        {
          "id": "MDRW2100032347.1.11",
          "form": "해외여행 가보셨나요?",
          "original_form": "해외여행 가보셨나요?",
          "speaker_id": "2",
          "time": "20210905 17:11"
        },
        {
          "id": "MDRW2100032347.1.12",
          "form": "네 몇번 안되지만..name2님은요?",
          "original_form": "네 몇번 안되지만..&name2&님은요?",
          "speaker_id": "1",
          "time": "20210905 18:47"
        },
        {
          "id": "MDRW2100032347.1.13",
          "form": "저는 해외 한 번도 못 가봤어요ㅜㅜ\r\n어디 다녀와보셨어요?",
          "original_form": "저는 해외 한 번도 못 가봤어요ㅜㅜ\r\n어디 다녀와보셨어요?",
          "speaker_id": "2",
          "time": "20210905 18:55"
        },
        {
          "id": "MDRW2100032347.1.14",
          "form": "신혼여행으로 다녀온 몰디브가 기억에 남네요",
          "original_form": "신혼여행으로 다녀온 몰디브가 기억에 남네요",
          "speaker_id": "1",
          "time": "20210905 20:16"
        },
        {
          "id": "MDRW2100032347.1.15",
          "form": "몰디브 너무 좋았을 것 같아요!",
          "original_form": "몰디브 너무 좋았을 것 같아요!",
          "speaker_id": "2",
          "time": "20210905 20:31"
        },
        {
          "id": "MDRW2100032347.1.16",
          "form": "네 신혼여행지로 생각해보신곳 있으신가요",
          "original_form": "네 신혼여행지로 생각해보신곳 있으신가요",
          "speaker_id": "1",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032347.1.17",
          "form": "아니요..아직 결혼에 대한 진지한 생각을 하기 전 이여서, 신혼여행지까지는 생각 안 해봤네요",
          "original_form": "아니요..아직 결혼에 대한 진지한 생각을 하기 전 이여서, 신혼여행지까지는 생각 안 해봤네요",
          "speaker_id": "2",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100032347.1.18",
          "form": "그러시군요~! 저는 하와이랑 고민하다가 몰디브로 갔거든요",
          "original_form": "그러시군요~! 저는 하와이랑 고민하다가 몰디브로 갔거든요",
          "speaker_id": "1",
          "time": "20210906 05:46"
        }
