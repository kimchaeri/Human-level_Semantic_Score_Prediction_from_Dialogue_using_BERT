      "utterance": [
        {
          "id": "MDRW2100004505.1.1",
          "form": "name1님 개학이나 개강하셨나요?",
          "original_form": "&name1&님 개학이나 개강하셨나요?",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004505.1.2",
          "form": "이제 곧이죠 ㅎ",
          "original_form": "이제 곧이죠 ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004505.1.3",
          "form": "저흰느 수강신청을 늦게햐서",
          "original_form": "저흰느 수강신청을 늦게햐서",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004505.1.4",
          "form": "이제 곧할텐대",
          "original_form": "이제 곧할텐대",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004505.1.5",
          "form": "떨려요",
          "original_form": "떨려요",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004505.1.6",
          "form": "아 저는 어제 했어요",
          "original_form": "아 저는 어제 했어요",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004505.1.7",
          "form": "진짜 너무 떨리죠",
          "original_form": "진짜 너무 떨리죠",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004505.1.8",
          "form": "원하는 강의 잘 잡으셨나요?",
          "original_form": "원하는 강의 잘 잡으셨나요?",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004505.1.9",
          "form": "안전하게 가서 1순위 올클했습니다~",
          "original_form": "안전하게 가서 1순위 올클했습니다~",
          "speaker_id": "2",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004505.1.10",
          "form": "안전하게 가시나요 아니면 꿀강의 도전하시나요?",
          "original_form": "안전하게 가시나요 아니면 꿀강의 도전하시나요?",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004505.1.11",
          "form": "이야 선방하셨내요",
          "original_form": "이야 선방하셨내요",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004505.1.12",
          "form": "저는 안전하게 가는 편이에요",
          "original_form": "저는 안전하게 가는 편이에요",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004505.1.13",
          "form": "뭐든 적당히 만족하고 듣는 편이라",
          "original_form": "뭐든 적당히 만족하고 듣는 편이라",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004505.1.14",
          "form": "글고 꿀강의는 1학년때 이미 다들어둬서 ㅎㅎ",
          "original_form": "글고 꿀강의는 1학년때 이미 다들어둬서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004505.1.15",
          "form": "1학년 때 전공 과목이 없어서 꿀교양 많이 듣죠",
          "original_form": "1학년 때 전공 과목이 없어서 꿀교양 많이 듣죠",
          "speaker_id": "2",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004505.1.16",
          "form": "저는 전선이나 기선 듣느라 아직 못 들은 꿀교양은 많은데 도전하기가 쉽지 않더라고요",
          "original_form": "저는 전선이나 기선 듣느라 아직 못 들은 꿀교양은 많은데 도전하기가 쉽지 않더라고요",
          "speaker_id": "2",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004505.1.17",
          "form": "꿀교양은 인기가 많으니 어쩔수 없죠 ㅠㅠ",
          "original_form": "꿀교양은 인기가 많으니 어쩔수 없죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:19"
        },
        {
          "id": "MDRW2100004505.1.18",
          "form": "근데 전공이 어떻게 되세요?",
          "original_form": "근데 전공이 어떻게 되세요?",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004505.1.19",
          "form": "저는 공대쪽이에요",
          "original_form": "저는 공대쪽이에요",
          "speaker_id": "2",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004505.1.20",
          "form": "저도요 ㅎ",
          "original_form": "저도요 ㅎ",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004505.1.21",
          "form": "공감가서 슬쩍 여쭤봤어요",
          "original_form": "공감가서 슬쩍 여쭤봤어요",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004505.1.22",
          "form": "저희가 기초 과목이 좀 많죠ㅋㅋ",
          "original_form": "저희가 기초 과목이 좀 많죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004505.1.23",
          "form": "강의평 좋은 기초 과목 들으려면 교양은 자연스럽게 포기하게 되더라고요",
          "original_form": "강의평 좋은 기초 과목 들으려면 교양은 자연스럽게 포기하게 되더라고요",
          "speaker_id": "2",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004505.1.24",
          "form": "그래도 막학기쯤 가면",
          "original_form": "그래도 막학기쯤 가면",
          "speaker_id": "1",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004505.1.25",
          "form": "전공 조금만 들어도 되겠어요",
          "original_form": "전공 조금만 들어도 되겠어요",
          "speaker_id": "1",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004505.1.26",
          "form": "저는 교양 미리들었다가",
          "original_form": "저는 교양 미리들었다가",
          "speaker_id": "1",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004505.1.27",
          "form": "7전공하고 쓰러지는 줄 알았어요",
          "original_form": "7전공하고 쓰러지는 줄 알았어요",
          "speaker_id": "1",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004505.1.28",
          "form": "7전공 정말 상상만 해도 무섭네요",
          "original_form": "7전공 정말 상상만 해도 무섭네요",
          "speaker_id": "2",
          "time": "20210821 17:26"
        },
        {
          "id": "MDRW2100004505.1.29",
          "form": "수고하셨어요..",
          "original_form": "수고하셨어요..",
          "speaker_id": "2",
          "time": "20210821 17:27"
        }
      ]
    }
