      "utterance": [
        {
          "id": "MDRW2100042826.1.1",
          "form": "아 너무 웃긴다ㅋㅋㅋㅋ",
          "original_form": "아 너무 웃긴다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:42"
        },
        {
          "id": "MDRW2100042826.1.2",
          "form": "나도 장담을 할 수 없다.",
          "original_form": "나도 장담을 할 수 없다.",
          "speaker_id": "1",
          "time": "20210907 09:42"
        },
        {
          "id": "MDRW2100042826.1.3",
          "form": "무셔버 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "무셔버 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:42"
        },
        {
          "id": "MDRW2100042826.1.4",
          "form": "그래 누구도 믿을 수 없다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래 누구도 믿을 수 없다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:43"
        },
        {
          "id": "MDRW2100042826.1.5",
          "form": "이제 끝났다 아니야??",
          "original_form": "이제 끝났다 아니야??",
          "speaker_id": "2",
          "time": "20210907 09:43"
        },
        {
          "id": "MDRW2100042826.1.6",
          "form": "그러니깐 내가 또 욕할수도 ㅋㅋ 욕을 먹을수도 있는 ㅋㅋ",
          "original_form": "그러니깐 내가 또 욕할수도 ㅋㅋ 욕을 먹을수도 있는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:43"
        },
        {
          "id": "MDRW2100042826.1.7",
          "form": "그치 서로 먹고 먹히는ㅋㅋㅋㅋ",
          "original_form": "그치 서로 먹고 먹히는ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:43"
        },
        {
          "id": "MDRW2100042826.1.8",
          "form": "미라 언니 정말 오랜만에 본다 ㅋㅋㅋ",
          "original_form": "미라 언니 정말 오랜만에 본다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:44"
        },
        {
          "id": "MDRW2100042826.1.9",
          "form": "언니도 할 말 많을 듯",
          "original_form": "언니도 할 말 많을 듯",
          "speaker_id": "2",
          "time": "20210907 09:44"
        },
        {
          "id": "MDRW2100042826.1.10",
          "form": "그러니깐은 ㅋㅋ",
          "original_form": "그러니깐은 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:44"
        },
        {
          "id": "MDRW2100042826.1.11",
          "form": "다들 지금",
          "original_form": "다들 지금",
          "speaker_id": "1",
          "time": "20210907 09:44"
        },
        {
          "id": "MDRW2100042826.1.12",
          "form": "할말이 많다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "할말이 많다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:45"
        },
        {
          "id": "MDRW2100042826.1.13",
          "form": "그래 코로는 아니었으면 저녁에 막창 한번 갔을 텐데",
          "original_form": "그래 코로는 아니었으면 저녁에 막창 한번 갔을 텐데",
          "speaker_id": "2",
          "time": "20210907 09:45"
        },
        {
          "id": "MDRW2100042826.1.14",
          "form": "너무 오래 묵었다ㅋㅋㅋㅋ",
          "original_form": "너무 오래 묵었다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:45"
        },
        {
          "id": "MDRW2100042826.1.15",
          "form": "맞어 ㅠㅠ",
          "original_form": "맞어 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042826.1.16",
          "form": "먹으러 가고싶다",
          "original_form": "먹으러 가고싶다",
          "speaker_id": "1",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042826.1.17",
          "form": "그러니까 가서 먹는 그 맛인데",
          "original_form": "그러니까 가서 먹는 그 맛인데",
          "speaker_id": "2",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042826.1.18",
          "form": "저번에 막창가게 밤에 지나갔는데 정말 사람없긴하더라",
          "original_form": "저번에 막창가게 밤에 지나갔는데 정말 사람없긴하더라",
          "speaker_id": "1",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042826.1.19",
          "form": "그 오케이마트 위에",
          "original_form": "그 오케이마트 위에",
          "speaker_id": "1",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042826.1.20",
          "form": "음식점에서 가 솔직히 제일 위험하니까",
          "original_form": "음식점에서 가 솔직히 제일 위험하니까",
          "speaker_id": "2",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042826.1.21",
          "form": "근데 그 오케이 마트 위에는 원래 없지 않나",
          "original_form": "근데 그 오케이 마트 위에는 원래 없지 않나",
          "speaker_id": "2",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042826.1.22",
          "form": "양이 너무 작아서",
          "original_form": "양이 너무 작아서",
          "speaker_id": "2",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042826.1.23",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042826.1.24",
          "form": "그러니깐",
          "original_form": "그러니깐",
          "speaker_id": "1",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042826.1.25",
          "form": "우리도 한번 가고 안 가잖아ㅋㅋㅋㅋ",
          "original_form": "우리도 한번 가고 안 가잖아ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042826.1.26",
          "form": "맛도 막창 도둑이 더 맛있었 아마 이제",
          "original_form": "맛도 막창 도둑이 더 맛있었 아마 이제",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042826.1.27",
          "form": "그렇긴 하지",
          "original_form": "그렇긴 하지",
          "speaker_id": "1",
          "time": "20210907 09:47"
        }
      ]
