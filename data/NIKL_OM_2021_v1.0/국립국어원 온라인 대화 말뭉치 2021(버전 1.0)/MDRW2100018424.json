          "time": "20210827 01:54"
        },
        {
          "id": "MDRW2100018424.1.20",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210827 01:56"
        },
        {
          "id": "MDRW2100018424.1.21",
