          "time": "20210827 01:41"
        },
        {
          "id": "MDRW2100018424.1.4",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:41"
        },
        {
          "id": "MDRW2100018424.1.5",
          "form": "전자기기를",
          "original_form": "전자기기를",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018424.1.6",
          "form": "사고싶어요",
          "original_form": "사고싶어요",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018424.1.7",
          "form": "전자기기 어떤거요?",
          "original_form": "전자기기 어떤거요?",
          "speaker_id": "1",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018424.1.8",
          "form": "노트북이요!!!!",
          "original_form": "노트북이요!!!!",
          "speaker_id": "2",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018424.1.9",
          "form": "헉 저도... 노트북을 사야해요 ㅠㅠ",
          "original_form": "헉 저도... 노트북을 사야해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 01:46"
        },
        {
          "id": "MDRW2100018424.1.10",
          "form": "떡볶이 쏟은 뒤로 애가 고장이 났네요..",
          "original_form": "떡볶이 쏟은 뒤로 애가 고장이 났네요..",
          "speaker_id": "1",
          "time": "20210827 01:46"
        },
        {
          "id": "MDRW2100018424.1.11",
          "form": "와우ㅠ 저런",
          "original_form": "와우ㅠ 저런",
          "speaker_id": "2",
          "time": "20210827 01:47"
        },
        {
          "id": "MDRW2100018424.1.12",
          "form": "전 그램사고싶어요ㅠ",
          "original_form": "전 그램사고싶어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 01:47"
        },
        {
          "id": "MDRW2100018424.1.13",
          "form": "아 저두요 ㅠㅠ 지금 것도 엘지인데 엘지가 그래도 쓰기편하게 잘만드는 거 같아요",
          "original_form": "아 저두요 ㅠㅠ 지금 것도 엘지인데 엘지가 그래도 쓰기편하게 잘만드는 거 같아요",
          "speaker_id": "1",
          "time": "20210827 01:49"
        },
        {
          "id": "MDRW2100018424.1.14",
          "form": "근데 그램 넘 비싸단게 함정 ㅠ",
          "original_form": "근데 그램 넘 비싸단게 함정 ㅠ",
          "speaker_id": "1",
          "time": "20210827 01:49"
        },
        {
          "id": "MDRW2100018424.1.15",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 01:52"
        },
        {
          "id": "MDRW2100018424.1.16",
          "form": "가격너무사기아니예요?",
          "original_form": "가격너무사기아니예요?",
          "speaker_id": "2",
          "time": "20210827 01:52"
        },
        {
          "id": "MDRW2100018424.1.17",
          "form": "정말.. 가격 물어봤다가 일단 내려놨어요 ㅋㅋㅋㅠ",
          "original_form": "정말.. 가격 물어봤다가 일단 내려놨어요 ㅋㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210827 01:54"
        },
        {
          "id": "MDRW2100018424.1.18",
          "form": "할인 뜨면 사려구요...",
          "original_form": "할인 뜨면 사려구요...",
          "speaker_id": "1",
          "time": "20210827 01:54"
        },
        {
          "id": "MDRW2100018424.1.19",
          "form": "핸드폰부터 살까 싶기도 하고..",
          "original_form": "핸드폰부터 살까 싶기도 하고..",
