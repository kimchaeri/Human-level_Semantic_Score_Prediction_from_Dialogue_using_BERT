      "utterance": [
        {
          "id": "MDRW2100027379.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100027379.1.2",
          "form": "안녕하세요~ 최근에 쇼핑하셨던 물건이 뭐에요?",
          "original_form": "안녕하세요~ 최근에 쇼핑하셨던 물건이 뭐에요?",
          "speaker_id": "2",
          "time": "20210902 12:05"
        },
        {
          "id": "MDRW2100027379.1.3",
          "form": "아이돌 포토북 예약 구매 했어요",
          "original_form": "아이돌 포토북 예약 구매 했어요",
          "speaker_id": "1",
          "time": "20210902 12:05"
        },
        {
          "id": "MDRW2100027379.1.4",
          "form": "와 언제쯤 도착하나요? 설레시겠어요ㅋㅋ",
          "original_form": "와 언제쯤 도착하나요? 설레시겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 12:09"
        },
        {
          "id": "MDRW2100027379.1.5",
          "form": "9월말까지 예약이라 아직 멀었어요...ㅎㅎ",
          "original_form": "9월말까지 예약이라 아직 멀었어요...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:13"
        },
        {
          "id": "MDRW2100027379.1.6",
          "form": "미래의 나에게 주는 선물이네요!",
          "original_form": "미래의 나에게 주는 선물이네요!",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027379.1.7",
          "form": "저는 어제 새우산을 샀는데 앞으로 비소식이 없어졌어요..",
          "original_form": "저는 어제 새우산을 샀는데 앞으로 비소식이 없어졌어요..",
          "speaker_id": "2",
          "time": "20210902 12:22"
        },
        {
          "id": "MDRW2100027379.1.8",
          "form": "아... 장마가 곧 끝나죠",
          "original_form": "아... 장마가 곧 끝나죠",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027379.1.9",
          "form": "저도 내년의 절 위한 선물인걸로ㅜㅜㅋ",
          "original_form": "저도 내년의 절 위한 선물인걸로ㅜㅜㅋ",
          "speaker_id": "2",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100027379.1.10",
          "form": "아니에요 언젠가 눈이 올수도 있죠",
          "original_form": "아니에요 언젠가 눈이 올수도 있죠",
          "speaker_id": "1",
          "time": "20210902 12:26"
        },
        {
          "id": "MDRW2100027379.1.11",
          "form": "맞아요! 겨울에 눈이.. 많이?",
          "original_form": "맞아요! 겨울에 눈이.. 많이?",
          "speaker_id": "2",
          "time": "20210902 12:29"
        },
        {
          "id": "MDRW2100027379.1.12",
          "form": "적당히 왔으면 좋겠어요",
          "original_form": "적당히 왔으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210902 12:31"
        },
        {
          "id": "MDRW2100027379.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 12:31"
        },
        {
          "id": "MDRW2100027379.1.14",
          "form": "사고싶으신 물건 있으세요?",
          "original_form": "사고싶으신 물건 있으세요?",
          "speaker_id": "2",
          "time": "20210902 12:32"
        },
        {
          "id": "MDRW2100027379.1.15",
          "form": "저는 손목보호대 하나 사고 싶어요",
          "original_form": "저는 손목보호대 하나 사고 싶어요",
          "speaker_id": "1",
          "time": "20210902 12:33"
        },
        {
          "id": "MDRW2100027379.1.16",
          "form": "오 보고 계신거 있으세요?",
          "original_form": "오 보고 계신거 있으세요?",
          "speaker_id": "2",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100027379.1.17",
          "form": "보고 있는건 따로 없고",
          "original_form": "보고 있는건 따로 없고",
          "speaker_id": "1",
          "time": "20210902 12:34"
        },
        {
          "id": "MDRW2100027379.1.18",
          "form": "그림 그리는 사람들이 쓰는거 구하려구요",
          "original_form": "그림 그리는 사람들이 쓰는거 구하려구요",
          "speaker_id": "1",
          "time": "20210902 12:35"
        },
        {
          "id": "MDRW2100027379.1.19",
          "form": "몸으로 입증된 상품이 최고죠",
          "original_form": "몸으로 입증된 상품이 최고죠",
          "speaker_id": "2",
          "time": "20210902 12:36"
        }
