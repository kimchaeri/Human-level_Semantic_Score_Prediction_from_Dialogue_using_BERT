      "utterance": [
        {
          "id": "MDRW2100042701.1.1",
          "form": "갑자기바로다음에 얼어죽을ㅋㅋㄱㅋㅋㅋㅋㄱ",
          "original_form": "갑자기바로다음에 얼어죽을ㅋㅋㄱㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 02:00"
        },
        {
          "id": "MDRW2100042701.1.2",
          "form": "사랑은 얼어죽을ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "사랑은 얼어죽을ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 02:00"
        },
        {
          "id": "MDRW2100042701.1.3",
          "form": "제목이 좀 세죠",
          "original_form": "제목이 좀 세죠",
          "speaker_id": "1",
          "time": "20210907 02:01"
        },
        {
          "id": "MDRW2100042701.1.4",
          "form": "일단 곡은 신나긴 합니다",
          "original_form": "일단 곡은 신나긴 합니다",
          "speaker_id": "1",
          "time": "20210907 02:01"
        },
        {
          "id": "MDRW2100042701.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅇㅇㅇ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㄱㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 02:02"
        },
        {
          "id": "MDRW2100042701.1.6",
          "form": "가장신나네여",
          "original_form": "가장신나네여",
          "speaker_id": "2",
          "time": "20210907 02:02"
        },
        {
          "id": "MDRW2100042701.1.7",
          "form": "ㅋㅋㅋㄱㅋㄱㅋㄱㄱ",
          "original_form": "ㅋㅋㅋㄱㅋㄱㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210907 02:02"
        },
        {
          "id": "MDRW2100042701.1.8",
          "form": "노래가 전체적으로 통일감있어서",
          "original_form": "노래가 전체적으로 통일감있어서",
          "speaker_id": "2",
          "time": "20210907 02:04"
        },
        {
          "id": "MDRW2100042701.1.9",
          "form": "괜찮은데",
          "original_form": "괜찮은데",
          "speaker_id": "2",
