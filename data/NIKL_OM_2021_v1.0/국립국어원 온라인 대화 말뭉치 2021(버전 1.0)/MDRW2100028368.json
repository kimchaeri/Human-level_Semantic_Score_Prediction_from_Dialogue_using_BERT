      "utterance": [
        {
          "id": "MDRW2100028368.1.1",
          "form": "오 공연좋아하세요?",
          "original_form": "오 공연좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028368.1.2",
          "form": "기회만 되면 보려고 노력은 하는데 지방이라 쉽지않네요 ㅎㅎ",
          "original_form": "기회만 되면 보려고 노력은 하는데 지방이라 쉽지않네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100028368.1.3",
          "form": "공연 좋아하세요??",
          "original_form": "공연 좋아하세요??",
          "speaker_id": "1",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100028368.1.4",
          "form": "뮤지컬 좋아해요!",
          "original_form": "뮤지컬 좋아해요!",
          "speaker_id": "2",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100028368.1.5",
          "form": "표구하기가 어려워서 자주는 못보지만ㅜㅜ",
          "original_form": "표구하기가 어려워서 자주는 못보지만ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 20:17"
        },
        {
          "id": "MDRW2100028368.1.6",
          "form": "뮤지컬은 코어팬이 많아서 더 그런 것 같아요 ㅎㅎ",
          "original_form": "뮤지컬은 코어팬이 많아서 더 그런 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028368.1.7",
          "form": "엄청나더라구요ㄷㄷ",
          "original_form": "엄청나더라구요ㄷㄷ",
          "speaker_id": "2",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028368.1.8",
          "form": "그쵸 ㅋㅋㅋㅋ 신입 입장에서는 좀 무서워요 ㅎㅎ",
          "original_form": "그쵸 ㅋㅋㅋㅋ 신입 입장에서는 좀 무서워요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028368.1.9",
          "form": "텃세같은 분위기가ㅋㅋ",
          "original_form": "텃세같은 분위기가ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028368.1.10",
          "form": "전시회도 좋은데 지식이 없어서 슬퍼요ㅠㅠ",
          "original_form": "전시회도 좋은데 지식이 없어서 슬퍼요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028368.1.11",
          "form": "저도 예술적 지식이 풍부한 건 아니지만 그냥 부딪히고 있어요 ㅎㅎ",
          "original_form": "저도 예술적 지식이 풍부한 건 아니지만 그냥 부딪히고 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028368.1.12",
          "form": "봐야 는다고해서 보는 것부터 하려구요 ㅎㅎ",
          "original_form": "봐야 는다고해서 보는 것부터 하려구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028368.1.13",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028368.1.14",
          "form": "멋지시네요",
          "original_form": "멋지시네요",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028368.1.15",
          "form": "아니에요ㅠ 그냥 가는거에요 ㅎㅎ",
          "original_form": "아니에요ㅠ 그냥 가는거에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028368.1.16",
          "form": "요즘은 코로나때문에 공연을 잘 안해서 슬퍼요ㅠㅠ",
          "original_form": "요즘은 코로나때문에 공연을 잘 안해서 슬퍼요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028368.1.17",
          "form": "클래식같은것도 들으러가고싶은데",
          "original_form": "클래식같은것도 들으러가고싶은데",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028368.1.18",
          "form": "맞아요ㅠ 잠깐 입장 받나싶더니 또 막혔더라구요ㅠㅠ",
          "original_form": "맞아요ㅠ 잠깐 입장 받나싶더니 또 막혔더라구요ㅠㅠ",
          "speaker_id": "1",
