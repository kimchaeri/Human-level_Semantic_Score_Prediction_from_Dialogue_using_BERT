      "utterance": [
        {
          "id": "MDRW2100000983.1.1",
          "form": "주말에 비가 온다더라구요...",
          "original_form": "주말에 비가 온다더라구요...",
          "speaker_id": "1",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100000983.1.2",
          "form": "제가사는지역은 벌써 비가 왕창내리고있어요 ㅜ",
          "original_form": "제가사는지역은 벌써 비가 왕창내리고있어요 ㅜ",
          "speaker_id": "2",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000983.1.3",
          "form": "벌써요??? 여기는 서울인데 안와요",
          "original_form": "벌써요??? 여기는 서울인데 안와요",
          "speaker_id": "1",
          "time": "20210819 21:09"
        },
        {
          "id": "MDRW2100000983.1.4",
          "form": "거기는 어디죠??",
          "original_form": "거기는 어디죠??",
          "speaker_id": "1",
          "time": "20210819 21:09"
        },
        {
          "id": "MDRW2100000983.1.5",
          "form": "여기 경상도에요 비미친듯이와요ㅜ",
          "original_form": "여기 경상도에요 비미친듯이와요ㅜ",
          "speaker_id": "2",
          "time": "20210819 21:12"
        },
        {
          "id": "MDRW2100000983.1.6",
          "form": "아 ㅜㅠ 뉴스에서 계속 나오더라구요",
          "original_form": "아 ㅜㅠ 뉴스에서 계속 나오더라구요",
          "speaker_id": "1",
          "time": "20210819 21:12"
        },
        {
          "id": "MDRW2100000983.1.7",
          "form": "ㅜㅜ요새 날이 계속 꾸리하더니 쏟아지네요",
          "original_form": "ㅜㅜ요새 날이 계속 꾸리하더니 쏟아지네요",
          "speaker_id": "2",
          "time": "20210819 21:14"
        },
        {
          "id": "MDRW2100000983.1.8",
          "form": "장마라던데 언제끝날런지",
          "original_form": "장마라던데 언제끝날런지",
          "speaker_id": "2",
          "time": "20210819 21:14"
        },
        {
          "id": "MDRW2100000983.1.9",
          "form": "지반 낮은 곳 잠기고 그런다던데 ㅠㅜ 비 무서워요",
          "original_form": "지반 낮은 곳 잠기고 그런다던데 ㅠㅜ 비 무서워요",
          "speaker_id": "1",
          "time": "20210819 21:14"
        },
        {
          "id": "MDRW2100000983.1.10",
          "form": "그럴거같아요 쏟아지는게 그냥그칠비가 아닌것같아서ㅜㅜ",
          "original_form": "그럴거같아요 쏟아지는게 그냥그칠비가 아닌것같아서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 21:16"
        },
        {
          "id": "MDRW2100000983.1.11",
          "form": "8월 이맘때에 원래 비가 왔던지 기억이 안나네요...",
          "original_form": "8월 이맘때에 원래 비가 왔던지 기억이 안나네요...",
          "speaker_id": "1",
          "time": "20210819 21:17"
        },
        {
          "id": "MDRW2100000983.1.12",
          "form": "작년에는 비만왔던기억이나는데 그땐 진짜 너무왔어서 그전에가 저도 기억이안나네요",
          "original_form": "작년에는 비만왔던기억이나는데 그땐 진짜 너무왔어서 그전에가 저도 기억이안나네요",
          "speaker_id": "2",
          "time": "20210819 21:18"
        },
        {
          "id": "MDRW2100000983.1.13",
          "form": "원래이렇게왔었는지 ㅎㅎ..",
          "original_form": "원래이렇게왔었는지 ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210819 21:18"
        },
        {
          "id": "MDRW2100000983.1.14",
          "form": "날씨 어플에 너무 비만 있으니까 보기 싫어요 ㅋㅋㅋ",
          "original_form": "날씨 어플에 너무 비만 있으니까 보기 싫어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:18"
        },
        {
          "id": "MDRW2100000983.1.15",
          "form": "그니까요 습하고 찝찝하고",
          "original_form": "그니까요 습하고 찝찝하고",
          "speaker_id": "2",
          "time": "20210819 21:20"
        },
        {
          "id": "MDRW2100000983.1.16",
          "form": "지금 천둥도치네요ㅜ",
          "original_form": "지금 천둥도치네요ㅜ",
          "speaker_id": "2",
          "time": "20210819 21:20"
        },
        {
          "id": "MDRW2100000983.1.17",
          "form": "와... 예사 날씨가 아니네요 ㅠㅜ",
          "original_form": "와... 예사 날씨가 아니네요 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100000983.1.18",
          "form": "여기도 곧 소식이 있을거같네요 ㅠㅠ",
          "original_form": "여기도 곧 소식이 있을거같네요 ㅠㅠ",
          "speaker_id": "1",
