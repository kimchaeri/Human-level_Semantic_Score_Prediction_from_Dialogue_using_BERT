      "utterance": [
        {
          "id": "MDRW2100007231.1.1",
          "form": "어떻게 남편분이 또 성씨시네요 ㅎㅎ",
          "original_form": "어떻게 남편분이 또 성씨시네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007231.1.2",
          "form": "이름 완전 비슷하면서 예쁘게 잘 지으셨어요!!",
          "original_form": "이름 완전 비슷하면서 예쁘게 잘 지으셨어요!!",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007231.1.3",
          "form": "ㅎㅎ 성시경님 덕분이죠 ㅎ",
          "original_form": "ㅎㅎ 성시경님 덕분이죠 ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100007231.1.4",
          "form": "음식솜씨도 좋으시고",
          "original_form": "음식솜씨도 좋으시고",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007231.1.5",
          "form": "오 음식도 잘하시는군요!",
          "original_form": "오 음식도 잘하시는군요!",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007231.1.6",
          "form": "온앤오프에서 직접 소세지만드시는거 나오던데 보셨나요?",
          "original_form": "온앤오프에서 직접 소세지만드시는거 나오던데 보셨나요?",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007231.1.7",
          "form": "헐 소세지도 직접 만드시나요?",
          "original_form": "헐 소세지도 직접 만드시나요?",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007231.1.8",
          "form": "못봤어요ㅠㅠ",
          "original_form": "못봤어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007231.1.9",
          "form": "직접만드시더라구요 ㅎ",
          "original_form": "직접만드시더라구요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007231.1.10",
          "form": "장비 구매하셔서 ㅎㅎ",
          "original_form": "장비 구매하셔서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007231.1.11",
          "form": "그소세지랑 맥주드시는데 와~~\r\n그앞에가서 앉고싶더라구요 ㅋㅋ",
          "original_form": "그소세지랑 맥주드시는데 와~~\r\n그앞에가서 앉고싶더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007231.1.12",
          "form": "와 완전 멋져요",
          "original_form": "와 완전 멋져요",
          "speaker_id": "1",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007231.1.13",
          "form": "공장에서만 만들수 있는줄 알았는데...",
          "original_form": "공장에서만 만들수 있는줄 알았는데...",
          "speaker_id": "1",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007231.1.14",
          "form": "저도 그런줄만알았는데 수제소세지를 직접 만들더라구요",
          "original_form": "저도 그런줄만알았는데 수제소세지를 직접 만들더라구요",
          "speaker_id": "2",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007231.1.15",
          "form": "재료 직접준비해서 버무리고 기계에 넣어서 돌돌말아서  직접만드시더라구요\r\n보고 와~대박 이러면서봤어요",
          "original_form": "재료 직접준비해서 버무리고 기계에 넣어서 돌돌말아서  직접만드시더라구요\r\n보고 와~대박 이러면서봤어요",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007231.1.16",
          "form": "와 진짜 신기해요",
          "original_form": "와 진짜 신기해요",
          "speaker_id": "1",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007231.1.17",
          "form": "다 잘하시는구나",
          "original_form": "다 잘하시는구나",
          "speaker_id": "1",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007231.1.18",
          "form": "노래도 잘하시고 음식도 잘만드시고",
          "original_form": "노래도 잘하시고 음식도 잘만드시고",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007231.1.19",
          "form": "말도 잘하시고",
          "original_form": "말도 잘하시고",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007231.1.20",
          "form": "왜 아직 혼자이신지 ㅎㅎ\r\n아님 계속 혼자이시길 바래봐요 ㅎ",
          "original_form": "왜 아직 혼자이신지 ㅎㅎ\r\n아님 계속 혼자이시길 바래봐요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007231.1.21",
          "form": "악담인가요?ㅋㅋ",
          "original_form": "악담인가요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007231.1.22",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007231.1.23",
          "form": "그래도 팬분들을 위해...! 혼자이신 것도 괜찮을 거 같기도ㅋㅋㅋ",
          "original_form": "그래도 팬분들을 위해...! 혼자이신 것도 괜찮을 거 같기도ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007231.1.24",
          "form": "ㅋㅋㅋㅋ 성시경님 큰일났군요 ㅎ",
          "original_form": "ㅋㅋㅋㅋ 성시경님 큰일났군요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007231.1.25",
          "form": "귀간지러우시겠어요 ㅎ",
          "original_form": "귀간지러우시겠어요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007231.1.26",
          "form": "ㅋㅋㅋㅋㅋ 워낙 완벽하시니까요",
          "original_form": "ㅋㅋㅋㅋㅋ 워낙 완벽하시니까요",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007231.1.27",
