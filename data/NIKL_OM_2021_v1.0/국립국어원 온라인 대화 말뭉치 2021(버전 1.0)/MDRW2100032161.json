      "utterance": [
        {
          "id": "MDRW2100032161.1.1",
          "form": "그럼 여행중에 구매하신거 있어요?",
          "original_form": "그럼 여행중에 구매하신거 있어요?",
          "speaker_id": "1",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100032161.1.2",
          "form": "펭귄 열쇠고리 엄마랑 커플로 샀어요",
          "original_form": "펭귄 열쇠고리 엄마랑 커플로 샀어요",
          "speaker_id": "2",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100032161.1.3",
          "form": "에버랜드에서요",
          "original_form": "에버랜드에서요",
          "speaker_id": "2",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100032161.1.4",
          "form": "헐 귀엽겠다 인형이에요? 아니면 금속?",
          "original_form": "헐 귀엽겠다 인형이에요? 아니면 금속?",
          "speaker_id": "1",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100032161.1.5",
          "form": "아크릴?",
          "original_form": "아크릴?",
          "speaker_id": "1",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100032161.1.6",
          "form": "플라스틱?제랑 금속제 하나씩요ㅎㅎ",
          "original_form": "플라스틱?제랑 금속제 하나씩요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100032161.1.7",
          "form": "완전 귀여워용",
          "original_form": "완전 귀여워용",
          "speaker_id": "2",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100032161.1.8",
          "form": "프로필사진 낙점",
          "original_form": "프로필사진 낙점",
          "speaker_id": "2",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100032161.1.9",
          "form": "ㅋㅋㅋㅋㅋ 진짜 귀엽겠어요 어디다 거셨어요?",
          "original_form": "ㅋㅋㅋㅋㅋ 진짜 귀엽겠어요 어디다 거셨어요?",
          "speaker_id": "1",
          "time": "20210905 13:38"
        },
        {
          "id": "MDRW2100032161.1.10",
          "form": "저는 가방에 걸었어요 ㅎㅅㅎ 완죤 깜찍...!",
          "original_form": "저는 가방에 걸었어요 ㅎㅅㅎ 완죤 깜찍...!",
          "speaker_id": "2",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032161.1.11",
          "form": "저 에버랜드 갔을때 놀란게 이동하려면 뮤조건 기념품 샾을 지나가야하더라고요??",
          "original_form": "저 에버랜드 갔을때 놀란게 이동하려면 뮤조건 기념품 샾을 지나가야하더라고요??",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100032161.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100032161.1.13",
          "form": "눈 딱감고 나왔지만 부모님하고 온 아기들은  통제가 불가능해 보였어요 ㅋㅋㅋ",
          "original_form": "눈 딱감고 나왔지만 부모님하고 온 아기들은  통제가 불가능해 보였어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100032161.1.14",
          "form": "부모님들 표정이 아찔해보였던건 기분 탓일까요",
          "original_form": "부모님들 표정이 아찔해보였던건 기분 탓일까요",
          "speaker_id": "1",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100032161.1.15",
          "form": "맞아요, 곳곳에 기념품샵ㅋㅋㅋ 다큰 우리 가족도 지나치지 못했습니다",
          "original_form": "맞아요, 곳곳에 기념품샵ㅋㅋㅋ 다큰 우리 가족도 지나치지 못했습니다",
          "speaker_id": "2",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100032161.1.16",
          "form": "ㅋㅋㅋㅋㅋ 원래 놀러가면 하나씩 사오는거죠 추억이잖아요",
          "original_form": "ㅋㅋㅋㅋㅋ 원래 놀러가면 하나씩 사오는거죠 추억이잖아요",
          "speaker_id": "1",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032161.1.17",
          "form": "오늘은 뭐 하실거에요?",
          "original_form": "오늘은 뭐 하실거에요?",
          "speaker_id": "1",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032161.1.18",
          "form": "지금 인사동 북촌 찍고 경복궁 가려구요",
          "original_form": "지금 인사동 북촌 찍고 경복궁 가려구요",
          "speaker_id": "2",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032161.1.19",
          "form": "한국의 정취를 느끼는 코스",
          "original_form": "한국의 정취를 느끼는 코스",
          "speaker_id": "2",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100032161.1.20",
          "form": "어제 용인 아니셨어요?",
          "original_form": "어제 용인 아니셨어요?",
          "speaker_id": "1",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100032161.1.21",
          "form": "홍길동이세요!",
          "original_form": "홍길동이세요!",
          "speaker_id": "1",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100032161.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100032161.1.23",
          "form": "동에번쩍 서에번쩍",
          "original_form": "동에번쩍 서에번쩍",
          "speaker_id": "2",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100032161.1.24",
          "form": "name1님은 최근에 물건 산거 있으세요?",
          "original_form": "&name1&님은 최근에 물건 산거 있으세요?",
          "speaker_id": "2",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100032161.1.25",
          "form": "흠 저는 초고속 충전선이랑 유선 이어폰이요!",
          "original_form": "흠 저는 초고속 충전선이랑 유선 이어폰이요!",
          "speaker_id": "1",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032161.1.26",
          "form": "오 휴대폰 관련기기들이네요",
          "original_form": "오 휴대폰 관련기기들이네요",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032161.1.27",
          "form": "제가 뭐 샀었는지 얘기한 적 있나요?",
          "original_form": "제가 뭐 샀었는지 얘기한 적 있나요?",
