      "utterance": [
        {
          "id": "MDRW2100042058.1.1",
          "form": "저는 전공이 행정이고 경영 복전했어요!",
          "original_form": "저는 전공이 행정이고 경영 복전했어요!",
          "speaker_id": "2",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100042058.1.2",
          "form": "오오 그러섰군요 대박",
          "original_form": "오오 그러섰군요 대박",
          "speaker_id": "1",
          "time": "20210906 20:28"
        },
        {
          "id": "MDRW2100042058.1.3",
          "form": "제가 학교였어도",
          "original_form": "제가 학교였어도",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042058.1.4",
          "form": "채용하고싶은",
          "original_form": "채용하고싶은",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042058.1.5",
          "form": "전공을 공부하셨는데요?!?!",
          "original_form": "전공을 공부하셨는데요?!?!",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042058.1.6",
          "form": "name2님 저는 이제 퇴근해요,,, 비가 많이 오네요,,",
          "original_form": "&name2&님 저는 이제 퇴근해요,,, 비가 많이 오네요,,",
          "speaker_id": "1",
          "time": "20210906 20:29"
        },
        {
          "id": "MDRW2100042058.1.7",
          "form": "헐 지금 비많이오는데ㅠㅠ 아니 이시간퇴근이라뇨",
          "original_form": "헐 지금 비많이오는데ㅠㅠ 아니 이시간퇴근이라뇨",
          "speaker_id": "2",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100042058.1.8",
          "form": "워라밸을 보장하라",
          "original_form": "워라밸을 보장하라",
          "speaker_id": "2",
          "time": "20210906 20:50"
        },
        {
          "id": "MDRW2100042058.1.9",
          "form": "으하하하",
          "original_form": "으하하하",
          "speaker_id": "1",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042058.1.10",
          "form": "놀라운 사실을 알려드릴게요",
          "original_form": "놀라운 사실을 알려드릴게요",
          "speaker_id": "1",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042058.1.11",
          "form": "저희 연구실 정도면 워라밸 좋은거랍니다.....",
          "original_form": "저희 연구실 정도면 워라밸 좋은거랍니다.....",
          "speaker_id": "1",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042058.1.12",
          "form": "헐 이게 무슨 비극적인",
          "original_form": "헐 이게 무슨 비극적인",
          "speaker_id": "2",
          "time": "20210906 20:51"
        },
        {
          "id": "MDRW2100042058.1.13",
          "form": "저녁은 드신건가요",
          "original_form": "저녁은 드신건가요",
          "speaker_id": "2",
          "time": "20210906 20:52"
        },
        {
          "id": "MDRW2100042058.1.14",
          "form": "그럼요 그럼요",
          "original_form": "그럼요 그럼요",
          "speaker_id": "1",
          "time": "20210906 20:52"
        },
        {
          "id": "MDRW2100042058.1.15",
          "form": "주로 도시락을 싸오거나",
          "original_form": "주로 도시락을 싸오거나",
          "speaker_id": "1",
          "time": "20210906 20:52"
        },
        {
          "id": "MDRW2100042058.1.16",
          "form": "별미가 땡길 땐 나가서 사먹거나,,,",
          "original_form": "별미가 땡길 땐 나가서 사먹거나,,,",
          "speaker_id": "1",
          "time": "20210906 20:52"
        },
        {
          "id": "MDRW2100042058.1.17",
          "form": "그런답니다?!",
          "original_form": "그런답니다?!",
          "speaker_id": "1",
          "time": "20210906 20:52"
        },
        {
          "id": "MDRW2100042058.1.18",
          "form": "name2님 혹시",
          "original_form": "&name2&님 혹시",
          "speaker_id": "1",
          "time": "20210906 20:52"
        },
        {
          "id": "MDRW2100042058.1.19",
          "form": "소그노 유튜브 보세요?!",
          "original_form": "소그노 유튜브 보세요?!",
          "speaker_id": "1",
          "time": "20210906 20:52"
        },
        {
          "id": "MDRW2100042058.1.20",
          "form": "당연하죠!!!!",
          "original_form": "당연하죠!!!!",
          "speaker_id": "2",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042058.1.21",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "1",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042058.1.22",
          "form": "어쩐지",
          "original_form": "어쩐지",
          "speaker_id": "1",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042058.1.23",
          "form": "포비피엠 좀",
          "original_form": "포비피엠 좀",
          "speaker_id": "1",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042058.1.24",
          "form": "다녀오신 분 같나",
          "original_form": "다녀오신 분 같나",
          "speaker_id": "1",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042058.1.25",
          "form": "같다고느꼈습니다",
          "original_form": "같다고느꼈습니다",
          "speaker_id": "1",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042058.1.26",
          "form": "어떤가요 제 촉?!?!",
          "original_form": "어떤가요 제 촉?!?!",
          "speaker_id": "1",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042058.1.27",
          "form": "ㅋㄱㅋㅋㅋㅋㅋㅋㅋ아ㅠ 아녜요ㅋㅋㄱㅋㅋㅋ못갔어요ㅠ",
          "original_form": "ㅋㄱㅋㅋㅋㅋㅋㅋㅋ아ㅠ 아녜요ㅋㅋㄱㅋㅋㅋ못갔어요ㅠ",
          "speaker_id": "2",
          "time": "20210906 20:53"
        },
        {
          "id": "MDRW2100042058.1.28",
          "form": "가고싶은데 페미니스트인 친구가 1명인데 그친구가 코로나 끝나야 가겠대서",
          "original_form": "가고싶은데 페미니스트인 친구가 1명인데 그친구가 코로나 끝나야 가겠대서",
          "speaker_id": "2",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042058.1.29",
          "form": "혼자언제가지 가서 얼그레이진토닉 마셔야되는데",
          "original_form": "혼자언제가지 가서 얼그레이진토닉 마셔야되는데",
          "speaker_id": "2",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042058.1.30",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042058.1.31",
          "form": "각만재고있어요,,",
          "original_form": "각만재고있어요,,",
          "speaker_id": "2",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042058.1.32",
          "form": "저도 지금까지 3번 갔나?",
          "original_form": "저도 지금까지 3번 갔나?",
          "speaker_id": "1",
          "time": "20210906 20:54"
        },
        {
          "id": "MDRW2100042058.1.33",
          "form": "아니근데 코로나 끝날 때까지 기다리면",
          "original_form": "아니근데 코로나 끝날 때까지 기다리면",
          "speaker_id": "1",
          "time": "20210906 20:55"
        },
        {
          "id": "MDRW2100042058.1.34",
          "form": "부러워요!!!",
          "original_form": "부러워요!!!",
          "speaker_id": "2",
          "time": "20210906 20:55"
        },
        {
          "id": "MDRW2100042058.1.35",
          "form": "너무 오래 걸릴 것 같은데...",
          "original_form": "너무 오래 걸릴 것 같은데...",
          "speaker_id": "1",
          "time": "20210906 20:55"
        },
        {
          "id": "MDRW2100042058.1.36",
          "form": "name2님 그러면 공학대학 다니실 때 어떠셨어요ㅠ",
          "original_form": "&name2&님 그러면 공학대학 다니실 때 어떠셨어요ㅠ",
