      "utterance": [
        {
          "id": "MDRW2100019993.1.1",
          "form": "복귀하고 돈많이안씀",
          "original_form": "복귀하고 돈많이안씀",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019993.1.2",
          "form": "1주년이라",
          "original_form": "1주년이라",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019993.1.3",
          "form": "원스토어에서 할인쿠폰 받은거로 다 결제하지?",
          "original_form": "원스토어에서 할인쿠폰 받은거로 다 결제하지?",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019993.1.4",
          "form": "겁나 퍼주거든",
          "original_form": "겁나 퍼주거든",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019993.1.5",
          "form": "중독자가 따로없네",
          "original_form": "중독자가 따로없네",
          "speaker_id": "1",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019993.1.6",
          "form": "결제하면 원스토어로",
          "original_form": "결제하면 원스토어로",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019993.1.7",
          "form": "ㅏ긴함",
          "original_form": "ㅏ긴함",
          "speaker_id": "2",
          "time": "20210827 23:08"
        },
        {
          "id": "MDRW2100019993.1.8",
          "form": "근데 스트레스 푼다면 뭐 할말없다만",
          "original_form": "근데 스트레스 푼다면 뭐 할말없다만",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019993.1.9",
          "form": "원스토어 혜자패키지 넘많음",
          "original_form": "원스토어 혜자패키지 넘많음",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019993.1.10",
          "form": "난 스트레스없는디",
          "original_form": "난 스트레스없는디",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019993.1.11",
          "form": "부자가 못되겠지",
          "original_form": "부자가 못되겠지",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019993.1.12",
          "form": "부자된다",
          "original_form": "부자된다",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019993.1.13",
          "form": "소과금이란 없는것이다",
          "original_form": "소과금이란 없는것이다",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019993.1.14",
          "form": "부자아빠다 나는",
          "original_form": "부자아빠다 나는",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019993.1.15",
          "form": "쓰다보면 중독자처럼",
          "original_form": "쓰다보면 중독자처럼",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019993.1.16",
          "form": "탕진하게됨",
          "original_form": "탕진하게됨",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019993.1.17",
          "form": "아님",
          "original_form": "아님",
          "speaker_id": "2",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019993.1.18",
          "form": "탕진안함",
          "original_form": "탕진안함",
          "speaker_id": "2",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019993.1.19",
          "form": "가난한아빠 되고싶지 않으면 게임에 돈쓰지 말거라",
          "original_form": "가난한아빠 되고싶지 않으면 게임에 돈쓰지 말거라",
          "speaker_id": "1",
          "time": "20210827 23:10"
        }
