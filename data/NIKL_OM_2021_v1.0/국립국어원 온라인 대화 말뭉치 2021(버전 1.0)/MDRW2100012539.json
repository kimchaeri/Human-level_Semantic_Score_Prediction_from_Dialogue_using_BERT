        },
        {
          "id": "MDRW2100012539.1.2",
          "form": "너무 싫어",
          "original_form": "너무 싫어",
          "speaker_id": "1",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100012539.1.3",
          "form": "왜 싫어",
          "original_form": "왜 싫어",
          "speaker_id": "2",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100012539.1.4",
          "form": "과제가 너무 많아",
          "original_form": "과제가 너무 많아",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100012539.1.5",
          "form": "원래 그렇게 많았어?",
          "original_form": "원래 그렇게 많았어?",
          "speaker_id": "2",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100012539.1.6",
          "form": "아니 비대면이라 많아",
          "original_form": "아니 비대면이라 많아",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100012539.1.7",
          "form": "얼른 대면 해야할텐데",
          "original_form": "얼른 대면 해야할텐데",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012539.1.8",
          "form": "그건 더 싫어",
          "original_form": "그건 더 싫어",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012539.1.9",
          "form": "왜",
          "original_form": "왜",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012539.1.10",
          "form": "우선 일찍 일어나야해",
          "original_form": "우선 일찍 일어나야해",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012539.1.11",
          "form": "일어나면되지",
          "original_form": "일어나면되지",
          "speaker_id": "2",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012539.1.12",
          "form": "일어나서 산 꼭대기같은 학교를 올라가야해",
          "original_form": "일어나서 산 꼭대기같은 학교를 올라가야해",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012539.1.13",
          "form": "그건 좀 힘들겠네",
          "original_form": "그건 좀 힘들겠네",
          "speaker_id": "2",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012539.1.14",
          "form": "게다가 대면하면 수업도 연달아들어야해",
          "original_form": "게다가 대면하면 수업도 연달아들어야해",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012539.1.15",
          "form": "비대면은?",
          "original_form": "비대면은?",
          "speaker_id": "2",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012539.1.16",
          "form": "온라인 강의 식이라 내가 듣고싶을때 들으면 돼",
          "original_form": "온라인 강의 식이라 내가 듣고싶을때 들으면 돼",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012539.1.17",
          "form": "너무 루즈해지는거 아냐?",
          "original_form": "너무 루즈해지는거 아냐?",
          "speaker_id": "2",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100012539.1.18",
          "form": "그렇긴한데 학교에서 강의듣는거보단 나아",
          "original_form": "그렇긴한데 학교에서 강의듣는거보단 나아",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100012539.1.19",
          "form": "수업은 원래 학교에서 듣는거야",
          "original_form": "수업은 원래 학교에서 듣는거야",
          "speaker_id": "2",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100012539.1.20",
          "form": "싫어",
          "original_form": "싫어",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100012539.1.21",
          "form": "그리고 비대면은 시험도 오픈북이잖아",
          "original_form": "그리고 비대면은 시험도 오픈북이잖아",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100012539.1.22",
          "form": "대신 더 어려운거아냐?",
          "original_form": "대신 더 어려운거아냐?",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100012539.1.23",
          "form": "타임어택이긴한데 비대면은 학점도 훨씬 잘줘",
          "original_form": "타임어택이긴한데 비대면은 학점도 훨씬 잘줘",
          "speaker_id": "1",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100012539.1.24",
          "form": "그래서 6전공을 듣는구나",
          "original_form": "그래서 6전공을 듣는구나",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100012539.1.25",
          "form": "맞아 전공 미리 다 들어놓으려고",
          "original_form": "맞아 전공 미리 다 들어놓으려고",
          "speaker_id": "1",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100012539.1.26",
          "form": "잔머리만 늘었네",
          "original_form": "잔머리만 늘었네",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100012539.1.27",
          "form": "나 다음주부터 실시간 강의해야해",
          "original_form": "나 다음주부터 실시간 강의해야해",
