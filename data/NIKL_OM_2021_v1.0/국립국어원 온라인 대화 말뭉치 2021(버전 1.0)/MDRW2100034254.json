      "utterance": [
        {
          "id": "MDRW2100034254.1.1",
          "form": "그러게 아메리카노랑 어울린다",
          "original_form": "그러게 아메리카노랑 어울린다",
          "speaker_id": "2",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034254.1.2",
          "form": "옹옹 ㅎㅎ 따뜻한 아메리카노 한잔~",
          "original_form": "옹옹 ㅎㅎ 따뜻한 아메리카노 한잔~",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034254.1.3",
          "form": "그래맞다",
          "original_form": "그래맞다",
          "speaker_id": "2",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034254.1.4",
          "form": "이따 케익 만들어서 커피랑 같이 먹자",
          "original_form": "이따 케익 만들어서 커피랑 같이 먹자",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034254.1.5",
          "form": "그래 근데 잠 안올텐데",
          "original_form": "그래 근데 잠 안올텐데",
          "speaker_id": "2",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034254.1.6",
          "form": "아 맞넵..",
          "original_form": "아 맞넵..",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034254.1.7",
          "form": "디카페인으로 먹자",
          "original_form": "디카페인으로 먹자",
          "speaker_id": "2",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034254.1.8",
          "form": "오 그럼되겠군 ㅋㅋ",
          "original_form": "오 그럼되겠군 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100034254.1.9",
          "form": "집에 있어?",
