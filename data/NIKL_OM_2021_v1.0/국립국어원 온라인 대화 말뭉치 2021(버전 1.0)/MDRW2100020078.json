      "utterance": [
        {
          "id": "MDRW2100020078.1.1",
          "form": "전 전자렌지 푸딩이랑 찌는 푸딩을 해봤어요",
          "original_form": "전 전자렌지 푸딩이랑 찌는 푸딩을 해봤어요",
          "speaker_id": "1",
          "time": "20210827 23:36"
        },
        {
          "id": "MDRW2100020078.1.2",
          "form": "결과물은 어땠나요?",
          "original_form": "결과물은 어땠나요?",
          "speaker_id": "2",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020078.1.3",
          "form": "이게 집 전자렌지나 머그잔마다 최적의 전자렌지 시간이 달라서 시행착오가 좀 필요해보였어요",
          "original_form": "이게 집 전자렌지나 머그잔마다 최적의 전자렌지 시간이 달라서 시행착오가 좀 필요해보였어요",
          "speaker_id": "1",
          "time": "20210827 23:37"
        },
        {
          "id": "MDRW2100020078.1.4",
          "form": "하지만 여러번 만들고 시간 양 스킬이 생기면 괜춘할듯한?",
          "original_form": "하지만 여러번 만들고 시간 양 스킬이 생기면 괜춘할듯한?",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020078.1.5",
          "form": "계란찜과 푸딩의 차이는 뭐가 판가름하나요?",
          "original_form": "계란찜과 푸딩의 차이는 뭐가 판가름하나요?",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020078.1.6",
          "form": "시간..?",
          "original_form": "시간..?",
          "speaker_id": "2",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020078.1.7",
          "form": "아직 찌는 방법은",
          "original_form": "아직 찌는 방법은",
          "speaker_id": "1",
          "time": "20210827 23:38"
        },
        {
          "id": "MDRW2100020078.1.8",
          "form": "말못했어요 ㅋㅋㅋ",
          "original_form": "말못했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:39"
        },
        {
          "id": "MDRW2100020078.1.9",
          "form": "앗 ㅋㅋㅋㅋㅋ 그러네요",
          "original_form": "앗 ㅋㅋㅋㅋㅋ 그러네요",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020078.1.10",
          "form": "찌는건 어떤가요",
          "original_form": "찌는건 어떤가요",
          "speaker_id": "2",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020078.1.11",
          "form": "찜방법은 속도가느려요",
          "original_form": "찜방법은 속도가느려요",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020078.1.12",
          "form": "그래서 조절이쉬워요",
          "original_form": "그래서 조절이쉬워요",
          "speaker_id": "1",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020078.1.13",
          "form": "아 그런 장점이 있네여",
          "original_form": "아 그런 장점이 있네여",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020078.1.14",
          "form": "대신 확인하려고 뚜껑을 열면 얼굴이 김이 쫙~",
          "original_form": "대신 확인하려고 뚜껑을 열면 얼굴이 김이 쫙~",
          "speaker_id": "1",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020078.1.15",
          "form": "맛도 있고 피부미용도 되겠네요 ㅋㅋㅋ",
          "original_form": "맛도 있고 피부미용도 되겠네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020078.1.16",
          "form": "ㅠㅜ 뜨거운 김이라서 이게 쫌 번거로워요",
          "original_form": "ㅠㅜ 뜨거운 김이라서 이게 쫌 번거로워요",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020078.1.17",
          "form": "그래서 전자렌지 찜 몇가지를 설탕양 불. 시간. 그릇등을 달리해서 테스트하면",
          "original_form": "그래서 전자렌지 찜 몇가지를 설탕양 불. 시간. 그릇등을 달리해서 테스트하면",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020078.1.18",
          "form": "하면 어떻게되게요?",
          "original_form": "하면 어떻게되게요?",
          "speaker_id": "1",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020078.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020078.1.20",
          "form": "덜익은 계란찜 더익은 계란찜이 돼요~",
          "original_form": "덜익은 계란찜 더익은 계란찜이 돼요~",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020078.1.21",
          "form": "맛있는 푸딩 1개를 만드는 과정에서 계란찜 같은 푸딩 계란찜과 푸딩중간 푸딩 등등이 7~8개 생기기 때문에",
          "original_form": "맛있는 푸딩 1개를 만드는 과정에서 계란찜 같은 푸딩 계란찜과 푸딩중간 푸딩 등등이 7~8개 생기기 때문에",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020078.1.22",
          "form": "하나 맛있는게 나와도 이젠...됐다....하고 ...질려버려요",
          "original_form": "하나 맛있는게 나와도 이젠...됐다....하고 ...질려버려요",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020078.1.23",
          "form": "슬프네요 ㅋㅋㅋㅋㅋ ㅠㅠㅠ",
          "original_form": "슬프네요 ㅋㅋㅋㅋㅋ ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020078.1.24",
          "form": "요즘 푸딩이라고 하면",
          "original_form": "요즘 푸딩이라고 하면",
          "speaker_id": "2",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020078.1.25",
          "form": "탱글탱글한 계란푸딩이 아니라",
          "original_form": "탱글탱글한 계란푸딩이 아니라",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020078.1.26",
          "form": "바나나 푸딩같은? 종류를 많이 팔더라구요",
          "original_form": "바나나 푸딩같은? 종류를 많이 팔더라구요",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020078.1.27",
          "form": "젤라틴이 들어간 푸딩이요?",
          "original_form": "젤라틴이 들어간 푸딩이요?",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020078.1.28",
          "form": "계란익히는 푸딩말고 커스터드크림에 젤라틴 넣는건 쉬워요",
          "original_form": "계란익히는 푸딩말고 커스터드크림에 젤라틴 넣는건 쉬워요",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020078.1.29",
          "form": "아뇨 빵하고 빵 사이에",
          "original_form": "아뇨 빵하고 빵 사이에",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020078.1.30",
          "form": "크림을 넣어서",
          "original_form": "크림을 넣어서",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020078.1.31",
          "form": "약간 질척하게 떠먹는?",
          "original_form": "약간 질척하게 떠먹는?",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020078.1.32",
          "form": "전 계란푸딩 먹고싶은데",
          "original_form": "전 계란푸딩 먹고싶은데",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020078.1.33",
          "form": "파는 곳 찾기가 의외로 어려워요",
          "original_form": "파는 곳 찾기가 의외로 어려워요",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020078.1.34",
          "form": "일본식 계란푸딩 저도 참 좋아하는데요",
          "original_form": "일본식 계란푸딩 저도 참 좋아하는데요",
          "speaker_id": "1",
          "time": "20210827 23:53"
        }
      ]
