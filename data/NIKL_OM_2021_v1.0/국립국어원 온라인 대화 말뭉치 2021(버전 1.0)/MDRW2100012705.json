      "utterance": [
        {
          "id": "MDRW2100012705.1.1",
          "form": "개학한 기분이 어때요?",
          "original_form": "개학한 기분이 어때요?",
          "speaker_id": "1",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100012705.1.2",
          "form": "아직 안했는데.. 일주일 남았어요..",
          "original_form": "아직 안했는데.. 일주일 남았어요..",
          "speaker_id": "2",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100012705.1.3",
          "form": "아 일주일 남으셨구나",
          "original_form": "아 일주일 남으셨구나",
          "speaker_id": "1",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100012705.1.4",
          "form": "그럼 대학생이에요?",
          "original_form": "그럼 대학생이에요?",
          "speaker_id": "1",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100012705.1.5",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100012705.1.6",
          "form": "name1은 대학생이신가요?",
          "original_form": "&name1&은 대학생이신가요?",
          "speaker_id": "2",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100012705.1.7",
          "form": "저는 대학교 졸업했어요 ㅠㅜ",
          "original_form": "저는 대학교 졸업했어요 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100012705.1.8",
          "form": "수강 신청은 잘 성공하셨는지요 ㅎㅎ",
          "original_form": "수강 신청은 잘 성공하셨는지요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 00:06"
        },
        {
          "id": "MDRW2100012705.1.9",
          "form": "네ㅎㅎ 올클했네요!",
          "original_form": "네ㅎㅎ 올클했네요!",
          "speaker_id": "2",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012705.1.10",
          "form": "와 축하드려요!! 행복한 2학기 보낼 수 있겠네요",
          "original_form": "와 축하드려요!! 행복한 2학기 보낼 수 있겠네요",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012705.1.11",
          "form": "ㅎㅎ 감사합니다!",
          "original_form": "ㅎㅎ 감사합니다!",
          "speaker_id": "2",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012705.1.12",
          "form": "근데 코로나 때문에 요즘 수업은 어떻게 해요?ㅜ",
          "original_form": "근데 코로나 때문에 요즘 수업은 어떻게 해요?ㅜ",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012705.1.13",
          "form": "수업따라 다른데 저는 집에서 듣는거 위주로 짰어요",
          "original_form": "수업따라 다른데 저는 집에서 듣는거 위주로 짰어요",
          "speaker_id": "2",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012705.1.14",
          "form": "아ㅜㅜ 원격수업 듣는 건 어때요? 집중 잘되나요?",
          "original_form": "아ㅜㅜ 원격수업 듣는 건 어때요? 집중 잘되나요?",
          "speaker_id": "1",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012705.1.15",
          "form": "ㅎㅎ..집중안돼요..",
          "original_form": "ㅎㅎ..집중안돼요..",
          "speaker_id": "2",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012705.1.16",
          "form": "아.. 원격수업 vs 등교수업 뭐가 더 좋아요?",
          "original_form": "아.. 원격수업 vs 등교수업 뭐가 더 좋아요?",
          "speaker_id": "1",
          "time": "20210825 00:10"
        },
        {
          "id": "MDRW2100012705.1.17",
          "form": "전 그래도 원격수업이요ㅋㅋ",
          "original_form": "전 그래도 원격수업이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:11"
        },
        {
          "id": "MDRW2100012705.1.18",
          "form": "어 의외네요 캠퍼스라이프를 포기하시다니..",
          "original_form": "어 의외네요 캠퍼스라이프를 포기하시다니..",
          "speaker_id": "1",
          "time": "20210825 00:11"
        },
        {
          "id": "MDRW2100012705.1.19",
          "form": "집에서 멀어서요..",
          "original_form": "집에서 멀어서요..",
          "speaker_id": "2",
          "time": "20210825 00:12"
        },
        {
          "id": "MDRW2100012705.1.20",
          "form": "앗.. 통학러이시구나..",
          "original_form": "앗.. 통학러이시구나..",
          "speaker_id": "1",
          "time": "20210825 00:12"
        },
        {
          "id": "MDRW2100012705.1.21",
          "form": "넵..",
          "original_form": "넵..",
          "speaker_id": "2",
          "time": "20210825 00:12"
        },
        {
          "id": "MDRW2100012705.1.22",
          "form": "그럼 동기들이나 선후배랑 친한 편이에요?",
