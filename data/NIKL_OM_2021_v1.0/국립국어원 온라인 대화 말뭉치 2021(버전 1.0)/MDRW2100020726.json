      "utterance": [
        {
          "id": "MDRW2100020726.1.1",
          "form": "라면먹어야 될것 같아요",
          "original_form": "라면먹어야 될것 같아요",
          "speaker_id": "2",
          "time": "20210828 11:41"
        },
        {
          "id": "MDRW2100020726.1.2",
          "form": "무슨 라면 드실건가요?",
          "original_form": "무슨 라면 드실건가요?",
          "speaker_id": "1",
          "time": "20210828 11:41"
        },
        {
          "id": "MDRW2100020726.1.3",
          "form": "진라면요ㅋㅋㅋㅋ",
          "original_form": "진라면요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:42"
        },
        {
          "id": "MDRW2100020726.1.4",
          "form": "순한맛? 매운맛? 어떤건가요?",
          "original_form": "순한맛? 매운맛? 어떤건가요?",
          "speaker_id": "1",
          "time": "20210828 11:43"
        },
        {
          "id": "MDRW2100020726.1.5",
          "form": "순한맛요ㅋㅋㅋㄱ",
          "original_form": "순한맛요ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210828 11:43"
        },
        {
          "id": "MDRW2100020726.1.6",
          "form": "라면이 지겨우시다면 백종원표 라볶이 해드셔보세요",
          "original_form": "라면이 지겨우시다면 백종원표 라볶이 해드셔보세요",
          "speaker_id": "1",
          "time": "20210828 11:43"
        },
        {
          "id": "MDRW2100020726.1.7",
          "form": "백종원꺼맛있어요!",
          "original_form": "백종원꺼맛있어요!",
          "speaker_id": "2",
          "time": "20210829 08:05"
        },
        {
          "id": "MDRW2100020726.1.8",
          "form": "맞아요! 단짠단짠! 맛없을수가 없어요",
          "original_form": "맞아요! 단짠단짠! 맛없을수가 없어요",
          "speaker_id": "1",
          "time": "20210829 10:58"
        },
        {
          "id": "MDRW2100020726.1.9",
          "form": "백종원음식은 워낙 너무나도 유명해서ㅋㅋㅋ",
          "original_form": "백종원음식은 워낙 너무나도 유명해서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
          "id": "MDRW2100020726.1.10",
          "form": "오늘 점심때 머먹지....",
          "original_form": "오늘 점심때 머먹지....",
          "speaker_id": "2",
          "time": "20210829 11:17"
        },
        {
          "id": "MDRW2100020726.1.11",
          "form": "전 떡볶이 먹었어요ㅋㅋ",
          "original_form": "전 떡볶이 먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:10"
        },
        {
          "id": "MDRW2100020726.1.12",
          "form": "저는 햄버거먹었머요",
          "original_form": "저는 햄버거먹었머요",
          "speaker_id": "2",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100020726.1.13",
          "form": "어디꺼 드셨나요?",
          "original_form": "어디꺼 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 14:51"
        },
        {
          "id": "MDRW2100020726.1.14",
          "form": "맥도날드먹었어요",
          "original_form": "맥도날드먹었어요",
          "speaker_id": "2",
          "time": "20210829 14:55"
        }
