      "utterance": [
        {
          "id": "MDRW2100006221.1.1",
          "form": "리유저블컵 잇으세요?",
          "original_form": "리유저블컵 잇으세요?",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100006221.1.2",
          "form": "리유저블컵보단 텀블러 있어요 님은 가지고 있나요?",
          "original_form": "리유저블컵보단 텀블러 있어요 님은 가지고 있나요?",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100006221.1.3",
          "form": "네 스벅이서 받앗어요",
          "original_form": "네 스벅이서 받앗어요",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100006221.1.4",
          "form": "자주사용하세요?",
          "original_form": "자주사용하세요?",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100006221.1.5",
          "form": "스벅 하얀색 그건가.... 엄청 자주 쓰는 건 아니고 따뜻한 물 마실때 써요",
          "original_form": "스벅 하얀색 그건가.... 엄청 자주 쓰는 건 아니고 따뜻한 물 마실때 써요",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100006221.1.6",
          "form": "네네 ㅋㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100006221.1.7",
          "form": "할인 안받으시나요",
          "original_form": "할인 안받으시나요",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100006221.1.8",
          "form": "전 보통 컵 사용하거든요 님은 할인 받으시나요?",
          "original_form": "전 보통 컵 사용하거든요 님은 할인 받으시나요?",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100006221.1.9",
          "form": "저요 네 텀블러로",
          "original_form": "저요 네 텀블러로",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100006221.1.10",
          "form": "할인받아용",
          "original_form": "할인받아용",
          "speaker_id": "1",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100006221.1.11",
          "form": "환경을 위해 다른건 참여하시나요?",
          "original_form": "환경을 위해 다른건 참여하시나요?",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100006221.1.12",
          "form": "음..걍 배달 안시키려고 합니다",
          "original_form": "음..걍 배달 안시키려고 합니다",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100006221.1.13",
          "form": "배달요? 배달용기때문에요?",
          "original_form": "배달요? 배달용기때문에요?",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100006221.1.14",
          "form": "네 너무 많이나와요 스레기가",
          "original_form": "네 너무 많이나와요 스레기가",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100006221.1.15",
          "form": "하긴 쓰레기가 마구 나와요 별것도 없는데...",
          "original_form": "하긴 쓰레기가 마구 나와요 별것도 없는데...",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006221.1.16",
          "form": "그죠 넘 아까워요",
          "original_form": "그죠 넘 아까워요",
          "speaker_id": "1",
          "time": "20210907 22:09"
        }
      ]
