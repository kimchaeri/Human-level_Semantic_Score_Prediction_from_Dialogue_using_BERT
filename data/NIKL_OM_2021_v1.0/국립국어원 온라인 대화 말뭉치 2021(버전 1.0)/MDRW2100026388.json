      "utterance": [
        {
          "id": "MDRW2100026388.1.1",
          "form": "오늘은 평소보다 좀 더웠죠",
          "original_form": "오늘은 평소보다 좀 더웠죠",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026388.1.2",
          "form": "오 저희동네는",
          "original_form": "오 저희동네는",
          "speaker_id": "2",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026388.1.3",
          "form": "비와서",
          "original_form": "비와서",
          "speaker_id": "2",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026388.1.4",
          "form": "추웠네요",
          "original_form": "추웠네요",
          "speaker_id": "2",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100026388.1.5",
          "form": "비왔군요 많이 오는 곳은 잠겼다던데 피해는 없었나요?ㅠㅠ",
          "original_form": "비왔군요 많이 오는 곳은 잠겼다던데 피해는 없었나요?ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:57"
        },
        {
          "id": "MDRW2100026388.1.6",
          "form": "그정도는 아니에요 그냥 계속오는정도요 ㅋㅋ",
          "original_form": "그정도는 아니에요 그냥 계속오는정도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026388.1.7",
          "form": "비오고 많이 안더워지지않았나요?",
          "original_form": "비오고 많이 안더워지지않았나요?",
          "speaker_id": "2",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026388.1.8",
          "form": "여기는 잠깐 이슬비 정도 떨어진 거라 어제보다 훨씬 더웠어요 3~4도 높더라구요",
          "original_form": "여기는 잠깐 이슬비 정도 떨어진 거라 어제보다 훨씬 더웠어요 3~4도 높더라구요",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026388.1.9",
          "form": "땀이 좀 나는 정도? ㅎㅎ",
          "original_form": "땀이 좀 나는 정도? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026388.1.10",
          "form": "허 습하죠?",
          "original_form": "허 습하죠?",
          "speaker_id": "2",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026388.1.11",
          "form": "네 엄청 습해요 ㅠㅠ 다시 초여름으로 돌아간 듯한 기분이네요",
          "original_form": "네 엄청 습해요 ㅠㅠ 다시 초여름으로 돌아간 듯한 기분이네요",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026388.1.12",
          "form": "작년 이맘때쯤 사진 보니까 가디건을 입고 다녔더라구요",
          "original_form": "작년 이맘때쯤 사진 보니까 가디건을 입고 다녔더라구요",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026388.1.13",
          "form": "올해는 정말 더웠어요 그쵸",
          "original_form": "올해는 정말 더웠어요 그쵸",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026388.1.14",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026388.1.15",
          "form": "작년이 하나도 안더웠던거네요",
          "original_form": "작년이 하나도 안더웠던거네요",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026388.1.16",
          "form": "미세먼지도 그렇고 날씨가 휙휙 변하는 걸 보면 기후변화가 진행되고 있구나 생각이 들기도 하고요",
          "original_form": "미세먼지도 그렇고 날씨가 휙휙 변하는 걸 보면 기후변화가 진행되고 있구나 생각이 들기도 하고요",
          "speaker_id": "1",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100026388.1.17",
          "form": "약간 동남아",
          "original_form": "약간 동남아",
          "speaker_id": "2",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100026388.1.18",
          "form": "같아지는 느낌 있어요",
          "original_form": "같아지는 느낌 있어요",
          "speaker_id": "2",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100026388.1.19",
          "form": "너무습하고더워서 ㅜ",
          "original_form": "너무습하고더워서 ㅜ",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100026388.1.20",
          "form": "아 그러네요 ㅠㅠ 잊을 만하면 반도국이라는 걸 느끼게 해주는 ㅋㅋㅋ ㅠㅠ",
          "original_form": "아 그러네요 ㅠㅠ 잊을 만하면 반도국이라는 걸 느끼게 해주는 ㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026388.1.21",
          "form": "옷도 꺼내고 집어넣기 애매해요",
          "original_form": "옷도 꺼내고 집어넣기 애매해요",
          "speaker_id": "1",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100026388.1.22",
          "form": "그니까요 빨리 긴팔입고싶은데",
          "original_form": "그니까요 빨리 긴팔입고싶은데",
          "speaker_id": "2",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026388.1.23",
          "form": "슬슬 긴남방이나 셔츠가 그리워요",
          "original_form": "슬슬 긴남방이나 셔츠가 그리워요",
          "speaker_id": "1",
          "time": "20210901 22:11"
        }
      ]
    }
  ]
