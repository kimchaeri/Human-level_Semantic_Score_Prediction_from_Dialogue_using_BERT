      "utterance": [
        {
          "id": "MDRW2100042379.1.1",
          "form": "운동 좋아하세요??",
          "original_form": "운동 좋아하세요??",
          "speaker_id": "1",
          "time": "20210906 21:54"
        },
        {
          "id": "MDRW2100042379.1.2",
          "form": "운동 자주 하세용?",
          "original_form": "운동 자주 하세용?",
          "speaker_id": "1",
          "time": "20210906 21:56"
        },
        {
          "id": "MDRW2100042379.1.3",
          "form": "네  운동 좋아해요",
          "original_form": "네  운동 좋아해요",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042379.1.4",
          "form": "자주까지는  안하지만요ㅋㅋ",
          "original_form": "자주까지는  안하지만요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042379.1.5",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042379.1.6",
          "form": "저는 산책만 나가요 ㅋㅋㅋㅋ",
          "original_form": "저는 산책만 나가요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042379.1.7",
          "form": "운동 어떤거 하세요??",
          "original_form": "운동 어떤거 하세요??",
          "speaker_id": "1",
          "time": "20210906 21:57"
        },
        {
          "id": "MDRW2100042379.1.8",
          "form": "산책도 좋죠!",
          "original_form": "산책도 좋죠!",
          "speaker_id": "2",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100042379.1.9",
          "form": "저도 산책이나 근력운동해요",
          "original_form": "저도 산책이나 근력운동해요",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042379.1.10",
          "form": "근육이 너무 없어서요",
          "original_form": "근육이 너무 없어서요",
          "speaker_id": "2",
          "time": "20210906 21:59"
        },
        {
          "id": "MDRW2100042379.1.11",
          "form": "오오 근력운동 너무 힘들어요 ㅠㅠ",
          "original_form": "오오 근력운동 너무 힘들어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042379.1.12",
          "form": "스쿼트 할려고해도 주저 앉드라구요 ㅋㅋ",
          "original_form": "스쿼트 할려고해도 주저 앉드라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042379.1.13",
          "form": "마자요 지금도 힘들지만 처음에는 잘 안되니까 하기도 싫고 그러더라구요ㅠ",
          "original_form": "마자요 지금도 힘들지만 처음에는 잘 안되니까 하기도 싫고 그러더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210907 00:31"
        },
        {
          "id": "MDRW2100042379.1.14",
          "form": "그래도 하다보면 점점 늘려가나바요 ㅎㅎ",
          "original_form": "그래도 하다보면 점점 늘려가나바요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:17"
        },
        {
          "id": "MDRW2100042379.1.15",
          "form": "삘받을때 하면 ㅋㅋㅋㅋ",
          "original_form": "삘받을때 하면 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:17"
        },
        {
          "id": "MDRW2100042379.1.16",
          "form": "마자요 조금씩 느리면 되더라구요\n삘받을때 하면 진짜 잘돼요",
          "original_form": "마자요 조금씩 느리면 되더라구요\n삘받을때 하면 진짜 잘돼요",
          "speaker_id": "2",
          "time": "20210907 12:02"
        },
        {
          "id": "MDRW2100042379.1.17",
          "form": "에어로빅 같은거 허고 싶어요 춤추는",
          "original_form": "에어로빅 같은거 허고 싶어요 춤추는",
          "speaker_id": "1",
          "time": "20210907 12:25"
        },
        {
          "id": "MDRW2100042379.1.18",
          "form": "마자요 그런것도 재밌죠\n신나게 춤추면서 살도 빠지면 최고죠!",
          "original_form": "마자요 그런것도 재밌죠\n신나게 춤추면서 살도 빠지면 최고죠!",
          "speaker_id": "2",
          "time": "20210907 12:26"
        },
        {
          "id": "MDRW2100042379.1.19",
          "form": "저는 요새 배드민턴  배우고 싶더라구요",
          "original_form": "저는 요새 배드민턴  배우고 싶더라구요",
          "speaker_id": "2",
          "time": "20210907 12:27"
        },
        {
          "id": "MDRW2100042379.1.20",
          "form": "오오 배드민턴 재밋죠!!!!",
          "original_form": "오오 배드민턴 재밋죠!!!!",
          "speaker_id": "1",
          "time": "20210907 12:27"
        },
        {
          "id": "MDRW2100042379.1.21",
          "form": "알려주는곳이 따로 잇나요?",
          "original_form": "알려주는곳이 따로 잇나요?",
          "speaker_id": "1",
          "time": "20210907 12:28"
        },
        {
          "id": "MDRW2100042379.1.22",
          "form": "배드민턴 클럽이라고 있더라구요",
          "original_form": "배드민턴 클럽이라고 있더라구요",
          "speaker_id": "2",
          "time": "20210907 12:28"
        },
        {
          "id": "MDRW2100042379.1.23",
          "form": "거기 가면 레슨도 받을수있고 하더라구요",
          "original_form": "거기 가면 레슨도 받을수있고 하더라구요",
          "speaker_id": "2",
          "time": "20210907 12:28"
        },
        {
          "id": "MDRW2100042379.1.24",
          "form": "우아 너무 좋네요",
          "original_form": "우아 너무 좋네요",
          "speaker_id": "1",
          "time": "20210907 12:29"
        }
      ]
    }
