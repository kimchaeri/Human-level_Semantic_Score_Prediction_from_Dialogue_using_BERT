      "utterance": [
        {
          "id": "MDRW2100020815.1.1",
          "form": "저는 집안일을 제가 거의 다 해요",
          "original_form": "저는 집안일을 제가 거의 다 해요",
          "speaker_id": "2",
          "time": "20210828 13:00"
        },
        {
          "id": "MDRW2100020815.1.2",
          "form": "아 저도 그래요! 가정주부신가요",
          "original_form": "아 저도 그래요! 가정주부신가요",
          "speaker_id": "1",
          "time": "20210828 13:00"
        },
        {
          "id": "MDRW2100020815.1.3",
          "form": "일도 하지만 저 혼자하네요ㅠ",
          "original_form": "일도 하지만 저 혼자하네요ㅠ",
          "speaker_id": "2",
          "time": "20210828 13:00"
        },
        {
          "id": "MDRW2100020815.1.4",
          "form": "앗 그건 너무한데요",
          "original_form": "앗 그건 너무한데요",
          "speaker_id": "1",
          "time": "20210828 13:01"
        },
        {
          "id": "MDRW2100020815.1.5",
          "form": "그쵸 가사분담을 하면 좋을텐데",
          "original_form": "그쵸 가사분담을 하면 좋을텐데",
          "speaker_id": "2",
          "time": "20210828 13:01"
        },
        {
          "id": "MDRW2100020815.1.6",
          "form": "빨래랑 설거지 분리수거 같은것만이라도 나눠서 하자고 해보세요!",
          "original_form": "빨래랑 설거지 분리수거 같은것만이라도 나눠서 하자고 해보세요!",
          "speaker_id": "1",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100020815.1.7",
          "form": "그러면 너무 좋겠네요",
          "original_form": "그러면 너무 좋겠네요",
          "speaker_id": "2",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100020815.1.8",
          "form": "저는 혼자 살기때문에 제가 다 하지만",
          "original_form": "저는 혼자 살기때문에 제가 다 하지만",
          "speaker_id": "1",
          "time": "20210828 13:02"
        },
        {
          "id": "MDRW2100020815.1.9",
          "form": "저도 가족들한테 같이하자고 말을 꺼내봐야겠어요",
          "original_form": "저도 가족들한테 같이하자고 말을 꺼내봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020815.1.10",
          "form": "네 혼자서 다 하는건 좀 불합리한 것 같아요ㅠㅠ",
          "original_form": "네 혼자서 다 하는건 좀 불합리한 것 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020815.1.11",
          "form": "네 그리고 좀 부담이 되네요",
          "original_form": "네 그리고 좀 부담이 되네요",
          "speaker_id": "2",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020815.1.12",
          "form": "맞아요 가사노동도 엄연히 일이고..!",
          "original_form": "맞아요 가사노동도 엄연히 일이고..!",
          "speaker_id": "1",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020815.1.13",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020815.1.14",
          "form": "또 일도하시면서 가사까지 전부다 부담하시는 건 좀 그래요",
          "original_form": "또 일도하시면서 가사까지 전부다 부담하시는 건 좀 그래요",
          "speaker_id": "1",
          "time": "20210828 13:04"
        },
