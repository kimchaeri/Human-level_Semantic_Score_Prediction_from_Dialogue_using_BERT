      "utterance": [
        {
          "id": "MDRW2100006136.1.1",
          "form": "추석때 가족들 만나시나요?",
          "original_form": "추석때 가족들 만나시나요?",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006136.1.2",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006136.1.3",
          "form": "저희집이 큰집이긴한데",
          "original_form": "저희집이 큰집이긴한데",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006136.1.4",
          "form": "설날처럼 그냥 영통하려구용",
          "original_form": "설날처럼 그냥 영통하려구용",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006136.1.5",
          "form": "헉 영통하세요?",
          "original_form": "헉 영통하세요?",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006136.1.6",
          "form": "네ㅋㅋㅋ영통으로 절했어요ㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋ영통으로 절했어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006136.1.7",
          "form": "와 ㅋㅋㅋㅋ 진짜 새롭네요",
          "original_form": "와 ㅋㅋㅋㅋ 진짜 새롭네요",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006136.1.8",
          "form": "저흰 아마 큰집 가게될거같아요",
          "original_form": "저흰 아마 큰집 가게될거같아요",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006136.1.9",
          "form": "ㅋㅋㅋㅋ친척분들 많으신가용",
          "original_form": "ㅋㅋㅋㅋ친척분들 많으신가용",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100006136.1.10",
          "form": "아뇨 그렇게 많지 않아요 ㅋㅋㅋ",
          "original_form": "아뇨 그렇게 많지 않아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100006136.1.11",
          "form": "그래서 아마 이번 방역때 좀 느슨해진 기준으로",
          "original_form": "그래서 아마 이번 방역때 좀 느슨해진 기준으로",
          "speaker_id": "1",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100006136.1.12",
          "form": "외식할 수도 있을거같아요",
          "original_form": "외식할 수도 있을거같아요",
          "speaker_id": "1",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100006136.1.13",
          "form": "오오 좋네요",
          "original_form": "오오 좋네요",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100006136.1.14",
          "form": "좋은건진 모르겠어요 ㅋㅋㅋㅋ",
          "original_form": "좋은건진 모르겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100006136.1.15",
          "form": "이번에 백신맞은 사람들이 많아져서 완화한 것도 있는 거같아요",
          "original_form": "이번에 백신맞은 사람들이 많아져서 완화한 것도 있는 거같아요",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100006136.1.16",
          "form": "그쵸 백신 맞으면 걸려도 덜 심각한가보더라구요",
          "original_form": "그쵸 백신 맞으면 걸려도 덜 심각한가보더라구요",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100006136.1.17",
          "form": "네ㅜㅜ100퍼 안걸리는 건 아니어도 걸리면 별로 안아프고 넘어간다고해서",
          "original_form": "네ㅜㅜ100퍼 안걸리는 건 아니어도 걸리면 별로 안아프고 넘어간다고해서",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006136.1.18",
          "form": "그래서 맞았어요저도",
          "original_form": "그래서 맞았어요저도",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006136.1.19",
          "form": "전 지금 예약만 잡아놓은 상태인데",
          "original_form": "전 지금 예약만 잡아놓은 상태인데",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100006136.1.20",
          "form": "부작용 잘못걸리면 출혈 잇대서 좀 불안해요",
          "original_form": "부작용 잘못걸리면 출혈 잇대서 좀 불안해요",
          "speaker_id": "1",
          "time": "20210907 21:11"
        }
