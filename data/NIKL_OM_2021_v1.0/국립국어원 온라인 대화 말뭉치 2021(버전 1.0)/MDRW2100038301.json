      "utterance": [
        {
          "id": "MDRW2100038301.1.1",
          "form": "잘하는 요리있으세요??",
          "original_form": "잘하는 요리있으세요??",
          "speaker_id": "2",
          "time": "20210902 09:07"
        },
        {
          "id": "MDRW2100038301.1.2",
          "form": "잘하는 요리는 없어요...",
          "original_form": "잘하는 요리는 없어요...",
          "speaker_id": "1",
          "time": "20210902 09:08"
        },
        {
          "id": "MDRW2100038301.1.3",
          "form": "해봤자 레토르트라서...ㅋㅋ",
          "original_form": "해봤자 레토르트라서...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:08"
        },
        {
          "id": "MDRW2100038301.1.4",
          "form": "그래도 한다는게대단한거같아요",
          "original_form": "그래도 한다는게대단한거같아요",
          "speaker_id": "2",
          "time": "20210902 09:12"
        },
        {
          "id": "MDRW2100038301.1.5",
          "form": "name2님은 요리 잘하세요?",
          "original_form": "&name2&님은 요리 잘하세요?",
          "speaker_id": "1",
          "time": "20210902 09:13"
        },
        {
          "id": "MDRW2100038301.1.6",
          "form": "아니요 ㅠㅠ",
          "original_form": "아니요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 09:17"
        },
        {
          "id": "MDRW2100038301.1.7",
          "form": "그러면 아예 해본적 없으세요?",
          "original_form": "그러면 아예 해본적 없으세요?",
          "speaker_id": "1",
          "time": "20210902 09:18"
        },
        {
          "id": "MDRW2100038301.1.8",
          "form": "라면이런건 하는편이예요 ㅋㅋ",
          "original_form": "라면이런건 하는편이예요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:19"
        },
        {
          "id": "MDRW2100038301.1.9",
          "form": "ㅋㅋㅋ 그건 생존을 위한 요리죠",
          "original_form": "ㅋㅋㅋ 그건 생존을 위한 요리죠",
          "speaker_id": "1",
          "time": "20210902 09:20"
        },
        {
          "id": "MDRW2100038301.1.10",
          "form": "맞아요 ㅋㅋㅋㅋㅋ간단한것들은 하는거같아요",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ간단한것들은 하는거같아요",
          "speaker_id": "2",
          "time": "20210902 09:37"
        },
        {
          "id": "MDRW2100038301.1.11",
          "form": "그럼 라면 외에 다른거 해본게 있으신가요?",
          "original_form": "그럼 라면 외에 다른거 해본게 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 09:44"
        },
        {
          "id": "MDRW2100038301.1.12",
          "form": "밥이랑",
          "original_form": "밥이랑",
          "speaker_id": "2",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100038301.1.13",
          "form": "후라이??",
          "original_form": "후라이??",
          "speaker_id": "2",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100038301.1.14",
          "form": "아니 후라이랰ㅋㅋㅋ",
          "original_form": "아니 후라이랰ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100038301.1.15",
          "form": "계란 말이 이런겋들",
          "original_form": "계란 말이 이런겋들",
          "speaker_id": "2",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100038301.1.16",
          "form": "해봤어요",
          "original_form": "해봤어요",
          "speaker_id": "2",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100038301.1.17",
          "form": "저 계란말이는 어려워서",
          "original_form": "저 계란말이는 어려워서",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038301.1.18",
          "form": "매번 실패해요",
          "original_form": "매번 실패해요",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038301.1.19",
          "form": "저도 그래서 사각팬에서만해요 ㅋㅋ",
          "original_form": "저도 그래서 사각팬에서만해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:50"
        },
        {
          "id": "MDRW2100038301.1.20",
          "form": "저도 사각팬 하나 장만할까봐요",
          "original_form": "저도 사각팬 하나 장만할까봐요",
          "speaker_id": "1",
          "time": "20210902 09:53"
        },
        {
          "id": "MDRW2100038301.1.21",
          "form": "저도 요리마음먹규샀다가일주일쓰고",
          "original_form": "저도 요리마음먹규샀다가일주일쓰고",
          "speaker_id": "2",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100038301.1.22",
          "form": "엄마가 요긴하게 쓰는중이예요 ㅋㅋ",
          "original_form": "엄마가 요긴하게 쓰는중이예요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:05"
        },
        {
          "id": "MDRW2100038301.1.23",
          "form": "ㅋㅋ 부모님이 좋아하시는군요",
          "original_form": "ㅋㅋ 부모님이 좋아하시는군요",
          "speaker_id": "1",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100038301.1.24",
          "form": "맞아요 제돈으로 사서 개꿀이죠머 ㅋㅋ",
          "original_form": "맞아요 제돈으로 사서 개꿀이죠머 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100038301.1.25",
          "form": "그러시겠어요ㅋㅋ",
          "original_form": "그러시겠어요ㅋㅋ",
