      "utterance": [
        {
          "id": "MDRW2100035598.1.1",
          "form": "피자스쿨 가성비 짱!!!!!",
          "original_form": "피자스쿨 가성비 짱!!!!!",
          "speaker_id": "2",
          "time": "20210829 21:03"
        },
        {
          "id": "MDRW2100035598.1.2",
          "form": "피자스쿨에서 제일 좋아하는 피자는??!",
          "original_form": "피자스쿨에서 제일 좋아하는 피자는??!",
          "speaker_id": "1",
          "time": "20210829 21:03"
        },
        {
          "id": "MDRW2100035598.1.3",
          "form": "하나둘셋!",
          "original_form": "하나둘셋!",
          "speaker_id": "1",
          "time": "20210829 21:03"
        },
        {
          "id": "MDRW2100035598.1.4",
          "form": "고구마",
          "original_form": "고구마{emoji:♡}",
          "speaker_id": "1",
          "time": "20210829 21:03"
        },
        {
          "id": "MDRW2100035598.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ저혼자했네욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저혼자했네욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100035598.1.6",
          "form": "ㅋㅋㅋㅋㅋ앗",
          "original_form": "ㅋㅋㅋㅋㅋ앗",
          "speaker_id": "2",
          "time": "20210829 21:05"
        },
        {
          "id": "MDRW2100035598.1.7",
          "form": "저는 멕시칸바이트 좋아해요-",
          "original_form": "저는 멕시칸바이트 좋아해요-",
          "speaker_id": "2",
          "time": "20210829 21:05"
        },
        {
          "id": "MDRW2100035598.1.8",
          "form": "고구마도 완전 맛나쥬ㅋㅋ",
          "original_form": "고구마도 완전 맛나쥬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:05"
        },
        {
          "id": "MDRW2100035598.1.9",
          "form": "고르기 어려웠음ㅋㅋ",
          "original_form": "고르기 어려웠음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:05"
        },
        {
          "id": "MDRW2100035598.1.10",
          "form": "사실저는 거기서 고구마만 먹어욬ㅋㅋㅋㅋ",
          "original_form": "사실저는 거기서 고구마만 먹어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:06"
        },
        {
          "id": "MDRW2100035598.1.11",
          "form": "멕시칸바이트는 어떤맛이쥬~?",
          "original_form": "멕시칸바이트는 어떤맛이쥬~?",
          "speaker_id": "1",
          "time": "20210829 21:06"
        },
        {
          "id": "MDRW2100035598.1.12",
          "form": "매콤한데",
          "original_form": "매콤한데",
          "speaker_id": "2",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100035598.1.13",
          "form": "치즈바이트랑 고구마무스가 있어서",
          "original_form": "치즈바이트랑 고구마무스가 있어서",
          "speaker_id": "2",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100035598.1.14",
          "form": "완전 조화가 굿이에염",
          "original_form": "완전 조화가 굿이에염{emoji:♡}",
          "speaker_id": "2",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100035598.1.15",
          "form": "오오 거기도 고구마가 들어가는군여!",
          "original_form": "오오 거기도 고구마가 들어가는군여!",
          "speaker_id": "1",
          "time": "20210829 21:15"
        },
        {
          "id": "MDRW2100035598.1.16",
          "form": "피자스쿨 고구마는 진짜 넘사벽인것 같아요~~",
          "original_form": "피자스쿨 고구마는 진짜 넘사벽인것 같아요~~",
          "speaker_id": "1",
          "time": "20210829 21:15"
        },
        {
          "id": "MDRW2100035598.1.17",
          "form": "그쵸ㅜㅜ 넘나 맛있어요",
          "original_form": "그쵸ㅜㅜ 넘나 맛있어요",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100035598.1.18",
          "form": "이사오기전에는 길 건너에\n피자스쿨있어서 자주 먹었는데",
          "original_form": "이사오기전에는 길 건너에\n피자스쿨있어서 자주 먹었는데",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100035598.1.19",
          "form": "이 동네는 어디있나 찾아봐야겠어여",
          "original_form": "이 동네는 어디있나 찾아봐야겠어여",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100035598.1.20",
          "form": "피자스쿨도 가성비 맛집이죠! ㅋㅋㅋ",
          "original_form": "피자스쿨도 가성비 맛집이죠! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100035598.1.21",
          "form": "동네마다 그래도 찾으면 있을거 같아요!! 중딩애들이 좋아하잖아요~~ ㅋㅋㅋ 가격대비 양많아서",
          "original_form": "동네마다 그래도 찾으면 있을거 같아요!! 중딩애들이 좋아하잖아요~~ ㅋㅋㅋ 가격대비 양많아서",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100035598.1.22",
          "form": "맞아요ㅋㅋ 홀은 항상\n중고등학생으로 \n가득차 있던 기억이 있네요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ 홀은 항상\n중고등학생으로 \n가득차 있던 기억이 있네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035598.1.23",
          "form": "그쳐 ㅋㅋㅋㅋ 저도 고딩때 친구들이랑 자주 갔던것 같아요~",
          "original_form": "그쳐 ㅋㅋㅋㅋ 저도 고딩때 친구들이랑 자주 갔던것 같아요~",
          "speaker_id": "1",
          "time": "20210829 21:54"
        },
        {
          "id": "MDRW2100035598.1.24",
          "form": "저는 그때는 피자마루요ㅋㅋ",
          "original_form": "저는 그때는 피자마루요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:55"
        },
        {
          "id": "MDRW2100035598.1.25",
          "form": "아 맞다 ㅋㅋㅋ그땐 브랜드 되게 많았죠@!",
          "original_form": "아 맞다 ㅋㅋㅋ그땐 브랜드 되게 많았죠@!",
          "speaker_id": "1",
          "time": "20210829 22:03"
        },
        {
          "id": "MDRW2100035598.1.26",
          "form": "59쌀피자? 또... 종류되게많았던거 같아요 ㅋㅋㅋ",
          "original_form": "59쌀피자? 또... 종류되게많았던거 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
