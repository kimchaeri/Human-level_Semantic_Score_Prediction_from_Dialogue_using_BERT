      "utterance": [
        {
          "id": "MDRW2100024645.1.1",
          "form": "안녕하세요 반갑습니다^^ 요즘 비가 자주 오던데 지역에 비피해는 없나요?",
          "original_form": "안녕하세요 반갑습니다^^ 요즘 비가 자주 오던데 지역에 비피해는 없나요?",
          "speaker_id": "1",
          "time": "20210831 16:32"
        },
        {
          "id": "MDRW2100024645.1.2",
          "form": "비가 계속 오긴한데 다행히 침수피해같은건 없어요",
          "original_form": "비가 계속 오긴한데 다행히 침수피해같은건 없어요",
          "speaker_id": "2",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100024645.1.3",
          "form": "거기도 비가 계속 오는군요.. 여기도 하루 종일 와요",
          "original_form": "거기도 비가 계속 오는군요.. 여기도 하루 종일 와요",
          "speaker_id": "1",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100024645.1.4",
          "form": "비 좋아하나요?",
          "original_form": "비 좋아하나요?",
          "speaker_id": "1",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100024645.1.5",
          "form": "어... 실내에 있을땐 좋아해요",
          "original_form": "어... 실내에 있을땐 좋아해요",
          "speaker_id": "2",
          "time": "20210831 16:40"
        },
        {
          "id": "MDRW2100024645.1.6",
          "form": "사실 해가 쨍쨍한걸 그닥 좋아하지 않거든요",
          "original_form": "사실 해가 쨍쨍한걸 그닥 좋아하지 않거든요",
          "speaker_id": "2",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100024645.1.7",
          "form": "아 그런가요? 저는 화창한 날씨를 좋아해서요",
          "original_form": "아 그런가요? 저는 화창한 날씨를 좋아해서요",
          "speaker_id": "1",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100024645.1.8",
          "form": "그럼 name2님은 어느 계절을 가장 좋아하나요?",
          "original_form": "그럼 &name2&님은 어느 계절을 가장 좋아하나요?",
          "speaker_id": "1",
          "time": "20210831 16:43"
        },
        {
          "id": "MDRW2100024645.1.9",
          "form": "가을이요",
          "original_form": "가을이요",
          "speaker_id": "2",
          "time": "20210831 16:47"
        },
        {
          "id": "MDRW2100024645.1.10",
          "form": "하늘 파랗고 공기 선선하고 참 좋죠",
          "original_form": "하늘 파랗고 공기 선선하고 참 좋죠",
          "speaker_id": "2",
          "time": "20210831 16:47"
        },
        {
          "id": "MDRW2100024645.1.11",
          "form": "저랑 같네요.. 저도 가을 좋아해요",
          "original_form": "저랑 같네요.. 저도 가을 좋아해요",
          "speaker_id": "1",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100024645.1.12",
          "form": "맑고 파란 높은 하늘 선선한 공기",
          "original_form": "맑고 파란 높은 하늘 선선한 공기",
          "speaker_id": "1",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100024645.1.13",
          "form": "무엇보다 단풍을 좋아한답니다",
          "original_form": "무엇보다 단풍을 좋아한답니다",
          "speaker_id": "1",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100024645.1.14",
          "form": "단풍좋죠",
          "original_form": "단풍좋죠",
          "speaker_id": "2",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100024645.1.15",
          "form": "전 산이나 숲을 좋아해서",
          "original_form": "전 산이나 숲을 좋아해서",
          "speaker_id": "2",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100024645.1.16",
          "form": "그럼 단풍 보러 갔던 산 있나요? 내장산이나 설악산 같은",
          "original_form": "그럼 단풍 보러 갔던 산 있나요? 내장산이나 설악산 같은",
          "speaker_id": "1",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100024645.1.17",
          "form": "가을쯤에 휴양림을가거나 템플스테이를 하곤 해요",
          "original_form": "가을쯤에 휴양림을가거나 템플스테이를 하곤 해요",
          "speaker_id": "2",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100024645.1.18",
          "form": "속리산이랑 오대산 남산 북한산 관악산...",
          "original_form": "속리산이랑 오대산 남산 북한산 관악산...",
          "speaker_id": "2",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100024645.1.19",
          "form": "언젠간 지리산도 올라보고 싶은데",
          "original_form": "언젠간 지리산도 올라보고 싶은데",
          "speaker_id": "2",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100024645.1.20",
          "form": "정말 산 많이 다녔네요",
          "original_form": "정말 산 많이 다녔네요",
          "speaker_id": "1",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024645.1.21",
          "form": "정상까지 다 갔던건가요?",
          "original_form": "정상까지 다 갔던건가요?",
          "speaker_id": "1",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024645.1.22",
          "form": "가장 높은 봉을 오른건 아니지만",
          "original_form": "가장 높은 봉을 오른건 아니지만",
          "speaker_id": "2",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100024645.1.23",
          "form": "일단 등산하면 정상은 찍고싶어지더라구요",
          "original_form": "일단 등산하면 정상은 찍고싶어지더라구요",
          "speaker_id": "2",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100024645.1.24",
          "form": "대단합니다.. 저는 거의 그냥 산 중턱",
          "original_form": "대단합니다.. 저는 거의 그냥 산 중턱",
          "speaker_id": "1",
          "time": "20210831 16:55"
        },
        {
          "id": "MDRW2100024645.1.25",
          "form": "글쿤요. 전 딱히 등산이 목적이 아닐때도",
          "original_form": "글쿤요. 전 딱히 등산이 목적이 아닐때도",
          "speaker_id": "2",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100024645.1.26",
          "form": "기왕온거 정상은 찍어야지 같은 맘이라 그렇게 되더라구요",
          "original_form": "기왕온거 정상은 찍어야지 같은 맘이라 그렇게 되더라구요",
          "speaker_id": "2",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100024645.1.27",
          "form": "의지가 강하시네요",
          "original_form": "의지가 강하시네요",
          "speaker_id": "1",
          "time": "20210831 17:13"
        },
        {
          "id": "MDRW2100024645.1.28",
          "form": "지금 날씨는 어때요?",
          "original_form": "지금 날씨는 어때요?",
          "speaker_id": "1",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100024645.1.29",
          "form": "여기는 아주 오랜만에 해가 떴어요",
          "original_form": "여기는 아주 오랜만에 해가 떴어요",
