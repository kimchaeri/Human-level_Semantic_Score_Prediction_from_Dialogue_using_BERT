      "utterance": [
        {
          "id": "MDRW2100033425.1.1",
          "form": "연애는 잘하고 계신가요",
          "original_form": "연애는 잘하고 계신가요",
          "speaker_id": "1",
          "time": "20210907 08:14"
        },
        {
          "id": "MDRW2100033425.1.2",
          "form": "저는 기혼자예요~~",
          "original_form": "저는 기혼자예요~~",
          "speaker_id": "2",
          "time": "20210907 08:15"
        },
        {
          "id": "MDRW2100033425.1.3",
          "form": "오 같은 처지시군요",
          "original_form": "오 같은 처지시군요",
          "speaker_id": "1",
          "time": "20210907 08:15"
        },
        {
          "id": "MDRW2100033425.1.4",
          "form": "아이도 있으신가요?",
          "original_form": "아이도 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 08:17"
        },
        {
          "id": "MDRW2100033425.1.5",
          "form": "아직이요 ㅎㅎ name2님은요?",
          "original_form": "아직이요 ㅎㅎ &name2&님은요?",
          "speaker_id": "1",
          "time": "20210907 08:30"
        },
        {
          "id": "MDRW2100033425.1.6",
          "form": "저는 아들 둘과 제일큰아들ㅋㅋㅋ 함께하고 있어요ㅋㅋ",
          "original_form": "저는 아들 둘과 제일큰아들ㅋㅋㅋ 함께하고 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:32"
        },
        {
          "id": "MDRW2100033425.1.7",
          "form": "ㅋㅋ큰아들 ㅋㅋㅋ",
          "original_form": "ㅋㅋ큰아들 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100033425.1.8",
          "form": "저희 와이프도 항상 그러는데",
          "original_form": "저희 와이프도 항상 그러는데",
          "speaker_id": "1",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100033425.1.9",
          "form": "앗, 남자분이셨군요!ㅎㅎ\r\n아이도 없으시면 항상 신혼이시겠네요",
          "original_form": "앗, 남자분이셨군요!ㅎㅎ\r\n아이도 없으시면 항상 신혼이시겠네요",
          "speaker_id": "2",
          "time": "20210907 10:06"
        },
        {
          "id": "MDRW2100033425.1.10",
          "form": "ㅋㅋ 꼭 그렇지는 않던데요!",
          "original_form": "ㅋㅋ 꼭 그렇지는 않던데요!",
          "speaker_id": "1",
          "time": "20210907 10:28"
        },
        {
          "id": "MDRW2100033425.1.11",
          "form": "왜요??\r\n애로 사항이 있으신가요?ㅠㅠ",
          "original_form": "왜요??\r\n애로 사항이 있으신가요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 10:44"
        },
        {
          "id": "MDRW2100033425.1.12",
          "form": "좋다가 싸우다가 반복이죠 ㅎㅎ",
          "original_form": "좋다가 싸우다가 반복이죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 12:50"
        },
        {
          "id": "MDRW2100033425.1.13",
          "form": "다른 사람둘이 한가정을 이뤄서 맞춰살기가 쉽지는 않지요 ㅎㅎ",
          "original_form": "다른 사람둘이 한가정을 이뤄서 맞춰살기가 쉽지는 않지요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100033425.1.14",
          "form": "저는 결혼한지 9년이 되었는데도 싸워요 ㅎㅎ",
          "original_form": "저는 결혼한지 9년이 되었는데도 싸워요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100033425.1.15",
          "form": "아들 둘이라니 힘드시겠어요 ㅋㅋ전쟁터라는데",
          "original_form": "아들 둘이라니 힘드시겠어요 ㅋㅋ전쟁터라는데",
          "speaker_id": "1",
          "time": "20210907 13:55"
        },
        {
          "id": "MDRW2100033425.1.16",
          "form": "네 집안은 폭탄맞았구 ,\r\n저는 맨날 소리지르죠",
          "original_form": "네 집안은 폭탄맞았구 ,\r\n저는 맨날 소리지르죠",
