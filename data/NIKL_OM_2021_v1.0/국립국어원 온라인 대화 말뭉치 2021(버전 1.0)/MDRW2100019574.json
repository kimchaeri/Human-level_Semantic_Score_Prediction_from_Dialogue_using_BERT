        {
          "id": "MDRW2100019574.1.1",
          "form": "여행!!!!!",
          "original_form": "여행!!!!!",
          "speaker_id": "2",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100019574.1.2",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019574.1.3",
          "form": "완전좋아해요!!!!",
          "original_form": "완전좋아해요!!!!",
          "speaker_id": "2",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019574.1.4",
          "form": "사진찍고여행가믄거 취미라",
          "original_form": "사진찍고여행가믄거 취미라",
          "speaker_id": "2",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019574.1.5",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019574.1.6",
          "form": "제인스타에도 사진많아녀!!!",
          "original_form": "제인스타에도 사진많아녀!!!",
          "speaker_id": "2",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019574.1.7",
          "form": "어디가 젤 좋으셨어요?",
          "original_form": "어디가 젤 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019574.1.8",
          "form": "파리요!!!",
          "original_form": "파리요!!!",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100019574.1.9",
          "form": "와 진짜 좋았겠어요",
          "original_form": "와 진짜 좋았겠어요",
          "speaker_id": "1",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100019574.1.10",
          "form": "음식은 어때요??",
          "original_form": "음식은 어때요??",
          "speaker_id": "1",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100019574.1.11",
          "form": "하 맛있었어요,,,",
          "original_form": "하 맛있었어요,,,",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100019574.1.12",
          "form": "와인과 맥주가 빠지지않고",
          "original_form": "와인과 맥주가 빠지지않고",
          "speaker_id": "2",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100019574.1.13",
          "form": "와인 ㅠㅠ 먹고싶네요",
          "original_form": "와인 ㅠㅠ 먹고싶네요",
          "speaker_id": "1",
          "time": "20210827 21:27"
        },
        {
          "id": "MDRW2100019574.1.14",
          "form": "하 저두여",
          "original_form": "하 저두여",
          "speaker_id": "2",
          "time": "20210827 21:27"
        },
        {
          "id": "MDRW2100019574.1.15",
          "form": "파리말고 국내에서는요??",
          "original_form": "파리말고 국내에서는요??",
          "speaker_id": "1",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100019574.1.16",
          "form": "음...거의다가본거같아여!!!",
          "original_form": "음...거의다가본거같아여!!!",
          "speaker_id": "2",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100019574.1.17",
          "form": "강릉이 젤 좋았어요 전!!!",
          "original_form": "강릉이 젤 좋았어요 전!!!",
          "speaker_id": "2",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100019574.1.18",
          "form": "누나는여??!",
          "original_form": "누나는여??!",
          "speaker_id": "2",
          "time": "20210827 21:28"
        },
        {
          "id": "MDRW2100019574.1.19",
          "form": "흠 딱히 가본데가 없어서 저는 ㅜㅜ",
          "original_form": "흠 딱히 가본데가 없어서 저는 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100019574.1.20",
          "form": "걍 친구들이랑 가깝게간 가평이 좋았던거같아요 ㅋㅋㅋ",
          "original_form": "걍 친구들이랑 가깝게간 가평이 좋았던거같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:29"
        },
        {
          "id": "MDRW2100019574.1.21",
          "form": "아하아",
          "original_form": "아하아",
          "speaker_id": "2",
          "time": "20210827 21:29"
        },
        {
