      "utterance": [
        {
          "id": "MDRW2100040349.1.1",
          "form": "부산여행 또 어디로 가세요 루트가",
          "original_form": "부산여행 또 어디로 가세요 루트가",
          "speaker_id": "2",
          "time": "20210904 16:57"
        },
        {
          "id": "MDRW2100040349.1.2",
          "form": "해운대에서 2박하니 해운대구경하고 국제시장이나 자갈치시장가보고..음!!",
          "original_form": "해운대에서 2박하니 해운대구경하고 국제시장이나 자갈치시장가보고..음!!",
          "speaker_id": "1",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100040349.1.3",
          "form": "아직정확히안정했어요",
          "original_form": "아직정확히안정했어요",
          "speaker_id": "1",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100040349.1.4",
          "form": "스카이캡슐만 예약하려구요",
          "original_form": "스카이캡슐만 예약하려구요",
          "speaker_id": "1",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100040349.1.5",
          "form": "아 네네 그거 저도 타보진못했지만",
          "original_form": "아 네네 그거 저도 타보진못했지만",
          "speaker_id": "2",
          "time": "20210904 16:59"
        },
        {
          "id": "MDRW2100040349.1.6",
          "form": "자갈치보단 국제 추천할게요",
          "original_form": "자갈치보단 국제 추천할게요",
          "speaker_id": "2",
          "time": "20210904 16:59"
        },
        {
          "id": "MDRW2100040349.1.7",
          "form": "아..그래요? 아주좋은정보네요",
          "original_form": "아..그래요? 아주좋은정보네요",
          "speaker_id": "1",
          "time": "20210904 16:59"
        },
        {
          "id": "MDRW2100040349.1.8",
          "form": "네네 남포동 이재모 피자도 추천드려요",
          "original_form": "네네 남포동 이재모 피자도 추천드려요",
          "speaker_id": "2",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100040349.1.9",
          "form": "아.그래요? 애들완전좋아하겠어요",
          "original_form": "아.그래요? 애들완전좋아하겠어요",
          "speaker_id": "1",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100040349.1.10",
          "form": "적어놓을께요",
          "original_form": "적어놓을께요",
          "speaker_id": "1",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100040349.1.11",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100040349.1.12",
          "form": "카페는 연경재 추천드려요 남포동 연경재 중앙동",
          "original_form": "카페는 연경재 추천드려요 남포동 연경재 중앙동",
          "speaker_id": "2",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100040349.1.13",
          "form": "네네 영도도 좋아요 요즘",
          "original_form": "네네 영도도 좋아요 요즘",
          "speaker_id": "2",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100040349.1.14",
          "form": "오~~넘넘좋아요",
          "original_form": "오~~넘넘좋아요",
          "speaker_id": "1",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100040349.1.15",
          "form": "이런꿀팁을주시다니",
          "original_form": "이런꿀팁을주시다니",
          "speaker_id": "1",
          "time": "20210904 17:02"
        },
        {
          "id": "MDRW2100040349.1.16",
          "form": "하나도 빠짐없이 적고있네요",
          "original_form": "하나도 빠짐없이 적고있네요",
          "speaker_id": "1",
          "time": "20210904 17:03"
        },
        {
          "id": "MDRW2100040349.1.17",
          "form": "오 별거 아닌데 감사해요",
          "original_form": "오 별거 아닌데 감사해요",
          "speaker_id": "2",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100040349.1.18",
          "form": "인터넷으론 진짜를 찾기가어려운데 현지인 맛집이최고죠!!",
          "original_form": "인터넷으론 진짜를 찾기가어려운데 현지인 맛집이최고죠!!",
          "speaker_id": "1",
          "time": "20210904 17:04"
        },
        {
          "id": "MDRW2100040349.1.19",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "2",
          "time": "20210904 17:05"
        },
        {
          "id": "MDRW2100040349.1.20",
          "form": "해운대면 달맞이도 가보셔요",
          "original_form": "해운대면 달맞이도 가보셔요",
          "speaker_id": "2",
          "time": "20210904 17:05"
        },
        {
          "id": "MDRW2100040349.1.21",
          "form": "제가 진짜 감사드려요",
          "original_form": "제가 진짜 감사드려요",
          "speaker_id": "1",
          "time": "20210904 17:05"
        },
        {
          "id": "MDRW2100040349.1.22",
          "form": "애들도 신나서 적고있네요",
          "original_form": "애들도 신나서 적고있네요",
          "speaker_id": "1",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100040349.1.23",
          "form": "부산엔 아는사람 하나도없다보니 물어볼수도없고",
          "original_form": "부산엔 아는사람 하나도없다보니 물어볼수도없고",
          "speaker_id": "1",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100040349.1.24",
          "form": "심심이하길잘했네요",
          "original_form": "심심이하길잘했네요",
          "speaker_id": "1",
          "time": "20210904 17:09"
        },
        {
          "id": "MDRW2100040349.1.25",
          "form": "그러네요 다행이에요 ㅋㅋ",
          "original_form": "그러네요 다행이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100040349.1.26",
          "form": "또 뮬어볼거있음 물어보셔요",
          "original_form": "또 뮬어볼거있음 물어보셔요",
          "speaker_id": "2",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100040349.1.27",
          "form": "아 구리고 루지도 생겼어요 기장에",
          "original_form": "아 구리고 루지도 생겼어요 기장에",
          "speaker_id": "2",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100040349.1.28",
          "form": "여기도 애들 좋아할거같아요",
          "original_form": "여기도 애들 좋아할거같아요",
          "speaker_id": "2",
