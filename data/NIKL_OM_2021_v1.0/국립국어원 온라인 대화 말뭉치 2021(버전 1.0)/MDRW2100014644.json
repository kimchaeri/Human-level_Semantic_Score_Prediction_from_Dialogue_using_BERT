      "utterance": [
        {
          "id": "MDRW2100014644.1.1",
          "form": "여행가자",
          "original_form": "여행가자",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014644.1.2",
          "form": "여행 어디가고 싶어?",
          "original_form": "여행 어디가고 싶어?",
          "speaker_id": "2",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014644.1.3",
          "form": "나 너랑 경주",
          "original_form": "나 너랑 경주",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014644.1.4",
          "form": "다음주말에",
          "original_form": "다음주말에",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014644.1.5",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014644.1.6",
          "form": "나도 모르는 약속이 있었니?",
          "original_form": "나도 모르는 약속이 있었니?",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014644.1.7",
          "form": "어머 잊었니",
          "original_form": "어머 잊었니",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014644.1.8",
          "form": "매우 놀랍다",
          "original_form": "매우 놀랍다",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014644.1.9",
          "form": "실망이야",
          "original_form": "실망이야",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014644.1.10",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014644.1.11",
          "form": "9월 첫주",
          "original_form": "9월 첫주",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014644.1.12",
          "form": "금요일에 가자",
          "original_form": "금요일에 가자",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014644.1.13",
          "form": "언제 정한거니",
          "original_form": "언제 정한거니",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014644.1.14",
          "form": "일요일에 올라오고",
          "original_form": "일요일에 올라오고",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014644.1.15",
          "form": "방금 ㅎㅎ",
          "original_form": "방금 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014644.1.16",
          "form": "그랬구나",
          "original_form": "그랬구나",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014644.1.17",
          "form": "놀랍다",
          "original_form": "놀랍다",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014644.1.18",
          "form": "마음을 읽었어야지",
          "original_form": "마음을 읽었어야지",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014644.1.19",
          "form": "여긴 실망스러운게 아니야",
          "original_form": "여긴 실망스러운게 아니야",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014644.1.20",
          "form": "그건 아직 무리지",
          "original_form": "그건 아직 무리지",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014644.1.21",
          "form": "그래도 기회를 줄게",
          "original_form": "그래도 기회를 줄게",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014644.1.22",
          "form": "텔레파시까진 안되는걸",
          "original_form": "텔레파시까진 안되는걸",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014644.1.23",
          "form": "담주 주말에 가자",
          "original_form": "담주 주말에 가자",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014644.1.24",
          "form": "노력해보자 우리",
          "original_form": "노력해보자 우리",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014644.1.25",
          "form": "아마",
          "original_form": "아마",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014644.1.26",
          "form": "안될꺼야",
          "original_form": "안될꺼야",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014644.1.27",
          "form": "경주에는 볼 거리가 많아",
          "original_form": "경주에는 볼 거리가 많아",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014644.1.28",
          "form": "첨성대",
          "original_form": "첨성대",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014644.1.29",
          "form": "불국사",
          "original_form": "불국사",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014644.1.30",
          "form": "친구야",
          "original_form": "친구야",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014644.1.31",
          "form": "여행은 어렵구나",
          "original_form": "여행은 어렵구나",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014644.1.32",
          "form": "알겠어",
          "original_form": "알겠어",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014644.1.33",
          "form": "경주 가본적은 있니",
          "original_form": "경주 가본적은 있니",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014644.1.34",
          "form": "아 근데 이거 왜 못 웃게 해놨어",
          "original_form": "아 근데 이거 왜 못 웃게 해놨어",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014644.1.35",
          "form": "의미없는 대화래",
          "original_form": "의미없는 대화래",
          "speaker_id": "1",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014644.1.36",
          "form": "경주 초등학교때 가봤지",
          "original_form": "경주 초등학교때 가봤지",
          "speaker_id": "2",
          "time": "20210825 20:08"
        },
        {
          "id": "MDRW2100014644.1.37",
          "form": "어연 20년이구나",
          "original_form": "어연 20년이구나",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014644.1.38",
          "form": "웃긴걸 웃기다고 못하네",
          "original_form": "웃긴걸 웃기다고 못하네",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014644.1.39",
          "form": "아니다 그럼 너무 늙네",
          "original_form": "아니다 그럼 너무 늙네",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014644.1.40",
          "form": "15년",
          "original_form": "15년",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014644.1.41",
          "form": "20년을 빼면",
          "original_form": "20년을 빼면",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014644.1.42",
          "form": "7살이네",
          "original_form": "7살이네",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014644.1.43",
          "form": "나도 그정도 된 것 같아",
          "original_form": "나도 그정도 된 것 같아",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014644.1.44",
          "form": "살아있네...",
          "original_form": "살아있네...",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014644.1.45",
          "form": "7살 때는 갔던 곳 있니",
          "original_form": "7살 때는 갔던 곳 있니",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014644.1.46",
          "form": "7살땐",
          "original_form": "7살땐",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014644.1.47",
          "form": "대전에서 살았었지",
          "original_form": "대전에서 살았었지",
          "speaker_id": "2",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014644.1.48",
          "form": "대전은 성심당이 좋지",
          "original_form": "대전은 성심당이 좋지",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014644.1.49",
          "form": "성심당만 좋지",
          "original_form": "성심당만 좋지",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014644.1.50",
          "form": "빵이 맛있지",
          "original_form": "빵이 맛있지",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014644.1.51",
          "form": "그건 그랫지",
          "original_form": "그건 그랫지",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014644.1.52",
          "form": "빵은 진짜 맛있었지",
          "original_form": "빵은 진짜 맛있었지",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014644.1.53",
          "form": "대전은 볼게 없었어",
          "original_form": "대전은 볼게 없었어",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014644.1.54",
          "form": "여행은 역시 부산",
          "original_form": "여행은 역시 부산",
          "speaker_id": "1",
          "time": "20210825 20:10"
        }
      ]
    }
