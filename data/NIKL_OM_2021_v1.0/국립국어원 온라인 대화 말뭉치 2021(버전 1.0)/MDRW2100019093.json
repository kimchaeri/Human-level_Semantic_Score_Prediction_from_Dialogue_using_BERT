        {
          "id": "MDRW2100019093.1.1",
          "form": "로제떡보끼 좋아하세요",
          "original_form": "로제떡보끼 좋아하세요",
          "speaker_id": "1",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019093.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019093.1.3",
          "form": "드셔보셨어요?",
          "original_form": "드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019093.1.4",
          "form": "네 저도 좋아해요",
          "original_form": "네 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019093.1.5",
          "form": "어디거 드세요?",
          "original_form": "어디거 드세요?",
          "speaker_id": "2",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019093.1.6",
          "form": "저는 배떡이요 님은?",
          "original_form": "저는 배떡이요 님은?",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019093.1.7",
          "form": "저는 신전 먹어요 ㅋㅋㅋ",
          "original_form": "저는 신전 먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019093.1.8",
          "form": "브랜드가 많아 골라먹는 재미가 있엉ㆍ느",
          "original_form": "브랜드가 많아 골라먹는 재미가 있엉ㆍ느",
          "speaker_id": "1",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019093.1.9",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "1",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019093.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019093.1.11",
          "form": "요새 다 있더라구여",
          "original_form": "요새 다 있더라구여",
          "speaker_id": "2",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019093.1.12",
          "form": "어디어디 드셔보셨어요?",
          "original_form": "어디어디 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019093.1.13",
          "form": "저 엽떡 신전 죠스",
          "original_form": "저 엽떡 신전 죠스",
          "speaker_id": "1",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019093.1.14",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019093.1.15",
          "form": "많이드셔보셨네요",
          "original_form": "많이드셔보셨네요",
          "speaker_id": "2",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019093.1.16",
          "form": "ㅋㅋㅋ어쩌다보니",
          "original_form": "ㅋㅋㅋ어쩌다보니",
          "speaker_id": "1",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019093.1.17",
          "form": "배달단골됐어요",
          "original_form": "배달단골됐어요",
          "speaker_id": "1",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019093.1.18",
          "form": "저두요 요새 배달을 하도 많이 시켜서 ㅋㅋㅋ",
          "original_form": "저두요 요새 배달을 하도 많이 시켜서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019093.1.19",
          "form": "코로나땜에 더 자주 시키는듯요",
          "original_form": "코로나땜에 더 자주 시키는듯요",
