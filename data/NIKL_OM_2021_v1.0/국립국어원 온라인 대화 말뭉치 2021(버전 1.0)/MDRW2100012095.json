      "utterance": [
        {
          "id": "MDRW2100012095.1.1",
          "form": "최고의 효심",
          "original_form": "최고의 효심",
          "speaker_id": "2",
          "time": "20210824 19:07"
        },
        {
          "id": "MDRW2100012095.1.2",
          "form": "맛있게 먹기",
          "original_form": "맛있게 먹기",
          "speaker_id": "2",
          "time": "20210824 19:07"
        },
        {
          "id": "MDRW2100012095.1.3",
          "form": "그래요 자식의 건강이 효의 실천이죠",
          "original_form": "그래요 자식의 건강이 효의 실천이죠",
          "speaker_id": "1",
          "time": "20210824 19:07"
        },
        {
          "id": "MDRW2100012095.1.4",
          "form": "아참 저녁먹고 냉장고 구석에 있던 매실청을 찾아냈어요",
          "original_form": "아참 저녁먹고 냉장고 구석에 있던 매실청을 찾아냈어요",
          "speaker_id": "2",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012095.1.5",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012095.1.6",
          "form": "몇년 묵었는지 거의 매실주가 되어가는 녀석을",
          "original_form": "몇년 묵었는지 거의 매실주가 되어가는 녀석을",
          "speaker_id": "2",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012095.1.7",
          "form": "얼음넣어서 호로록",
          "original_form": "얼음넣어서 호로록",
          "speaker_id": "1",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012095.1.8",
          "form": "탄산수에 탔습니다",
          "original_form": "탄산수에 탔습니다",
          "speaker_id": "2",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012095.1.9",
          "form": "완벽",
          "original_form": "완벽",
          "speaker_id": "1",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012095.1.10",
          "form": "캬~~~",
          "original_form": "캬~~~",
          "speaker_id": "2",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012095.1.11",
          "form": "맛이 좋더라고요",
          "original_form": "맛이 좋더라고요",
          "speaker_id": "2",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012095.1.12",
          "form": "구와 신의 만남",
          "original_form": "구와 신의 만남",
          "speaker_id": "1",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012095.1.13",
          "form": "매실청의 현대적 재해석",
          "original_form": "매실청의 현대적 재해석",
          "speaker_id": "2",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012095.1.14",
          "form": "매실소다~",
          "original_form": "매실소다~",
          "speaker_id": "2",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012095.1.15",
          "form": "맛잘알~",
          "original_form": "맛잘알~",
          "speaker_id": "1",
          "time": "20210824 19:09"
        },
        {
          "id": "MDRW2100012095.1.16",
          "form": "진정한 현대인이십니다",
          "original_form": "진정한 현대인이십니다",
          "speaker_id": "1",
          "time": "20210824 19:09"
        },
        {
          "id": "MDRW2100012095.1.17",
          "form": "ㅋㅋ 한식당에서 밥먹으면 매실나오잖아요",
          "original_form": "ㅋㅋ 한식당에서 밥먹으면 매실나오잖아요",
          "speaker_id": "2",
          "time": "20210824 19:09"
        },
        {
          "id": "MDRW2100012095.1.18",
          "form": "ㅋㅋ맞아요ㅋㅋ",
          "original_form": "ㅋㅋ맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:09"
        },
        {
          "id": "MDRW2100012095.1.19",
          "form": "그런 너낌으로 호로록 했습니다",
          "original_form": "그런 너낌으로 호로록 했습니다",
          "speaker_id": "2",
          "time": "20210824 19:09"
        },
        {
          "id": "MDRW2100012095.1.20",
          "form": "딱 그느낌인데요ㅋㅋ",
          "original_form": "딱 그느낌인데요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:09"
        },
        {
          "id": "MDRW2100012095.1.21",
          "form": "건강식마니아 어머니는.. 달다고 타박하셨지만..",
          "original_form": "건강식마니아 어머니는.. 달다고 타박하셨지만..",
          "speaker_id": "2",
          "time": "20210824 19:09"
        },
        {
          "id": "MDRW2100012095.1.22",
          "form": "전 단게 좋아욧",
          "original_form": "전 단게 좋아욧",
          "speaker_id": "2",
          "time": "20210824 19:09"
        },
        {
          "id": "MDRW2100012095.1.23",
          "form": "ㅋㅋㅋ어머니..그맛에 먹는겁니다..",
          "original_form": "ㅋㅋㅋ어머니..그맛에 먹는겁니다..",
          "speaker_id": "1",
          "time": "20210824 19:09"
        },
        {
          "id": "MDRW2100012095.1.24",
          "form": "ㅋㅋㅋㅋ맞습니다",
          "original_form": "ㅋㅋㅋㅋ맞습니다",
          "speaker_id": "2",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100012095.1.25",
          "form": "저나트륨 저당을 부르짖으시는 어머니 덕분에",
          "original_form": "저나트륨 저당을 부르짖으시는 어머니 덕분에",
          "speaker_id": "2",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100012095.1.26",
          "form": "전 항상 소금과 설탕을 추가로 넣어먹는답니다..",
          "original_form": "전 항상 소금과 설탕을 추가로 넣어먹는답니다..",
          "speaker_id": "2",
          "time": "20210824 19:10"
        }
      ]
    }
