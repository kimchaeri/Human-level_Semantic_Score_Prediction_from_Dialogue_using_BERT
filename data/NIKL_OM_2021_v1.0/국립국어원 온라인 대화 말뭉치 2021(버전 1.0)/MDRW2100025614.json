        {
          "id": "MDRW2100025614.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210901 14:22"
        },
        {
          "id": "MDRW2100025614.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210901 14:23"
        },
        {
          "id": "MDRW2100025614.1.3",
          "form": "저도ㅠ여건이",
          "original_form": "저도ㅠ여건이",
          "speaker_id": "1",
          "time": "20210901 14:35"
        },
        {
          "id": "MDRW2100025614.1.4",
          "form": "전 키우다 하늘로 가서 그뒤로 아직 ㅎㅎ",
          "original_form": "전 키우다 하늘로 가서 그뒤로 아직 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100025614.1.5",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025614.1.6",
          "form": "강아지 키우고싶은데",
          "original_form": "강아지 키우고싶은데",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025614.1.7",
          "form": "전 아직은 마음의 준비가 ㅎㅎ",
          "original_form": "전 아직은 마음의 준비가 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100025614.1.8",
          "form": "저누시간이",
          "original_form": "저누시간이",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100025614.1.9",
          "form": "반려동뮬 키운다는데 참 힘들어서",
          "original_form": "반려동뮬 키운다는데 참 힘들어서",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100025614.1.10",
          "form": "심사숙고 해야죠 ㅎ",
          "original_form": "심사숙고 해야죠 ㅎ",
          "speaker_id": "2",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100025614.1.11",
          "form": "관심있으시면 먼저 생각해야조",
          "original_form": "관심있으시면 먼저 생각해야조",
          "speaker_id": "1",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100025614.1.12",
          "form": "진짜  육아 하는거 비슷한거 같아요 ㅎㅎ",
          "original_form": "진짜  육아 하는거 비슷한거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100025614.1.13",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100025614.1.14",
          "form": "저는 좀다 시간이 지나면 생각해보려규요",
          "original_form": "저는 좀다 시간이 지나면 생각해보려규요",
          "speaker_id": "2",
          "time": "20210901 22:53"
        },
        {
          "id": "MDRW2100025614.1.15",
          "form": "여건되면요",
          "original_form": "여건되면요",
