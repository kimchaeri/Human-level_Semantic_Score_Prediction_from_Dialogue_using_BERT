      "utterance": [
        {
          "id": "MDRW2100020758.1.1",
          "form": "요즘 젊은 사람들은 티비 별로 안 본다고 하던데",
          "original_form": "요즘 젊은 사람들은 티비 별로 안 본다고 하던데",
          "speaker_id": "1",
          "time": "20210828 12:12"
        },
        {
          "id": "MDRW2100020758.1.2",
          "form": "좋아하는 티비 프로그램 있으세요",
          "original_form": "좋아하는 티비 프로그램 있으세요",
          "speaker_id": "1",
          "time": "20210828 12:12"
        },
        {
          "id": "MDRW2100020758.1.3",
          "form": "OTT서비스 많이들 이용하죠ㅋㅋ",
          "original_form": "OTT서비스 많이들 이용하죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:12"
        },
        {
          "id": "MDRW2100020758.1.4",
          "form": "드라마랑 예능이랑 이것저것 봐요",
          "original_form": "드라마랑 예능이랑 이것저것 봐요",
          "speaker_id": "2",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100020758.1.5",
          "form": "놀면 뭐하니 젤 챙겨보고있어요ㅋㅋ",
          "original_form": "놀면 뭐하니 젤 챙겨보고있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100020758.1.6",
          "form": "보시는 프로그램 있으세요??",
          "original_form": "보시는 프로그램 있으세요??",
          "speaker_id": "2",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100020758.1.7",
          "form": "저도  놀면   뭐하니  좋아해요",
          "original_form": "저도  놀면   뭐하니  좋아해요",
          "speaker_id": "1",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100020758.1.8",
          "form": "지난주 김연자랑",
          "original_form": "지난주 김연자랑",
          "speaker_id": "1",
          "time": "20210828 12:13"
        },
        {
          "id": "MDRW2100020758.1.9",
          "form": "손연재 선수 나올때",
          "original_form": "손연재 선수 나올때",
          "speaker_id": "1",
          "time": "20210828 12:14"
        },
        {
          "id": "MDRW2100020758.1.10",
          "form": "아니아니 탁구선수",
          "original_form": "아니아니 탁구선수",
          "speaker_id": "1",
          "time": "20210828 12:14"
        },
        {
          "id": "MDRW2100020758.1.11",
          "form": "신유빈",
          "original_form": "신유빈",
          "speaker_id": "1",
          "time": "20210828 12:14"
        },
        {
          "id": "MDRW2100020758.1.12",
          "form": "너무 재미있게 봤네요",
          "original_form": "너무 재미있게 봤네요",
          "speaker_id": "1",
          "time": "20210828 12:14"
        },
        {
          "id": "MDRW2100020758.1.13",
          "form": "맞아요ㅋㅋ 재밌었어요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ 재밌었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:14"
        },
        {
          "id": "MDRW2100020758.1.14",
          "form": "오랜만에 무도 멤버들도 나와서 반갑더라구요",
          "original_form": "오랜만에 무도 멤버들도 나와서 반갑더라구요",
          "speaker_id": "2",
          "time": "20210828 12:14"
        },
        {
          "id": "MDRW2100020758.1.15",
          "form": "유재석씨랑 다들  호흡도 잘 맞고",
          "original_form": "유재석씨랑 다들  호흡도 잘 맞고",
          "speaker_id": "1",
          "time": "20210828 12:15"
        },
        {
          "id": "MDRW2100020758.1.16",
          "form": "참 반갑고 재미있었어요",
          "original_form": "참 반갑고 재미있었어요",
          "speaker_id": "1",
          "time": "20210828 12:15"
        },
        {
          "id": "MDRW2100020758.1.17",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:15"
        },
        {
          "id": "MDRW2100020758.1.18",
          "form": "드라마 보시는거 있으세요??",
          "original_form": "드라마 보시는거 있으세요??",
          "speaker_id": "2",
          "time": "20210828 12:15"
        },
        {
          "id": "MDRW2100020758.1.19",
          "form": "최근에  악마판사를   정말",
          "original_form": "최근에  악마판사를   정말",
          "speaker_id": "1",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100020758.1.20",
          "form": "재미있게  봤어요",
          "original_form": "재미있게  봤어요",
          "speaker_id": "1",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100020758.1.21",
          "form": "아 재밌었다구 하더라고요",
          "original_form": "아 재밌었다구 하더라고요",
          "speaker_id": "2",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100020758.1.22",
          "form": "지성 연기 잘하죠",
          "original_form": "지성 연기 잘하죠",
          "speaker_id": "2",
          "time": "20210828 12:16"
        },
        {
          "id": "MDRW2100020758.1.23",
          "form": "정말   최고의 연기자라고 생각해요",
          "original_form": "정말   최고의 연기자라고 생각해요",
          "speaker_id": "1",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020758.1.24",
          "form": "인물도  어쩜  그리  훤칠하니",
          "original_form": "인물도  어쩜  그리  훤칠하니",
          "speaker_id": "1",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020758.1.25",
          "form": "잘생겨서 생각도  반듯하고",
          "original_form": "잘생겨서 생각도  반듯하고",
          "speaker_id": "1",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020758.1.26",
          "form": "맞아요 좋은사람 같아요",
          "original_form": "맞아요 좋은사람 같아요",
          "speaker_id": "2",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020758.1.27",
          "form": "본업도 잘하고ㅋㅋ",
          "original_form": "본업도 잘하고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:17"
        },
        {
          "id": "MDRW2100020758.1.28",
          "form": "이보영도 연기잘해서 부부가 한드라마 같이나오는거 보기싶기도 해요ㅋㅋ",
          "original_form": "이보영도 연기잘해서 부부가 한드라마 같이나오는거 보기싶기도 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100020758.1.29",
          "form": "아유 그렇죠 두 사람이 참 잘 어울려요",
          "original_form": "아유 그렇죠 두 사람이 참 잘 어울려요",
          "speaker_id": "1",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100020758.1.30",
          "form": "너무 잘 만났어",
          "original_form": "너무 잘 만났어",
          "speaker_id": "1",
          "time": "20210828 12:18"
        },
        {
          "id": "MDRW2100020758.1.31",
          "form": "맞아요 너무 이쁜 커플이에요",
          "original_form": "맞아요 너무 이쁜 커플이에요",
          "speaker_id": "2",
          "time": "20210828 12:19"
        },
        {
          "id": "MDRW2100020758.1.32",
          "form": "유튜브에서 보시는거 있으세요??",
          "original_form": "유튜브에서 보시는거 있으세요??",
          "speaker_id": "2",
          "time": "20210828 12:19"
        },
        {
          "id": "MDRW2100020758.1.33",
          "form": "삼프로티비 한번씩 들어요",
          "original_form": "삼프로티비 한번씩 들어요",
          "speaker_id": "1",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100020758.1.34",
          "form": "삼프로티비는 뭐에요??",
          "original_form": "삼프로티비는 뭐에요??",
          "speaker_id": "2",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100020758.1.35",
          "form": "어떤 컨테츠인지..??",
          "original_form": "어떤 컨테츠인지..??",
          "speaker_id": "2",
          "time": "20210828 12:20"
        },
        {
          "id": "MDRW2100020758.1.36",
          "form": "주식 전문가들이",
          "original_form": "주식 전문가들이",
          "speaker_id": "1",
          "time": "20210828 12:24"
        },
        {
          "id": "MDRW2100020758.1.37",
          "form": "매일 분석해주는데",
          "original_form": "매일 분석해주는데",
          "speaker_id": "1",
          "time": "20210828 12:24"
        },
        {
          "id": "MDRW2100020758.1.38",
          "form": "유용한 정보가 많아요",
          "original_form": "유용한 정보가 많아요",
          "speaker_id": "1",
          "time": "20210828 12:24"
        }
      ]
    }
