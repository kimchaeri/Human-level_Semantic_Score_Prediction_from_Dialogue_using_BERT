      "utterance": [
        {
          "id": "MDRW2100040113.1.1",
          "form": "그거 활캐 쓰면",
          "original_form": "그거 활캐 쓰면",
          "speaker_id": "2",
          "time": "20210904 10:25"
        },
        {
          "id": "MDRW2100040113.1.2",
          "form": "좀 쉬워",
          "original_form": "좀 쉬워",
          "speaker_id": "2",
          "time": "20210904 10:25"
        },
        {
          "id": "MDRW2100040113.1.3",
          "form": "그래서 나",
          "original_form": "그래서 나",
          "speaker_id": "1",
          "time": "20210904 10:25"
        },
        {
          "id": "MDRW2100040113.1.4",
          "form": "2.1에서 에일로이 받으면",
          "original_form": "2.1에서 에일로이 받으면",
          "speaker_id": "1",
          "time": "20210904 10:25"
        },
        {
          "id": "MDRW2100040113.1.5",
          "form": "타탈 바로 잡아주려고",
          "original_form": "타탈 바로 잡아주려고",
          "speaker_id": "1",
          "time": "20210904 10:25"
        },
        {
          "id": "MDRW2100040113.1.6",
          "form": "멀리 이동해도 몇발짝만 쫒아가면 공격가능하니까",
          "original_form": "멀리 이동해도 몇발짝만 쫒아가면 공격가능하니까",
          "speaker_id": "2",
          "time": "20210904 10:25"
        },
        {
          "id": "MDRW2100040113.1.7",
          "form": "근데 엠버말고는",
          "original_form": "근데 엠버말고는",
          "speaker_id": "1",
          "time": "20210904 10:26"
        },
        {
          "id": "MDRW2100040113.1.8",
          "form": "다 오토타겟팅 되나?",
          "original_form": "다 오토타겟팅 되나?",
          "speaker_id": "1",
          "time": "20210904 10:26"
        },
        {
          "id": "MDRW2100040113.1.9",
          "form": "요이미야는 되는거같긴 한데...",
          "original_form": "요이미야는 되는거같긴 한데...",
          "speaker_id": "1",
          "time": "20210904 10:26"
        },
        {
          "id": "MDRW2100040113.1.10",
          "form": "일단 피슬은 가능이야",
          "original_form": "일단 피슬은 가능이야",
          "speaker_id": "2",
          "time": "20210904 10:26"
        },
        {
          "id": "MDRW2100040113.1.11",
          "form": "왜 엠버만 안되는걸까",
          "original_form": "왜 엠버만 안되는걸까",
          "speaker_id": "2",
          "time": "20210904 10:26"
        },
        {
          "id": "MDRW2100040113.1.12",
          "form": "ㅇㅎ 그럼 5성캐들은 다 되겠네",
          "original_form": "ㅇㅎ 그럼 5성캐들은 다 되겠네",
          "speaker_id": "1",
          "time": "20210904 10:26"
        },
        {
          "id": "MDRW2100040113.1.13",
          "form": "걔도 그래도 4성인데....",
          "original_form": "걔도 그래도 4성인데....",
          "speaker_id": "2",
          "time": "20210904 10:26"
        },
        {
          "id": "MDRW2100040113.1.14",
          "form": "근데 활 오토타겟팅",
          "original_form": "근데 활 오토타겟팅",
          "speaker_id": "2",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100040113.1.15",
          "form": "거리 멀어지면 가끔",
          "original_form": "거리 멀어지면 가끔",
          "speaker_id": "2",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100040113.1.16",
          "form": "지 맘대로 쏴",
          "original_form": "지 맘대로 쏴",
          "speaker_id": "2",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100040113.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ그럴거같아",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그럴거같아",
          "speaker_id": "1",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100040113.1.18",
          "form": "엠버는 기본지급이라..ㅎ...",
          "original_form": "엠버는 기본지급이라..ㅎ...",
          "speaker_id": "1",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100040113.1.19",
          "form": "아니 그래도",
          "original_form": "아니 그래도",
          "speaker_id": "2",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100040113.1.20",
          "form": "스킬도 다 구린데",
          "original_form": "스킬도 다 구린데",
          "speaker_id": "2",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100040113.1.21",
          "form": "평타라도 좀 쏘게 해주지.....",
          "original_form": "평타라도 좀 쏘게 해주지.....",
          "speaker_id": "2",
          "time": "20210904 10:27"
        },
        {
          "id": "MDRW2100040113.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 여태까지는",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 여태까지는",
          "speaker_id": "1",
          "time": "20210904 10:28"
        },
        {
          "id": "MDRW2100040113.1.23",
          "form": "전무했던 불화살캐의 희소성으로",
          "original_form": "전무했던 불화살캐의 희소성으로",
          "speaker_id": "1",
          "time": "20210904 10:28"
        },
        {
          "id": "MDRW2100040113.1.24",
          "form": "그 책임을 다한듯...",
          "original_form": "그 책임을 다한듯...",
          "speaker_id": "1",
          "time": "20210904 10:28"
        },
        {
          "id": "MDRW2100040113.1.25",
          "form": "애잔...",
          "original_form": "애잔...",
          "speaker_id": "2",
          "time": "20210904 10:28"
        },
        {
          "id": "MDRW2100040113.1.26",
          "form": "근데 요이미야는",
          "original_form": "근데 요이미야는",
          "speaker_id": "1",
          "time": "20210904 10:29"
        },
        {
          "id": "MDRW2100040113.1.27",
          "form": "불화살 5성치고는",
          "original_form": "불화살 5성치고는",
          "speaker_id": "1",
          "time": "20210904 10:29"
        },
        {
          "id": "MDRW2100040113.1.28",
          "form": "평이 그닥이더라",
          "original_form": "평이 그닥이더라",
          "speaker_id": "1",
          "time": "20210904 10:29"
        },
        {
          "id": "MDRW2100040113.1.29",
          "form": "근데 그럴만 한게",
          "original_form": "근데 그럴만 한게",
          "speaker_id": "2",
          "time": "20210904 10:29"
        },
        {
          "id": "MDRW2100040113.1.30",
          "form": "해란귀들이 던지는 폭약통보다",
          "original_form": "해란귀들이 던지는 폭약통보다",
          "speaker_id": "2",
          "time": "20210904 10:29"
        },
        {
          "id": "MDRW2100040113.1.31",
          "form": "별로잖아",
          "original_form": "별로잖아",
          "speaker_id": "2",
          "time": "20210904 10:29"
        },
        {
          "id": "MDRW2100040113.1.32",
          "form": "스킬이",
          "original_form": "스킬이",
          "speaker_id": "2",
          "time": "20210904 10:29"
        },
        {
          "id": "MDRW2100040113.1.33",
          "form": "ㅇㅇ 그건 폭탄이라 임팩트라도 있지",
          "original_form": "ㅇㅇ 그건 폭탄이라 임팩트라도 있지",
          "speaker_id": "1",
          "time": "20210904 10:31"
        },
        {
          "id": "MDRW2100040113.1.34",
          "form": "그 폭약도 사실 걔가 판거 아니야?",
          "original_form": "그 폭약도 사실 걔가 판거 아니야?",
          "speaker_id": "2",
          "time": "20210904 10:32"
        },
        {
          "id": "MDRW2100040113.1.35",
          "form": "폭죽가게잔아",
          "original_form": "폭죽가게잔아",
          "speaker_id": "2",
          "time": "20210904 10:32"
        },
        {
          "id": "MDRW2100040113.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그럴..지도...?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그럴..지도...?",
          "speaker_id": "1",
          "time": "20210904 10:52"
        },
        {
          "id": "MDRW2100040113.1.37",
          "form": "나름 유서깊은 폭죽가게라는 설정이니까..",
          "original_form": "나름 유서깊은 폭죽가게라는 설정이니까..",
          "speaker_id": "1",
          "time": "20210904 10:53"
        },
        {
          "id": "MDRW2100040113.1.38",
          "form": "그닉가...",
          "original_form": "그닉가...",
          "speaker_id": "2",
          "time": "20210904 10:53"
        }
      ]
    }
  ]
