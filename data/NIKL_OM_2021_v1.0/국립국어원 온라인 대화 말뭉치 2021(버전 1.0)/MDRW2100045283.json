        {
          "id": "MDRW2100045283.1.1",
          "form": "가족은 몇 명 있으세요",
          "original_form": "가족은 몇 명 있으세요",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045283.1.2",
          "form": "저는 혼자 자취하고 있습니다",
          "original_form": "저는 혼자 자취하고 있습니다",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045283.1.3",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045283.1.4",
          "form": "외롭지 않으신가요",
          "original_form": "외롭지 않으신가요",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045283.1.5",
          "form": "저는 고양이 두 마리가 있어서",
          "original_form": "저는 고양이 두 마리가 있어서",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045283.1.6",
          "form": "완전 부럽네요",
          "original_form": "완전 부럽네요",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045283.1.7",
          "form": "안 외로우신가요",
          "original_form": "안 외로우신가요",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045283.1.8",
          "form": "저는 맨날 친구 불러요",
          "original_form": "저는 맨날 친구 불러요",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045283.1.9",
          "form": "것도 좋죠",
          "original_form": "것도 좋죠",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045283.1.10",
          "form": "고양이 키우기 안 힘드나요",
          "original_form": "고양이 키우기 안 힘드나요",
          "speaker_id": "1",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045283.1.11",
          "form": "개보다 낫다는",
          "original_form": "개보다 낫다는",
