      "utterance": [
        {
          "id": "MDRW2100025951.1.1",
          "form": "안녕하세요 name2님",
          "original_form": "안녕하세요 &name2&님",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025951.1.2",
          "form": "동생 결혼 하신다구용",
          "original_form": "동생 결혼 하신다구용",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025951.1.3",
          "form": "오 맞아요!!",
          "original_form": "오 맞아요!!",
          "speaker_id": "1",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100025951.1.4",
          "form": "곧결혼해요 ㅠㅠㅎㅎ",
          "original_form": "곧결혼해요 ㅠㅠㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025951.1.5",
          "form": "어느예식장에서 하나용",
          "original_form": "어느예식장에서 하나용",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025951.1.6",
          "form": "노비아갈라 전자관점에서 해요 ㅎㅎ",
          "original_form": "노비아갈라 전자관점에서 해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025951.1.7",
          "form": "name2님 결혼하셨나요?ㅎㅎ",
          "original_form": "&name2&님 결혼하셨나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100025951.1.8",
          "form": "넵 저도 했어용 ㅎㅎㅎ name1님은용",
          "original_form": "넵 저도 했어용 ㅎㅎㅎ &name1&님은용",
          "speaker_id": "2",
          "time": "20210901 18:20"
        },
        {
          "id": "MDRW2100025951.1.9",
          "form": "저두 했어요!! 결혼하신지 얼마나 되셨나요??",
          "original_form": "저두 했어요!! 결혼하신지 얼마나 되셨나요??",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100025951.1.10",
          "form": "이제 1년반이용 ㅎㅎ",
          "original_form": "이제 1년반이용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100025951.1.11",
          "form": "혼인신고언제하실거용",
          "original_form": "혼인신고언제하실거용",
          "speaker_id": "2",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100025951.1.12",
          "form": "오오 신혼이네요! 저도 이제곧 1년! 힛",
          "original_form": "오오 신혼이네요! 저도 이제곧 1년! 힛",
          "speaker_id": "1",
          "time": "20210901 18:22"
        },
        {
          "id": "MDRW2100025951.1.13",
          "form": "혼인신고...아직 계획은 없어용",
          "original_form": "혼인신고...아직 계획은 없어용",
          "speaker_id": "1",
          "time": "20210901 18:23"
        },
        {
          "id": "MDRW2100025951.1.14",
          "form": "늦게해도 괜찮은 것겉아요 ㅎㅎ",
          "original_form": "늦게해도 괜찮은 것겉아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100025951.1.15",
          "form": "결혼생활은 어때용",
          "original_form": "결혼생활은 어때용",
          "speaker_id": "2",
          "time": "20210901 18:25"
        },
        {
          "id": "MDRW2100025951.1.16",
          "form": "처음에는 어색하고 적응이 안되었는데 지금은 괜찮아요.ㅎㅎ 코로나 시국에 결혼해서 우울하긴했지만 빨리잘한거같아요 ㅎㅎ",
          "original_form": "처음에는 어색하고 적응이 안되었는데 지금은 괜찮아요.ㅎㅎ 코로나 시국에 결혼해서 우울하긴했지만 빨리잘한거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100025951.1.17",
          "form": "맞아요 지금 했으면 더 우울했을 것 같아요",
          "original_form": "맞아요 지금 했으면 더 우울했을 것 같아요",
          "speaker_id": "2",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100025951.1.18",
          "form": "남편은 잘해주나요",
          "original_form": "남편은 잘해주나요",
          "speaker_id": "2",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100025951.1.19",
          "form": "남편...ㅋㅋ연애때랑 똑같...한결같네욥",
          "original_form": "남편...ㅋㅋ연애때랑 똑같...한결같네욥",
          "speaker_id": "1",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100025951.1.20",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100025951.1.21",
          "form": "전 아들키우는 느낌이네요 ㅋㅋㅋㅋ",
          "original_form": "전 아들키우는 느낌이네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025951.1.22",
          "form": "오 ㅋㅋㅋ전 시어머니..모시는느낌이네요ㅜㅜ",
          "original_form": "오 ㅋㅋㅋ전 시어머니..모시는느낌이네요ㅜㅜ",
