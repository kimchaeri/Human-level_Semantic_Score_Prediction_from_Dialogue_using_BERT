      "utterance": [
        {
          "id": "MDRW2100018835.1.1",
          "form": "재택근무 하시나요?",
          "original_form": "재택근무 하시나요?",
          "speaker_id": "1",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018835.1.2",
          "form": "재택휴식중이에요.. 백수라..",
          "original_form": "재택휴식중이에요.. 백수라..",
          "speaker_id": "2",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018835.1.3",
          "form": "지금 재택근무?",
          "original_form": "지금 재택근무?",
          "speaker_id": "2",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018835.1.4",
          "form": "재택휴식 귀여워요 ㅋㅋㅋ",
          "original_form": "재택휴식 귀여워요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018835.1.5",
          "form": "아니요 지금은 대면근무에요ㅠ",
          "original_form": "아니요 지금은 대면근무에요ㅠ",
          "speaker_id": "1",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018835.1.6",
          "form": "재택근무도 뭔가 부서원들끼리 눈치싸움이라 피곤해요",
          "original_form": "재택근무도 뭔가 부서원들끼리 눈치싸움이라 피곤해요",
          "speaker_id": "1",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018835.1.7",
          "form": "돌아가면서 하시는거에요?",
          "original_form": "돌아가면서 하시는거에요?",
          "speaker_id": "2",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018835.1.8",
          "form": "네네 근무조를 짜요",
          "original_form": "네네 근무조를 짜요",
          "speaker_id": "1",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100018835.1.9",
          "form": "다들 거의 재택은 휴가개념으로 생각하능거같아요",
          "original_form": "다들 거의 재택은 휴가개념으로 생각하능거같아요",
          "speaker_id": "1",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100018835.1.10",
          "form": "엄연히 일하능건데",
          "original_form": "엄연히 일하능건데",
          "speaker_id": "1",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100018835.1.11",
          "form": "ㅋㅋㅋㅋ 대면 업무인데 재택도 가능해요?",
          "original_form": "ㅋㅋㅋㅋ 대면 업무인데 재택도 가능해요?",
          "speaker_id": "2",
          "time": "20210827 12:44"
        },
        {
          "id": "MDRW2100018835.1.12",
          "form": "아 대면근무 재택근무 혼합이에요",
          "original_form": "아 대면근무 재택근무 혼합이에요",
          "speaker_id": "1",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018835.1.13",
          "form": "재택노트북 들고 들어가서 그걸로 업무해요",
          "original_form": "재택노트북 들고 들어가서 그걸로 업무해요",
          "speaker_id": "1",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018835.1.14",
          "form": "회의는 줌으로",
          "original_form": "회의는 줌으로",
          "speaker_id": "1",
          "time": "20210827 12:45"
        },
        {
          "id": "MDRW2100018835.1.15",
          "form": "근데 재택해보면.. 굳이 회사나가야하는 생각들지 않나여?",
          "original_form": "근데 재택해보면.. 굳이 회사나가야하는 생각들지 않나여?",
          "speaker_id": "2",
          "time": "20210827 12:47"
        },
        {
          "id": "MDRW2100018835.1.16",
          "form": "정말그래요",
          "original_form": "정말그래요",
          "speaker_id": "1",
          "time": "20210827 12:48"
        },
        {
          "id": "MDRW2100018835.1.17",
          "form": "굳이 사람들과의 피곤한 관계..기싸움도 없고",
          "original_form": "굳이 사람들과의 피곤한 관계..기싸움도 없고",
          "speaker_id": "1",
          "time": "20210827 12:48"
        },
        {
          "id": "MDRW2100018835.1.18",
          "form": "정말 온리 일만 하면되니 ...",
          "original_form": "정말 온리 일만 하면되니 ...",
          "speaker_id": "1",
          "time": "20210827 12:48"
        },
        {
          "id": "MDRW2100018835.1.19",
          "form": "코로나 끝나도 많이 변할거 같아요..",
          "original_form": "코로나 끝나도 많이 변할거 같아요..",
          "speaker_id": "2",
          "time": "20210827 12:49"
        },
        {
          "id": "MDRW2100018835.1.20",
          "form": "사장들이... 아.. 사람 없어도 돌아가네..라는 인식 가지기 시작한듯..",
          "original_form": "사장들이... 아.. 사람 없어도 돌아가네..라는 인식 가지기 시작한듯..",
          "speaker_id": "2",
          "time": "20210827 12:50"
        },
        {
          "id": "MDRW2100018835.1.21",
          "form": "그쵸 ㅋㅋㅋ돌아가긴 돌아가니",
          "original_form": "그쵸 ㅋㅋㅋ돌아가긴 돌아가니",
          "speaker_id": "1",
          "time": "20210827 12:50"
        },
        {
          "id": "MDRW2100018835.1.22",
          "form": "재택근무하면 회사에서 따로 감시하나여?",
          "original_form": "재택근무하면 회사에서 따로 감시하나여?",
          "speaker_id": "2",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100018835.1.23",
          "form": "컴터가동 시간 체크한다거나..",
          "original_form": "컴터가동 시간 체크한다거나..",
          "speaker_id": "2",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100018835.1.24",
          "form": "업무포털 접속 시간이랑 카톡 근무보고를 체크해요",
          "original_form": "업무포털 접속 시간이랑 카톡 근무보고를 체크해요",
