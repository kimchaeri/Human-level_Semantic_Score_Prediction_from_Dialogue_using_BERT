      "utterance": [
        {
          "id": "MDRW2100010402.1.1",
          "form": "주제바꾸셔도 됩니당",
          "original_form": "주제바꾸셔도 됩니당",
          "speaker_id": "1",
          "time": "20210823 17:30"
        },
        {
          "id": "MDRW2100010402.1.2",
          "form": "아침고요수목원 저도 갔었는데",
          "original_form": "아침고요수목원 저도 갔었는데",
          "speaker_id": "2",
          "time": "20210823 17:32"
        },
        {
          "id": "MDRW2100010402.1.3",
          "form": "좋았겠어요",
          "original_form": "좋았겠어요",
          "speaker_id": "2",
          "time": "20210823 17:32"
        },
        {
          "id": "MDRW2100010402.1.4",
          "form": "사람은 많던가요?",
          "original_form": "사람은 많던가요?",
          "speaker_id": "2",
          "time": "20210823 17:34"
        },
        {
          "id": "MDRW2100010402.1.5",
          "form": "아무래도 코로나땜에 전에갔을때보다 적었어요ㅠ",
          "original_form": "아무래도 코로나땜에 전에갔을때보다 적었어요ㅠ",
          "speaker_id": "1",
          "time": "20210823 17:36"
        },
        {
          "id": "MDRW2100010402.1.6",
          "form": "제주도 가고싶었는데 갑자기 4단계 격상돼서 알아보려다 포기했어요",
          "original_form": "제주도 가고싶었는데 갑자기 4단계 격상돼서 알아보려다 포기했어요",
          "speaker_id": "2",
          "time": "20210823 17:37"
        },
        {
          "id": "MDRW2100010402.1.7",
          "form": "갑자기 격상돼서ㅠ 제주도는 성산일출봉이 좋아요",
          "original_form": "갑자기 격상돼서ㅠ 제주도는 성산일출봉이 좋아요",
          "speaker_id": "1",
          "time": "20210823 17:38"
        },
        {
          "id": "MDRW2100010402.1.8",
          "form": "멤버모집 실패로ㅋ 6월에  갈려다 못갔어요",
          "original_form": "멤버모집 실패로ㅋ 6월에  갈려다 못갔어요",
          "speaker_id": "2",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100010402.1.9",
          "form": "올해는 여행 틀린듯요",
          "original_form": "올해는 여행 틀린듯요",
          "speaker_id": "2",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100010402.1.10",
          "form": "멀리가려면 모이기가 힘들죠",
          "original_form": "멀리가려면 모이기가 힘들죠",
          "speaker_id": "1",
          "time": "20210823 17:44"
        },
        {
          "id": "MDRW2100010402.1.11",
          "form": "특히 코로나땜에ㅠ",
          "original_form": "특히 코로나땜에ㅠ",
          "speaker_id": "1",
          "time": "20210823 17:45"
        },
        {
          "id": "MDRW2100010402.1.12",
          "form": "맞아요 운전도 할줄알아야되는데",
          "original_form": "맞아요 운전도 할줄알아야되는데",
          "speaker_id": "2",
          "time": "20210823 17:45"
        },
        {
          "id": "MDRW2100010402.1.13",
          "form": "다들 장롱면허라ㅋ",
          "original_form": "다들 장롱면허라ㅋ",
          "speaker_id": "2",
          "time": "20210823 17:45"
        },
        {
          "id": "MDRW2100010402.1.14",
          "form": "ㅋㅋㅋㅋ운전진짜 쉽지않아요",
          "original_form": "ㅋㅋㅋㅋ운전진짜 쉽지않아요",
          "speaker_id": "1",
          "time": "20210823 17:49"
        },
        {
          "id": "MDRW2100010402.1.15",
          "form": "그니까요 피곤하고 보조석타는게 젤 최고인데",
          "original_form": "그니까요 피곤하고 보조석타는게 젤 최고인데",
          "speaker_id": "2",
          "time": "20210823 17:50"
        },
        {
          "id": "MDRW2100010402.1.16",
          "form": "얼른코로나 종식돼서 친구차타고 놀러가고싶어요ㅋㅋ",
          "original_form": "얼른코로나 종식돼서 친구차타고 놀러가고싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:57"
        },
        {
          "id": "MDRW2100010402.1.17",
          "form": "저도요ㅎ 작년부터 여행 스톱이라 답답해죽겠어요",
          "original_form": "저도요ㅎ 작년부터 여행 스톱이라 답답해죽겠어요",
          "speaker_id": "2",
          "time": "20210823 17:57"
        },
        {
          "id": "MDRW2100010402.1.18",
          "form": "맞아요 해외여행도 가고싶은데 못가고..!",
          "original_form": "맞아요 해외여행도 가고싶은데 못가고..!",
