      "utterance": [
        {
          "id": "MDRW2100022439.1.1",
          "form": "맛집조아하시나요",
          "original_form": "맛집조아하시나요",
          "speaker_id": "1",
          "time": "20210829 14:02"
        },
        {
          "id": "MDRW2100022439.1.2",
          "form": "좋아해요!",
          "original_form": "좋아해요!",
          "speaker_id": "2",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100022439.1.3",
          "form": "어떤메뉴요?",
          "original_form": "어떤메뉴요?",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100022439.1.4",
          "form": "대부분 다 좋아해요",
          "original_form": "대부분 다 좋아해요",
          "speaker_id": "2",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100022439.1.5",
          "form": "막 홍어 이런거 아닌이상 한식 양식 등등",
          "original_form": "막 홍어 이런거 아닌이상 한식 양식 등등",
          "speaker_id": "2",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100022439.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100022439.1.7",
          "form": "요즘 마라탕 많이 먹는데",
          "original_form": "요즘 마라탕 많이 먹는데",
          "speaker_id": "2",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100022439.1.8",
          "form": "전 마라탕 먹으면 속이 안좋더라구요ㅜ",
          "original_form": "전 마라탕 먹으면 속이 안좋더라구요ㅜ",
          "speaker_id": "2",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100022439.1.9",
          "form": "저도 마라탕 좋아하는데ㅜ",
          "original_form": "저도 마라탕 좋아하는데ㅜ",
          "speaker_id": "1",
          "time": "20210829 14:16"
        },
        {
          "id": "MDRW2100022439.1.10",
          "form": "마라탕 주위에 인기많죠",
          "original_form": "마라탕 주위에 인기많죠",
          "speaker_id": "2",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100022439.1.11",
          "form": "동네에도 많이 생겼더라구요",
          "original_form": "동네에도 많이 생겼더라구요",
          "speaker_id": "2",
          "time": "20210829 14:18"
        },
        {
          "id": "MDRW2100022439.1.12",
          "form": "마라샹궈도 좋아해요",
          "original_form": "마라샹궈도 좋아해요",
          "speaker_id": "1",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022439.1.13",
          "form": "마라샹궈 잘하는 집 있다고 데려가려하던데",
          "original_form": "마라샹궈 잘하는 집 있다고 데려가려하던데",
          "speaker_id": "2",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022439.1.14",
          "form": "많이 자극적이진않나요?",
          "original_form": "많이 자극적이진않나요?",
          "speaker_id": "2",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022439.1.15",
          "form": "궁금해여",
          "original_form": "궁금해여",
          "speaker_id": "2",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022439.1.16",
          "form": "네네 넘 맛있어요ㅠㅜ",
          "original_form": "네네 넘 맛있어요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210829 14:26"
        },
        {
          "id": "MDRW2100022439.1.17",
          "form": "오 그럼 나중에 한번",
          "original_form": "오 그럼 나중에 한번",
          "speaker_id": "2",
          "time": "20210829 14:30"
        },
        {
          "id": "MDRW2100022439.1.18",
          "form": "들려볼게요",
          "original_form": "들려볼게요",
          "speaker_id": "2",
          "time": "20210829 14:30"
        },
        {
          "id": "MDRW2100022439.1.19",
          "form": "마라탕도 좋아하시니까~",
          "original_form": "마라탕도 좋아하시니까~",
          "speaker_id": "1",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100022439.1.20",
          "form": "아 저 마라탕 속 안좋아서 잘 못먹어요ㅜ",
          "original_form": "아 저 마라탕 속 안좋아서 잘 못먹어요ㅜ",
          "speaker_id": "2",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100022439.1.21",
          "form": "맛은 괜찮은데",
          "original_form": "맛은 괜찮은데",
          "speaker_id": "2",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100022439.1.22",
          "form": "위랑 장이 개복치라",
          "original_form": "위랑 장이 개복치라",
          "speaker_id": "2",
          "time": "20210829 14:32"
        },
        {
          "id": "MDRW2100022439.1.23",
          "form": "아아 그럼 자극적인거 조심하셔야겠네요",
          "original_form": "아아 그럼 자극적인거 조심하셔야겠네요",
          "speaker_id": "1",
          "time": "20210829 14:33"
        }
      ]
