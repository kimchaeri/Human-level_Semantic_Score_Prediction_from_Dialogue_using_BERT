      "utterance": [
        {
          "id": "MDRW2100032048.1.1",
          "form": "오늘도 집안일 하긴 틀렸어 ㅠ",
          "original_form": "오늘도 집안일 하긴 틀렸어 ㅠ",
          "speaker_id": "2",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100032048.1.2",
          "form": "힘내서 해야지",
          "original_form": "힘내서 해야지",
          "speaker_id": "1",
          "time": "20210905 11:54"
        },
        {
          "id": "MDRW2100032048.1.3",
          "form": "그날에 비오고 먹을 것도 없어",
          "original_form": "그날에 비오고 먹을 것도 없어",
          "speaker_id": "2",
          "time": "20210905 15:49"
        },
        {
          "id": "MDRW2100032048.1.4",
          "form": "집안일 어떻게 해?",
          "original_form": "집안일 어떻게 해?",
          "speaker_id": "2",
          "time": "20210905 15:49"
        },
        {
          "id": "MDRW2100032048.1.5",
          "form": "힘없으니까 내가 할게",
          "original_form": "힘없으니까 내가 할게",
          "speaker_id": "1",
          "time": "20210905 15:51"
        },
        {
          "id": "MDRW2100032048.1.6",
          "form": "나도 해야는데... 막 눕고 싶어져",
          "original_form": "나도 해야는데... 막 눕고 싶어져",
          "speaker_id": "2",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100032048.1.7",
          "form": "형광등도 버려야는데 택배상자도 정리하고",
          "original_form": "형광등도 버려야는데 택배상자도 정리하고",
          "speaker_id": "2",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100032048.1.8",
          "form": "내일부터 집안일 하면 돼 이틀까진 푹 쉬어",
          "original_form": "내일부터 집안일 하면 돼 이틀까진 푹 쉬어",
          "speaker_id": "1",
          "time": "20210905 19:41"
        },
        {
          "id": "MDRW2100032048.1.9",
          "form": "낼부턴 잘 할게 ㅎㅎ",
          "original_form": "낼부턴 잘 할게 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 19:47"
        },
        {
          "id": "MDRW2100032048.1.10",
          "form": "같이 잘 하자 집안일 혼자하면 힘들어",
          "original_form": "같이 잘 하자 집안일 혼자하면 힘들어",
          "speaker_id": "1",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100032048.1.11",
          "form": "일단 버리기부터 하자",
          "original_form": "일단 버리기부터 하자",
          "speaker_id": "2",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100032048.1.12",
          "form": "쓰레기봉투 준비해야겠네 일반 봉투랑",
          "original_form": "쓰레기봉투 준비해야겠네 일반 봉투랑",
          "speaker_id": "1",
          "time": "20210905 19:51"
        },
        {
          "id": "MDRW2100032048.1.13",
          "form": "역시나 별로 못했어",
          "original_form": "역시나 별로 못했어",
          "speaker_id": "2",
          "time": "20210907 07:36"
        },
        {
          "id": "MDRW2100032048.1.14",
          "form": "오늘 컨디션 어때?",
          "original_form": "오늘 컨디션 어때?",
          "speaker_id": "1",
          "time": "20210907 07:37"
        },
        {
          "id": "MDRW2100032048.1.15",
          "form": "괜찮아 오늘부터 다시 정말로 집안일 하자",
          "original_form": "괜찮아 오늘부터 다시 정말로 집안일 하자",
          "speaker_id": "2",
          "time": "20210907 07:39"
        },
        {
          "id": "MDRW2100032048.1.16",
          "form": "오케이 집안일은 미루면 안돼",
          "original_form": "오케이 집안일은 미루면 안돼",
          "speaker_id": "1",
          "time": "20210907 07:40"
        }
