      "utterance": [
        {
          "id": "MDRW2100020289.1.1",
          "form": "저녁은 드셨나요?",
          "original_form": "저녁은 드셨나요?",
          "speaker_id": "1",
          "time": "20210828 01:01"
        },
        {
          "id": "MDRW2100020289.1.2",
          "form": "헉 신기해요 변경이 되는군요",
          "original_form": "헉 신기해요 변경이 되는군요",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020289.1.3",
          "form": "저 야채곱창 하나에 알곱창하나 볶음밥까지",
          "original_form": "저 야채곱창 하나에 알곱창하나 볶음밥까지",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020289.1.4",
          "form": "야무지게 조졌어요",
          "original_form": "야무지게 조졌어요",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020289.1.5",
          "form": "name1은 뭐 드셨어요 ?",
          "original_form": "&name1&은 뭐 드셨어요 ?",
          "speaker_id": "2",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020289.1.6",
          "form": "와 잘 드셨네요!!",
          "original_form": "와 잘 드셨네요!!",
          "speaker_id": "1",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020289.1.7",
          "form": "저는 쭈삼불고기!",
          "original_form": "저는 쭈삼불고기!",
          "speaker_id": "1",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020289.1.8",
          "form": "불금엔 이런거 먹어줘야져",
          "original_form": "불금엔 이런거 먹어줘야져",
          "speaker_id": "1",
          "time": "20210828 01:02"
        },
        {
          "id": "MDRW2100020289.1.9",
          "form": "헐 저 내일 주꾸미 먹어요",
          "original_form": "헐 저 내일 주꾸미 먹어요",
          "speaker_id": "2",
          "time": "20210828 01:03"
        },
        {
          "id": "MDRW2100020289.1.10",
          "form": "가리는 음식 있으세요 ?",
          "original_form": "가리는 음식 있으세요 ?",
          "speaker_id": "2",
          "time": "20210828 01:03"
        },
        {
          "id": "MDRW2100020289.1.11",
          "form": "저는 없어요 ,,",
          "original_form": "저는 없어요 ,,",
          "speaker_id": "2",
          "time": "20210828 01:03"
        },
        {
          "id": "MDRW2100020289.1.12",
          "form": "아저는 편식이심해요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아저는 편식이심해요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020289.1.13",
          "form": "프로육식러랄까요...",
          "original_form": "프로육식러랄까요...",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020289.1.14",
          "form": "아 채소를 안좋아하세요 ?",
          "original_form": "아 채소를 안좋아하세요 ?",
          "speaker_id": "2",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020289.1.15",
          "form": "어 그런쪽이 아니라 음 두부, 콩 싫어하고 이런?",
          "original_form": "어 그런쪽이 아니라 음 두부, 콩 싫어하고 이런?",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020289.1.16",
          "form": "샐러드는 좋아해요!",
          "original_form": "샐러드는 좋아해요!",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020289.1.17",
          "form": "아 콩은 정말 정이 안가죠",
          "original_form": "아 콩은 정말 정이 안가죠",
          "speaker_id": "2",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020289.1.18",
          "form": "샐러디라고 샐러드가게 드셔보셨어요 ? 저세상맛이에요",
          "original_form": "샐러디라고 샐러드가게 드셔보셨어요 ? 저세상맛이에요",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020289.1.19",
          "form": "헐! 그거 동네에 있긴한데 아직 안 먹어봤어요",
          "original_form": "헐! 그거 동네에 있긴한데 아직 안 먹어봤어요",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020289.1.20",
          "form": "추천메뉴 혹시 있으신가요?",
          "original_form": "추천메뉴 혹시 있으신가요?",
          "speaker_id": "1",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020289.1.21",
          "form": "풀네임은 모르겠는데 우삼겹!!",
          "original_form": "풀네임은 모르겠는데 우삼겹!!",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020289.1.22",
          "form": "우삼겹이 들어간 샐러드가 있어요 존맛",
          "original_form": "우삼겹이 들어간 샐러드가 있어요 존맛",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020289.1.23",
          "form": "와.. 벌써 맛있다",
          "original_form": "와.. 벌써 맛있다",
          "speaker_id": "1",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020289.1.24",
          "form": "기억해둘게요!",
          "original_form": "기억해둘게요!",
          "speaker_id": "1",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020289.1.25",
          "form": "그래도 요즘 샐러드 전문점이 많이 생겼더라구요! 먹어보진 못했지만ㅋㅋ",
          "original_form": "그래도 요즘 샐러드 전문점이 많이 생겼더라구요! 먹어보진 못했지만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020289.1.26",
          "form": "그리고 요즘엔 로제떡볶이에 빠졌어요",
          "original_form": "그리고 요즘엔 로제떡볶이에 빠졌어요",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020289.1.27",
          "form": "로제떡볶이도 유행이죠ㅋㅋㅋㅋ",
          "original_form": "로제떡볶이도 유행이죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:10"
        },
        {
          "id": "MDRW2100020289.1.28",
          "form": "여기저기 많이 드셔보셨어요?",
          "original_form": "여기저기 많이 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210828 01:10"
        }
      ]
