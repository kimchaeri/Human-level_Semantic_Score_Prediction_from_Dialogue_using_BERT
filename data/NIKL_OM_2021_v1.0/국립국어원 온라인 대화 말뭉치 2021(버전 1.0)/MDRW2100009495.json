      "utterance": [
        {
          "id": "MDRW2100009495.1.1",
          "form": "페인스크릭 만든 회사에서 새로 추리게임 또 낸대",
          "original_form": "페인스크릭 만든 회사에서 새로 추리게임 또 낸대",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009495.1.2",
          "form": "데모 버전 다운받아서 해 봤는데",
          "original_form": "데모 버전 다운받아서 해 봤는데",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009495.1.3",
          "form": "그것도 재밌더라",
          "original_form": "그것도 재밌더라",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009495.1.4",
          "form": "이 회사 게임은 믿고 플레이 해도 괜찮을 것 같아",
          "original_form": "이 회사 게임은 믿고 플레이 해도 괜찮을 것 같아",
          "speaker_id": "2",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009495.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210823 00:03"
        },
        {
          "id": "MDRW2100009495.1.6",
          "form": "보통 추리게임은",
          "original_form": "보통 추리게임은",
          "speaker_id": "1",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009495.1.7",
          "form": "살인사건 추리하는 거지?",
          "original_form": "살인사건 추리하는 거지?",
          "speaker_id": "1",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009495.1.8",
          "form": "응 그렇지",
          "original_form": "응 그렇지",
          "speaker_id": "2",
          "time": "20210823 00:04"
        },
        {
          "id": "MDRW2100009495.1.9",
          "form": "아, 추리게임의 탈을 쓴 공포게임 한 적도 있어",
          "original_form": "아, 추리게임의 탈을 쓴 공포게임 한 적도 있어",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009495.1.10",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009495.1.11",
          "form": "결국 추리 요소가 없지는 않았지만",
          "original_form": "결국 추리 요소가 없지는 않았지만",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009495.1.12",
          "form": "추리게임 그래서 은근 오싹해ㅠ",
          "original_form": "추리게임 그래서 은근 오싹해ㅠ",
          "speaker_id": "1",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009495.1.13",
          "form": "맞아...추리게임이면 은근히 공포 요소가 따라오나봐",
          "original_form": "맞아...추리게임이면 은근히 공포 요소가 따라오나봐",
          "speaker_id": "2",
          "time": "20210823 00:05"
        },
        {
          "id": "MDRW2100009495.1.14",
          "form": "방탈출도 은근 오싹하고",
          "original_form": "방탈출도 은근 오싹하고",
          "speaker_id": "1",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009495.1.15",
          "form": "이 게임은 stories untold",
          "original_form": "이 게임은 stories untold",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009495.1.16",
          "form": "방탈출하고싶다",
          "original_form": "방탈출하고싶다",
          "speaker_id": "1",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009495.1.17",
          "form": "(의식의 흐름)",
          "original_form": "(의식의 흐름)",
          "speaker_id": "1",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009495.1.18",
          "form": "방탈출 나도 하고싶다",
          "original_form": "방탈출 나도 하고싶다",
          "speaker_id": "2",
          "time": "20210823 00:06"
        },
        {
          "id": "MDRW2100009495.1.19",
          "form": "stories untold 는 어때?",
          "original_form": "stories untold 는 어때?",
          "speaker_id": "1",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009495.1.20",
          "form": "비록 대모이지만",
          "original_form": "비록 대모이지만",
          "speaker_id": "1",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009495.1.21",
          "form": "4개의 챕터로 구성되어 있는데",
          "original_form": "4개의 챕터로 구성되어 있는데",
          "speaker_id": "2",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009495.1.22",
          "form": "한 챕터마다 분위기나 진행방식이 완전히 달라",
          "original_form": "한 챕터마다 분위기나 진행방식이 완전히 달라",
          "speaker_id": "2",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009495.1.23",
          "form": "계속이어서 말해줘ㅎ",
          "original_form": "계속이어서 말해줘ㅎ",
          "speaker_id": "1",
          "time": "20210823 00:09"
        }
      ]
