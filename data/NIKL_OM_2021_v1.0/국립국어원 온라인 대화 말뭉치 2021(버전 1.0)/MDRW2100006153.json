      "utterance": [
        {
          "id": "MDRW2100006153.1.1",
          "form": "반려동물을 키우시나요?",
          "original_form": "반려동물을 키우시나요?",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100006153.1.2",
          "form": "아니요 ㅎㅎ name1님은?",
          "original_form": "아니요 ㅎㅎ &name1&님은?",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006153.1.3",
          "form": "저도 지금은 안키워요.. ㅎ",
          "original_form": "저도 지금은 안키워요.. ㅎ",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006153.1.4",
          "form": "그럼 개나 고양이를 좋아하시나요?",
          "original_form": "그럼 개나 고양이를 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100006153.1.5",
          "form": "아니..오...",
          "original_form": "아니..오...",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100006153.1.6",
          "form": "무서워요ㅋㅋ",
          "original_form": "무서워요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100006153.1.7",
          "form": "티비로 보는것도요?",
          "original_form": "티비로 보는것도요?",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006153.1.8",
          "form": "티비는 괜찮아요",
          "original_form": "티비는 괜찮아요",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006153.1.9",
          "form": "귀여운건 좋아합니다",
          "original_form": "귀여운건 좋아합니다",
          "speaker_id": "2",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100006153.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100006153.1.11",
          "form": "전 고양이 보는거 좋아해요",
          "original_form": "전 고양이 보는거 좋아해요",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100006153.1.12",
          "form": "유튜브에 매탈남이라는 분의 고양이를 아시나요?",
          "original_form": "유튜브에 매탈남이라는 분의 고양이를 아시나요?",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100006153.1.13",
          "form": "아니요 고양튜버신가요 ㅎㅎ",
          "original_form": "아니요 고양튜버신가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100006153.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100006153.1.15",
          "form": "ㅋㅋ아 저 알레르기 비염도 있어서 ㅎㅎ",
          "original_form": "ㅋㅋ아 저 알레르기 비염도 있어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006153.1.16",
          "form": "원래는 다를걸 알려주는 유튜버였는데 마당에 놀러온 길고양이랑 친구가 되고",
          "original_form": "원래는 다를걸 알려주는 유튜버였는데 마당에 놀러온 길고양이랑 친구가 되고",
          "speaker_id": "1",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100006153.1.17",
          "form": "그 길고양이가 위험한곳에 낳은 아깽이를 거두어 키우는 과정을",
          "original_form": "그 길고양이가 위험한곳에 낳은 아깽이를 거두어 키우는 과정을",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100006153.1.18",
          "form": "고양이 알못인 유튜버가 실시간으로 시청자에게 도움을 청하면서 고양이 유튜버가 됐는데요",
          "original_form": "고양이 알못인 유튜버가 실시간으로 시청자에게 도움을 청하면서 고양이 유튜버가 됐는데요",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006153.1.19",
          "form": "그 어미고양이 누리가 매우 귀염고 똑똑해요...!",
          "original_form": "그 어미고양이 누리가 매우 귀염고 똑똑해요...!",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100006153.1.20",
          "form": "오 나중에 한번 볼께요",
          "original_form": "오 나중에 한번 볼께요",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006153.1.21",
          "form": "매탈남!",
          "original_form": "매탈남!",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100006153.1.22",
          "form": "아저씨가 점점 고양이에게 감기는 과정을 볼수 있어요ㅋㅋㅋㅋ",
          "original_form": "아저씨가 점점 고양이에게 감기는 과정을 볼수 있어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:29"
        }
      ]
