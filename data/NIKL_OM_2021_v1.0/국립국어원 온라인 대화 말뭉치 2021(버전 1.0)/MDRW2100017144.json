      "utterance": [
        {
          "id": "MDRW2100017144.1.1",
          "form": "토끼 장난감에는 뭐가 있어",
          "original_form": "토끼 장난감에는 뭐가 있어",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017144.1.2",
          "form": "토끼 용품",
          "original_form": "토끼 용품",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017144.1.3",
          "form": "이갈이",
          "original_form": "이갈이",
          "speaker_id": "1",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017144.1.4",
          "form": "미네랄 이갈이... 석고처럼생긴거",
          "original_form": "미네랄 이갈이... 석고처럼생긴거",
          "speaker_id": "1",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017144.1.5",
          "form": "장난감이야...?",
          "original_form": "장난감이야...?",
          "speaker_id": "2",
          "time": "20210826 17:39"
        },
        {
          "id": "MDRW2100017144.1.6",
          "form": "ㅇㅇ 그리고 바나나줄기같은거로 만든 공",
          "original_form": "ㅇㅇ 그리고 바나나줄기같은거로 만든 공",
          "speaker_id": "1",
          "time": "20210826 17:40"
        },
        {
          "id": "MDRW2100017144.1.7",
          "form": "공은 굴려??",
          "original_form": "공은 굴려??",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017144.1.8",
          "form": "물어뜯어서 먹어",
          "original_form": "물어뜯어서 먹어",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017144.1.9",
          "form": "귀엽네...",
          "original_form": "귀엽네...",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017144.1.10",
          "form": "건초 같은 느낌인가",
          "original_form": "건초 같은 느낌인가",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017144.1.11",
          "form": "맛있는건초로 만든거지 맞아",
          "original_form": "맛있는건초로 만든거지 맞아",
          "speaker_id": "1",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017144.1.12",
          "form": "맛있는 바나나 줄기 건초",
          "original_form": "맛있는 바나나 줄기 건초",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017144.1.13",
          "form": "지금은 다먹고 디비잔다",
          "original_form": "지금은 다먹고 디비잔다",
          "speaker_id": "1",
          "time": "20210828 22:01"
        },
        {
          "id": "MDRW2100017144.1.14",
          "form": "흐앙 귀여워",
          "original_form": "흐앙 귀여워",
          "speaker_id": "2",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100017144.1.15",
          "form": "토끼는 먹고 자고",
          "original_form": "토끼는 먹고 자고",
          "speaker_id": "2",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100017144.1.16",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "2",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100017144.1.17",
          "form": "얼마 못살잖아",
          "original_form": "얼마 못살잖아",
          "speaker_id": "1",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100017144.1.18",
          "form": "인간이 배려해야지",
          "original_form": "인간이 배려해야지",
          "speaker_id": "1",
          "time": "20210828 22:06"
        },
        {
          "id": "MDRW2100017144.1.19",
          "form": "그런 삶 부럽",
          "original_form": "그런 삶 부럽",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100017144.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100017144.1.21",
          "form": "먹고자기만 하는건 뭔가 아쉬워",
          "original_form": "먹고자기만 하는건 뭔가 아쉬워",
          "speaker_id": "1",
          "time": "20210828 22:13"
        },
        {
          "id": "MDRW2100017144.1.22",
          "form": "내가 더 뭔가 하고싶어",
          "original_form": "내가 더 뭔가 하고싶어",
          "speaker_id": "1",
          "time": "20210828 22:13"
        }
      ]
    }
