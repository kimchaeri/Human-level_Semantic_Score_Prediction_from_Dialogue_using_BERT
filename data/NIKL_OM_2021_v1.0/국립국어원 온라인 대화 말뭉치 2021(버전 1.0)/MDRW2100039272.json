        },
        {
          "id": "MDRW2100039272.1.2",
          "form": "맛대갈...",
          "original_form": "맛대갈...",
          "speaker_id": "2",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100039272.1.3",
          "form": "나는 현미로만해먹어",
          "original_form": "나는 현미로만해먹어",
          "speaker_id": "1",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100039272.1.4",
          "form": "없어서그래용",
          "original_form": "없어서그래용",
          "speaker_id": "2",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100039272.1.5",
          "form": "노노 최소 섞어야대",
          "original_form": "노노 최소 섞어야대",
          "speaker_id": "2",
          "time": "20210903 15:27"
        },
        {
          "id": "MDRW2100039272.1.6",
          "form": "나는 차라리 퍼플라이스",
          "original_form": "나는 차라리 퍼플라이스",
          "speaker_id": "2",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039272.1.7",
          "form": "아그거도조아",
          "original_form": "아그거도조아",
          "speaker_id": "1",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039272.1.8",
          "form": "곤약은 별륜가",
          "original_form": "곤약은 별륜가",
          "speaker_id": "1",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039272.1.9",
          "form": "콩도 완두콩x 강낭콩x",
          "original_form": "콩도 완두콩x 강낭콩x",
          "speaker_id": "2",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039272.1.10",
          "form": "병아리콩까지만 합의",
          "original_form": "병아리콩까지만 합의",
          "speaker_id": "2",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039272.1.11",
          "form": "곤약 소화가안돼",
          "original_form": "곤약 소화가안돼",
          "speaker_id": "2",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039272.1.12",
          "form": "뭐야나콩좋아햐",
          "original_form": "뭐야나콩좋아햐",
          "speaker_id": "1",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039272.1.13",
          "form": "콩만골라먹어",
          "original_form": "콩만골라먹어",
          "speaker_id": "1",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039272.1.14",
          "form": "어우시러",
          "original_form": "어우시러",
          "speaker_id": "2",
          "time": "20210903 15:28"
        },
        {
          "id": "MDRW2100039272.1.15",
          "form": "콩밥하면 엄빠 밥먼저떠",
          "original_form": "콩밥하면 엄빠 밥먼저떠",
          "speaker_id": "2",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100039272.1.16",
          "form": "헐 나조아",
          "original_form": "헐 나조아",
          "speaker_id": "1",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100039272.1.17",
          "form": "울 강아지들이",
          "original_form": "울 강아지들이",
          "speaker_id": "1",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100039272.1.18",
          "form": "내 콩 다먹어",
          "original_form": "내 콩 다먹어",
          "speaker_id": "1",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100039272.1.19",
          "form": "헐 입양하고싶다",
          "original_form": "헐 입양하고싶다",
          "speaker_id": "2",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100039272.1.20",
          "form": "먼지는 생선만 고기만...",
          "original_form": "먼지는 생선만 고기만...",
          "speaker_id": "2",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100039272.1.21",
          "form": "뭐야 난 양보하는거",
          "original_form": "뭐야 난 양보하는거",
          "speaker_id": "1",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100039272.1.22",
          "form": "고기미치지",
          "original_form": "고기미치지",
          "speaker_id": "1",
          "time": "20210903 15:29"
        },
        {
          "id": "MDRW2100039272.1.23",
          "form": "ㅎ 고기 못참지",
          "original_form": "ㅎ 고기 못참지",
          "speaker_id": "2",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100039272.1.24",
          "form": "무가당 요거트에 시리얼먹을까 저녁",
          "original_form": "무가당 요거트에 시리얼먹을까 저녁",
