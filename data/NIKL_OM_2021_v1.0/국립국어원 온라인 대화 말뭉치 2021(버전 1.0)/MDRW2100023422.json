      "utterance": [
        {
          "id": "MDRW2100023422.1.1",
          "form": "역시 자식이야기는 끝이없음ㅎㅎ",
          "original_form": "역시 자식이야기는 끝이없음ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 02:00"
        },
        {
          "id": "MDRW2100023422.1.2",
          "form": "네네ㅎㅎ",
          "original_form": "네네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 02:00"
        },
        {
          "id": "MDRW2100023422.1.3",
          "form": "그래도 잘먹어서 예쁘겟어요 ㅎㅎ",
          "original_form": "그래도 잘먹어서 예쁘겟어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:00"
        },
        {
          "id": "MDRW2100023422.1.4",
          "form": "애기가 천재라고 느낄때있어요?",
          "original_form": "애기가 천재라고 느낄때있어요?",
          "speaker_id": "2",
          "time": "20210830 02:00"
        },
        {
          "id": "MDRW2100023422.1.5",
          "form": "엄청좋죠ㅠ",
          "original_form": "엄청좋죠ㅠ",
          "speaker_id": "2",
          "time": "20210830 02:00"
        },
        {
          "id": "MDRW2100023422.1.6",
          "form": "음 책 좋아해서요",
          "original_form": "음 책 좋아해서요",
          "speaker_id": "1",
          "time": "20210830 02:01"
        },
        {
          "id": "MDRW2100023422.1.7",
          "form": "장염이라고 못먹으면 진짜ㅠ",
          "original_form": "장염이라고 못먹으면 진짜ㅠ",
          "speaker_id": "2",
          "time": "20210830 02:01"
        },
        {
          "id": "MDRW2100023422.1.8",
          "form": "우와ㅎㅎ",
          "original_form": "우와ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 02:01"
        },
        {
          "id": "MDRW2100023422.1.9",
          "form": "저번에 저 손다쳤는데",
          "original_form": "저번에 저 손다쳤는데",
          "speaker_id": "1",
          "time": "20210830 02:01"
        },
        {
          "id": "MDRW2100023422.1.10",
          "form": "그런데요?ㅎ",
          "original_form": "그런데요?ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:01"
        },
        {
          "id": "MDRW2100023422.1.11",
          "form": "엄마 왜 혈소판이 안와 엄마 손에 피 계속나는데 빨리 오라해  하는데 ㅎㅎ",
          "original_form": "엄마 왜 혈소판이 안와 엄마 손에 피 계속나는데 빨리 오라해  하는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:01"
        },
        {
          "id": "MDRW2100023422.1.12",
          "form": "ㅎㅎㅎㅎㅎ 천재네요ㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎ 천재네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 02:02"
        },
        {
          "id": "MDRW2100023422.1.13",
          "form": "이야 5살이?ㅎ",
          "original_form": "이야 5살이?ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:02"
        },
        {
          "id": "MDRW2100023422.1.14",
          "form": "아이큐테스트 한번해보세요ㅎ",
          "original_form": "아이큐테스트 한번해보세요ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:02"
        },
        {
          "id": "MDRW2100023422.1.15",
          "form": "천재는 아니에요 ㅎㅎ",
          "original_form": "천재는 아니에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:02"
        },
        {
          "id": "MDRW2100023422.1.16",
          "form": "요즘 저도 느끼는게 기억력이 너무 뛰어나서ㅎ",
          "original_form": "요즘 저도 느끼는게 기억력이 너무 뛰어나서ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:02"
        },
        {
          "id": "MDRW2100023422.1.17",
          "form": "어떤거요 ㅎㅎ",
          "original_form": "어떤거요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:02"
        },
        {
          "id": "MDRW2100023422.1.18",
          "form": "천재는 아니지만ㅎ 그래도 그렇게 말한다는게 대단한거네요ㅎ",
          "original_form": "천재는 아니지만ㅎ 그래도 그렇게 말한다는게 대단한거네요ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:03"
        },
        {
          "id": "MDRW2100023422.1.19",
          "form": "엄청어렸을때 약속한거를 안까먹어요 ㅡㅎ",
          "original_form": "엄청어렸을때 약속한거를 안까먹어요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210830 02:03"
        },
        {
          "id": "MDRW2100023422.1.20",
          "form": "음 ㅋㅋ 그쵸",
          "original_form": "음 ㅋㅋ 그쵸",
          "speaker_id": "1",
          "time": "20210830 02:03"
        },
        {
          "id": "MDRW2100023422.1.21",
          "form": "말도 안했을때 꺼를ㅎ",
          "original_form": "말도 안했을때 꺼를ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:03"
        },
        {
          "id": "MDRW2100023422.1.22",
          "form": "근데 보통 그런거는 뭐사준다는거 위주에요ㅎㅎㅎ",
          "original_form": "근데 보통 그런거는 뭐사준다는거 위주에요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 02:03"
        },
        {
          "id": "MDRW2100023422.1.23",
          "form": "ㅋㅋㅋ 뭔지 알죠~",
          "original_form": "ㅋㅋㅋ 뭔지 알죠~",
          "speaker_id": "1",
          "time": "20210830 02:03"
        },
        {
          "id": "MDRW2100023422.1.24",
          "form": "티비 채널 기억 하눈거랑",
          "original_form": "티비 채널 기억 하눈거랑",
          "speaker_id": "1",
          "time": "20210830 02:04"
        },
        {
          "id": "MDRW2100023422.1.25",
          "form": "맞아요ㅎㅎ 공감ㅎ",
          "original_form": "맞아요ㅎㅎ 공감ㅎ",
          "speaker_id": "2",
          "time": "20210830 02:04"
        },
        {
          "id": "MDRW2100023422.1.26",
          "form": "저희딸은 절 닮아서 음치같은데",
          "original_form": "저희딸은 절 닮아서 음치같은데",
          "speaker_id": "2",
          "time": "20210830 02:04"
        },
        {
          "id": "MDRW2100023422.1.27",
          "form": "ㅋㅋㅋㅋ 숫자배운게 지니한테  채널틀어주라고 할쳐고 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 숫자배운게 지니한테  채널틀어주라고 할쳐고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 02:04"
        },
        {
          "id": "MDRW2100023422.1.28",
          "form": "노래를 다외우더라구요ㅎ 100인 뭐죠?ㅎ 제목이?ㅎ 우리나라를 빛낸 100인의 영웅이었나?",
          "original_form": "노래를 다외우더라구요ㅎ 100인 뭐죠?ㅎ 제목이?ㅎ 우리나라를 빛낸 100인의 영웅이었나?",
          "speaker_id": "2",
          "time": "20210830 02:05"
        },
        {
          "id": "MDRW2100023422.1.29",
          "form": "오옹 ㅎㅎ",
          "original_form": "오옹 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:05"
        },
        {
          "id": "MDRW2100023422.1.30",
          "form": "기억력 좋네요  ㅎㅎㅎ",
          "original_form": "기억력 좋네요  ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 02:05"
        },
        {
          "id": "MDRW2100023422.1.31",
          "form": "예전에는 못했는뎇요즘은 친구야 말해요ㅎㅎㅎ",
          "original_form": "예전에는 못했는뎇요즘은 친구야 말해요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 02:05"
        },
        {
          "id": "MDRW2100023422.1.32",
          "form": "아 근데 지니도 웃긴게",
          "original_form": "아 근데 지니도 웃긴게",
          "speaker_id": "1",
          "time": "20210830 02:05"
        },
        {
          "id": "MDRW2100023422.1.33",
          "form": "근데ㅎ 저희 딸이 올해 1월까지 말못했어요ㅠ",
          "original_form": "근데ㅎ 저희 딸이 올해 1월까지 말못했어요ㅠ",
          "speaker_id": "2",
          "time": "20210830 02:05"
        },
        {
          "id": "MDRW2100023422.1.34",
          "form": "너무 걱정했었어요ㅠ",
          "original_form": "너무 걱정했었어요ㅠ",
          "speaker_id": "2",
          "time": "20210830 02:05"
        },
        {
          "id": "MDRW2100023422.1.35",
          "form": "아 몇개월인데요",
          "original_form": "아 몇개월인데요",
          "speaker_id": "1",
          "time": "20210830 02:06"
        },
        {
          "id": "MDRW2100023422.1.36",
          "form": "아가 몇개월이였는데요",
          "original_form": "아가 몇개월이였는데요",
          "speaker_id": "1",
          "time": "20210830 02:06"
        }
      ]
