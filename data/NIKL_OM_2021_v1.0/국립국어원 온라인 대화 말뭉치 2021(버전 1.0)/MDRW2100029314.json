      "utterance": [
        {
          "id": "MDRW2100029314.1.1",
          "form": "늦게주무셨군여",
          "original_form": "늦게주무셨군여",
          "speaker_id": "2",
          "time": "20210903 05:45"
        },
        {
          "id": "MDRW2100029314.1.2",
          "form": "잠들었다가 저때 깨서 다시 잤어요 ㅋㅋㅋ",
          "original_form": "잠들었다가 저때 깨서 다시 잤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 06:40"
        },
        {
          "id": "MDRW2100029314.1.3",
          "form": "엄청 일찍 일어나시는군여",
          "original_form": "엄청 일찍 일어나시는군여",
          "speaker_id": "1",
          "time": "20210903 06:40"
        },
        {
          "id": "MDRW2100029314.1.4",
          "form": "네 저 빨리나가야해서요 ㅎㅎ",
          "original_form": "네 저 빨리나가야해서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 07:55"
        },
        {
          "id": "MDRW2100029314.1.5",
          "form": "여친이 겨드랑이가 아프대요",
          "original_form": "여친이 겨드랑이가 아프대요",
          "speaker_id": "2",
          "time": "20210903 08:10"
        },
        {
          "id": "MDRW2100029314.1.6",
          "form": "림프절?",
          "original_form": "림프절?",
          "speaker_id": "2",
          "time": "20210903 08:10"
        },
        {
          "id": "MDRW2100029314.1.7",
          "form": "오잉 진짜 온데 다 아프신가보네요... ㅠㅠ무섭다",
          "original_form": "오잉 진짜 온데 다 아프신가보네요... ㅠㅠ무섭다",
          "speaker_id": "1",
          "time": "20210903 09:24"
        },
        {
          "id": "MDRW2100029314.1.8",
          "form": "여친 언니도 겨드랑이쪽이 부었었대요",
          "original_form": "여친 언니도 겨드랑이쪽이 부었었대요",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029314.1.9",
          "form": "저희 교무부장님 동생분도 그렇다네요",
          "original_form": "저희 교무부장님 동생분도 그렇다네요",
          "speaker_id": "1",
          "time": "20210903 12:52"
        },
        {
          "id": "MDRW2100029314.1.10",
          "form": "이번엔 그런 증상에 젤 많은가봐요",
          "original_form": "이번엔 그런 증상에 젤 많은가봐요",
          "speaker_id": "1",
          "time": "20210903 12:52"
        },
        {
          "id": "MDRW2100029314.1.11",
          "form": "남자들은 겨드랑이 얘기는 못들었는데",
          "original_form": "남자들은 겨드랑이 얘기는 못들었는데",
          "speaker_id": "2",
          "time": "20210903 14:42"
        },
        {
          "id": "MDRW2100029314.1.12",
          "form": "여자들한테 나타나는 증상인가봐요",
          "original_form": "여자들한테 나타나는 증상인가봐요",
          "speaker_id": "2",
          "time": "20210903 14:43"
        },
        {
          "id": "MDRW2100029314.1.13",
          "form": "그러게요 전 벌써부터 맞지도 않았는데 컨디션 안좋아서 걱정입니다ㅋㅋㅋ",
          "original_form": "그러게요 전 벌써부터 맞지도 않았는데 컨디션 안좋아서 걱정입니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:12"
        },
        {
          "id": "MDRW2100029314.1.14",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100029314.1.15",
          "form": "어디 아프세요?",
          "original_form": "어디 아프세요?",
          "speaker_id": "2",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100029314.1.16",
          "form": "어우 하루종일 두통이 심해서 걱정이었네요ㅋㅋ 이번주 너무 과로했더니 ㅋㅋ",
          "original_form": "어우 하루종일 두통이 심해서 걱정이었네요ㅋㅋ 이번주 너무 과로했더니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:44"
        },
        {
          "id": "MDRW2100029314.1.17",
          "form": "몸조리 잘하세요",
          "original_form": "몸조리 잘하세요",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100029314.1.18",
          "form": "여친은 이제 좀 괜찮대요",
          "original_form": "여친은 이제 좀 괜찮대요",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100029314.1.19",
          "form": "수요일 저녁부터 금요일오후까지 거의 이틀 앓더니",
          "original_form": "수요일 저녁부터 금요일오후까지 거의 이틀 앓더니",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100029314.1.20",
          "form": "오오! 다행이에요",
          "original_form": "오오! 다행이에요",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100029314.1.21",
          "form": "이제 여친이랑 즐겁게 데이트 할 수 있겠네요 ㅋㅋㅋ",
          "original_form": "이제 여친이랑 즐겁게 데이트 할 수 있겠네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100029314.1.22",
          "form": "낼이시죠? 누가 얘기하기론 30퍼센트 정도가",
          "original_form": "낼이시죠? 누가 얘기하기론 30퍼센트 정도가",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100029314.1.23",
          "form": "부작용에 시달린다던데",
          "original_form": "부작용에 시달린다던데",
          "speaker_id": "2",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100029314.1.24",
          "form": "2차 잘맞고 오셔요",
          "original_form": "2차 잘맞고 오셔요",
          "speaker_id": "2",
          "time": "20210904 08:12"
        },
        {
          "id": "MDRW2100029314.1.25",
          "form": "넹 감사합니당ㅎㅎㅎㅎ 즐거운 데이트하시길~~ 좋겠당~~",
          "original_form": "넹 감사합니당ㅎㅎㅎㅎ 즐거운 데이트하시길~~ 좋겠당~~",
          "speaker_id": "1",
          "time": "20210904 09:39"
        },
        {
          "id": "MDRW2100029314.1.26",
          "form": "언제 맞으셔요?",
          "original_form": "언제 맞으셔요?",
          "speaker_id": "2",
          "time": "20210904 10:40"
        },
        {
          "id": "MDRW2100029314.1.27",
          "form": "이틀만 버티시면 평일데이트가 가능하실거에요",
          "original_form": "이틀만 버티시면 평일데이트가 가능하실거에요",
          "speaker_id": "2",
          "time": "20210904 10:40"
        },
        {
          "id": "MDRW2100029314.1.28",
          "form": "저 열시에 맞았어요ㅋㅋㅋ 이제 약 세시간 지났네오! 아직까진 좀 뻐근한 거 말고는 별 반응 없어요",
          "original_form": "저 열시에 맞았어요ㅋㅋㅋ 이제 약 세시간 지났네오! 아직까진 좀 뻐근한 거 말고는 별 반응 없어요",
          "speaker_id": "1",
          "time": "20210904 12:44"
        },
        {
          "id": "MDRW2100029314.1.29",
          "form": "평일 바빠가지고 일요일 데이트 계획해요 ㅋㅋㅋ ㅠ",
          "original_form": "평일 바빠가지고 일요일 데이트 계획해요 ㅋㅋㅋ ㅠ",
          "speaker_id": "1",
          "time": "20210904 12:45"
        },
        {
          "id": "MDRW2100029314.1.30",
          "form": "아 바쁘시구나ㅜㅜ",
          "original_form": "아 바쁘시구나ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 16:32"
        },
        {
          "id": "MDRW2100029314.1.31",
          "form": "여친 만났는데 다 괜찮은데 주사맞은쪽 겨드랑이랑",
          "original_form": "여친 만났는데 다 괜찮은데 주사맞은쪽 겨드랑이랑",
          "speaker_id": "2",
          "time": "20210904 16:33"
        },
        {
          "id": "MDRW2100029314.1.32",
          "form": "피곤한거만 있대요",
          "original_form": "피곤한거만 있대요",
          "speaker_id": "2",
          "time": "20210904 16:33"
        },
        {
          "id": "MDRW2100029314.1.33",
          "form": "지금 차에서 자고있어서",
          "original_form": "지금 차에서 자고있어서",
          "speaker_id": "2",
          "time": "20210904 16:33"
        },
        {
          "id": "MDRW2100029314.1.34",
          "form": "심심해서 여기 들어왔어요 ㅋㅋㅋ",
          "original_form": "심심해서 여기 들어왔어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:33"
        },
        {
          "id": "MDRW2100029314.1.35",
          "form": "저도 아직까진 맞은 부분 팔만 좀 뻐근해요",
          "original_form": "저도 아직까진 맞은 부분 팔만 좀 뻐근해요",
          "speaker_id": "1",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100029314.1.36",
          "form": "별 증상이 아직 없는 거 같아요",
          "original_form": "별 증상이 아직 없는 거 같아요",
          "speaker_id": "1",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100029314.1.37",
          "form": "맛난거 드시구",
          "original_form": "맛난거 드시구",
          "speaker_id": "2",
          "time": "20210904 17:12"
        },
        {
          "id": "MDRW2100029314.1.38",
          "form": "푹 쉬세요",
          "original_form": "푹 쉬세요",
          "speaker_id": "2",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100029314.1.39",
          "form": "12시간부터가 진짜라는데",
          "original_form": "12시간부터가 진짜라는데",
          "speaker_id": "1",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100029314.1.40",
          "form": "여친은",
          "original_form": "여친은",
          "speaker_id": "2",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100029314.1.41",
          "form": "9시간뒤쯤",
          "original_form": "9시간뒤쯤",
          "speaker_id": "2",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100029314.1.42",
          "form": "그때부터",
          "original_form": "그때부터",
          "speaker_id": "2",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100029314.1.43",
          "form": "입질오더라구요",
          "original_form": "입질오더라구요",
          "speaker_id": "2",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100029314.1.44",
          "form": "오 9시간..!",
          "original_form": "오 9시간..!",
          "speaker_id": "1",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100029314.1.45",
          "form": "체온계 옆에두시고",
          "original_form": "체온계 옆에두시고",
          "speaker_id": "2",
          "time": "20210904 17:13"
        },
        {
          "id": "MDRW2100029314.1.46",
          "form": "이상하면 바로 체온재고 해열제드세요",
          "original_form": "이상하면 바로 체온재고 해열제드세요",
          "speaker_id": "2",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100029314.1.47",
          "form": "옆에 꺼내놔야겠네요ㅋㅋㅋ",
          "original_form": "옆에 꺼내놔야겠네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100029314.1.48",
          "form": "증상이 사라지면 피곤함만 남는거 같아요",
          "original_form": "증상이 사라지면 피곤함만 남는거 같아요",
          "speaker_id": "2",
          "time": "20210904 17:14"
        },
        {
          "id": "MDRW2100029314.1.49",
          "form": "ㅋㅋㅋㅋ후기 들려드릴래도 이미 전문가시네요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ후기 들려드릴래도 이미 전문가시네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:15"
        },
        {
          "id": "MDRW2100029314.1.50",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100029314.1.51",
          "form": "궁금해요",
          "original_form": "궁금해요",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100029314.1.52",
          "form": "저는 한사람 얘기만 들은거니깐 ㅋㅋ",
          "original_form": "저는 한사람 얘기만 들은거니깐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100029314.1.53",
          "form": "네ㅋㅋㅋ 이미 입장하셨나용",
          "original_form": "네ㅋㅋㅋ 이미 입장하셨나용",
          "speaker_id": "1",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100029314.1.54",
          "form": "이제 슬슬 2차 맞은사람 늘어나는거 같아요 ㅎㅎ",
          "original_form": "이제 슬슬 2차 맞은사람 늘어나는거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 17:30"
        },
        {
          "id": "MDRW2100029314.1.55",
          "form": "그러게요 언제맞으세여",
          "original_form": "그러게요 언제맞으세여",
          "speaker_id": "1",
          "time": "20210904 17:31"
        },
        {
          "id": "MDRW2100029314.1.56",
          "form": "저 멀었죠",
          "original_form": "저 멀었죠",
          "speaker_id": "2",
          "time": "20210904 17:36"
        },
        {
          "id": "MDRW2100029314.1.57",
          "form": "저번주에 맞았으니까",
          "original_form": "저번주에 맞았으니까",
          "speaker_id": "2",
          "time": "20210904 17:37"
        },
        {
          "id": "MDRW2100029314.1.58",
          "form": "10월 9일요",
          "original_form": "10월 9일요",
          "speaker_id": "2",
          "time": "20210904 17:37"
        },
        {
          "id": "MDRW2100029314.1.59",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:37"
        },
        {
