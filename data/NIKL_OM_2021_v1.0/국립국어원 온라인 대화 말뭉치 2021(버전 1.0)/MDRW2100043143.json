      "utterance": [
        {
          "id": "MDRW2100043143.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043143.1.2",
          "form": "아침 드시는 편이신가요?",
          "original_form": "아침 드시는 편이신가요?",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043143.1.3",
          "form": "안녕하세요! 네 아침 꼭 먹어요 ㅎㅎname1님은요",
          "original_form": "안녕하세요! 네 아침 꼭 먹어요 ㅎㅎ&name1&님은요",
          "speaker_id": "2",
          "time": "20210907 15:05"
        },
        {
          "id": "MDRW2100043143.1.4",
          "form": "저는 잠이 많아서 아침은 포기하는 경우가 많아요ㅎㅎ",
          "original_form": "저는 잠이 많아서 아침은 포기하는 경우가 많아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043143.1.5",
          "form": "아침 간단하게 드시나요?",
          "original_form": "아침 간단하게 드시나요?",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043143.1.6",
          "form": "아하 네 전 아침 못 먹더라도 두유나 시리얼 이런 거라도 먹으려고 해요",
          "original_form": "아하 네 전 아침 못 먹더라도 두유나 시리얼 이런 거라도 먹으려고 해요",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043143.1.7",
          "form": "오 좋은거 같아요ㅎㅎ",
          "original_form": "오 좋은거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043143.1.8",
          "form": "간단하게라도 먹는거!!",
          "original_form": "간단하게라도 먹는거!!",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043143.1.9",
          "form": "저녁엔 무얼 드셨나요?",
          "original_form": "저녁엔 무얼 드셨나요?",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043143.1.10",
          "form": "만둣국 먹었어요!!집에서",
          "original_form": "만둣국 먹었어요!!집에서",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043143.1.11",
          "form": "우와 맛있는거 드셨네요!!",
          "original_form": "우와 맛있는거 드셨네요!!",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043143.1.12",
          "form": "직접 만드셨나요?",
          "original_form": "직접 만드셨나요?",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043143.1.13",
          "form": "네 그냥 만두 몇개 넣고 후루룩 끓였어요 ㅋㅋ",
          "original_form": "네 그냥 만두 몇개 넣고 후루룩 끓였어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043143.1.14",
          "form": "만둣국도 끓이기 어려워 보이는데 대단하세요!!",
          "original_form": "만둣국도 끓이기 어려워 보이는데 대단하세요!!",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043143.1.15",
          "form": "만두 쪄먹는거, 구워먹는거, 만둣국 어떻게 먹는거 좋아하세요?",
          "original_form": "만두 쪄먹는거, 구워먹는거, 만둣국 어떻게 먹는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043143.1.16",
          "form": "아니에요 사실 요리도 아닌 수준이라 ㅋㅋ",
          "original_form": "아니에요 사실 요리도 아닌 수준이라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043143.1.17",
          "form": "저는 다 좋은데 국이 젤 좋네요",
          "original_form": "저는 다 좋은데 국이 젤 좋네요",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043143.1.18",
          "form": "오 저는 찐만두 좋아해요ㅎㅎ",
          "original_form": "오 저는 찐만두 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043143.1.19",
          "form": "김치, 고기만두 중에 뭐 좋아하시나요?",
          "original_form": "김치, 고기만두 중에 뭐 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043143.1.20",
          "form": "브랜드따라 다른거 같은데 원래는 고기 더 잘먹고 풀무원은 김치로요 ㅋㅋ",
          "original_form": "브랜드따라 다른거 같은데 원래는 고기 더 잘먹고 풀무원은 김치로요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043143.1.21",
          "form": "오 풀무원 김치만두 나중에 먹어봐야겠네요",
          "original_form": "오 풀무원 김치만두 나중에 먹어봐야겠네요",
