      "utterance": [
        {
          "id": "MDRW2100018924.1.1",
          "form": "좋아하는 연예인 있나요?",
          "original_form": "좋아하는 연예인 있나요?",
          "speaker_id": "1",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018924.1.2",
          "form": "요새는 오마이걸이 좋더라구요 ㅎㅎ 좋아하는 연예인 있으세요?",
          "original_form": "요새는 오마이걸이 좋더라구요 ㅎㅎ 좋아하는 연예인 있으세요?",
          "speaker_id": "2",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018924.1.3",
          "form": "저는 박서준 스탈 좋더라구요",
          "original_form": "저는 박서준 스탈 좋더라구요",
          "speaker_id": "1",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018924.1.4",
          "form": "박서준 잘생겼죠 ㅎㅎ 저도 배우쪽은 남배우가 좋더라구요",
          "original_form": "박서준 잘생겼죠 ㅎㅎ 저도 배우쪽은 남배우가 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018924.1.5",
          "form": "진지와 귀욤을 다 가진 외모가 끌리더라구요",
          "original_form": "진지와 귀욤을 다 가진 외모가 끌리더라구요",
          "speaker_id": "1",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018924.1.6",
          "form": "ㅎㅎ 첨에 무슨 작품보고 좋아하셨어요?",
          "original_form": "ㅎㅎ 첨에 무슨 작품보고 좋아하셨어요?",
          "speaker_id": "2",
          "time": "20210827 13:47"
        },
        {
          "id": "MDRW2100018924.1.7",
          "form": "킬미힐미요. 황정음이랑 지성 나온거요",
          "original_form": "킬미힐미요. 황정음이랑 지성 나온거요",
          "speaker_id": "1",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100018924.1.8",
          "form": "김비서가 왜 그럴까도 좋았구요",
          "original_form": "김비서가 왜 그럴까도 좋았구요",
          "speaker_id": "1",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100018924.1.9",
          "form": "아 저도 킬미힐미 가끔 틀면 봤는데 재미있게 봤어요ㅋㅋㅋㅋ 지성 연기가 넘 웃겼어요",
          "original_form": "아 저도 킬미힐미 가끔 틀면 봤는데 재미있게 봤어요ㅋㅋㅋㅋ 지성 연기가 넘 웃겼어요",
          "speaker_id": "2",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100018924.1.10",
          "form": "맞아요. 특히 여고생 요나로 변신했을 때 진짜 웃겼어요",
          "original_form": "맞아요. 특히 여고생 요나로 변신했을 때 진짜 웃겼어요",
          "speaker_id": "1",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018924.1.11",
          "form": "요나랑 박서준이랑 뽀뽀 했었죠 ㅋ",
          "original_form": "요나랑 박서준이랑 뽀뽀 했었죠 ㅋ",
          "speaker_id": "1",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018924.1.12",
          "form": "저도 요나 나올때 너무 재미있더라구요ㅋㅋㅋㅋ 근데 진짜 신선했어요 어떻게 그렇게 여러명을 연기할까요ㅋㅋㅋㅋ 요나 연기는 진짜 ㅋㅋㅋㅋ",
          "original_form": "저도 요나 나올때 너무 재미있더라구요ㅋㅋㅋㅋ 근데 진짜 신선했어요 어떻게 그렇게 여러명을 연기할까요ㅋㅋㅋㅋ 요나 연기는 진짜 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:52"
        },
        {
          "id": "MDRW2100018924.1.13",
          "form": "지성이니까 가능했던거 같아요",
          "original_form": "지성이니까 가능했던거 같아요",
          "speaker_id": "1",
          "time": "20210827 13:52"
        },
        {
          "id": "MDRW2100018924.1.14",
          "form": "요나 자꾸 생각나네요. 박서준한테 오빠오빠 하면서 머리 귀에 꽂는 모습이요 ㅎ",
          "original_form": "요나 자꾸 생각나네요. 박서준한테 오빠오빠 하면서 머리 귀에 꽂는 모습이요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 13:53"
        },
        {
          "id": "MDRW2100018924.1.15",
          "form": "그렇게 연기 다양하게 소화하는 배우들 보면 대단한 것 같아요 ",
          "original_form": "그렇게 연기 다양하게 소화하는 배우들 보면 대단한 것 같아요 ",
          "speaker_id": "2",
          "time": "20210827 13:56"
        },
        {
          "id": "MDRW2100018924.1.16",
          "form": "배우 아무나 하는게 아니ㄴ거 같아요",
          "original_form": "배우 아무나 하는게 아니ㄴ거 같아요",
          "speaker_id": "1",
          "time": "20210827 13:57"
        },
        {
          "id": "MDRW2100018924.1.17",
          "form": "지성은 인성도 바르고 연기 논란도 없어서 좋은거 같아요",
          "original_form": "지성은 인성도 바르고 연기 논란도 없어서 좋은거 같아요",
          "speaker_id": "1",
          "time": "20210827 13:58"
        }
      ]
    }
