      "utterance": [
        {
          "id": "MDRW2100007455.1.1",
          "form": "요리하는건 별로 안좋아해요",
          "original_form": "요리하는건 별로 안좋아해요",
          "speaker_id": "2",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007455.1.2",
          "form": "ㅋㅋㅋㅋ저도요",
          "original_form": "ㅋㅋㅋㅋ저도요",
          "speaker_id": "1",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007455.1.3",
          "form": "거의 사먹거나 시댁에서 먹거나 밀키트사요",
          "original_form": "거의 사먹거나 시댁에서 먹거나 밀키트사요",
          "speaker_id": "1",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007455.1.4",
          "form": "어쩌다 요리 뽐뿌오면 블로그보고 하기도하는데",
          "original_form": "어쩌다 요리 뽐뿌오면 블로그보고 하기도하는데",
          "speaker_id": "1",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007455.1.5",
          "form": "하고나서 치우고 설거지하는게 세상 귀찮더라고요",
          "original_form": "하고나서 치우고 설거지하는게 세상 귀찮더라고요",
          "speaker_id": "1",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007455.1.6",
          "form": "밀키트도 좋긴하던데",
          "original_form": "밀키트도 좋긴하던데",
          "speaker_id": "2",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007455.1.7",
          "form": "먹는걸 사먹다보니 지출이 어마어마해요ㅠ",
          "original_form": "먹는걸 사먹다보니 지출이 어마어마해요ㅠ",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007455.1.8",
          "form": "하는게 힘들더라구요 ㅎ",
          "original_form": "하는게 힘들더라구요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007455.1.9",
          "form": "그나마 내가 재료를 안사는거에 위안을둬요",
          "original_form": "그나마 내가 재료를 안사는거에 위안을둬요",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007455.1.10",
          "form": "자주먹는 음식있어요?",
          "original_form": "자주먹는 음식있어요?",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007455.1.11",
          "form": "저는 밀푀나베유? 그거 야채고기야채고기 만들어져있는줄알았어요",
          "original_form": "저는 밀푀나베유? 그거 야채고기야채고기 만들어져있는줄알았어요",
          "speaker_id": "2",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007455.1.12",
          "form": "그거 쌓다가 목뼈부러지는거같았어요 ㅋ",
          "original_form": "그거 쌓다가 목뼈부러지는거같았어요 ㅋ",
          "speaker_id": "2",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007455.1.13",
          "form": "주로 밥에 애들땜에 미역국 먹는거같아요",
          "original_form": "주로 밥에 애들땜에 미역국 먹는거같아요",
          "speaker_id": "2",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007455.1.14",
          "form": "그래도 몸 생각한댜그 꾸역꾸역 먹었는데 지금생각하면 소름 돋아요",
          "original_form": "그래도 몸 생각한댜그 꾸역꾸역 먹었는데 지금생각하면 소름 돋아요",
          "speaker_id": "1",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007455.1.15",
          "form": "자주 드시는거 있으세요?",
          "original_form": "자주 드시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007455.1.16",
          "form": "족발이요ㅋㅋ어제는사먹고 오늘은시댁에서 해주시고",
          "original_form": "족발이요ㅋㅋ어제는사먹고 오늘은시댁에서 해주시고",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007455.1.17",
          "form": "남편이너무좋아해요ㅠㅠ",
          "original_form": "남편이너무좋아해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007455.1.18",
          "form": "래브라도리트리버키우는데 얘도 좋아하고",
          "original_form": "래브라도리트리버키우는데 얘도 좋아하고",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007455.1.19",
          "form": "연애6년때부터 족발을 주1회는 필수로 먹었던지라 저는 그만먹고싶어요ㅋㅋ",
          "original_form": "연애6년때부터 족발을 주1회는 필수로 먹었던지라 저는 그만먹고싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007455.1.20",
          "form": "ㅎㅎㅎ 님은 머좋아하시는데요?",
          "original_form": "ㅎㅎㅎ 님은 머좋아하시는데요?",
          "speaker_id": "2",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007455.1.21",
          "form": "닭강정이나 삼계탕 과일이요",
          "original_form": "닭강정이나 삼계탕 과일이요",
          "speaker_id": "1",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007455.1.22",
          "form": "주1회  필수로 삼계탕먹어요",
          "original_form": "주1회  필수로 삼계탕먹어요",
          "speaker_id": "1",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007455.1.23",
          "form": "닭종류군요",
          "original_form": "닭종류군요",
          "speaker_id": "2",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007455.1.24",
          "form": "삼계탕을 주1회요? 와",
          "original_form": "삼계탕을 주1회요? 와",
          "speaker_id": "2",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007455.1.25",
          "form": "네 너무맛있어요!!!!",
          "original_form": "네 너무맛있어요!!!!",
