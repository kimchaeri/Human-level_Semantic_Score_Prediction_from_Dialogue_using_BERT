      "utterance": [
        {
          "id": "MDRW2100034005.1.1",
          "form": "굴전 이나 굴찜 좋아하시나요?",
          "original_form": "굴전 이나 굴찜 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100034005.1.2",
          "form": "굴전은 좋아하고 찜으로는 안먹어봤내요",
          "original_form": "굴전은 좋아하고 찜으로는 안먹어봤내요",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100034005.1.3",
          "form": "맛있나요?",
          "original_form": "맛있나요?",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100034005.1.4",
          "form": "저는 익힌건 안좋아해서 안먹어봤어요",
          "original_form": "저는 익힌건 안좋아해서 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100034005.1.5",
          "form": "아 생굴 좋아하세요?",
          "original_form": "아 생굴 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100034005.1.6",
          "form": "네!생굴도 좋아하고 어리굴젓같은것도 좋아해요",
          "original_form": "네!생굴도 좋아하고 어리굴젓같은것도 좋아해요",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100034005.1.7",
          "form": "뭔가 해산물 엄청 잘드실것같내요",
          "original_form": "뭔가 해산물 엄청 잘드실것같내요",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100034005.1.8",
          "form": "간장게장도 좋아하새요?",
          "original_form": "간장게장도 좋아하새요?",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100034005.1.9",
          "form": "정확히 보셨어요",
          "original_form": "정확히 보셨어요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100034005.1.10",
          "form": "네 없어서 못먹어요",
          "original_form": "네 없어서 못먹어요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100034005.1.11",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100034005.1.12",
          "form": "저도 없어서 못먹죠...",
          "original_form": "저도 없어서 못먹죠...",
          "speaker_id": "1",
          "time": "20210826 11:54"
        },
        {
          "id": "MDRW2100034005.1.13",
          "form": "특히 회 엄청 사랑해요",
          "original_form": "특히 회 엄청 사랑해요",
          "speaker_id": "1",
          "time": "20210826 11:54"
        },
        {
          "id": "MDRW2100034005.1.14",
          "form": "회! 너무 좋죠ㅠㅠ",
          "original_form": "회! 너무 좋죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100034005.1.15",
          "form": "무슨 회 특히 좋아하시나요",
          "original_form": "무슨 회 특히 좋아하시나요",
          "speaker_id": "2",
          "time": "20210826 11:55"
        },
        {
          "id": "MDRW2100034005.1.16",
          "form": "연어랑 광어 너무 좋아요..",
          "original_form": "연어랑 광어 너무 좋아요..",
          "speaker_id": "1",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100034005.1.17",
          "form": "초밥도 땅기네요 하...",
          "original_form": "초밥도 땅기네요 하...",
          "speaker_id": "1",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100034005.1.18",
          "form": "연어광어세트 이제 어딜가도 있어서 너무 좋아요...",
          "original_form": "연어광어세트 이제 어딜가도 있어서 너무 좋아요...",
          "speaker_id": "2",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100034005.1.19",
          "form": "천재적인 궁합",
          "original_form": "천재적인 궁합",
          "speaker_id": "2",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100034005.1.20",
          "form": "맞아요 정말..",
          "original_form": "맞아요 정말..",
          "speaker_id": "1",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100034005.1.21",
          "form": "새상은 아름다워여",
          "original_form": "새상은 아름다워여",
          "speaker_id": "1",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100034005.1.22",
          "form": "새우도 진짜 좋아해요",
          "original_form": "새우도 진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100034005.1.23",
          "form": "너무 맛나..",
          "original_form": "너무 맛나..",
          "speaker_id": "1",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100034005.1.24",
          "form": "계절마다 맛있는 회는 또 어찌나 매번 나오는지",
          "original_form": "계절마다 맛있는 회는 또 어찌나 매번 나오는지",
          "speaker_id": "2",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100034005.1.25",
          "form": "최고예요...",
          "original_form": "최고예요...",
          "speaker_id": "2",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100034005.1.26",
          "form": "아아 맞아요",
          "original_form": "아아 맞아요",
          "speaker_id": "1",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100034005.1.27",
          "form": "여름에는 잘 못먹는데",
          "original_form": "여름에는 잘 못먹는데",
          "speaker_id": "1",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100034005.1.28",
          "form": "이제는 또 열심히 ㅎ",
          "original_form": "이제는 또 열심히 ㅎ",
          "speaker_id": "1",
          "time": "20210826 12:03"
        }
      ]
    }
