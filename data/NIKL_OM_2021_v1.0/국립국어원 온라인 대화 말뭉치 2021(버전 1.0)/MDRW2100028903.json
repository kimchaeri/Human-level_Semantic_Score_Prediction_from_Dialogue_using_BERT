      "utterance": [
        {
          "id": "MDRW2100028903.1.1",
          "form": "그래서 꽃게탕 끓여드실꺼에요?ㅎㅎ",
          "original_form": "그래서 꽃게탕 끓여드실꺼에요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:39"
        },
        {
          "id": "MDRW2100028903.1.2",
          "form": "찜좀 먹구 나머진  소분해서 냉동요",
          "original_form": "찜좀 먹구 나머진  소분해서 냉동요",
          "speaker_id": "1",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028903.1.3",
          "form": "덕분애 알볼로 빠이",
          "original_form": "덕분애 알볼로 빠이",
          "speaker_id": "1",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028903.1.4",
          "form": "우와우와ㅎㅎ",
          "original_form": "우와우와ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028903.1.5",
          "form": "가족들 좋아했겟네요ㅎㅎ",
          "original_form": "가족들 좋아했겟네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028903.1.6",
          "form": "알블로는 원래 안됫을꺼애요ㅎ",
          "original_form": "알블로는 원래 안됫을꺼애요ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028903.1.7",
          "form": "근데 알볼로 디게디게 먹고싶어서",
          "original_form": "근데 알볼로 디게디게 먹고싶어서",
          "speaker_id": "1",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028903.1.8",
          "form": "유튜브가 계속 올리는바람에ㅎㅎ",
          "original_form": "유튜브가 계속 올리는바람에ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:40"
        },
        {
          "id": "MDRW2100028903.1.9",
          "form": "벌써 구독자만 3만명이니ㅎㅎ",
          "original_form": "벌써 구독자만 3만명이니ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028903.1.10",
          "form": "아까 시키려고 하니 온라인 주문 막움 ㅋ",
          "original_form": "아까 시키려고 하니 온라인 주문 막움 ㅋ",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028903.1.11",
          "form": "글져글져ㅎ",
          "original_form": "글져글져ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028903.1.12",
          "form": "전이미 점심부터 포기함ㅎㅎ",
          "original_form": "전이미 점심부터 포기함ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028903.1.13",
          "form": "내께 아니엿나봅니다 ㅋ",
          "original_form": "내께 아니엿나봅니다 ㅋ",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028903.1.14",
          "form": "암요암요ㅎㅎ",
          "original_form": "암요암요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028903.1.15",
          "form": "아 오늘 애기 눈부어서 병원다녀오규",
          "original_form": "아 오늘 애기 눈부어서 병원다녀오규",
          "speaker_id": "1",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028903.1.16",
          "form": "봐봐요ㅎ 신세계ㅎㅎ 내께 아니라고 포기한순간",
          "original_form": "봐봐요ㅎ 신세계ㅎㅎ 내께 아니라고 포기한순간",
          "speaker_id": "2",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028903.1.17",
          "form": "혜자ㅎ",
          "original_form": "혜자ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:41"
        },
        {
