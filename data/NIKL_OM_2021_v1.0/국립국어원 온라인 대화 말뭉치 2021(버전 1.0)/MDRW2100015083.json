      "utterance": [
        {
          "id": "MDRW2100015083.1.1",
          "form": "크리스 파인이 스타트렉 비욘드에서 커크역",
          "original_form": "크리스 파인이 스타트렉 비욘드에서 커크역",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015083.1.2",
          "form": "원더우먼에 나온 남자인거네",
          "original_form": "원더우먼에 나온 남자인거네",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015083.1.3",
          "form": "얼굴 보면 알꺼다",
          "original_form": "얼굴 보면 알꺼다",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100015083.1.4",
          "form": "잠시만 찾아보께ㅋㅋ",
          "original_form": "잠시만 찾아보께ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015083.1.5",
          "form": "출연료가 1150만 달러 갑자기 출연료가 확 많아졌다",
          "original_form": "출연료가 1150만 달러 갑자기 출연료가 확 많아졌다",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015083.1.6",
          "form": "얼굴 본거 같기도 하고ㅋㅋ 근데 영화를 보 지 못함",
          "original_form": "얼굴 본거 같기도 하고ㅋㅋ 근데 영화를 보 지 못함",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015083.1.7",
          "form": "15위 우리가 아는 키아누 리브스",
          "original_form": "15위 우리가 아는 키아누 리브스",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015083.1.8",
          "form": "키아누 많이 죽었네 15 위라니",
          "original_form": "키아누 많이 죽었네 15 위라니",
          "speaker_id": "1",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015083.1.9",
          "form": "ㅋㅋ 매트릭스4 찍었네",
          "original_form": "ㅋㅋ 매트릭스4 찍었네",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015083.1.10",
          "form": "또 나오나?  갈수록 재미 없어",
          "original_form": "또 나오나?  갈수록 재미 없어",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015083.1.11",
          "form": "그냥 3에서 끝나는게 좋았을텐데 더 할 얘기가 있었나",
          "original_form": "그냥 3에서 끝나는게 좋았을텐데 더 할 얘기가 있었나",
          "speaker_id": "2",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100015083.1.12",
          "form": "그니깐 나와도 재미 없을듯 14위는?",
          "original_form": "그니깐 나와도 재미 없을듯 14위는?",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015083.1.13",
          "form": "심지어 이번 12월 개봉",
          "original_form": "심지어 이번 12월 개봉",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015083.1.14",
          "form": "망했네. 요즘 시국에 보겠나?",
          "original_form": "망했네. 요즘 시국에 보겠나?",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015083.1.15",
          "form": "그러게 제작비도 엄청 들었을껀데",
