      "utterance": [
        {
          "id": "MDRW2100041377.1.1",
          "form": "학교다니시나요",
          "original_form": "학교다니시나요",
          "speaker_id": "2",
          "time": "20210905 22:48"
        },
        {
          "id": "MDRW2100041377.1.2",
          "form": "졸업한지 조금 됐어요 ㅎㅎ",
          "original_form": "졸업한지 조금 됐어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100041377.1.3",
          "form": "name2이님은요?",
          "original_form": "&name2&이님은요?",
          "speaker_id": "1",
          "time": "20210905 22:51"
        },
        {
          "id": "MDRW2100041377.1.4",
          "form": "저두요 그리워요",
          "original_form": "저두요 그리워요",
          "speaker_id": "2",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100041377.1.5",
          "form": "어떤 과목 제일 좋아하셨어요??",
          "original_form": "어떤 과목 제일 좋아하셨어요??",
          "speaker_id": "1",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100041377.1.6",
          "form": "수학이요...",
          "original_form": "수학이요...",
          "speaker_id": "2",
          "time": "20210905 22:55"
        },
        {
          "id": "MDRW2100041377.1.7",
          "form": "수학!! 어떤 부분이 좋으셨어요?",
          "original_form": "수학!! 어떤 부분이 좋으셨어요?",
          "speaker_id": "1",
          "time": "20210905 22:56"
        },
        {
          "id": "MDRW2100041377.1.8",
          "form": "저는 확률이랑 도형 좋아했어요",
          "original_form": "저는 확률이랑 도형 좋아했어요",
          "speaker_id": "1",
          "time": "20210905 22:56"
        },
        {
          "id": "MDRW2100041377.1.9",
          "form": "오홍 수학 거의다..?  ㅋㅋㅋㅋ  도형은 별로였고 기하는 조아햇어용 ㅋㅋㅋ 아님 그냥 대수..?",
          "original_form": "오홍 수학 거의다..?  ㅋㅋㅋㅋ  도형은 별로였고 기하는 조아햇어용 ㅋㅋㅋ 아님 그냥 대수..?",
          "speaker_id": "2",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100041377.1.10",
          "form": "우와.. 대수... ㄷㄷㄷㄷ",
          "original_form": "우와.. 대수... ㄷㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210905 23:01"
        },
        {
          "id": "MDRW2100041377.1.11",
          "form": "아마 굉장히 잘 하셨던 것 같아요",
          "original_form": "아마 굉장히 잘 하셨던 것 같아요",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041377.1.12",
          "form": "잘 하셨던 비법이 있으신가요?",
          "original_form": "잘 하셨던 비법이 있으신가요?",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041377.1.13",
          "form": "아니요.... 그냥 했어요 ㅋㅋㅋㅋㅋ",
          "original_form": "아니요.... 그냥 했어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041377.1.14",
          "form": "안외워도돼서 조아햇어요 ㅋㅋ",
          "original_form": "안외워도돼서 조아햇어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100041377.1.15",
          "form": "이런 분들이 꼭 전교 1등 하고 그랬던 것 같은데 ㅋㅋㅋ",
          "original_form": "이런 분들이 꼭 전교 1등 하고 그랬던 것 같은데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041377.1.16",
          "form": "다른 과목보단 암기가 적긴 하지만",
          "original_form": "다른 과목보단 암기가 적긴 하지만",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041377.1.17",
          "form": "응용력이 많이 필요한 과목이라",
          "original_form": "응용력이 많이 필요한 과목이라",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041377.1.18",
          "form": "응용력이 굉장히 좋으셨나봐요!",
          "original_form": "응용력이 굉장히 좋으셨나봐요!",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041377.1.19",
          "form": "아이큐테스트도 엄청 높으셨을것 같아요",
          "original_form": "아이큐테스트도 엄청 높으셨을것 같아요",
          "speaker_id": "1",
          "time": "20210905 23:03"
        },
        {
          "id": "MDRW2100041377.1.20",
          "form": "ㅋㅋㅋㅋ그건 아니에요",
          "original_form": "ㅋㅋㅋㅋ그건 아니에요",
          "speaker_id": "2",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100041377.1.21",
          "form": "싫어햇던 과목 잇나요 ㅋㅋ",
          "original_form": "싫어햇던 과목 잇나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:04"
        },
        {
          "id": "MDRW2100041377.1.22",
          "form": "체육이랑 물리요",
          "original_form": "체육이랑 물리요",
          "speaker_id": "1",
          "time": "20210905 23:06"
        },
        {
          "id": "MDRW2100041377.1.23",
          "form": "먹짱이님은요?",
          "original_form": "먹짱이님은요?",
          "speaker_id": "1",
          "time": "20210905 23:07"
        },
        {
          "id": "MDRW2100041377.1.24",
          "form": "ㅋㅋㅋ도덕 사회...?",
          "original_form": "ㅋㅋㅋ도덕 사회...?",
          "speaker_id": "2",
          "time": "20210905 23:16"
        },
        {
          "id": "MDRW2100041377.1.25",
          "form": "도덕 문제가 넘 애매한게많아가지고 ㅋㅋㅋ",
          "original_form": "도덕 문제가 넘 애매한게많아가지고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:17"
        },
        {
          "id": "MDRW2100041377.1.26",
          "form": "아... 맞아요!",
          "original_form": "아... 맞아요!",
          "speaker_id": "1",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100041377.1.27",
          "form": "착한거만 고르면 된다고 하셨는데",
          "original_form": "착한거만 고르면 된다고 하셨는데",
          "speaker_id": "1",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100041377.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100041377.1.29",
          "form": "너무 다 애매했던 것 같아요",
          "original_form": "너무 다 애매했던 것 같아요",
          "speaker_id": "1",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100041377.1.30",
          "form": "생각보다 철학적인거도 잇는거도같고",
          "original_form": "생각보다 철학적인거도 잇는거도같고",
          "speaker_id": "2",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100041377.1.31",
          "form": "도덕 선생님은 어떤 분이셨어요?",
          "original_form": "도덕 선생님은 어떤 분이셨어요?",
          "speaker_id": "1",
          "time": "20210905 23:32"
        },
        {
          "id": "MDRW2100041377.1.32",
          "form": "쌤은 조앗어요 ㅋㅋㅋㅋ",
          "original_form": "쌤은 조앗어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:33"
        },
        {
          "id": "MDRW2100041377.1.33",
          "form": "친근한 큰아빠같은",
          "original_form": "친근한 큰아빠같은",
          "speaker_id": "2",
          "time": "20210905 23:34"
        },
        {
          "id": "MDRW2100041377.1.34",
          "form": "인기도많고 ㅋㅋㅋ",
          "original_form": "인기도많고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 23:34"
        },
        {
          "id": "MDRW2100041377.1.35",
          "form": "우와 멋쟁이 선생님이셨나봐요!",
          "original_form": "우와 멋쟁이 선생님이셨나봐요!",
          "speaker_id": "1",
          "time": "20210905 23:35"
        }
      ]
