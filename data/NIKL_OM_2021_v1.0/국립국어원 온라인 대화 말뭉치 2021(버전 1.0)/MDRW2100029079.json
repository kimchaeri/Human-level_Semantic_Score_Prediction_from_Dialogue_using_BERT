      "utterance": [
        {
          "id": "MDRW2100029079.1.1",
          "form": "최근에 쇼핑하신 거 있나요??",
          "original_form": "최근에 쇼핑하신 거 있나요??",
          "speaker_id": "1",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029079.1.2",
          "form": "저 메론 꽃게 샀어요 님은요",
          "original_form": "저 메론 꽃게 샀어요 님은요",
          "speaker_id": "2",
          "time": "20210902 23:35"
        },
        {
          "id": "MDRW2100029079.1.3",
          "form": "아 ㅋㅋㅋㅋㅋ 꽃게",
          "original_form": "아 ㅋㅋㅋㅋㅋ 꽃게",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100029079.1.4",
          "form": "저는 어제 바디미스트 샀어용",
          "original_form": "저는 어제 바디미스트 샀어용",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029079.1.5",
          "form": "비누향이 좋더라구요",
          "original_form": "비누향이 좋더라구요",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029079.1.6",
          "form": "오 어디 껄까요?",
          "original_form": "오 어디 껄까요?",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029079.1.7",
          "form": "멈칫 바디미스트예요 ㅎㅎ",
          "original_form": "멈칫 바디미스트예요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029079.1.8",
          "form": "들어본 적 있으세요??",
          "original_form": "들어본 적 있으세요??",
          "speaker_id": "1",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029079.1.9",
          "form": "아 바디미스트가 이름 이였군요 ㅋㅋㅋ",
          "original_form": "아 바디미스트가 이름 이였군요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029079.1.10",
          "form": "들어본거같아요",
          "original_form": "들어본거같아요",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100029079.1.11",
          "form": "ㅎㅎ 지속력도 은근히 좋더라구요",
          "original_form": "ㅎㅎ 지속력도 은근히 좋더라구요",
          "speaker_id": "1",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029079.1.12",
          "form": "선물 받고 싶은 물건이 있을까요??",
          "original_form": "선물 받고 싶은 물건이 있을까요??",
          "speaker_id": "1",
          "time": "20210902 23:50"
        },
        {
          "id": "MDRW2100029079.1.13",
          "form": "저는 요즘 운동기구 필요해서 실내자전거요 ㅋㅋㅋㅋ",
          "original_form": "저는 요즘 운동기구 필요해서 실내자전거요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100029079.1.14",
          "form": "오 저 매일 실내자전거 타는데!",
          "original_form": "오 저 매일 실내자전거 타는데!",
          "speaker_id": "1",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100029079.1.15",
          "form": "헐 추천 해주세요",
          "original_form": "헐 추천 해주세요",
          "speaker_id": "2",
          "time": "20210902 23:57"
        },
        {
          "id": "MDRW2100029079.1.16",
          "form": "집에서 타시죠?",
          "original_form": "집에서 타시죠?",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029079.1.17",
          "form": "살진짜 빠지나요?",
          "original_form": "살진짜 빠지나요?",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100029079.1.18",
          "form": "네 집에서 타요",
          "original_form": "네 집에서 타요",
          "speaker_id": "1",
          "time": "20210903 10:52"
        },
        {
          "id": "MDRW2100029079.1.19",
          "form": "집에 있는 게 엑스 바이크네요 무난하고 좋은 것 같아요",
          "original_form": "집에 있는 게 엑스 바이크네요 무난하고 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100029079.1.20",
          "form": "오 저 그거 살까말까중이에요",
          "original_form": "오 저 그거 살까말까중이에요",
          "speaker_id": "2",
          "time": "20210903 10:53"
        },
        {
          "id": "MDRW2100029079.1.21",
          "form": "살은... 제가 빡세게 타는 건 아니라서 유지하는 데엔 도움이 되는 것 같네요 ㅠㅠ",
          "original_form": "살은... 제가 빡세게 타는 건 아니라서 유지하는 데엔 도움이 되는 것 같네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100029079.1.22",
          "form": "오 티비보면서 타면 좋을것같아요 ㅎㅎ",
          "original_form": "오 티비보면서 타면 좋을것같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100029079.1.23",
          "form": "저보다 열심히 타시면 살 빠질 것 같아요!",
          "original_form": "저보다 열심히 타시면 살 빠질 것 같아요!",
          "speaker_id": "1",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100029079.1.24",
          "form": "그거 심박수를 높게해야 좋데요",
          "original_form": "그거 심박수를 높게해야 좋데요",
          "speaker_id": "2",
          "time": "20210903 10:54"
        },
        {
          "id": "MDRW2100029079.1.25",
          "form": "맞아요 유튜브 보면서 타도 시간 잘 가요 ㅎㅎ",
          "original_form": "맞아요 유튜브 보면서 타도 시간 잘 가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 10:55"
        },
        {
          "id": "MDRW2100029079.1.26",
          "form": "그쵸 근데 전 너무 힘들어서 설렁설렁 타요...",
          "original_form": "그쵸 근데 전 너무 힘들어서 설렁설렁 타요...",
          "speaker_id": "1",
          "time": "20210903 10:55"
        },
        {
          "id": "MDRW2100029079.1.27",
          "form": "맞아요 타면 힘들것같아요 ㅋㅋ 땀이 줄줄난데요",
          "original_form": "맞아요 타면 힘들것같아요 ㅋㅋ 땀이 줄줄난데요",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029079.1.28",
          "form": "땀이줄줄난다더라구요 ㅎㅎ",
          "original_form": "땀이줄줄난다더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 10:56"
        },
        {
          "id": "MDRW2100029079.1.29",
          "form": "조립은 누가하셨어요?",
          "original_form": "조립은 누가하셨어요?",
          "speaker_id": "2",
          "time": "20210903 10:56"
        }
      ]
    }
