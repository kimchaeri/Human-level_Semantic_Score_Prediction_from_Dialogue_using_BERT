      "utterance": [
        {
          "id": "MDRW2100012808.1.1",
          "form": "다이어트 해보셨어요?",
          "original_form": "다이어트 해보셨어요?",
          "speaker_id": "1",
          "time": "20210825 12:56"
        },
        {
          "id": "MDRW2100012808.1.2",
          "form": "아뇨 맘 먹고 제대로 해 본 적은 없는 것 같아요. name1님은요?",
          "original_form": "아뇨 맘 먹고 제대로 해 본 적은 없는 것 같아요. &name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 12:57"
        },
        {
          "id": "MDRW2100012808.1.3",
          "form": "저는 그냥 잘 안먹어서 빼는 정도였어요",
          "original_form": "저는 그냥 잘 안먹어서 빼는 정도였어요",
          "speaker_id": "1",
          "time": "20210825 12:59"
        },
        {
          "id": "MDRW2100012808.1.4",
          "form": "그렇게하니 살은빠지는데 부작용있는거같았어요",
          "original_form": "그렇게하니 살은빠지는데 부작용있는거같았어요",
          "speaker_id": "1",
          "time": "20210825 12:59"
        },
        {
          "id": "MDRW2100012808.1.5",
          "form": "무리하게 하다 건강 상하면 큰일이에요 ㅠ",
          "original_form": "무리하게 하다 건강 상하면 큰일이에요 ㅠ",
          "speaker_id": "2",
          "time": "20210825 13:10"
        },
        {
          "id": "MDRW2100012808.1.6",
          "form": "저는 무리까지는 아니고 조금 굶는 정도였는데",
          "original_form": "저는 무리까지는 아니고 조금 굶는 정도였는데",
          "speaker_id": "1",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100012808.1.7",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100012808.1.8",
          "form": "다시 찔때 좀 안건강해진단 느낌받아서요",
          "original_form": "다시 찔때 좀 안건강해진단 느낌받아서요",
          "speaker_id": "1",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100012808.1.9",
          "form": "전 안 먹으면 속쓰려서 조금이라도 먹으려고 해요",
          "original_form": "전 안 먹으면 속쓰려서 조금이라도 먹으려고 해요",
          "speaker_id": "2",
          "time": "20210825 13:11"
        },
        {
          "id": "MDRW2100012808.1.10",
          "form": "네 그게좋을거에요",
          "original_form": "네 그게좋을거에요",
          "speaker_id": "1",
          "time": "20210825 13:13"
        },
        {
          "id": "MDRW2100012808.1.11",
          "form": "요즘에는 가끔 탄산 마실 때도 슈가프리 제품 찾게 되더라구요",
          "original_form": "요즘에는 가끔 탄산 마실 때도 슈가프리 제품 찾게 되더라구요",
          "speaker_id": "2",
          "time": "20210825 13:15"
        },
        {
          "id": "MDRW2100012808.1.12",
          "form": "맛은 어때요? 그럼 안달아요?",
          "original_form": "맛은 어때요? 그럼 안달아요?",
          "speaker_id": "1",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100012808.1.13",
          "form": "설탕대체물 넣은거에요/",
          "original_form": "설탕대체물 넣은거에요/",
          "speaker_id": "1",
          "time": "20210825 13:16"
        },
        {
          "id": "MDRW2100012808.1.14",
          "form": "대체당 들어간 것도 있고 그냥 일반 탄산수도 있어요",
          "original_form": "대체당 들어간 것도 있고 그냥 일반 탄산수도 있어요",
          "speaker_id": "2",
          "time": "20210825 13:17"
        },
        {
          "id": "MDRW2100012808.1.15",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210825 13:20"
        },
        {
          "id": "MDRW2100012808.1.16",
          "form": "파는 입장에선",
          "original_form": "파는 입장에선",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100012808.1.17",
          "form": "자꾸 먹고싶게해야하니까 음식을 더 자극적으로",
          "original_form": "자꾸 먹고싶게해야하니까 음식을 더 자극적으로",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100012808.1.18",
          "form": "만드는거 같던데요",
          "original_form": "만드는거 같던데요",
          "speaker_id": "1",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100012808.1.19",
          "form": "요즘 너무 매운맛 음식 많아져서 부담스러워요",
          "original_form": "요즘 너무 매운맛 음식 많아져서 부담스러워요",
          "speaker_id": "2",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100012808.1.20",
          "form": "네 저도 매운거 잘 못먹어요",
          "original_form": "네 저도 매운거 잘 못먹어요",
          "speaker_id": "1",
          "time": "20210825 13:25"
        }
      ]
