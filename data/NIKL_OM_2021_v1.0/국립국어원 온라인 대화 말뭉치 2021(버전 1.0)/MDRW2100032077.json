      "utterance": [
        {
          "id": "MDRW2100032077.1.1",
          "form": "소스가 타르타르인가요",
          "original_form": "소스가 타르타르인가요",
          "speaker_id": "2",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100032077.1.2",
          "form": "아 생선까스인가",
          "original_form": "아 생선까스인가",
          "speaker_id": "2",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100032077.1.3",
          "form": "어 생선 가스가 타르타르 였어요",
          "original_form": "어 생선 가스가 타르타르 였어요",
          "speaker_id": "1",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100032077.1.4",
          "form": "타르타르 소스 좋아해요",
          "original_form": "타르타르 소스 좋아해요",
          "speaker_id": "2",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100032077.1.5",
          "form": "저도 타르타르 소스 좋아해요",
          "original_form": "저도 타르타르 소스 좋아해요",
          "speaker_id": "1",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100032077.1.6",
          "form": "뭔가 상큼하고 느끼함을 잡아 줘요",
          "original_form": "뭔가 상큼하고 느끼함을 잡아 줘요",
          "speaker_id": "2",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100032077.1.7",
          "form": "대신동에 첫차 왕돈까스라는 곳이 생겼는데 돈까스가 엄청 커요",
          "original_form": "대신동에 첫차 왕돈까스라는 곳이 생겼는데 돈까스가 엄청 커요",
          "speaker_id": "1",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100032077.1.8",
          "form": "연어 먹을 때 하얀 소스도 좋아요",
          "original_form": "연어 먹을 때 하얀 소스도 좋아요",
          "speaker_id": "2",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100032077.1.9",
          "form": "동대신동역 근처에 소미돈까스라고 있는데주문 받아서 만들기 때문에 나오는 건 늦지만 맛은 괜찮지요",
          "original_form": "동대신동역 근처에 소미돈까스라고 있는데주문 받아서 만들기 때문에 나오는 건 늦지만 맛은 괜찮지요",
          "speaker_id": "1",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100032077.1.10",
          "form": "돈까스는 소스 없어도 맛있죠",
          "original_form": "돈까스는 소스 없어도 맛있죠",
          "speaker_id": "2",
          "time": "20210905 12:09"
        },
        {
          "id": "MDRW2100032077.1.11",
          "form": "튀겼는데 맛이 없을 수가 없죠",
          "original_form": "튀겼는데 맛이 없을 수가 없죠",
          "speaker_id": "2",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100032077.1.12",
          "form": "전에 남자친구가 탕수육을 해 줬는데 소스 안 버렸을 때 고기 바로 튀겨 나왔을 때 먹어 봤는데 정말 맛있더라고요",
          "original_form": "전에 남자친구가 탕수육을 해 줬는데 소스 안 버렸을 때 고기 바로 튀겨 나왔을 때 먹어 봤는데 정말 맛있더라고요",
          "speaker_id": "1",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100032077.1.13",
          "form": "맞아요 튀김은 맛이 없을 수가 없어요",
          "original_form": "맞아요 튀김은 맛이 없을 수가 없어요",
          "speaker_id": "1",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100032077.1.14",
          "form": "맞아요 탕수육도 갇혔을 때 진짜 맛있어요",
          "original_form": "맞아요 탕수육도 갇혔을 때 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100032077.1.15",
          "form": "맞아요 튀겼을때 그 감동은 잊지 못 하겠네요 소스는 별로였거든요",
          "original_form": "맞아요 튀겼을때 그 감동은 잊지 못 하겠네요 소스는 별로였거든요",
          "speaker_id": "1",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100032077.1.16",
          "form": "고기가 좋아야 될 거 같더라고요",
          "original_form": "고기가 좋아야 될 거 같더라고요",
          "speaker_id": "1",
          "time": "20210905 12:10"
        },
        {
          "id": "MDRW2100032077.1.17",
          "form": "갓튀긴 돈까스도 먹어보고 싶네요 지금 당장 right now",
          "original_form": "갓튀긴 돈까스도 먹어보고 싶네요 지금 당장 right now",
          "speaker_id": "1",
          "time": "20210905 12:11"
        },
        {
          "id": "MDRW2100032077.1.18",
          "form": "제주도에 줄서서먹는 돈까스가 있지요",
          "original_form": "제주도에 줄서서먹는 돈까스가 있지요",
          "speaker_id": "2",
          "time": "20210905 12:11"
        },
        {
          "id": "MDRW2100032077.1.19",
          "form": "혹시 방송에 나온 그 집인가요",
          "original_form": "혹시 방송에 나온 그 집인가요",
          "speaker_id": "1",
          "time": "20210905 12:11"
        },
        {
          "id": "MDRW2100032077.1.20",
          "form": "연돈이라고 아시나요",
          "original_form": "연돈이라고 아시나요",
          "speaker_id": "2",
          "time": "20210905 12:11"
        }
      ]
