      "utterance": [
        {
          "id": "MDRW2100009686.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "1",
          "time": "20210823 03:37"
        },
        {
          "id": "MDRW2100009686.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210823 03:37"
        },
        {
          "id": "MDRW2100009686.1.3",
          "form": "혹시 최근에 가고 싶은곳 있어?",
          "original_form": "혹시 최근에 가고 싶은곳 있어?",
          "speaker_id": "1",
          "time": "20210823 03:38"
        },
        {
          "id": "MDRW2100009686.1.4",
          "form": "코로나 상황 을 배제하고 말야",
          "original_form": "코로나 상황 을 배제하고 말야",
          "speaker_id": "1",
          "time": "20210823 03:38"
        },
        {
          "id": "MDRW2100009686.1.5",
          "form": "코로나가 끝나면",
          "original_form": "코로나가 끝나면",
          "speaker_id": "2",
          "time": "20210823 03:38"
        },
        {
          "id": "MDRW2100009686.1.6",
          "form": "제주도에 가보고 싶네요",
          "original_form": "제주도에 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210823 03:38"
        },
        {
          "id": "MDRW2100009686.1.7",
          "form": "해외도 좋고요",
          "original_form": "해외도 좋고요",
          "speaker_id": "2",
          "time": "20210823 03:38"
        },
        {
          "id": "MDRW2100009686.1.8",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210823 03:38"
        },
        {
          "id": "MDRW2100009686.1.9",
          "form": "난 일본에 가보고 싶긴해",
          "original_form": "난 일본에 가보고 싶긴해",
          "speaker_id": "1",
          "time": "20210823 03:39"
        },
        {
          "id": "MDRW2100009686.1.10",
          "form": "애니메이션이나 게임을 좋아하거든",
          "original_form": "애니메이션이나 게임을 좋아하거든",
          "speaker_id": "1",
          "time": "20210823 03:39"
        },
        {
          "id": "MDRW2100009686.1.11",
          "form": "개인적으로 방사능 위험이 있지 않을까 걱정되지만",
          "original_form": "개인적으로 방사능 위험이 있지 않을까 걱정되지만",
          "speaker_id": "1",
          "time": "20210823 03:39"
        },
        {
          "id": "MDRW2100009686.1.12",
          "form": "너무 낙관적인가?",
          "original_form": "너무 낙관적인가?",
          "speaker_id": "1",
          "time": "20210823 03:39"
        },
        {
          "id": "MDRW2100009686.1.13",
          "form": "그래도 가보고 싶은걸",
          "original_form": "그래도 가보고 싶은걸",
          "speaker_id": "1",
          "time": "20210823 03:40"
        },
        {
          "id": "MDRW2100009686.1.14",
          "form": "그래도 가보고 싶으면 가는거죠",
          "original_form": "그래도 가보고 싶으면 가는거죠",
          "speaker_id": "2",
          "time": "20210823 03:40"
        },
        {
          "id": "MDRW2100009686.1.15",
          "form": "그 위험한건 어쩔수 없지만",
          "original_form": "그 위험한건 어쩔수 없지만",
          "speaker_id": "2",
          "time": "20210823 03:40"
        },
        {
          "id": "MDRW2100009686.1.16",
          "form": "일본에 닌텐도 테마파크가 생겼가던데",
          "original_form": "일본에 닌텐도 테마파크가 생겼가던데",
          "speaker_id": "2",
          "time": "20210823 03:40"
        },
        {
          "id": "MDRW2100009686.1.17",
          "form": "저도 꼭가고 싶네요",
          "original_form": "저도 꼭가고 싶네요",
          "speaker_id": "2",
          "time": "20210823 03:40"
        },
        {
          "id": "MDRW2100009686.1.18",
          "form": "디즈니 랜드도 좋고",
          "original_form": "디즈니 랜드도 좋고",
          "speaker_id": "2",
          "time": "20210823 03:40"
        },
        {
          "id": "MDRW2100009686.1.19",
          "form": "면요리나 초밥도 먹고싶고요",
          "original_form": "면요리나 초밥도 먹고싶고요",
          "speaker_id": "2",
          "time": "20210823 03:40"
        },
        {
          "id": "MDRW2100009686.1.20",
          "form": "디즈니랜드는 필수지!",
          "original_form": "디즈니랜드는 필수지!",
          "speaker_id": "1",
          "time": "20210823 03:41"
        },
        {
          "id": "MDRW2100009686.1.21",
          "form": "혹시 술 좋아해?",
          "original_form": "혹시 술 좋아해?",
          "speaker_id": "1",
          "time": "20210823 03:42"
        },
        {
          "id": "MDRW2100009686.1.22",
          "form": "아니요 먹지는 않아요",
          "original_form": "아니요 먹지는 않아요",
          "speaker_id": "2",
          "time": "20210823 03:42"
        },
        {
          "id": "MDRW2100009686.1.23",
          "form": "다만 지식적으로는 좋아해요",
          "original_form": "다만 지식적으로는 좋아해요",
          "speaker_id": "2",
          "time": "20210823 03:42"
        },
        {
          "id": "MDRW2100009686.1.24",
          "form": "금주?",
          "original_form": "금주?",
          "speaker_id": "1",
          "time": "20210823 03:42"
        },
        {
          "id": "MDRW2100009686.1.25",
          "form": "신념쪽이에요",
          "original_form": "신념쪽이에요",
          "speaker_id": "2",
          "time": "20210823 03:42"
        },
        {
          "id": "MDRW2100009686.1.26",
          "form": "특이하네",
          "original_form": "특이하네",
          "speaker_id": "1",
          "time": "20210823 03:42"
        },
        {
          "id": "MDRW2100009686.1.27",
          "form": "종교쪽?",
          "original_form": "종교쪽?",
          "speaker_id": "1",
          "time": "20210823 03:42"
        },
        {
          "id": "MDRW2100009686.1.28",
          "form": "절반정도?",
          "original_form": "절반정도?",
          "speaker_id": "2",
          "time": "20210823 03:43"
        },
        {
          "id": "MDRW2100009686.1.29",
          "form": "나중에 먹을 계획은있어요",
          "original_form": "나중에 먹을 계획은있어요",
          "speaker_id": "2",
          "time": "20210823 03:43"
        },
        {
          "id": "MDRW2100009686.1.30",
          "form": "소주빼고요",
          "original_form": "소주빼고요",
          "speaker_id": "2",
          "time": "20210823 03:43"
        },
        {
          "id": "MDRW2100009686.1.31",
          "form": "ㅋㅋㅋㅋ 그렇긴 하지",
          "original_form": "ㅋㅋㅋㅋ 그렇긴 하지",
          "speaker_id": "1",
          "time": "20210823 03:43"
        },
        {
          "id": "MDRW2100009686.1.32",
          "form": "여행가본데는 있어?",
          "original_form": "여행가본데는 있어?",
          "speaker_id": "1",
          "time": "20210823 03:44"
        },
        {
          "id": "MDRW2100009686.1.33",
          "form": "싱가포르가봤네요",
          "original_form": "싱가포르가봤네요",
          "speaker_id": "2",
          "time": "20210823 03:44"
        },
        {
          "id": "MDRW2100009686.1.34",
          "form": "국내는 조금 돌아는 다녀봤는데",
          "original_form": "국내는 조금 돌아는 다녀봤는데",
          "speaker_id": "2",
          "time": "20210823 03:44"
        },
        {
          "id": "MDRW2100009686.1.35",
          "form": "말하기엔 견식이좀 짧네요",
          "original_form": "말하기엔 견식이좀 짧네요",
          "speaker_id": "2",
          "time": "20210823 03:44"
        },
        {
          "id": "MDRW2100009686.1.36",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "2",
          "time": "20210823 03:44"
        },
        {
          "id": "MDRW2100009686.1.37",
          "form": "나도 내가 사는곳 주변말고는 별로",
          "original_form": "나도 내가 사는곳 주변말고는 별로",
          "speaker_id": "1",
          "time": "20210823 03:44"
        },
        {
          "id": "MDRW2100009686.1.38",
          "form": "그나마 학생때 조금?",
          "original_form": "그나마 학생때 조금?",
          "speaker_id": "1",
          "time": "20210823 03:44"
        },
        {
          "id": "MDRW2100009686.1.39",
          "form": "그래봐야 체험학습 수준이었지만",
          "original_form": "그래봐야 체험학습 수준이었지만",
          "speaker_id": "1",
          "time": "20210823 03:45"
        },
        {
          "id": "MDRW2100009686.1.40",
          "form": "지금도 먹고살기 바쁘고",
          "original_form": "지금도 먹고살기 바쁘고",
          "speaker_id": "1",
          "time": "20210823 03:45"
        },
        {
          "id": "MDRW2100009686.1.41",
          "form": "코로나도 있으니까",
          "original_form": "코로나도 있으니까",
          "speaker_id": "1",
          "time": "20210823 03:45"
        },
        {
          "id": "MDRW2100009686.1.42",
          "form": "상황이 안좋긴하지",
          "original_form": "상황이 안좋긴하지",
          "speaker_id": "1",
          "time": "20210823 03:45"
        },
        {
          "id": "MDRW2100009686.1.43",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210823 03:45"
        },
        {
          "id": "MDRW2100009686.1.44",
          "form": "빨리 코로나가 종식되긴 해야하는데",
          "original_form": "빨리 코로나가 종식되긴 해야하는데",
          "speaker_id": "2",
          "time": "20210823 03:45"
        },
        {
          "id": "MDRW2100009686.1.45",
          "form": "만약 여행을가면",
          "original_form": "만약 여행을가면",
          "speaker_id": "2",
          "time": "20210823 03:46"
        },
        {
          "id": "MDRW2100009686.1.46",
          "form": "로마쪽에 가보고싶네요",
          "original_form": "로마쪽에 가보고싶네요",
          "speaker_id": "2",
          "time": "20210823 03:46"
        },
        {
          "id": "MDRW2100009686.1.47",
          "form": "그리스에 가서 신전도 보고싶고요",
          "original_form": "그리스에 가서 신전도 보고싶고요",
          "speaker_id": "2",
          "time": "20210823 03:46"
        },
        {
          "id": "MDRW2100009686.1.48",
          "form": "난 미국",
          "original_form": "난 미국",
          "speaker_id": "1",
          "time": "20210823 03:46"
        },
        {
          "id": "MDRW2100009686.1.49",
          "form": "기름진거 먹고싶네 ㅋㅋㅋㅋ",
          "original_form": "기름진거 먹고싶네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 03:46"
        },
        {
          "id": "MDRW2100009686.1.50",
          "form": "ㅋㅋㅋㅋ 기름의 나라죠",
          "original_form": "ㅋㅋㅋㅋ 기름의 나라죠",
          "speaker_id": "2",
          "time": "20210823 03:46"
        },
        {
          "id": "MDRW2100009686.1.51",
          "form": "기름이 많으면 민주주의를 선사해주기도 하고 말야",
          "original_form": "기름이 많으면 민주주의를 선사해주기도 하고 말야",
          "speaker_id": "1",
          "time": "20210823 03:47"
        },
        {
          "id": "MDRW2100009686.1.52",
          "form": "하루 빨리 여행가고 싶네요",
          "original_form": "하루 빨리 여행가고 싶네요",
          "speaker_id": "2",
          "time": "20210823 03:47"
        },
        {
          "id": "MDRW2100009686.1.53",
          "form": "사실 자금도 문제지만",
          "original_form": "사실 자금도 문제지만",
          "speaker_id": "2",
          "time": "20210823 03:47"
        },
        {
          "id": "MDRW2100009686.1.54",
          "form": "제주도 정도는 갈수있겠죠",
          "original_form": "제주도 정도는 갈수있겠죠",
          "speaker_id": "2",
          "time": "20210823 03:47"
        },
        {
          "id": "MDRW2100009686.1.55",
          "form": "여객선도 꽤싸고",
          "original_form": "여객선도 꽤싸고",
          "speaker_id": "1",
          "time": "20210823 03:47"
        },
        {
          "id": "MDRW2100009686.1.56",
          "form": "음식 가격은 잘 모르겠네",
          "original_form": "음식 가격은 잘 모르겠네",
          "speaker_id": "1",
          "time": "20210823 03:48"
        },
        {
          "id": "MDRW2100009686.1.57",
          "form": "역시 비쌀려나",
          "original_form": "역시 비쌀려나",
          "speaker_id": "1",
          "time": "20210823 03:48"
        },
        {
          "id": "MDRW2100009686.1.58",
          "form": "아무래도 관광도시니까",
          "original_form": "아무래도 관광도시니까",
          "speaker_id": "2",
          "time": "20210823 03:48"
        },
        {
          "id": "MDRW2100009686.1.59",
          "form": "비싸지 않을까요",
          "original_form": "비싸지 않을까요",
          "speaker_id": "2",
          "time": "20210823 03:48"
        },
        {
          "id": "MDRW2100009686.1.60",
          "form": "회는 안먹을거긴한데",
          "original_form": "회는 안먹을거긴한데",
          "speaker_id": "2",
          "time": "20210823 03:48"
        },
        {
          "id": "MDRW2100009686.1.61",
          "form": "안먹으면 죄일려나 ㅋㅋㅋ",
          "original_form": "안먹으면 죄일려나 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 03:48"
        },
        {
          "id": "MDRW2100009686.1.62",
          "form": "ㅋㅋㅋㅋ 그럴수도?",
          "original_form": "ㅋㅋㅋㅋ 그럴수도?",
          "speaker_id": "1",
          "time": "20210823 03:49"
        },
        {
          "id": "MDRW2100009686.1.63",
          "form": "근데 술 안좋아하면야",
          "original_form": "근데 술 안좋아하면야",
          "speaker_id": "1",
          "time": "20210823 03:49"
        },
        {
          "id": "MDRW2100009686.1.64",
          "form": "굳이 먹을필요는 없지",
          "original_form": "굳이 먹을필요는 없지",
          "speaker_id": "1",
          "time": "20210823 03:49"
        },
        {
          "id": "MDRW2100009686.1.65",
          "form": "아 제주도에 주조장이 몇군데 있을텐데",
          "original_form": "아 제주도에 주조장이 몇군데 있을텐데",
          "speaker_id": "1",
          "time": "20210823 03:49"
        },
        {
          "id": "MDRW2100009686.1.66",
          "form": "최근에 제주맥주라는곳도 있고",
          "original_form": "최근에 제주맥주라는곳도 있고",
          "speaker_id": "1",
          "time": "20210823 03:49"
        },
        {
          "id": "MDRW2100009686.1.67",
          "form": "와인계열도 있을거야",
          "original_form": "와인계열도 있을거야",
          "speaker_id": "1",
          "time": "20210823 03:49"
        },
        {
          "id": "MDRW2100009686.1.68",
          "form": "와 그건 흥미롭네요",
          "original_form": "와 그건 흥미롭네요",
          "speaker_id": "2",
          "time": "20210823 03:49"
        },
