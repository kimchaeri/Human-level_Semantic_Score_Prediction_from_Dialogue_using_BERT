      "utterance": [
        {
          "id": "MDRW2100043952.1.1",
          "form": "오늘 드신 음식 중 뭐가 제일 맛있었나요?",
          "original_form": "오늘 드신 음식 중 뭐가 제일 맛있었나요?",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100043952.1.2",
          "form": "저 만두 먹었는데 맛있었어요 ㅋㅋ",
          "original_form": "저 만두 먹었는데 맛있었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:02"
        },
        {
          "id": "MDRW2100043952.1.3",
          "form": "오 만두 맛있죠",
          "original_form": "오 만두 맛있죠",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100043952.1.4",
          "form": "전 검은콩 들깨 수제비요",
          "original_form": "전 검은콩 들깨 수제비요",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043952.1.5",
          "form": "헐 더 맛난거 드셨네요..",
          "original_form": "헐 더 맛난거 드셨네요..",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043952.1.6",
          "form": "제가 들깨 들어간건 다 좋아해서",
          "original_form": "제가 들깨 들어간건 다 좋아해서",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043952.1.7",
          "form": "몇 달 전에 맛집 알게되서 거기꺼로 자주 먹어요",
          "original_form": "몇 달 전에 맛집 알게되서 거기꺼로 자주 먹어요",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100043952.1.8",
          "form": "들깨의 참 맛을 좀 늦게 알았죠",
          "original_form": "들깨의 참 맛을 좀 늦게 알았죠",
          "speaker_id": "2",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100043952.1.9",
          "form": "구수하잖아요 속도 편하고",
          "original_form": "구수하잖아요 속도 편하고",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043952.1.10",
          "form": "들깨순두부도 맛있는데",
          "original_form": "들깨순두부도 맛있는데",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043952.1.11",
          "form": "순두부랑도 잘 어울리나봐요?",
          "original_form": "순두부랑도 잘 어울리나봐요?",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100043952.1.12",
          "form": "네 완전.. 근데 들깨말고 들은건 없어요 ㅋㅋㅋ",
          "original_form": "네 완전.. 근데 들깨말고 들은건 없어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043952.1.13",
          "form": "둘 다 맛있는 음식이니까 맛있을 것 같아요",
          "original_form": "둘 다 맛있는 음식이니까 맛있을 것 같아요",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043952.1.14",
          "form": "네 고소하니 맛나요 부드럽고",
          "original_form": "네 고소하니 맛나요 부드럽고",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043952.1.15",
          "form": "오 한번 먹어 봐야겠어요",
          "original_form": "오 한번 먹어 봐야겠어요",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043952.1.16",
          "form": "평소에 3끼 다 드시나요?",
          "original_form": "평소에 3끼 다 드시나요?",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100043952.1.17",
          "form": "네 억지로 먹게 되네요",
          "original_form": "네 억지로 먹게 되네요",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043952.1.18",
          "form": "저도 3끼 꼬박꼬박 다 먹어요.. 뭔가 거르면 안될거 같은 느낌에 ㅋㅋ",
