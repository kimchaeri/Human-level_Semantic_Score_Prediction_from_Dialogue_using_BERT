      "utterance": [
        {
          "id": "MDRW2100002630.1.1",
          "form": "좋아하는 음식있으신가요?",
          "original_form": "좋아하는 음식있으신가요?",
          "speaker_id": "1",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002630.1.2",
          "form": "저는 치킨 좋아해요 name1님은요?",
          "original_form": "저는 치킨 좋아해요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210820 14:07"
        },
        {
          "id": "MDRW2100002630.1.3",
          "form": "저는 초밥을좋아해요ㅋㅋㅋ",
          "original_form": "저는 초밥을좋아해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100002630.1.4",
          "form": "무슨치킨좋아하나요",
          "original_form": "무슨치킨좋아하나요",
          "speaker_id": "1",
          "time": "20210820 14:11"
        },
        {
          "id": "MDRW2100002630.1.5",
          "form": "간장치킨 좋아해요! 어떤 초밥 제일 좋아하세요?",
          "original_form": "간장치킨 좋아해요! 어떤 초밥 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002630.1.6",
          "form": "전다좋아해요 초밥은ㅋㅋㅋ",
          "original_form": "전다좋아해요 초밥은ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:20"
        },
        {
          "id": "MDRW2100002630.1.7",
          "form": "광어나 광어지느러미 이런거 더좋아해요~!",
          "original_form": "광어나 광어지느러미 이런거 더좋아해요~!",
          "speaker_id": "1",
          "time": "20210820 14:20"
        },
        {
          "id": "MDRW2100002630.1.8",
          "form": "광어 맛있어요 ㅎㅎ 저는 연어가 제일 좋아요!",
          "original_form": "광어 맛있어요 ㅎㅎ 저는 연어가 제일 좋아요!",
          "speaker_id": "2",
          "time": "20210820 14:22"
        },
        {
          "id": "MDRW2100002630.1.9",
          "form": "저도 연어파이다가 광어파가 되었습니다ㅋㅋㅋ",
          "original_form": "저도 연어파이다가 광어파가 되었습니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:26"
        },
        {
          "id": "MDRW2100002630.1.10",
          "form": "매운건잘드시나요?",
          "original_form": "매운건잘드시나요?",
          "speaker_id": "1",
          "time": "20210820 14:26"
        },
        {
          "id": "MDRW2100002630.1.11",
          "form": "잘은 못 먹는데 좋아해요ㅎㅎ 누누님은요?",
          "original_form": "잘은 못 먹는데 좋아해요ㅎㅎ 누누님은요?",
          "speaker_id": "2",
          "time": "20210820 14:26"
        },
        {
          "id": "MDRW2100002630.1.12",
          "form": "저도 잘은못먹어요ㅋㅋㅋ",
          "original_form": "저도 잘은못먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002630.1.13",
          "form": "엽떡드시나요?",
          "original_form": "엽떡드시나요?",
          "speaker_id": "1",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002630.1.14",
          "form": "엽떡 초보맛 먹어요 ㅋㅋ",
          "original_form": "엽떡 초보맛 먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:33"
        },
        {
          "id": "MDRW2100002630.1.15",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 14:35"
        },
        {
          "id": "MDRW2100002630.1.16",
          "form": "면사리는 우동사리요",
          "original_form": "면사리는 우동사리요",
          "speaker_id": "1",
          "time": "20210820 14:35"
        },
        {
          "id": "MDRW2100002630.1.17",
          "form": "어떤사리좋아하시나요",
          "original_form": "어떤사리좋아하시나요",
          "speaker_id": "1",
          "time": "20210820 14:35"
        },
        {
          "id": "MDRW2100002630.1.18",
          "form": "헐 저도요 우동이 맛있어요 ㅋㅋㅋㅋ",
          "original_form": "헐 저도요 우동이 맛있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:35"
        },
        {
          "id": "MDRW2100002630.1.19",
          "form": "로제맛 드셔보셨나요?",
          "original_form": "로제맛 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210820 14:36"
        },
        {
          "id": "MDRW2100002630.1.20",
          "form": "엽떡에선안먹어봤네요ㅋㅋㅋ 배떡은먹어봤는데",
          "original_form": "엽떡에선안먹어봤네요ㅋㅋㅋ 배떡은먹어봤는데",
