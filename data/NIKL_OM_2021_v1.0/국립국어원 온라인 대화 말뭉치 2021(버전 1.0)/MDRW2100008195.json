      "utterance": [
        {
          "id": "MDRW2100008195.1.1",
          "form": "최근 산 꿀템있음 자랑해주세요",
          "original_form": "최근 산 꿀템있음 자랑해주세요",
          "speaker_id": "1",
          "time": "20210823 11:57"
        },
        {
          "id": "MDRW2100008195.1.2",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 11:57"
        },
        {
          "id": "MDRW2100008195.1.3",
          "form": "꿀템은 머가 있을지...ㅎ",
          "original_form": "꿀템은 머가 있을지...ㅎ",
          "speaker_id": "2",
          "time": "20210823 12:01"
        },
        {
          "id": "MDRW2100008195.1.4",
          "form": "저는 딱히 생각나는게 없네요",
          "original_form": "저는 딱히 생각나는게 없네요",
          "speaker_id": "2",
          "time": "20210823 12:01"
        },
        {
          "id": "MDRW2100008195.1.5",
          "form": "꿀템으로사신거 있으세요?",
          "original_form": "꿀템으로사신거 있으세요?",
          "speaker_id": "2",
          "time": "20210823 12:01"
        },
        {
          "id": "MDRW2100008195.1.6",
          "form": "제가 신일 선풍기 샀단 이야기했던가욥",
          "original_form": "제가 신일 선풍기 샀단 이야기했던가욥",
          "speaker_id": "1",
          "time": "20210823 12:02"
        },
        {
          "id": "MDRW2100008195.1.7",
          "form": "아뇨?",
          "original_form": "아뇨?",
          "speaker_id": "2",
          "time": "20210823 12:05"
        },
        {
          "id": "MDRW2100008195.1.8",
          "form": "무선인가요?",
          "original_form": "무선인가요?",
          "speaker_id": "2",
          "time": "20210823 12:05"
        },
        {
          "id": "MDRW2100008195.1.9",
          "form": "아뇨 그냥 평범한 선풍기네요",
          "original_form": "아뇨 그냥 평범한 선풍기네요",
          "speaker_id": "1",
          "time": "20210823 12:06"
        },
        {
          "id": "MDRW2100008195.1.10",
          "form": "지금 엄청기다중이에요",
          "original_form": "지금 엄청기다중이에요",
          "speaker_id": "1",
          "time": "20210823 12:06"
        },
        {
          "id": "MDRW2100008195.1.11",
          "form": "기다리는 중이에요 ㅋㅋ",
          "original_form": "기다리는 중이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 12:06"
        },
        {
          "id": "MDRW2100008195.1.12",
          "form": "아직 안온거예요?",
          "original_form": "아직 안온거예요?",
          "speaker_id": "2",
          "time": "20210823 12:06"
        },
        {
          "id": "MDRW2100008195.1.13",
          "form": "토욜에 결제했어요 ㅋㅋ",
          "original_form": "토욜에 결제했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 12:07"
        },
        {
          "id": "MDRW2100008195.1.14",
          "form": "아..머가 다른점이 있는줄알았어요",
          "original_form": "아..머가 다른점이 있는줄알았어요",
          "speaker_id": "2",
          "time": "20210823 12:08"
        },
        {
          "id": "MDRW2100008195.1.15",
          "form": "저는 지난주에 홈플에서 쿠폰뿌려서요",
          "original_form": "저는 지난주에 홈플에서 쿠폰뿌려서요",
          "speaker_id": "2",
          "time": "20210823 12:08"
        },
        {
          "id": "MDRW2100008195.1.16",
          "form": "계란10개 990원에 샀어요 ㅎ",
          "original_form": "계란10개 990원에 샀어요 ㅎ",
          "speaker_id": "2",
          "time": "20210823 12:09"
        },
        {
          "id": "MDRW2100008195.1.17",
          "form": "오 얼마에 사셨는지 여쭤봐도 될까요",
          "original_form": "오 얼마에 사셨는지 여쭤봐도 될까요",
          "speaker_id": "1",
          "time": "20210823 12:09"
        },
        {
          "id": "MDRW2100008195.1.18",
          "form": "아하 저는 선풍기를 ㅋ 사셨다는줄 홈플에서",
          "original_form": "아하 저는 선풍기를 ㅋ 사셨다는줄 홈플에서",
          "speaker_id": "1",
          "time": "20210823 12:09"
        },
        {
          "id": "MDRW2100008195.1.19",
          "form": "아 그거 아세요",
          "original_form": "아 그거 아세요",
          "speaker_id": "1",
          "time": "20210823 12:09"
        },
        {
          "id": "MDRW2100008195.1.20",
          "form": "혹시 폰 sk세요",
          "original_form": "혹시 폰 sk세요",
          "speaker_id": "1",
          "time": "20210823 12:09"
        },
        {
          "id": "MDRW2100008195.1.21",
          "form": "머요?",
          "original_form": "머요?",
          "speaker_id": "2",
          "time": "20210823 12:09"
        },
        {
          "id": "MDRW2100008195.1.22",
          "form": "가족결합되있으세요?",
          "original_form": "가족결합되있으세요?",
          "speaker_id": "1",
          "time": "20210823 12:09"
        },
        {
          "id": "MDRW2100008195.1.23",
          "form": "네 sk예요",
          "original_form": "네 sk예요",
          "speaker_id": "2",
          "time": "20210823 12:10"
        },
        {
          "id": "MDRW2100008195.1.24",
          "form": "저희는 sk에 가족결합되있는데",
          "original_form": "저희는 sk에 가족결합되있는데",
          "speaker_id": "1",
          "time": "20210823 12:10"
        },
        {
          "id": "MDRW2100008195.1.25",
          "form": "네 가족결합이죠",
          "original_form": "네 가족결합이죠",
          "speaker_id": "2",
          "time": "20210823 12:10"
        },
        {
          "id": "MDRW2100008195.1.26",
          "form": "그게 1년마다 자동갱신되잖아요",
          "original_form": "그게 1년마다 자동갱신되잖아요",
          "speaker_id": "1",
          "time": "20210823 12:10"
        },
        {
          "id": "MDRW2100008195.1.27",
          "form": "바보같이 몰랐는데",
          "original_form": "바보같이 몰랐는데",
          "speaker_id": "1",
          "time": "20210823 12:10"
        },
        {
          "id": "MDRW2100008195.1.28",
          "form": "그래요 갱신되요?",
          "original_form": "그래요 갱신되요?",
          "speaker_id": "2",
          "time": "20210823 12:10"
        },
        {
          "id": "MDRW2100008195.1.29",
          "form": "그거 그냥 자동갱신되게 안두고",
          "original_form": "그거 그냥 자동갱신되게 안두고",
          "speaker_id": "1",
          "time": "20210823 12:10"
        },
        {
          "id": "MDRW2100008195.1.30",
          "form": "직접전화해서 갱신한다고하면 사은품 주더라고요",
          "original_form": "직접전화해서 갱신한다고하면 사은품 주더라고요",
          "speaker_id": "1",
          "time": "20210823 12:11"
        },
        {
          "id": "MDRW2100008195.1.31",
          "form": "가족결합 1년 해줘서 고맙다고",
          "original_form": "가족결합 1년 해줘서 고맙다고",
          "speaker_id": "1",
          "time": "20210823 12:11"
        },
        {
          "id": "MDRW2100008195.1.32",
          "form": "헉 진짜요?",
          "original_form": "헉 진짜요?",
          "speaker_id": "2",
          "time": "20210823 12:11"
        },
        {
          "id": "MDRW2100008195.1.33",
          "form": "홈플 9만원 상품권을요",
          "original_form": "홈플 9만원 상품권을요",
          "speaker_id": "1",
          "time": "20210823 12:11"
        },
        {
          "id": "MDRW2100008195.1.34",
          "form": "이때까지 10년은 못받아먹었어요",
          "original_form": "이때까지 10년은 못받아먹었어요",
          "speaker_id": "1",
          "time": "20210823 12:11"
        },
        {
          "id": "MDRW2100008195.1.35",
          "form": "대박이네요",
          "original_form": "대박이네요",
          "speaker_id": "2",
          "time": "20210823 12:11"
        },
        {
          "id": "MDRW2100008195.1.36",
          "form": "와 그사실알고 배아파죽는줄",
          "original_form": "와 그사실알고 배아파죽는줄",
          "speaker_id": "1",
          "time": "20210823 12:11"
        },
        {
          "id": "MDRW2100008195.1.37",
          "form": "저도 못받아먹은지 오래네요 ㅜㅠ",
          "original_form": "저도 못받아먹은지 오래네요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210823 12:12"
        },
        {
          "id": "MDRW2100008195.1.38",
          "form": "인터넷만 그런줄알았어요",
          "original_form": "인터넷만 그런줄알았어요",
          "speaker_id": "2",
          "time": "20210823 12:12"
        }
      ]
