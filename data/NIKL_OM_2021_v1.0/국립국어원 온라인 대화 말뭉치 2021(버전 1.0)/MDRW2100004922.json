      "utterance": [
        {
          "id": "MDRW2100004922.1.1",
          "form": "저녁드셨나요?",
          "original_form": "저녁드셨나요?",
          "speaker_id": "1",
          "time": "20210821 19:18"
        },
        {
          "id": "MDRW2100004922.1.2",
          "form": "저는 오늘 점심에 삼겹살과 비빔국수를 든든히 먹어서",
          "original_form": "저는 오늘 점심에 삼겹살과 비빔국수를 든든히 먹어서",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004922.1.3",
          "form": "저녁은 간단히 샐러드 먹었어요",
          "original_form": "저녁은 간단히 샐러드 먹었어요",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004922.1.4",
          "form": "와! 저도 삼겹살먹었는데!! 저녁은 감자전 먹으려구요ㅋ",
          "original_form": "와! 저도 삼겹살먹었는데!! 저녁은 감자전 먹으려구요ㅋ",
          "speaker_id": "1",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004922.1.5",
          "form": "오호 오늘은 삼겹살데이인가요!!",
          "original_form": "오호 오늘은 삼겹살데이인가요!!",
          "speaker_id": "2",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004922.1.6",
          "form": "감자전도 맛있겠어요",
          "original_form": "감자전도 맛있겠어요",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004922.1.7",
          "form": "그러게요 비와서그른가봐요 자꾸 기름진것들만 먹어요ㅋ",
          "original_form": "그러게요 비와서그른가봐요 자꾸 기름진것들만 먹어요ㅋ",
          "speaker_id": "1",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004922.1.8",
          "form": "비 오는 날에는 기름 지글지글 소리가 당기죠!",
          "original_form": "비 오는 날에는 기름 지글지글 소리가 당기죠!",
          "speaker_id": "2",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004922.1.9",
          "form": "그죠 그래서그런가봐요 신기해요ㅋ\r\n맛집이나 카페도 좋아하시나요?",
          "original_form": "그죠 그래서그런가봐요 신기해요ㅋ\r\n맛집이나 카페도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210821 19:28"
        },
        {
          "id": "MDRW2100004922.1.10",
          "form": "네 맛집탐방 하는 것 좋아하는데",
          "original_form": "네 맛집탐방 하는 것 좋아하는데",
          "speaker_id": "2",
          "time": "20210821 19:28"
        },
        {
          "id": "MDRW2100004922.1.11",
          "form": "요즘은 코로나 땜에 자제중이예요",
          "original_form": "요즘은 코로나 땜에 자제중이예요",
          "speaker_id": "2",
          "time": "20210821 19:29"
        },
        {
          "id": "MDRW2100004922.1.12",
          "form": "근데 요즘은 맛집이 배달/택배 서비스도 많이 해서",
          "original_form": "근데 요즘은 맛집이 배달/택배 서비스도 많이 해서",
          "speaker_id": "2",
          "time": "20210821 19:29"
        },
        {
          "id": "MDRW2100004922.1.13",
          "form": "몇번 시켜먹기도 했어요",
          "original_form": "몇번 시켜먹기도 했어요",
          "speaker_id": "2",
          "time": "20210821 19:29"
        },
        {
          "id": "MDRW2100004922.1.14",
          "form": "코로나땜에 맛집이나 카페 가기좀그렇죠ㅜㅜ",
          "original_form": "코로나땜에 맛집이나 카페 가기좀그렇죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 19:31"
        },
        {
          "id": "MDRW2100004922.1.15",
          "form": "네 아무래도",
          "original_form": "네 아무래도",
          "speaker_id": "2",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004922.1.16",
          "form": "특히 맛집은 코로나 시국에도 바글거려서",
          "original_form": "특히 맛집은 코로나 시국에도 바글거려서",
          "speaker_id": "2",
          "time": "20210821 19:44"
        },
        {
          "id": "MDRW2100004922.1.17",
          "form": "가기가 좀 꺼려져요",
          "original_form": "가기가 좀 꺼려져요",
          "speaker_id": "2",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004922.1.18",
          "form": "맞아요ㅠ자영업 어렸다든데 맛집은 제외인걸로ㅋㅋ",
          "original_form": "맞아요ㅠ자영업 어렸다든데 맛집은 제외인걸로ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:45"
        },
        {
          "id": "MDRW2100004922.1.19",
          "form": "네 맛집은 오히려 더 잘된대요",
          "original_form": "네 맛집은 오히려 더 잘된대요",
          "speaker_id": "2",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004922.1.20",
          "form": "사람들도 많이 오고",
          "original_form": "사람들도 많이 오고",
          "speaker_id": "2",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004922.1.21",
          "form": "택배/배달 수도 많아서",
          "original_form": "택배/배달 수도 많아서",
          "speaker_id": "2",
          "time": "20210821 19:46"
        },
        {
          "id": "MDRW2100004922.1.22",
          "form": "그렇군요 코로나라 사람 없을줄알고 더 많나싶기도하고ㅎㅎ",
          "original_form": "그렇군요 코로나라 사람 없을줄알고 더 많나싶기도하고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:46"
        }
      ]
    }
