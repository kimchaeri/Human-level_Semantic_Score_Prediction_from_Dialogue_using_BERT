      "utterance": [
        {
          "id": "MDRW2100006409.1.1",
          "form": "남편은 안중에도 없나여?",
          "original_form": "남편은 안중에도 없나여?",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100006409.1.2",
          "form": "음.......",
          "original_form": "음.......",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100006409.1.3",
          "form": "그러기엔\r\n너무 덩치가 산만해서",
          "original_form": "그러기엔\r\n너무 덩치가 산만해서",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100006409.1.4",
          "form": "걸리적거리는군요",
          "original_form": "걸리적거리는군요",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100006409.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100006409.1.6",
          "form": "왜그래요",
          "original_form": "왜그래요",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100006409.1.7",
          "form": "ㅋㅋ아니 저는 통금도 있었고",
          "original_form": "ㅋㅋ아니 저는 통금도 있었고",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100006409.1.8",
          "form": "엄빠랑 오빠가 너무 구속해서",
          "original_form": "엄빠랑 오빠가 너무 구속해서",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100006409.1.9",
          "form": "결혼하면 자유로워질줄 알았는데",
          "original_form": "결혼하면 자유로워질줄 알았는데",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100006409.1.10",
          "form": "더 답답하게 사는거 같아요",
          "original_form": "더 답답하게 사는거 같아요",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100006409.1.11",
          "form": "코시국이라고\r\n아무도 못만나게 하더니만",
          "original_form": "코시국이라고\r\n아무도 못만나게 하더니만",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100006409.1.12",
          "form": "지금 답답해요?",
          "original_form": "지금 답답해요?",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100006409.1.13",
          "form": "직장동료 다른 부서 간다고\r\n오늘 밥먹고 온다는데",
          "original_form": "직장동료 다른 부서 간다고\r\n오늘 밥먹고 온다는데",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100006409.1.14",
          "form": "그동안 참았던게 터졌나봐요ㅋㅋ",
          "original_form": "그동안 참았던게 터졌나봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100006409.1.15",
          "form": "싸우셨어요?",
          "original_form": "싸우셨어요?",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006409.1.16",
          "form": "아뇨 ㅋㅋ 안싸웠어요",
          "original_form": "아뇨 ㅋㅋ 안싸웠어요",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006409.1.17",
          "form": "한번도 남편이 뭐한다했을때",
          "original_form": "한번도 남편이 뭐한다했을때",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006409.1.18",
          "form": "노 라고 한적 없었거든요 ㅋㅋㅈ",
          "original_form": "노 라고 한적 없었거든요 ㅋㅋㅈ",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100006409.1.19",
          "form": "근데 오늘은 왜 그랬을까요 제가 ㅋㅋ",
          "original_form": "근데 오늘은 왜 그랬을까요 제가 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006409.1.20",
          "form": "참았던게 터졌다길래",
          "original_form": "참았던게 터졌다길래",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006409.1.21",
          "form": "그게 피자빵인가요",
          "original_form": "그게 피자빵인가요",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006409.1.22",
          "form": "밥먹는다고 해서",
          "original_form": "밥먹는다고 해서",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006409.1.23",
          "form": "노라고 했어요?",
          "original_form": "노라고 했어요?",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006409.1.24",
          "form": "제 친구 수원으로 이사가게 되서",
          "original_form": "제 친구 수원으로 이사가게 되서",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100006409.1.25",
          "form": "가면 보기힘들듯해서 만난다 했을때",
          "original_form": "가면 보기힘들듯해서 만난다 했을때",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100006409.1.26",
          "form": "코시국에 누굴 만나냐고 집에 있으라더니",
          "original_form": "코시국에 누굴 만나냐고 집에 있으라더니",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100006409.1.27",
          "form": "본인은 회사동료들이랑 저녁먹고\r\n올때도 많고 ㅎㅎ",
          "original_form": "본인은 회사동료들이랑 저녁먹고\r\n올때도 많고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100006409.1.28",
          "form": "아웃사이더보다 더 속사포로",
          "original_form": "아웃사이더보다 더 속사포로",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100006409.1.29",
          "form": "얘기했더니 남편이 당황했어쇼",
          "original_form": "얘기했더니 남편이 당황했어쇼",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006409.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006409.1.31",
          "form": "당연히 먹고오라할줄 알았다고",
          "original_form": "당연히 먹고오라할줄 알았다고",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006409.1.32",
          "form": "응 그래서 잘 먹고 오라고^^",
          "original_form": "응 그래서 잘 먹고 오라고^^",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006409.1.33",
          "form": "코시국이고 뭐고 이제 없어요",
          "original_form": "코시국이고 뭐고 이제 없어요",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006409.1.34",
          "form": "했더니 아무말도 못하고",
          "original_form": "했더니 아무말도 못하고",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006409.1.35",
          "form": "위드코로나 한다는데",
          "original_form": "위드코로나 한다는데",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006409.1.36",
          "form": "전화도 못 끊고 있더라구요",
          "original_form": "전화도 못 끊고 있더라구요",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006409.1.37",
          "form": "언제까지 코로나땜에 아무것도 못하나요",
          "original_form": "언제까지 코로나땜에 아무것도 못하나요",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100006409.1.38",
          "form": "그런거 엄청 예민해요 ㅋㅋ",
          "original_form": "그런거 엄청 예민해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100006409.1.39",
          "form": "가을타는거 아니라 갑갑해서 그러는거 아니에요?",
          "original_form": "가을타는거 아니라 갑갑해서 그러는거 아니에요?",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100006409.1.40",
          "form": "맞아요 ㅋㅋ 그게 정답일수도",
          "original_form": "맞아요 ㅋㅋ 그게 정답일수도",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100006409.1.41",
          "form": "친한 언니가 저보고",
          "original_form": "친한 언니가 저보고",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100006409.1.42",
          "form": "우울증 올거 같데요",
          "original_form": "우울증 올거 같데요",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100006409.1.43",
          "form": "친구도 만나고 하세요",
          "original_form": "친구도 만나고 하세요",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100006409.1.44",
          "form": "엄청 참고 사는거 같다고",
          "original_form": "엄청 참고 사는거 같다고",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100006409.1.45",
          "form": "못만나게 해서 ㅎㅎ",
          "original_form": "못만나게 해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100006409.1.46",
          "form": "예전 같았으면\r\n아 뭐래 내가 누굴 만나던ㅋㅋ",
          "original_form": "예전 같았으면\r\n아 뭐래 내가 누굴 만나던ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100006409.1.47",
          "form": "연애때도",
          "original_form": "연애때도",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100006409.1.48",
          "form": "이러고 나갔을텐데",
          "original_form": "이러고 나갔을텐데",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100006409.1.49",
          "form": "그러셨어요?",
          "original_form": "그러셨어요?",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100006409.1.50",
          "form": "아이들 있으니 싸우거나 다투는거\r\n보이기 싫으니 참게 되더라구여",
          "original_form": "아이들 있으니 싸우거나 다투는거\r\n보이기 싫으니 참게 되더라구여",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100006409.1.51",
          "form": "참는건 참는건데",
          "original_form": "참는건 참는건데",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100006409.1.52",
          "form": "썸탈때부터도 좀 구속하는 타입이었져 ㅋ",
          "original_form": "썸탈때부터도 좀 구속하는 타입이었져 ㅋ",
          "speaker_id": "1",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100006409.1.53",
          "form": "해소는 해야죠",
          "original_form": "해소는 해야죠",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100006409.1.54",
          "form": "육아스트레스",
          "original_form": "육아스트레스",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100006409.1.55",
          "form": "네:) 안그래도 그래야겠어서",
          "original_form": "네:) 안그래도 그래야겠어서",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100006409.1.56",
          "form": "문센 신청해서 어제 다녀왔어요!",
          "original_form": "문센 신청해서 어제 다녀왔어요!",
          "speaker_id": "1",
          "time": "20210908 00:03"
        }
      ]
    }
