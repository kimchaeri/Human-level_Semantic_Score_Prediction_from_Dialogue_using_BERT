      "utterance": [
        {
          "id": "MDRW2100033664.1.1",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100033664.1.2",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100033664.1.3",
          "form": "벌레키워요",
          "original_form": "벌레키워요",
          "speaker_id": "2",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100033664.1.4",
          "form": "전 밀월 거저리 키워요",
          "original_form": "전 밀월 거저리 키워요",
          "speaker_id": "1",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100033664.1.5",
          "form": "도마뱀이 있어서요 ㅎㅎ",
          "original_form": "도마뱀이 있어서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100033664.1.6",
          "form": "어떤 벌레 키우세요",
          "original_form": "어떤 벌레 키우세요",
          "speaker_id": "1",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100033664.1.7",
          "form": "밀웜 키우다 보니 거저리되서 어쩌다 엄청 많아졌어요",
          "original_form": "밀웜 키우다 보니 거저리되서 어쩌다 엄청 많아졌어요",
          "speaker_id": "1",
          "time": "20210824 22:46"
        },
        {
          "id": "MDRW2100033664.1.8",
          "form": "오 저도 밀웜 키웠었어요 ㅎㅎㅎ 거저리 귀여운데",
          "original_form": "오 저도 밀웜 키웠었어요 ㅎㅎㅎ 거저리 귀여운데",
          "speaker_id": "2",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100033664.1.9",
          "form": "지금은 사슴벌레 장수풍뎅이요",
          "original_form": "지금은 사슴벌레 장수풍뎅이요",
          "speaker_id": "2",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100033664.1.10",
          "form": "귀엽죠 근데 너무 번식이 잘돼요",
          "original_form": "귀엽죠 근데 너무 번식이 잘돼요",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100033664.1.11",
          "form": "이젠 좀 걱정돼요",
          "original_form": "이젠 좀 걱정돼요",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100033664.1.12",
          "form": "저 장수 풍뎅이 키워 봤어요",
          "original_form": "저 장수 풍뎅이 키워 봤어요",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100033664.1.13",
          "form": "도마뱀이 궁금해요\r\n 손많이 가나요?",
          "original_form": "도마뱀이 궁금해요\r\n 손많이 가나요?",
          "speaker_id": "2",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100033664.1.14",
          "form": "아 이녀석들 딴건 괜찮은데",
          "original_form": "아 이녀석들 딴건 괜찮은데",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100033664.1.15",
          "form": "밥을 안먹어요",
          "original_form": "밥을 안먹어요",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100033664.1.16",
          "form": "아 아이 키우듯이 밥안먹으면 걱정인가요??",
          "original_form": "아 아이 키우듯이 밥안먹으면 걱정인가요??",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100033664.1.17",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100033664.1.18",
          "form": "저 정말 하루종일 시도해요 밥주려고",
          "original_form": "저 정말 하루종일 시도해요 밥주려고",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100033664.1.19",
          "form": "그렇구나 역시 생명들 돌보는건 쉽지않아요",
          "original_form": "그렇구나 역시 생명들 돌보는건 쉽지않아요",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100033664.1.20",
          "form": "얼마나 되셨어요?? 키은지",
          "original_form": "얼마나 되셨어요?? 키은지",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100033664.1.21",
          "form": "2년요",
          "original_form": "2년요",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100033664.1.22",
          "form": "네 지금도 겁나요 잘 못키울까봐요",
          "original_form": "네 지금도 겁나요 잘 못키울까봐요",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100033664.1.23",
          "form": "2년이나 되도 그런마음이군요.. 함부러 들이면 안되겠어요ㅡ 전 쉽게 생각했어요",
          "original_form": "2년이나 되도 그런마음이군요.. 함부러 들이면 안되겠어요ㅡ 전 쉽게 생각했어요",
          "speaker_id": "2",
          "time": "20210824 22:56"
        },
        {
          "id": "MDRW2100033664.1.24",
          "form": "맞아요 저도 지금 그생각요",
          "original_form": "맞아요 저도 지금 그생각요",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100033664.1.25",
          "form": "아 이젠 앞으로 8년은 더 살텐데",
          "original_form": "아 이젠 앞으로 8년은 더 살텐데",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100033664.1.26",
          "form": "걱정돼요",
          "original_form": "걱정돼요",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100033664.1.27",
          "form": "암수 한쌍 기르세요?",
          "original_form": "암수 한쌍 기르세요?",
          "speaker_id": "2",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100033664.1.28",
          "form": "아뇨 암2이요",
          "original_form": "아뇨 암2이요",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100033664.1.29",
          "form": "싸운다해서 동성으로요",
          "original_form": "싸운다해서 동성으로요",
          "speaker_id": "1",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100033664.1.30",
          "form": "목욕이나 흙갈이 이런것도 하나요?",
          "original_form": "목욕이나 흙갈이 이런것도 하나요?",
          "speaker_id": "2",
          "time": "20210824 22:59"
        },
        {
          "id": "MDRW2100033664.1.31",
          "form": "예하죠",
          "original_form": "예하죠",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100033664.1.32",
          "form": "목욕 일주일한번씩요",
          "original_form": "목욕 일주일한번씩요",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100033664.1.33",
          "form": "베딩른 나무 조각 같은건데",
          "original_form": "베딩른 나무 조각 같은건데",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100033664.1.34",
          "form": "4개월한번요",
          "original_form": "4개월한번요",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100033664.1.35",
          "form": "아 나무조각 가는건 주기가 그래도 꽤 기네요.",
          "original_form": "아 나무조각 가는건 주기가 그래도 꽤 기네요.",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100033664.1.36",
          "form": "파충류 키워보는게 어려서부터 소원중 하나였어요",
          "original_form": "파충류 키워보는게 어려서부터 소원중 하나였어요",
          "speaker_id": "2",
