      "utterance": [
        {
          "id": "MDRW2100023167.1.1",
          "form": "그렇습니다요...",
          "original_form": "그렇습니다요...",
          "speaker_id": "2",
          "time": "20210829 22:24"
        },
        {
          "id": "MDRW2100023167.1.2",
          "form": "ㅋㅋㅋ 오늘은 과일 같은거로만 떼우심?",
          "original_form": "ㅋㅋㅋ 오늘은 과일 같은거로만 떼우심?",
          "speaker_id": "2",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100023167.1.3",
          "form": "저 오늘 점심이 짬뽕이였거든요 하하",
          "original_form": "저 오늘 점심이 짬뽕이였거든요 하하",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023167.1.4",
          "form": "양심상 저녁은 과일샐러드요 ..",
          "original_form": "양심상 저녁은 과일샐러드요 ..",
          "speaker_id": "1",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100023167.1.5",
          "form": "과일도 탄수화물이니까.. \n짬뽕? 배달이요?",
          "original_form": "과일도 탄수화물이니까.. \n짬뽕? 배달이요?",
          "speaker_id": "2",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023167.1.6",
          "form": "아니용 포장했습니다ㅎㅎ",
          "original_form": "아니용 포장했습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:27"
        },
        {
          "id": "MDRW2100023167.1.7",
          "form": "탄수+탄수란 .. 최고죠",
          "original_form": "탄수+탄수란 .. 최고죠",
          "speaker_id": "1",
          "time": "20210829 22:28"
        },
        {
          "id": "MDRW2100023167.1.8",
          "form": "날도 더운데.. 매운게 목구녕으로 넘어가던가여?",
          "original_form": "날도 더운데.. 매운게 목구녕으로 넘어가던가여?",
          "speaker_id": "2",
          "time": "20210829 22:29"
        },
        {
          "id": "MDRW2100023167.1.9",
          "form": "이열치열 상당히 좋아합니다요",
          "original_form": "이열치열 상당히 좋아합니다요",
          "speaker_id": "1",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023167.1.10",
          "form": "근데 한여름은 좀 . . 거부할께요",
          "original_form": "근데 한여름은 좀 . . 거부할께요",
          "speaker_id": "1",
          "time": "20210829 22:31"
        },
        {
          "id": "MDRW2100023167.1.11",
          "form": "아.. 매운음식을 원래 잘드십니까요?",
          "original_form": "아.. 매운음식을 원래 잘드십니까요?",
          "speaker_id": "2",
          "time": "20210829 22:32"
        },
        {
          "id": "MDRW2100023167.1.12",
          "form": "넵 ㅎㅎ 전 매운거 먹고 스트레스 풉니다요 ㅎㅎ",
          "original_form": "넵 ㅎㅎ 전 매운거 먹고 스트레스 풉니다요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:33"
        },
        {
          "id": "MDRW2100023167.1.13",
          "form": "저.. 그.. 매운거 먹음 ..(๑Θ๑)",
          "original_form": "저.. 그.. 매운거 먹음 ..(๑{emoji:❛}Θ{emoji:❛}๑)",
          "speaker_id": "2",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023167.1.14",
          "form": "다음날..",
          "original_form": "다음날..",
          "speaker_id": "2",
          "time": "20210829 22:34"
        },
        {
          "id": "MDRW2100023167.1.15",
          "form": "혹시... 제가 생각하는 그것인가요",
          "original_form": "혹시... 제가 생각하는 그것인가요",
          "speaker_id": "1",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023167.1.16",
          "form": "(꒪ȏ꒪;) 똥.꼬.에 불나지 않나여?",
          "original_form": "(꒪ȏ꒪;) 똥.꼬.에 불나지 않나여?",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023167.1.17",
          "form": "옙 맞습니다요 .. 하지만 이겨내야죠 !",
          "original_form": "옙 맞습니다요 .. 하지만 이겨내야죠 !",
          "speaker_id": "1",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100023167.1.18",
          "form": "매운 음식은 주로 어떤거요? 김치찌게? 된장찌게?",
          "original_form": "매운 음식은 주로 어떤거요? 김치찌게? 된장찌게?",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023167.1.19",
          "form": "전 된장찌개 좋아해요 ㅋㅋㅋ",
