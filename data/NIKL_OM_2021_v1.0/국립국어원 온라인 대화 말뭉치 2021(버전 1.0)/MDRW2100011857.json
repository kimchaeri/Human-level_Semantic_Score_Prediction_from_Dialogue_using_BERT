      "utterance": [
        {
          "id": "MDRW2100011857.1.1",
          "form": "재택근무 해보셨나요",
          "original_form": "재택근무 해보셨나요",
          "speaker_id": "1",
          "time": "20210824 16:46"
        },
        {
          "id": "MDRW2100011857.1.2",
          "form": "저는 작년에만 해봤어요ㅠ",
          "original_form": "저는 작년에만 해봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210824 17:03"
        },
        {
          "id": "MDRW2100011857.1.3",
          "form": "몇개월 해보셨어요?",
          "original_form": "몇개월 해보셨어요?",
          "speaker_id": "1",
          "time": "20210824 17:04"
        },
        {
          "id": "MDRW2100011857.1.4",
          "form": "1개월 정도요! 그 이후에는 얄짤 없더라구요",
          "original_form": "1개월 정도요! 그 이후에는 얄짤 없더라구요",
          "speaker_id": "2",
          "time": "20210824 17:08"
        },
        {
          "id": "MDRW2100011857.1.5",
          "form": "ㅋㅋㅋ 재택근무가 입맛에 맞으셯군요",
          "original_form": "ㅋㅋㅋ 재택근무가 입맛에 맞으셯군요",
          "speaker_id": "1",
          "time": "20210824 17:09"
        },
        {
          "id": "MDRW2100011857.1.6",
          "form": "일단 출퇴근이 없으니 여유가 늘더라구요",
          "original_form": "일단 출퇴근이 없으니 여유가 늘더라구요",
          "speaker_id": "2",
          "time": "20210824 17:10"
        },
        {
          "id": "MDRW2100011857.1.7",
          "form": "상사 없으니까 좋죠",
          "original_form": "상사 없으니까 좋죠",
          "speaker_id": "1",
          "time": "20210824 17:20"
        },
        {
          "id": "MDRW2100011857.1.8",
          "form": "넘사벽장점",
          "original_form": "넘사벽장점",
          "speaker_id": "1",
          "time": "20210824 17:20"
        },
        {
          "id": "MDRW2100011857.1.9",
          "form": "아 그건 정말 인정이요...하지만 연락이 온다죠ㅠㅠ",
          "original_form": "아 그건 정말 인정이요...하지만 연락이 온다죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011857.1.10",
          "form": "으 정말싫네요",
          "original_form": "으 정말싫네요",
          "speaker_id": "1",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100011857.1.11",
          "form": "뭐먹을지 고민하는게 힘들어요",
          "original_form": "뭐먹을지 고민하는게 힘들어요",
          "speaker_id": "1",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100011857.1.12",
          "form": "회사에서는 급식 주는데ㅋㅋ",
          "original_form": "회사에서는 급식 주는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100011857.1.13",
          "form": "급식...집에서도 급식이 필요해요ㅠ",
          "original_form": "급식...집에서도 급식이 필요해요ㅠ",
          "speaker_id": "2",
          "time": "20210824 19:47"
        },
        {
          "id": "MDRW2100011857.1.14",
          "form": "영양균형 맞춘 음식 소중해요..",
          "original_form": "영양균형 맞춘 음식 소중해요..",
          "speaker_id": "1",
          "time": "20210824 19:47"
        },
        {
          "id": "MDRW2100011857.1.15",
          "form": "집에서는 아무렇게나 먹고 영양제 챙겨먹는 게 더 편하죠ㅋㅋ",
          "original_form": "집에서는 아무렇게나 먹고 영양제 챙겨먹는 게 더 편하죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:50"
        },
