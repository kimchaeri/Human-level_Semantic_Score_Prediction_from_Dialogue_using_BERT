      "utterance": [
        {
          "id": "MDRW2100001738.1.1",
          "form": "오늘 홍삼먹었어?",
          "original_form": "오늘 홍삼먹었어?",
          "speaker_id": "2",
          "time": "20210819 23:57"
        },
        {
          "id": "MDRW2100001738.1.2",
          "form": "응. 먹었지",
          "original_form": "응. 먹었지",
          "speaker_id": "1",
          "time": "20210819 23:58"
        },
        {
          "id": "MDRW2100001738.1.3",
          "form": "올~~~~이제 안챙겨줘도 잘먹네.  이뿌네^^",
          "original_form": "올~~~~이제 안챙겨줘도 잘먹네.  이뿌네^^",
          "speaker_id": "2",
          "time": "20210819 23:58"
        },
        {
          "id": "MDRW2100001738.1.4",
          "form": "ㅋ 딴건안먹어도 홍삼은먹지. 다먹음 또 사주는건가?",
          "original_form": "ㅋ 딴건안먹어도 홍삼은먹지. 다먹음 또 사주는건가?",
          "speaker_id": "1",
          "time": "20210819 23:59"
        },
        {
          "id": "MDRW2100001738.1.5",
          "form": "당근~~ 그거하나 먹는데 또 사야지",
          "original_form": "당근~~ 그거하나 먹는데 또 사야지",
          "speaker_id": "2",
          "time": "20210819 23:59"
        },
        {
          "id": "MDRW2100001738.1.6",
          "form": "열씨미 출석해서 포인트모으고^^",
          "original_form": "열씨미 출석해서 포인트모으고^^",
          "speaker_id": "2",
          "time": "20210819 23:59"
        },
        {
          "id": "MDRW2100001738.1.7",
          "form": "그래 열심히 해~~",
          "original_form": "그래 열심히 해~~",
          "speaker_id": "1",
          "time": "20210819 23:59"
        },
        {
          "id": "MDRW2100001738.1.8",
          "form": "넌 야식좀 먹지말고. 병원에서 살빼라고했다며",
          "original_form": "넌 야식좀 먹지말고. 병원에서 살빼라고했다며",
          "speaker_id": "1",
          "time": "20210820 00:00"
        },
        {
          "id": "MDRW2100001738.1.9",
          "form": "응ㅠ 슬프다.  삶의 낙이없다",
          "original_form": "응ㅠ 슬프다.  삶의 낙이없다",
          "speaker_id": "2",
          "time": "20210820 00:01"
        },
        {
          "id": "MDRW2100001738.1.10",
          "form": "그래도 요즘에 쫌 빠졌다^^",
          "original_form": "그래도 요즘에 쫌 빠졌다^^",
          "speaker_id": "2",
          "time": "20210820 00:01"
        },
        {
          "id": "MDRW2100001738.1.11",
          "form": "어디가?",
          "original_form": "어디가?",
          "speaker_id": "1",
          "time": "20210820 00:01"
        },
        {
          "id": "MDRW2100001738.1.12",
          "form": "뭐래~~ 5키로빠진거야",
          "original_form": "뭐래~~ 5키로빠진거야",
          "speaker_id": "2",
          "time": "20210820 00:02"
        },
        {
          "id": "MDRW2100001738.1.13",
          "form": "안믿기겠지만~~",
          "original_form": "안믿기겠지만~~",
          "speaker_id": "2",
          "time": "20210820 00:02"
        },
        {
          "id": "MDRW2100001738.1.14",
          "form": "잘했다.  쫌만더빼자",
          "original_form": "잘했다.  쫌만더빼자",
          "speaker_id": "1",
          "time": "20210820 00:02"
        },
        {
          "id": "MDRW2100001738.1.15",
          "form": "응 그래야지.  자꾸붓고 간수치도높고 불안은하네",
          "original_form": "응 그래야지.  자꾸붓고 간수치도높고 불안은하네",
          "speaker_id": "2",
          "time": "20210820 00:02"
        },
        {
          "id": "MDRW2100001738.1.16",
          "form": "건강하게 살아야하는데",
          "original_form": "건강하게 살아야하는데",
          "speaker_id": "2",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100001738.1.17",
          "form": "나랑 저녁에 쫌걷자. 이제 쫌 시원해졌으니까",
          "original_form": "나랑 저녁에 쫌걷자. 이제 쫌 시원해졌으니까",
          "speaker_id": "1",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100001738.1.18",
          "form": "누워만있지말고",
          "original_form": "누워만있지말고",
          "speaker_id": "1",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100001738.1.19",
          "form": "응ㅠ 울신랑뿐이네.  나 걱정해주는건",
          "original_form": "응ㅠ 울신랑뿐이네.  나 걱정해주는건",
          "speaker_id": "2",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100001738.1.20",
          "form": "건강하게 오래 삽시다~",
          "original_form": "건강하게 오래 삽시다~",
          "speaker_id": "1",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100001738.1.21",
          "form": "응. 오메가랑 비타민이랑 잘챙겨먹고",
          "original_form": "응. 오메가랑 비타민이랑 잘챙겨먹고",
          "speaker_id": "2",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100001738.1.22",
          "form": "샐러드도 먹고~~ 기름진거 덜먹음 살도빠지고 좋을꺼야",
          "original_form": "샐러드도 먹고~~ 기름진거 덜먹음 살도빠지고 좋을꺼야",
          "speaker_id": "2",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100001738.1.23",
          "form": "나도 같이 빼야지 배가 장난아니다",
          "original_form": "나도 같이 빼야지 배가 장난아니다",
          "speaker_id": "1",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100001738.1.24",
          "form": "ㅎㅎ 자기가 더 살찌긴했어~",
          "original_form": "ㅎㅎ 자기가 더 살찌긴했어~",
