      "utterance": [
        {
          "id": "MDRW2100029780.1.1",
          "form": "일은 적성에 맞으세요??",
          "original_form": "일은 적성에 맞으세요??",
          "speaker_id": "2",
          "time": "20210903 13:28"
        },
        {
          "id": "MDRW2100029780.1.2",
          "form": "재미있거나 하지는 않은데",
          "original_form": "재미있거나 하지는 않은데",
          "speaker_id": "1",
          "time": "20210903 13:33"
        },
        {
          "id": "MDRW2100029780.1.3",
          "form": "그냥 업무강도 낮고해서 만족하고 있어요",
          "original_form": "그냥 업무강도 낮고해서 만족하고 있어요",
          "speaker_id": "1",
          "time": "20210903 13:33"
        },
        {
          "id": "MDRW2100029780.1.4",
          "form": "name2님은 직장 다니사나요?",
          "original_form": "&name2&님은 직장 다니사나요?",
          "speaker_id": "1",
          "time": "20210903 13:33"
        },
        {
          "id": "MDRW2100029780.1.5",
          "form": "저는 전업주부에요..",
          "original_form": "저는 전업주부에요..",
          "speaker_id": "2",
          "time": "20210903 13:33"
        },
        {
          "id": "MDRW2100029780.1.6",
          "form": "전에는 그것으로 만족했는데",
          "original_form": "전에는 그것으로 만족했는데",
          "speaker_id": "2",
          "time": "20210903 13:34"
        },
        {
          "id": "MDRW2100029780.1.7",
          "form": "작년부터 코로나로 사람들도 못만나고 하니까",
          "original_form": "작년부터 코로나로 사람들도 못만나고 하니까",
          "speaker_id": "2",
          "time": "20210903 13:34"
        },
        {
          "id": "MDRW2100029780.1.8",
          "form": "알바같은 거라도 하고 싶네요",
          "original_form": "알바같은 거라도 하고 싶네요",
          "speaker_id": "2",
          "time": "20210903 13:34"
        },
        {
          "id": "MDRW2100029780.1.9",
          "form": "제가 사람 많나는 걸 좋아하거든요",
          "original_form": "제가 사람 많나는 걸 좋아하거든요",
          "speaker_id": "2",
          "time": "20210903 13:34"
        },
        {
          "id": "MDRW2100029780.1.10",
          "form": "제 아내도 결혼전에",
          "original_form": "제 아내도 결혼전에",
          "speaker_id": "1",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100029780.1.11",
          "form": "일을 그만두었는데",
          "original_form": "일을 그만두었는데",
          "speaker_id": "1",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100029780.1.12",
          "form": "요즘 다시 일하려고 알아보고 있어요",
          "original_form": "요즘 다시 일하려고 알아보고 있어요",
          "speaker_id": "1",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100029780.1.13",
          "form": "도저히 집에만 못있겠다고 그러더라고요",
          "original_form": "도저히 집에만 못있겠다고 그러더라고요",
          "speaker_id": "1",
          "time": "20210903 13:35"
        },
        {
          "id": "MDRW2100029780.1.14",
          "form": "요즘 공공기관 아르바이트 많으니 그런거 지원해보세요",
          "original_form": "요즘 공공기관 아르바이트 많으니 그런거 지원해보세요",
          "speaker_id": "1",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100029780.1.15",
          "form": "안그래도 정보 얻고 있어요.. 공공근로가 괜찮다 하더라구요",
          "original_form": "안그래도 정보 얻고 있어요.. 공공근로가 괜찮다 하더라구요",
          "speaker_id": "2",
          "time": "20210903 13:37"
        },
        {
          "id": "MDRW2100029780.1.16",
          "form": "그동안은 시간 될때 운동다니고 그랬는데",
          "original_form": "그동안은 시간 될때 운동다니고 그랬는데",
          "speaker_id": "2",
          "time": "20210903 13:37"
        },
        {
          "id": "MDRW2100029780.1.17",
          "form": "요새는 운동도 위험해서 못다니니까요",
          "original_form": "요새는 운동도 위험해서 못다니니까요",
          "speaker_id": "2",
          "time": "20210903 13:38"
        },
        {
          "id": "MDRW2100029780.1.18",
          "form": "알바라도 해야하지 않을까 싶네요",
          "original_form": "알바라도 해야하지 않을까 싶네요",
          "speaker_id": "2",
          "time": "20210903 13:38"
        },
        {
          "id": "MDRW2100029780.1.19",
          "form": "똑똑똑~ 계신가요~",
          "original_form": "똑똑똑~ 계신가요~",
          "speaker_id": "2",
          "time": "20210905 07:58"
        },
        {
          "id": "MDRW2100029780.1.20",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "1",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100029780.1.21",
          "form": "어저 오늘 날씨 너무 좋죠",
          "original_form": "어저 오늘 날씨 너무 좋죠",
          "speaker_id": "1",
          "time": "20210905 07:59"
        },
        {
          "id": "MDRW2100029780.1.22",
          "form": "엄청 좋았어요~ 그래서 날씨 즐기시느라 저와의 대화를 잊으셨나봐요~",
          "original_form": "엄청 좋았어요~ 그래서 날씨 즐기시느라 저와의 대화를 잊으셨나봐요~",
          "speaker_id": "2",
          "time": "20210905 08:00"
        },
        {
          "id": "MDRW2100029780.1.23",
          "form": "오늘은 휴일이라 출근 안하셨겠군요",
          "original_form": "오늘은 휴일이라 출근 안하셨겠군요",
          "speaker_id": "2",
          "time": "20210905 08:00"
        },
        {
          "id": "MDRW2100029780.1.24",
          "form": "네 집에서 쉬고 있어요",
          "original_form": "네 집에서 쉬고 있어요",
          "speaker_id": "1",
          "time": "20210905 08:00"
        },
        {
          "id": "MDRW2100029780.1.25",
          "form": "아르바이트는 찾으셨나요?",
          "original_form": "아르바이트는 찾으셨나요?",
          "speaker_id": "1",
          "time": "20210905 08:01"
        },
        {
          "id": "MDRW2100029780.1.26",
          "form": "알아봤더니 행정복지센터에 신청하면 된대요",
          "original_form": "알아봤더니 행정복지센터에 신청하면 된대요",
          "speaker_id": "2",
          "time": "20210905 08:02"
        },
        {
          "id": "MDRW2100029780.1.27",
          "form": "자리가 꽤 있나봐요",
          "original_form": "자리가 꽤 있나봐요",
          "speaker_id": "2",
          "time": "20210905 08:02"
        },
        {
          "id": "MDRW2100029780.1.28",
          "form": "네 저희 장모님도",
          "original_form": "네 저희 장모님도",
          "speaker_id": "1",
          "time": "20210905 08:02"
        },
        {
          "id": "MDRW2100029780.1.29",
          "form": "신청하셔서 학교에서 방역도우미로 일하시는데",
          "original_form": "신청하셔서 학교에서 방역도우미로 일하시는데",
          "speaker_id": "1",
          "time": "20210905 08:02"
        },
        {
          "id": "MDRW2100029780.1.30",
          "form": "하루에 세시간 정도만 하신다고 하더라고요",
          "original_form": "하루에 세시간 정도만 하신다고 하더라고요",
          "speaker_id": "1",
          "time": "20210905 08:02"
        },
        {
          "id": "MDRW2100029780.1.31",
          "form": "만족해 하셔요",
          "original_form": "만족해 하셔요",
          "speaker_id": "1",
          "time": "20210905 08:02"
        },
        {
          "id": "MDRW2100029780.1.32",
          "form": "급여도 좋고 시간 너무 뺏기지도 않고",
          "original_form": "급여도 좋고 시간 너무 뺏기지도 않고",
          "speaker_id": "1",
          "time": "20210905 08:03"
        },
        {
          "id": "MDRW2100029780.1.33",
          "form": "아이들 만나는것도 즐겁다 하시고",
          "original_form": "아이들 만나는것도 즐겁다 하시고",
          "speaker_id": "1",
          "time": "20210905 08:03"
        },
        {
          "id": "MDRW2100029780.1.34",
          "form": "방역도우미도 괜찮을 듯 하네요",
          "original_form": "방역도우미도 괜찮을 듯 하네요",
          "speaker_id": "2",
          "time": "20210905 08:04"
        },
        {
          "id": "MDRW2100029780.1.35",
          "form": "네 체온 검사하고 마스크 똑바로 쓰도록 지도하고 공공시설물 소독하는 업무정도인가봐요",
          "original_form": "네 체온 검사하고 마스크 똑바로 쓰도록 지도하고 공공시설물 소독하는 업무정도인가봐요",
          "speaker_id": "1",
          "time": "20210905 08:05"
        },
        {
          "id": "MDRW2100029780.1.36",
          "form": "만족하시더라고요",
          "original_form": "만족하시더라고요",
          "speaker_id": "1",
          "time": "20210905 08:05"
        }
      ]
    }
