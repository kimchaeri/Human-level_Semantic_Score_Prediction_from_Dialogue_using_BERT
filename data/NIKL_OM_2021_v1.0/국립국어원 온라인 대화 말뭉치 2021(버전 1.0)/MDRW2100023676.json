        {
          "id": "MDRW2100023676.1.51",
          "form": "그거",
          "original_form": "그거",
          "speaker_id": "1",
          "time": "20210830 14:29"
        },
        {
          "id": "MDRW2100023676.1.52",
          "form": "와 나 겁나 똑똑해 생각해냄",
          "original_form": "와 나 겁나 똑똑해 생각해냄",
          "speaker_id": "2",
          "time": "20210830 14:29"
        },
        {
