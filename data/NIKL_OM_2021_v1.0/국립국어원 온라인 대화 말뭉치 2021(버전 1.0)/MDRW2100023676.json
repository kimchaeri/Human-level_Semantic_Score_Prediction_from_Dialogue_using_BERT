      "utterance": [
        {
          "id": "MDRW2100023676.1.1",
          "form": "그런 것도 있음?",
          "original_form": "그런 것도 있음?",
          "speaker_id": "2",
          "time": "20210830 14:23"
        },
        {
          "id": "MDRW2100023676.1.2",
          "form": "어휴",
          "original_form": "어휴",
          "speaker_id": "2",
          "time": "20210830 14:23"
        },
        {
          "id": "MDRW2100023676.1.3",
          "form": "남자한테해주는거임",
          "original_form": "남자한테해주는거임",
          "speaker_id": "1",
          "time": "20210830 14:23"
        },
        {
          "id": "MDRW2100023676.1.4",
          "form": "유튜브",
          "original_form": "유튜브",
          "speaker_id": "1",
          "time": "20210830 14:24"
        },
        {
          "id": "MDRW2100023676.1.5",
          "form": "에",
          "original_form": "에",
          "speaker_id": "1",
          "time": "20210830 14:24"
        },
        {
          "id": "MDRW2100023676.1.6",
          "form": "답프로포즈쳐봐",
          "original_form": "답프로포즈쳐봐",
          "speaker_id": "1",
          "time": "20210830 14:24"
        },
        {
          "id": "MDRW2100023676.1.7",
          "form": "많이나옴",
          "original_form": "많이나옴",
          "speaker_id": "1",
          "time": "20210830 14:24"
        },
        {
          "id": "MDRW2100023676.1.8",
          "form": "어휴",
          "original_form": "어휴",
          "speaker_id": "2",
          "time": "20210830 14:24"
        },
        {
          "id": "MDRW2100023676.1.9",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210830 14:25"
        },
        {
          "id": "MDRW2100023676.1.10",
          "form": "그치만 굳이 쳐보고 싶진 않다",
          "original_form": "그치만 굳이 쳐보고 싶진 않다",
          "speaker_id": "2",
          "time": "20210830 14:25"
        },
        {
          "id": "MDRW2100023676.1.11",
          "form": "근데난그런것보다",
          "original_form": "근데난그런것보다",
          "speaker_id": "1",
          "time": "20210830 14:25"
        },
        {
          "id": "MDRW2100023676.1.12",
          "form": "내 알고리즘에 그런 걸 뜨게 하고 싶지 않아",
          "original_form": "내 알고리즘에 그런 걸 뜨게 하고 싶지 않아",
          "speaker_id": "2",
          "time": "20210830 14:25"
        },
        {
          "id": "MDRW2100023676.1.13",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210830 14:25"
        },
        {
          "id": "MDRW2100023676.1.14",
          "form": "그거",
          "original_form": "그거",
          "speaker_id": "1",
          "time": "20210830 14:25"
        },
        {
          "id": "MDRW2100023676.1.15",
          "form": "남자한테그렇게돈",
          "original_form": "남자한테그렇게돈",
          "speaker_id": "1",
          "time": "20210830 14:25"
        },
        {
          "id": "MDRW2100023676.1.16",
          "form": "다뺏기고",
          "original_form": "다뺏기고",
          "speaker_id": "1",
          "time": "20210830 14:25"
        },
        {
          "id": "MDRW2100023676.1.17",
          "form": "축의금이나",
          "original_form": "축의금이나",
          "speaker_id": "1",
          "time": "20210830 14:25"
        },
        {
          "id": "MDRW2100023676.1.18",
          "form": "스드메?",
          "original_form": "스드메?",
          "speaker_id": "1",
          "time": "20210830 14:25"
        },
        {
          "id": "MDRW2100023676.1.19",
          "form": "이런식으로",
          "original_form": "이런식으로",
          "speaker_id": "1",
          "time": "20210830 14:26"
        },
        {
          "id": "MDRW2100023676.1.20",
          "form": "비혼여자돈뺏어가는거",
          "original_form": "비혼여자돈뺏어가는거",
          "speaker_id": "1",
          "time": "20210830 14:26"
        },
        {
          "id": "MDRW2100023676.1.21",
          "form": "ㅇㅈ 그래서 비혼식 하는 거라며",
          "original_form": "ㅇㅈ 그래서 비혼식 하는 거라며",
          "speaker_id": "2",
          "time": "20210830 14:26"
        },
        {
          "id": "MDRW2100023676.1.22",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210830 14:26"
        },
        {
          "id": "MDRW2100023676.1.23",
          "form": "스드메도원래",
          "original_form": "스드메도원래",
          "speaker_id": "1",
          "time": "20210830 14:26"
        },
        {
          "id": "MDRW2100023676.1.24",
          "form": "외국에서는",
          "original_form": "외국에서는",
          "speaker_id": "1",
          "time": "20210830 14:26"
        },
        {
          "id": "MDRW2100023676.1.25",
          "form": "어차피 결혼 안하는데 축의금만 내기 아까우니까",
          "original_form": "어차피 결혼 안하는데 축의금만 내기 아까우니까",
          "speaker_id": "2",
          "time": "20210830 14:26"
        },
        {
          "id": "MDRW2100023676.1.26",
          "form": "남편이해주는건데",
          "original_form": "남편이해주는건데",
          "speaker_id": "1",
          "time": "20210830 14:26"
        },
        {
          "id": "MDRW2100023676.1.27",
          "form": "눈치보이니까",
          "original_form": "눈치보이니까",
          "speaker_id": "1",
          "time": "20210830 14:26"
        },
        {
          "id": "MDRW2100023676.1.28",
          "form": "친구들돈으로열어달라고하잖아",
          "original_form": "친구들돈으로열어달라고하잖아",
          "speaker_id": "1",
          "time": "20210830 14:26"
        },
        {
          "id": "MDRW2100023676.1.29",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210830 14:26"
        },
        {
          "id": "MDRW2100023676.1.30",
          "form": "근데비혼식하면약간",
          "original_form": "근데비혼식하면약간",
          "speaker_id": "1",
          "time": "20210830 14:27"
        },
        {
          "id": "MDRW2100023676.1.31",
          "form": "눈치주는분위기도싫음",
          "original_form": "눈치주는분위기도싫음",
          "speaker_id": "1",
          "time": "20210830 14:27"
        },
        {
          "id": "MDRW2100023676.1.32",
          "form": "스드메를 친구 돈으로 열어달라 한다고?",
          "original_form": "스드메를 친구 돈으로 열어달라 한다고?",
          "speaker_id": "2",
          "time": "20210830 14:27"
        },
        {
          "id": "MDRW2100023676.1.33",
          "form": "유난이라는식으로",
          "original_form": "유난이라는식으로",
          "speaker_id": "1",
          "time": "20210830 14:27"
        },
        {
          "id": "MDRW2100023676.1.34",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210830 14:27"
        },
        {
          "id": "MDRW2100023676.1.35",
          "form": "엥 누가 눈치줘 눈치로 눌러버려",
          "original_form": "엥 누가 눈치줘 눈치로 눌러버려",
          "speaker_id": "2",
          "time": "20210830 14:27"
        },
        {
          "id": "MDRW2100023676.1.36",
          "form": "스드메는 결혼식 치장 아니야?",
          "original_form": "스드메는 결혼식 치장 아니야?",
          "speaker_id": "2",
          "time": "20210830 14:27"
        },
        {
          "id": "MDRW2100023676.1.37",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210830 14:27"
        },
        {
          "id": "MDRW2100023676.1.38",
          "form": "스드메가아닌가",
          "original_form": "스드메가아닌가",
          "speaker_id": "1",
          "time": "20210830 14:27"
        },
        {
          "id": "MDRW2100023676.1.39",
          "form": "그거",
          "original_form": "그거",
          "speaker_id": "1",
          "time": "20210830 14:27"
        },
        {
          "id": "MDRW2100023676.1.40",
          "form": "결혼하고나서",
          "original_form": "결혼하고나서",
          "speaker_id": "1",
          "time": "20210830 14:27"
        },
        {
          "id": "MDRW2100023676.1.41",
          "form": "친구들하고파티여는거",
          "original_form": "친구들하고파티여는거",
          "speaker_id": "1",
          "time": "20210830 14:27"
        },
        {
          "id": "MDRW2100023676.1.42",
          "form": "뭐라하는지기억안남",
          "original_form": "뭐라하는지기억안남",
          "speaker_id": "1",
          "time": "20210830 14:27"
        },
        {
          "id": "MDRW2100023676.1.43",
          "form": "아 결혼 하기 전에 파티 하는거",
          "original_form": "아 결혼 하기 전에 파티 하는거",
          "speaker_id": "2",
          "time": "20210830 14:28"
        },
        {
          "id": "MDRW2100023676.1.44",
          "form": "뮤슨무슨파티였음",
          "original_form": "뮤슨무슨파티였음",
          "speaker_id": "1",
          "time": "20210830 14:28"
        },
        {
          "id": "MDRW2100023676.1.45",
          "form": "약간",
          "original_form": "약간",
          "speaker_id": "1",
          "time": "20210830 14:28"
        },
        {
          "id": "MDRW2100023676.1.46",
          "form": "브라이덜",
          "original_form": "브라이덜",
          "speaker_id": "2",
          "time": "20210830 14:28"
        },
        {
          "id": "MDRW2100023676.1.47",
          "form": "일걸?",
          "original_form": "일걸?",
          "speaker_id": "2",
          "time": "20210830 14:28"
