        },
        {
          "id": "MDRW2100014016.1.2",
          "form": "졸졸졸",
          "original_form": "졸졸졸",
          "speaker_id": "1",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100014016.1.3",
          "form": "제가 졸졸졸",
          "original_form": "제가 졸졸졸",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014016.1.4",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014016.1.5",
          "form": "근데 우리 name3 원래 닭을 씻기면 안되는데",
          "original_form": "근데 우리 &name3& 원래 닭을 씻기면 안되는데",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014016.1.6",
          "form": "진짜 기대를 져버리지않네요",
          "original_form": "진짜 기대를 져버리지않네요",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014016.1.7",
          "form": "애가 애완닭(?)이다보니",
          "original_form": "애가 애완닭(?)이다보니",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014016.1.8",
          "form": "씻겼나요",
          "original_form": "씻겼나요",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014016.1.9",
          "form": "제가 맨날 닭가슴살에 코박고 있으니",
          "original_form": "제가 맨날 닭가슴살에 코박고 있으니",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014016.1.10",
          "form": "아부지가 어느날부터 엘라스틴으로 씻기더라고요",
          "original_form": "아부지가 어느날부터 엘라스틴으로 씻기더라고요",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014016.1.11",
          "form": "그래서 그 아이가 지나갈때마다",
          "original_form": "그래서 그 아이가 지나갈때마다",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014016.1.12",
          "form": "ㅋㅋㅋ엘라스틴이요.",
          "original_form": "ㅋㅋㅋ엘라스틴이요.",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014016.1.13",
          "form": "엘라스틱냄새가 확 났답니다",
          "original_form": "엘라스틱냄새가 확 났답니다",
          "speaker_id": "2",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014016.1.14",
          "form": "닭이 꼬끼오하고 울지 않아요",
          "original_form": "닭이 꼬끼오하고 울지 않아요",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014016.1.15",
          "form": "흐규규규규규 이럽니다",
          "original_form": "흐규규규규규 이럽니다",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014016.1.16",
          "form": "사람 샴푸로 씻겨도 되는거에쇼?",
          "original_form": "사람 샴푸로 씻겨도 되는거에쇼?",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014016.1.17",
          "form": "거짓말",
          "original_form": "거짓말",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014016.1.18",
          "form": "뭔 흐규규규울어요 닭이",
          "original_form": "뭔 흐규규규울어요 닭이",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014016.1.19",
          "form": "내가 아무리 닭을 모를다고",
          "original_form": "내가 아무리 닭을 모를다고",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014016.1.20",
          "form": "옥장판 사게하시네",
          "original_form": "옥장판 사게하시네",
          "speaker_id": "1",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014016.1.21",
          "form": "ㅇㄴ ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㄴ ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014016.1.22",
          "form": "찐.",
          "original_form": "찐.",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014016.1.23",
          "form": "흐규규규...흐규... 꼭이오!!",
          "original_form": "흐규규규...흐규... 꼭이오!!",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014016.1.24",
          "form": "와 찐이라고",
          "original_form": "와 찐이라고",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014016.1.25",
          "form": "이런다고요",
          "original_form": "이런다고요",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014016.1.26",
          "form": "진심",
          "original_form": "진심",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014016.1.27",
          "form": "이걸 믿어말아",
          "original_form": "이걸 믿어말아",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014016.1.28",
          "form": "허거거거걱 이렇게 표현도 해요",
          "original_form": "허거거거걱 이렇게 표현도 해요",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014016.1.29",
          "form": "와 진짜 거짓말",
          "original_form": "와 진짜 거짓말",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014016.1.30",
          "form": "진심 닭 숨소리 들어보세요 찐이라그요",
          "original_form": "진심 닭 숨소리 들어보세요 찐이라그요",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014016.1.31",
          "form": "뭔닭이 허거거걱울어요",
          "original_form": "뭔닭이 허거거걱울어요",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014016.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아나 말하니까 웃기넼ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아나 말하니까 웃기넼ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
