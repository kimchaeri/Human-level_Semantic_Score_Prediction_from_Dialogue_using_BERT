      "utterance": [
        {
          "id": "MDRW2100041505.1.1",
          "form": "직장인이세요?",
          "original_form": "직장인이세요?",
          "speaker_id": "1",
          "time": "20210906 08:02"
        },
        {
          "id": "MDRW2100041505.1.2",
          "form": "넹넹!!",
          "original_form": "넹넹!!",
          "speaker_id": "2",
          "time": "20210906 08:03"
        },
        {
          "id": "MDRW2100041505.1.3",
          "form": "출근하셨낭ㅅ",
          "original_form": "출근하셨낭ㅅ",
          "speaker_id": "1",
          "time": "20210906 08:03"
        },
        {
          "id": "MDRW2100041505.1.4",
          "form": "넹넹 name1님은 출근하셨나요",
          "original_form": "넹넹 &name1&님은 출근하셨나요",
          "speaker_id": "2",
          "time": "20210906 08:05"
        },
        {
          "id": "MDRW2100041505.1.5",
          "form": "네 저는 8시출근 삥뽕님은요?",
          "original_form": "네 저는 8시출근 삥뽕님은요?",
          "speaker_id": "1",
          "time": "20210906 08:13"
        },
        {
          "id": "MDRW2100041505.1.6",
          "form": "저도 8시출근인데",
          "original_form": "저도 8시출근인데",
          "speaker_id": "2",
          "time": "20210906 08:13"
        },
        {
          "id": "MDRW2100041505.1.7",
          "form": "8시까지 준비를해야해서",
          "original_form": "8시까지 준비를해야해서",
          "speaker_id": "2",
          "time": "20210906 08:13"
        },
        {
          "id": "MDRW2100041505.1.8",
          "form": "7시30분에 도착해야해려 ㅠㅠ",
          "original_form": "7시30분에 도착해야해려 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 08:14"
        },
        {
          "id": "MDRW2100041505.1.9",
          "form": "아 저도요 ㅜㅜ 님도 일찍 출근하시는군요",
          "original_form": "아 저도요 ㅜㅜ 님도 일찍 출근하시는군요",
          "speaker_id": "1",
          "time": "20210906 08:14"
        },
        {
          "id": "MDRW2100041505.1.10",
          "form": "퇴근은요?",
          "original_form": "퇴근은요?",
          "speaker_id": "1",
          "time": "20210906 08:15"
        },
        {
          "id": "MDRW2100041505.1.11",
          "form": "5시입니당!!!",
          "original_form": "5시입니당!!!",
          "speaker_id": "2",
          "time": "20210906 08:15"
        },
        {
          "id": "MDRW2100041505.1.12",
          "form": "야근도없어요",
          "original_form": "야근도없어요",
          "speaker_id": "2",
          "time": "20210906 08:16"
        },
        {
          "id": "MDRW2100041505.1.13",
          "form": "서비스 직이라서 ㅋㅋㅋ",
          "original_form": "서비스 직이라서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 08:16"
        },
        {
          "id": "MDRW2100041505.1.14",
          "form": "오 대박ㅋㅋㅋ 저도 5시퇴근",
          "original_form": "오 대박ㅋㅋㅋ 저도 5시퇴근",
          "speaker_id": "1",
          "time": "20210906 08:18"
        },
        {
          "id": "MDRW2100041505.1.15",
          "form": "서비스직은 보통 스케쥴아닌가요?",
          "original_form": "서비스직은 보통 스케쥴아닌가요?",
          "speaker_id": "1",
          "time": "20210906 08:18"
        },
        {
          "id": "MDRW2100041505.1.16",
          "form": "저도 했었는데..",
          "original_form": "저도 했었는데..",
          "speaker_id": "1",
          "time": "20210906 08:18"
        },
        {
          "id": "MDRW2100041505.1.17",
          "form": "맙나요 하지만",
          "original_form": "맙나요 하지만",
          "speaker_id": "2",
          "time": "20210906 08:23"
        },
        {
          "id": "MDRW2100041505.1.18",
          "form": "거의 똑같아요",
          "original_form": "거의 똑같아요",
          "speaker_id": "2",
          "time": "20210906 08:24"
        },
        {
          "id": "MDRW2100041505.1.19",
          "form": "먄날오픈",
          "original_form": "먄날오픈",
          "speaker_id": "2",
          "time": "20210906 08:24"
        },
        {
          "id": "MDRW2100041505.1.20",
          "form": "오 어떤 일 하시는데요?",
          "original_form": "오 어떤 일 하시는데요?",
          "speaker_id": "1",
          "time": "20210906 08:57"
        },
        {
          "id": "MDRW2100041505.1.21",
          "form": "그냥커피만 주구장창뽑기..?ㅋㅋㅋㅋㅋ",
          "original_form": "그냥커피만 주구장창뽑기..?ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 09:21"
        },
        {
          "id": "MDRW2100041505.1.22",
          "form": "오오 바리스타시구나",
          "original_form": "오오 바리스타시구나",
          "speaker_id": "1",
          "time": "20210906 09:23"
        },
        {
          "id": "MDRW2100041505.1.23",
          "form": "저도 한 3년 했었어요 ㅋㅋㅋ",
          "original_form": "저도 한 3년 했었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 09:23"
        },
        {
          "id": "MDRW2100041505.1.24",
          "form": "자격증따셨너요??",
          "original_form": "자격증따셨너요??",
          "speaker_id": "2",
          "time": "20210906 09:24"
        },
        {
          "id": "MDRW2100041505.1.25",
          "form": "저는 아직 ㅠㅠ",
          "original_form": "저는 아직 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 09:24"
        },
        {
          "id": "MDRW2100041505.1.26",
          "form": "앗 저는 2급만요ㅋㅋㅋ 자격증은 쉬워요 1급은 모르겠지먼",
          "original_form": "앗 저는 2급만요ㅋㅋㅋ 자격증은 쉬워요 1급은 모르겠지먼",
          "speaker_id": "1",
          "time": "20210906 09:24"
        },
        {
          "id": "MDRW2100041505.1.27",
          "form": "아진짜요??",
          "original_form": "아진짜요??",
          "speaker_id": "2",
          "time": "20210906 09:25"
        },
        {
          "id": "MDRW2100041505.1.28",
          "form": "학원에서 배우셨나여??",
          "original_form": "학원에서 배우셨나여??",
          "speaker_id": "2",
          "time": "20210906 09:25"
        },
        {
          "id": "MDRW2100041505.1.29",
