      "utterance": [
        {
          "id": "MDRW2100026077.1.1",
          "form": "그냥 국에다가 밥비벼서",
          "original_form": "그냥 국에다가 밥비벼서",
          "speaker_id": "1",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100026077.1.2",
          "form": "사료가 없지 ㅎㅎ",
          "original_form": "사료가 없지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100026077.1.3",
          "form": "사료뿐이랴 ㅡㅡ",
          "original_form": "사료뿐이랴 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210901 19:36"
        },
        {
          "id": "MDRW2100026077.1.4",
          "form": "간식도  만들더라",
          "original_form": "간식도  만들더라",
          "speaker_id": "1",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026077.1.5",
          "form": "자식이나 같은거지",
          "original_form": "자식이나 같은거지",
          "speaker_id": "2",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026077.1.6",
          "form": "소중한동생이고  자식",
          "original_form": "소중한동생이고  자식",
          "speaker_id": "2",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026077.1.7",
          "form": "잘 먹는거보면 만들어주고 싶지",
          "original_form": "잘 먹는거보면 만들어주고 싶지",
          "speaker_id": "1",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026077.1.8",
          "form": "근데 귀찮지ㅎㅎ",
          "original_form": "근데 귀찮지ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026077.1.9",
          "form": "털도 예쁘대",
          "original_form": "털도 예쁘대",
          "speaker_id": "1",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026077.1.10",
          "form": "동물 키우고 싶다",
          "original_form": "동물 키우고 싶다",
          "speaker_id": "2",
          "time": "20210901 19:37"
        },
        {
          "id": "MDRW2100026077.1.11",
          "form": "병원비가 감당되어야 할거야",
          "original_form": "병원비가 감당되어야 할거야",
          "speaker_id": "1",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026077.1.12",
          "form": "미용도 시켜야하구",
          "original_form": "미용도 시켜야하구",
          "speaker_id": "1",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026077.1.13",
          "form": "응 기본비가들어",
          "original_form": "응 기본비가들어",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026077.1.14",
          "form": "꾸밈비",
          "original_form": "꾸밈비",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026077.1.15",
          "form": "아프면 마음아파",
          "original_form": "아프면 마음아파",
          "speaker_id": "1",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026077.1.16",
          "form": "푸들이 건강하대",
          "original_form": "푸들이 건강하대",
          "speaker_id": "2",
          "time": "20210901 19:38"
        },
        {
          "id": "MDRW2100026077.1.17",
          "form": "응 불독은 아프대",
          "original_form": "응 불독은 아프대",
          "speaker_id": "1",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100026077.1.18",
          "form": "시고르자브종이 최고야",
          "original_form": "시고르자브종이 최고야",
