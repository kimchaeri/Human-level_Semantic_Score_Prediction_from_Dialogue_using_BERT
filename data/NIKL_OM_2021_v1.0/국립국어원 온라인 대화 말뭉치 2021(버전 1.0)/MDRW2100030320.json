      "utterance": [
        {
          "id": "MDRW2100030320.1.1",
          "form": "난 어른이 되면",
          "original_form": "난 어른이 되면",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030320.1.2",
          "form": "집안일만 히면서 백수로 사는게 꿈이야 사실..",
          "original_form": "집안일만 히면서 백수로 사는게 꿈이야 사실..",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030320.1.3",
          "form": "오 ㅎㅎ",
          "original_form": "오 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030320.1.4",
          "form": "난 집안일도 안하구 싶어",
          "original_form": "난 집안일도 안하구 싶어",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030320.1.5",
          "form": "집에서 빌붙어살면서",
          "original_form": "집에서 빌붙어살면서",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030320.1.6",
          "form": "그냥 딩굴거리고 싶엉~~",
          "original_form": "그냥 딩굴거리고 싶엉~~",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100030320.1.7",
          "form": "그건 좀 양심이없다고 생각해 난 ㅎㅎ",
          "original_form": "그건 좀 양심이없다고 생각해 난 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030320.1.8",
          "form": "아니!!!당연히",
          "original_form": "아니!!!당연히",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030320.1.9",
          "form": "돈많이벌어서",
          "original_form": "돈많이벌어서",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030320.1.10",
          "form": "로봇청소기같은걸",
          "original_form": "로봇청소기같은걸",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030320.1.11",
          "form": "사야지 ㅎㅎ",
          "original_form": "사야지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030320.1.12",
          "form": "그때쯤 되면 집안일로봇같은게",
          "original_form": "그때쯤 되면 집안일로봇같은게",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030320.1.13",
          "form": "그렇긴 하지",
          "original_form": "그렇긴 하지",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030320.1.14",
          "form": "더 잘 나올거야",
          "original_form": "더 잘 나올거야",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030320.1.15",
          "form": "하지만 누군가는 집안일을해야해",
          "original_form": "하지만 누군가는 집안일을해야해",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030320.1.16",
          "form": "너가 개발해봐 ㅎㅎ",
          "original_form": "너가 개발해봐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030320.1.17",
          "form": "나오긴해도 완벽하진않겟지",
          "original_form": "나오긴해도 완벽하진않겟지",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030320.1.18",
          "form": "점점 발전하는거지",
          "original_form": "점점 발전하는거지",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100030320.1.19",
          "form": "과학 발전속도는 점점 빨라진대!",
          "original_form": "과학 발전속도는 점점 빨라진대!",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030320.1.20",
          "form": "그러면 나중에는",
          "original_form": "그러면 나중에는",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030320.1.21",
          "form": "집안일 안하고",
          "original_form": "집안일 안하고",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030320.1.22",
          "form": "집안일두 기계가 대신 해주면 정말 좋갯지 ㅎ",
          "original_form": "집안일두 기계가 대신 해주면 정말 좋갯지 ㅎ",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030320.1.23",
          "form": "집에서 가족들이랑 행복하게 살면",
          "original_form": "집에서 가족들이랑 행복하게 살면",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030320.1.24",
          "form": "좋겠다 ㅎㅎ",
          "original_form": "좋겠다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030320.1.25",
          "form": "맞아!!",
          "original_form": "맞아!!",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100030320.1.26",
          "form": "그게 꿈이얌",
          "original_form": "그게 꿈이얌",
