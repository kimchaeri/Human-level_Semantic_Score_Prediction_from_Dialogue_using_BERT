      "utterance": [
        {
          "id": "MDRW2100043413.1.1",
          "form": "저녁 드셨어요?",
          "original_form": "저녁 드셨어요?",
          "speaker_id": "2",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043413.1.2",
          "form": "넹 먹었어여!",
          "original_form": "넹 먹었어여!",
          "speaker_id": "1",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043413.1.3",
          "form": "뭐 드셨나요!!",
          "original_form": "뭐 드셨나요!!",
          "speaker_id": "2",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100043413.1.4",
          "form": "저 택배주문시킨 닭갈비 해 먹었어요!",
          "original_form": "저 택배주문시킨 닭갈비 해 먹었어요!",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043413.1.5",
          "form": "오오 그럼 밀키트같이 되어있는 거였나요?",
          "original_form": "오오 그럼 밀키트같이 되어있는 거였나요?",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043413.1.6",
          "form": "네 밀키트같은거 맞아용ㅋㅋㅋㅋ",
          "original_form": "네 밀키트같은거 맞아용ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043413.1.7",
          "form": "첨시켜먹었는데 맛있었습니닷",
          "original_form": "첨시켜먹었는데 맛있었습니닷",
          "speaker_id": "1",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043413.1.8",
          "form": "요즘 밀키트 정말 많이 주문해서 먹더라구요!",
          "original_form": "요즘 밀키트 정말 많이 주문해서 먹더라구요!",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043413.1.9",
          "form": "맛이 진짜 괜찮나봐요!",
          "original_form": "맛이 진짜 괜찮나봐요!",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043413.1.10",
          "form": "편하기도 하고 맛도 괜찮아서 좋드라구요ㅋㅋㅋㅋ",
          "original_form": "편하기도 하고 맛도 괜찮아서 좋드라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043413.1.11",
          "form": "name2님은 뭐 드셨나요?",
          "original_form": "&name2&님은 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043413.1.12",
          "form": "저는 김치찜 시켜먹었어요!",
          "original_form": "저는 김치찜 시켜먹었어요!",
          "speaker_id": "2",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043413.1.13",
          "form": "넘 맛있었습니다",
          "original_form": "넘 맛있었습니다",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043413.1.14",
          "form": "김치찜 좋아하시나봐용!",
          "original_form": "김치찜 좋아하시나봐용!",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043413.1.15",
          "form": "맛있죠 김치찜..",
          "original_form": "맛있죠 김치찜..",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043413.1.16",
          "form": "네! 고기랑 김치라니.. 환상적이죠ㅠㅠ",
          "original_form": "네! 고기랑 김치라니.. 환상적이죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043413.1.17",
          "form": "김가루도 뿌려가지구ㅠㅠ",
          "original_form": "김가루도 뿌려가지구ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043413.1.18",
          "form": "와 저 침고였어요",
          "original_form": "와 저 침고였어요",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043413.1.19",
          "form": "그렇게 하면 진짜 밥도둑 뚝딱인데!ㅠㅠ",
          "original_form": "그렇게 하면 진짜 밥도둑 뚝딱인데!ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043413.1.20",
          "form": "그쵸!! 사실 계란후라이도 있었어요.. 끝내주죠ㅠㅠ",
          "original_form": "그쵸!! 사실 계란후라이도 있었어요.. 끝내주죠ㅠㅠ",
