      "utterance": [
        {
          "id": "MDRW2100020858.1.1",
          "form": "요리 혹시 하세요?",
          "original_form": "요리 혹시 하세요?",
          "speaker_id": "1",
          "time": "20210828 13:30"
        },
        {
          "id": "MDRW2100020858.1.2",
          "form": "네 간단한거는요!",
          "original_form": "네 간단한거는요!",
          "speaker_id": "2",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020858.1.3",
          "form": "샌드위치 만드는거 좋아하세요?",
          "original_form": "샌드위치 만드는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020858.1.4",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020858.1.5",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020858.1.6",
          "form": "근데 샌드위치 직접만드는 것보다 사먹는 게 맛이 압승이라 잘 해먹진 않아요ㅋㅋ",
          "original_form": "근데 샌드위치 직접만드는 것보다 사먹는 게 맛이 압승이라 잘 해먹진 않아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:32"
        },
        {
          "id": "MDRW2100020858.1.7",
          "form": "샌드위치 만드는 재주가 없나봐요",
          "original_form": "샌드위치 만드는 재주가 없나봐요",
          "speaker_id": "1",
          "time": "20210828 13:33"
        },
        {
          "id": "MDRW2100020858.1.8",
          "form": "자주 해먹는 음식 있어요?",
          "original_form": "자주 해먹는 음식 있어요?",
          "speaker_id": "1",
          "time": "20210828 13:33"
        },
        {
          "id": "MDRW2100020858.1.9",
          "form": "저는 파스타랑 토마토 카레요!",
          "original_form": "저는 파스타랑 토마토 카레요!",
          "speaker_id": "2",
          "time": "20210828 13:34"
        },
        {
          "id": "MDRW2100020858.1.10",
          "form": "생선구이도 가끔해요ㅎㅎ",
          "original_form": "생선구이도 가끔해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 13:34"
        },
        {
          "id": "MDRW2100020858.1.11",
          "form": "토마토카레는 무슨 카레예요? 맛있을 것 같아요!!",
          "original_form": "토마토카레는 무슨 카레예요? 맛있을 것 같아요!!",
          "speaker_id": "1",
          "time": "20210828 13:34"
        },
        {
          "id": "MDRW2100020858.1.12",
          "form": "토마토를 삶아서 껍질을 빼고 카레를 넣어요ㅎㅎ",
          "original_form": "토마토를 삶아서 껍질을 빼고 카레를 넣어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 13:35"
        },
        {
          "id": "MDRW2100020858.1.13",
          "form": "야채 육수로 카레 만드는거랑 비슷해요!",
          "original_form": "야채 육수로 카레 만드는거랑 비슷해요!",
          "speaker_id": "2",
          "time": "20210828 13:35"
        },
        {
          "id": "MDRW2100020858.1.14",
          "form": "토마토는 형체없게 으깨버리는 거예요?",
          "original_form": "토마토는 형체없게 으깨버리는 거예요?",
          "speaker_id": "1",
          "time": "20210828 13:36"
        },
        {
          "id": "MDRW2100020858.1.15",
          "form": "네 맞아요! 토마토 주스처럼생각하시면돼요",
          "original_form": "네 맞아요! 토마토 주스처럼생각하시면돼요",
          "speaker_id": "2",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100020858.1.16",
          "form": "맛있을까? 싶지만 생각보다 카레 풍미가 확살아요",
          "original_form": "맛있을까? 싶지만 생각보다 카레 풍미가 확살아요",
          "speaker_id": "2",
          "time": "20210828 13:37"
        },
        {
          "id": "MDRW2100020858.1.17",
          "form": "토마토 넣을 생각 못해봤는데 꿀팁이네요!!!",
          "original_form": "토마토 넣을 생각 못해봤는데 꿀팁이네요!!!",
          "speaker_id": "1",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100020858.1.18",
          "form": "저는 백종원레시피 보고 풍미 살리는 용으론 양파 정도만 써봤거든요",
          "original_form": "저는 백종원레시피 보고 풍미 살리는 용으론 양파 정도만 써봤거든요",
          "speaker_id": "1",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100020858.1.19",
          "form": "양파도좋죠ㅋㅋ 그 도마 쓰시는 김에 토마토도 썰어서 넣어보세요",
          "original_form": "양파도좋죠ㅋㅋ 그 도마 쓰시는 김에 토마토도 썰어서 넣어보세요",
          "speaker_id": "2",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100020858.1.20",
          "form": "같은 설거지양에 훌륭한맛이 됩니당",
          "original_form": "같은 설거지양에 훌륭한맛이 됩니당",
          "speaker_id": "2",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100020858.1.21",
          "form": "ㅋㅋㅋ 꼭 해먹어볼게요 진짜요ㅋㅋ",
          "original_form": "ㅋㅋㅋ 꼭 해먹어볼게요 진짜요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100020858.1.22",
          "form": "박막례할머니 카레떡볶이에 빠져서 그거 해먹느라 카레가루 집에 상시 구비돼있어요ㅋㅋ",
          "original_form": "박막례할머니 카레떡볶이에 빠져서 그거 해먹느라 카레가루 집에 상시 구비돼있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100020858.1.23",
          "form": "토마토만 사면 되네요",
          "original_form": "토마토만 사면 되네요",
          "speaker_id": "1",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100020858.1.24",
          "form": "ㅋㅋ와 막례쓰 떡볶이 맛있어요?",
          "original_form": "ㅋㅋ와 막례쓰 떡볶이 맛있어요?",
          "speaker_id": "2",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100020858.1.25",
          "form": "왜냐면 저 지금 카레와 떡이 마침 있거든요",
          "original_form": "왜냐면 저 지금 카레와 떡이 마침 있거든요",
          "speaker_id": "2",
          "time": "20210828 13:48"
        },
        {
          "id": "MDRW2100020858.1.26",
          "form": "엄청요!!!!!!! ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "엄청요!!!!!!! ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:49"
        },
        {
          "id": "MDRW2100020858.1.27",
          "form": "중독성 있는 맛이에요",
          "original_form": "중독성 있는 맛이에요",
          "speaker_id": "1",
          "time": "20210828 13:49"
        }
      ]
