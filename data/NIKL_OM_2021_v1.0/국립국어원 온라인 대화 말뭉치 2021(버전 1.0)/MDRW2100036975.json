      "utterance": [
        {
          "id": "MDRW2100036975.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 15:54"
        },
        {
          "id": "MDRW2100036975.1.2",
          "form": "여행 좋아하는 편이에요ㅋㅋ근데 많이 가보진못했어요",
          "original_form": "여행 좋아하는 편이에요ㅋㅋ근데 많이 가보진못했어요",
          "speaker_id": "2",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100036975.1.3",
          "form": "제일 기억에 남는 여행 있으세요?",
          "original_form": "제일 기억에 남는 여행 있으세요?",
          "speaker_id": "2",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100036975.1.4",
          "form": "저도 여행좋아해요!",
          "original_form": "저도 여행좋아해요!",
          "speaker_id": "1",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100036975.1.5",
          "form": "전 우유니가 가장기억에남습니다ㅜ",
          "original_form": "전 우유니가 가장기억에남습니다ㅜ",
          "speaker_id": "1",
          "time": "20210831 15:55"
        },
        {
          "id": "MDRW2100036975.1.6",
          "form": "우와 우유니...!!!!",
          "original_form": "우와 우유니...!!!!",
          "speaker_id": "2",
          "time": "20210831 15:56"
        },
        {
          "id": "MDRW2100036975.1.7",
          "form": "저 티비에서 보고 너무 가보고싶었는데",
          "original_form": "저 티비에서 보고 너무 가보고싶었는데",
          "speaker_id": "2",
          "time": "20210831 15:56"
        },
        {
          "id": "MDRW2100036975.1.8",
          "form": "저도 가보고싶던곳중하나라 무대뽀로 갔다왔어요 ㅎㅎㅎㅎ",
          "original_form": "저도 가보고싶던곳중하나라 무대뽀로 갔다왔어요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 15:58"
        },
        {
          "id": "MDRW2100036975.1.9",
          "form": "아닛 사진많이 찍으셨죠?라고보냈는데 없어졌네요ㅜㅜ",
          "original_form": "아닛 사진많이 찍으셨죠?라고보냈는데 없어졌네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100036975.1.10",
          "form": "네 가끔안보내지더라구여ㅜ",
          "original_form": "네 가끔안보내지더라구여ㅜ",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100036975.1.11",
          "form": "사진엄청찍었어요 ㅋㅋㅋㅋㅋㅋㅋ남은건 사진뿐",
          "original_form": "사진엄청찍었어요 ㅋㅋㅋㅋㅋㅋㅋ남은건 사진뿐",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100036975.1.12",
          "form": "ㅋㅋㅋㅋ기념품도 많이사시나요",
          "original_form": "ㅋㅋㅋㅋ기념품도 많이사시나요",
          "speaker_id": "2",
          "time": "20210907 23:28"
        },
        {
          "id": "MDRW2100036975.1.13",
          "form": "기념품은 자석이쁜거있음사왔어요 멀리가다보니 짐늘리기싫어가지고 ㅎㅎ",
          "original_form": "기념품은 자석이쁜거있음사왔어요 멀리가다보니 짐늘리기싫어가지고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100036975.1.14",
          "form": "오오 자석...! 저는 뱃지같은거 사오고싶어요ㅜ",
          "original_form": "오오 자석...! 저는 뱃지같은거 사오고싶어요ㅜ",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100036975.1.15",
          "form": "저도 뱃지살걸그랬어요 냉장고덕지덕지붙이니까 ㅎㅎ....",
          "original_form": "저도 뱃지살걸그랬어요 냉장고덕지덕지붙이니까 ㅎㅎ....",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100036975.1.16",
          "form": "뱃지가 작고 모으기편할거같긴해여",
          "original_form": "뱃지가 작고 모으기편할거같긴해여",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100036975.1.17",
          "form": "저는 에코백에 뱃지 다는걸 좋아해서요ㅋㅋㅋ",
          "original_form": "저는 에코백에 뱃지 다는걸 좋아해서요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100036975.1.18",
          "form": "그래서 이쁜거있으면 모아요",
          "original_form": "그래서 이쁜거있으면 모아요",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100036975.1.19",
          "form": "에코백에 이쁠거같아요 ㅎㅎ",
          "original_form": "에코백에 이쁠거같아요 ㅎㅎ",
          "speaker_id": "1",
