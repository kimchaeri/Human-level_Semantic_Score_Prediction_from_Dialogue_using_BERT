      "utterance": [
        {
          "id": "MDRW2100026263.1.1",
          "form": "언니 카톡",
          "original_form": "언니 카톡",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100026263.1.2",
          "form": "한번만 확인좀요",
          "original_form": "한번만 확인좀요",
          "speaker_id": "1",
          "time": "20210901 21:10"
        },
        {
          "id": "MDRW2100026263.1.3",
          "form": "우웅",
          "original_form": "우웅",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100026263.1.4",
          "form": "구래서 밥 먹고 와서 다시 일하는데",
          "original_form": "구래서 밥 먹고 와서 다시 일하는데",
          "speaker_id": "2",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100026263.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026263.1.6",
          "form": "오늘 은행이랑 우체국이랑 가야했는데",
          "original_form": "오늘 은행이랑 우체국이랑 가야했는데",
          "speaker_id": "2",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026263.1.7",
          "form": "추가로 병원을 가야되는거야",
          "original_form": "추가로 병원을 가야되는거야",
          "speaker_id": "2",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026263.1.8",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026263.1.9",
          "form": "병원은 누가 법카로 병원비 내가지고 그거 취소하러...",
          "original_form": "병원은 누가 법카로 병원비 내가지고 그거 취소하러...",
          "speaker_id": "2",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026263.1.10",
          "form": "외근을 병원까지 가나요?",
          "original_form": "외근을 병원까지 가나요?",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026263.1.11",
          "form": "회사카드는 왜 가져갔대요?",
          "original_form": "회사카드는 왜 가져갔대요?",
          "speaker_id": "1",
          "time": "20210901 21:12"
        },
        {
          "id": "MDRW2100026263.1.12",
          "form": "공사현장에서 누가 다쳤는데 그걸 법카로 해결한거지... 그러면 안되는데..",
          "original_form": "공사현장에서 누가 다쳤는데 그걸 법카로 해결한거지... 그러면 안되는데..",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026263.1.13",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026263.1.14",
          "form": "그분이 잘모르셨나봐요",
          "original_form": "그분이 잘모르셨나봐요",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026263.1.15",
          "form": "회사내규가 있을텐데",
          "original_form": "회사내규가 있을텐데",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026263.1.16",
          "form": "근데 여기서 또 팀장이 내가 바쁘면 나눠서 가겠다고 그러는데 나눠서 갈게 없는게 은행하고 우체국이 가는방향이 같고 병원이 따로 다른방향인데 취소할 카드랑 영수증이랑 다 내가 가지고 나왓는데",
          "original_form": "근데 여기서 또 팀장이 내가 바쁘면 나눠서 가겠다고 그러는데 나눠서 갈게 없는게 은행하고 우체국이 가는방향이 같고 병원이 따로 다른방향인데 취소할 카드랑 영수증이랑 다 내가 가지고 나왓는데",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026263.1.17",
          "form": "그냥 말만 그러는거지",
          "original_form": "그냥 말만 그러는거지",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026263.1.18",
          "form": "아이고!!!!!!",
          "original_form": "아이고!!!!!!",
          "speaker_id": "1",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026263.1.19",
          "form": "말이 오히려 마이너스된다는 걸 팀장님이 모르시나보네요",
          "original_form": "말이 오히려 마이너스된다는 걸 팀장님이 모르시나보네요",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026263.1.20",
          "form": "상사가 상사노릇을 못했네요",
          "original_form": "상사가 상사노릇을 못했네요",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026263.1.21",
          "form": "그리고 내 일을 도와주고 싶으면 혼자 다녀오면 되는데 꼭 신입하고 같이간다고 그러는거야",
          "original_form": "그리고 내 일을 도와주고 싶으면 혼자 다녀오면 되는데 꼭 신입하고 같이간다고 그러는거야",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026263.1.22",
          "form": "놀고 오겠다는거지 그게",
          "original_form": "놀고 오겠다는거지 그게",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026263.1.23",
          "form": "이번주는 신입이 현장 출근하는 주간이거든",
          "original_form": "이번주는 신입이 현장 출근하는 주간이거든",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026263.1.24",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026263.1.25",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026263.1.26",
          "form": "그래서 그냥 내가 다 외근 돌고 들어왓엉..",
          "original_form": "그래서 그냥 내가 다 외근 돌고 들어왓엉..",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026263.1.27",
          "form": "그것도 좋다 이거야",
          "original_form": "그것도 좋다 이거야",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026263.1.28",
          "form": "여기까지도 그렇게 화나진 않앗어",
          "original_form": "여기까지도 그렇게 화나진 않앗어",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026263.1.29",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026263.1.30",
          "form": "언니 혼자서 업무를 다했는데도요?",
          "original_form": "언니 혼자서 업무를 다했는데도요?",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026263.1.31",
          "form": "응 여기까진 뭐 평상시에도 그러니까 참을만했어",
          "original_form": "응 여기까진 뭐 평상시에도 그러니까 참을만했어",
          "speaker_id": "2",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026263.1.32",
          "form": "완전 무임승차잖아요. 너무 화나네요",
          "original_form": "완전 무임승차잖아요. 너무 화나네요",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026263.1.33",
          "form": "어제 경비반장님께서 직원들 먹으라고 포도를 주신게 있는데 남아서 냉장고에 넣어뒀던게 있어서 신입하고 팀장님 먹으라고 씻어서 접시에 올려서 쟁반에 해서 줬는데",
          "original_form": "어제 경비반장님께서 직원들 먹으라고 포도를 주신게 있는데 남아서 냉장고에 넣어뒀던게 있어서 신입하고 팀장님 먹으라고 씻어서 접시에 올려서 쟁반에 해서 줬는데",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026263.1.34",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026263.1.35",
          "form": "외근 다녀오니까 그게 반 정도 먹은것이 그대로 싱크대에 들어가있는거야",
          "original_form": "외근 다녀오니까 그게 반 정도 먹은것이 그대로 싱크대에 들어가있는거야",
          "speaker_id": "2",
          "time": "20210901 21:19"
        }
      ]
