      "utterance": [
        {
          "id": "MDRW2100019347.1.1",
          "form": "솔직히 우리도 해운대 있고 해서",
          "original_form": "솔직히 우리도 해운대 있고 해서",
          "speaker_id": "2",
          "time": "20210827 17:57"
        },
        {
          "id": "MDRW2100019347.1.2",
          "form": "이정도지",
          "original_form": "이정도지",
          "speaker_id": "2",
          "time": "20210827 17:57"
        },
        {
          "id": "MDRW2100019347.1.3",
          "form": "다른 지역은...",
          "original_form": "다른 지역은...",
          "speaker_id": "2",
          "time": "20210827 17:57"
        },
        {
          "id": "MDRW2100019347.1.4",
          "form": "몇천만원자리 그림 얼마짜리 그림 이렇게 올라오는 게시물보면",
          "original_form": "몇천만원자리 그림 얼마짜리 그림 이렇게 올라오는 게시물보면",
          "speaker_id": "1",
          "time": "20210827 17:58"
        },
        {
          "id": "MDRW2100019347.1.5",
          "form": "댓글로 사람들이",
          "original_form": "댓글로 사람들이",
          "speaker_id": "1",
          "time": "20210827 17:58"
        },
        {
          "id": "MDRW2100019347.1.6",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210827 17:58"
        },
        {
          "id": "MDRW2100019347.1.7",
          "form": "실제로.보면. 뭐가.. 느으껴진다",
          "original_form": "실제로.보면. 뭐가.. 느으껴진다",
          "speaker_id": "1",
          "time": "20210827 17:58"
        },
        {
          "id": "MDRW2100019347.1.8",
          "form": "이러는데",
          "original_form": "이러는데",
          "speaker_id": "1",
          "time": "20210827 17:58"
        },
        {
          "id": "MDRW2100019347.1.9",
          "form": "맞아...",
          "original_form": "맞아...",
          "speaker_id": "2",
          "time": "20210827 17:58"
        },
        {
          "id": "MDRW2100019347.1.10",
          "form": "나도 그림 실제로 보고 그런거 느ㅏㅏ껴보고싶음ㅋㅋㅋㅋ",
          "original_form": "나도 그림 실제로 보고 그런거 느ㅏㅏ껴보고싶음ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:58"
        },
        {
          "id": "MDRW2100019347.1.11",
          "form": "근데 진짜 그런거 하면 사람들 엄청 몰리잖아",
          "original_form": "근데 진짜 그런거 하면 사람들 엄청 몰리잖아",
          "speaker_id": "2",
          "time": "20210827 17:59"
        },
        {
          "id": "MDRW2100019347.1.12",
          "form": "그치...ㅋㅋㅋㅋㅋ",
          "original_form": "그치...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:59"
        },
        {
          "id": "MDRW2100019347.1.13",
          "form": "좀 여러 지역 해주면 그런것도 좀 줄어들텐데",
          "original_form": "좀 여러 지역 해주면 그런것도 좀 줄어들텐데",
          "speaker_id": "2",
          "time": "20210827 17:59"
        },
        {
          "id": "MDRW2100019347.1.14",
          "form": "슬프다 ㅠㅠ",
          "original_form": "슬프다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 17:59"
        },
        {
          "id": "MDRW2100019347.1.15",
          "form": "맞아 ㅡ_ㅠ",
          "original_form": "맞아 ㅡ_ㅠ",
          "speaker_id": "1",
          "time": "20210827 17:59"
        },
        {
          "id": "MDRW2100019347.1.16",
          "form": "우리 사촌언니도",
          "original_form": "우리 사촌언니도",
          "speaker_id": "2",
          "time": "20210827 18:00"
        },
        {
          "id": "MDRW2100019347.1.17",
          "form": "뮤직컬 엄청 좋아해서",
          "original_form": "뮤직컬 엄청 좋아해서",
          "speaker_id": "2",
          "time": "20210827 18:00"
        },
        {
          "id": "MDRW2100019347.1.18",
          "form": "서울 엄청 가거든 근데 버스비랑 표값 하면",
          "original_form": "서울 엄청 가거든 근데 버스비랑 표값 하면",
          "speaker_id": "2",
          "time": "20210827 18:00"
        },
        {
          "id": "MDRW2100019347.1.19",
          "form": "그 지역 사는 사람들 2배로 쓰더라..",
          "original_form": "그 지역 사는 사람들 2배로 쓰더라..",
          "speaker_id": "2",
          "time": "20210827 18:00"
        },
        {
          "id": "MDRW2100019347.1.20",
          "form": "맞아 한서영 최유진 엑소콘서트 가는거만봐도ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아 한서영 최유진 엑소콘서트 가는거만봐도ㅋㅋㅋㅋㅋㅋ",
