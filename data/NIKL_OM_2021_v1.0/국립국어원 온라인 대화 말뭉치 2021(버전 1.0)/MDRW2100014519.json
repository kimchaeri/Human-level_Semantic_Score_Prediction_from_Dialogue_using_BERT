        },
        {
          "id": "MDRW2100014519.1.2",
          "form": "책을 안 사 네",
          "original_form": "책을 안 사 네",
          "speaker_id": "2",
          "time": "20210825 20:49"
        },
        {
          "id": "MDRW2100014519.1.3",
          "form": "중고 책도 팔아야 되고",
          "original_form": "중고 책도 팔아야 되고",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014519.1.4",
          "form": "중고도 잘 못팔겠드라",
