      "utterance": [
        {
          "id": "MDRW2100045030.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그러게요 재밌어서 자꾸 떠들다 보니",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그러게요 재밌어서 자꾸 떠들다 보니",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045030.1.2",
          "form": "판이 후딱후딱 갈아지나봐요",
          "original_form": "판이 후딱후딱 갈아지나봐요",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045030.1.3",
          "form": "저 지인분한테",
          "original_form": "저 지인분한테",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045030.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045030.1.5",
          "form": "여기서 name2",
          "original_form": "여기서 &name2&",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045030.1.6",
          "form": "라는 분한테 정들었다했더니",
          "original_form": "라는 분한테 정들었다했더니",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045030.1.7",
          "form": "닉네임 마음에든대요ㅋㅋ",
          "original_form": "닉네임 마음에든대요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045030.1.8",
          "form": "ㅋㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045030.1.9",
          "form": "방금 닉네임 말씀해주셔서",
          "original_form": "방금 닉네임 말씀해주셔서",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045030.1.10",
          "form": "저도 바로 앉았어요",
          "original_form": "저도 바로 앉았어요",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045030.1.11",
          "form": "등 쭉 펴!",
          "original_form": "등 쭉 펴!",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045030.1.12",
          "form": "전 거북목안되게",
          "original_form": "전 거북목안되게",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045030.1.13",
          "form": "누워있답니다(?)",
          "original_form": "누워있답니다(?)",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045030.1.14",
          "form": "최고의 방법이네요",
          "original_form": "최고의 방법이네요",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045030.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045030.1.16",
          "form": "저두 홀랑 누울래요",
          "original_form": "저두 홀랑 누울래요",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045030.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045030.1.18",
          "form": "이번엔 제 저녁밥을 맞춰보실래요?",
          "original_form": "이번엔 제 저녁밥을 맞춰보실래요?",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045030.1.19",
          "form": "환영이예요!!!",
          "original_form": "환영이예요!!!",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045030.1.20",
          "form": "넵 시작하시죠",
          "original_form": "넵 시작하시죠",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045030.1.21",
          "form": "안 매운 음식인가요?",
          "original_form": "안 매운 음식인가요?",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045030.1.22",
          "form": "매운 음식이에요",
          "original_form": "매운 음식이에요",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045030.1.23",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045030.1.24",
          "form": "빨간색 요리인가요?",
          "original_form": "빨간색 요리인가요?",
          "speaker_id": "2",
          "time": "20210908 00:40"
