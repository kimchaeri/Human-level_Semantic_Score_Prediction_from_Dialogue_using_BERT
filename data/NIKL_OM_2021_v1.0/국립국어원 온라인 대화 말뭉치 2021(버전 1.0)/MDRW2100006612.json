      "utterance": [
        {
          "id": "MDRW2100006612.1.1",
          "form": "저녁은 머뭑었으",
          "original_form": "저녁은 머뭑었으",
          "speaker_id": "1",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100006612.1.2",
          "form": "기숙사 밥이지",
          "original_form": "기숙사 밥이지",
          "speaker_id": "2",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100006612.1.3",
          "form": "어묵이랑 마늘쫑이랑 밥이랑",
          "original_form": "어묵이랑 마늘쫑이랑 밥이랑",
          "speaker_id": "2",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100006612.1.4",
          "form": "그냥 평범한 저녁이더라 ㅋㅋㅋ",
          "original_form": "그냥 평범한 저녁이더라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100006612.1.5",
          "form": "맛있어??",
          "original_form": "맛있어??",
          "speaker_id": "1",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100006612.1.6",
          "form": "긱사 밥 맛없지 않나",
          "original_form": "긱사 밥 맛없지 않나",
          "speaker_id": "1",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100006612.1.7",
          "form": "먹어보지 않았나?",
          "original_form": "먹어보지 않았나?",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100006612.1.8",
          "form": "1학년 때 내 카드로 해서",
          "original_form": "1학년 때 내 카드로 해서",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100006612.1.9",
          "form": "아ㅏ닌데유",
          "original_form": "아ㅏ닌데유",
          "speaker_id": "1",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100006612.1.10",
          "form": "누구야..",
          "original_form": "누구야..",
          "speaker_id": "1",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100006612.1.11",
          "form": "누구랑 헷갈리는거야..",
          "original_form": "누구랑 헷갈리는거야..",
          "speaker_id": "1",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100006612.1.12",
          "form": "나 황당해..;",
          "original_form": "나 황당해..;",
          "speaker_id": "1",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100006612.1.13",
          "form": "몰라 하도 카드 여기저기 빌려주고다녀서",
          "original_form": "몰라 하도 카드 여기저기 빌려주고다녀서",
          "speaker_id": "2",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100006612.1.14",
          "form": "미안해 일로 와",
          "original_form": "미안해 일로 와",
          "speaker_id": "2",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100006612.1.15",
          "form": "당장 카드 넘겨줄게",
          "original_form": "당장 카드 넘겨줄게",
          "speaker_id": "2",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100006612.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100006612.1.17",
          "form": "어이없어.. 참나",
          "original_form": "어이없어.. 참나",
          "speaker_id": "1",
          "time": "20210908 03:22"
        },
        {
          "id": "MDRW2100006612.1.18",
          "form": "긱사 밥 나 한번도 못먹어봤어",
          "original_form": "긱사 밥 나 한번도 못먹어봤어",
          "speaker_id": "1",
          "time": "20210908 03:23"
        },
        {
          "id": "MDRW2100006612.1.19",
          "form": "반성할게",
          "original_form": "반성할게",
          "speaker_id": "2",
          "time": "20210908 03:23"
        },
        {
          "id": "MDRW2100006612.1.20",
          "form": "먹어보고싶으면",
          "original_form": "먹어보고싶으면",
