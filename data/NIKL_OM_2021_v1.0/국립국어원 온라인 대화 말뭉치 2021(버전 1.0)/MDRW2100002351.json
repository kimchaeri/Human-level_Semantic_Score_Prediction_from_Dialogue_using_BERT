      "utterance": [
        {
          "id": "MDRW2100002351.1.1",
          "form": "졸업하고 학교 가본적있어?",
          "original_form": "졸업하고 학교 가본적있어?",
          "speaker_id": "1",
          "time": "20210820 10:09"
        },
        {
          "id": "MDRW2100002351.1.2",
          "form": "응 버스정류장에 딱 도착하는순간 너무 좋아서 눈물이 핑 돌던데",
          "original_form": "응 버스정류장에 딱 도착하는순간 너무 좋아서 눈물이 핑 돌던데",
          "speaker_id": "2",
          "time": "20210820 10:09"
        },
        {
          "id": "MDRW2100002351.1.3",
          "form": "학교 언재가봣어?",
          "original_form": "학교 언재가봣어?",
          "speaker_id": "1",
          "time": "20210820 10:09"
        },
        {
          "id": "MDRW2100002351.1.4",
          "form": "학교 건물을 보고 기분이 들뜨는것은 수업을 안들어서겠지?",
          "original_form": "학교 건물을 보고 기분이 들뜨는것은 수업을 안들어서겠지?",
          "speaker_id": "2",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002351.1.5",
          "form": "그건 기억이 안나네...",
          "original_form": "그건 기억이 안나네...",
          "speaker_id": "2",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002351.1.6",
          "form": "아무래도그렇지",
          "original_form": "아무래도그렇지",
          "speaker_id": "1",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002351.1.7",
          "form": "교수님 만나러 같이 가기로 했던 거 기억 나?",
          "original_form": "교수님 만나러 같이 가기로 했던 거 기억 나?",
          "speaker_id": "2",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002351.1.8",
          "form": "교수님이 우리를 기다리실 거 같아",
          "original_form": "교수님이 우리를 기다리실 거 같아",
          "speaker_id": "2",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002351.1.9",
          "form": "헉 완전 까먹고잇엇어",
          "original_form": "헉 완전 까먹고잇엇어",
          "speaker_id": "1",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002351.1.10",
          "form": "올해가볼까",
          "original_form": "올해가볼까",
          "speaker_id": "1",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002351.1.11",
          "form": "근데교수님 계시나",
          "original_form": "근데교수님 계시나",
          "speaker_id": "1",
          "time": "20210820 10:10"
        },
        {
          "id": "MDRW2100002351.1.12",
          "form": "당연하지! 그 근처 사시잖아",
          "original_form": "당연하지! 그 근처 사시잖아",
          "speaker_id": "2",
          "time": "20210820 10:11"
        },
        {
          "id": "MDRW2100002351.1.13",
          "form": "앗 그래? 테르미니 사달라해야갰다",
          "original_form": "앗 그래? 테르미니 사달라해야갰다",
          "speaker_id": "1",
          "time": "20210820 10:11"
        },
        {
          "id": "MDRW2100002351.1.14",
          "form": "테르미니 자주갔는데 너랑",
          "original_form": "테르미니 자주갔는데 너랑",
          "speaker_id": "1",
          "time": "20210820 10:11"
        },
        {
          "id": "MDRW2100002351.1.15",
          "form": "기억나니",
          "original_form": "기억나니",
          "speaker_id": "1",
          "time": "20210820 10:11"
        },
        {
          "id": "MDRW2100002351.1.16",
          "form": "나 아닌데 누구랑 자주갔니...?",
          "original_form": "나 아닌데 누구랑 자주갔니...?",
          "speaker_id": "2",
          "time": "20210820 10:11"
        },
        {
          "id": "MDRW2100002351.1.17",
          "form": "name3이 거기서 일할 때 좋았는데",
          "original_form": "&name3&이 거기서 일할 때 좋았는데",
          "speaker_id": "2",
          "time": "20210820 10:12"
        },
        {
          "id": "MDRW2100002351.1.18",
          "form": "자주갔어 ㅡㅡ",
          "original_form": "자주갔어 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210820 10:12"
        },
        {
          "id": "MDRW2100002351.1.19",
          "form": "샐러드파스타 많이먹엇는대",
          "original_form": "샐러드파스타 많이먹엇는대",
          "speaker_id": "1",
          "time": "20210820 10:12"
        },
        {
          "id": "MDRW2100002351.1.20",
          "form": "그랬던 거 같기도 하고...",
          "original_form": "그랬던 거 같기도 하고...",
          "speaker_id": "2",
          "time": "20210820 10:13"
        },
        {
          "id": "MDRW2100002351.1.21",
          "form": "학교 앞에 오던 타코야끼 트럭 기억나?",
          "original_form": "학교 앞에 오던 타코야끼 트럭 기억나?",
          "speaker_id": "2",
          "time": "20210820 10:13"
        },
        {
          "id": "MDRW2100002351.1.22",
          "form": "밀가루 반죽뿐이엇지만 맛잇엇던 타코야끼..",
          "original_form": "밀가루 반죽뿐이엇지만 맛잇엇던 타코야끼..",
          "speaker_id": "1",
          "time": "20210820 10:13"
        },
        {
          "id": "MDRW2100002351.1.23",
          "form": "양 많이 줘서 이슈였다가 노상방뇨하고 손안씻어서 이슈로 바뀌었지..........",
          "original_form": "양 많이 줘서 이슈였다가 노상방뇨하고 손안씻어서 이슈로 바뀌었지..........",
          "speaker_id": "2",
