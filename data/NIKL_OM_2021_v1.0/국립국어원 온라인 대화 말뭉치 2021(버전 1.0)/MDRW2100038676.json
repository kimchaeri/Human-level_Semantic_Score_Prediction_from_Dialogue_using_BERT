        },
        {
          "id": "MDRW2100038676.1.2",
          "form": "어ㄱㅈㅋㅈㄱㅈㄱ",
          "original_form": "어ㄱㅈㅋㅈㄱㅈㄱ",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100038676.1.3",
          "form": "볶음면극찬",
          "original_form": "볶음면극찬",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100038676.1.4",
          "form": "비오니수제비때리구싶다",
          "original_form": "비오니수제비때리구싶다",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100038676.1.5",
          "form": "내가 잘못한건가 ㅋㅋㅋ",
          "original_form": "내가 잘못한건가 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100038676.1.6",
          "form": "오오수제비 맛집 아니",
          "original_form": "오오수제비 맛집 아니",
          "speaker_id": "1",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100038676.1.7",
          "form": "쌀쌀하네 이재",
          "original_form": "쌀쌀하네 이재",
          "speaker_id": "1",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100038676.1.8",
          "form": "잘못한건없지ㅋㄱㄱㅋㅋ",
          "original_form": "잘못한건없지ㅋㄱㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:57"
        },
        {
          "id": "MDRW2100038676.1.9",
          "form": "아니맛집몰라 ㅠ0 ㅠㅡ수제비맛집도있낭",
          "original_form": "아니맛집몰라 ㅠ0 ㅠㅡ수제비맛집도있낭",
          "speaker_id": "2",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100038676.1.10",
          "form": "부산에",
          "original_form": "부산에",
          "speaker_id": "2",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100038676.1.11",
          "form": "몰라",
          "original_form": "몰라",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100038676.1.12",
          "form": "수제비 꿇여먹어?",
          "original_form": "수제비 꿇여먹어?",
          "speaker_id": "1",
          "time": "20210902 18:58"
        },
        {
          "id": "MDRW2100038676.1.13",
          "form": "아니 못해먹어\r\nㅋㅋㅋㅋㅋㄱ비가아직많이오는감",
          "original_form": "아니 못해먹어\r\nㅋㅋㅋㅋㅋㄱ비가아직많이오는감",
          "speaker_id": "2",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100038676.1.14",
          "form": "내리는거보입니깍",
          "original_form": "내리는거보입니깍",
          "speaker_id": "2",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100038676.1.15",
          "form": "비 안오는거같은데?",
          "original_form": "비 안오는거같은데?",
          "speaker_id": "1",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100038676.1.16",
          "form": "오늘 좀 쌀쌀하니 좋다",
          "original_form": "오늘 좀 쌀쌀하니 좋다",
          "speaker_id": "1",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100038676.1.17",
          "form": "얼른 쾌청한 날씨...",
          "original_form": "얼른 쾌청한 날씨...",
          "speaker_id": "1",
          "time": "20210902 18:59"
        },
        {
          "id": "MDRW2100038676.1.18",
          "form": "아 그친건가봐오모낭",
          "original_form": "아 그친건가봐오모낭",
          "speaker_id": "2",
          "time": "20210902 19:26"
