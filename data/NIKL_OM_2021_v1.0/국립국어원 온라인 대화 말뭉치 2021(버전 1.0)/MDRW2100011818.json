      "utterance": [
        {
          "id": "MDRW2100011818.1.1",
          "form": "학교 강의듣는데 너무 듣기싫어요??",
          "original_form": "학교 강의듣는데 너무 듣기싫어요??",
          "speaker_id": "1",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100011818.1.2",
          "form": "아 학생이세요??",
          "original_form": "아 학생이세요??",
          "speaker_id": "2",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100011818.1.3",
          "form": "방통대 다니구있습니다ㅜㅜ",
          "original_form": "방통대 다니구있습니다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100011818.1.4",
          "form": "직장이랑 병행하기 너무 힘드네요",
          "original_form": "직장이랑 병행하기 너무 힘드네요",
          "speaker_id": "1",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100011818.1.5",
          "form": "아이고 하나만 하기도 힘든데 병행이라니..",
          "original_form": "아이고 하나만 하기도 힘든데 병행이라니..",
          "speaker_id": "2",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100011818.1.6",
          "form": "방통대는 벌써 개강 했나요??",
          "original_form": "방통대는 벌써 개강 했나요??",
          "speaker_id": "2",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100011818.1.7",
          "form": "지금은 강의듣고있옹ㅅ",
          "original_form": "지금은 강의듣고있옹ㅅ",
          "speaker_id": "1",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011818.1.8",
          "form": "개 강 했습니다",
          "original_form": "개 강 했습니다",
          "speaker_id": "1",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011818.1.9",
          "form": "과제해야합니다 이제..",
          "original_form": "과제해야합니다 이제..",
          "speaker_id": "1",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011818.1.10",
          "form": "어우 엄청 빠르네요..",
          "original_form": "어우 엄청 빠르네요..",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011818.1.11",
          "form": "저도 9월에 개강인데 너무 싫어요..",
          "original_form": "저도 9월에 개강인데 너무 싫어요..",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011818.1.12",
          "form": "ㅠㅠㅠㅠㅜ진짜코로나때문에",
          "original_form": "ㅠㅠㅠㅠㅜ진짜코로나때문에",
          "speaker_id": "1",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100011818.1.13",
          "form": "과제지옥된거",
          "original_form": "과제지옥된거",
          "speaker_id": "1",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100011818.1.14",
          "form": "너무힘들지랂나요",
          "original_form": "너무힘들지랂나요",
          "speaker_id": "1",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100011818.1.15",
          "form": "아... 진짜 맞아요",
          "original_form": "아... 진짜 맞아요",
          "speaker_id": "2",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100011818.1.16",
          "form": "차라리 학교가는게나을거같기두하고",
          "original_form": "차라리 학교가는게나을거같기두하고",
          "speaker_id": "1",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100011818.1.17",
          "form": "전 이번이 막학기인데 학교 못가고 끝나는거ㅜ아쉬워서",
          "original_form": "전 이번이 막학기인데 학교 못가고 끝나는거ㅜ아쉬워서",
          "speaker_id": "2",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100011818.1.18",
          "form": "일부러 대면수업도 잡았어요",
          "original_form": "일부러 대면수업도 잡았어요",
          "speaker_id": "2",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100011818.1.19",
          "form": "ㅠㅠㅠㅠㅠ막학기면 더 아쉽겠다...",
          "original_form": "ㅠㅠㅠㅠㅠ막학기면 더 아쉽겠다...",
          "speaker_id": "1",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100011818.1.20",
          "form": "진짜 학교가는게 그리워지다니;;;",
          "original_form": "진짜 학교가는게 그리워지다니;;;",
          "speaker_id": "1",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100011818.1.21",
          "form": "그래도 코로나때문에 성적은 더 잘 줘서",
          "original_form": "그래도 코로나때문에 성적은 더 잘 줘서",
          "speaker_id": "2",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100011818.1.22",
          "form": "오 저희두!!!!",
          "original_form": "오 저희두!!!!",
          "speaker_id": "1",
          "time": "20210824 17:16"
        },
        {
          "id": "MDRW2100011818.1.23",
          "form": "나쁜걱 같지만은 않아요",
          "original_form": "나쁜걱 같지만은 않아요",
