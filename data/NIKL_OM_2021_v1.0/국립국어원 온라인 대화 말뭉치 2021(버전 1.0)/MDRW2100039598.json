      "utterance": [
        {
          "id": "MDRW2100039598.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210904 12:00"
        },
        {
          "id": "MDRW2100039598.1.2",
          "form": "아니요! 어릴땐 키웠어요!",
          "original_form": "아니요! 어릴땐 키웠어요!",
          "speaker_id": "2",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100039598.1.3",
          "form": "아 그러셧구나",
          "original_form": "아 그러셧구나",
          "speaker_id": "1",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100039598.1.4",
          "form": "전 지금 키워요",
          "original_form": "전 지금 키워요",
          "speaker_id": "1",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100039598.1.5",
          "form": "몇살인가요",
          "original_form": "몇살인가요",
          "speaker_id": "2",
          "time": "20210904 12:28"
        },
        {
          "id": "MDRW2100039598.1.6",
          "form": "1살이여ㅎ",
          "original_form": "1살이여ㅎ",
          "speaker_id": "1",
          "time": "20210904 15:21"
        },
        {
          "id": "MDRW2100039598.1.7",
          "form": "강아진가용",
          "original_form": "강아진가용",
          "speaker_id": "2",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100039598.1.8",
          "form": "네 비숑이에요",
          "original_form": "네 비숑이에요",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100039598.1.9",
          "form": "산책 자주 나가시나요",
          "original_form": "산책 자주 나가시나요",
          "speaker_id": "2",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100039598.1.10",
          "form": "하루에 한번씩 하려고 노력해요ㅎ",
          "original_form": "하루에 한번씩 하려고 노력해요ㅎ",
          "speaker_id": "1",
          "time": "20210904 19:22"
        },
        {
          "id": "MDRW2100039598.1.11",
          "form": "산책 나가자고 하면 알아듣나요? ㅋㅋ 티비보면 막 신나하던데",
          "original_form": "산책 나가자고 하면 알아듣나요? ㅋㅋ 티비보면 막 신나하던데",
          "speaker_id": "2",
          "time": "20210904 20:06"
        },
        {
          "id": "MDRW2100039598.1.12",
          "form": "네 가자~~하면 좋아해요ㅎ",
          "original_form": "네 가자~~하면 좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210904 22:19"
        },
        {
          "id": "MDRW2100039598.1.13",
          "form": "ㅋㅋㅋ 강아지들 그런거 알아듣는 거 너무 신기해요 ㅎㅎ",
          "original_form": "ㅋㅋㅋ 강아지들 그런거 알아듣는 거 너무 신기해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100039598.1.14",
          "form": "개인기도 있나요? ㅎㅎ",
          "original_form": "개인기도 있나요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100039598.1.15",
          "form": "앉아 하면 앉는정도ㅋ",
          "original_form": "앉아 하면 앉는정도ㅋ",
          "speaker_id": "1",
          "time": "20210904 23:17"
