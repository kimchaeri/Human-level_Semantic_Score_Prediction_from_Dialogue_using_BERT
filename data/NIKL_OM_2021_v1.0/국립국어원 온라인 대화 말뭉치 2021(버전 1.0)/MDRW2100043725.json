      "utterance": [
        {
          "id": "MDRW2100043725.1.1",
          "form": "쫀득..! 으으으으음",
          "original_form": "쫀득..! 으으으으음",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043725.1.2",
          "form": "크림이 들어가나요?",
          "original_form": "크림이 들어가나요?",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043725.1.3",
          "form": "네....! 크림인데 이 음식만의 크림 호칭이 있어요!",
          "original_form": "네....! 크림인데 이 음식만의 크림 호칭이 있어요!",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043725.1.4",
          "form": "핫 슈크림이 들어가나보군요 맞죠?",
          "original_form": "핫 슈크림이 들어가나보군요 맞죠?",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043725.1.5",
          "form": "안타깝게도 슈크림이 아니에요ㅠ",
          "original_form": "안타깝게도 슈크림이 아니에요ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043725.1.6",
          "form": "약간 단단한 크림이랍니다",
          "original_form": "약간 단단한 크림이랍니다",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043725.1.7",
          "form": "으으으으으으으음 단단한 크림이라..",
          "original_form": "으으으으으으으음 단단한 크림이라..",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043725.1.8",
          "form": "차가운 음식인가요?",
          "original_form": "차가운 음식인가요?",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043725.1.9",
          "form": "네!!! 시원하게 보관해야해요",
          "original_form": "네!!! 시원하게 보관해야해요",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043725.1.10",
          "form": "냉동은 아니구요",
          "original_form": "냉동은 아니구요",
          "speaker_id": "1",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043725.1.11",
          "form": "아아아아 어떤 거지",
          "original_form": "아아아아 어떤 거지",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043725.1.12",
          "form": "크림이 흰색인가요?",
          "original_form": "크림이 흰색인가요?",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043725.1.13",
          "form": "아니요! 크림색도 다양하답니다",
          "original_form": "아니요! 크림색도 다양하답니다",
          "speaker_id": "1",
          "time": "20210907 20:18"
        },
        {
          "id": "MDRW2100043725.1.14",
          "form": "ㅋㅋㅋㅋ이전 힌트 까먹었어요 빵이 들어간댔나요?",
          "original_form": "ㅋㅋㅋㅋ이전 힌트 까먹었어요 빵이 들어간댔나요?",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100043725.1.15",
          "form": "빵의 일종일수있어요...! 쫀득쫀득하고 시원하게 보관하는 디저트랍니다",
          "original_form": "빵의 일종일수있어요...! 쫀득쫀득하고 시원하게 보관하는 디저트랍니다",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100043725.1.16",
          "form": "앗... 그럼 혹시 케이크류인가요?",
          "original_form": "앗... 그럼 혹시 케이크류인가요?",
          "speaker_id": "2",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043725.1.17",
          "form": "케이크 아니에요!",
          "original_form": "케이크 아니에요!",
          "speaker_id": "1",
          "time": "20210907 20:21"
        },
        {
          "id": "MDRW2100043725.1.18",
          "form": "이게 프랑스 디저트였나...?",
          "original_form": "이게 프랑스 디저트였나...?",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043725.1.19",
          "form": "악! 어떤거지",
          "original_form": "악! 어떤거지",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043725.1.20",
          "form": "유럽어디쪽인데 기억이 잘 안나네요ㅋㅋ",
          "original_form": "유럽어디쪽인데 기억이 잘 안나네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043725.1.21",
          "form": "얇은것도 있고 뚱띠한거도 있어요ㅋㅋ",
          "original_form": "얇은것도 있고 뚱띠한거도 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043725.1.22",
          "form": "혹시 갈색 음식인가요?",
          "original_form": "혹시 갈색 음식인가요?",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043725.1.23",
          "form": "뚱띠 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "뚱띠 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043725.1.24",
          "form": "갈색도 있고 색은 다양해요!!!",
          "original_form": "갈색도 있고 색은 다양해요!!!",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100043725.1.25",
          "form": "얇은것도 있고 뚱뚱한것도 있는 그 디저트...!",
          "original_form": "얇은것도 있고 뚱뚱한것도 있는 그 디저트...!",
          "speaker_id": "1",
          "time": "20210907 20:23"
        }
      ]
    }
  ]
