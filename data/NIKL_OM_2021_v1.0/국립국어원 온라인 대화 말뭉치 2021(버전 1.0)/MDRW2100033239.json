      "utterance": [
        {
          "id": "MDRW2100033239.1.1",
          "form": "밖에 비가 많이와요",
          "original_form": "밖에 비가 많이와요",
          "speaker_id": "1",
          "time": "20210906 20:04"
        },
        {
          "id": "MDRW2100033239.1.2",
          "form": "저희는 조금 와요",
          "original_form": "저희는 조금 와요",
          "speaker_id": "2",
          "time": "20210906 20:32"
        },
        {
          "id": "MDRW2100033239.1.3",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100033239.1.4",
          "form": "저 퇴근할땐 많이왔었어요",
          "original_form": "저 퇴근할땐 많이왔었어요",
          "speaker_id": "1",
          "time": "20210906 21:29"
        },
        {
          "id": "MDRW2100033239.1.5",
          "form": "아.. 힘드셨겠네요 ㅠ 비올때 퇴근이라니 ㅠ",
          "original_form": "아.. 힘드셨겠네요 ㅠ 비올때 퇴근이라니 ㅠ",
          "speaker_id": "2",
          "time": "20210906 21:30"
        },
        {
          "id": "MDRW2100033239.1.6",
          "form": "내일도 비가 온다네요ㅜㅜ",
          "original_form": "내일도 비가 온다네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 21:58"
        },
        {
          "id": "MDRW2100033239.1.7",
          "form": "으어...........",
          "original_form": "으어...........",
          "speaker_id": "2",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100033239.1.8",
          "form": "가을장마래요",
          "original_form": "가을장마래요",
          "speaker_id": "1",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100033239.1.9",
          "form": "저도 그 소리는 들어본거같아요 ㅎㅎ",
          "original_form": "저도 그 소리는 들어본거같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100033239.1.10",
          "form": "비를 싫어해서 그런지... 그만왔으면 싶네요 ㅋㅋ",
          "original_form": "비를 싫어해서 그런지... 그만왔으면 싶네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100033239.1.11",
          "form": "저도 이제는 좀 그만 왓으면 ㅠㅠ해요..... 여름도 가고 비도 그만오고",
          "original_form": "저도 이제는 좀 그만 왓으면 ㅠㅠ해요..... 여름도 가고 비도 그만오고",
          "speaker_id": "2",
          "time": "20210906 23:17"
        },
        {
          "id": "MDRW2100033239.1.12",
          "form": "가을이 좀 맑고 화창해야 되는데 말이죠",
          "original_form": "가을이 좀 맑고 화창해야 되는데 말이죠",
          "speaker_id": "1",
          "time": "20210906 23:18"
        },
        {
          "id": "MDRW2100033239.1.13",
          "form": "그러니까요 ㅠㅠㅠ 가을을 느끼고 싶습니다",
          "original_form": "그러니까요 ㅠㅠㅠ 가을을 느끼고 싶습니다",
          "speaker_id": "2",
          "time": "20210906 23:18"
        },
        {
          "id": "MDRW2100033239.1.14",
          "form": "맨날 퍼붓고있으니ㅜㅜ",
          "original_form": "맨날 퍼붓고있으니ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 23:19"
        },
        {
          "id": "MDRW2100033239.1.15",
          "form": "이러다가 금방 추워지겠죠?",
          "original_form": "이러다가 금방 추워지겠죠?",
          "speaker_id": "1",
          "time": "20210906 23:19"
        },
        {
          "id": "MDRW2100033239.1.16",
          "form": "네... 지금도 밤낮은 쌀쌀하지 않나요?",
          "original_form": "네... 지금도 밤낮은 쌀쌀하지 않나요?",
          "speaker_id": "2",
          "time": "20210906 23:20"
        },
        {
          "id": "MDRW2100033239.1.17",
