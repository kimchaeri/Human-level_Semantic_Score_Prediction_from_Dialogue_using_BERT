      "utterance": [
        {
          "id": "MDRW2100013996.1.1",
          "form": "곰표 팝콘 나온거 봤어?",
          "original_form": "곰표 팝콘 나온거 봤어?",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013996.1.2",
          "form": "ㅇㅇ 넘 귀여워",
          "original_form": "ㅇㅇ 넘 귀여워",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013996.1.3",
          "form": "씨유 편의점에 판다던데",
          "original_form": "씨유 편의점에 판다던데",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013996.1.4",
          "form": "한번 먹어보고싶다ㅋㅋ",
          "original_form": "한번 먹어보고싶다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013996.1.5",
          "form": "좋아 같이 사러가자",
          "original_form": "좋아 같이 사러가자",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013996.1.6",
          "form": "곰표 젤리도 있다던데 그것도 사고싶다",
          "original_form": "곰표 젤리도 있다던데 그것도 사고싶다",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013996.1.7",
          "form": "젤리도 좋다ㅋㅋ 그건 곰모양인가?",
          "original_form": "젤리도 좋다ㅋㅋ 그건 곰모양인가?",
          "speaker_id": "1",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013996.1.8",
          "form": "ㅇㅇ 곰 모양이야",
          "original_form": "ㅇㅇ 곰 모양이야",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013996.1.9",
          "form": "민트맛이래",
          "original_form": "민트맛이래",
          "speaker_id": "2",
          "time": "20210825 16:47"
        },
        {
          "id": "MDRW2100013996.1.10",
          "form": "초록색이랑 하얀색 있대",
          "original_form": "초록색이랑 하얀색 있대",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013996.1.11",
          "form": "초록곰 귀엽겠다ㅋㅋ",
          "original_form": "초록곰 귀엽겠다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013996.1.12",
          "form": "나중에 같이 사러가자",
          "original_form": "나중에 같이 사러가자",
          "speaker_id": "1",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013996.1.13",
          "form": "곰표 아이스크림도 있대 넘 먹고싶다ㅠㅠ",
          "original_form": "곰표 아이스크림도 있대 넘 먹고싶다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013996.1.14",
          "form": "우리동네 편의점에 팔라나ㅠ 있음 좋겠다",
          "original_form": "우리동네 편의점에 팔라나ㅠ 있음 좋겠다",
          "speaker_id": "1",
          "time": "20210825 16:49"
        },
        {
          "id": "MDRW2100013996.1.15",
          "form": "곰모양 넘 귀여워서 콜라보제품 많아져서 좋다ㅋㅋ",
          "original_form": "곰모양 넘 귀여워서 콜라보제품 많아져서 좋다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:50"
        },
        {
          "id": "MDRW2100013996.1.16",
          "form": "그러게 레트로 유행이라고 이런거 하나봐",
          "original_form": "그러게 레트로 유행이라고 이런거 하나봐",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013996.1.17",
          "form": "나름 추억돋고 좋다ㅋㅋ",
          "original_form": "나름 추억돋고 좋다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100013996.1.18",
          "form": "그럼 이따 편의점 같이가는거다!!",
          "original_form": "그럼 이따 편의점 같이가는거다!!",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100013996.1.19",
          "form": "오키오키 같이가자",
          "original_form": "오키오키 같이가자",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100013996.1.20",
          "form": "가는길에 두껍 떡볶이인가 그것도 사고싶은데 있으려나",
          "original_form": "가는길에 두껍 떡볶이인가 그것도 사고싶은데 있으려나",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100013996.1.21",
          "form": "헉 떡볶이도 나왔어?!?",
          "original_form": "헉 떡볶이도 나왔어?!?",
          "speaker_id": "2",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100013996.1.22",
          "form": "그렇대 요즘 두꺼비 잘나가ㅋㅋ",
          "original_form": "그렇대 요즘 두꺼비 잘나가ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100013996.1.23",
          "form": "맛도 있대서 궁금해",
          "original_form": "맛도 있대서 궁금해",
          "speaker_id": "1",
