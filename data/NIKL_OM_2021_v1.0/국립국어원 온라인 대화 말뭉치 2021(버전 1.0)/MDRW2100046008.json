        },
        {
          "id": "MDRW2100046008.1.2",
          "form": "그럼 안 돼",
          "original_form": "그럼 안 돼",
          "speaker_id": "1",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100046008.1.3",
          "form": "그래도 살 빼야 돼",
          "original_form": "그래도 살 빼야 돼",
          "speaker_id": "2",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100046008.1.4",
          "form": "그러다 건강만 가져",
          "original_form": "그러다 건강만 가져",
