        {
          "id": "MDRW2100028498.1.5",
          "form": "버리러?",
          "original_form": "버리러?",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028498.1.6",
          "form": "ㅇㅇ그렇지",
          "original_form": "ㅇㅇ그렇지",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028498.1.7",
          "form": "오늘 나갔을때 버리지",
          "original_form": "오늘 나갔을때 버리지",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028498.1.8",
          "form": "낼 또 가?",
          "original_form": "낼 또 가?",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028498.1.9",
          "form": "야",
          "original_form": "야",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028498.1.10",
          "form": "쓰",
          "original_form": "쓰",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028498.1.11",
          "form": "레기",
          "original_form": "레기",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028498.1.12",
          "form": "얼마나 많이 나오는지 아냐?",
          "original_form": "얼마나 많이 나오는지 아냐?",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028498.1.13",
          "form": "많이 나오긴 하지",
          "original_form": "많이 나오긴 하지",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028498.1.14",
          "form": "근데 토욜에",
          "original_form": "근데 토욜에",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028498.1.15",
          "form": "택배 올 거 있는데",
          "original_form": "택배 올 거 있는데",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028498.1.16",
          "form": "그거까지 합쳐서 갈까",
          "original_form": "그거까지 합쳐서 갈까",
          "speaker_id": "2",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028498.1.17",
          "form": "ㅇㅇ한번에",
          "original_form": "ㅇㅇ한번에",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028498.1.18",
          "form": "엄마도 택배 올거 있잖아",
          "original_form": "엄마도 택배 올거 있잖아",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028498.1.19",
          "form": "아 그러니까 롯데택배 왜 안오냐",
          "original_form": "아 그러니까 롯데택배 왜 안오냐",
          "speaker_id": "2",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028498.1.20",
          "form": "롯데택배 원래 일 느려",
          "original_form": "롯데택배 원래 일 느려",
          "speaker_id": "1",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028498.1.21",
          "form": "바로바로 처리 안 하더라",
          "original_form": "바로바로 처리 안 하더라",
          "speaker_id": "1",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028498.1.22",
          "form": "거기 짜증난다",
          "original_form": "거기 짜증난다",
