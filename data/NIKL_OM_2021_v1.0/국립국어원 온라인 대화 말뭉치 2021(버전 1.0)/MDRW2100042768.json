        {
          "id": "MDRW2100042768.1.1",
          "form": "그거 커피 대신",
          "original_form": "그거 커피 대신",
          "speaker_id": "2",
          "time": "20210907 08:47"
        },
        {
          "id": "MDRW2100042768.1.2",
          "form": "마시는건데",
          "original_form": "마시는건데",
          "speaker_id": "2",
          "time": "20210907 08:47"
        },
        {
          "id": "MDRW2100042768.1.3",
          "form": "커피를 포기하지 못하겠다ㅠ",
          "original_form": "커피를 포기하지 못하겠다ㅠ",
          "speaker_id": "1",
          "time": "20210907 08:47"
        },
        {
          "id": "MDRW2100042768.1.4",
          "form": "커피마저 끊으면",
          "original_form": "커피마저 끊으면",
          "speaker_id": "1",
          "time": "20210907 08:47"
        },
        {
          "id": "MDRW2100042768.1.5",
          "form": "커피향이나는거래",
          "original_form": "커피향이나는거래",
          "speaker_id": "2",
          "time": "20210907 08:47"
        },
        {
          "id": "MDRW2100042768.1.6",
          "form": "삶의 의욕이 없어질 것 같다ㅠ",
          "original_form": "삶의 의욕이 없어질 것 같다ㅠ",
          "speaker_id": "1",
          "time": "20210907 08:48"
        },
        {
          "id": "MDRW2100042768.1.7",
          "form": "근데 그거 마시고 살이빠졌다네",
          "original_form": "근데 그거 마시고 살이빠졌다네",
          "speaker_id": "2",
          "time": "20210907 08:48"
        },
        {
          "id": "MDRW2100042768.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:48"
        },
        {
          "id": "MDRW2100042768.1.9",
          "form": "수시로 마시는 거야?",
          "original_form": "수시로 마시는 거야?",
          "speaker_id": "1",
          "time": "20210907 08:48"
        },
        {
          "id": "MDRW2100042768.1.10",
          "form": "나는  그거 마실까싶어서ㅋㅋㅋ커피대신",
          "original_form": "나는  그거 마실까싶어서ㅋㅋㅋ커피대신",
          "speaker_id": "2",
          "time": "20210907 08:48"
        },
        {
          "id": "MDRW2100042768.1.11",
          "form": "수시로 마시려면 우엉차가 또 효과가있다든데",
          "original_form": "수시로 마시려면 우엉차가 또 효과가있다든데",
          "speaker_id": "2",
          "time": "20210907 08:48"
        },
        {
          "id": "MDRW2100042768.1.12",
          "form": "난 마시는 거는",
          "original_form": "난 마시는 거는",
          "speaker_id": "1",
          "time": "20210907 08:48"
        },
        {
          "id": "MDRW2100042768.1.13",
          "form": "힘들 것 같다ㅋㅋㅋㅋ",
          "original_form": "힘들 것 같다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:49"
        },
        {
          "id": "MDRW2100042768.1.14",
          "form": "여튼ㅋㅋㅋㅋㅋㅋㅋㅋ웃긴건 알아보고는있다는 건데 ㅋㅋ",
          "original_form": "여튼ㅋㅋㅋㅋㅋㅋㅋㅋ웃긴건 알아보고는있다는 건데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:49"
        },
        {
          "id": "MDRW2100042768.1.15",
          "form": "은근히 물 마시는 것도 힘든ㅋㅋㅋㅋ",
          "original_form": "은근히 물 마시는 것도 힘든ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:49"
        },
        {
          "id": "MDRW2100042768.1.16",
          "form": "중요한건",
          "original_form": "중요한건",
          "speaker_id": "2",
          "time": "20210907 08:49"
        },
        {
          "id": "MDRW2100042768.1.17",
          "form": "운동안함",
