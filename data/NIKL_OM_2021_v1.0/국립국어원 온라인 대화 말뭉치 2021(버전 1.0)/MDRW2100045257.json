      "utterance": [
        {
          "id": "MDRW2100045257.1.1",
          "form": "아직",
          "original_form": "아직",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045257.1.2",
          "form": "사실 너무 귀찮아서",
          "original_form": "사실 너무 귀찮아서",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045257.1.3",
          "form": "걍 꼬르륵거리면서 잘까 말까",
          "original_form": "걍 꼬르륵거리면서 잘까 말까",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045257.1.4",
          "form": "고민중이야....",
          "original_form": "고민중이야....",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045257.1.5",
          "form": "이래서 요새",
          "original_form": "이래서 요새",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045257.1.6",
          "form": "밥을 안먹게된다니까",
          "original_form": "밥을 안먹게된다니까",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100045257.1.7",
          "form": "ㅋㅋㅋㅋ큐ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ큐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045257.1.8",
          "form": "뭐라도 먹어...",
          "original_form": "뭐라도 먹어...",
          "speaker_id": "1",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045257.1.9",
          "form": "ㅇㅇ결심을 다지고",
          "original_form": "ㅇㅇ결심을 다지고",
          "speaker_id": "2",
          "time": "20210908 01:35"
        },
        {
          "id": "MDRW2100045257.1.10",
          "form": "일어났어",
          "original_form": "일어났어",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045257.1.11",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045257.1.12",
          "form": "물을 끓이고 만두를 돌리겟다",
          "original_form": "물을 끓이고 만두를 돌리겟다",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045257.1.13",
          "form": "그래 간단한거라도 먹어",
          "original_form": "그래 간단한거라도 먹어",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045257.1.14",
          "form": "커피를 마실까 했는데",
          "original_form": "커피를 마실까 했는데",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045257.1.15",
          "form": "요새 염증이 자꾸 생겨서",
          "original_form": "요새 염증이 자꾸 생겨서",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045257.1.16",
          "form": "물을 또",
          "original_form": "물을 또",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045257.1.17",
          "form": "2리터씩",
          "original_form": "2리터씩",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045257.1.18",
          "form": "부어줘야겠어",
          "original_form": "부어줘야겠어",
          "speaker_id": "2",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045257.1.19",
          "form": "ㅇㅎ 그럼 커피 마시면 안되지",
          "original_form": "ㅇㅎ 그럼 커피 마시면 안되지",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045257.1.20",
          "form": "졸린데 지금 자고싶지 않아....",
          "original_form": "졸린데 지금 자고싶지 않아....",
          "speaker_id": "2",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045257.1.21",
          "form": "물 많이 마셔도",
          "original_form": "물 많이 마셔도",
          "speaker_id": "1",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045257.1.22",
          "form": "근육이 없으면 체수분 저장이 안된대",
          "original_form": "근육이 없으면 체수분 저장이 안된대",
          "speaker_id": "1",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045257.1.23",
          "form": "나 근육은 많은데",
          "original_form": "나 근육은 많은데",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045257.1.24",
          "form": "그냥 물을 ㄹㅇ오지게 안마시는걸껄",
          "original_form": "그냥 물을 ㄹㅇ오지게 안마시는걸껄",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045257.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 물을 마셔...!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 물을 마셔...!",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045257.1.26",
          "form": "근데 2리터면 많이 마시는건데?",
          "original_form": "근데 2리터면 많이 마시는건데?",
          "speaker_id": "1",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045257.1.27",
          "form": "아 그거는",
          "original_form": "아 그거는",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045257.1.28",
          "form": "염증 생기면",
          "original_form": "염증 생기면",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045257.1.29",
          "form": "아 슬슬 마셔야할 때구나",
          "original_form": "아 슬슬 마셔야할 때구나",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045257.1.30",
          "form": "하고 몰아 마시는거라......",
          "original_form": "하고 몰아 마시는거라......",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045257.1.31",
          "form": "와 일케쓰니까",
          "original_form": "와 일케쓰니까",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045257.1.32",
          "form": "ㄹㅇ 1도 안건강해보이고",
          "original_form": "ㄹㅇ 1도 안건강해보이고",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045257.1.33",
          "form": "건강 조진사람같네",
          "original_form": "건강 조진사람같네",
          "speaker_id": "2",
          "time": "20210908 01:39"
        },
        {
          "id": "MDRW2100045257.1.34",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045257.1.35",
          "form": "만두만 먹게?",
          "original_form": "만두만 먹게?",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045257.1.36",
          "form": "아니 컵라면도 먹을거야",
          "original_form": "아니 컵라면도 먹을거야",
          "speaker_id": "2",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045257.1.37",
          "form": "ㅋㅋㅋㅋㅋ 역시 스파게티 콕콕을 빼놓을 수 없지",
          "original_form": "ㅋㅋㅋㅋㅋ 역시 스파게티 콕콕을 빼놓을 수 없지",
          "speaker_id": "1",
          "time": "20210908 01:41"
        }
      ]
