      "utterance": [
        {
          "id": "MDRW2100021973.1.1",
          "form": "어떤 일 하시나요",
          "original_form": "어떤 일 하시나요",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021973.1.2",
          "form": "저는 금융권에서 일하고있어요!",
          "original_form": "저는 금융권에서 일하고있어요!",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021973.1.3",
          "form": "오~~~ㅃ",
          "original_form": "오~~~ㅃ",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021973.1.4",
          "form": "뭔가 멋지네요",
          "original_form": "뭔가 멋지네요",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021973.1.5",
          "form": "경제관념이 투철하실것 같은",
          "original_form": "경제관념이 투철하실것 같은",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021973.1.6",
          "form": "어우 아니에요",
          "original_form": "어우 아니에요",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021973.1.7",
          "form": "name1은 무슨일하세요?",
          "original_form": "&name1&은 무슨일하세요?",
          "speaker_id": "2",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021973.1.8",
          "form": "돈 좋아하긴해여 ㅎ",
          "original_form": "돈 좋아하긴해여 ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021973.1.9",
          "form": "저는 누구든 주변에 한둘은 있을법한 종류의 사무직이에요",
          "original_form": "저는 누구든 주변에 한둘은 있을법한 종류의 사무직이에요",
          "speaker_id": "1",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021973.1.10",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021973.1.11",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021973.1.12",
          "form": "저도 돈은 좋아하지만 잘 못 법니다",
          "original_form": "저도 돈은 좋아하지만 잘 못 법니다",
          "speaker_id": "1",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021973.1.13",
          "form": "저도요 ㅎ",
          "original_form": "저도요 ㅎ",
          "speaker_id": "2",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021973.1.14",
          "form": "좋아만 해요..",
          "original_form": "좋아만 해요..",
          "speaker_id": "2",
          "time": "20210829 00:21"
        },
        {
          "id": "MDRW2100021973.1.15",
          "form": "발령이 잦아서 업무도 자주 바뀌는 편인데 저는 신기하게 계속 비슷한 계열의 일을 하고 있어요",
          "original_form": "발령이 잦아서 업무도 자주 바뀌는 편인데 저는 신기하게 계속 비슷한 계열의 일을 하고 있어요",
          "speaker_id": "1",
          "time": "20210829 00:23"
        },
        {
          "id": "MDRW2100021973.1.16",
          "form": "업무는 잘 맞으세요?",
          "original_form": "업무는 잘 맞으세요?",
          "speaker_id": "2",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021973.1.17",
          "form": "글쎄요",
          "original_form": "글쎄요",
          "speaker_id": "1",
          "time": "20210829 00:24"
        },
        {
          "id": "MDRW2100021973.1.18",
          "form": "다들 그러시겠지만 저는 일하는 것 자체가 적성에 안 맞는 것 같아요",
          "original_form": "다들 그러시겠지만 저는 일하는 것 자체가 적성에 안 맞는 것 같아요",
          "speaker_id": "1",
          "time": "20210829 00:25"
        },
        {
          "id": "MDRW2100021973.1.19",
          "form": "노는 거 최고",
          "original_form": "노는 거 최고",
          "speaker_id": "1",
          "time": "20210829 00:25"
        },
        {
          "id": "MDRW2100021973.1.20",
          "form": "ㅋㅋㅋㄱㅋㄱㅋㄱㄱㅋㅋㄱ그건 맞아요",
          "original_form": "ㅋㅋㅋㄱㅋㄱㅋㄱㄱㅋㅋㄱ그건 맞아요",
          "speaker_id": "2",
          "time": "20210829 00:25"
        },
        {
          "id": "MDRW2100021973.1.21",
          "form": "그냥 놀고싶습니다!",
          "original_form": "그냥 놀고싶습니다!",
          "speaker_id": "2",
          "time": "20210829 00:25"
        },
        {
          "id": "MDRW2100021973.1.22",
          "form": "근데.. 그러면서도 죽을때까지 일하고싶은 이상한 마음이에요",
          "original_form": "근데.. 그러면서도 죽을때까지 일하고싶은 이상한 마음이에요",
          "speaker_id": "2",
          "time": "20210829 00:25"
        },
        {
          "id": "MDRW2100021973.1.23",
          "form": "정말 놀고싶은데... 일을 놓고 싶지는 않아요",
          "original_form": "정말 놀고싶은데... 일을 놓고 싶지는 않아요",
          "speaker_id": "2",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100021973.1.24",
          "form": "아 그럼 부지런하신 거예요",
          "original_form": "아 그럼 부지런하신 거예요",
          "speaker_id": "1",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100021973.1.25",
          "form": "어우 저 진짜 게을러요",
          "original_form": "어우 저 진짜 게을러요",
          "speaker_id": "2",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100021973.1.26",
          "form": "전 로또 당첨을 노리며 대충살기 위해 열심히 사는 사람이라 해야할까요",
          "original_form": "전 로또 당첨을 노리며 대충살기 위해 열심히 사는 사람이라 해야할까요",
          "speaker_id": "1",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021973.1.27",
          "form": "제 책상... 할말은 많지만 할수없어요",
          "original_form": "제 책상... 할말은 많지만 할수없어요",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021973.1.28",
          "form": "로또 당첨...",
          "original_form": "로또 당첨...",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021973.1.29",
          "form": "간절해요",
          "original_form": "간절해요",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021973.1.30",
          "form": "사실 퇴사 생각하거든요 ㅋㅋㅋㅋ",
          "original_form": "사실 퇴사 생각하거든요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021973.1.31",
          "form": "로또 안 사거든요,,ㅋㅋㅋㅋㅋ",
          "original_form": "로또 안 사거든요,,ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021973.1.32",
          "form": "하싴ㅋㅋㅋㅋ 맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "하싴ㅋㅋㅋㅋ 맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100021973.1.33",
          "form": "사지도 않으면서 당첨 생각하는 사람ㅋㅋㅋㅋㅋ",
          "original_form": "사지도 않으면서 당첨 생각하는 사람ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100021973.1.34",
          "form": "로또는 안사지만",
          "original_form": "로또는 안사지만",
          "speaker_id": "2",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100021973.1.35",
          "form": "아 넘 재밌다ㅋㅋㅋㅋㅋ",
          "original_form": "아 넘 재밌다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:28"
        }
      ]
