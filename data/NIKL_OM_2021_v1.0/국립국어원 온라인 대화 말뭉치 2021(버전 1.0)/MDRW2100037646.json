      "utterance": [
        {
          "id": "MDRW2100037646.1.1",
          "form": "키우는 동물 있으세요?",
          "original_form": "키우는 동물 있으세요?",
          "speaker_id": "2",
          "time": "20210901 14:27"
        },
        {
          "id": "MDRW2100037646.1.2",
          "form": "네 강아지 키워여ㅎ",
          "original_form": "네 강아지 키워여ㅎ",
          "speaker_id": "1",
          "time": "20210901 14:28"
        },
        {
          "id": "MDRW2100037646.1.3",
          "form": "오~ 어떤 종이요?",
          "original_form": "오~ 어떤 종이요?",
          "speaker_id": "2",
          "time": "20210901 14:34"
        },
        {
          "id": "MDRW2100037646.1.4",
          "form": "전 예전에 말티즈 키웠었는데",
          "original_form": "전 예전에 말티즈 키웠었는데",
          "speaker_id": "2",
          "time": "20210901 15:03"
        },
        {
          "id": "MDRW2100037646.1.5",
          "form": "전 비숑이여ㄴㅅ",
          "original_form": "전 비숑이여ㄴㅅ",
          "speaker_id": "1",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100037646.1.6",
          "form": "말티즈도 이뻐요",
          "original_form": "말티즈도 이뻐요",
          "speaker_id": "1",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100037646.1.7",
          "form": "비숑.. 너무 예쁜데..",
          "original_form": "비숑.. 너무 예쁜데..",
          "speaker_id": "2",
          "time": "20210901 16:04"
        },
        {
          "id": "MDRW2100037646.1.8",
          "form": "말티즈는 털이 길어서 묶어주기 좋았죠..ㅎㅎㅎ",
          "original_form": "말티즈는 털이 길어서 묶어주기 좋았죠..ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 16:04"
        },
        {
          "id": "MDRW2100037646.1.9",
          "form": "비숑은 뽀글뽀글",
          "original_form": "비숑은 뽀글뽀글",
          "speaker_id": "1",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100037646.1.10",
          "form": "엉켜요ㅠ",
          "original_form": "엉켜요ㅠ",
          "speaker_id": "1",
          "time": "20210901 16:11"
        },
        {
          "id": "MDRW2100037646.1.11",
          "form": "그렇구나 말티즈는 직모~ 그래서 예쁘더라구요 ㅎㅎ",
          "original_form": "그렇구나 말티즈는 직모~ 그래서 예쁘더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100037646.1.12",
          "form": "비숑은 귀엽잖아요",
          "original_form": "비숑은 귀엽잖아요",
          "speaker_id": "2",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100037646.1.13",
          "form": "귀여워서 다 용서되요ㅋ",
          "original_form": "귀여워서 다 용서되요ㅋ",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100037646.1.14",
          "form": "말똥말똥",
          "original_form": "말똥말똥",
          "speaker_id": "1",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100037646.1.15",
          "form": "ㅋㅋㅋ 귀여운게 다죠",
          "original_form": "ㅋㅋㅋ 귀여운게 다죠",
          "speaker_id": "2",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100037646.1.16",
          "form": "귀여운게 최고에요",
          "original_form": "귀여운게 최고에요",
          "speaker_id": "2",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100037646.1.17",
          "form": "1마리 키우세요?",
          "original_form": "1마리 키우세요?",
          "speaker_id": "2",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100037646.1.18",
          "form": "네 1마리여",
          "original_form": "네 1마리여",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037646.1.19",
          "form": "저도 강아지 1마리 키웠어요..",
          "original_form": "저도 강아지 1마리 키웠어요..",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100037646.1.20",
          "form": "여러마리 키우는 집들도 많길래요",
          "original_form": "여러마리 키우는 집들도 많길래요",
          "speaker_id": "2",
          "time": "20210901 19:39"
        },
        {
          "id": "MDRW2100037646.1.21",
          "form": "비숑 이름은 뭐예요?",
          "original_form": "비숑 이름은 뭐예요?",
          "speaker_id": "2",
          "time": "20210901 19:40"
        },
        {
          "id": "MDRW2100037646.1.22",
          "form": "name3이여",
          "original_form": "&name3&이여",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100037646.1.23",
          "form": "아들이 지었어요ㅎ",
          "original_form": "아들이 지었어요ㅎ",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100037646.1.24",
          "form": "네 우리 말티즈는 하니였어요 ㅎㅎ",
