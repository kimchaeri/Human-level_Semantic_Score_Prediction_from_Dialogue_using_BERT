      "utterance": [
        {
          "id": "MDRW2100021662.1.1",
          "form": "안녕하세용 파운데이션 뭐 쓰세요?",
          "original_form": "안녕하세용 파운데이션 뭐 쓰세요?",
          "speaker_id": "1",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100021662.1.2",
          "form": "저는 원래 파데 안써요 안그래도 입문해볼까하다가 코로나때문에 화장 잘 안할거 같더라구요",
          "original_form": "저는 원래 파데 안써요 안그래도 입문해볼까하다가 코로나때문에 화장 잘 안할거 같더라구요",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021662.1.3",
          "form": "그래도 언젠가 살건데",
          "original_form": "그래도 언젠가 살건데",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021662.1.4",
          "form": "추천해주실수있나용",
          "original_form": "추천해주실수있나용",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021662.1.5",
          "form": "헉 파운데이션 안쓰신다니 피부가 좋으신가봐용",
          "original_form": "헉 파운데이션 안쓰신다니 피부가 좋으신가봐용",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021662.1.6",
          "form": "피부타입이 어떻게 되세용?",
          "original_form": "피부타입이 어떻게 되세용?",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021662.1.7",
          "form": "건성이요 조금만 촉촉한거 발라도 번들번들...ㅠ",
          "original_form": "건성이요 조금만 촉촉한거 발라도 번들번들...ㅠ",
          "speaker_id": "2",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021662.1.8",
          "form": "엥 지성이요",
          "original_form": "엥 지성이요",
          "speaker_id": "2",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021662.1.9",
          "form": "아ㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021662.1.10",
          "form": "지성 파운데이션 헤라 블랙쿠션이 유명한것같아용",
          "original_form": "지성 파운데이션 헤라 블랙쿠션이 유명한것같아용",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021662.1.11",
          "form": "쿠션말고 리퀴드류는",
          "original_form": "쿠션말고 리퀴드류는",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021662.1.12",
          "form": "랑콤 뗑이돌 파운데이션이용!",
          "original_form": "랑콤 뗑이돌 파운데이션이용!",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021662.1.13",
          "form": "둘다 보송보송해요",
          "original_form": "둘다 보송보송해요",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100021662.1.14",
          "form": "오옹옹ㅇ",
          "original_form": "오옹옹ㅇ",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021662.1.15",
          "form": "제가 피부가 어두우뉴ㅕㄴ인데",
          "original_form": "제가 피부가 어두우뉴ㅕㄴ인데",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021662.1.16",
          "form": "괜찮나요??",
          "original_form": "괜찮나요??",
          "speaker_id": "2",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100021662.1.17",
          "form": "네네 둘다",
          "original_form": "네네 둘다",
          "speaker_id": "1",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100021662.1.18",
          "form": "호수가 여러가지로",
          "original_form": "호수가 여러가지로",
          "speaker_id": "1",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100021662.1.19",
          "form": "다양하게 나와용",
          "original_form": "다양하게 나와용",
          "speaker_id": "1",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100021662.1.20",
          "form": "화장품 비교어플 사이트에서도 이거 두개는 평이 좋더라구용",
          "original_form": "화장품 비교어플 사이트에서도 이거 두개는 평이 좋더라구용",
          "speaker_id": "1",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100021662.1.21",
          "form": "아하ㅏ 꼭 사볼게요",
          "original_form": "아하ㅏ 꼭 사볼게요",
          "speaker_id": "2",
          "time": "20210828 21:32"
        },
        {
          "id": "MDRW2100021662.1.22",
          "form": "name1님은 어떤 파데쓰세요?",
          "original_form": "&name1&님은 어떤 파데쓰세요?",
          "speaker_id": "2",
          "time": "20210828 21:33"
        },
        {
          "id": "MDRW2100021662.1.23",
          "form": "저는 복합성인데 피부가 어떤 부분은 지성이고 어떤부분은 건조해서",
          "original_form": "저는 복합성인데 피부가 어떤 부분은 지성이고 어떤부분은 건조해서",
          "speaker_id": "1",
          "time": "20210828 21:33"
        },
        {
          "id": "MDRW2100021662.1.24",
          "form": "적당히 보송한거 써요",
          "original_form": "적당히 보송한거 써요",
          "speaker_id": "1",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100021662.1.25",
          "form": "정샘물 스킨누더 파운데이션이라고 있는데",
          "original_form": "정샘물 스킨누더 파운데이션이라고 있는데",
          "speaker_id": "1",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100021662.1.26",
          "form": "이거써용!",
          "original_form": "이거써용!",
          "speaker_id": "1",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100021662.1.27",
          "form": "복합성이면 관리하기 힘들겠어요ㅜ",
          "original_form": "복합성이면 관리하기 힘들겠어요ㅜ",
          "speaker_id": "2",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100021662.1.28",
          "form": "좀 까다로워요ㅜㅜ",
          "original_form": "좀 까다로워요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100021662.1.29",
          "form": "피부 부위별로 기초 케어를 다르게해줘야해서ㅠㅠ",
          "original_form": "피부 부위별로 기초 케어를 다르게해줘야해서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100021662.1.30",
          "form": "아아ㅏㅏㅠㅜ",
          "original_form": "아아ㅏㅏㅠㅜ",
          "speaker_id": "2",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100021662.1.31",
          "form": "그럼 립은 뭐 바르세요?",
          "original_form": "그럼 립은 뭐 바르세요?",
          "speaker_id": "2",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100021662.1.32",
          "form": "저 쓰던 립이 떨어져서",
          "original_form": "저 쓰던 립이 떨어져서",
          "speaker_id": "2",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100021662.1.33",
          "form": "mlbb종류로 찾고있는데",
          "original_form": "mlbb종류로 찾고있는데",
          "speaker_id": "2",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100021662.1.34",
          "form": "립은 브랜드 안가리고 여러가지써용ㅎㅎ",
          "original_form": "립은 브랜드 안가리고 여러가지써용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100021662.1.35",
          "form": "휘뚜루마뚜루 롬앤 틴트도 자주 쓰고",
          "original_form": "휘뚜루마뚜루 롬앤 틴트도 자주 쓰고",
          "speaker_id": "1",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100021662.1.36",
          "form": "입생로랑 따뚜아쥬 16호도 예뻐요",
          "original_form": "입생로랑 따뚜아쥬 16호도 예뻐요",
          "speaker_id": "1",
          "time": "20210828 21:38"
        },
        {
          "id": "MDRW2100021662.1.37",
          "form": "방금 네이버에 찾아보고왔는데",
          "original_form": "방금 네이버에 찾아보고왔는데",
          "speaker_id": "2",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021662.1.38",
          "form": "입생로랑꺼 세상 너무 예뻐요ㅜㅜ",
          "original_form": "입생로랑꺼 세상 너무 예뻐요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 21:40"
        }
      ]
