      "utterance": [
        {
          "id": "MDRW2100026521.1.1",
          "form": "좋아하는 음료 있어요?",
          "original_form": "좋아하는 음료 있어요?",
          "speaker_id": "1",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100026521.1.2",
          "form": "맥주요!ㅋㅋㅋ",
          "original_form": "맥주요!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026521.1.3",
          "form": "오 무슨맥주 좋아하세요?",
          "original_form": "오 무슨맥주 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026521.1.4",
          "form": "스텔라요~",
          "original_form": "스텔라요~",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026521.1.5",
          "form": "맥주는 대부분 좋아해요 IPA 빼고...",
          "original_form": "맥주는 대부분 좋아해요 IPA 빼고...",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026521.1.6",
          "form": "오 저는 칭따오 좋아해요",
          "original_form": "오 저는 칭따오 좋아해요",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026521.1.7",
          "form": "오 칭따오 무난하죠",
          "original_form": "오 칭따오 무난하죠",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026521.1.8",
          "form": "맥주에 관심이 많아서 다양하게 마셔봤는ㄷ",
          "original_form": "맥주에 관심이 많아서 다양하게 마셔봤는ㄷ",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026521.1.9",
          "form": "칭따오는... 딱 한국인들이 좋아할 만큼 무난하고 시원한 것 같아요",
          "original_form": "칭따오는... 딱 한국인들이 좋아할 만큼 무난하고 시원한 것 같아요",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026521.1.10",
          "form": "그쵸ㅋㅋㅋ 호불호 없을 맛!",
          "original_form": "그쵸ㅋㅋㅋ 호불호 없을 맛!",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026521.1.11",
          "form": "저는 병에든 칭따오가 그렇게 맛있더라구요",
          "original_form": "저는 병에든 칭따오가 그렇게 맛있더라구요",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026521.1.12",
          "form": "맞아요 칭따오는 병이 최고지 캔보단 병이구 생맥은 한국에서 안 먹어봤네용",
          "original_form": "맞아요 칭따오는 병이 최고지 캔보단 병이구 생맥은 한국에서 안 먹어봤네용",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026521.1.13",
          "form": "맥주 외엔 하이볼도 좋아해요",
          "original_form": "맥주 외엔 하이볼도 좋아해요",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026521.1.14",
          "form": "오 하이볼 어때요",
          "original_form": "오 하이볼 어때요",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026521.1.15",
          "form": "저는 잘 못먹어봤어요",
          "original_form": "저는 잘 못먹어봤어요",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026521.1.16",
          "form": "양주에 레몬 맛 좋아하시면 추천이에요!",
          "original_form": "양주에 레몬 맛 좋아하시면 추천이에요!",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026521.1.17",
          "form": "사케 잘 드시는 분들도 꽤 좋아했던 듯?",
          "original_form": "사케 잘 드시는 분들도 꽤 좋아했던 듯?",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026521.1.18",
          "form": "오 맛있을거 같은데?? 다음에 한번 도전해 볼게요",
          "original_form": "오 맛있을거 같은데?? 다음에 한번 도전해 볼게요",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026521.1.19",
          "form": "혹시 추천하시는 브랜드 있나요ㅎㅎ",
          "original_form": "혹시 추천하시는 브랜드 있나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026521.1.20",
          "form": "ㅎㅎ요새는 대부분 다 팔더라구요 하이볼은 브랜드라고 하기 어렵구",
          "original_form": "ㅎㅎ요새는 대부분 다 팔더라구요 하이볼은 브랜드라고 하기 어렵구",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026521.1.21",
          "form": "베이스가 뭔지 보셔야 해요",
          "original_form": "베이스가 뭔지 보셔야 해요",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026521.1.22",
          "form": "저도 하이볼은 잘 모르지만... 그냥 기본 하이볼 마시면 무난하실 듯해요",
          "original_form": "저도 하이볼은 잘 모르지만... 그냥 기본 하이볼 마시면 무난하실 듯해요",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026521.1.23",
          "form": "오 베이스가 뭐에따라 다른거군요",
          "original_form": "오 베이스가 뭐에따라 다른거군요",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026521.1.24",
          "form": "이따 인터넷에 제대로 찾아볼게요",
          "original_form": "이따 인터넷에 제대로 찾아볼게요",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100026521.1.25",
          "form": "하이볼 꼭 도전해봐야겠어요",
          "original_form": "하이볼 꼭 도전해봐야겠어요",
          "speaker_id": "1",
          "time": "20210901 23:47"
        }
      ]
    }
  ]
