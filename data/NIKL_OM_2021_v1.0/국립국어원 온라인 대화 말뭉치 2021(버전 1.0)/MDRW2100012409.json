      "utterance": [
        {
          "id": "MDRW2100012409.1.1",
          "form": "여행가고 싶지 않나요?",
          "original_form": "여행가고 싶지 않나요?",
          "speaker_id": "1",
          "time": "20210824 21:50"
        },
        {
          "id": "MDRW2100012409.1.2",
          "form": "여행 흠흠",
          "original_form": "여행 흠흠",
          "speaker_id": "2",
          "time": "20210824 21:53"
        },
        {
          "id": "MDRW2100012409.1.3",
          "form": "제주도 가고싶어요",
          "original_form": "제주도 가고싶어요",
          "speaker_id": "2",
          "time": "20210824 21:53"
        },
        {
          "id": "MDRW2100012409.1.4",
          "form": "제주도는 수학여행 때 가보고 안가봐서 아쉬움이 많아요",
          "original_form": "제주도는 수학여행 때 가보고 안가봐서 아쉬움이 많아요",
          "speaker_id": "2",
          "time": "20210824 21:53"
        },
        {
          "id": "MDRW2100012409.1.5",
          "form": "제주도 정말 드라이브만해도 힐링되는 곳 같아요",
          "original_form": "제주도 정말 드라이브만해도 힐링되는 곳 같아요",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012409.1.6",
          "form": "제주도 가봤어여?",
          "original_form": "제주도 가봤어여?",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012409.1.7",
          "form": "네 가봤죠 딱 한번이었지만요",
          "original_form": "네 가봤죠 딱 한번이었지만요",
          "speaker_id": "1",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100012409.1.8",
          "form": "오 친구들이랑?",
          "original_form": "오 친구들이랑?",
          "speaker_id": "2",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100012409.1.9",
          "form": "수학여행이요",
          "original_form": "수학여행이요",
          "speaker_id": "1",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100012409.1.10",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210824 21:59"
        },
        {
          "id": "MDRW2100012409.1.11",
          "form": "제주도 가면 뭐 하고싶어요?",
          "original_form": "제주도 가면 뭐 하고싶어요?",
          "speaker_id": "2",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100012409.1.12",
          "form": "저는 올레길 걸어보고 싶어요",
          "original_form": "저는 올레길 걸어보고 싶어요",
          "speaker_id": "1",
          "time": "20210824 22:00"
        },
        {
          "id": "MDRW2100012409.1.13",
          "form": "우와 좋다아",
          "original_form": "우와 좋다아",
          "speaker_id": "2",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012409.1.14",
          "form": "나는 스킨스쿠버",
          "original_form": "나는 스킨스쿠버",
          "speaker_id": "2",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012409.1.15",
          "form": "넘넘 재밌을 것 같아요",
          "original_form": "넘넘 재밌을 것 같아요",
          "speaker_id": "2",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100012409.1.16",
          "form": "오...  스킨스쿠버하는 너를 구경할기ㅣ",
          "original_form": "오...  스킨스쿠버하는 너를 구경할기ㅣ",
          "speaker_id": "1",
          "time": "20210824 22:05"
        },
        {
          "id": "MDRW2100012409.1.17",
          "form": "왜 같이 뛰어들자",
          "original_form": "왜 같이 뛰어들자",
          "speaker_id": "2",
          "time": "20210824 22:05"
        },
        {
          "id": "MDRW2100012409.1.18",
          "form": "문어 잡자 어때",
          "original_form": "문어 잡자 어때",
          "speaker_id": "2",
          "time": "20210824 22:05"
        },
        {
          "id": "MDRW2100012409.1.19",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100012409.1.20",
          "form": "나는 응원할게",
          "original_form": "나는 응원할게",
          "speaker_id": "1",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100012409.1.21",
          "form": "왜ㅡㅡ",
          "original_form": "왜ㅡㅡ",
          "speaker_id": "2",
          "time": "20210824 22:06"
        },
        {
          "id": "MDRW2100012409.1.22",
          "form": "그러면 요트 타면서 낚시하자",
          "original_form": "그러면 요트 타면서 낚시하자",
          "speaker_id": "2",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012409.1.23",
          "form": "회 뜨자",
          "original_form": "회 뜨자",
          "speaker_id": "2",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012409.1.24",
          "form": "맛있겠지",
          "original_form": "맛있겠지",
          "speaker_id": "2",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012409.1.25",
          "form": "보글보글 매운탕",
          "original_form": "보글보글 매운탕",
          "speaker_id": "2",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012409.1.26",
          "form": "와 군침돈다",
          "original_form": "와 군침돈다",
          "speaker_id": "2",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012409.1.27",
          "form": "옛날에 수학여행 가면 감귤초콜릿 사갖고 갔는데 ㅋㅋㅋㅋㅋ",
          "original_form": "옛날에 수학여행 가면 감귤초콜릿 사갖고 갔는데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:07"
        },
        {
          "id": "MDRW2100012409.1.28",
          "form": "회는 누가 뜰건데?! 너가?",
          "original_form": "회는 누가 뜰건데?! 너가?",
          "speaker_id": "1",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012409.1.29",
          "form": "감귤 초콜릿 다 사가지 않았어? 나도 사감ㅎㅎ",
          "original_form": "감귤 초콜릿 다 사가지 않았어? 나도 사감ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 22:11"
        },
        {
          "id": "MDRW2100012409.1.30",
          "form": "아니 너가!",
          "original_form": "아니 너가!",
          "speaker_id": "2",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012409.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ역시 똑같구만",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ역시 똑같구만",
          "speaker_id": "2",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012409.1.32",
          "form": "제주도 가면 얼만큼 있고싶어?",
          "original_form": "제주도 가면 얼만큼 있고싶어?",
          "speaker_id": "2",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012409.1.33",
          "form": "진짜 마음같아서는 한달!",
          "original_form": "진짜 마음같아서는 한달!",
          "speaker_id": "1",
          "time": "20210824 22:12"
        },
        {
          "id": "MDRW2100012409.1.34",
          "form": "맞아....와 너뮤 좋겠다",
          "original_form": "맞아....와 너뮤 좋겠다",
          "speaker_id": "2",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012409.1.35",
          "form": "바닷바람도 느끼고싶어",
          "original_form": "바닷바람도 느끼고싶어",
          "speaker_id": "2",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012409.1.36",
          "form": "한달살이~ 차 렌트해서...",
          "original_form": "한달살이~ 차 렌트해서...",
          "speaker_id": "1",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012409.1.37",
          "form": "요즘 진짜 제주도 넘 가고싶다",
          "original_form": "요즘 진짜 제주도 넘 가고싶다",
          "speaker_id": "2",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012409.1.38",
          "form": "키야",
          "original_form": "키야",
          "speaker_id": "2",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012409.1.39",
          "form": "행-복",
          "original_form": "행-복",
          "speaker_id": "2",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012409.1.40",
          "form": "캠핑카 렌트해서 한달사는거지",
          "original_form": "캠핑카 렌트해서 한달사는거지",
          "speaker_id": "1",
          "time": "20210824 22:13"
        },
        {
          "id": "MDRW2100012409.1.41",
          "form": "힐링",
          "original_form": "힐링",
          "speaker_id": "1",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012409.1.42",
          "form": "좋네....나랑 가자구",
          "original_form": "좋네....나랑 가자구",
          "speaker_id": "2",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012409.1.43",
          "form": "해외는 어디가고싶어?",
          "original_form": "해외는 어디가고싶어?",
          "speaker_id": "2",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012409.1.44",
          "form": "그래 꼭 같이 제주도가자",
          "original_form": "그래 꼭 같이 제주도가자",
          "speaker_id": "1",
          "time": "20210824 22:14"
        },
        {
          "id": "MDRW2100012409.1.45",
          "form": "해외는... 터키!",
          "original_form": "해외는... 터키!",
          "speaker_id": "1",
          "time": "20210824 22:15"
        },
        {
          "id": "MDRW2100012409.1.46",
          "form": "오 터키 왜?",
          "original_form": "오 터키 왜?",
          "speaker_id": "2",
          "time": "20210824 22:16"
        },
        {
          "id": "MDRW2100012409.1.47",
          "form": "터키젤리도 먹고 싶고... 건축물들이 넘 견고하고 멋있더라",
          "original_form": "터키젤리도 먹고 싶고... 건축물들이 넘 견고하고 멋있더라",
          "speaker_id": "1",
          "time": "20210824 22:16"
        },
        {
          "id": "MDRW2100012409.1.48",
          "form": "오호 터키젤리? 나도 궁금하다 먹보대장 혜수 넘나궁금",
          "original_form": "오호 터키젤리? 나도 궁금하다 먹보대장 혜수 넘나궁금",
          "speaker_id": "2",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012409.1.49",
          "form": "난 해외는 잘모르겠다",
          "original_form": "난 해외는 잘모르겠다",
          "speaker_id": "2",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012409.1.50",
          "form": "아직 한국 여행도 못가봐서",
          "original_form": "아직 한국 여행도 못가봐서",
          "speaker_id": "2",
          "time": "20210824 22:19"
        },
        {
          "id": "MDRW2100012409.1.51",
          "form": "제주도 말고 또 어디가고싶어?",
          "original_form": "제주도 말고 또 어디가고싶어?",
          "speaker_id": "1",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012409.1.52",
          "form": "강릉!",
          "original_form": "강릉!",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012409.1.53",
          "form": "부산!",
          "original_form": "부산!",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012409.1.54",
          "form": "부산 가고싶어",
          "original_form": "부산 가고싶어",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012409.1.55",
          "form": "한번도 안가봄 ㅠㅠ",
          "original_form": "한번도 안가봄 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012409.1.56",
          "form": "나도 부산은 안가봤네",
          "original_form": "나도 부산은 안가봤네",
          "speaker_id": "1",
          "time": "20210824 22:32"
        },
        {
          "id": "MDRW2100012409.1.57",
          "form": "넌 어디어디 여행가봤냐",
          "original_form": "넌 어디어디 여행가봤냐",
          "speaker_id": "2",
          "time": "20210824 22:34"
        },
        {
          "id": "MDRW2100012409.1.58",
          "form": "여수, 강릉, 속초..!",
          "original_form": "여수, 강릉, 속초..!",
          "speaker_id": "1",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100012409.1.59",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100012409.1.60",
          "form": "야 나도",
          "original_form": "야 나도",
          "speaker_id": "2",
          "time": "20210825 17:50"
        },
        {
          "id": "MDRW2100012409.1.61",
          "form": "여수 좋았어",
          "original_form": "여수 좋았어",
          "speaker_id": "2",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100012409.1.62",
          "form": "여수 케이블카 좋았다~!",
          "original_form": "여수 케이블카 좋았다~!",
          "speaker_id": "1",
          "time": "20210825 17:51"
        },
        {
          "id": "MDRW2100012409.1.63",
          "form": "rgrg 진짜 또 가고싶다",
          "original_form": "rgrg 진짜 또 가고싶다",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100012409.1.64",
          "form": "난 패키지여행으로 갔거든",
          "original_form": "난 패키지여행으로 갔거든",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100012409.1.65",
