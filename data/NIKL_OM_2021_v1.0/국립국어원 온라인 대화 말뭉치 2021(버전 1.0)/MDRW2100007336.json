      "utterance": [
        {
          "id": "MDRW2100007336.1.1",
          "form": "싸이 부활ㅋㅋㅋㅋ",
          "original_form": "싸이 부활ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007336.1.2",
          "form": "싸이해보셨나요..",
          "original_form": "싸이해보셨나요..",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007336.1.3",
          "form": "저 싸이 초등학생때 해봤어요!",
          "original_form": "저 싸이 초등학생때 해봤어요!",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007336.1.4",
          "form": "저도 해봤어요!",
          "original_form": "저도 해봤어요!",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007336.1.5",
          "form": "다 흑역사지만...",
          "original_form": "다 흑역사지만...",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007336.1.6",
          "form": "그러고 없어졌어서 다행히 큰 흑역사는 없는..",
          "original_form": "그러고 없어졌어서 다행히 큰 흑역사는 없는..",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007336.1.7",
          "form": "엄청 유행이었죠 한때",
          "original_form": "엄청 유행이었죠 한때",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007336.1.8",
          "form": "나는 사진 올릴 줄 몰랐어서 다행이다",
          "original_form": "나는 사진 올릴 줄 몰랐어서 다행이다",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007336.1.9",
          "form": "저도 사라진다고했을때 다 삭제했어서 남아있진 않아요",
          "original_form": "저도 사라진다고했을때 다 삭제했어서 남아있진 않아요",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007336.1.10",
          "form": "맨날 퍼가요~만 해서 다행이다ㅠ",
          "original_form": "맨날 퍼가요~{emoji:♡}만 해서 다행이다ㅠ",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007336.1.11",
          "form": "어우 흑역사는",
          "original_form": "어우 흑역사는",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007336.1.12",
          "form": "전 일기도 썼었는데",
          "original_form": "전 일기도 썼었는데",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007336.1.13",
          "form": "없애셨네욬ㅋㅋㅋㅋㅋ",
          "original_form": "없애셨네욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007336.1.14",
          "form": "절대 에센에스에서 해선 안될 짓",
          "original_form": "절대 에센에스에서 해선 안될 짓",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007336.1.15",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007336.1.16",
          "form": "싸이월드 연예인들 거 보는 것도 재밌었는데",
          "original_form": "싸이월드 연예인들 거 보는 것도 재밌었는데",
          "speaker_id": "1",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007336.1.17",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007336.1.18",
          "form": "그리고 싸이잘꾸미는 친구",
          "original_form": "그리고 싸이잘꾸미는 친구",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007336.1.19",
          "form": "진짜 신기했어요",
          "original_form": "진짜 신기했어요",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007336.1.20",
          "form": "맞아요 미니미 꾸미고 그랬었는데",
          "original_form": "맞아요 미니미 꾸미고 그랬었는데",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007336.1.21",
          "form": "싸이 부활한다는데 다시 할 맘 있으신가요?",
          "original_form": "싸이 부활한다는데 다시 할 맘 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007336.1.22",
          "form": "어우.....",
          "original_form": "어우.....",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007336.1.23",
          "form": "저는 안될거 같아요ㅠㅋㅋㅋㅋㅋ",
          "original_form": "저는 안될거 같아요ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007336.1.24",
          "form": "혹시 하실 생각 있으신가요?",
          "original_form": "혹시 하실 생각 있으신가요?",
          "speaker_id": "2",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007336.1.25",
          "form": "저는 지금 있는 인스타 같은 것도 잘 안해서 안할 거 같아요",
          "original_form": "저는 지금 있는 인스타 같은 것도 잘 안해서 안할 거 같아요",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007336.1.26",
          "form": "근데 주변 사람들이 하면 계정은 만들 것 같아요",
          "original_form": "근데 주변 사람들이 하면 계정은 만들 것 같아요",
          "speaker_id": "1",
          "time": "20210822 01:32"
        }
      ]
    }
