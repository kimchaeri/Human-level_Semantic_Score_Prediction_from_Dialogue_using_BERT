      "utterance": [
        {
          "id": "MDRW2100029744.1.1",
          "form": "설거지중이요~",
          "original_form": "설거지중이요~",
          "speaker_id": "2",
          "time": "20210903 13:30"
        },
        {
          "id": "MDRW2100029744.1.2",
          "form": "맛점 하셨쎄요?^^",
          "original_form": "맛점 하셨쎄요?^^",
          "speaker_id": "2",
          "time": "20210903 13:30"
        },
        {
          "id": "MDRW2100029744.1.3",
          "form": "네.. 돼지고기에 밥 먹었어요",
          "original_form": "네.. 돼지고기에 밥 먹었어요",
          "speaker_id": "1",
          "time": "20210903 13:31"
        },
        {
          "id": "MDRW2100029744.1.4",
          "form": "저는 소꼬기에  밥 먹었어요",
          "original_form": "저는 소꼬기에  밥 먹었어요",
          "speaker_id": "2",
          "time": "20210903 13:43"
        },
        {
          "id": "MDRW2100029744.1.5",
          "form": "소고기 드셨군요",
          "original_form": "소고기 드셨군요",
          "speaker_id": "1",
          "time": "20210903 13:44"
        },
        {
          "id": "MDRW2100029744.1.6",
          "form": "고기를 많이 좋아하진 않지만 역시 단백질은 한번씩 먹어줘야쥬~",
          "original_form": "고기를 많이 좋아하진 않지만 역시 단백질은 한번씩 먹어줘야쥬~",
          "speaker_id": "2",
          "time": "20210903 13:44"
        },
        {
          "id": "MDRW2100029744.1.7",
          "form": "사드신 건가요? 집에서 요리?",
          "original_form": "사드신 건가요? 집에서 요리?",
          "speaker_id": "1",
          "time": "20210903 13:44"
        },
        {
          "id": "MDRW2100029744.1.8",
          "form": "식당가면 돈 더 들잖아요",
          "original_form": "식당가면 돈 더 들잖아요",
          "speaker_id": "2",
          "time": "20210903 13:44"
        },
        {
          "id": "MDRW2100029744.1.9",
          "form": "그 돈으로 한팩 더~^^",
          "original_form": "그 돈으로 한팩 더~^^",
          "speaker_id": "2",
          "time": "20210903 13:45"
        },
        {
          "id": "MDRW2100029744.1.10",
          "form": "소고기 사와서 집에서 해드셨군요..",
          "original_form": "소고기 사와서 집에서 해드셨군요..",
          "speaker_id": "1",
          "time": "20210903 13:45"
        },
        {
          "id": "MDRW2100029744.1.11",
          "form": "저랑 비슷하네요~ 저도 외식은 그닥이라서요..ㅎㅎ",
          "original_form": "저랑 비슷하네요~ 저도 외식은 그닥이라서요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 13:45"
        },
        {
          "id": "MDRW2100029744.1.12",
          "form": "집밥이 맘 편해요.",
          "original_form": "집밥이 맘 편해요.",
          "speaker_id": "2",
          "time": "20210903 13:45"
        },
        {
          "id": "MDRW2100029744.1.13",
          "form": "진짜 남이 해주는 밥 먹고싶을때 빼고는요.ㅎ ㅎ",
          "original_form": "진짜 남이 해주는 밥 먹고싶을때 빼고는요.ㅎ ㅎ",
          "speaker_id": "2",
          "time": "20210903 13:46"
        },
        {
          "id": "MDRW2100029744.1.14",
          "form": "사실,외식메뉴 정하기도 쉽진 않더라구요",
          "original_form": "사실,외식메뉴 정하기도 쉽진 않더라구요",
          "speaker_id": "2",
          "time": "20210903 13:46"
        },
        {
          "id": "MDRW2100029744.1.15",
          "form": "맞아요.. 먹을건 천지인데 그다지 끌리지 않아요",
          "original_form": "맞아요.. 먹을건 천지인데 그다지 끌리지 않아요",
          "speaker_id": "1",
          "time": "20210903 19:20"
        },
        {
          "id": "MDRW2100029744.1.16",
          "form": "저녁은 드셨나요?",
          "original_form": "저녁은 드셨나요?",
          "speaker_id": "2",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100029744.1.17",
          "form": "저녁은 딸이 치킨은 사와서 먹었어요",
          "original_form": "저녁은 딸이 치킨은 사와서 먹었어요",
          "speaker_id": "1",
          "time": "20210903 19:40"
        },
        {
          "id": "MDRW2100029744.1.18",
          "form": "특별식이네요~^^",
          "original_form": "특별식이네요~^^",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100029744.1.19",
          "form": "네.. 간만에 먹었네요",
          "original_form": "네.. 간만에 먹었네요",
          "speaker_id": "1",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100029744.1.20",
          "form": "치킨 자주 드세요?",
          "original_form": "치킨 자주 드세요?",
          "speaker_id": "1",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100029744.1.21",
          "form": "아니요~애들 먹을때 어쩌다 한두조각 정도 맛만 봐요",
          "original_form": "아니요~애들 먹을때 어쩌다 한두조각 정도 맛만 봐요",
          "speaker_id": "2",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100029744.1.22",
          "form": "그렇군요.. 저도 맛은 있는데 애들 마니 먹으라고 조금 먹어요",
          "original_form": "그렇군요.. 저도 맛은 있는데 애들 마니 먹으라고 조금 먹어요",
