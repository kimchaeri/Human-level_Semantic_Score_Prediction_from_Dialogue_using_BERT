        {
          "id": "MDRW2100047231.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 00:55"
        },
        {
          "id": "MDRW2100047231.1.2",
          "form": "안녕하세요! 중고거래 많이 하세요?",
          "original_form": "안녕하세요! 중고거래 많이 하세요?",
          "speaker_id": "2",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100047231.1.3",
          "form": "네 종종해요!",
          "original_form": "네 종종해요!",
          "speaker_id": "1",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100047231.1.4",
          "form": "저두요! 저는 옷 많이 팔아요ㅋㅋㅋ",
          "original_form": "저두요! 저는 옷 많이 팔아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100047231.1.5",
          "form": "저는 최근에 버즈 팔았어요",
          "original_form": "저는 최근에 버즈 팔았어요",
          "speaker_id": "1",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100047231.1.6",
          "form": "저는 그 정도 가격까지는 안 팔아봤는데",
          "original_form": "저는 그 정도 가격까지는 안 팔아봤는데",
          "speaker_id": "2",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100047231.1.7",
          "form": "거래 괜찮았나요?",
          "original_form": "거래 괜찮았나요?",
          "speaker_id": "2",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100047231.1.8",
          "form": "네 잘팔렸어요ㅋㅋ",
          "original_form": "네 잘팔렸어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100047231.1.9",
          "form": "ㅋㅋㅋㅋ다행이네요",
          "original_form": "ㅋㅋㅋㅋ다행이네요",
          "speaker_id": "2",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100047231.1.10",
          "form": "이상한 사람 만난 적 있으세요?",
          "original_form": "이상한 사람 만난 적 있으세요?",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100047231.1.11",
          "form": "많이 만났죠ㅠㅠ",
          "original_form": "많이 만났죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100047231.1.12",
          "form": "저는 진짜 진상은 아직 못 봤는데",
          "original_form": "저는 진짜 진상은 아직 못 봤는데",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100047231.1.13",
          "form": "어떤 사람 있었어요?ㅠㅠ",
          "original_form": "어떤 사람 있었어요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100047231.1.14",
          "form": "깎아달라는 사람이요",
          "original_form": "깎아달라는 사람이요",
          "speaker_id": "1",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100047231.1.15",
          "form": "아 역시.. 얼마나 깎아달랬어요?",
          "original_form": "아 역시.. 얼마나 깎아달랬어요?",
          "speaker_id": "2",
          "time": "20210826 12:47"
        },
        {
          "id": "MDRW2100047231.1.16",
          "form": "반이요ㅋㅋㅋ",
          "original_form": "반이요ㅋㅋㅋ",
