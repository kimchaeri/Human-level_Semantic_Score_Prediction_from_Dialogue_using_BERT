      "utterance": [
        {
          "id": "MDRW2100006376.1.1",
          "form": "저는 점심 아님 저녁에 시킬텐뎅",
          "original_form": "저는 점심 아님 저녁에 시킬텐뎅",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100006376.1.2",
          "form": "곱도리를 배달해주다니",
          "original_form": "곱도리를 배달해주다니",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100006376.1.3",
          "form": "ㅋㅋname1님댁도 있지 않을까요?",
          "original_form": "ㅋㅋ&name1&님댁도 있지 않을까요?",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100006376.1.4",
          "form": "근데 전 저녁에 시켜야해서",
          "original_form": "근데 전 저녁에 시켜야해서",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100006376.1.5",
          "form": "혼자 먹어야할듯",
          "original_form": "혼자 먹어야할듯",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100006376.1.6",
          "form": "오잉? 왜 혼자 먹어요?",
          "original_form": "오잉? 왜 혼자 먹어요?",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100006376.1.7",
          "form": "저녁에 시킴\r\n가족들 다같이 먹을수 있잖아용",
          "original_form": "저녁에 시킴\r\n가족들 다같이 먹을수 있잖아용",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100006376.1.8",
          "form": "엄마가 곱창 안드세요",
          "original_form": "엄마가 곱창 안드세요",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100006376.1.9",
          "form": "동생도",
          "original_form": "동생도",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100006376.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100006376.1.11",
          "form": "아ㅠㅠ 그 맛있는걸 ㅜㅜ",
          "original_form": "아ㅠㅠ 그 맛있는걸 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100006376.1.12",
          "form": "저도 안먹다가",
          "original_form": "저도 안먹다가",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100006376.1.13",
          "form": "여친이 먹게 해줬어요ㅋㅋㅋ",
          "original_form": "여친이 먹게 해줬어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100006376.1.14",
          "form": "맛난곳 데려가서",
          "original_form": "맛난곳 데려가서",
          "speaker_id": "1",
          "time": "20210907 23:29"
        },
        {
          "id": "MDRW2100006376.1.15",
          "form": "먹고",
          "original_form": "먹고",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100006376.1.16",
          "form": "그때부터 먹기시작",
          "original_form": "그때부터 먹기시작",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100006376.1.17",
          "form": "오ㅋㅋ여친한테 잘하세여ㅋㅋㅋ",
          "original_form": "오ㅋㅋ여친한테 잘하세여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100006376.1.18",
          "form": "저는 순대국밥도 좋아하는데",
          "original_form": "저는 순대국밥도 좋아하는데",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100006376.1.19",
          "form": "여친도요",
          "original_form": "여친도요",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100006376.1.20",
          "form": "남자들은 다 좋아하는줄 알았더니",
          "original_form": "남자들은 다 좋아하는줄 알았더니",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100006376.1.21",
          "form": "순대국밥 좋아함",
          "original_form": "순대국밥 좋아함",
          "speaker_id": "1",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100006376.1.22",
          "form": "전 남친중에 헤어질때",
          "original_form": "전 남친중에 헤어질때",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100006376.1.23",
          "form": "얘기한 사람도 있었어옄ㅋㅋㅋㅋㅋ",
          "original_form": "얘기한 사람도 있었어옄ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100006376.1.24",
          "form": "사실 자긴 순대국밥 싫어한다곸ㅋㅋ",
          "original_form": "사실 자긴 순대국밥 싫어한다곸ㅋㅋ",
