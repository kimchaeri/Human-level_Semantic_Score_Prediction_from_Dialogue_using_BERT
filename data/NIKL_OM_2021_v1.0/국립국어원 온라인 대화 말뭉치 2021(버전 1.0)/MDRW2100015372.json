      "utterance": [
        {
          "id": "MDRW2100015372.1.1",
          "form": "스포츠 좋아하시는거 있으세요  ㅎㅎ ?",
          "original_form": "스포츠 좋아하시는거 있으세요  ㅎㅎ ?",
          "speaker_id": "1",
          "time": "20210825 22:52"
        },
        {
          "id": "MDRW2100015372.1.2",
          "form": "아뇨ㅠㅠ이번에 올림픽만 봤어요ㅎㅎ",
          "original_form": "아뇨ㅠㅠ이번에 올림픽만 봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:53"
        },
        {
          "id": "MDRW2100015372.1.3",
          "form": "아 올림픽 엄청 재밌었어요 ㅎㅎ",
          "original_form": "아 올림픽 엄청 재밌었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015372.1.4",
          "form": "다 보셨나요?",
          "original_form": "다 보셨나요?",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015372.1.5",
          "form": "아 그 몇몇만 본거같아요ㅠㅠ",
          "original_form": "아 그 몇몇만 본거같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:55"
        },
        {
          "id": "MDRW2100015372.1.6",
          "form": "저도 양궁이랑 배구 위주로 봤어요 ㅎㅎ",
          "original_form": "저도 양궁이랑 배구 위주로 봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015372.1.7",
          "form": "오 저두요ㅋㅋㅋ재밌었어요",
          "original_form": "오 저두요ㅋㅋㅋ재밌었어요",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015372.1.8",
          "form": "양궁 진짜 ㅠㅠ 다 보셨나요?",
          "original_form": "양궁 진짜 ㅠㅠ 다 보셨나요?",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015372.1.9",
          "form": "제가 다 떨렸어요 ㅋㅋ",
          "original_form": "제가 다 떨렸어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015372.1.10",
          "form": "네 ㅎㅎ감동이었어요ㅠㅠ",
          "original_form": "네 ㅎㅎ감동이었어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015372.1.11",
          "form": "맞죠 ㅠㅠ 배구도 감동이였어요",
          "original_form": "맞죠 ㅠㅠ 배구도 감동이였어요",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015372.1.12",
          "form": "8강 이길줄 몰랐는데 !",
          "original_form": "8강 이길줄 몰랐는데 !",
          "speaker_id": "1",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015372.1.13",
          "form": "네 진짜 영화같았어요...ㅠㅠㅜㅜ",
          "original_form": "네 진짜 영화같았어요...ㅠㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015372.1.14",
          "form": "직관도 해보고 싶어요 ㅠ 직관같은것도 안해보셨나요 ?",
          "original_form": "직관도 해보고 싶어요 ㅠ 직관같은것도 안해보셨나요 ?",
          "speaker_id": "1",
          "time": "20210825 23:05"
        },
        {
          "id": "MDRW2100015372.1.15",
          "form": "야구만 예전에 한번 해봤어요ㅎㅎ",
          "original_form": "야구만 예전에 한번 해봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015372.1.16",
          "form": "오 저도 야구는 해봤어요 ㅎㅎ 어떠셨어요?",
          "original_form": "오 저도 야구는 해봤어요 ㅎㅎ 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015372.1.17",
          "form": "전 야구 좋아해서 재밌는데 또 룰 모르면 재미는 없을거같아요 ㅠ",
          "original_form": "전 야구 좋아해서 재밌는데 또 룰 모르면 재미는 없을거같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015372.1.18",
          "form": "아 맞아요ㅠㅠ",
          "original_form": "아 맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:34"
        }
