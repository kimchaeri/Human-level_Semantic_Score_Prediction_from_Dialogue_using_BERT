      "utterance": [
        {
          "id": "MDRW2100014439.1.1",
          "form": "최근에 쇼핑 하셨나용 ?",
          "original_form": "최근에 쇼핑 하셨나용 ?",
          "speaker_id": "1",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100014439.1.2",
          "form": "네ㅎㅎ 가을옷샀어요ㅎㅎ 쇼핑한거 있으세요?",
          "original_form": "네ㅎㅎ 가을옷샀어요ㅎㅎ 쇼핑한거 있으세요?",
          "speaker_id": "2",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014439.1.3",
          "form": "어 저도 니트 샀어요 ㅎㅎ !",
          "original_form": "어 저도 니트 샀어요 ㅎㅎ !",
          "speaker_id": "1",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014439.1.4",
          "form": "옷은 사도사도 없는거같아요 ㅎㅎ",
          "original_form": "옷은 사도사도 없는거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014439.1.5",
          "form": "색깔이쁜 니트보니까 또 물욕이ㅎㅎ",
          "original_form": "색깔이쁜 니트보니까 또 물욕이ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014439.1.6",
          "form": "맞아요 작년에 뭐입었는지ㅋㅋ",
          "original_form": "맞아요 작년에 뭐입었는지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014439.1.7",
          "form": "ㅋㅋㅋ 맞아요 니트도 사고싶고",
          "original_form": "ㅋㅋㅋ 맞아요 니트도 사고싶고",
          "speaker_id": "1",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014439.1.8",
          "form": "자켓도 사고싶은데 아직 맘에 드는게 없더라구요",
          "original_form": "자켓도 사고싶은데 아직 맘에 드는게 없더라구요",
          "speaker_id": "1",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014439.1.9",
          "form": "오 맞아요 자켓..저는 트렌치샀어요 ㅋㅋ 몇번이나 입을지ㅜ",
          "original_form": "오 맞아요 자켓..저는 트렌치샀어요 ㅋㅋ 몇번이나 입을지ㅜ",
          "speaker_id": "2",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014439.1.10",
          "form": "ㅋㅋㅋㅋ맞아요 가을도 엄청 짧아서 쇼핑하기 애매한거같아요",
          "original_form": "ㅋㅋㅋㅋ맞아요 가을도 엄청 짧아서 쇼핑하기 애매한거같아요",
          "speaker_id": "1",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014439.1.11",
          "form": "맞아요 그래서 역시즌 숏패딩도 보고있어요ㅋㅋ",
          "original_form": "맞아요 그래서 역시즌 숏패딩도 보고있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100014439.1.12",
          "form": "아 맞아요 패딩같은거 딱 지금 사야 싸지않나요 ??",
          "original_form": "아 맞아요 패딩같은거 딱 지금 사야 싸지않나요 ??",
          "speaker_id": "1",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100014439.1.13",
          "form": "네 그냥 패딩만 있어서 이것저것 구경중이에요ㅎㅎ",
          "original_form": "네 그냥 패딩만 있어서 이것저것 구경중이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:11"
        },
        {
          "id": "MDRW2100014439.1.14",
          "form": "저도 좀 봐야겠어요 ! ㅎㅎ",
          "original_form": "저도 좀 봐야겠어요 ! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:12"
        },
        {
          "id": "MDRW2100014439.1.15",
          "form": "숏패딩 작년에 하나 샀는데 되게 잘 입어지더라구요",
          "original_form": "숏패딩 작년에 하나 샀는데 되게 잘 입어지더라구요",
          "speaker_id": "1",
          "time": "20210825 19:13"
        },
        {
          "id": "MDRW2100014439.1.16",
          "form": "오 꼭 사야겠네요ㅋㅋㅋ",
          "original_form": "오 꼭 사야겠네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014439.1.17",
          "form": "요즘은 또 롱패딩보단 숏패딩 많이 입으니까 ㅎㅎ",
          "original_form": "요즘은 또 롱패딩보단 숏패딩 많이 입으니까 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014439.1.18",
          "form": "사두면 좋아요",
