          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044020.1.2",
          "form": "새우깡 사오면 되나용?",
          "original_form": "새우깡 사오면 되나용?",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044020.1.3",
          "form": "그래 하나만 사 줘",
          "original_form": "그래 하나만 사 줘",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044020.1.4",
          "form": "알았다 ㅋㅋ",
          "original_form": "알았다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044020.1.5",
          "form": "나도 먹을거 사와야겠다",
          "original_form": "나도 먹을거 사와야겠다",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044020.1.6",
          "form": "그래 너무 많이 사지 마라",
          "original_form": "그래 너무 많이 사지 마라",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044020.1.7",
          "form": "웅웅 돈 없다 ㅋㅋ",
          "original_form": "웅웅 돈 없다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044020.1.8",
          "form": "과자는 몸에 안 좋다",
          "original_form": "과자는 몸에 안 좋다",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044020.1.9",
          "form": "웅..어쩌다 한번씩 먹는거니까 .. 괜찮다 ㅋ",
          "original_form": "웅..어쩌다 한번씩 먹는거니까 .. 괜찮다 ㅋ",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044020.1.10",
          "form": "칼로리 낮은 과자로 사 와라",
          "original_form": "칼로리 낮은 과자로 사 와라",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100044020.1.11",
          "form": "웅웅 ㅋㅋ",
          "original_form": "웅웅 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044020.1.12",
          "form": "그런 과자가 있는가..",
          "original_form": "그런 과자가 있는가..",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044020.1.13",
          "form": "안 튀긴 거로 사면 되지",
          "original_form": "안 튀긴 거로 사면 되지",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044020.1.14",
          "form": "그렇넹",
          "original_form": "그렇넹",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044020.1.15",
          "form": "갈 거면 빨리 다녀와",
          "original_form": "갈 거면 빨리 다녀와",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044020.1.16",
          "form": "아까 치킨 시켰는데 좀 밀리나보다",
