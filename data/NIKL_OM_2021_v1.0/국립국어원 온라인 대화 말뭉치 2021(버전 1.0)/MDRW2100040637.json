      "utterance": [
        {
          "id": "MDRW2100040637.1.1",
          "form": "남들 다 본건 다 봤달까",
          "original_form": "남들 다 본건 다 봤달까",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040637.1.2",
          "form": "공전의 히트를 친 작품은 보기는 다 본것같아요",
          "original_form": "공전의 히트를 친 작품은 보기는 다 본것같아요",
          "speaker_id": "2",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040637.1.3",
          "form": "어릴때 유행한만화가뭐였어요? ㅋㅋ 전 포켓몬스터 ㅋㅋㅋ",
          "original_form": "어릴때 유행한만화가뭐였어요? ㅋㅋ 전 포켓몬스터 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:29"
        },
        {
          "id": "MDRW2100040637.1.4",
          "form": "저때도 그거 인기많았죠.",
          "original_form": "저때도 그거 인기많았죠.",
          "speaker_id": "2",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040637.1.5",
          "form": "근데 전 세일러문이 제 주기억인것 같네요",
          "original_form": "근데 전 세일러문이 제 주기억인것 같네요",
          "speaker_id": "2",
          "time": "20210904 23:30"
        },
        {
          "id": "MDRW2100040637.1.6",
          "form": "아마 세일러문 다음에 포켓몬을 본것 같아요",
          "original_form": "아마 세일러문 다음에 포켓몬을 본것 같아요",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040637.1.7",
          "form": "세일러문도인기많았어요 ㅜㅜ",
          "original_form": "세일러문도인기많았어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040637.1.8",
          "form": "저도요저도요 그때쯤 명탐정 코난도 나왔었어요 ㅋㅋㅋ",
          "original_form": "저도요저도요 그때쯤 명탐정 코난도 나왔었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040637.1.9",
          "form": "저랑 비슷한 세대신듯 ㅎㅎ",
          "original_form": "저랑 비슷한 세대신듯 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100040637.1.10",
          "form": "코난은 언제 끝날까요...",
          "original_form": "코난은 언제 끝날까요...",
          "speaker_id": "2",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100040637.1.11",
          "form": "그러게요 ㅋㅋㅋㅋ",
          "original_form": "그러게요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100040637.1.12",
          "form": "한 50권까진 읽다가 포기함...",
          "original_form": "한 50권까진 읽다가 포기함...",
          "speaker_id": "2",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100040637.1.13",
          "form": "그냥 제대로 나이를 먹었어도 벌써 성인일텐데요...",
          "original_form": "그냥 제대로 나이를 먹었어도 벌써 성인일텐데요...",
          "speaker_id": "1",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100040637.1.14",
          "form": "어른되면 알려줘.,",
          "original_form": "어른되면 알려줘.,",
          "speaker_id": "2",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100040637.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100040637.1.16",
          "form": "코난요즘에나오는거보면 막 시대의 흐름에 따라 스마트폰 쓴대요 ㅋㅋㅋㅋ",
          "original_form": "코난요즘에나오는거보면 막 시대의 흐름에 따라 스마트폰 쓴대요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100040637.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100040637.1.18",
          "form": "끝낼생각이 없군요...",
          "original_form": "끝낼생각이 없군요...",
          "speaker_id": "2",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100040637.1.19",
          "form": "너무 성공해도 결론적으로 작품을 망치는것같아요",
          "original_form": "너무 성공해도 결론적으로 작품을 망치는것같아요",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040637.1.20",
          "form": "그러니까요 ㅋㅋㅋ 여러가지 어른의 사정이 얽혀 완결나지 않는거 같긴하지만 ㅋㅋㅋ",
          "original_form": "그러니까요 ㅋㅋㅋ 여러가지 어른의 사정이 얽혀 완결나지 않는거 같긴하지만 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040637.1.21",
          "form": "물론 성공하는게 좋은데! 성공해야 작가님들이 창작할 동력을 얻는건데!",
          "original_form": "물론 성공하는게 좋은데! 성공해야 작가님들이 창작할 동력을 얻는건데!",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100040637.1.22",
          "form": "그치만 이건 너무하잖아요ㅠ",
          "original_form": "그치만 이건 너무하잖아요ㅠ",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100040637.1.23",
          "form": "그런의미에서 포켓몬은 진짜 머리좋은거같아요 ㅋㅋ",
          "original_form": "그런의미에서 포켓몬은 진짜 머리좋은거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100040637.1.24",
          "form": "계속 주인공바꿔가며 계속나와요 ㅋㅋ 물론원작이 게임이긴하지만 ㅋㅋ",
          "original_form": "계속 주인공바꿔가며 계속나와요 ㅋㅋ 물론원작이 게임이긴하지만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100040637.1.25",
          "form": "하지만 포켓몬도 전 지우에서 멈췄어요ㅎㅎ",
          "original_form": "하지만 포켓몬도 전 지우에서 멈췄어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100040637.1.26",
          "form": "저도 애니는 거기서 멈췄는데 포켓몬고라는 게임이 나올줄이야..",
          "original_form": "저도 애니는 거기서 멈췄는데 포켓몬고라는 게임이 나올줄이야..",
          "speaker_id": "1",
          "time": "20210904 23:38"
        }
      ]
