      "utterance": [
        {
          "id": "MDRW2100000487.1.1",
          "form": "요번에 도쿄올림픽 보셨어요?ㅋㅋㅋ",
          "original_form": "요번에 도쿄올림픽 보셨어요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000487.1.2",
          "form": "네! 엄청 챙겨봤어요",
          "original_form": "네! 엄청 챙겨봤어요",
          "speaker_id": "2",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000487.1.3",
          "form": "너무 재밌지 않았어요?",
          "original_form": "너무 재밌지 않았어요?",
          "speaker_id": "2",
          "time": "20210819 16:23"
        },
        {
          "id": "MDRW2100000487.1.4",
          "form": "그러니까요ㅠㅜ 하계올림픽이 이렇게 재밌는지 첨 알았어요ㅋㅋㅋ",
          "original_form": "그러니까요ㅠㅜ 하계올림픽이 이렇게 재밌는지 첨 알았어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000487.1.5",
          "form": "어떤 스포츠가 제일 재밌으셨어요?",
          "original_form": "어떤 스포츠가 제일 재밌으셨어요?",
          "speaker_id": "1",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000487.1.6",
          "form": "수영도 너무 재밌었고",
          "original_form": "수영도 너무 재밌었고",
          "speaker_id": "2",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000487.1.7",
          "form": "뭐니뭐니해도 양궁?",
          "original_form": "뭐니뭐니해도 양궁?",
          "speaker_id": "2",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000487.1.8",
          "form": "양궁 너무 재밌었죠ㅠㅜㅜ안산 선수 진짜 대단해요",
          "original_form": "양궁 너무 재밌었죠ㅠㅜㅜ안산 선수 진짜 대단해요",
          "speaker_id": "1",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000487.1.9",
          "form": "저는 양궁, 배구 재미있게 봤어요ㅋㅋㅋ",
          "original_form": "저는 양궁, 배구 재미있게 봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000487.1.10",
          "form": "안산 선수 최고!!",
          "original_form": "안산 선수 최고!!",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000487.1.11",
          "form": "너무 멋있었어요 정말",
          "original_form": "너무 멋있었어요 정말",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000487.1.12",
          "form": "아! 높이뛰기도요!",
          "original_form": "아! 높이뛰기도요!",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000487.1.13",
          "form": "진짜 울면서 응원했어요",
          "original_form": "진짜 울면서 응원했어요",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000487.1.14",
          "form": "맞아요ㅋㄲㅋㄱㄱ아 그리고 싸이클이랑 스케이트보드 경기도 해서 봤는데 엄청 재밌더라고요ㅋㅋ",
          "original_form": "맞아요ㅋㄲㅋㄱㄱ아 그리고 싸이클이랑 스케이트보드 경기도 해서 봤는데 엄청 재밌더라고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000487.1.15",
          "form": "클라이밍도 재밌게봤어요!",
          "original_form": "클라이밍도 재밌게봤어요!",
          "speaker_id": "1",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000487.1.16",
          "form": "반면 아쉬웠던 경기도 많았어요 그쵸",
          "original_form": "반면 아쉬웠던 경기도 많았어요 그쵸",
          "speaker_id": "2",
          "time": "20210819 16:31"
        },
        {
          "id": "MDRW2100000487.1.17",
          "form": "야구..또르르",
          "original_form": "야구..또르르",
          "speaker_id": "2",
          "time": "20210819 16:31"
        },
        {
          "id": "MDRW2100000487.1.18",
          "form": "야구,,,,ㅋㅋㅋ맞아요 기대 많이 하면서 봤는데 이번에 정말 실망스럽더라고요ㅠㅜ",
          "original_form": "야구,,,,ㅋㅋㅋ맞아요 기대 많이 하면서 봤는데 이번에 정말 실망스럽더라고요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000487.1.19",
          "form": "저는 원래 배구 좋아해서 이번에 정말 재밌게 봤어요ㅠㅜ",
          "original_form": "저는 원래 배구 좋아해서 이번에 정말 재밌게 봤어요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000487.1.20",
          "form": "김연경 선수 진짜 존경할 정도로 잘하더라고요",
          "original_form": "김연경 선수 진짜 존경할 정도로 잘하더라고요",
          "speaker_id": "2",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000487.1.21",
          "form": "팀이 으쌰으쌰하는 게 너무 보기 좋았어요",
          "original_form": "팀이 으쌰으쌰하는 게 너무 보기 좋았어요",
          "speaker_id": "2",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000487.1.22",
          "form": "진짜 팀스포츠가 그래서 재밌는 것 같아요ㅋㅋㅋ",
          "original_form": "진짜 팀스포츠가 그래서 재밌는 것 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000487.1.23",
          "form": "김연경선수 이번이 마지막 올림픽이라는 게 슬프더라고요ㅠㅜ",
          "original_form": "김연경선수 이번이 마지막 올림픽이라는 게 슬프더라고요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000487.1.24",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000487.1.25",
