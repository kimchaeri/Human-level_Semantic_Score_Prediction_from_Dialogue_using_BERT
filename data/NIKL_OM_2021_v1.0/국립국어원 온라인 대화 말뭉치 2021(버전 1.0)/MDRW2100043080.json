      "utterance": [
        {
          "id": "MDRW2100043080.1.1",
          "form": "반려동물 키워본 경험 있으세요?",
          "original_form": "반려동물 키워본 경험 있으세요?",
          "speaker_id": "1",
          "time": "20210907 14:02"
        },
        {
          "id": "MDRW2100043080.1.2",
          "form": "예전에 새들 키운적 있어요",
          "original_form": "예전에 새들 키운적 있어요",
          "speaker_id": "2",
          "time": "20210907 16:22"
        },
        {
          "id": "MDRW2100043080.1.3",
          "form": "새요? 와 무슨 새요?",
          "original_form": "새요? 와 무슨 새요?",
          "speaker_id": "1",
          "time": "20210907 16:25"
        },
        {
          "id": "MDRW2100043080.1.4",
          "form": "백문조라고 하얀새에요ㅎㅎ",
          "original_form": "백문조라고 하얀새에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 16:41"
        },
        {
          "id": "MDRW2100043080.1.5",
          "form": "아뇨 지금 처음 듣는데 듣기만 해도 엄청 예쁠 거 같네요",
          "original_form": "아뇨 지금 처음 듣는데 듣기만 해도 엄청 예쁠 거 같네요",
          "speaker_id": "1",
          "time": "20210907 16:44"
        },
        {
          "id": "MDRW2100043080.1.6",
          "form": "참새과라 딱 참새사이즈에요",
          "original_form": "참새과라 딱 참새사이즈에요",
          "speaker_id": "2",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100043080.1.7",
          "form": "참새과요? 사이즈도 귀엽네요",
          "original_form": "참새과요? 사이즈도 귀엽네요",
          "speaker_id": "1",
          "time": "20210907 16:53"
        },
        {
          "id": "MDRW2100043080.1.8",
          "form": "그런데 키우기 쉽나요?",
          "original_form": "그런데 키우기 쉽나요?",
          "speaker_id": "1",
          "time": "20210907 16:53"
        },
        {
          "id": "MDRW2100043080.1.9",
          "form": "참새는 사람  근처만 와도 도망가던데",
          "original_form": "참새는 사람  근처만 와도 도망가던데",
          "speaker_id": "1",
          "time": "20210907 16:53"
        },
        {
          "id": "MDRW2100043080.1.10",
          "form": "애기 때부터 밥 먹여가며 길들인 녀석들은 손 위에 잘 올라오고 그래요ㅎㅎ",
          "original_form": "애기 때부터 밥 먹여가며 길들인 녀석들은 손 위에 잘 올라오고 그래요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 16:56"
        },
        {
          "id": "MDRW2100043080.1.11",
          "form": "어머나",
          "original_form": "어머나",
          "speaker_id": "1",
          "time": "20210907 16:57"
        },
        {
          "id": "MDRW2100043080.1.12",
          "form": "새는 한번도 못키워봤는데",
          "original_form": "새는 한번도 못키워봤는데",
          "speaker_id": "1",
          "time": "20210907 16:57"
        },
        {
          "id": "MDRW2100043080.1.13",
          "form": "너무 너무 키워보고 싶네요",
          "original_form": "너무 너무 키워보고 싶네요",
          "speaker_id": "1",
          "time": "20210907 16:57"
        },
        {
          "id": "MDRW2100043080.1.14",
          "form": "소리도 참새소리랑 비슷한가요?",
          "original_form": "소리도 참새소리랑 비슷한가요?",
          "speaker_id": "1",
          "time": "20210907 16:57"
        },
        {
          "id": "MDRW2100043080.1.15",
          "form": "다른 동물 키워보신 적 있으신가요?",
          "original_form": "다른 동물 키워보신 적 있으신가요?",
          "speaker_id": "2",
          "time": "20210907 16:58"
        },
        {
          "id": "MDRW2100043080.1.16",
          "form": "참새하고 조금 다르긴 하지만 비슷했어요ㅎㅎ",
          "original_form": "참새하고 조금 다르긴 하지만 비슷했어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 16:58"
        },
        {
          "id": "MDRW2100043080.1.17",
          "form": "저는 강아지요",
          "original_form": "저는 강아지요",
          "speaker_id": "1",
          "time": "20210907 17:01"
        },
        {
          "id": "MDRW2100043080.1.18",
          "form": "말티즈 키워봤어요",
          "original_form": "말티즈 키워봤어요",
          "speaker_id": "1",
          "time": "20210907 17:01"
        },
        {
          "id": "MDRW2100043080.1.19",
          "form": "일반적이죠",
          "original_form": "일반적이죠",
          "speaker_id": "1",
          "time": "20210907 17:01"
        },
        {
          "id": "MDRW2100043080.1.20",
          "form": "말티즈 귀엽죠ㅎㅎ",
          "original_form": "말티즈 귀엽죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 17:02"
        },
        {
          "id": "MDRW2100043080.1.21",
          "form": "몇 년이나 키우셨어요?",
          "original_form": "몇 년이나 키우셨어요?",
          "speaker_id": "2",
          "time": "20210907 17:03"
        },
        {
          "id": "MDRW2100043080.1.22",
          "form": "몇년 못키우고 우리 곁을 떠났어요",
          "original_form": "몇년 못키우고 우리 곁을 떠났어요",
          "speaker_id": "1",
          "time": "20210907 17:04"
        },
        {
          "id": "MDRW2100043080.1.23",
          "form": "너무 슬펐죠",
          "original_form": "너무 슬펐죠",
          "speaker_id": "1",
          "time": "20210907 17:05"
        },
        {
          "id": "MDRW2100043080.1.24",
          "form": "반려동물 키우는 건 그게 정말 힘든 것 같아요 ㅠㅜ",
          "original_form": "반려동물 키우는 건 그게 정말 힘든 것 같아요 ㅠㅜ",
          "speaker_id": "2",
