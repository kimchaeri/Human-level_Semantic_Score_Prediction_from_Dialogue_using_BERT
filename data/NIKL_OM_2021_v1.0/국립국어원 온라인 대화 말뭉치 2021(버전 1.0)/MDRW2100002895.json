      "utterance": [
        {
          "id": "MDRW2100002895.1.1",
          "form": "테이프로 붙여주는거 은근 행복이에요",
          "original_form": "테이프로 붙여주는거 은근 행복이에요",
          "speaker_id": "1",
          "time": "20210820 16:20"
        },
        {
          "id": "MDRW2100002895.1.2",
          "form": "최근에 산 물건중에 만족스러우신거 있나요?",
          "original_form": "최근에 산 물건중에 만족스러우신거 있나요?",
          "speaker_id": "1",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002895.1.3",
          "form": "며칠전에 플랫슈즈하나 구매했는데 괜찮더라구요",
          "original_form": "며칠전에 플랫슈즈하나 구매했는데 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002895.1.4",
          "form": "락포트도 가끔 직구 좋아요ㅎㅎ",
          "original_form": "락포트도 가끔 직구 좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002895.1.5",
          "form": "브랜디라는 사이트에서 신규가입하면 50프로 쿠폰줘서 샀는데",
          "original_form": "브랜디라는 사이트에서 신규가입하면 50프로 쿠폰줘서 샀는데",
          "speaker_id": "2",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002895.1.6",
          "form": "카드할인도 받고해서 3만원짜리 이래저래 6천원에 샀어요ㅎ",
          "original_form": "카드할인도 받고해서 3만원짜리 이래저래 6천원에 샀어요ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002895.1.7",
          "form": "저도 신규가입 쿠폰 때문에ㅋㅋ 가입 사이트가 많아요",
          "original_form": "저도 신규가입 쿠폰 때문에ㅋㅋ 가입 사이트가 많아요",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002895.1.8",
          "form": "메모해야겠네요. 6천원!!",
          "original_form": "메모해야겠네요. 6천원!!",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002895.1.9",
          "form": "락포트는 비싸죠?ㅎ",
          "original_form": "락포트는 비싸죠?ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002895.1.10",
          "form": "시즌에 직구하면 할인률이 높은데",
          "original_form": "시즌에 직구하면 할인률이 높은데",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002895.1.11",
          "form": "일단 너무 편해요",
          "original_form": "일단 너무 편해요",
          "speaker_id": "1",
          "time": "20210820 16:33"
        },
        {
          "id": "MDRW2100002895.1.12",
          "form": "한번 신어보고싶긴한데 비싸서 선뜻 못사겠더라구요",
          "original_form": "한번 신어보고싶긴한데 비싸서 선뜻 못사겠더라구요",
          "speaker_id": "2",
          "time": "20210820 16:34"
        },
        {
          "id": "MDRW2100002895.1.13",
          "form": "직구 시작하면 삶이 달라지는데",
          "original_form": "직구 시작하면 삶이 달라지는데",
          "speaker_id": "1",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002895.1.14",
          "form": "귀찮기는 해요",
          "original_form": "귀찮기는 해요",
          "speaker_id": "1",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002895.1.15",
          "form": "직구하는방법을 몰라서 시작도 못하고있어요",
          "original_form": "직구하는방법을 몰라서 시작도 못하고있어요",
          "speaker_id": "2",
          "time": "20210820 16:35"
        },
        {
          "id": "MDRW2100002895.1.16",
          "form": "저도 블로그보면서 시작..",
          "original_form": "저도 블로그보면서 시작..",
          "speaker_id": "1",
          "time": "20210820 17:37"
        },
        {
          "id": "MDRW2100002895.1.17",
          "form": "어렵긴 해요",
          "original_form": "어렵긴 해요",
          "speaker_id": "1",
          "time": "20210820 17:37"
        },
        {
          "id": "MDRW2100002895.1.18",
          "form": "특히 사이즈미스가ㅠ",
          "original_form": "특히 사이즈미스가ㅠ",
          "speaker_id": "1",
          "time": "20210820 17:37"
        },
        {
          "id": "MDRW2100002895.1.19",
          "form": "관세도 내야되죠?",
          "original_form": "관세도 내야되죠?",
          "speaker_id": "2",
          "time": "20210820 17:41"
        },
        {
          "id": "MDRW2100002895.1.20",
          "form": "사이즈안맞으면 대략 난감이네요ㅎㅎ",
          "original_form": "사이즈안맞으면 대략 난감이네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 17:42"
        },
        {
          "id": "MDRW2100002895.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210820 17:42"
        },
        {
          "id": "MDRW2100002895.1.22",
          "form": "해외로 반품할수도 없고",
          "original_form": "해외로 반품할수도 없고",
          "speaker_id": "2",
          "time": "20210820 17:42"
        },
        {
          "id": "MDRW2100002895.1.23",
          "form": "그런데 중고거래 잘 하면 돼요ㅋ",
          "original_form": "그런데 중고거래 잘 하면 돼요ㅋ",
          "speaker_id": "1",
          "time": "20210820 17:42"
        },
        {
          "id": "MDRW2100002895.1.24",
          "form": "역시 귀찮지요ㅋ",
          "original_form": "역시 귀찮지요ㅋ",
          "speaker_id": "1",
          "time": "20210820 17:42"
        },
        {
          "id": "MDRW2100002895.1.25",
          "form": "맞아요 스벅 할인카드 잘챙겨야해요",
          "original_form": "맞아요 스벅 할인카드 잘챙겨야해요",
