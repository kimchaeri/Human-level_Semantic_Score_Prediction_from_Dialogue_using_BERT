      "utterance": [
        {
          "id": "MDRW2100045742.1.1",
          "form": "매번 피할 수는 없고 서글퍼욬ㅋㅋㅋ큐ㅠ",
          "original_form": "매번 피할 수는 없고 서글퍼욬ㅋㅋㅋ큐ㅠ",
          "speaker_id": "1",
          "time": "20210908 06:31"
        },
        {
          "id": "MDRW2100045742.1.2",
          "form": "왜 높으신분들은 그런델 좋아할까요",
          "original_form": "왜 높으신분들은 그런델 좋아할까요",
          "speaker_id": "2",
          "time": "20210908 06:32"
        },
        {
          "id": "MDRW2100045742.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ좀 그 동네가 잘못하긴 했어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ좀 그 동네가 잘못하긴 했어요",
          "speaker_id": "1",
          "time": "20210908 06:34"
        },
        {
          "id": "MDRW2100045742.1.4",
          "form": "모임을 할 수 있는 넓은 식당이 별로 없어서",
          "original_form": "모임을 할 수 있는 넓은 식당이 별로 없어서",
          "speaker_id": "1",
          "time": "20210908 06:34"
        },
        {
          "id": "MDRW2100045742.1.5",
          "form": "그런 지옥의 맛으로도 역사와 전통으로 자리잡고 말아버렸죠",
          "original_form": "그런 지옥의 맛으로도 역사와 전통으로 자리잡고 말아버렸죠",
          "speaker_id": "1",
          "time": "20210908 06:34"
        },
        {
          "id": "MDRW2100045742.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ name1님 말하는거 너무 웃기다구욬ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ &name1&님 말하는거 너무 웃기다구욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:35"
        },
        {
          "id": "MDRW2100045742.1.7",
          "form": "서브웨이는 뭐 좋아하세요?",
          "original_form": "서브웨이는 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045742.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니 그 정도에옄ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아니 그 정도에옄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045742.1.9",
          "form": "저는 에그마요!",
          "original_form": "저는 에그마요!",
          "speaker_id": "1",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045742.1.10",
          "form": "칼로리가 슬프지만",
          "original_form": "칼로리가 슬프지만",
          "speaker_id": "1",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045742.1.11",
          "form": "계란 좋아하세요?",
          "original_form": "계란 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045742.1.12",
          "form": "넵 계란 사랑하죠!!",
          "original_form": "넵 계란 사랑하죠!!",
          "speaker_id": "1",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045742.1.13",
          "form": "마요네즈는 더 사랑하구요!",
          "original_form": "마요네즈는 더 사랑하구요!",
          "speaker_id": "1",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045742.1.14",
          "form": "ㅋㅋㅋㄱ계란 완전식품이죠",
          "original_form": "ㅋㅋㅋㄱ계란 완전식품이죠",
          "speaker_id": "2",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045742.1.15",
          "form": "짱짱",
          "original_form": "짱짱",
          "speaker_id": "2",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045742.1.16",
          "form": "저는 그래도 고기가 더 좋아욧",
          "original_form": "저는 그래도 고기가 더 좋아욧",
          "speaker_id": "2",
          "time": "20210908 06:36"
        },
        {
          "id": "MDRW2100045742.1.17",
          "form": "터키나 비엘티 아니면 치킨도 좋아요",
          "original_form": "터키나 비엘티 아니면 치킨도 좋아요",
          "speaker_id": "2",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045742.1.18",
          "form": "터키 완전 자주 먹었죠ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "터키 완전 자주 먹었죠ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045742.1.19",
          "form": "터키 맛있져 ㅋㅋㅋ그래도 계란이 더 좋으세요!",
          "original_form": "터키 맛있져 ㅋㅋㅋ그래도 계란이 더 좋으세요!",
          "speaker_id": "2",
          "time": "20210908 06:37"
        },
        {
          "id": "MDRW2100045742.1.20",
          "form": "풀드포크? 도 가끔 먹었던 것 같아요",
          "original_form": "풀드포크? 도 가끔 먹었던 것 같아요",
          "speaker_id": "1",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045742.1.21",
          "form": "어떻게 변해요",
          "original_form": "어떻게 변해요",
          "speaker_id": "2",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045742.1.22",
          "form": "요즘 마요네즈를 잘 못먹었더니 마요네즈가 좀 그리워서...",
          "original_form": "요즘 마요네즈를 잘 못먹었더니 마요네즈가 좀 그리워서...",
          "speaker_id": "1",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045742.1.23",
          "form": "고기도 옳고 마요네즈도 옳고",
          "original_form": "고기도 옳고 마요네즈도 옳고",
          "speaker_id": "1",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045742.1.24",
          "form": "다 옳습니다",
          "original_form": "다 옳습니다",
          "speaker_id": "1",
          "time": "20210908 06:38"
        },
        {
          "id": "MDRW2100045742.1.25",
          "form": "고기 구우면서 마요네즈 바르면 맛있다던데",
          "original_form": "고기 구우면서 마요네즈 바르면 맛있다던데",
          "speaker_id": "1",
          "time": "20210908 06:39"
        },
        {
          "id": "MDRW2100045742.1.26",
          "form": "그냥 맛있는거 + 맛있는거 = 더 맛있는거가 아닐까요?",
          "original_form": "그냥 맛있는거 + 맛있는거 = 더 맛있는거가 아닐까요?",
          "speaker_id": "1",
          "time": "20210908 06:39"
        },
        {
          "id": "MDRW2100045742.1.27",
          "form": "오 제 뱃살이 늘어나는게 보이는것 같아요 ㅋㅋㅋㅋ",
          "original_form": "오 제 뱃살이 늘어나는게 보이는것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:41"
        },
        {
          "id": "MDRW2100045742.1.28",
          "form": "아니 근데 서브웨이는 야채 많이 넣어달라해도 똑같은거 같아요 ㅠㅠ 좀 더 주지",
          "original_form": "아니 근데 서브웨이는 야채 많이 넣어달라해도 똑같은거 같아요 ㅠㅠ 좀 더 주지",
          "speaker_id": "2",
          "time": "20210908 06:42"
        },
        {
          "id": "MDRW2100045742.1.29",
          "form": "야채가 적어서 아쉬워요",
          "original_form": "야채가 적어서 아쉬워요",
          "speaker_id": "2",
          "time": "20210908 06:42"
        }
      ]
