      "utterance": [
        {
          "id": "MDRW2100002454.1.1",
          "form": "잘못눌렀다 아들",
          "original_form": "잘못눌렀다 아들",
          "speaker_id": "1",
          "time": "20210820 12:26"
        },
        {
          "id": "MDRW2100002454.1.2",
          "form": "알아요 있다이어서해요",
          "original_form": "알아요 있다이어서해요",
          "speaker_id": "2",
          "time": "20210820 12:27"
        },
        {
          "id": "MDRW2100002454.1.3",
          "form": "닭맛있었지",
          "original_form": "닭맛있었지",
          "speaker_id": "1",
          "time": "20210820 18:44"
        },
        {
          "id": "MDRW2100002454.1.4",
          "form": "네당연하죠",
          "original_form": "네당연하죠",
          "speaker_id": "2",
          "time": "20210820 18:44"
        },
        {
          "id": "MDRW2100002454.1.5",
          "form": "하긴 남이해주는것도맛있지",
          "original_form": "하긴 남이해주는것도맛있지",
          "speaker_id": "1",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100002454.1.6",
          "form": "그렇죠 프렌차이즈잔아요",
          "original_form": "그렇죠 프렌차이즈잔아요",
          "speaker_id": "2",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100002454.1.7",
          "form": "기업서만든거지",
          "original_form": "기업서만든거지",
          "speaker_id": "1",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100002454.1.8",
          "form": "근데 그건있어요  튀기는기술",
          "original_form": "근데 그건있어요  튀기는기술",
          "speaker_id": "2",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100002454.1.9",
          "form": "그게가장중요하겠지 아들",
          "original_form": "그게가장중요하겠지 아들",
          "speaker_id": "1",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100002454.1.10",
          "form": "네 우리가는집은 잘튀겨요",
          "original_form": "네 우리가는집은 잘튀겨요",
          "speaker_id": "2",
          "time": "20210820 18:46"
        },
        {
          "id": "MDRW2100002454.1.11",
          "form": "그래 오늘은 편하게 저녁먹었다",
          "original_form": "그래 오늘은 편하게 저녁먹었다",
          "speaker_id": "1",
          "time": "20210820 18:46"
        },
        {
          "id": "MDRW2100002454.1.12",
          "form": "네 오늘은 셰프에선물에 외국인나오네",
          "original_form": "네 오늘은 셰프에선물에 외국인나오네",
          "speaker_id": "2",
          "time": "20210820 18:46"
        },
        {
          "id": "MDRW2100002454.1.13",
          "form": "난 할매가90넘은게 더 신기하다",
          "original_form": "난 할매가90넘은게 더 신기하다",
          "speaker_id": "1",
          "time": "20210820 18:47"
        },
        {
          "id": "MDRW2100002454.1.14",
          "form": "바느질도잘하시네요",
          "original_form": "바느질도잘하시네요",
          "speaker_id": "2",
          "time": "20210820 18:47"
        },
        {
          "id": "MDRW2100002454.1.15",
          "form": "주부경력이 얼마나되겠니",
          "original_form": "주부경력이 얼마나되겠니",
          "speaker_id": "1",
          "time": "20210820 18:47"
        },
        {
          "id": "MDRW2100002454.1.16",
          "form": "하긴 엄마도 엄청됐죠",
          "original_form": "하긴 엄마도 엄청됐죠",
          "speaker_id": "2",
          "time": "20210820 18:47"
        },
        {
          "id": "MDRW2100002454.1.17",
          "form": "저 할매에비한다면야",
          "original_form": "저 할매에비한다면야",
          "speaker_id": "1",
          "time": "20210820 18:48"
        },
        {
          "id": "MDRW2100002454.1.18",
          "form": "하긴 그래도 엄마도연식이좀되셨죠",
          "original_form": "하긴 그래도 엄마도연식이좀되셨죠",
          "speaker_id": "2",
          "time": "20210820 18:48"
        },
        {
          "id": "MDRW2100002454.1.19",
          "form": "주부경력짱 엄마다",
          "original_form": "주부경력짱 엄마다",
          "speaker_id": "1",
          "time": "20210820 18:48"
        },
        {
          "id": "MDRW2100002454.1.20",
          "form": "인정합니다",
          "original_form": "인정합니다",
          "speaker_id": "2",
          "time": "20210820 18:48"
        },
        {
          "id": "MDRW2100002454.1.21",
          "form": "잡초잘베네 외국인",
          "original_form": "잡초잘베네 외국인",
          "speaker_id": "1",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100002454.1.22",
          "form": "할머니가 칭찬해주시네요",
          "original_form": "할머니가 칭찬해주시네요",
          "speaker_id": "2",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100002454.1.23",
          "form": "우리아들도 집안일잘하지",
          "original_form": "우리아들도 집안일잘하지",
          "speaker_id": "1",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100002454.1.24",
          "form": "집에오래있다보니 저도경력자죠",
          "original_form": "집에오래있다보니 저도경력자죠",
          "speaker_id": "2",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100002454.1.25",
          "form": "저기도 아들잘한다하네",
          "original_form": "저기도 아들잘한다하네",
          "speaker_id": "1",
          "time": "20210820 18:50"
        },
        {
          "id": "MDRW2100002454.1.26",
          "form": "그게 엄마마음인가보죠",
          "original_form": "그게 엄마마음인가보죠",
          "speaker_id": "2",
          "time": "20210820 18:50"
        },
        {
          "id": "MDRW2100002454.1.27",
          "form": "와 맛있겠다",
          "original_form": "와 맛있겠다",
          "speaker_id": "1",
          "time": "20210820 18:50"
        },
        {
          "id": "MDRW2100002454.1.28",
          "form": "오리나왔나봐요 오리고기",
          "original_form": "오리나왔나봐요 오리고기",
          "speaker_id": "2",
          "time": "20210820 18:50"
        },
        {
          "id": "MDRW2100002454.1.29",
          "form": "오리고기좋지",
          "original_form": "오리고기좋지",
          "speaker_id": "1",
          "time": "20210820 18:50"
        },
        {
          "id": "MDRW2100002454.1.30",
          "form": "낼오리고기어때요",
          "original_form": "낼오리고기어때요",
          "speaker_id": "2",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100002454.1.31",
          "form": "아들 엄마힘들다",
          "original_form": "아들 엄마힘들다",
          "speaker_id": "1",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100002454.1.32",
          "form": "눼눼",
          "original_form": "눼눼",
          "speaker_id": "2",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100002454.1.33",
          "form": "요리한다",
          "original_form": "요리한다",
          "speaker_id": "1",
          "time": "20210820 18:51"
        },
        {
          "id": "MDRW2100002454.1.34",
          "form": "불가리아식이래요",
          "original_form": "불가리아식이래요",
          "speaker_id": "2",
          "time": "20210820 18:52"
        },
        {
          "id": "MDRW2100002454.1.35",
          "form": "그거나내가하는거나",
          "original_form": "그거나내가하는거나",
          "speaker_id": "1",
          "time": "20210820 18:52"
        },
        {
          "id": "MDRW2100002454.1.36",
          "form": "엄마낼오리좀",
          "original_form": "엄마낼오리좀",
          "speaker_id": "2",
          "time": "20210820 18:52"
        },
        {
          "id": "MDRW2100002454.1.37",
          "form": "아들 보는거랑 만드는건다르지",
          "original_form": "아들 보는거랑 만드는건다르지",
          "speaker_id": "1",
          "time": "20210820 18:52"
        },
        {
          "id": "MDRW2100002454.1.38",
          "form": "눼",
          "original_form": "눼",
          "speaker_id": "2",
          "time": "20210820 18:52"
        },
        {
          "id": "MDRW2100002454.1.39",
          "form": "엄마백신접종한지얼마안되었어",
          "original_form": "엄마백신접종한지얼마안되었어",
          "speaker_id": "1",
          "time": "20210820 18:53"
        },
        {
          "id": "MDRW2100002454.1.40",
          "form": "알아요 제가할까욧",
          "original_form": "알아요 제가할까욧",
          "speaker_id": "2",
          "time": "20210820 18:53"
        },
        {
          "id": "MDRW2100002454.1.41",
          "form": "됐네 집안일은 내전문이요",
          "original_form": "됐네 집안일은 내전문이요",
          "speaker_id": "1",
          "time": "20210820 18:53"
        },
        {
          "id": "MDRW2100002454.1.42",
          "form": "배우면되지롱",
          "original_form": "배우면되지롱",
          "speaker_id": "2",
          "time": "20210820 18:53"
        },
        {
          "id": "MDRW2100002454.1.43",
          "form": "음되았다",
          "original_form": "음되았다",
          "speaker_id": "1",
          "time": "20210820 18:54"
        },
        {
          "id": "MDRW2100002454.1.44",
          "form": "넹 강아지가 파리보더니 정신못차리네",
          "original_form": "넹 강아지가 파리보더니 정신못차리네",
          "speaker_id": "2",
          "time": "20210820 18:54"
        },
        {
          "id": "MDRW2100002454.1.45",
          "form": "신기한가보다",
          "original_form": "신기한가보다",
          "speaker_id": "1",
          "time": "20210820 18:54"
        },
        {
          "id": "MDRW2100002454.1.46",
          "form": "날라다니니깐 그렇죠",
          "original_form": "날라다니니깐 그렇죠",
          "speaker_id": "2",
          "time": "20210820 18:55"
        },
        {
          "id": "MDRW2100002454.1.47",
          "form": "친구하나생겼네",
          "original_form": "친구하나생겼네",
          "speaker_id": "1",
          "time": "20210820 18:55"
        },
        {
          "id": "MDRW2100002454.1.48",
          "form": "심심하던차에잘됐네요",
          "original_form": "심심하던차에잘됐네요",
          "speaker_id": "2",
          "time": "20210820 18:55"
        },
        {
          "id": "MDRW2100002454.1.49",
          "form": "자세가귀엽네",
          "original_form": "자세가귀엽네",
          "speaker_id": "1",
          "time": "20210820 18:55"
        },
        {
          "id": "MDRW2100002454.1.50",
          "form": "울집개는귀여움빼면시체죠",
          "original_form": "울집개는귀여움빼면시체죠",
          "speaker_id": "2",
          "time": "20210820 18:56"
        },
        {
          "id": "MDRW2100002454.1.51",
          "form": "그게 우리집에있는이유지",
          "original_form": "그게 우리집에있는이유지",
          "speaker_id": "1",
          "time": "20210820 18:56"
        },
        {
          "id": "MDRW2100002454.1.52",
          "form": "그렇죠 이름하야 애완견",
          "original_form": "그렇죠 이름하야 애완견",
          "speaker_id": "2",
          "time": "20210820 18:56"
        },
        {
          "id": "MDRW2100002454.1.53",
          "form": "오늘도 아쉽게도 끝날시간이네",
          "original_form": "오늘도 아쉽게도 끝날시간이네",
          "speaker_id": "1",
          "time": "20210820 18:57"
        },
        {
          "id": "MDRW2100002454.1.54",
          "form": "내고향정말좋아하셔",
          "original_form": "내고향정말좋아하셔",
          "speaker_id": "2",
          "time": "20210820 18:57"
        },
        {
          "id": "MDRW2100002454.1.55",
          "form": "내고향생각나잔니",
          "original_form": "내고향생각나잔니",
          "speaker_id": "1",
          "time": "20210820 18:57"
        },
        {
          "id": "MDRW2100002454.1.56",
          "form": "귀촌어때요",
          "original_form": "귀촌어때요",
          "speaker_id": "2",
          "time": "20210820 18:57"
        },
        {
          "id": "MDRW2100002454.1.57",
          "form": "그립긴하지만 나는 서울이좋다",
          "original_form": "그립긴하지만 나는 서울이좋다",
          "speaker_id": "1",
          "time": "20210820 18:57"
        }
      ]
    }
