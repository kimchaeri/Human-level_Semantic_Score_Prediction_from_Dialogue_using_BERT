      "utterance": [
        {
          "id": "MDRW2100036232.1.1",
          "form": "심장말고는건강하신가요",
          "original_form": "심장말고는건강하신가요",
          "speaker_id": "2",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100036232.1.2",
          "form": "영양제는잘챙겨먹나요",
          "original_form": "영양제는잘챙겨먹나요",
          "speaker_id": "2",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100036232.1.3",
          "form": "ㅋㅋㅋㅋㅋ요즘은 구냥 유산균만 먹는거같아요",
          "original_form": "ㅋㅋㅋㅋㅋ요즘은 구냥 유산균만 먹는거같아요",
          "speaker_id": "1",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100036232.1.4",
          "form": "잘챙겨먹어야하는데까먹네요",
          "original_form": "잘챙겨먹어야하는데까먹네요",
          "speaker_id": "2",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100036232.1.5",
          "form": "저는 정수기 옆에 쌓아놔서 물마실때 보이긴 해요",
          "original_form": "저는 정수기 옆에 쌓아놔서 물마실때 보이긴 해요",
          "speaker_id": "1",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100036232.1.6",
          "form": "오 역시보이는데계ㅣ속두고먹어야해요",
          "original_form": "오 역시보이는데계ㅣ속두고먹어야해요",
          "speaker_id": "2",
          "time": "20210830 20:29"
        },
        {
          "id": "MDRW2100036232.1.7",
          "form": "ㅋㅋㅋㅋ마자요 아님 안먹어",
          "original_form": "ㅋㅋㅋㅋ마자요 아님 안먹어",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100036232.1.8",
          "form": "우리잘챙겨먹자구요..",
          "original_form": "우리잘챙겨먹자구요..",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100036232.1.9",
          "form": "ㅋㅋㅋㅋㅋ머머 챙겨먹어요 요즘",
          "original_form": "ㅋㅋㅋㅋㅋ머머 챙겨먹어요 요즘",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100036232.1.10",
          "form": "사놓은건개많은데",
          "original_form": "사놓은건개많은데",
          "speaker_id": "2",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100036232.1.11",
          "form": "ㅇ먹는건유산균...?",
          "original_form": "ㅇ먹는건유산균...?",
          "speaker_id": "2",
          "time": "20210830 21:32"
        },
        {
          "id": "MDRW2100036232.1.12",
          "form": "오늘뭐입구갓어요",
          "original_form": "오늘뭐입구갓어요",
          "speaker_id": "2",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100036232.1.13",
          "form": "오늘 반팔티에 롱스커트용",
          "original_form": "오늘 반팔티에 롱스커트용",
          "speaker_id": "1",
          "time": "20210831 18:43"
        },
        {
          "id": "MDRW2100036232.1.14",
          "form": "이쁜거입구가셨네용",
          "original_form": "이쁜거입구가셨네용",
          "speaker_id": "2",
          "time": "20210831 18:43"
        },
        {
          "id": "MDRW2100036232.1.15",
          "form": "가디건도챙기셨나요",
          "original_form": "가디건도챙기셨나요",
          "speaker_id": "2",
          "time": "20210831 18:44"
        },
        {
          "id": "MDRW2100036232.1.16",
          "form": "아니용ㅋㅋㅋㅋㅋ덥던데요?",
          "original_form": "아니용ㅋㅋㅋㅋㅋ덥던데요?",
          "speaker_id": "1",
          "time": "20210831 18:46"
        },
        {
          "id": "MDRW2100036232.1.17",
          "form": "아 더워서 안챙기셨군요 실내에어컨안춥나용",
          "original_form": "아 더워서 안챙기셨군요 실내에어컨안춥나용",
          "speaker_id": "2",
          "time": "20210831 20:42"
        },
        {
          "id": "MDRW2100036232.1.18",
          "form": "추워오 근데 수업하다보면 또 더워용",
          "original_form": "추워오 근데 수업하다보면 또 더워용",
          "speaker_id": "1",
          "time": "20210831 21:19"
        },
        {
          "id": "MDRW2100036232.1.19",
          "form": "열정적인 센세이",
          "original_form": "열정적인 센세이",
          "speaker_id": "2",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100036232.1.20",
          "form": "더울만해요",
          "original_form": "더울만해요",
          "speaker_id": "2",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100036232.1.21",
          "form": "ㅋㅋㅋㅋㅋ마스크 쓰고 얘기해서 그런가",
          "original_form": "ㅋㅋㅋㅋㅋ마스크 쓰고 얘기해서 그런가",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100036232.1.22",
          "form": "그것도 영향받겠다\r\n근데계속설명하구그럼덥지더워",
          "original_form": "그것도 영향받겠다\r\n근데계속설명하구그럼덥지더워",
          "speaker_id": "2",
          "time": "20210831 21:26"
        }
      ]
    }
