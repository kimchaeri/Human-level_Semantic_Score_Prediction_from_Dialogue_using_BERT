      "utterance": [
        {
          "id": "MDRW2100016688.1.1",
          "form": "요새 무슨 노래 듣니?",
          "original_form": "요새 무슨 노래 듣니?",
          "speaker_id": "2",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100016688.1.2",
          "form": "좋은 노래 추천해줘",
          "original_form": "좋은 노래 추천해줘",
          "speaker_id": "2",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100016688.1.3",
          "form": "나 요새 다시  kpop에 빠졌어",
          "original_form": "나 요새 다시  kpop에 빠졌어",
          "speaker_id": "1",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100016688.1.4",
          "form": "더보이즈 스릴라이드 추천이요 ㅎ",
          "original_form": "더보이즈 스릴라이드 추천이요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100016688.1.5",
          "form": "선우정아에서 아이돌로 넘어왔니?",
          "original_form": "선우정아에서 아이돌로 넘어왔니?",
          "speaker_id": "2",
          "time": "20210826 14:45"
        },
        {
          "id": "MDRW2100016688.1.6",
          "form": "처음 듣는 노래네 다음에 한번 들어볼게",
          "original_form": "처음 듣는 노래네 다음에 한번 들어볼게",
          "speaker_id": "2",
          "time": "20210826 14:45"
        },
        {
          "id": "MDRW2100016688.1.7",
          "form": "ㅎㅎ 다 좋지만 kpop 너무 재밌다",
          "original_form": "ㅎㅎ 다 좋지만 kpop 너무 재밌다",
          "speaker_id": "1",
          "time": "20210826 14:46"
        },
        {
          "id": "MDRW2100016688.1.8",
          "form": "너도 추천해줘",
          "original_form": "너도 추천해줘",
          "speaker_id": "1",
          "time": "20210826 14:46"
        },
        {
          "id": "MDRW2100016688.1.9",
          "form": "나는 아쉽지만 요새 듣는 노래가 없어..ㅎ...",
          "original_form": "나는 아쉽지만 요새 듣는 노래가 없어..ㅎ...",
          "speaker_id": "2",
          "time": "20210826 14:46"
        },
        {
          "id": "MDRW2100016688.1.10",
          "form": "ㅣ음악 어플도 중지 했더니 모르겠더라구",
          "original_form": "ㅣ음악 어플도 중지 했더니 모르겠더라구",
          "speaker_id": "2",
          "time": "20210826 14:46"
        },
        {
          "id": "MDRW2100016688.1.11",
          "form": "머라구??! 그럼 뭐해 또 드라마만 봐",
          "original_form": "머라구??! 그럼 뭐해 또 드라마만 봐",
          "speaker_id": "1",
          "time": "20210826 14:46"
        },
        {
          "id": "MDRW2100016688.1.12",
          "form": "드라마 ost나 좀 들었지",
          "original_form": "드라마 ost나 좀 들었지",
          "speaker_id": "2",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016688.1.13",
          "form": "드라마 세 편 보고있는데",
          "original_form": "드라마 세 편 보고있는데",
          "speaker_id": "2",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016688.1.14",
          "form": "두 편이 이번주에 끝나ㅠ",
          "original_form": "두 편이 이번주에 끝나ㅠ",
          "speaker_id": "2",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016688.1.15",
          "form": "오 정말 바쁘구나? 드라마 뭐봐?",
          "original_form": "오 정말 바쁘구나? 드라마 뭐봐?",
          "speaker_id": "1",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016688.1.16",
          "form": "진짜 바쁘네..",
          "original_form": "진짜 바쁘네..",
          "speaker_id": "1",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016688.1.17",
          "form": "너는 나의 봄, 미치지 않고서야, 슬기로운 의사생활2",
          "original_form": "너는 나의 봄, 미치지 않고서야, 슬기로운 의사생활2",
          "speaker_id": "2",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016688.1.18",
          "form": "너는 나의 봄은 화요일에 끝났고",
          "original_form": "너는 나의 봄은 화요일에 끝났고",
          "speaker_id": "2",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016688.1.19",
          "form": "미치지 않고서야는 오늘 끝나",
          "original_form": "미치지 않고서야는 오늘 끝나",
          "speaker_id": "2",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016688.1.20",
          "form": "와 부지런하구나",
          "original_form": "와 부지런하구나",
          "speaker_id": "1",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016688.1.21",
          "form": "제일 재밌는거 뭐야?",
          "original_form": "제일 재밌는거 뭐야?",
          "speaker_id": "1",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016688.1.22",
          "form": "ㅎㅎ이제 오늘 지나면 한가해지겠지",
          "original_form": "ㅎㅎ이제 오늘 지나면 한가해지겠지",
          "speaker_id": "2",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016688.1.23",
          "form": "미치지 않고서야가 제일 재밌어!!",
          "original_form": "미치지 않고서야가 제일 재밌어!!",
          "speaker_id": "2",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016688.1.24",
          "form": "보는 사람이 너무 적어서 아쉬울 정도야ㅠ",
          "original_form": "보는 사람이 너무 적어서 아쉬울 정도야ㅠ",
          "speaker_id": "2",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016688.1.25",
          "form": "오 누구나와?",
          "original_form": "오 누구나와?",
          "speaker_id": "1",
          "time": "20210826 14:48"
        },
        {
          "id": "MDRW2100016688.1.26",
          "form": "문소리 정재영 이상엽 나오는데ㅋㅋ 리얼 오피스라이프야",
          "original_form": "문소리 정재영 이상엽 나오는데ㅋㅋ 리얼 오피스라이프야",
