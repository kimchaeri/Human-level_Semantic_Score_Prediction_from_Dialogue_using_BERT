        {
          "id": "MDRW2100033618.1.1",
          "form": "저녁 뭐 드싱거에요???",
          "original_form": "저녁 뭐 드싱거에요???",
          "speaker_id": "1",
          "time": "20210824 17:24"
        },
        {
          "id": "MDRW2100033618.1.2",
          "form": "저녁은 삼겹살입니다!",
          "original_form": "저녁은 삼겹살입니다!",
          "speaker_id": "2",
          "time": "20210824 17:25"
        },
        {
          "id": "MDRW2100033618.1.3",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100033618.1.4",
          "form": "삼겹",
          "original_form": "삼겹",
          "speaker_id": "1",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100033618.1.5",
          "form": "완전 탁월한 메뉴입니다",
          "original_form": "완전 탁월한 메뉴입니다",
          "speaker_id": "1",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100033618.1.6",
          "form": "name1님은 뭐드실거예요?",
          "original_form": "&name1&님은 뭐드실거예요?",
          "speaker_id": "2",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100033618.1.7",
          "form": "저 고민이에요 ㅠㅠㅠ",
          "original_form": "저 고민이에요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100033618.1.8",
          "form": "떡볶이 먹을지..",
          "original_form": "떡볶이 먹을지..",
          "speaker_id": "1",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100033618.1.9",
          "form": "먹고싶으면 드셔야죠",
          "original_form": "먹고싶으면 드셔야죠",
          "speaker_id": "2",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100033618.1.10",
          "form": "질러버리세요!",
          "original_form": "질러버리세요!",
          "speaker_id": "2",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100033618.1.11",
          "form": "ㅋㅋㅋ 떡볶이랑 마라탕 중 뭐가 나을까요?",
          "original_form": "ㅋㅋㅋ 떡볶이랑 마라탕 중 뭐가 나을까요?",
          "speaker_id": "1",
          "time": "20210824 17:29"
        },
        {
          "id": "MDRW2100033618.1.12",
          "form": "음 저는 마라탕이요",
          "original_form": "음 저는 마라탕이요",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100033618.1.13",
          "form": "선택 감사해요 ㅎ",
          "original_form": "선택 감사해요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100033618.1.14",
          "form": "은근 결정이 힘들거등요!",
          "original_form": "은근 결정이 힘들거등요!",
          "speaker_id": "1",
          "time": "20210824 17:37"
        },
        {
          "id": "MDRW2100033618.1.15",
          "form": "조금이라도 야채와 단백질이 다양한 마라탕이 땡겼어요ㅋㅋㅋㅋㅋ",
          "original_form": "조금이라도 야채와 단백질이 다양한 마라탕이 땡겼어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:38"
        },
        {
          "id": "MDRW2100033618.1.16",
          "form": "마라샹궈도 좋아하세요?",
          "original_form": "마라샹궈도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 17:39"
        },
        {
          "id": "MDRW2100033618.1.17",
          "form": "좋아하는데 안먹어봤어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "좋아하는데 안먹어봤어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100033618.1.18",
          "form": "마라탕하고 비슷한맛이죠??",
          "original_form": "마라탕하고 비슷한맛이죠??",
          "speaker_id": "1",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100033618.1.19",
          "form": "마라탕 볶음 버전이라고 들었어요",
          "original_form": "마라탕 볶음 버전이라고 들었어요",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100033618.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100033618.1.21",
          "form": "근데 전 탕이 더 좋아서요",
          "original_form": "근데 전 탕이 더 좋아서요",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100033618.1.22",
          "form": "하지만 궁금해요ㅋㅋㅋ",
          "original_form": "하지만 궁금해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100033618.1.23",
          "form": "친구여러명을 모아야겠군요ㅋㅋㅋㅋ",
          "original_form": "친구여러명을 모아야겠군요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:51"
        },
        {
          "id": "MDRW2100033618.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ 그래도ㅜ요즘은 개인적으로도 먹기 가능이니까",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그래도ㅜ요즘은 개인적으로도 먹기 가능이니까",
          "speaker_id": "1",
          "time": "20210824 17:51"
        },
        {
          "id": "MDRW2100033618.1.25",
          "form": "혼자서도 거뜬해요",
          "original_form": "혼자서도 거뜬해요",
