      "utterance": [
        {
          "id": "MDRW2100037464.1.1",
          "form": "더 자세히 보여주죠ㅋㅋ",
          "original_form": "더 자세히 보여주죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 00:26"
        },
        {
          "id": "MDRW2100037464.1.2",
          "form": "뇌가 움직이는거 봤던 기억이 있어요ㅋㅋ",
          "original_form": "뇌가 움직이는거 봤던 기억이 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 00:27"
        },
        {
          "id": "MDRW2100037464.1.3",
          "form": "네...?",
          "original_form": "네...?",
          "speaker_id": "1",
          "time": "20210901 12:13"
        },
        {
          "id": "MDRW2100037464.1.4",
          "form": "저는 아마 그거 보면 진쨔",
          "original_form": "저는 아마 그거 보면 진쨔",
          "speaker_id": "1",
          "time": "20210901 12:13"
        },
        {
          "id": "MDRW2100037464.1.5",
          "form": "3초만에 기절할걸요 ㅠ.ㅠ",
          "original_form": "3초만에 기절할걸요 ㅠ.ㅠ",
          "speaker_id": "1",
          "time": "20210901 12:13"
        },
        {
          "id": "MDRW2100037464.1.6",
          "form": "name2님 대단하시군요...",
          "original_form": "&name2&님 대단하시군요...",
          "speaker_id": "1",
          "time": "20210901 12:13"
        },
        {
          "id": "MDRW2100037464.1.7",
          "form": "칼싸움해서 다치고 이런 정도 영화는 보실수 있죠?",
          "original_form": "칼싸움해서 다치고 이런 정도 영화는 보실수 있죠?",
          "speaker_id": "2",
          "time": "20210901 13:19"
        },
        {
          "id": "MDRW2100037464.1.8",
          "form": "네네!! 전 내장만 아니면 괜찮아요",
          "original_form": "네네!! 전 내장만 아니면 괜찮아요",
          "speaker_id": "1",
          "time": "20210901 13:24"
        },
        {
          "id": "MDRW2100037464.1.9",
          "form": "킹덤은 또... 그럭저럭 잘 봤거단요",
          "original_form": "킹덤은 또... 그럭저럭 잘 봤거단요",
          "speaker_id": "1",
          "time": "20210901 13:24"
        },
        {
          "id": "MDRW2100037464.1.10",
          "form": "ㅎ.ㅎ",
          "original_form": "ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210901 13:24"
        },
        {
          "id": "MDRW2100037464.1.11",
          "form": "킹덤 재밌다는데 아직 못봤어요ㅠ",
          "original_form": "킹덤 재밌다는데 아직 못봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210901 13:31"
        },
        {
          "id": "MDRW2100037464.1.12",
          "form": "킹덤에 혹시 귀신같은거 나오는지요ㅠㅠ",
          "original_form": "킹덤에 혹시 귀신같은거 나오는지요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 13:31"
        },
        {
          "id": "MDRW2100037464.1.13",
          "form": "헉 아뇨!! 귀신은 안 나오고",
          "original_form": "헉 아뇨!! 귀신은 안 나오고",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100037464.1.14",
          "form": "좀비는.. 나와요!",
          "original_form": "좀비는.. 나와요!",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100037464.1.15",
          "form": "전 좀비는 잘봐요ㅎ 보기 꺼림칙하긴한데 진짜 판타지느낌이라서요ㅋㅋ",
          "original_form": "전 좀비는 잘봐요ㅎ 보기 꺼림칙하긴한데 진짜 판타지느낌이라서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:39"
        },
        {
          "id": "MDRW2100037464.1.16",
          "form": "귀신은 좀더 있음직하다 해야하나..",
          "original_form": "귀신은 좀더 있음직하다 해야하나..",
          "speaker_id": "2",
          "time": "20210901 14:40"
        },
        {
          "id": "MDRW2100037464.1.17",
          "form": "아 헛!!! 뭔지 알아요",
          "original_form": "아 헛!!! 뭔지 알아요",
          "speaker_id": "1",
          "time": "20210901 15:33"
        },
        {
          "id": "MDRW2100037464.1.18",
          "form": "저도 귀신 나오는 거 보면 괜히",
          "original_form": "저도 귀신 나오는 거 보면 괜히",
          "speaker_id": "1",
          "time": "20210901 15:33"
        },
        {
          "id": "MDRW2100037464.1.19",
          "form": "침대 밑 확인하고 자요",
          "original_form": "침대 밑 확인하고 자요",
          "speaker_id": "1",
          "time": "20210901 15:33"
        },
        {
          "id": "MDRW2100037464.1.20",
          "form": "전 확인도 못해요ㅋㅋㅋ 진짜 잇음 어쩌죠?ㅋㅋㅋㅋ",
          "original_form": "전 확인도 못해요ㅋㅋㅋ 진짜 잇음 어쩌죠?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:42"
        },
        {
          "id": "MDRW2100037464.1.21",
          "form": "어라라.. 거기까지는 생각 못했지만...",
          "original_form": "어라라.. 거기까지는 생각 못했지만...",
          "speaker_id": "1",
          "time": "20210901 16:29"
        },
        {
          "id": "MDRW2100037464.1.22",
          "form": "눈 마주치는 순간 전 기절할걸요...",
          "original_form": "눈 마주치는 순간 전 기절할걸요...",
          "speaker_id": "1",
          "time": "20210901 16:29"
        },
        {
          "id": "MDRW2100037464.1.23",
          "form": "ㅋㅋㅋ저도요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ저도요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100037464.1.24",
          "form": "그래도 공포물 잘볼수 있는거 부럽네요",
          "original_form": "그래도 공포물 잘볼수 있는거 부럽네요",
          "speaker_id": "2",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100037464.1.25",
          "form": "name2님 넷플릭스에서 그럼 뭘 제일 재밌게 보셨나요?@",
          "original_form": "&name2&님 넷플릭스에서 그럼 뭘 제일 재밌게 보셨나요?@",
          "speaker_id": "1",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100037464.1.26",
          "form": "전 미드 많이봐요 name1님은 미드보세요?",
          "original_form": "전 미드 많이봐요 &name1&님은 미드보세요?",
          "speaker_id": "2",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100037464.1.27",
          "form": "저 예전에 그 오뉴블?",
          "original_form": "저 예전에 그 오뉴블?",
          "speaker_id": "1",
          "time": "20210901 20:30"
