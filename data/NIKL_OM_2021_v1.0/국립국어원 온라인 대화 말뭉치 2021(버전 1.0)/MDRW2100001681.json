      "utterance": [
        {
          "id": "MDRW2100001681.1.1",
          "form": "집에 음료수가 없네~",
          "original_form": "집에 음료수가 없네~",
          "speaker_id": "1",
          "time": "20210819 18:01"
        },
        {
          "id": "MDRW2100001681.1.2",
          "form": "음료는 생수가 최고지!!",
          "original_form": "음료는 생수가 최고지!!",
          "speaker_id": "2",
          "time": "20210819 18:02"
        },
        {
          "id": "MDRW2100001681.1.3",
          "form": "탄산음료는 최악",
          "original_form": "탄산음료는 최악",
          "speaker_id": "2",
          "time": "20210819 18:02"
        },
        {
          "id": "MDRW2100001681.1.4",
          "form": "그렇나",
          "original_form": "그렇나",
          "speaker_id": "1",
          "time": "20210819 18:02"
        },
        {
          "id": "MDRW2100001681.1.5",
          "form": "난 제로콜라가 좋은뎅",
          "original_form": "난 제로콜라가 좋은뎅",
          "speaker_id": "1",
          "time": "20210819 18:03"
        },
        {
          "id": "MDRW2100001681.1.6",
          "form": "여자가 마시면 칼슘을 빼간다 ㅠ",
          "original_form": "여자가 마시면 칼슘을 빼간다 ㅠ",
          "speaker_id": "2",
          "time": "20210819 18:03"
        },
        {
          "id": "MDRW2100001681.1.7",
          "form": "홀 ㅠㅠ 괴담 아님?ㅋㅋ",
          "original_form": "홀 ㅠㅠ 괴담 아님?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:04"
        },
        {
          "id": "MDRW2100001681.1.8",
          "form": "머카누 ㅠ 따뜻한 물 한잔이 최고지",
          "original_form": "머카누 ㅠ 따뜻한 물 한잔이 최고지",
          "speaker_id": "2",
          "time": "20210819 18:05"
        },
        {
          "id": "MDRW2100001681.1.9",
          "form": "ㅠㅠ 물은 물배만 차는뎁..",
          "original_form": "ㅠㅠ 물은 물배만 차는뎁..",
          "speaker_id": "1",
          "time": "20210819 18:05"
        },
        {
          "id": "MDRW2100001681.1.10",
          "form": "우리몸이 70%가 수분이라 물이 부족하면 여러가지 질병이 온다",
          "original_form": "우리몸이 70%가 수분이라 물이 부족하면 여러가지 질병이 온다",
          "speaker_id": "2",
          "time": "20210819 18:06"
        },
        {
          "id": "MDRW2100001681.1.11",
          "form": "글쿠나",
          "original_form": "글쿠나",
          "speaker_id": "1",
          "time": "20210819 18:06"
        },
        {
          "id": "MDRW2100001681.1.12",
          "form": "물이 보약이지 일어나자마다 자기전엔 물이 최고여 !!",
          "original_form": "물이 보약이지 일어나자마다 자기전엔 물이 최고여 !!",
          "speaker_id": "2",
          "time": "20210819 18:07"
        },
        {
          "id": "MDRW2100001681.1.13",
          "form": "구래 물좀 많이 마셔야겠네",
          "original_form": "구래 물좀 많이 마셔야겠네",
          "speaker_id": "1",
          "time": "20210819 18:07"
        },
        {
          "id": "MDRW2100001681.1.14",
          "form": "물을 자주자주 마셔야지 건강해지지",
          "original_form": "물을 자주자주 마셔야지 건강해지지",
          "speaker_id": "2",
          "time": "20210819 18:08"
        },
        {
          "id": "MDRW2100001681.1.15",
