      "utterance": [
        {
          "id": "MDRW2100027567.1.1",
          "form": "요즘 읽고있는 책 있어?",
          "original_form": "요즘 읽고있는 책 있어?",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027567.1.2",
          "form": "최근에 강의 교재말고 읽었던 책이 없어. ...사실 강의 교재도 안 읽었어..",
          "original_form": "최근에 강의 교재말고 읽었던 책이 없어. ...사실 강의 교재도 안 읽었어..",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027567.1.3",
          "form": "넌 책 많이 읽니?",
          "original_form": "넌 책 많이 읽니?",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027567.1.4",
          "form": "난 주로 판소위주로 읽는 듯",
          "original_form": "난 주로 판소위주로 읽는 듯",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027567.1.5",
          "form": "웹소설",
          "original_form": "웹소설",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027567.1.6",
          "form": "판타지 소설? 그것도 책이야?",
          "original_form": "판타지 소설? 그것도 책이야?",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027567.1.7",
          "form": "아니 웹소설은ㅋㅋㅋㅋㅋㄱㄲ",
          "original_form": "아니 웹소설은ㅋㅋㅋㅋㅋㄱㄲ",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027567.1.8",
          "form": "웹소설도 도서긴 하니까",
          "original_form": "웹소설도 도서긴 하니까",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027567.1.9",
          "form": "웹소설은 책 아니지ㅋㅋㅋㅋㄱㅋ",
          "original_form": "웹소설은 책 아니지ㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027567.1.10",
          "form": "유흥거리에 더 가깝지 않나",
          "original_form": "유흥거리에 더 가깝지 않나",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027567.1.11",
          "form": "추리소설도 책인데",
          "original_form": "추리소설도 책인데",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027567.1.12",
          "form": "책넘버 찍히면 도서지",
          "original_form": "책넘버 찍히면 도서지",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027567.1.13",
          "form": "아 나 추리소설 개좋아함",
          "original_form": "아 나 추리소설 개좋아함",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027567.1.14",
          "form": "맞네",
          "original_form": "맞네",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027567.1.15",
          "form": "도서네.",
          "original_form": "도서네.",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027567.1.16",
          "form": "추리소설 인상깊게 읽은거있냐",
          "original_form": "추리소설 인상깊게 읽은거있냐",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027567.1.17",
          "form": "나 그나마 글씨 보일 때 마지막으로 읽었던 책이 셜록홈즈야",
          "original_form": "나 그나마 글씨 보일 때 마지막으로 읽었던 책이 셜록홈즈야",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027567.1.18",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027567.1.19",
          "form": "그게 마지막이 될줄은 몰랐어",
          "original_form": "그게 마지막이 될줄은 몰랐어",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027567.1.20",
          "form": "사실 셜록홈즈 초딩때 봐서",
          "original_form": "사실 셜록홈즈 초딩때 봐서",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027567.1.21",
          "form": "잘 기억안남",
          "original_form": "잘 기억안남",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027567.1.22",
          "form": "그거 시리즈인줄도 모르고 덜컥 6권부터 구매했는데",
          "original_form": "그거 시리즈인줄도 모르고 덜컥 6권부터 구매했는데",
          "speaker_id": "2",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100027567.1.23",
          "form": "아 진짜? 난 도서관에서 봤는데",
          "original_form": "아 진짜? 난 도서관에서 봤는데",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027567.1.24",
          "form": "재밌어서 전권 사버림",
          "original_form": "재밌어서 전권 사버림",
          "speaker_id": "2",
          "time": "20210902 13:40"
        }
      ]
    }
