      "utterance": [
        {
          "id": "MDRW2100033592.1.1",
          "form": "네! 제일 쏠쏠해서",
          "original_form": "네! 제일 쏠쏠해서",
          "speaker_id": "2",
          "time": "20210824 16:17"
        },
        {
          "id": "MDRW2100033592.1.2",
          "form": "이젠 돈은 무테크로 벌어요...",
          "original_form": "이젠 돈은 무테크로 벌어요...",
          "speaker_id": "2",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100033592.1.3",
          "form": "오 ㅋㅋ ㅋㅋㅋ 그거 하면 신기할 거 같아요",
          "original_form": "오 ㅋㅋ ㅋㅋㅋ 그거 하면 신기할 거 같아요",
          "speaker_id": "1",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100033592.1.4",
          "form": "마을 이름이 뭐에요?",
          "original_form": "마을 이름이 뭐에요?",
          "speaker_id": "1",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100033592.1.5",
          "form": "주섬주섬이요",
          "original_form": "주섬주섬이요",
          "speaker_id": "2",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100033592.1.6",
          "form": "오전가격 오후가격 코인마냥",
          "original_form": "오전가격 오후가격 코인마냥",
          "speaker_id": "2",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100033592.1.7",
          "form": "계속 확인해요",
          "original_form": "계속 확인해요",
          "speaker_id": "2",
          "time": "20210824 16:18"
        },
        {
          "id": "MDRW2100033592.1.8",
          "form": "아 주섬주섬ㅋㅋ",
          "original_form": "아 주섬주섬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100033592.1.9",
          "form": "귀엽네요",
          "original_form": "귀엽네요",
          "speaker_id": "1",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100033592.1.10",
          "form": "잘 지었어요",
          "original_form": "잘 지었어요",
          "speaker_id": "1",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100033592.1.11",
          "form": "닌텐도 있으세요?",
          "original_form": "닌텐도 있으세요?",
          "speaker_id": "2",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100033592.1.12",
          "form": "저 완전 완전 옛날꺼 알죠?",
          "original_form": "저 완전 완전 옛날꺼 알죠?",
          "speaker_id": "1",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100033592.1.13",
          "form": "초창기 그거 있어요ㅋㅋ",
          "original_form": "초창기 그거 있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100033592.1.14",
          "form": "닌텐도3ds 요?",
          "original_form": "닌텐도3ds 요?",
          "speaker_id": "2",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100033592.1.15",
          "form": "아마도요,,?",
          "original_form": "아마도요,,?",
          "speaker_id": "1",
          "time": "20210824 16:28"
        },
        {
          "id": "MDRW2100033592.1.16",
          "form": "초창기 동숲 가능항거요 ㄹ",
          "original_form": "초창기 동숲 가능항거요 ㄹ",
          "speaker_id": "1",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100033592.1.17",
          "form": "어떤게임 가지고 계셔요?",
          "original_form": "어떤게임 가지고 계셔요?",
          "speaker_id": "2",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100033592.1.18",
          "form": "동숲도 있고 그 포켓몬도 했었어요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "동숲도 있고 그 포켓몬도 했었어요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:29"
        },
        {
          "id": "MDRW2100033592.1.19",
          "form": "포켓몬좋아하시나요?",
          "original_form": "포켓몬좋아하시나요?",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100033592.1.20",
          "form": "저는 골드 라는 고전 명작만해봤어요",
          "original_form": "저는 골드 라는 고전 명작만해봤어요",
          "speaker_id": "2",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100033592.1.21",
          "form": "네 할때 좋아했어요ㅋㅋ",
          "original_form": "네 할때 좋아했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:30"
        },
        {
          "id": "MDRW2100033592.1.22",
          "form": "스타팅 무슨 포켓몬이였나요",
          "original_form": "스타팅 무슨 포켓몬이였나요",
          "speaker_id": "2",
          "time": "20210824 16:31"
        },
        {
          "id": "MDRW2100033592.1.23",
          "form": "물 풀 불 인가 중에 하나 고르는거요",
          "original_form": "물 풀 불 인가 중에 하나 고르는거요",
          "speaker_id": "2",
          "time": "20210824 16:31"
        },
        {
          "id": "MDRW2100033592.1.24",
          "form": "저는 물이요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 물이요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 16:32"
        },
        {
          "id": "MDRW2100033592.1.25",
          "form": "팽도리 키웠어요",
          "original_form": "팽도리 키웠어요",
          "speaker_id": "1",
          "time": "20210824 16:32"
        }
      ]
    }
