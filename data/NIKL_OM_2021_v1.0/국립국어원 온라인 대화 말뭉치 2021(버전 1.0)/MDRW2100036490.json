      "utterance": [
        {
          "id": "MDRW2100036490.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "1",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036490.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036490.1.3",
          "form": "name1님은 키우시나요?",
          "original_form": "&name1&님은 키우시나요?",
          "speaker_id": "2",
          "time": "20210830 23:50"
        },
        {
          "id": "MDRW2100036490.1.4",
          "form": "네! 고양이요!",
          "original_form": "네! 고양이요!",
          "speaker_id": "1",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100036490.1.5",
          "form": "고양이 좋아하세요?",
          "original_form": "고양이 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100036490.1.6",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036490.1.7",
          "form": "어떤 종인가요?",
          "original_form": "어떤 종인가요?",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100036490.1.8",
          "form": "아메숏이요!",
          "original_form": "아메숏이요!",
          "speaker_id": "1",
          "time": "20210831 00:00"
        },
        {
          "id": "MDRW2100036490.1.9",
          "form": "무늬가 진짜 이뻐요!",
          "original_form": "무늬가 진짜 이뻐요!",
          "speaker_id": "1",
          "time": "20210831 00:01"
        },
        {
          "id": "MDRW2100036490.1.10",
          "form": "사진으로 방금 봤는데 정말 귀엽네요",
          "original_form": "사진으로 방금 봤는데 정말 귀엽네요",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100036490.1.11",
          "form": "한마리키우나요?",
          "original_form": "한마리키우나요?",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100036490.1.12",
          "form": "그쵸!!",
          "original_form": "그쵸!!",
          "speaker_id": "1",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036490.1.13",
          "form": "알아주시니 기쁘네욥",
          "original_form": "알아주시니 기쁘네욥",
          "speaker_id": "1",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036490.1.14",
          "form": "2마리입니당!",
          "original_form": "2마리입니당!",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036490.1.15",
          "form": "동물 키우고 싶지않으세요?",
          "original_form": "동물 키우고 싶지않으세요?",
          "speaker_id": "1",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036490.1.16",
          "form": "저는 동물을 제가 잘 키울 자신이 없어서요ㅠㅠ",
          "original_form": "저는 동물을 제가 잘 키울 자신이 없어서요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036490.1.17",
          "form": "비용문제도 있고",
          "original_form": "비용문제도 있고",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100036490.1.18",
          "form": "비용 많이 들지 않나요?",
          "original_form": "비용 많이 들지 않나요?",
          "speaker_id": "2",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100036490.1.19",
          "form": "아ㅠ 그쵸 확실히 많이 들긴해요",
          "original_form": "아ㅠ 그쵸 확실히 많이 들긴해요",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036490.1.20",
          "form": "그래도 기쁨이랄까요?",
          "original_form": "그래도 기쁨이랄까요?",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036490.1.21",
          "form": "그게 더 크다보니 신경이 안쓰이더라구요",
          "original_form": "그게 더 크다보니 신경이 안쓰이더라구요",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100036490.1.22",
          "form": "못해줘서 미안하기만 합니다ㅠ",
          "original_form": "못해줘서 미안하기만 합니다ㅠ",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100036490.1.23",
          "form": "그마음 고양이들도 알고 있을거예요ㅠㅠ",
          "original_form": "그마음 고양이들도 알고 있을거예요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036490.1.24",
          "form": "근데 고양이들은 정말 산책 안시키나요??",
          "original_form": "근데 고양이들은 정말 산책 안시키나요??",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100036490.1.25",
          "form": "흡ㅜㅠ",
          "original_form": "흡ㅜㅠ",
          "speaker_id": "1",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036490.1.26",
          "form": "네 병원에 물어보니 진짜 몇백분의 일이라시더라구요",
          "original_form": "네 병원에 물어보니 진짜 몇백분의 일이라시더라구요",
          "speaker_id": "1",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036490.1.27",
          "form": "길냥이였던경우 제외하고요!",
          "original_form": "길냥이였던경우 제외하고요!",
          "speaker_id": "1",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100036490.1.28",
          "form": "그런유행으로 스트레스 받는애들이 많을거라시니 약간 생각하고있다가 바로 접었었어요",
          "original_form": "그런유행으로 스트레스 받는애들이 많을거라시니 약간 생각하고있다가 바로 접었었어요",
          "speaker_id": "1",
          "time": "20210831 00:15"
        },
        {
          "id": "MDRW2100036490.1.29",
          "form": "그럼 보통의 집냥이들은 집에서만 사는게 최선이군요?",
          "original_form": "그럼 보통의 집냥이들은 집에서만 사는게 최선이군요?",
          "speaker_id": "2",
          "time": "20210831 00:16"
        },
        {
          "id": "MDRW2100036490.1.30",
          "form": "그게 좋다더라구요",
          "original_form": "그게 좋다더라구요",
          "speaker_id": "1",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100036490.1.31",
          "form": "잘못했다가 놓치면 끝이라기도해서 무섭기도하구요ㅠ",
          "original_form": "잘못했다가 놓치면 끝이라기도해서 무섭기도하구요ㅠ",
          "speaker_id": "1",
          "time": "20210831 00:17"
        },
        {
          "id": "MDRW2100036490.1.32",
          "form": "오!!그렇군요",
          "original_form": "오!!그렇군요",
          "speaker_id": "2",
          "time": "20210831 00:18"
        }
      ]
    }
