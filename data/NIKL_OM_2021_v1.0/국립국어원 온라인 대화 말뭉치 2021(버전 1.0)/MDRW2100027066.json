      "utterance": [
        {
          "id": "MDRW2100027066.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 08:46"
        },
        {
          "id": "MDRW2100027066.1.2",
          "form": "안녕하세요 평소 운동하세요?",
          "original_form": "안녕하세요 평소 운동하세요?",
          "speaker_id": "2",
          "time": "20210902 08:47"
        },
        {
          "id": "MDRW2100027066.1.3",
          "form": "홈트 조금 하고 있어요",
          "original_form": "홈트 조금 하고 있어요",
          "speaker_id": "1",
          "time": "20210902 08:47"
        },
        {
          "id": "MDRW2100027066.1.4",
          "form": "홈트 영상 보고하세요?",
          "original_form": "홈트 영상 보고하세요?",
          "speaker_id": "2",
          "time": "20210902 09:15"
        },
        {
          "id": "MDRW2100027066.1.5",
          "form": "네ㅎㅎ 유튜브 보고 해요",
          "original_form": "네ㅎㅎ 유튜브 보고 해요",
          "speaker_id": "1",
          "time": "20210902 09:17"
        },
        {
          "id": "MDRW2100027066.1.6",
          "form": "오누구보고 하세용? 전 땅끄부부요 ㅋㅋㅋ",
          "original_form": "오누구보고 하세용? 전 땅끄부부요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:28"
        },
        {
          "id": "MDRW2100027066.1.7",
          "form": "저는 빅씨쓰요",
          "original_form": "저는 빅씨쓰요",
          "speaker_id": "1",
          "time": "20210902 09:29"
        },
        {
          "id": "MDRW2100027066.1.8",
          "form": "운동 시간 얼마정도하세요??",
          "original_form": "운동 시간 얼마정도하세요??",
          "speaker_id": "2",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100027066.1.9",
          "form": "하루에 30분씩만 하고 있어요",
          "original_form": "하루에 30분씩만 하고 있어요",
          "speaker_id": "1",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027066.1.10",
          "form": "그것도 대단하시네요",
          "original_form": "그것도 대단하시네요",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027066.1.11",
          "form": "챙겨드시는 건강식품은 있으세요?",
          "original_form": "챙겨드시는 건강식품은 있으세요?",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100027066.1.12",
          "form": "따로 없어요",
          "original_form": "따로 없어요",
          "speaker_id": "1",
          "time": "20210902 10:15"
        },
        {
          "id": "MDRW2100027066.1.13",
          "form": "저번에 비타민이 동나고 안샀어요",
          "original_form": "저번에 비타민이 동나고 안샀어요",
          "speaker_id": "1",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100027066.1.14",
          "form": "저도 유산균만 먹고있어요",
          "original_form": "저도 유산균만 먹고있어요",
          "speaker_id": "2",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100027066.1.15",
          "form": "화장실만 잘가면 돼요ㅠ",
          "original_form": "화장실만 잘가면 돼요ㅠ",
          "speaker_id": "2",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100027066.1.16",
          "form": "유산균 챙겨드시는 분이 많군요",
          "original_form": "유산균 챙겨드시는 분이 많군요",
          "speaker_id": "1",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100027066.1.17",
          "form": "아무래도 부담이 덜해서 그런가봐요",
          "original_form": "아무래도 부담이 덜해서 그런가봐요",
          "speaker_id": "2",
          "time": "20210902 11:48"
        },
        {
          "id": "MDRW2100027066.1.18",
          "form": "백신은 예약하셨어요?",
          "original_form": "백신은 예약하셨어요?",
          "speaker_id": "2",
          "time": "20210902 11:48"
        },
        {
          "id": "MDRW2100027066.1.19",
          "form": "아니요 좀 나중에 맞기로 했어요",
          "original_form": "아니요 좀 나중에 맞기로 했어요",
          "speaker_id": "1",
          "time": "20210902 11:49"
        },
        {
          "id": "MDRW2100027066.1.20",
          "form": "개강해버려서 맞기 애매하더라구요",
          "original_form": "개강해버려서 맞기 애매하더라구요",
          "speaker_id": "1",
          "time": "20210902 11:49"
        },
        {
          "id": "MDRW2100027066.1.21",
          "form": "아~ 개강하면 애매하죠ㅠ",
          "original_form": "아~ 개강하면 애매하죠ㅠ",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100027066.1.22",
          "form": "중간고사 끝나고가 좋을거 같아서",
          "original_form": "중간고사 끝나고가 좋을거 같아서",
          "speaker_id": "1",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100027066.1.23",
          "form": "잔여백신만 봅니다",
          "original_form": "잔여백신만 봅니다",
          "speaker_id": "1",
          "time": "20210902 17:12"
        },
        {
          "id": "MDRW2100027066.1.24",
          "form": "ㅎㅎ 좋습니다ㅜㅠ얼른 맞고 자유의 몸이 되십시다 ㅎㅎ",
          "original_form": "ㅎㅎ 좋습니다ㅜㅠ얼른 맞고 자유의 몸이 되십시다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 17:17"
        },
        {
          "id": "MDRW2100027066.1.25",
          "form": "좋아요ㅠㅠ",
          "original_form": "좋아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 17:21"
        },
        {
          "id": "MDRW2100027066.1.26",
          "form": "ㅎㅎ 컨디션 안좋으심 꼭 미루세요ㅠㅜ 몸아픕니다",
          "original_form": "ㅎㅎ 컨디션 안좋으심 꼭 미루세요ㅠㅜ 몸아픕니다",
          "speaker_id": "2",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100027066.1.27",
          "form": "그럼 그때도 한번 지켜봐야겠네요",
          "original_form": "그럼 그때도 한번 지켜봐야겠네요",
          "speaker_id": "1",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100027066.1.28",
          "form": "조언 감사합니다",
          "original_form": "조언 감사합니다",
          "speaker_id": "1",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100027066.1.29",
          "form": "아니에요 ㅎㅎ ㅠㅠ 제가 아팠어서오ㅠ",
          "original_form": "아니에요 ㅎㅎ ㅠㅠ 제가 아팠어서오ㅠ",
          "speaker_id": "2",
          "time": "20210902 17:49"
        }
      ]
