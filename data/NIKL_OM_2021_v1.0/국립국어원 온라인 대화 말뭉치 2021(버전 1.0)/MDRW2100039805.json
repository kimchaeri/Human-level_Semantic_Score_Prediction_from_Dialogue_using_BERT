      "utterance": [
        {
          "id": "MDRW2100039805.1.1",
          "form": "만능채칼같은거 사고싶은데",
          "original_form": "만능채칼같은거 사고싶은데",
          "speaker_id": "1",
          "time": "20210903 22:02"
        },
        {
          "id": "MDRW2100039805.1.2",
          "form": "요리 똥손이 도구탓할꺼같고 ㅎㅎㅎ",
          "original_form": "요리 똥손이 도구탓할꺼같고 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:02"
        },
        {
          "id": "MDRW2100039805.1.3",
          "form": "똥손이라 요리도 많이 안할것같으니",
          "original_form": "똥손이라 요리도 많이 안할것같으니",
          "speaker_id": "1",
          "time": "20210903 22:03"
        },
        {
          "id": "MDRW2100039805.1.4",
          "form": "곰돌이 브이채칼 눈여겨보고 있어여ㅋㅋ",
          "original_form": "곰돌이 브이채칼 눈여겨보고 있어여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:03"
        },
        {
          "id": "MDRW2100039805.1.5",
          "form": "도구들 사는거 자제 하고 있어요 ㅎ",
          "original_form": "도구들 사는거 자제 하고 있어요 ㅎ",
          "speaker_id": "1",
          "time": "20210903 22:03"
        },
        {
          "id": "MDRW2100039805.1.6",
          "form": "곰돌이 다지기 있어요 ㅎㅎㅎ",
          "original_form": "곰돌이 다지기 있어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:03"
        },
        {
          "id": "MDRW2100039805.1.7",
          "form": "요리는 장비빨이죠",
          "original_form": "요리는 장비빨이죠",
          "speaker_id": "2",
          "time": "20210903 22:03"
        },
        {
          "id": "MDRW2100039805.1.8",
          "form": "아기일때 이유식한다고 사놓고는",
          "original_form": "아기일때 이유식한다고 사놓고는",
          "speaker_id": "1",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100039805.1.9",
          "form": "결국엔 이유식 배달을 했다느용 ㅎ",
          "original_form": "결국엔 이유식 배달을 했다느용 ㅎ",
          "speaker_id": "1",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100039805.1.10",
          "form": "아ㅋㅋ 배달이 편하긴하져",
          "original_form": "아ㅋㅋ 배달이 편하긴하져",
          "speaker_id": "2",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100039805.1.11",
          "form": "그뒤로 장비는 자제 ㅎㅎㅎ",
          "original_form": "그뒤로 장비는 자제 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:04"
        },
        {
          "id": "MDRW2100039805.1.12",
          "form": "집에 돌체구스토도 있었는데",
          "original_form": "집에 돌체구스토도 있었는데",
          "speaker_id": "1",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039805.1.13",
          "form": "음 역시 몇번쓰다 말까요",
          "original_form": "음 역시 몇번쓰다 말까요",
          "speaker_id": "2",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039805.1.14",
          "form": "당근에 보냈어요 ㅎㅎㅎ",
          "original_form": "당근에 보냈어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039805.1.15",
          "form": "사보고 안쓰면 당근에 보내면 되죠 ㅎㅎㅎ",
          "original_form": "사보고 안쓰면 당근에 보내면 되죠 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:05"
        },
        {
          "id": "MDRW2100039805.1.16",
          "form": "팔기도 쉽지않더라고요",
          "original_form": "팔기도 쉽지않더라고요",
          "speaker_id": "2",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039805.1.17",
          "form": "그래도 채칼은 인기 있지 않나요???",
          "original_form": "그래도 채칼은 인기 있지 않나요???",
          "speaker_id": "1",
          "time": "20210903 22:06"
        },
        {
          "id": "MDRW2100039805.1.18",
          "form": "그럴까여 \r\n그럼아예 중고로 사보는것도 갠찮을수 잇겟네요",
          "original_form": "그럴까여 \r\n그럼아예 중고로 사보는것도 갠찮을수 잇겟네요",
          "speaker_id": "2",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039805.1.19",
          "form": "굳굳!!!",
          "original_form": "굳굳!!!",
          "speaker_id": "1",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039805.1.20",
          "form": "그게 더 좋겠네요",
