      "utterance": [
        {
          "id": "MDRW2100033053.1.1",
          "form": "밑창이 너무 다 닳긴 했더라 ㅠ",
          "original_form": "밑창이 너무 다 닳긴 했더라 ㅠ",
          "speaker_id": "2",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100033053.1.2",
          "form": "ㅇㅇ 비 올 때 물 샘 ㅠㅠ",
          "original_form": "ㅇㅇ 비 올 때 물 샘 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100033053.1.3",
          "form": "아궁.. 그럼 미리 말하지",
          "original_form": "아궁.. 그럼 미리 말하지",
          "speaker_id": "2",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100033053.1.4",
          "form": "괜찮아 해뜬 날은 신을만 해 ㅎㅎ",
          "original_form": "괜찮아 해뜬 날은 신을만 해 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 14:46"
        },
        {
          "id": "MDRW2100033053.1.5",
          "form": "훌.. 빨리 바꿔야겠다",
          "original_form": "훌.. 빨리 바꿔야겠다",
          "speaker_id": "2",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100033053.1.6",
          "form": "원하는 브랜드 있어?",
          "original_form": "원하는 브랜드 있어?",
          "speaker_id": "2",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100033053.1.7",
          "form": "지금 신고 있는 언더아머가 좋은데 편하고",
          "original_form": "지금 신고 있는 언더아머가 좋은데 편하고",
          "speaker_id": "1",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100033053.1.8",
          "form": "디자인이 깔끔하긴 하더라",
          "original_form": "디자인이 깔끔하긴 하더라",
          "speaker_id": "2",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100033053.1.9",
          "form": "약간 타이트하게 나와서 발을 꽉 잡아주는 느낌이야",
          "original_form": "약간 타이트하게 나와서 발을 꽉 잡아주는 느낌이야",
          "speaker_id": "1",
          "time": "20210906 14:47"
        },
        {
          "id": "MDRW2100033053.1.10",
          "form": "ㅇㅇ 그럼 언더아머에서 찾아볼께",
          "original_form": "ㅇㅇ 그럼 언더아머에서 찾아볼께",
          "speaker_id": "2",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100033053.1.11",
          "form": "고마워 ㅎㅎ",
          "original_form": "고마워 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100033053.1.12",
          "form": "두 켤레 살까봐 갈아신게",
          "original_form": "두 켤레 살까봐 갈아신게",
          "speaker_id": "2",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100033053.1.13",
          "form": "그래주면 너무 고맙고 ㅎㅎ",
          "original_form": "그래주면 너무 고맙고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 14:48"
        },
        {
          "id": "MDRW2100033053.1.14",
          "form": "ㅇㅇ 찾아서 링크 보내 줄께요",
          "original_form": "ㅇㅇ 찾아서 링크 보내 줄께요",
          "speaker_id": "2",
          "time": "20210906 14:49"
        },
        {
          "id": "MDRW2100033053.1.15",
          "form": "고마워~ 새 신발 곧 생기겠다 ㅎㅎ",
          "original_form": "고마워~ 새 신발 곧 생기겠다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 14:50"
        },
        {
          "id": "MDRW2100033053.1.16",
          "form": "가족 추석 선물로 어떤 거 하는게 좋을까?",
