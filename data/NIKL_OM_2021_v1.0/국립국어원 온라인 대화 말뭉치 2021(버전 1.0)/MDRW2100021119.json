      "utterance": [
        {
          "id": "MDRW2100021119.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210828 16:01"
        },
        {
          "id": "MDRW2100021119.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100021119.1.3",
          "form": "오늘 오랜만에 날씨가 맑아요",
          "original_form": "오늘 오랜만에 날씨가 맑아요",
          "speaker_id": "1",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100021119.1.4",
          "form": "그쵸~ 여기도 해가 쨍쨍인거같아요!",
          "original_form": "그쵸~ 여기도 해가 쨍쨍인거같아요!",
          "speaker_id": "2",
          "time": "20210828 16:03"
        },
        {
          "id": "MDRW2100021119.1.5",
          "form": "이렇게 쨍쨍한게 얼마만인지 모르겠어요",
          "original_form": "이렇게 쨍쨍한게 얼마만인지 모르겠어요",
          "speaker_id": "1",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100021119.1.6",
          "form": "계속 흐리고 비왔던터라..",
          "original_form": "계속 흐리고 비왔던터라..",
          "speaker_id": "1",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100021119.1.7",
          "form": "그러게요 ! 오전에 집에서 전 덥더라구요ㅠㅠ",
          "original_form": "그러게요 ! 오전에 집에서 전 덥더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 16:06"
        },
        {
          "id": "MDRW2100021119.1.8",
          "form": "요즘 가만히 있으면 괜찮고, 많이 움직이면 덥고 그렇더라구요",
          "original_form": "요즘 가만히 있으면 괜찮고, 많이 움직이면 덥고 그렇더라구요",
          "speaker_id": "1",
          "time": "20210828 16:07"
        },
        {
          "id": "MDRW2100021119.1.9",
          "form": "맞아요ㅠㅠ 밥먹을때 음식이 뜨거우니 더웠어요ㅠ",
          "original_form": "맞아요ㅠㅠ 밥먹을때 음식이 뜨거우니 더웠어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:13"
        },
        {
          "id": "MDRW2100021119.1.10",
          "form": "날이좋아도 나가기도그렇고~ 전 요번주는 집콕이에요ㅠ",
          "original_form": "날이좋아도 나가기도그렇고~ 전 요번주는 집콕이에요ㅠ",
          "speaker_id": "2",
          "time": "20210828 16:13"
        },
        {
          "id": "MDRW2100021119.1.11",
          "form": "저도 집콕이에요ㅎㅎ",
          "original_form": "저도 집콕이에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 16:15"
        },
        {
          "id": "MDRW2100021119.1.12",
          "form": "아직 뜨거운음식 먹기에는 좀 더운듯 해요",
          "original_form": "아직 뜨거운음식 먹기에는 좀 더운듯 해요",
          "speaker_id": "1",
          "time": "20210828 16:16"
        },
        {
          "id": "MDRW2100021119.1.13",
          "form": "날씨가좋으니 아이들뛰노는소리가들리네요 ㅎ 집에서 공원이보인는데사람도 많고~",
          "original_form": "날씨가좋으니 아이들뛰노는소리가들리네요 ㅎ 집에서 공원이보인는데사람도 많고~",
          "speaker_id": "2",
          "time": "20210828 16:54"
        },
        {
          "id": "MDRW2100021119.1.14",
          "form": "공원이랑 집이랑 거리가 가까우시군요!",
          "original_form": "공원이랑 집이랑 거리가 가까우시군요!",
          "speaker_id": "1",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100021119.1.15",
          "form": "네 ! 아파트3개아파트사이에 공원이있어요~ 횡단보도를 하나건너긴해도 앞동이랑 보이네요~이른아침부터 트랙돌면서 운동하는분들도보여요 ㅎ작게 ㅎㅎ",
          "original_form": "네 ! 아파트3개아파트사이에 공원이있어요~ 횡단보도를 하나건너긴해도 앞동이랑 보이네요~이른아침부터 트랙돌면서 운동하는분들도보여요 ㅎ작게 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021119.1.16",
          "form": "역시 날씨가 좋으니 나가고 싶은 마음이 드는건 같은 마음인가봐요",
          "original_form": "역시 날씨가 좋으니 나가고 싶은 마음이 드는건 같은 마음인가봐요",
          "speaker_id": "1",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021119.1.17",
          "form": "아이들이 날좋을면 외부활동하면 좋은데 ㅜ 진짜 작년부터 날씨가좋아도 나쁘도 걱정이에요",
          "original_form": "아이들이 날좋을면 외부활동하면 좋은데 ㅜ 진짜 작년부터 날씨가좋아도 나쁘도 걱정이에요",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021119.1.18",
          "form": "그쵸 요즘은 날씨가 좋아도 나가는게 걱정되죠ㅜㅜ",
          "original_form": "그쵸 요즘은 날씨가 좋아도 나가는게 걱정되죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021119.1.19",
          "form": "맞아요 진짜 여름휴가도안갔는데",
          "original_form": "맞아요 진짜 여름휴가도안갔는데",
          "speaker_id": "2",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100021119.1.20",
          "form": "다들가긴가는거같아요 ㅠ",
          "original_form": "다들가긴가는거같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100021119.1.21",
          "form": "그러니까요!",
          "original_form": "그러니까요!",
          "speaker_id": "1",
          "time": "20210828 18:08"
        }
      ]
    }
