      "utterance": [
        {
          "id": "MDRW2100040660.1.1",
          "form": "바나나킥 생각햇는데ㅣㅣ",
          "original_form": "바나나킥 생각햇는데ㅣㅣ",
          "speaker_id": "2",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100040660.1.2",
          "form": "큰거있고 작은거있다그래서ㅜㅜㅜㅜ",
          "original_form": "큰거있고 작은거있다그래서ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100040660.1.3",
          "form": "이미 버스는떠났어",
          "original_form": "이미 버스는떠났어",
          "speaker_id": "1",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100040660.1.4",
          "form": "세상은 냉정하다구",
          "original_form": "세상은 냉정하다구",
          "speaker_id": "1",
          "time": "20210904 23:45"
        },
        {
          "id": "MDRW2100040660.1.5",
          "form": "난 한사이즈밖에 몰라가꼬ㅜㅜㅜ아쉬워",
          "original_form": "난 한사이즈밖에 몰라가꼬ㅜㅜㅜ아쉬워",
          "speaker_id": "2",
          "time": "20210904 23:45"
        },
        {
          "id": "MDRW2100040660.1.6",
          "form": "나시작이얔ㅋㅋ",
          "original_form": "나시작이얔ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:46"
        },
        {
          "id": "MDRW2100040660.1.7",
          "form": "봉지과자야 상자과자야?",
          "original_form": "봉지과자야 상자과자야?",
          "speaker_id": "1",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100040660.1.8",
          "form": "상자야!",
          "original_form": "상자야!",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100040660.1.9",
          "form": "초코가들어있어?",
          "original_form": "초코가들어있어?",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100040660.1.10",
          "form": "응들어있어!",
          "original_form": "응들어있어!",
          "speaker_id": "2",
          "time": "20210904 23:49"
        },
        {
          "id": "MDRW2100040660.1.11",
          "form": "원형이야?",
          "original_form": "원형이야?",
          "speaker_id": "1",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100040660.1.12",
          "form": "ㅇㅇ원형이야",
          "original_form": "ㅇㅇ원형이야",
          "speaker_id": "2",
          "time": "20210904 23:50"
        },
        {
          "id": "MDRW2100040660.1.13",
          "form": "안에 뭐들어있니?",
          "original_form": "안에 뭐들어있니?",
          "speaker_id": "1",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100040660.1.14",
          "form": "아니!",
          "original_form": "아니!",
          "speaker_id": "2",
          "time": "20210904 23:52"
        },
        {
          "id": "MDRW2100040660.1.15",
          "form": "헐 뭐지 ㅋㅋㅋㅋㅋㅋ 어려워졌다 ㅋㅋㅋ초코인데 안에뭐가안들어있다고??ㅋㅋ",
          "original_form": "헐 뭐지 ㅋㅋㅋㅋㅋㅋ 어려워졌다 ㅋㅋㅋ초코인데 안에뭐가안들어있다고??ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:54"
        },
        {
          "id": "MDRW2100040660.1.16",
          "form": "초코파이나 몽쉘이 아니란거잖아 ㅋㅋㅋ",
          "original_form": "초코파이나 몽쉘이 아니란거잖아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:54"
        },
        {
          "id": "MDRW2100040660.1.17",
          "form": "오뜨는 동그랗지않다구! ㅋㅋㅋ",
          "original_form": "오뜨는 동그랗지않다구! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:54"
        },
        {
          "id": "MDRW2100040660.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아웃겨ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아웃겨ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:54"
        },
        {
          "id": "MDRW2100040660.1.19",
          "form": "아 내가 전혀가닥을 잘못잡고있는기분인데 ㅋㅋㅋ",
          "original_form": "아 내가 전혀가닥을 잘못잡고있는기분인데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:55"
        },
        {
          "id": "MDRW2100040660.1.20",
          "form": "ㅋㅋㅋㅋ얼른해얼른",
          "original_form": "ㅋㅋㅋㅋ얼른해얼른",
          "speaker_id": "2",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100040660.1.21",
          "form": "바삭바삭해?",
          "original_form": "바삭바삭해?",
          "speaker_id": "1",
          "time": "20210904 23:59"
        },
        {
          "id": "MDRW2100040660.1.22",
          "form": "어.......바삭바삭해!",
          "original_form": "어.......바삭바삭해!",
          "speaker_id": "2",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100040660.1.23",
          "form": "개별포장되어있니?",
          "original_form": "개별포장되어있니?",
          "speaker_id": "1",
          "time": "20210905 00:00"
        },
        {
          "id": "MDRW2100040660.1.24",
          "form": "어!개별포장이야",
          "original_form": "어!개별포장이야",
          "speaker_id": "2",
          "time": "20210905 00:03"
        },
        {
          "id": "MDRW2100040660.1.25",
          "form": "헐 코코볼이아니야?? ㅋㅋㅋㅋㅋ잠깐 생각해보자   ...",
          "original_form": "헐 코코볼이아니야?? ㅋㅋㅋㅋㅋ잠깐 생각해보자   ...",
          "speaker_id": "1",
          "time": "20210905 00:04"
        },
        {
          "id": "MDRW2100040660.1.26",
          "form": "아니 그런과자가 있어ㅡ ㅋㅋㅋㅋㅋ 개별포장인데 안에뭐가 안들었다고?? ㅋㅋㅋㅋ",
          "original_form": "아니 그런과자가 있어ㅡ ㅋㅋㅋㅋㅋ 개별포장인데 안에뭐가 안들었다고?? ㅋㅋㅋㅋ",
