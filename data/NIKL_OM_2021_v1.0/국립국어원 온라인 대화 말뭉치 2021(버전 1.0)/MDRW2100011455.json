        {
          "id": "MDRW2100011455.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210824 12:48"
        },
        {
          "id": "MDRW2100011455.1.2",
          "form": "날씨가 흐리흐리해서 국수 먹었어요",
          "original_form": "날씨가 흐리흐리해서 국수 먹었어요",
          "speaker_id": "2",
          "time": "20210824 12:48"
        },
        {
          "id": "MDRW2100011455.1.3",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210824 12:48"
        },
        {
          "id": "MDRW2100011455.1.4",
          "form": "한식이요",
          "original_form": "한식이요",
          "speaker_id": "1",
          "time": "20210824 12:49"
        },
        {
          "id": "MDRW2100011455.1.5",
          "form": "식사하셨으니 커피한잔의 여유 타임 즐기세요 ㅎㅎ",
          "original_form": "식사하셨으니 커피한잔의 여유 타임 즐기세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 12:50"
        },
        {
          "id": "MDRW2100011455.1.6",
          "form": "그러고ㅜ있어요",
          "original_form": "그러고ㅜ있어요",
          "speaker_id": "1",
          "time": "20210824 12:51"
        },
        {
          "id": "MDRW2100011455.1.7",
          "form": "핫 or 아이스?",
          "original_form": "핫 or 아이스?",
          "speaker_id": "2",
          "time": "20210824 13:00"
        },
        {
          "id": "MDRW2100011455.1.8",
          "form": "아이스죠",
          "original_form": "아이스죠",
          "speaker_id": "1",
          "time": "20210824 20:36"
        },
        {
          "id": "MDRW2100011455.1.9",
          "form": "저도 겨울에도 아이스 ㅎㅎ",
          "original_form": "저도 겨울에도 아이스 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:36"
        },
        {
          "id": "MDRW2100011455.1.10",
          "form": "얼죽아죠",
          "original_form": "얼죽아죠",
          "speaker_id": "1",
          "time": "20210824 20:42"
        },
        {
          "id": "MDRW2100011455.1.11",
          "form": "전 우유들어가면 텁텁해져서 아메리카노만 마셔요",
          "original_form": "전 우유들어가면 텁텁해져서 아메리카노만 마셔요",
          "speaker_id": "2",
          "time": "20210824 20:44"
        },
        {
          "id": "MDRW2100011455.1.12",
          "form": "저도 아메요",
          "original_form": "저도 아메요",
          "speaker_id": "1",
