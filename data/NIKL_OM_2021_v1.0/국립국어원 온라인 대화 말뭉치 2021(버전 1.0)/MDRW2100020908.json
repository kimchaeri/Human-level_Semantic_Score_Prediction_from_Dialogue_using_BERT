        {
          "id": "MDRW2100020908.1.1",
          "form": "개학한 소감은?",
          "original_form": "개학한 소감은?",
          "speaker_id": "1",
          "time": "20210828 14:03"
        },
        {
          "id": "MDRW2100020908.1.2",
          "form": "아직 안했는데 별로네요",
          "original_form": "아직 안했는데 별로네요",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020908.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020908.1.4",
          "form": "ㅋㅋㅋ저는 직장인이랍니다 ㅜㅜ",
          "original_form": "ㅋㅋㅋ저는 직장인이랍니다 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020908.1.5",
          "form": "언제 개학해요?",
          "original_form": "언제 개학해요?",
          "speaker_id": "1",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020908.1.6",
          "form": "다음주에요",
          "original_form": "다음주에요",
          "speaker_id": "2",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100020908.1.7",
          "form": "9월 1일에 하나봐요?",
          "original_form": "9월 1일에 하나봐요?",
          "speaker_id": "1",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100020908.1.8",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100020908.1.9",
          "form": "그러면 개학이 아니라 개강 아니에요?",
          "original_form": "그러면 개학이 아니라 개강 아니에요?",
          "speaker_id": "1",
          "time": "20210828 15:55"
        },
        {
          "id": "MDRW2100020908.1.10",
          "form": "네 대학생이에요",
          "original_form": "네 대학생이에요",
          "speaker_id": "2",
          "time": "20210828 15:59"
        },
        {
          "id": "MDRW2100020908.1.11",
          "form": "수강신청 잘했어요?",
          "original_form": "수강신청 잘했어요?",
          "speaker_id": "1",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100020908.1.12",
          "form": "성공?",
          "original_form": "성공?",
          "speaker_id": "1",
          "time": "20210828 16:02"
        },
        {
          "id": "MDRW2100020908.1.13",
          "form": "네ㅋㅋ 올클했어요",
          "original_form": "네ㅋㅋ 올클했어요",
          "speaker_id": "2",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100020908.1.14",
          "form": "와 축하드려요 ㅎㅎ",
          "original_form": "와 축하드려요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100020908.1.15",
          "form": "항상 올클 하세요?",
          "original_form": "항상 올클 하세요?",
          "speaker_id": "1",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100020908.1.16",
          "form": "네 용병 써서 같이 하거든요",
          "original_form": "네 용병 써서 같이 하거든요",
          "speaker_id": "2",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100020908.1.17",
          "form": "용병은 어떻게 쓰는거에요?",
          "original_form": "용병은 어떻게 쓰는거에요?",
          "speaker_id": "1",
          "time": "20210828 20:20"
        },
        {
          "id": "MDRW2100020908.1.18",
          "form": "같은 아이디로 접속해서 수강신청하는거예요",
