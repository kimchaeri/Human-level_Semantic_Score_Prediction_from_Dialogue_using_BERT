      "utterance": [
        {
          "id": "MDRW2100046205.1.1",
          "form": "느낌이 안좋길래",
          "original_form": "느낌이 안좋길래",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046205.1.2",
          "form": "사진 좀 찍어달라했더니",
          "original_form": "사진 좀 찍어달라했더니",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046205.1.3",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046205.1.4",
          "form": "당당하게 중나사진 그대로더라구요",
          "original_form": "당당하게 중나사진 그대로더라구요",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046205.1.5",
          "form": "와 ㅋㅋㅋㅋ",
          "original_form": "와 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100046205.1.6",
          "form": "뻔뻔해요",
          "original_form": "뻔뻔해요",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046205.1.7",
          "form": "그래서 먼가 이상하다싶어서",
          "original_form": "그래서 먼가 이상하다싶어서",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046205.1.8",
          "form": "그때또",
          "original_form": "그때또",
          "speaker_id": "1",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100046205.1.9",
          "form": "아이들을 목욕탕에 넣어놯거든요",
          "original_form": "아이들을 목욕탕에 넣어놯거든요",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046205.1.10",
          "form": "근데 아이들이 샤워젤로 온 욕실을",
          "original_form": "근데 아이들이 샤워젤로 온 욕실을",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046205.1.11",
          "form": "난장판을 ㅜㅜ",
          "original_form": "난장판을 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046205.1.12",
          "form": "아이코야",
          "original_form": "아이코야",
          "speaker_id": "2",
          "time": "20210908 11:12"
        },
        {
          "id": "MDRW2100046205.1.13",
          "form": "그래서 그거 수습하고나서",
          "original_form": "그래서 그거 수습하고나서",
          "speaker_id": "1",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046205.1.14",
          "form": "두배로 정신없으셨겠네요",
          "original_form": "두배로 정신없으셨겠네요",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046205.1.15",
          "form": "중나가서 글찾으니",
          "original_form": "중나가서 글찾으니",
          "speaker_id": "1",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046205.1.16",
          "form": "글이 삭제",
          "original_form": "글이 삭제",
          "speaker_id": "1",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046205.1.17",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046205.1.18",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046205.1.19",
          "form": "백퍼사기꾼이구낭 싶었죠",
          "original_form": "백퍼사기꾼이구낭 싶었죠",
          "speaker_id": "1",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046205.1.20",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046205.1.21",
          "form": "돈안보내길천만다행 ㅎ",
          "original_form": "돈안보내길천만다행 ㅎ",
          "speaker_id": "1",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046205.1.22",
          "form": "입금안해서 다행이에요",
          "original_form": "입금안해서 다행이에요",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046205.1.23",
          "form": "네 정말요",
          "original_form": "네 정말요",
          "speaker_id": "2",
          "time": "20210908 11:13"
        },
        {
          "id": "MDRW2100046205.1.24",
          "form": "25만원정도였는데",
          "original_form": "25만원정도였는데",
          "speaker_id": "1",
          "time": "20210908 11:14"
        },
        {
          "id": "MDRW2100046205.1.25",
          "form": "애기용품을 사기치다니",
          "original_form": "애기용품을 사기치다니",
