      "utterance": [
        {
          "id": "MDRW2100018301.1.1",
          "form": "name3는 잘 자나요?",
          "original_form": "&name3&는 잘 자나요?",
          "speaker_id": "1",
          "time": "20210827 00:17"
        },
        {
          "id": "MDRW2100018301.1.2",
          "form": "name3 아침부터 난리입니다",
          "original_form": "&name3& 아침부터 난리입니다",
          "speaker_id": "2",
          "time": "20210827 07:51"
        },
        {
          "id": "MDRW2100018301.1.3",
          "form": "ㅋㅋ name3 왜요:)\r\n아침부터 또 똥꼬발랄해요?",
          "original_form": "ㅋㅋ &name3& 왜요:)\r\n아침부터 또 똥꼬발랄해요?",
          "speaker_id": "1",
          "time": "20210827 09:19"
        },
        {
          "id": "MDRW2100018301.1.4",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "2",
          "time": "20210827 09:31"
        },
        {
          "id": "MDRW2100018301.1.5",
          "form": "일가지말라고 난리입니더",
          "original_form": "일가지말라고 난리입니더",
          "speaker_id": "2",
          "time": "20210827 09:31"
        },
        {
          "id": "MDRW2100018301.1.6",
          "form": "밥먹더니 힘나가지고",
          "original_form": "밥먹더니 힘나가지고",
          "speaker_id": "2",
          "time": "20210827 09:31"
        },
        {
          "id": "MDRW2100018301.1.7",
          "form": "앜ㅋㅋㅋㅋ 너무 귀여웡\r\nname3가 문앞에서 막아서던가요?  ㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ 너무 귀여웡\r\n&name3&가 문앞에서 막아서던가요?  ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 09:40"
        },
        {
          "id": "MDRW2100018301.1.8",
          "form": "칸막이가 되어있어서 ㅋㅋ",
          "original_form": "칸막이가 되어있어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 09:40"
        },
        {
          "id": "MDRW2100018301.1.9",
          "form": "거기앞에서 계속 짖엉..ㅜ",
          "original_form": "거기앞에서 계속 짖엉..ㅜ",
          "speaker_id": "2",
          "time": "20210827 09:41"
        },
        {
          "id": "MDRW2100018301.1.10",
          "form": "name4랑 name3때문에\r\n문있는데다가 안전문\r\n설치해뒀나보당ㅋㅋ",
          "original_form": "&name4&랑 &name3&때문에\r\n문있는데다가 안전문\r\n설치해뒀나보당ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 09:48"
        },
        {
          "id": "MDRW2100018301.1.11",
          "form": "잉 그렇게 짖고 있음\r\n두고 나오는 맴이 그렇지ㅜㅜ",
          "original_form": "잉 그렇게 짖고 있음\r\n두고 나오는 맴이 그렇지ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 09:48"
        },
        {
          "id": "MDRW2100018301.1.12",
          "form": "글춍 ㅠㅠㅠ",
          "original_form": "글춍 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 10:07"
        },
        {
          "id": "MDRW2100018301.1.13",
          "form": "그래도 내걍이있어가지고",
          "original_form": "그래도 내걍이있어가지고",
          "speaker_id": "2",
          "time": "20210827 10:07"
        },
        {
          "id": "MDRW2100018301.1.14",
          "form": "괜차나 조금",
          "original_form": "괜차나 조금",
          "speaker_id": "2",
          "time": "20210827 10:07"
        },
        {
          "id": "MDRW2100018301.1.15",
          "form": "뭐야ㅋㅋ\r\nname5이 있는데도\r\nname3가 가지말라했오? ㅋㅋ",
          "original_form": "뭐야ㅋㅋ\r\n&name5&이 있는데도\r\n&name3&가 가지말라했오? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:12"
        },
        {
          "id": "MDRW2100018301.1.16",
          "form": "name5이는 name4랑 따로 방에있어가지고 ㅋㅋㅋ",
          "original_form": "&name5&이는 &name4&랑 따로 방에있어가지고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:17"
        },
        {
          "id": "MDRW2100018301.1.17",
          "form": "아ㅠㅠ 그랬구낭\r\nname4랑 name3랑 친해지면\r\n다들 나가더라도\r\n좀 덜 심심하고 좋을텐뎅",
          "original_form": "아ㅠㅠ 그랬구낭\r\n&name4&랑 &name3&랑 친해지면\r\n다들 나가더라도\r\n좀 덜 심심하고 좋을텐뎅",
          "speaker_id": "1",
          "time": "20210827 10:25"
        },
        {
          "id": "MDRW2100018301.1.18",
          "form": "name4야 마음의 문을 열어죠ㅜㅜㅋ",
          "original_form": "&name4&야 마음의 문을 열어죠ㅜㅜㅋ",
          "speaker_id": "1",
          "time": "20210827 10:25"
        },
        {
          "id": "MDRW2100018301.1.19",
