      "utterance": [
        {
          "id": "MDRW2100031883.1.1",
          "form": "와이프는",
          "original_form": "와이프는",
          "speaker_id": "2",
          "time": "20210905 07:25"
        },
        {
          "id": "MDRW2100031883.1.2",
          "form": "그렇게 요즘에는 집안에 하는 사람 많다고 하더라고요",
          "original_form": "그렇게 요즘에는 집안에 하는 사람 많다고 하더라고요",
          "speaker_id": "1",
          "time": "20210905 07:25"
        },
        {
          "id": "MDRW2100031883.1.3",
          "form": "쉴틈이 없어요 ㅋㅋ",
          "original_form": "쉴틈이 없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:25"
        },
        {
          "id": "MDRW2100031883.1.4",
          "form": "일주일에 동료랑  술한잔하기 어렵구요",
          "original_form": "일주일에 동료랑  술한잔하기 어렵구요",
          "speaker_id": "2",
          "time": "20210905 07:26"
        },
        {
          "id": "MDRW2100031883.1.5",
          "form": "맞아요 그러면 좀더 집안일 많이 하세요",
          "original_form": "맞아요 그러면 좀더 집안일 많이 하세요",
          "speaker_id": "1",
          "time": "20210905 07:26"
        },
        {
          "id": "MDRW2100031883.1.6",
          "form": "무서워요",
          "original_form": "무서워요",
          "speaker_id": "2",
          "time": "20210905 07:26"
        },
        {
          "id": "MDRW2100031883.1.7",
          "form": "한번하면",
          "original_form": "한번하면",
          "speaker_id": "2",
          "time": "20210905 07:26"
        },
        {
          "id": "MDRW2100031883.1.8",
          "form": "열번한것처럼",
          "original_form": "열번한것처럼",
          "speaker_id": "2",
          "time": "20210905 07:26"
        },
        {
          "id": "MDRW2100031883.1.9",
          "form": "뭐라하니까 ㅋㅋ",
          "original_form": "뭐라하니까 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:26"
        },
        {
          "id": "MDRW2100031883.1.10",
          "form": "맨날 술묵는다그러고 ㅠ",
          "original_form": "맨날 술묵는다그러고 ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:26"
        },
        {
          "id": "MDRW2100031883.1.11",
          "form": "그래서 솔직히 한 사람이 돈 벌고 한 사람이 집에서 일하고",
          "original_form": "그래서 솔직히 한 사람이 돈 벌고 한 사람이 집에서 일하고",
          "speaker_id": "1",
          "time": "20210905 07:26"
        },
        {
          "id": "MDRW2100031883.1.12",
          "form": "들들 볶입니다",
          "original_form": "들들 볶입니다",
          "speaker_id": "2",
          "time": "20210905 07:26"
        },
        {
          "id": "MDRW2100031883.1.13",
          "form": "그게 애한테도 좋은 거 같거든요",
          "original_form": "그게 애한테도 좋은 거 같거든요",
          "speaker_id": "1",
          "time": "20210905 07:26"
        },
        {
          "id": "MDRW2100031883.1.14",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210905 07:27"
        },
        {
          "id": "MDRW2100031883.1.15",
          "form": "24개월까지는",
          "original_form": "24개월까지는",
          "speaker_id": "2",
          "time": "20210905 07:27"
        },
        {
          "id": "MDRW2100031883.1.16",
          "form": "그래야됩니다",
          "original_form": "그래야됩니다",
          "speaker_id": "2",
          "time": "20210905 07:27"
        },
        {
          "id": "MDRW2100031883.1.17",
          "form": "내년에는",
          "original_form": "내년에는",
          "speaker_id": "2",
          "time": "20210905 07:27"
        },
        {
          "id": "MDRW2100031883.1.18",
          "form": "진짜 이론상으로는 남자 가장도 있고 여자 가장도 있고",
          "original_form": "진짜 이론상으로는 남자 가장도 있고 여자 가장도 있고",
          "speaker_id": "1",
          "time": "20210905 07:27"
        },
        {
          "id": "MDRW2100031883.1.19",
          "form": "남자 주부도 있고 여자 주부도 있고",
          "original_form": "남자 주부도 있고 여자 주부도 있고",
          "speaker_id": "1",
          "time": "20210905 07:27"
        },
        {
          "id": "MDRW2100031883.1.20",
          "form": "여자가장이면 너무 좋죠",
          "original_form": "여자가장이면 너무 좋죠",
          "speaker_id": "2",
          "time": "20210905 07:27"
        },
        {
          "id": "MDRW2100031883.1.21",
          "form": "그러면 좋겠어요",
          "original_form": "그러면 좋겠어요",
          "speaker_id": "1",
          "time": "20210905 07:27"
        },
        {
          "id": "MDRW2100031883.1.22",
          "form": "월 500만되면",
          "original_form": "월 500만되면",
          "speaker_id": "2",
          "time": "20210905 07:27"
        },
        {
          "id": "MDRW2100031883.1.23",
          "form": "만약에 남자가 주부라고 해도 그게 쪽팔린게 아닌데",
          "original_form": "만약에 남자가 주부라고 해도 그게 쪽팔린게 아닌데",
          "speaker_id": "1",
          "time": "20210905 07:27"
        },
        {
          "id": "MDRW2100031883.1.24",
          "form": "하나도 안쪽팔려요 ㅋㅋ",
          "original_form": "하나도 안쪽팔려요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:27"
        },
        {
          "id": "MDRW2100031883.1.25",
          "form": "다들 집에서 일하는 거 폄하하니까",
          "original_form": "다들 집에서 일하는 거 폄하하니까",
          "speaker_id": "1",
          "time": "20210905 07:28"
        },
        {
          "id": "MDRW2100031883.1.26",
          "form": "잘할 자신오 있는데",
          "original_form": "잘할 자신오 있는데",
          "speaker_id": "2",
          "time": "20210905 07:28"
        },
        {
          "id": "MDRW2100031883.1.27",
          "form": "실제로 어머니 교실 같은데 나가면 남자는 못낀데요",
          "original_form": "실제로 어머니 교실 같은데 나가면 남자는 못낀데요",
          "speaker_id": "1",
          "time": "20210905 07:28"
        },
        {
          "id": "MDRW2100031883.1.28",
          "form": "저 잘할자신있는데 ㅋㅋ",
          "original_form": "저 잘할자신있는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:28"
        },
        {
          "id": "MDRW2100031883.1.29",
          "form": "학부모교실이겠죠;;;",
          "original_form": "학부모교실이겠죠;;;",
          "speaker_id": "1",
          "time": "20210905 07:28"
        },
        {
          "id": "MDRW2100031883.1.30",
          "form": "바뀌지 않을까요?",
          "original_form": "바뀌지 않을까요?",
