      "utterance": [
        {
          "id": "MDRW2100001817.1.1",
          "form": "헐 스포츠 좋아하세요?",
          "original_form": "헐 스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 21:23"
        },
        {
          "id": "MDRW2100001817.1.2",
          "form": "저는 관심이 없었는데 이번에 배구에 빠져서 코보리그 갈 생각입니다!",
          "original_form": "저는 관심이 없었는데 이번에 배구에 빠져서 코보리그 갈 생각입니다!",
          "speaker_id": "2",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001817.1.3",
          "form": "어떤 스포츠 좋아하세요?",
          "original_form": "어떤 스포츠 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 21:24"
        },
        {
          "id": "MDRW2100001817.1.4",
          "form": "저는 야구경기 보는거 좋아해요!!",
          "original_form": "저는 야구경기 보는거 좋아해요!!",
          "speaker_id": "1",
          "time": "20210819 21:27"
        },
        {
          "id": "MDRW2100001817.1.5",
          "form": "그래서 이번 올림픽도 기대하면서 봤는데..",
          "original_form": "그래서 이번 올림픽도 기대하면서 봤는데..",
          "speaker_id": "1",
          "time": "20210819 21:29"
        },
        {
          "id": "MDRW2100001817.1.6",
          "form": "대참사.. ㅠㅠ",
          "original_form": "대참사.. ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 21:29"
        },
        {
          "id": "MDRW2100001817.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ앜ㅋㅋㅋ저 미국이랑 한국 야구할때 삼겹살 먹고있었는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ앜ㅋㅋㅋ저 미국이랑 한국 야구할때 삼겹살 먹고있었는데",
          "speaker_id": "2",
          "time": "20210819 21:30"
        },
        {
          "id": "MDRW2100001817.1.8",
          "form": "응원하고있렀는데 삼겹살집 사장님이",
          "original_form": "응원하고있렀는데 삼겹살집 사장님이",
          "speaker_id": "2",
          "time": "20210819 21:30"
        },
        {
          "id": "MDRW2100001817.1.9",
          "form": "이길거라고 생각하는건 아니죠..?이래서 속상해ㅡㅏ더라구요",
          "original_form": "이길거라고 생각하는건 아니죠..?이래서 속상해ㅡㅏ더라구요",
          "speaker_id": "2",
          "time": "20210819 21:30"
        },
        {
          "id": "MDRW2100001817.1.10",
          "form": "네네 ㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:30"
        },
        {
          "id": "MDRW2100001817.1.11",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:30"
        },
        {
          "id": "MDRW2100001817.1.12",
          "form": "너무 슬픈데..",
          "original_form": "너무 슬픈데..",
          "speaker_id": "1",
          "time": "20210819 21:30"
        },
        {
          "id": "MDRW2100001817.1.13",
          "form": "전 기대 했는데..",
          "original_form": "전 기대 했는데..",
          "speaker_id": "1",
          "time": "20210819 21:30"
        },
        {
          "id": "MDRW2100001817.1.14",
          "form": "혹시나 했는데..",
          "original_form": "혹시나 했는데..",
          "speaker_id": "1",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001817.1.15",
          "form": "지더라구요..",
          "original_form": "지더라구요..",
          "speaker_id": "1",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001817.1.16",
          "form": "그럼 경기 직접 관람하신적 있으세요?",
          "original_form": "그럼 경기 직접 관람하신적 있으세요?",
          "speaker_id": "1",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001817.1.17",
          "form": "경기장 가서",
          "original_form": "경기장 가서",
          "speaker_id": "1",
          "time": "20210819 21:31"
        },
        {
          "id": "MDRW2100001817.1.18",
          "form": "네! 미국인 친구가 한국 놀러왔을 때",
          "original_form": "네! 미국인 친구가 한국 놀러왔을 때",
          "speaker_id": "2",
          "time": "20210819 21:33"
        },
        {
          "id": "MDRW2100001817.1.19",
          "form": "수원 데려가서 야구 봣어요!",
          "original_form": "수원 데려가서 야구 봣어요!",
          "speaker_id": "2",
          "time": "20210819 21:33"
        },
        {
          "id": "MDRW2100001817.1.20",
          "form": "와우 대박 미국인 친구..",
          "original_form": "와우 대박 미국인 친구..",
          "speaker_id": "1",
          "time": "20210819 21:33"
        },
        {
          "id": "MDRW2100001817.1.21",
          "form": "재밌더라구요 나중에 막 같이 욕하고",
          "original_form": "재밌더라구요 나중에 막 같이 욕하고",
          "speaker_id": "2",
          "time": "20210819 21:33"
        },
        {
          "id": "MDRW2100001817.1.22",
          "form": "ㅋㅋㅋ 뭐 드셨어요?",
          "original_form": "ㅋㅋㅋ 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100001817.1.23",
          "form": "전 야구 보면서 치킨 먹었거든요",
          "original_form": "전 야구 보면서 치킨 먹었거든요",
          "speaker_id": "1",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100001817.1.24",
          "form": "진짜 꿀맛!!!",
          "original_form": "진짜 꿀맛!!!",
          "speaker_id": "1",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100001817.1.25",
          "form": "저도 맥주에 치킨이요! 거기에 맥주 파는 사람도 있더라구요",
          "original_form": "저도 맥주에 치킨이요! 거기에 맥주 파는 사람도 있더라구요",
          "speaker_id": "2",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001817.1.26",
          "form": "야구 직관중에 가장 추천하는 좌석이 어딜ㅋ가요?",
          "original_form": "야구 직관중에 가장 추천하는 좌석이 어딜ㅋ가요?",
          "speaker_id": "2",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001817.1.27",
          "form": "헐 맥주.. 전 그때 미성년자라.. 못먹었어요",
          "original_form": "헐 맥주.. 전 그때 미성년자라.. 못먹었어요",
          "speaker_id": "1",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001817.1.28",
          "form": "직관 자리... 저도 많이는 안봤는데",
          "original_form": "직관 자리... 저도 많이는 안봤는데",
          "speaker_id": "1",
          "time": "20210819 21:37"
        },
        {
          "id": "MDRW2100001817.1.29",
          "form": "적당히 뒷자리가 좋더라구요",
          "original_form": "적당히 뒷자리가 좋더라구요",
          "speaker_id": "1",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100001817.1.30",
          "form": "왔다갔다 편하구",
          "original_form": "왔다갔다 편하구",
          "speaker_id": "1",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100001817.1.31",
          "form": "먹을거 많이 사러 나갔거든요 ㅎㅎ",
          "original_form": "먹을거 많이 사러 나갔거든요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100001817.1.32",
          "form": "헉 미성년자때부터 직관 다니셨구나 재밋었겠어요 엄청",
          "original_form": "헉 미성년자때부터 직관 다니셨구나 재밋었겠어요 엄청",
          "speaker_id": "2",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001817.1.33",
          "form": "야구는 어떤 팀 응원하세요?",
          "original_form": "야구는 어떤 팀 응원하세요?",
          "speaker_id": "2",
          "time": "20210819 21:39"
        },
        {
          "id": "MDRW2100001817.1.34",
          "form": "미자부터 다닌게 아니라..",
          "original_form": "미자부터 다닌게 아니라..",
          "speaker_id": "1",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001817.1.35",
          "form": "작년까지 미자였어요..",
          "original_form": "작년까지 미자였어요..",
          "speaker_id": "1",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001817.1.36",
          "form": "ㅋㅋㅋㅋㅎㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅎㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001817.1.37",
          "form": "야구는 기아 응원합니다!",
          "original_form": "야구는 기아 응원합니다!",
          "speaker_id": "1",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100001817.1.38",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ죄송해요 와 대박 20살 부럽다",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ죄송해요 와 대박 20살 부럽다",
