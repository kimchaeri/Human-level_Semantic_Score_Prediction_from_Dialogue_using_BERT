      "utterance": [
        {
          "id": "MDRW2100040765.1.1",
          "form": "1시간씼는친구도있더라구요",
          "original_form": "1시간씼는친구도있더라구요",
          "speaker_id": "2",
          "time": "20210905 04:02"
        },
        {
          "id": "MDRW2100040765.1.2",
          "form": "전 10분이긴하지만...",
          "original_form": "전 10분이긴하지만...{emoji:☆}",
          "speaker_id": "2",
          "time": "20210905 04:02"
        },
        {
          "id": "MDRW2100040765.1.3",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 04:02"
        },
        {
          "id": "MDRW2100040765.1.4",
          "form": "지금 머리 말리는 중입니다!",
          "original_form": "지금 머리 말리는 중입니다!",
          "speaker_id": "1",
          "time": "20210905 04:03"
        },
        {
          "id": "MDRW2100040765.1.5",
          "form": "name1님 백신맞으셨나용",
          "original_form": "&name1&님 백신맞으셨나용",
          "speaker_id": "2",
          "time": "20210905 04:03"
        },
        {
          "id": "MDRW2100040765.1.6",
          "form": "전9일에맞는답니다ㅠ",
          "original_form": "전9일에맞는답니다ㅠ",
          "speaker_id": "2",
          "time": "20210905 04:03"
        },
        {
          "id": "MDRW2100040765.1.7",
          "form": "네네 저 1차만 맞았어요!",
          "original_form": "네네 저 1차만 맞았어요!",
          "speaker_id": "1",
          "time": "20210905 04:03"
        },
        {
          "id": "MDRW2100040765.1.8",
          "form": "무섭네요옹",
          "original_form": "무섭네요옹",
          "speaker_id": "2",
          "time": "20210905 04:03"
        },
        {
          "id": "MDRW2100040765.1.9",
          "form": "10월 8일이 2차예요",
          "original_form": "10월 8일이 2차예요",
          "speaker_id": "1",
          "time": "20210905 04:03"
        },
        {
          "id": "MDRW2100040765.1.10",
          "form": "허거걱 어떠셨나요",
          "original_form": "허거걱 어떠셨나요",
          "speaker_id": "2",
          "time": "20210905 04:03"
        },
        {
          "id": "MDRW2100040765.1.11",
          "form": "헐ㄹ 빠르네용",
          "original_form": "헐ㄹ 빠르네용",
          "speaker_id": "2",
          "time": "20210905 04:03"
        },
        {
          "id": "MDRW2100040765.1.12",
          "form": "아 이제 1차 맞으시는건가요?",
          "original_form": "아 이제 1차 맞으시는건가요?",
          "speaker_id": "1",
          "time": "20210905 04:03"
        },
        {
          "id": "MDRW2100040765.1.13",
          "form": "저는 진짜",
          "original_form": "저는 진짜",
          "speaker_id": "1",
          "time": "20210905 04:03"
        },
        {
          "id": "MDRW2100040765.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210905 04:03"
        },
        {
          "id": "MDRW2100040765.1.15",
          "form": "아무렇지 않았어욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아무렇지 않았어욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 04:03"
        },
        {
          "id": "MDRW2100040765.1.16",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210905 04:03"
        },
        {
          "id": "MDRW2100040765.1.17",
          "form": "완전건강맨~",
          "original_form": "완전건강맨~",
          "speaker_id": "2",
          "time": "20210905 04:03"
        },
        {
          "id": "MDRW2100040765.1.18",
          "form": "앜ㅋㅋ",
          "original_form": "앜ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 04:04"
        },
        {
          "id": "MDRW2100040765.1.19",
          "form": "이게 들리는 소문에 의하면",
          "original_form": "이게 들리는 소문에 의하면",
          "speaker_id": "1",
          "time": "20210905 04:04"
        },
        {
          "id": "MDRW2100040765.1.20",
          "form": "평소에 운동 많이 하는 사람들이 아프다?",
          "original_form": "평소에 운동 많이 하는 사람들이 아프다?",
          "speaker_id": "1",
          "time": "20210905 04:04"
        },
        {
          "id": "MDRW2100040765.1.21",
          "form": "오....",
          "original_form": "오....",
          "speaker_id": "2",
          "time": "20210905 04:04"
        },
        {
          "id": "MDRW2100040765.1.22",
          "form": "그런 소리를 들었거든요... 근디",
          "original_form": "그런 소리를 들었거든요... 근디",
          "speaker_id": "1",
          "time": "20210905 04:04"
        },
        {
          "id": "MDRW2100040765.1.23",
          "form": "저는 허약맨,,,",
          "original_form": "저는 허약맨,,,",
          "speaker_id": "1",
          "time": "20210905 04:04"
        },
        {
          "id": "MDRW2100040765.1.24",
          "form": "뭔가 바이러스와 싸우는 대식세포가 일을 안해서",
          "original_form": "뭔가 바이러스와 싸우는 대식세포가 일을 안해서",
          "speaker_id": "2",
          "time": "20210905 04:04"
        },
        {
          "id": "MDRW2100040765.1.25",
          "form": "아닌가 사실 잘 모르겠네요,,",
          "original_form": "아닌가 사실 잘 모르겠네요,,",
          "speaker_id": "1",
          "time": "20210905 04:04"
        },
        {
          "id": "MDRW2100040765.1.26",
          "form": "안아픈거일수도있겠네요",
          "original_form": "안아픈거일수도있겠네요",
          "speaker_id": "2",
          "time": "20210905 04:04"
        },
        {
          "id": "MDRW2100040765.1.27",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 04:04"
        },
        {
          "id": "MDRW2100040765.1.28",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210905 04:04"
        },
        {
          "id": "MDRW2100040765.1.29",
          "form": "막 그런 이야기도 있고",
          "original_form": "막 그런 이야기도 있고",
          "speaker_id": "1",
          "time": "20210905 04:04"
        },
        {
          "id": "MDRW2100040765.1.30",
          "form": "허걱",
          "original_form": "허걱",
          "speaker_id": "2",
          "time": "20210905 04:04"
        },
        {
          "id": "MDRW2100040765.1.31",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "2",
          "time": "20210905 04:04"
        },
        {
          "id": "MDRW2100040765.1.32",
          "form": "아니면",
          "original_form": "아니면",
          "speaker_id": "1",
          "time": "20210905 04:04"
        },
        {
          "id": "MDRW2100040765.1.33",
          "form": "1차 맞고도 아팠던 사람들이",
          "original_form": "1차 맞고도 아팠던 사람들이",
          "speaker_id": "1",
          "time": "20210905 04:04"
        },
        {
          "id": "MDRW2100040765.1.34",
          "form": "옹",
          "original_form": "옹",
          "speaker_id": "2",
          "time": "20210905 04:05"
        },
        {
          "id": "MDRW2100040765.1.35",
          "form": "코로나에 걸린 경험이",
          "original_form": "코로나에 걸린 경험이",
          "speaker_id": "1",
          "time": "20210905 04:05"
        },
        {
          "id": "MDRW2100040765.1.36",
          "form": "있어서 그런거다",
          "original_form": "있어서 그런거다",
          "speaker_id": "1",
          "time": "20210905 04:05"
        },
        {
          "id": "MDRW2100040765.1.37",
          "form": "헉걱",
          "original_form": "헉걱",
          "speaker_id": "2",
          "time": "20210905 04:05"
        },
        {
          "id": "MDRW2100040765.1.38",
          "form": "그런 말도 있고....",
          "original_form": "그런 말도 있고....",
          "speaker_id": "1",
          "time": "20210905 04:05"
        },
        {
          "id": "MDRW2100040765.1.39",
          "form": "그으럴수도있겠네요",
          "original_form": "그으럴수도있겠네요",
          "speaker_id": "2",
          "time": "20210905 04:05"
        }
      ]
