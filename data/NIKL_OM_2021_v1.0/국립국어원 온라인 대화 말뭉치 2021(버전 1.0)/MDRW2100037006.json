      "utterance": [
        {
          "id": "MDRW2100037006.1.1",
          "form": "저녁뭐드실건가여",
          "original_form": "저녁뭐드실건가여",
          "speaker_id": "1",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100037006.1.2",
          "form": "막걸리한잔하고 자려구요",
          "original_form": "막걸리한잔하고 자려구요",
          "speaker_id": "2",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100037006.1.3",
          "form": "크으 안주는어떤걸루",
          "original_form": "크으 안주는어떤걸루",
          "speaker_id": "1",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100037006.1.4",
          "form": "걍 막걸리만으로",
          "original_form": "걍 막걸리만으로",
          "speaker_id": "2",
          "time": "20210831 17:08"
        },
        {
          "id": "MDRW2100037006.1.5",
          "form": "부침개는 귀찮아서",
          "original_form": "부침개는 귀찮아서",
          "speaker_id": "2",
          "time": "20210831 17:08"
        },
        {
          "id": "MDRW2100037006.1.6",
          "form": "아님 어제 온 벤토하나 뜯던지요",
          "original_form": "아님 어제 온 벤토하나 뜯던지요",
          "speaker_id": "2",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100037006.1.7",
          "form": "오 쌩 막걸리만으로",
          "original_form": "오 쌩 막걸리만으로",
          "speaker_id": "1",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100037006.1.8",
          "form": "술좀하시나봅니다",
          "original_form": "술좀하시나봅니다",
          "speaker_id": "1",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100037006.1.9",
          "form": "막걸리는 배불러서 ㅋㅋ",
          "original_form": "막걸리는 배불러서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100037006.1.10",
          "form": "주량이어떻게되시나요",
          "original_form": "주량이어떻게되시나요",
          "speaker_id": "1",
          "time": "20210831 17:09"
        },
        {
          "id": "MDRW2100037006.1.11",
          "form": "잘은 못마셔요 분위기따라",
          "original_form": "잘은 못마셔요 분위기따라",
          "speaker_id": "2",
          "time": "20210831 17:13"
        },
        {
          "id": "MDRW2100037006.1.12",
          "form": "왓다갔다",
          "original_form": "왓다갔다",
          "speaker_id": "2",
          "time": "20210831 17:13"
        },
        {
          "id": "MDRW2100037006.1.13",
          "form": "저도 그렇긴한데 전 딱 소주3잔",
          "original_form": "저도 그렇긴한데 전 딱 소주3잔",
          "speaker_id": "1",
          "time": "20210831 17:14"
        },
        {
          "id": "MDRW2100037006.1.14",
          "form": "잘마시면 4잔",
          "original_form": "잘마시면 4잔",
          "speaker_id": "1",
          "time": "20210831 17:14"
        },
        {
          "id": "MDRW2100037006.1.15",
          "form": "못마시면1잔이여",
          "original_form": "못마시면1잔이여",
          "speaker_id": "1",
          "time": "20210831 17:14"
        },
        {
          "id": "MDRW2100037006.1.16",
          "form": "전 처음부터 소주면 소주 맥주면 맥주 폭탄이면 폭탄",
          "original_form": "전 처음부터 소주면 소주 맥주면 맥주 폭탄이면 폭탄",
          "speaker_id": "2",
          "time": "20210831 17:15"
        },
        {
          "id": "MDRW2100037006.1.17",
          "form": "쭉 가야지 중간에 섞이면 답없어요",
          "original_form": "쭉 가야지 중간에 섞이면 답없어요",
          "speaker_id": "2",
          "time": "20210831 17:15"
        },
        {
          "id": "MDRW2100037006.1.18",
          "form": "오 ㅋㅋㅋㅋ뭔가 잘마실거같아요",
          "original_form": "오 ㅋㅋㅋㅋ뭔가 잘마실거같아요",
          "speaker_id": "1",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100037006.1.19",
          "form": "술 뭘 제일좋아하세요",
          "original_form": "술 뭘 제일좋아하세요",
          "speaker_id": "1",
          "time": "20210831 17:16"
        },
        {
          "id": "MDRW2100037006.1.20",
          "form": "요즘 주종은 맥주?",
          "original_form": "요즘 주종은 맥주?",
          "speaker_id": "2",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100037006.1.21",
          "form": "좋아하긴 막걸리요",
          "original_form": "좋아하긴 막걸리요",
          "speaker_id": "2",
          "time": "20210831 17:18"
        },
        {
          "id": "MDRW2100037006.1.22",
          "form": "맥주랑막걸리를 잘드시는군여",
          "original_form": "맥주랑막걸리를 잘드시는군여",
          "speaker_id": "1",
          "time": "20210831 17:19"
        }
      ]
