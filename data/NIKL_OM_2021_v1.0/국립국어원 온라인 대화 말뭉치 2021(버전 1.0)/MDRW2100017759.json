        },
        {
          "id": "MDRW2100017759.1.14",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "1",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100017759.1.15",
          "form": "연애때보다 결혼 후가 만족도가 더 높으신가요?",
          "original_form": "연애때보다 결혼 후가 만족도가 더 높으신가요?",
