      "utterance": [
        {
          "id": "MDRW2100025149.1.1",
          "form": "여행가고싶다",
          "original_form": "여행가고싶다",
          "speaker_id": "2",
          "time": "20210831 23:46"
        },
        {
          "id": "MDRW2100025149.1.2",
          "form": "코로나 때문에 여행 못가는게 너무 슬프다",
          "original_form": "코로나 때문에 여행 못가는게 너무 슬프다",
          "speaker_id": "1",
          "time": "20210831 23:46"
        },
        {
          "id": "MDRW2100025149.1.3",
          "form": "코로나 넘싫어~~~",
          "original_form": "코로나 넘싫어~~~",
          "speaker_id": "2",
          "time": "20210831 23:46"
        },
        {
          "id": "MDRW2100025149.1.4",
          "form": "외국 가고 싶었는데",
          "original_form": "외국 가고 싶었는데",
          "speaker_id": "1",
          "time": "20210831 23:46"
        },
        {
          "id": "MDRW2100025149.1.5",
          "form": "그러게 이제 못가잖아",
          "original_form": "그러게 이제 못가잖아",
          "speaker_id": "2",
          "time": "20210831 23:46"
        },
        {
          "id": "MDRW2100025149.1.6",
          "form": "제주도나 가자",
          "original_form": "제주도나 가자",
          "speaker_id": "2",
          "time": "20210831 23:46"
        },
        {
          "id": "MDRW2100025149.1.7",
          "form": "제주도 좋지",
          "original_form": "제주도 좋지",
          "speaker_id": "1",
          "time": "20210831 23:46"
        },
        {
          "id": "MDRW2100025149.1.8",
          "form": "수학여행으로 말고는 가본 적 없어",
          "original_form": "수학여행으로 말고는 가본 적 없어",
          "speaker_id": "1",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100025149.1.9",
          "form": "가서 회먹고오자",
          "original_form": "가서 회먹고오자",
          "speaker_id": "2",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100025149.1.10",
          "form": "오마카세 맛집알아왔어",
          "original_form": "오마카세 맛집알아왔어",
          "speaker_id": "2",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100025149.1.11",
          "form": "그거 먹고 둘레길 걷자",
          "original_form": "그거 먹고 둘레길 걷자",
          "speaker_id": "1",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100025149.1.12",
          "form": "제주도 풍경 보고 싶어",
          "original_form": "제주도 풍경 보고 싶어",
          "speaker_id": "1",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100025149.1.13",
          "form": "그래 좋지",
          "original_form": "그래 좋지",
          "speaker_id": "2",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100025149.1.14",
          "form": "자전거타니?",
          "original_form": "자전거타니?",
          "speaker_id": "2",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100025149.1.15",
          "form": "오 탈 수는 있어",
          "original_form": "오 탈 수는 있어",
          "speaker_id": "1",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100025149.1.16",
          "form": "엄청 비틀거리는데",
          "original_form": "엄청 비틀거리는데",
          "speaker_id": "1",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100025149.1.17",
          "form": "오 난 못타니까",
          "original_form": "오 난 못타니까",
          "speaker_id": "2",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100025149.1.18",
          "form": "너는 잘타?",
          "original_form": "너는 잘타?",
          "speaker_id": "1",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100025149.1.19",
          "form": "그냥 차타자",
          "original_form": "그냥 차타자",
          "speaker_id": "2",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100025149.1.20",
          "form": "왜 물어본건뎈ㅋㅋㅋㅋㅋㅋ",
          "original_form": "왜 물어본건뎈ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100025149.1.21",
          "form": "우리 둘다 면허있으니까",
          "original_form": "우리 둘다 면허있으니까",
          "speaker_id": "2",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100025149.1.22",
          "form": "스쿠터도 괜찮지 않을까??",
          "original_form": "스쿠터도 괜찮지 않을까??",
          "speaker_id": "1",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100025149.1.23",
          "form": "편한 여행이 될거같아",
          "original_form": "편한 여행이 될거같아",
          "speaker_id": "2",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100025149.1.24",
          "form": "우리 언니 스쿠터 빌려서 돌아다녔는데 좋았대!",
          "original_form": "우리 언니 스쿠터 빌려서 돌아다녔는데 좋았대!",
          "speaker_id": "1",
          "time": "20210831 23:49"
        },
        {
          "id": "MDRW2100025149.1.25",
          "form": "나쁘지않지",
          "original_form": "나쁘지않지",
          "speaker_id": "2",
          "time": "20210831 23:49"
        },
        {
          "id": "MDRW2100025149.1.26",
          "form": "괜찮은 계획인거같아",
          "original_form": "괜찮은 계획인거같아",
          "speaker_id": "2",
          "time": "20210831 23:49"
        }
      ]
    }
