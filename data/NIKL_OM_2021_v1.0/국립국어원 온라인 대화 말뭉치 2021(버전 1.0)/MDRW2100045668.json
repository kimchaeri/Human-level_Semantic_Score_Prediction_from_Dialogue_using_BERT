        {
          "id": "MDRW2100045668.1.2",
          "form": "너 당근마켓 해봤어?",
          "original_form": "너 당근마켓 해봤어?",
          "speaker_id": "1",
          "time": "20210908 05:03"
        },
        {
          "id": "MDRW2100045668.1.3",
          "form": "그거 요즘 유명하더라",
          "original_form": "그거 요즘 유명하더라",
          "speaker_id": "2",
          "time": "20210908 05:03"
        },
        {
          "id": "MDRW2100045668.1.4",
          "form": "중고거래 어플인데 해봤어?",
          "original_form": "중고거래 어플인데 해봤어?",
          "speaker_id": "1",
          "time": "20210908 05:03"
        },
        {
          "id": "MDRW2100045668.1.5",
          "form": "ㅋㅋ아니 안해봤어",
          "original_form": "ㅋㅋ아니 안해봤어",
          "speaker_id": "2",
          "time": "20210908 05:03"
        },
        {
          "id": "MDRW2100045668.1.6",
          "form": "나도 안했는데 해보려고",
          "original_form": "나도 안했는데 해보려고",
          "speaker_id": "1",
          "time": "20210908 05:03"
        },
        {
          "id": "MDRW2100045668.1.7",
          "form": "뭐 사려고그래?",
          "original_form": "뭐 사려고그래?",
          "speaker_id": "2",
          "time": "20210908 05:04"
        },
        {
          "id": "MDRW2100045668.1.8",
          "form": "나 화장대 사려고",
          "original_form": "나 화장대 사려고",
          "speaker_id": "1",
          "time": "20210908 05:04"
        },
        {
          "id": "MDRW2100045668.1.9",
          "form": "그거 동네마다 다르지",
          "original_form": "그거 동네마다 다르지",
          "speaker_id": "2",
          "time": "20210908 05:04"
        },
        {
          "id": "MDRW2100045668.1.10",
          "form": "응응 동네서 거래하는거",
          "original_form": "응응 동네서 거래하는거",
          "speaker_id": "1",
          "time": "20210908 05:04"
        },
        {
          "id": "MDRW2100045668.1.11",
          "form": "우리동네도 봐줄까?",
          "original_form": "우리동네도 봐줄까?",
          "speaker_id": "2",
          "time": "20210908 05:04"
        },
        {
          "id": "MDRW2100045668.1.12",
          "form": "응 너도 같이 봐주라",
          "original_form": "응 너도 같이 봐주라",
          "speaker_id": "1",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100045668.1.13",
          "form": "살거있는지 봐야겠다",
