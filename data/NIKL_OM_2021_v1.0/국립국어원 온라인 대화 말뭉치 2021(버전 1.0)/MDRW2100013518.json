      "utterance": [
        {
          "id": "MDRW2100013518.1.1",
          "form": "혹시 어떤 음식 좋아하세요?",
          "original_form": "혹시 어떤 음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013518.1.2",
          "form": "전 피자 좋아해요!",
          "original_form": "전 피자 좋아해요!",
          "speaker_id": "2",
          "time": "20210825 14:51"
        },
        {
          "id": "MDRW2100013518.1.3",
          "form": "어떤 피자 좋아하세요?",
          "original_form": "어떤 피자 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 14:53"
        },
        {
          "id": "MDRW2100013518.1.4",
          "form": "피자는 역시 포테이토 피자죠!",
          "original_form": "피자는 역시 포테이토 피자죠!",
          "speaker_id": "2",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100013518.1.5",
          "form": "저 어제 피자먹었는데",
          "original_form": "저 어제 피자먹었는데",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013518.1.6",
          "form": "부럽죠?",
          "original_form": "부럽죠?",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013518.1.7",
          "form": "ㅋㅋㅋ 너무 부럽다",
          "original_form": "ㅋㅋㅋ 너무 부럽다",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013518.1.8",
          "form": "무슨 피자 드셨나요ㅋㅋㅋ",
          "original_form": "무슨 피자 드셨나요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013518.1.9",
          "form": "치즈랑 페퍼로니 있는거였는데 이름이 기억안나네요",
          "original_form": "치즈랑 페퍼로니 있는거였는데 이름이 기억안나네요",
          "speaker_id": "1",
          "time": "20210825 16:19"
        },
        {
          "id": "MDRW2100013518.1.10",
          "form": "오 페퍼로니 맛있겠네요 어디 거 드셨어요?!",
          "original_form": "오 페퍼로니 맛있겠네요 어디 거 드셨어요?!",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100013518.1.11",
          "form": "치킨과 바람피자에서 시켰는데 좋았어요",
          "original_form": "치킨과 바람피자에서 시켰는데 좋았어요",
          "speaker_id": "1",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100013518.1.12",
          "form": "지금도 먹고싶네요",
          "original_form": "지금도 먹고싶네요",
          "speaker_id": "1",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100013518.1.13",
          "form": "오 처음 들어보는 브랜드인데 약간 피나치공 같은 건가요?",
          "original_form": "오 처음 들어보는 브랜드인데 약간 피나치공 같은 건가요?",
          "speaker_id": "2",
          "time": "20210825 17:28"
        },
        {
          "id": "MDRW2100013518.1.14",
          "form": "네 그거랑 비슷한거같아요",
          "original_form": "네 그거랑 비슷한거같아요",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100013518.1.15",
          "form": "혹시 라멘도 좋아하세요?",
          "original_form": "혹시 라멘도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100013518.1.16",
          "form": "아 라멘 좋죠! 집 근처에 없어서 못 먹어요ㅋㅋㅋ",
          "original_form": "아 라멘 좋죠! 집 근처에 없어서 못 먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100013518.1.17",
          "form": "어떤라멘 제일 좋아하세요?",
          "original_form": "어떤라멘 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100013518.1.18",
          "form": "전 그냥 일반라멘???  그거 제일 자주 먹는 거 같아요",
          "original_form": "전 그냥 일반라멘???  그거 제일 자주 먹는 거 같아요",
          "speaker_id": "2",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100013518.1.19",
          "form": "미소는 생각보다 별로고 매운 건 먹기 힘들더라구요",
          "original_form": "미소는 생각보다 별로고 매운 건 먹기 힘들더라구요",
          "speaker_id": "2",
          "time": "20210825 17:43"
        }
      ]
