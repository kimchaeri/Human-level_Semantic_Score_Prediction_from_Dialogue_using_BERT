      "utterance": [
        {
          "id": "MDRW2100032832.1.1",
          "form": "집놀이족ㅎㅎ",
          "original_form": "집놀이족ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 00:43"
        },
        {
          "id": "MDRW2100032832.1.2",
          "form": "대체.뭘 말하는 걸까요",
          "original_form": "대체.뭘 말하는 걸까요",
          "speaker_id": "1",
          "time": "20210906 00:43"
        },
        {
          "id": "MDRW2100032832.1.3",
          "form": "보석십자수?",
          "original_form": "보석십자수?",
          "speaker_id": "1",
          "time": "20210906 00:43"
        },
        {
          "id": "MDRW2100032832.1.4",
          "form": "감사요ㅎㅎ",
          "original_form": "감사요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 00:43"
        },
        {
          "id": "MDRW2100032832.1.5",
          "form": "십자수할줄아세뇨m?",
          "original_form": "십자수할줄아세뇨m?",
          "speaker_id": "2",
          "time": "20210906 00:43"
        },
        {
          "id": "MDRW2100032832.1.6",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210906 00:43"
        },
        {
          "id": "MDRW2100032832.1.7",
          "form": "그냥 집콕놀이아닐까뇨?",
          "original_form": "그냥 집콕놀이아닐까뇨?",
          "speaker_id": "2",
          "time": "20210906 00:43"
        },
        {
          "id": "MDRW2100032832.1.8",
          "form": "오 넷플릭스 같은게 떠오르네여",
          "original_form": "오 넷플릭스 같은게 떠오르네여",
          "speaker_id": "1",
          "time": "20210906 00:44"
        },
        {
          "id": "MDRW2100032832.1.9",
          "form": "집에서 영상보는더좋아하세요?",
          "original_form": "집에서 영상보는더좋아하세요?",
          "speaker_id": "2",
          "time": "20210906 00:44"
        },
        {
          "id": "MDRW2100032832.1.10",
          "form": "아참 혼자 사신다고하셨죠? 그럼 뭐하세요 평소에?",
          "original_form": "아참 혼자 사신다고하셨죠? 그럼 뭐하세요 평소에?",
          "speaker_id": "2",
          "time": "20210906 00:44"
        },
        {
          "id": "MDRW2100032832.1.11",
          "form": "명상 요리 그리고 제주 여행 좋아해여",
          "original_form": "명상 요리 그리고 제주 여행 좋아해여",
          "speaker_id": "1",
          "time": "20210906 00:45"
        },
        {
          "id": "MDRW2100032832.1.12",
          "form": "오름가거나 해변가거나",
          "original_form": "오름가거나 해변가거나",
          "speaker_id": "1",
          "time": "20210906 00:45"
        },
        {
          "id": "MDRW2100032832.1.13",
          "form": "넷플릭스도 있긴 하구요",
          "original_form": "넷플릭스도 있긴 하구요",
          "speaker_id": "1",
          "time": "20210906 00:46"
        },
        {
          "id": "MDRW2100032832.1.14",
          "form": "집콕은 제.스퇄은 아닌데 아 집꾸미기 공예 그런데 맞네요 홈루덴스족",
          "original_form": "집콕은 제.스퇄은 아닌데 아 집꾸미기 공예 그런데 맞네요 홈루덴스족",
          "speaker_id": "1",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100032832.1.15",
          "form": "ㅎㅎ 좋으시겟어요ㅎㅎ",
          "original_form": "ㅎㅎ 좋으시겟어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100032832.1.16",
          "form": "맞됴ㅎㅎ",
          "original_form": "맞됴ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100032832.1.17",
          "form": "집에서 모든걸 하는",
          "original_form": "집에서 모든걸 하는",
          "speaker_id": "1",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100032832.1.18",
          "form": "전 밖에 더 좋습니다",
          "original_form": "전 밖에 더 좋습니다",
          "speaker_id": "1",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100032832.1.19",
          "form": "집을 근데 렌트하신거잖아요?",
          "original_form": "집을 근데 렌트하신거잖아요?",
          "speaker_id": "2",
          "time": "20210906 00:47"
        },
        {
          "id": "MDRW2100032832.1.20",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210906 00:48"
        },
        {
          "id": "MDRW2100032832.1.21",
          "form": "근데 꾸미시는거에요?",
          "original_form": "근데 꾸미시는거에요?",
          "speaker_id": "2",
          "time": "20210906 00:48"
        },
        {
          "id": "MDRW2100032832.1.22",
          "form": "아깝잖아요ㅎㅎ",
