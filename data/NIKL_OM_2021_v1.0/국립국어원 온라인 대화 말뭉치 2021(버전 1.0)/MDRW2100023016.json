      "utterance": [
        {
          "id": "MDRW2100023016.1.1",
          "form": "결혼해서 행복한 사람들이 너무부러워요ㅎ",
          "original_form": "결혼해서 행복한 사람들이 너무부러워요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:29"
        },
        {
          "id": "MDRW2100023016.1.2",
          "form": "전 실패한거지만ㅎ 그래도ㅎㅎ",
          "original_form": "전 실패한거지만ㅎ 그래도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:29"
        },
        {
          "id": "MDRW2100023016.1.3",
          "form": "그러면 분위기 전환도할겸ㅎ 연애는 어떻게 하셨어요m?",
          "original_form": "그러면 분위기 전환도할겸ㅎ 연애는 어떻게 하셨어요m?",
          "speaker_id": "2",
          "time": "20210829 20:29"
        },
        {
          "id": "MDRW2100023016.1.4",
          "form": "아프시겠지만 힘내셔요!",
          "original_form": "아프시겠지만 힘내셔요!",
          "speaker_id": "1",
          "time": "20210829 20:29"
        },
        {
          "id": "MDRW2100023016.1.5",
          "form": "에이 아프진않아요ㅎ",
          "original_form": "에이 아프진않아요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100023016.1.6",
          "form": "연애는 어떻게 하게 되었냐구요?",
          "original_form": "연애는 어떻게 하게 되었냐구요?",
          "speaker_id": "1",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100023016.1.7",
          "form": "애기랑 같이 못살아서 그게 걱정이죠ㅎ",
          "original_form": "애기랑 같이 못살아서 그게 걱정이죠ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100023016.1.8",
          "form": "네네ㅎㅎㅎ",
          "original_form": "네네ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:30"
        },
        {
          "id": "MDRW2100023016.1.9",
          "form": "아 그렇구나 ㅜ",
          "original_form": "아 그렇구나 ㅜ",
          "speaker_id": "1",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100023016.1.10",
          "form": "만날수 없나요??",
          "original_form": "만날수 없나요??",
          "speaker_id": "1",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100023016.1.11",
          "form": "너무 보고싶을거같은데ㅜㅜㅜ 매일",
          "original_form": "너무 보고싶을거같은데ㅜㅜㅜ 매일",
          "speaker_id": "1",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100023016.1.12",
          "form": "연애는 교회에서ㅜㅎ",
          "original_form": "연애는 교회에서ㅜㅎ",
          "speaker_id": "1",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100023016.1.13",
          "form": "만나요ㅎㅎ 매월 10회이상ㅎㅎ",
          "original_form": "만나요ㅎㅎ 매월 10회이상ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:31"
        },
        {
          "id": "MDRW2100023016.1.14",
          "form": "아에 이혼할때 못박았어요ㅎ",
          "original_form": "아에 이혼할때 못박았어요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100023016.1.15",
          "form": "다행이네요 그건!",
          "original_form": "다행이네요 그건!",
          "speaker_id": "1",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100023016.1.16",
          "form": "만나야죠 아이에게도 그게 좋을거라 생각해요",
          "original_form": "만나야죠 아이에게도 그게 좋을거라 생각해요",
          "speaker_id": "1",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100023016.1.17",
          "form": "난 너없이는 살수있어도 아이없이는 살수없다고",
          "original_form": "난 너없이는 살수있어도 아이없이는 살수없다고",
          "speaker_id": "2",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100023016.1.18",
          "form": "글죠ㅎ 더노력하려구요ㅎ",
          "original_form": "글죠ㅎ 더노력하려구요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100023016.1.19",
          "form": "교회오빠였어요?",
          "original_form": "교회오빠였어요?",
          "speaker_id": "2",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100023016.1.20",
          "form": "아님 누나?",
          "original_form": "아님 누나?",
          "speaker_id": "2",
          "time": "20210829 20:32"
        },
        {
          "id": "MDRW2100023016.1.21",
          "form": "오빠 ㅋㅋㅋ",
          "original_form": "오빠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:33"
        },
        {
          "id": "MDRW2100023016.1.22",
          "form": "오빠ㅎㅎㅎㅎㅎ",
          "original_form": "오빠ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:34"
        },
        {
          "id": "MDRW2100023016.1.23",
          "form": "어땠어요?",
          "original_form": "어땠어요?",
          "speaker_id": "2",
          "time": "20210829 20:34"
        },
        {
          "id": "MDRW2100023016.1.24",
          "form": "뭐때문에 결혼까지 생각이들었어요?",
          "original_form": "뭐때문에 결혼까지 생각이들었어요?",
          "speaker_id": "2",
          "time": "20210829 20:34"
        },
        {
          "id": "MDRW2100023016.1.25",
          "form": "성실ㅋㅋㅋㅋ",
          "original_form": "성실ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:34"
        },
        {
          "id": "MDRW2100023016.1.26",
          "form": "나를 바라봄",
          "original_form": "나를 바라봄",
          "speaker_id": "1",
          "time": "20210829 20:34"
        },
        {
          "id": "MDRW2100023016.1.27",
          "form": "가치관이 동일",
          "original_form": "가치관이 동일",
          "speaker_id": "1",
          "time": "20210829 20:35"
        },
        {
          "id": "MDRW2100023016.1.28",
          "form": "오오ㅎㅎ",
          "original_form": "오오ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:35"
        },
        {
          "id": "MDRW2100023016.1.29",
          "form": "몇년연예하셨는데요?",
          "original_form": "몇년연예하셨는데요?",
          "speaker_id": "2",
          "time": "20210829 20:35"
        },
        {
          "id": "MDRW2100023016.1.30",
          "form": "연애",
          "original_form": "연애",
          "speaker_id": "2",
          "time": "20210829 20:35"
        },
        {
          "id": "MDRW2100023016.1.31",
          "form": "7개월 연애 ㅎㅏ고 결혼했어요ㅋㅋ",
          "original_form": "7개월 연애 ㅎㅏ고 결혼했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:36"
        },
        {
          "id": "MDRW2100023016.1.32",
          "form": "엇?ㅎㅎ",
          "original_form": "엇?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100023016.1.33",
          "form": "그렇게 짧게요?",
          "original_form": "그렇게 짧게요?",
          "speaker_id": "2",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100023016.1.34",
          "form": "전기가 파파박 튀었네요ㅎ",
          "original_form": "전기가 파파박 튀었네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100023016.1.35",
          "form": "ㅎㅎ 나이가 어린편이 아니라",
          "original_form": "ㅎㅎ 나이가 어린편이 아니라",
          "speaker_id": "1",
          "time": "20210829 20:37"
        },
        {
          "id": "MDRW2100023016.1.36",
          "form": "더 만나봤자 뭐 ㅎㅎ 같이 사는게 더 낫다생각이 들었어요",
          "original_form": "더 만나봤자 뭐 ㅎㅎ 같이 사는게 더 낫다생각이 들었어요",
          "speaker_id": "1",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100023016.1.37",
          "form": "아아아아ㅎㅎ",
          "original_form": "아아아아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:38"
        },
        {
          "id": "MDRW2100023016.1.38",
          "form": "늦게 결혼하셨군요ㅎ",
          "original_form": "늦게 결혼하셨군요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100023016.1.39",
          "form": "행복하시면됫죵ㅎㅎㅎ",
          "original_form": "행복하시면됫죵ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100023016.1.40",
          "form": "꼭 아이는 늦게요ㅎㅎ",
          "original_form": "꼭 아이는 늦게요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100023016.1.41",
          "form": "ㅎㅎㅎ 나이가 어린편이 아니라서ㅜㅜ",
          "original_form": "ㅎㅎㅎ 나이가 어린편이 아니라서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 20:40"
        },
        {
          "id": "MDRW2100023016.1.42",
          "form": "저흰 올해 계획은 있어요",
          "original_form": "저흰 올해 계획은 있어요",
          "speaker_id": "1",
          "time": "20210829 20:40"
        },
        {
          "id": "MDRW2100023016.1.43",
          "form": "오ㅎㅎ",
          "original_form": "오ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:40"
        },
        {
          "id": "MDRW2100023016.1.44",
          "form": "엽산하고",
          "original_form": "엽산하고",
          "speaker_id": "2",
          "time": "20210829 20:40"
        },
        {
          "id": "MDRW2100023016.1.45",
          "form": "잘챙겨드셔야해요ㅎ",
          "original_form": "잘챙겨드셔야해요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100023016.1.46",
          "form": "넵! 조만간 한약도 지으러 갈거같아요",
          "original_form": "넵! 조만간 한약도 지으러 갈거같아요",
          "speaker_id": "1",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100023016.1.47",
          "form": "임신할때 꼭 챙겨먹어야하는 영양제ㅎ 병원에서 설명해주시겠지만ㅎ 엽산은 임신전에 먹으면 좋다더라구요ㅎ",
          "original_form": "임신할때 꼭 챙겨먹어야하는 영양제ㅎ 병원에서 설명해주시겠지만ㅎ 엽산은 임신전에 먹으면 좋다더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100023016.1.48",
          "form": "오오ㅎㅎ 축하해요ㅎㅎ",
          "original_form": "오오ㅎㅎ 축하해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100023016.1.49",
          "form": "미리ㅎㅎㄹ",
          "original_form": "미리ㅎㅎㄹ",
          "speaker_id": "2",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100023016.1.50",
          "form": "네네 ㅎㅎ 감사합니다",
          "original_form": "네네 ㅎㅎ 감사합니다",
          "speaker_id": "1",
          "time": "20210829 20:41"
        },
        {
          "id": "MDRW2100023016.1.51",
          "form": "걱장하게 출산하시기 바랄께요ㅎㅎ",
          "original_form": "걱장하게 출산하시기 바랄께요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100023016.1.52",
          "form": "정말 축하해요ㅎ",
          "original_form": "정말 축하해요ㅎ",
          "speaker_id": "2",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100023016.1.53",
          "form": "나중에 보시면 아시겟지만 아이가 주는 행복은 세상 다가진것같애요",
          "original_form": "나중에 보시면 아시겟지만 아이가 주는 행복은 세상 다가진것같애요",
          "speaker_id": "2",
          "time": "20210829 20:42"
        },
        {
          "id": "MDRW2100023016.1.54",
          "form": "어우 너무 행복하죠 ㅜㅜ",
          "original_form": "어우 너무 행복하죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 20:43"
        },
        {
          "id": "MDRW2100023016.1.55",
          "form": "애기를 워낙 좋아해서",
          "original_form": "애기를 워낙 좋아해서",
          "speaker_id": "1",
          "time": "20210829 20:43"
        }
      ]
    }
  ]
