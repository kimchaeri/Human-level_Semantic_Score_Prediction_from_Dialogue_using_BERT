      "utterance": [
        {
          "id": "MDRW2100031125.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100031125.1.2",
          "form": "네 좋아해여 name1님은요",
          "original_form": "네 좋아해여 &name1&님은요",
          "speaker_id": "2",
          "time": "20210904 17:29"
        },
        {
          "id": "MDRW2100031125.1.3",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210904 17:30"
        },
        {
          "id": "MDRW2100031125.1.4",
          "form": "잘 못가서 그렇지",
          "original_form": "잘 못가서 그렇지",
          "speaker_id": "1",
          "time": "20210904 17:30"
        },
        {
          "id": "MDRW2100031125.1.5",
          "form": "그쵸 코로나이후로 그냥 스탑상태 ㅠㅠ",
          "original_form": "그쵸 코로나이후로 그냥 스탑상태 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 17:31"
        },
        {
          "id": "MDRW2100031125.1.6",
          "form": "좀 가려고하면 심각해지니까..",
          "original_form": "좀 가려고하면 심각해지니까..",
          "speaker_id": "2",
          "time": "20210904 17:31"
        },
        {
          "id": "MDRW2100031125.1.7",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "1",
          "time": "20210904 17:35"
        },
        {
          "id": "MDRW2100031125.1.8",
          "form": "원래 한참전에 비행기표 예약해두고 여행만 기다리면서 회사다니는게 좋았는데",
          "original_form": "원래 한참전에 비행기표 예약해두고 여행만 기다리면서 회사다니는게 좋았는데",
          "speaker_id": "1",
          "time": "20210904 17:36"
        },
        {
          "id": "MDRW2100031125.1.9",
          "form": "이제 그 낙이 없네요",
          "original_form": "이제 그 낙이 없네요",
          "speaker_id": "1",
          "time": "20210904 17:36"
        },
        {
          "id": "MDRW2100031125.1.10",
          "form": "진짜요 그냥 매년 시간만 흐르니까 너무 지루해요..",
          "original_form": "진짜요 그냥 매년 시간만 흐르니까 너무 지루해요..",
          "speaker_id": "2",
          "time": "20210904 17:38"
        },
        {
          "id": "MDRW2100031125.1.11",
          "form": "여행지 어디가 제일 기억에 남으세요?",
          "original_form": "여행지 어디가 제일 기억에 남으세요?",
          "speaker_id": "2",
          "time": "20210904 17:38"
        },
        {
          "id": "MDRW2100031125.1.12",
          "form": "해외는 다 좋았는데",
          "original_form": "해외는 다 좋았는데",
          "speaker_id": "1",
          "time": "20210904 17:39"
        },
        {
          "id": "MDRW2100031125.1.13",
          "form": "개인적으론",
          "original_form": "개인적으론",
          "speaker_id": "1",
          "time": "20210904 17:39"
        },
        {
          "id": "MDRW2100031125.1.14",
          "form": "가을에 10박11일로 갔던 프랑스랑 영국이여",
          "original_form": "가을에 10박11일로 갔던 프랑스랑 영국이여",
          "speaker_id": "1",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100031125.1.15",
          "form": "가을에가서 그런지",
          "original_form": "가을에가서 그런지",
          "speaker_id": "1",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100031125.1.16",
          "form": "날씨도 좋고",
          "original_form": "날씨도 좋고",
          "speaker_id": "1",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100031125.1.17",
          "form": "오 저도 유럽 갔었는데 영국 좋더라고요",
          "original_form": "오 저도 유럽 갔었는데 영국 좋더라고요",
          "speaker_id": "2",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100031125.1.18",
          "form": "그립네요..",
          "original_form": "그립네요..",
          "speaker_id": "2",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100031125.1.19",
          "form": "홍콩도 좋았는데",
          "original_form": "홍콩도 좋았는데",
          "speaker_id": "1",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100031125.1.20",
          "form": "홍콩 저도 마카오랑 같이 갔었는데 쇼핑하기 좋더라고요",
          "original_form": "홍콩 저도 마카오랑 같이 갔었는데 쇼핑하기 좋더라고요",
          "speaker_id": "2",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100031125.1.21",
          "form": "또 어디 가보셨어요?",
          "original_form": "또 어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100031125.1.22",
          "form": "저는 유럽은 영프스이 그담에 일본 홍콩 태국 베트남 이렇게 가본거 같네요~",
          "original_form": "저는 유럽은 영프스이 그담에 일본 홍콩 태국 베트남 이렇게 가본거 같네요~",
          "speaker_id": "2",
          "time": "20210904 17:42"
        },
        {
          "id": "MDRW2100031125.1.23",
          "form": "많이 가셨네요",
          "original_form": "많이 가셨네요",
          "speaker_id": "1",
          "time": "20210904 17:42"
        },
        {
          "id": "MDRW2100031125.1.24",
          "form": "동남아가 전 맞는거 같아요 ㅋㅋ 먹는 게",
          "original_form": "동남아가 전 맞는거 같아요 ㅋㅋ 먹는 게",
          "speaker_id": "2",
          "time": "20210904 17:43"
        },
        {
          "id": "MDRW2100031125.1.25",
          "form": "아무래도 그렇죠",
          "original_form": "아무래도 그렇죠",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100031125.1.26",
          "form": "영국에서 먹을게없어서",
          "original_form": "영국에서 먹을게없어서",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100031125.1.27",
          "form": "한식당가서 먹었던 ㅋㅋㅋ",
          "original_form": "한식당가서 먹었던 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100031125.1.28",
          "form": "아 영국은 진짜 ㅋㅋㅋ 물가도 넘 비싸구요..",
          "original_form": "아 영국은 진짜 ㅋㅋㅋ 물가도 넘 비싸구요..",
          "speaker_id": "2",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100031125.1.29",
          "form": "샌드위치가 젤 무난 ㅋㅋㅋ",
          "original_form": "샌드위치가 젤 무난 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100031125.1.30",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100031125.1.31",
          "form": "피쉬앤칩스 최악",
          "original_form": "피쉬앤칩스 최악",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100031125.1.32",
          "form": "저는 피쉬앤칩스는 그래도 뜨끈하니까 맛있더라고요 ㅋㅋ",
          "original_form": "저는 피쉬앤칩스는 그래도 뜨끈하니까 맛있더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:46"
        }
      ]
