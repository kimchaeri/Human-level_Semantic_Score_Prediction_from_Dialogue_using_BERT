      "utterance": [
        {
          "id": "MDRW2100046806.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211105 15:00"
        },
        {
          "id": "MDRW2100046806.1.2",
          "form": "안녕하세요! 날씨도 좋고 기분도 좋은 금요일이네요",
          "original_form": "안녕하세요! 날씨도 좋고 기분도 좋은 금요일이네요",
          "speaker_id": "2",
          "time": "20211105 15:00"
        },
        {
          "id": "MDRW2100046806.1.3",
          "form": "네 불금이네요",
          "original_form": "네 불금이네요",
          "speaker_id": "1",
          "time": "20211105 15:02"
        },
        {
          "id": "MDRW2100046806.1.4",
          "form": "날씨도 좋고",
          "original_form": "날씨도 좋고",
          "speaker_id": "1",
          "time": "20211105 15:03"
        },
        {
          "id": "MDRW2100046806.1.5",
          "form": "달리기 좋은 날이네요",
          "original_form": "달리기 좋은 날이네요",
          "speaker_id": "1",
          "time": "20211105 15:03"
        },
        {
          "id": "MDRW2100046806.1.6",
          "form": "헤헤",
          "original_form": "헤헤",
          "speaker_id": "1",
          "time": "20211105 15:03"
        },
        {
          "id": "MDRW2100046806.1.7",
          "form": "오오 달리기 좋아하시나요",
          "original_form": "오오 달리기 좋아하시나요",
          "speaker_id": "2",
          "time": "20211105 15:03"
        },
        {
          "id": "MDRW2100046806.1.8",
          "form": "저는 조금만 뛰어도 힘들어서.. 체력이 바닥이거든요 ㅎㅎ",
          "original_form": "저는 조금만 뛰어도 힘들어서.. 체력이 바닥이거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211105 15:04"
        },
        {
          "id": "MDRW2100046806.1.9",
          "form": "아뇨 그말이 아니라 술마시기 좋은날이라는 말이였어요",
          "original_form": "아뇨 그말이 아니라 술마시기 좋은날이라는 말이였어요",
          "speaker_id": "1",
          "time": "20211105 15:04"
        },
        {
          "id": "MDRW2100046806.1.10",
          "form": "저도 달리기는 못해요 ㅋㅋ",
          "original_form": "저도 달리기는 못해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211105 15:04"
        },
        {
          "id": "MDRW2100046806.1.11",
          "form": "아하 ㅋㅋㅋ 제가 헷갈렸네요 ",
          "original_form": "아하 ㅋㅋㅋ 제가 헷갈렸네요 ",
          "speaker_id": "2",
          "time": "20211105 15:05"
        },
        {
          "id": "MDRW2100046806.1.12",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211105 15:05"
        },
        {
          "id": "MDRW2100046806.1.13",
          "form": "저는 오늘 피맥 예정입니다 ㅋㅋ",
          "original_form": "저는 오늘 피맥 예정입니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211105 15:05"
        },
        {
          "id": "MDRW2100046806.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20211105 15:05"
        },
        {
          "id": "MDRW2100046806.1.15",
          "form": "저는 피자에 소주 좋아해요",
          "original_form": "저는 피자에 소주 좋아해요",
          "speaker_id": "1",
          "time": "20211105 15:05"
        },
        {
          "id": "MDRW2100046806.1.16",
          "form": "집에서 식구들이랑 피자에 맥주 마실라구요",
          "original_form": "집에서 식구들이랑 피자에 맥주 마실라구요",
          "speaker_id": "2",
          "time": "20211105 15:05"
        },
        {
          "id": "MDRW2100046806.1.17",
          "form": "오우 피자에 소주요?!",
          "original_form": "오우 피자에 소주요?!",
          "speaker_id": "2",
          "time": "20211105 15:05"
        },
        {
          "id": "MDRW2100046806.1.18",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20211105 15:05"
        },
        {
          "id": "MDRW2100046806.1.19",
          "form": "의외로 맛있어요",
          "original_form": "의외로 맛있어요",
          "speaker_id": "1",
          "time": "20211105 15:05"
        },
        {
          "id": "MDRW2100046806.1.20",
          "form": "오호라...오늘 한번 도전해볼까요",
          "original_form": "오호라...오늘 한번 도전해볼까요",
          "speaker_id": "2",
          "time": "20211105 15:06"
        },
        {
          "id": "MDRW2100046806.1.21",
          "form": "네 한번 드셔보세요 ㅋㅋ",
          "original_form": "네 한번 드셔보세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211105 15:06"
        },
        {
          "id": "MDRW2100046806.1.22",
          "form": "넵ㅋㅋ 추천해주실래요? 제가 소주를 잘 안마셔서",
          "original_form": "넵ㅋㅋ 추천해주실래요? 제가 소주를 잘 안마셔서",
          "speaker_id": "2",
          "time": "20211105 15:06"
        },
        {
          "id": "MDRW2100046806.1.23",
          "form": "종류를 잘 모릅니다",
          "original_form": "종류를 잘 모릅니다",
          "speaker_id": "2",
          "time": "20211105 15:07"
        },
        {
          "id": "MDRW2100046806.1.24",
          "form": "저는 진로 마셔요",
          "original_form": "저는 진로 마셔요",
          "speaker_id": "1",
          "time": "20211105 15:09"
        },
        {
          "id": "MDRW2100046806.1.25",
          "form": "진로.. 감사합니다 오늘 한번 피맥말고 .. 피소주..?",
          "original_form": "진로.. 감사합니다 오늘 한번 피맥말고 .. 피소주..?",
          "speaker_id": "2",
          "time": "20211105 15:10"
        },
        {
          "id": "MDRW2100046806.1.26",
          "form": "도전해보겠습니다 ㅋㅋ",
          "original_form": "도전해보겠습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211105 15:10"
        }
      ]
    }
