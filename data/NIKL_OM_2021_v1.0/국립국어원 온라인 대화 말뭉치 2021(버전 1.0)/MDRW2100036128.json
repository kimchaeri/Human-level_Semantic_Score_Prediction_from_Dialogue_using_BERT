      "utterance": [
        {
          "id": "MDRW2100036128.1.1",
          "form": "저녁은 뭘로 드실생각이세요??",
          "original_form": "저녁은 뭘로 드실생각이세요??",
          "speaker_id": "2",
          "time": "20210830 16:50"
        },
        {
          "id": "MDRW2100036128.1.2",
          "form": "야근해서 선택지가ㅜ",
          "original_form": "야근해서 선택지가ㅜ",
          "speaker_id": "1",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100036128.1.3",
          "form": "투다리서 안주로  떼웠네요",
          "original_form": "투다리서 안주로  떼웠네요",
          "speaker_id": "1",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100036128.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100036128.1.5",
          "form": "저는 주꾸미볶음에 고등어구이요",
          "original_form": "저는 주꾸미볶음에 고등어구이요",
          "speaker_id": "2",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100036128.1.6",
          "form": "매번 너무 본격적인 메뉴라",
          "original_form": "매번 너무 본격적인 메뉴라",
          "speaker_id": "1",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100036128.1.7",
          "form": "제가 쭈구리가 되네요ㅜㅇㅜ",
          "original_form": "제가 쭈구리가 되네요ㅜㅇㅜ",
          "speaker_id": "1",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100036128.1.8",
          "form": "ㅋㅋㅋ저는 주부잖아여ㅠㅠ",
          "original_form": "ㅋㅋㅋ저는 주부잖아여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100036128.1.9",
          "form": "전 사실 투다리안주가 더",
          "original_form": "전 사실 투다리안주가 더",
          "speaker_id": "2",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100036128.1.10",
          "form": "땡기네요",
          "original_form": "땡기네요",
          "speaker_id": "2",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100036128.1.11",
          "form": "해봐야 닭꼬치ㅜ",
          "original_form": "해봐야 닭꼬치ㅜ",
          "speaker_id": "1",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100036128.1.12",
          "form": "솔직히 여긴 너무 물려요",
          "original_form": "솔직히 여긴 너무 물려요",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100036128.1.13",
          "form": "맛있겠는데요?ㅋ",
          "original_form": "맛있겠는데요?ㅋ",
          "speaker_id": "2",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100036128.1.14",
          "form": "술도 한잔?",
          "original_form": "술도 한잔?",
          "speaker_id": "2",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100036128.1.15",
          "form": "술 마시러 가죠ㅜ",
          "original_form": "술 마시러 가죠ㅜ",
          "speaker_id": "1",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100036128.1.16",
          "form": "안주는 님의 주꾸미볶음 먹을수있음 영혼을 팔게요",
          "original_form": "안주는 님의 주꾸미볶음 먹을수있음 영혼을 팔게요",
          "speaker_id": "1",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100036128.1.17",
          "form": "이런ㅠㅠ 하남주꾸미 냉동을..ㅠㅠ",
          "original_form": "이런ㅠㅠ 하남주꾸미 냉동을..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 22:51"
        },
        {
          "id": "MDRW2100036128.1.18",
          "form": "그럼  영혼중 냉동부위만 팔게요'ㅁ'",
          "original_form": "그럼  영혼중 냉동부위만 팔게요'ㅁ'",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100036128.1.19",
          "form": "ㅋㅋㅋㅋ. 있으신거죠,ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ. 있으신거죠,ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100036128.1.20",
          "form": "대충어디쯤?ㅋㅋ",
          "original_form": "대충어디쯤?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100036128.1.21",
          "form": "하남주꾸미 냉동잘 나오더라구요",
          "original_form": "하남주꾸미 냉동잘 나오더라구요",
          "speaker_id": "2",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100036128.1.22",
          "form": "사태살...?",
          "original_form": "사태살...?",
          "speaker_id": "1",
          "time": "20210830 23:00"
        },
        {
          "id": "MDRW2100036128.1.23",
          "form": "냉동 양념두 같이 파나요?",
          "original_form": "냉동 양념두 같이 파나요?",
          "speaker_id": "1",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100036128.1.24",
          "form": "양념되어서 냉동으로 나와요 볶기만 하면 됩니닿 ㅎ",
          "original_form": "양념되어서 냉동으로 나와요 볶기만 하면 됩니닿 ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:13"
        }
      ]
    }
