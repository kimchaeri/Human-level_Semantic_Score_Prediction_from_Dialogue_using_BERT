      "utterance": [
        {
          "id": "MDRW2100043162.1.1",
          "form": "휴식가갔더니 떡 몇개주던데",
          "original_form": "휴식가갔더니 떡 몇개주던데",
          "speaker_id": "2",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100043162.1.2",
          "form": "먹어봤어?",
          "original_form": "먹어봤어?",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043162.1.3",
          "form": "무슨떡?",
          "original_form": "무슨떡?",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043162.1.4",
          "form": "인절민가 속에",
          "original_form": "인절민가 속에",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043162.1.5",
          "form": "머 들었더라",
          "original_form": "머 들었더라",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043162.1.6",
          "form": "인절미가?",
          "original_form": "인절미가?",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043162.1.7",
          "form": "밥알 인절미래",
          "original_form": "밥알 인절미래",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043162.1.8",
          "form": "별로 맛없을거 같음..",
          "original_form": "별로 맛없을거 같음..",
          "speaker_id": "1",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043162.1.9",
          "form": "아니야 많이 안 달고 진짜 맛있던데",
          "original_form": "아니야 많이 안 달고 진짜 맛있던데",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043162.1.10",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043162.1.11",
          "form": "속에 머 들었는데",
          "original_form": "속에 머 들었는데",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043162.1.12",
          "form": "안들은 것도 있고",
          "original_form": "안들은 것도 있고",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043162.1.13",
          "form": "팥들은 것도 있고",
          "original_form": "팥들은 것도 있고",
          "speaker_id": "2",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043162.1.14",
          "form": "팥 들은 거 궁금하다",
          "original_form": "팥 들은 거 궁금하다",
          "speaker_id": "1",
          "time": "20210907 15:17"
        },
        {
          "id": "MDRW2100043162.1.15",
          "form": "여기서 떡 보내야겠어 애원이한테",
          "original_form": "여기서 떡 보내야겠어 애원이한테",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043162.1.16",
          "form": "어딘지 알아?",
          "original_form": "어딘지 알아?",
          "speaker_id": "1",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043162.1.17",
          "form": "휴식가 앞에 카페에서 한대",
          "original_form": "휴식가 앞에 카페에서 한대",
          "speaker_id": "2",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043162.1.18",
          "form": "카페에서 떡집도 한대?",
          "original_form": "카페에서 떡집도 한대?",
          "speaker_id": "1",
          "time": "20210907 15:18"
        },
        {
          "id": "MDRW2100043162.1.19",
          "form": "거기서 손으로 바로바로 만들어 준다더라",
          "original_form": "거기서 손으로 바로바로 만들어 준다더라",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100043162.1.20",
          "form": "오 신기하다",
          "original_form": "오 신기하다",
          "speaker_id": "1",
          "time": "20210907 15:19"
        }
      ]
