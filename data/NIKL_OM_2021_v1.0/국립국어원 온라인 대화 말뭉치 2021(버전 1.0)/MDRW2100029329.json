      "utterance": [
        {
          "id": "MDRW2100029329.1.1",
          "form": "아우... 이거 어떡하나요",
          "original_form": "아우... 이거 어떡하나요",
          "speaker_id": "2",
          "time": "20210903 01:36"
        },
        {
          "id": "MDRW2100029329.1.2",
          "form": "사둔 복숭아 어제 다먹었는데ㅠㅠ",
          "original_form": "사둔 복숭아 어제 다먹었는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 01:37"
        },
        {
          "id": "MDRW2100029329.1.3",
          "form": "아이고ㅠㅜㅜ",
          "original_form": "아이고ㅠㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 01:37"
        },
        {
          "id": "MDRW2100029329.1.4",
          "form": "꼭 먹고싶을때 다떨어지더라고요ㅠ",
          "original_form": "꼭 먹고싶을때 다떨어지더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210903 01:37"
        },
        {
          "id": "MDRW2100029329.1.5",
          "form": "ㅋㅋㅋ이게다 과일 질문주셔서 그런겁니다!!",
          "original_form": "ㅋㅋㅋ이게다 과일 질문주셔서 그런겁니다!!",
          "speaker_id": "2",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029329.1.6",
          "form": "먹는걸 좋아해서 어쩔수 없었어요ㅋㅋㅋㅋㅋ",
          "original_form": "먹는걸 좋아해서 어쩔수 없었어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100029329.1.7",
          "form": "저도 먹기위해 산답니다ㅋㅋㅋㅋ",
          "original_form": "저도 먹기위해 산답니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100029329.1.8",
          "form": "그래두 복숭아동지만나서 기뻐요ㅋㅋㅋㅋㅋ",
          "original_form": "그래두 복숭아동지만나서 기뻐요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100029329.1.9",
          "form": "먹는데 진심이라 그렇게 된거같죠ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "먹는데 진심이라 그렇게 된거같죠ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:41"
        },
        {
          "id": "MDRW2100029329.1.10",
          "form": "진짜 먹는게 최고...",
          "original_form": "진짜 먹는게 최고...",
          "speaker_id": "2",
          "time": "20210903 01:43"
        },
        {
          "id": "MDRW2100029329.1.11",
          "form": "눈깜빡할 새에 사라져버리는 신기루같지만 놓을 수 없죠..ㅠ",
          "original_form": "눈깜빡할 새에 사라져버리는 신기루같지만 놓을 수 없죠..ㅠ",
          "speaker_id": "2",
          "time": "20210903 01:44"
        },
        {
          "id": "MDRW2100029329.1.12",
          "form": "그래서 맛있는거 먹기전엔 사진꼭 찍어요!",
          "original_form": "그래서 맛있는거 먹기전엔 사진꼭 찍어요!",
          "speaker_id": "1",
          "time": "20210903 01:44"
        },
        {
          "id": "MDRW2100029329.1.13",
          "form": "사진 중요합니다ㅋㅋㅋ",
          "original_form": "사진 중요합니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:46"
        },
        {
          "id": "MDRW2100029329.1.14",
          "form": "보고 나중에 또먹으러 가야해요ㅋㅋㅋ",
          "original_form": "보고 나중에 또먹으러 가야해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:47"
        },
        {
          "id": "MDRW2100029329.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ한때는 달력에 주기 체크하면서 먹었습니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋ한때는 달력에 주기 체크하면서 먹었습니다",
          "speaker_id": "2",
          "time": "20210903 01:49"
        },
        {
          "id": "MDRW2100029329.1.16",
          "form": "세상에ㅋㅋㅋㅋ얼마나 맛있는걸 드신거에요",
          "original_form": "세상에ㅋㅋㅋㅋ얼마나 맛있는걸 드신거에요",
          "speaker_id": "1",
          "time": "20210903 01:49"
        },
        {
          "id": "MDRW2100029329.1.17",
          "form": "그냥 평범한 것들인데 뭔가 겹치게 먹으면 아쉬웠어요ㅋㅋㅋㅋ",
