      "utterance": [
        {
          "id": "MDRW2100021067.1.1",
          "form": "겜좋아하시나요",
          "original_form": "겜좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100021067.1.2",
          "form": "저는 걍 모바일 리듬게임정도만 ?!?! ㅋㅋㅋㅋ",
          "original_form": "저는 걍 모바일 리듬게임정도만 ?!?! ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100021067.1.3",
          "form": "게임하시는 거 있으신가요?!?!",
          "original_form": "게임하시는 거 있으신가요?!?!",
          "speaker_id": "2",
          "time": "20210828 15:50"
        },
        {
          "id": "MDRW2100021067.1.4",
          "form": "피파온라인이요",
          "original_form": "피파온라인이요",
          "speaker_id": "1",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100021067.1.5",
          "form": "오호 ㅋㅋㅋ축구 ㅋㅋㅋㅋ",
          "original_form": "오호 ㅋㅋㅋ축구 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021067.1.6",
          "form": "집에서 하시나요?? 피씨방?!?!",
          "original_form": "집에서 하시나요?? 피씨방?!?!",
          "speaker_id": "2",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021067.1.7",
          "form": "집에서합니다ㅎㅎ",
          "original_form": "집에서합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021067.1.8",
          "form": "아예 몰라가지고 ㅋㅋㅋㅋㅋ 광고만 본정도?!?!",
          "original_form": "아예 몰라가지고 ㅋㅋㅋㅋㅋ 광고만 본정도?!?!",
          "speaker_id": "2",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021067.1.9",
          "form": "축구면 팀플레이잖아요 다른 사람들이랑 팀을 짜는건가요?",
          "original_form": "축구면 팀플레이잖아요 다른 사람들이랑 팀을 짜는건가요?",
          "speaker_id": "2",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100021067.1.10",
          "form": "아뇨 1대1 대결입니다",
          "original_form": "아뇨 1대1 대결입니다",
          "speaker_id": "1",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100021067.1.11",
          "form": "오잉 ㅋㅋㅋ생각했던것과 전혀 다르군요 ㅋㅋㅋ",
          "original_form": "오잉 ㅋㅋㅋ생각했던것과 전혀 다르군요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021067.1.12",
          "form": "피파온라이도 이스포츠대회같은거 있나요?",
          "original_form": "피파온라이도 이스포츠대회같은거 있나요?",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100021067.1.13",
          "form": "네네 있을거에요ㅎㅎ",
          "original_form": "네네 있을거에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021067.1.14",
          "form": "보시진 않나봐요 ㅋㅋㅋㅋ",
          "original_form": "보시진 않나봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100021067.1.15",
          "form": "제동생 보면 게임안할때는 게임영상을 유툽으로 보고있더라고요 ㅋㅋㅋ",
          "original_form": "제동생 보면 게임안할때는 게임영상을 유툽으로 보고있더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021067.1.16",
          "form": "오엄청 매니아네요ㅎㅎ",
          "original_form": "오엄청 매니아네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100021067.1.17",
          "form": "그르게요 ㅋㅋㅋ피파 매일 하시나요 ㅋㅋㅋㅋ",
          "original_form": "그르게요 ㅋㅋㅋ피파 매일 하시나요 ㅋㅋㅋㅋ",
