      "utterance": [
        {
          "id": "MDRW2100037466.1.1",
          "form": "어릴때 좋아하는게임없엇나용",
          "original_form": "어릴때 좋아하는게임없엇나용",
          "speaker_id": "2",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100037466.1.2",
          "form": "컴퓨터게임",
          "original_form": "컴퓨터게임",
          "speaker_id": "2",
          "time": "20210901 00:29"
        },
        {
          "id": "MDRW2100037466.1.3",
          "form": "저는 제트기 뿅뿅 날아다니는게 생각나네요",
          "original_form": "저는 제트기 뿅뿅 날아다니는게 생각나네요",
          "speaker_id": "1",
          "time": "20210901 00:32"
        },
        {
          "id": "MDRW2100037466.1.4",
          "form": "오.. 그거잘하세겨?",
          "original_form": "오.. 그거잘하세겨?",
          "speaker_id": "2",
          "time": "20210901 00:37"
        },
        {
          "id": "MDRW2100037466.1.5",
          "form": "아뇨 못하죠 근데 타임 ㅅㅍㄹㄷ라고 적립광고 봤어요",
          "original_form": "아뇨 못하죠 근데 타임 ㅅㅍㄹㄷ라고 적립광고 봤어요",
          "speaker_id": "1",
          "time": "20210901 00:38"
        },
        {
          "id": "MDRW2100037466.1.6",
          "form": "오 .그렇구나..\r\n주변에 하는사람 잇엇어요?",
          "original_form": "오 .그렇구나..\r\n주변에 하는사람 잇엇어요?",
          "speaker_id": "2",
          "time": "20210901 00:39"
        },
        {
          "id": "MDRW2100037466.1.7",
          "form": "저랑ㅊ언니요",
          "original_form": "저랑ㅊ언니요",
          "speaker_id": "1",
          "time": "20210901 00:39"
        },
        {
          "id": "MDRW2100037466.1.8",
          "form": "오.. 요즘엔안하시고요?",
          "original_form": "오.. 요즘엔안하시고요?",
          "speaker_id": "2",
          "time": "20210901 00:46"
        },
        {
          "id": "MDRW2100037466.1.9",
          "form": "네 그 게임파일조차 못찾아요",
          "original_form": "네 그 게임파일조차 못찾아요",
          "speaker_id": "1",
          "time": "20210901 00:48"
        },
        {
          "id": "MDRW2100037466.1.10",
          "form": "오.. 그러시구나..온라인게임은요?",
          "original_form": "오.. 그러시구나..온라인게임은요?",
          "speaker_id": "2",
          "time": "20210901 00:48"
        },
        {
          "id": "MDRW2100037466.1.11",
          "form": "온라인게임 예전엔 햤었죠",
          "original_form": "온라인게임 예전엔 햤었죠",
          "speaker_id": "1",
          "time": "20210901 00:49"
        },
        {
          "id": "MDRW2100037466.1.12",
          "form": "오ㅡ.뭐죠?",
          "original_form": "오ㅡ.뭐죠?",
          "speaker_id": "2",
          "time": "20210901 01:00"
        },
        {
          "id": "MDRW2100037466.1.13",
          "form": "그건 다음에 말하죠",
          "original_form": "그건 다음에 말하죠",
          "speaker_id": "1",
          "time": "20210901 01:03"
        },
        {
          "id": "MDRW2100037466.1.14",
          "form": "ㅋㅋㅋ재밋네 ㅋㅋ이사람ㅋㅋ",
