      "utterance": [
        {
          "id": "MDRW2100044499.1.1",
          "form": "처음 할 때 추천받은걸로 계속 쓰고있어요ㅋㅋ",
          "original_form": "처음 할 때 추천받은걸로 계속 쓰고있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044499.1.2",
          "form": "ㅋㅋ저도 맨처음 젤추천받았는데",
          "original_form": "ㅋㅋ저도 맨처음 젤추천받았는데",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044499.1.3",
          "form": "그뒤로 이것저것 하다 결국 또 젤쓰고있어요",
          "original_form": "그뒤로 이것저것 하다 결국 또 젤쓰고있어요",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044499.1.4",
          "form": "ㅋㅋㅋ전 잘 번지는 눈이라",
          "original_form": "ㅋㅋㅋ전 잘 번지는 눈이라",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044499.1.5",
          "form": "젤이 젤 편하더라고요",
          "original_form": "젤이 젤 편하더라고요",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044499.1.6",
          "form": "아 저두요ㅋㅋ",
          "original_form": "아 저두요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044499.1.7",
          "form": "갈색쓰세요?검정쓰세요?",
          "original_form": "갈색쓰세요?검정쓰세요?",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044499.1.8",
          "form": "갈색이요",
          "original_form": "갈색이요",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044499.1.9",
          "form": "갈색이 자연스럽고 짱인거같아요ㅋㅋ",
          "original_form": "갈색이 자연스럽고 짱인거같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044499.1.10",
          "form": "네 갈색도 그리고 나면 은근 진해서",
          "original_form": "네 갈색도 그리고 나면 은근 진해서",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044499.1.11",
          "form": "요즘은 자연스러운게 트렌디이기도 하니까요ㅋㅋ",
          "original_form": "요즘은 자연스러운게 트렌디이기도 하니까요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044499.1.12",
          "form": "네 저는 눈썹도 진갈색으로 해서",
          "original_form": "네 저는 눈썹도 진갈색으로 해서",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044499.1.13",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044499.1.14",
          "form": "저는 눈썹은 회색요ㅋㅋ",
          "original_form": "저는 눈썹은 회색요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044499.1.15",
          "form": "눈썹이 짙은편이라 갈색하면 어색하더라고요ㅜ",
          "original_form": "눈썹이 짙은편이라 갈색하면 어색하더라고요ㅜ",
          "speaker_id": "1",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044499.1.16",
          "form": "아아 저도 회색이었는데",
          "original_form": "아아 저도 회색이었는데",
          "speaker_id": "2",
          "time": "20210907 23:01"
        },
        {
          "id": "MDRW2100044499.1.17",
          "form": "염색하셨어요?",
          "original_form": "염색하셨어요?",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044499.1.18",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044499.1.19",
          "form": "흑발이예요",
          "original_form": "흑발이예요",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100044499.1.20",
          "form": "그래서 진갈색으로 그리시는군요",
          "original_form": "그래서 진갈색으로 그리시는군요",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
