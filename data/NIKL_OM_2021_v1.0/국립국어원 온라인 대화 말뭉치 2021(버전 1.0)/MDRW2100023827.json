      "utterance": [
        {
          "id": "MDRW2100023827.1.1",
          "form": "한국 폴로엔 보이즈 니트가 없넴ㅠ",
          "original_form": "한국 폴로엔 보이즈 니트가 없넴ㅠ",
          "speaker_id": "2",
          "time": "20210830 18:21"
        },
        {
          "id": "MDRW2100023827.1.2",
          "form": "아 그래? 흠",
          "original_form": "아 그래? 흠",
          "speaker_id": "1",
          "time": "20210830 18:22"
        },
        {
          "id": "MDRW2100023827.1.3",
          "form": "응 데님셔츠도 없고 힝ㅠ 옥스포드 셔츠는 있어서 그거 살까싶기도 하고",
          "original_form": "응 데님셔츠도 없고 힝ㅠ 옥스포드 셔츠는 있어서 그거 살까싶기도 하고",
          "speaker_id": "2",
          "time": "20210830 18:24"
        },
        {
          "id": "MDRW2100023827.1.4",
          "form": "요즘 폴로에 꽂혔나보구나 ㅋㅋㅋ",
          "original_form": "요즘 폴로에 꽂혔나보구나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:25"
        },
        {
          "id": "MDRW2100023827.1.5",
          "form": "첨엔 데님셔츠 알아보다가 니트 이뻐서 꽂혔는데 니트가 읎어ㅋㅋ",
          "original_form": "첨엔 데님셔츠 알아보다가 니트 이뻐서 꽂혔는데 니트가 읎어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:34"
        },
        {
          "id": "MDRW2100023827.1.6",
          "form": "옷 사기 힘드네",
          "original_form": "옷 사기 힘드네",
          "speaker_id": "1",
          "time": "20210830 18:39"
        },
        {
          "id": "MDRW2100023827.1.7",
          "form": "그니까 돈주고 사겠다는데도 옷이 읎어ㅋㅋ",
          "original_form": "그니까 돈주고 사겠다는데도 옷이 읎어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100023827.1.8",
          "form": "더 비싼돈을 주고 사라 이거지 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "더 비싼돈을 주고 사라 이거지 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100023827.1.9",
          "form": "그런 숨은뜻 따위 알고싶지않은데ㅋㅋㅋㅋㅋ",
          "original_form": "그런 숨은뜻 따위 알고싶지않은데ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100023827.1.10",
          "form": "자본주의 사회란 그런 것 ㅜㅜ 거기서 아둥바둥 싸게 사야 하는 숙명",
          "original_form": "자본주의 사회란 그런 것 ㅜㅜ 거기서 아둥바둥 싸게 사야 하는 숙명",
          "speaker_id": "1",
          "time": "20210830 18:43"
        },
        {
          "id": "MDRW2100023827.1.11",
          "form": "참내 폴로는 왜 직구를 막은건지...얄밉네",
          "original_form": "참내 폴로는 왜 직구를 막은건지...얄밉네",
          "speaker_id": "2",
          "time": "20210830 18:45"
        },
        {
          "id": "MDRW2100023827.1.12",
          "form": "메이시스도 엄청 취소 잘되더라 어떤 사람 스무번을 취소당했다곸ㅋㅋㅋ",
          "original_form": "메이시스도 엄청 취소 잘되더라 어떤 사람 스무번을 취소당했다곸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100023827.1.13",
          "form": "스무번이나 도전했다는거냐ㅋㅋ",
          "original_form": "스무번이나 도전했다는거냐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100023827.1.14",
          "form": "그것도 아마 계정 바꾸고 주소 바꾸고 카드 바꾸고 난리 일걸 ㅋㅋㅋ",
          "original_form": "그것도 아마 계정 바꾸고 주소 바꾸고 카드 바꾸고 난리 일걸 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100023827.1.15",
          "form": "아이고ㅋㅋㅋ속시끄릅다ㅋㅋㅋ",
          "original_form": "아이고ㅋㅋㅋ속시끄릅다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023827.1.16",
          "form": "쇼핑 힘드네ㅋㅋ",
          "original_form": "쇼핑 힘드네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:34"
        },
        {
          "id": "MDRW2100023827.1.17",
          "form": "쇼핑이 보통 일이 아니여 ㅋㅋ",
          "original_form": "쇼핑이 보통 일이 아니여 ㅋㅋ",
