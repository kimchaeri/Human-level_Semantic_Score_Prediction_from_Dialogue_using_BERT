      "utterance": [
        {
          "id": "MDRW2100014365.1.1",
          "form": "싸이월드가 부활하면",
          "original_form": "싸이월드가 부활하면",
          "speaker_id": "1",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014365.1.2",
          "form": "진짜 재미있겠다",
          "original_form": "진짜 재미있겠다",
          "speaker_id": "1",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014365.1.3",
          "form": "누가그래",
          "original_form": "누가그래",
          "speaker_id": "2",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014365.1.4",
          "form": "아무도 그런생각안할듯..",
          "original_form": "아무도 그런생각안할듯..",
          "speaker_id": "2",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014365.1.5",
          "form": "내가쓴도토리가 몇갠데 노래아까워..",
          "original_form": "내가쓴도토리가 몇갠데 노래아까워..",
          "speaker_id": "2",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014365.1.6",
          "form": "그래도 미니홈 꾸미는건 재밌었어",
          "original_form": "그래도 미니홈 꾸미는건 재밌었어",
          "speaker_id": "2",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014365.1.7",
          "form": "인스타 죽어가니까",
          "original_form": "인스타 죽어가니까",
          "speaker_id": "1",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014365.1.8",
          "form": "보완해서 나오면 좋겠는데",
          "original_form": "보완해서 나오면 좋겠는데",
          "speaker_id": "1",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014365.1.9",
          "form": "맞아 일촌평이랑",
          "original_form": "맞아 일촌평이랑",
          "speaker_id": "1",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014365.1.10",
          "form": "폐쇄성이 짙으면 잘 안되는거같아",
          "original_form": "폐쇄성이 짙으면 잘 안되는거같아",
          "speaker_id": "2",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014365.1.11",
          "form": "반짝하고 마는..?",
          "original_form": "반짝하고 마는..?",
          "speaker_id": "2",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014365.1.12",
          "form": "유입이 많아야돼",
          "original_form": "유입이 많아야돼",
          "speaker_id": "2",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014365.1.13",
          "form": "다시 부활하면 도토리도 다시 사야지",
          "original_form": "다시 부활하면 도토리도 다시 사야지",
          "speaker_id": "1",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014365.1.14",
          "form": "돈아끼자",
          "original_form": "돈아끼자",
          "speaker_id": "2",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014365.1.15",
          "form": "비지엠 이노래들으면전화해 해놓는다",
          "original_form": "비지엠 이노래들으면전화해 해놓는다",
          "speaker_id": "1",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014365.1.16",
          "form": "정말싫네요",
          "original_form": "정말싫네요",
          "speaker_id": "2",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014365.1.17",
          "form": "몽환의숲 ...이에게",
          "original_form": "몽환의숲 ...이에게",
          "speaker_id": "2",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014365.1.18",
          "form": "ㅋㅋㅋ이에겤ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ이에겤ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014365.1.19",
          "form": "시부야케이...이런게 대세였다고",
          "original_form": "시부야케이...이런게 대세였다고",
          "speaker_id": "2",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014365.1.20",
          "form": "프리스타일 와이가 진리다",
          "original_form": "프리스타일 와이가 진리다",
          "speaker_id": "1",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014365.1.21",
          "form": "시부야케이는 처음듣는데",
          "original_form": "시부야케이는 처음듣는데",
          "speaker_id": "1",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014365.1.22",
          "form": "프리템포 누자베스",
          "original_form": "프리템포 누자베스",
          "speaker_id": "2",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014365.1.23",
          "form": "한 장르야",
          "original_form": "한 장르야",
          "speaker_id": "2",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014365.1.24",
          "form": "시부야케이 ..",
          "original_form": "시부야케이 ..",
          "speaker_id": "2",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014365.1.25",
          "form": "두개씩 보내지말아주라",
          "original_form": "두개씩 보내지말아주라",
          "speaker_id": "1",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014365.1.26",
          "form": "그때가 그나마 일본전성기라",
          "original_form": "그때가 그나마 일본전성기라",
          "speaker_id": "2",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014365.1.27",
          "form": "아니난 하나만 보냈는데",
          "original_form": "아니난 하나만 보냈는데",
          "speaker_id": "2",
          "time": "20210825 18:43"
        }
      ]
    }
  ]
