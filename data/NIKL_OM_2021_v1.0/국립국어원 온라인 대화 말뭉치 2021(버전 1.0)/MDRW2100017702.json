      "utterance": [
        {
          "id": "MDRW2100017702.1.1",
          "form": "근데 넌 언제 결혼할거니",
          "original_form": "근데 넌 언제 결혼할거니",
          "speaker_id": "2",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017702.1.2",
          "form": "때되면요",
          "original_form": "때되면요",
          "speaker_id": "1",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017702.1.3",
          "form": "때가 언젠데",
          "original_form": "때가 언젠데",
          "speaker_id": "2",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017702.1.4",
          "form": "좋은사람이 나타나면요",
          "original_form": "좋은사람이 나타나면요",
          "speaker_id": "1",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017702.1.5",
          "form": "어떤 사람이 좋은 사람이니",
          "original_form": "어떤 사람이 좋은 사람이니",
          "speaker_id": "2",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017702.1.6",
          "form": "제 맘에 드는 사람이요",
          "original_form": "제 맘에 드는 사람이요",
          "speaker_id": "1",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017702.1.7",
          "form": "엄마가 소개해준 사람들은 다 별로야?",
          "original_form": "엄마가 소개해준 사람들은 다 별로야?",
          "speaker_id": "2",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017702.1.8",
          "form": "훌륭한 분들이신데 그냥 끌리지 않네요",
          "original_form": "훌륭한 분들이신데 그냥 끌리지 않네요",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017702.1.9",
          "form": "아니 남자가 거기서 거기지.. 뭘 끌리고 말고야",
          "original_form": "아니 남자가 거기서 거기지.. 뭘 끌리고 말고야",
          "speaker_id": "2",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017702.1.10",
          "form": "거기서 거기니까 끌리는 사람이랑 해야죠",
          "original_form": "거기서 거기니까 끌리는 사람이랑 해야죠",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017702.1.11",
          "form": "엄마는 네가 드레스 입는 모습을 보고 싶어",
          "original_form": "엄마는 네가 드레스 입는 모습을 보고 싶어",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017702.1.12",
          "form": "저도 드레스 입고 싶어요",
          "original_form": "저도 드레스 입고 싶어요",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017702.1.13",
          "form": "그럼 노력을 해야하지 안겠니?",
          "original_form": "그럼 노력을 해야하지 안겠니?",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017702.1.14",
          "form": "그래서 살빼려고 하고 있죠",
          "original_form": "그래서 살빼려고 하고 있죠",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017702.1.15",
          "form": "그것도 좋은데 일단 사람을 만나야 하잖아..",
          "original_form": "그것도 좋은데 일단 사람을 만나야 하잖아..",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017702.1.16",
          "form": "이시국이라 만나기 시른데",
          "original_form": "이시국이라 만나기 시른데",
          "speaker_id": "1",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017702.1.17",
          "form": "그래도 엄마 주변은 다들 시집장가 잘만 가더라",
          "original_form": "그래도 엄마 주변은 다들 시집장가 잘만 가더라",
          "speaker_id": "2",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017702.1.18",
          "form": "갈 사람은 가겠죠",
          "original_form": "갈 사람은 가겠죠",
          "speaker_id": "1",
          "time": "20210826 20:46"
        }
