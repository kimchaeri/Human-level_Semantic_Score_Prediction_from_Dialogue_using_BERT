      "utterance": [
        {
          "id": "MDRW2100033690.1.1",
          "form": "저 케이크요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 케이크요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100033690.1.2",
          "form": "스벅 자주 가시나요?",
          "original_form": "스벅 자주 가시나요?",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100033690.1.3",
          "form": "스벅 자주 가죠!",
          "original_form": "스벅 자주 가죠!",
          "speaker_id": "1",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100033690.1.4",
          "form": "스벅 바질토마토 그거 드셔봤나용",
          "original_form": "스벅 바질토마토 그거 드셔봤나용",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100033690.1.5",
          "form": "베이글이요!!",
          "original_form": "베이글이요!!",
          "speaker_id": "2",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100033690.1.6",
          "form": "헉 아뇨 전 스벅에서 음료랑 케이크 밖에 안 먹는 거 같아요!ㅋㅋㅋ",
          "original_form": "헉 아뇨 전 스벅에서 음료랑 케이크 밖에 안 먹는 거 같아요!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100033690.1.7",
          "form": "아하 바질 좋아하시면 먹어보세요",
          "original_form": "아하 바질 좋아하시면 먹어보세요",
          "speaker_id": "2",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100033690.1.8",
          "form": "맛있어요 ㅎ",
          "original_form": "맛있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100033690.1.9",
          "form": "바질토마토가 베이글 위에 올라간 건가요??",
          "original_form": "바질토마토가 베이글 위에 올라간 건가요??",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100033690.1.10",
          "form": "아뇨 바질하고 토마토 소스 뭐이렇게 섞인건데",
          "original_form": "아뇨 바질하고 토마토 소스 뭐이렇게 섞인건데",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100033690.1.11",
          "form": "전 완전 맛있게 먹어용",
          "original_form": "전 완전 맛있게 먹어용",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100033690.1.12",
          "form": "아하 추천 감사해요 한 번 먹어볼게요!",
          "original_form": "아하 추천 감사해요 한 번 먹어볼게요!",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100033690.1.13",
          "form": "네 맛있어요ㅋㅋㅋㅋㅋ",
          "original_form": "네 맛있어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:07"
        },
        {
          "id": "MDRW2100033690.1.14",
          "form": "저는 카페가면 케이크를 주로 먹어요",
          "original_form": "저는 카페가면 케이크를 주로 먹어요",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033690.1.15",
          "form": "어떤 케이크 드세요?",
          "original_form": "어떤 케이크 드세요?",
          "speaker_id": "2",
          "time": "20210824 23:10"
        },
        {
          "id": "MDRW2100033690.1.16",
          "form": "투썸에서는 아이스박스랑 딸기레어치즈를 주로 먹어요!",
          "original_form": "투썸에서는 아이스박스랑 딸기레어치즈를 주로 먹어요!",
          "speaker_id": "1",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100033690.1.17",
          "form": "아이스박스 최고죠 ㅠㅠㅠㅠㅠㅠ",
          "original_form": "아이스박스 최고죠 ㅠㅠㅠㅠㅠㅠ",
