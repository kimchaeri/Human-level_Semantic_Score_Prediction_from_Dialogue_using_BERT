        {
          "id": "MDRW2100007700.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210822 09:47"
        },
        {
          "id": "MDRW2100007700.1.2",
          "form": "아니요ㅠ",
          "original_form": "아니요ㅠ",
          "speaker_id": "2",
          "time": "20210822 09:50"
        },
        {
          "id": "MDRW2100007700.1.3",
          "form": "키우세요?",
          "original_form": "키우세요?",
          "speaker_id": "2",
          "time": "20210822 09:50"
        },
        {
          "id": "MDRW2100007700.1.4",
          "form": "저동 아직",
          "original_form": "저동 아직",
          "speaker_id": "1",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007700.1.5",
          "form": "엇.. 그러시구나",
          "original_form": "엇.. 그러시구나",
          "speaker_id": "2",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007700.1.6",
          "form": "강아지 키우고싶아요",
          "original_form": "강아지 키우고싶아요",
          "speaker_id": "1",
          "time": "20210822 09:53"
        },
        {
          "id": "MDRW2100007700.1.7",
          "form": "좋죠!!",
          "original_form": "좋죠!!",
          "speaker_id": "2",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007700.1.8",
          "form": "어떤 강아지 좋아하시나요?",
          "original_form": "어떤 강아지 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007700.1.9",
          "form": "강아지면 다",
          "original_form": "강아지면 다",
          "speaker_id": "1",
          "time": "20210822 09:56"
        },
        {
          "id": "MDRW2100007700.1.10",
          "form": "지금 동물농장 보고있어요ㅎㅎ",
          "original_form": "지금 동물농장 보고있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 09:58"
        },
        {
          "id": "MDRW2100007700.1.11",
          "form": "아직해요?",
          "original_form": "아직해요?",
          "speaker_id": "1",
          "time": "20210822 09:59"
        },
        {
          "id": "MDRW2100007700.1.12",
          "form": "넵ㅋㅋㅋ아직해요",
          "original_form": "넵ㅋㅋㅋ아직해요",
          "speaker_id": "2",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007700.1.13",
          "form": "동물 보니까 좋아요",
          "original_form": "동물 보니까 좋아요",
          "speaker_id": "2",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007700.1.14",
          "form": "오랴하네요",
          "original_form": "오랴하네요",
          "speaker_id": "1",
          "time": "20210822 10:02"
        },
        {
          "id": "MDRW2100007700.1.15",
          "form": "오래",
          "original_form": "오래",
          "speaker_id": "1",
          "time": "20210822 10:03"
        },
        {
          "id": "MDRW2100007700.1.16",
          "form": "동물농장 보면 동물 키우고 싶어요",
          "original_form": "동물농장 보면 동물 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210822 10:05"
        },
        {
          "id": "MDRW2100007700.1.17",
          "form": "보는거랑 키우는 거 달라서 ㅋㅋ",
