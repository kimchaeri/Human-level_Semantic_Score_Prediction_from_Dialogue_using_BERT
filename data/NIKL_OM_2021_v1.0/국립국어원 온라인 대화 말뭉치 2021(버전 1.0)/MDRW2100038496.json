        },
        {
          "id": "MDRW2100038496.1.2",
          "form": "그렇겠지",
          "original_form": "그렇겠지",
          "speaker_id": "2",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100038496.1.3",
          "form": "왜 그렇지 ..ㅋ",
          "original_form": "왜 그렇지 ..ㅋ",
          "speaker_id": "1",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100038496.1.4",
          "form": "보는 사람이 많으니까",
          "original_form": "보는 사람이 많으니까",
          "speaker_id": "2",
          "time": "20210902 14:38"
        },
        {
          "id": "MDRW2100038496.1.5",
          "form": "오 맞는말이군 ㅋㅋ",
          "original_form": "오 맞는말이군 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:39"
        },
        {
          "id": "MDRW2100038496.1.6",
          "form": "나도 ㅋㅋ 딱히 재미는 모르겠다",
          "original_form": "나도 ㅋㅋ 딱히 재미는 모르겠다",
          "speaker_id": "1",
          "time": "20210902 14:39"
        },
        {
          "id": "MDRW2100038496.1.7",
          "form": "난 별로 안 보는데",
          "original_form": "난 별로 안 보는데",
          "speaker_id": "2",
          "time": "20210902 14:39"
        },
        {
          "id": "MDRW2100038496.1.8",
          "form": "웅웅 쯔양은 근데 보니까 잼나더라 ㅋ",
          "original_form": "웅웅 쯔양은 근데 보니까 잼나더라 ㅋ",
          "speaker_id": "1",
          "time": "20210902 14:39"
        },
        {
          "id": "MDRW2100038496.1.9",
          "form": "그래 쯔양 잘 먹더라",
          "original_form": "그래 쯔양 잘 먹더라",
          "speaker_id": "2",
          "time": "20210902 14:39"
        },
        {
          "id": "MDRW2100038496.1.10",
          "form": "어떻게 그렇게 먹지 신기해",
          "original_form": "어떻게 그렇게 먹지 신기해",
          "speaker_id": "1",
          "time": "20210902 14:39"
        },
        {
          "id": "MDRW2100038496.1.11",
          "form": "위가 튼튼한가 보다",
          "original_form": "위가 튼튼한가 보다",
          "speaker_id": "2",
          "time": "20210902 14:40"
        },
        {
          "id": "MDRW2100038496.1.12",
          "form": "그런듯.. 부럽다",
          "original_form": "그런듯.. 부럽다",
