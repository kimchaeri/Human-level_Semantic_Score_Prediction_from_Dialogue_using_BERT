      "utterance": [
        {
          "id": "MDRW2100015671.1.1",
          "form": "어릴때 연예인 되고싶다고 생각한적있어요?ㅎ",
          "original_form": "어릴때 연예인 되고싶다고 생각한적있어요?ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:08"
        },
        {
          "id": "MDRW2100015671.1.2",
          "form": "아뇨  ㅎㅎㅎ",
          "original_form": "아뇨  ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 00:09"
        },
        {
          "id": "MDRW2100015671.1.3",
          "form": "어릴땐 엄청 조용하던 아이였어요",
          "original_form": "어릴땐 엄청 조용하던 아이였어요",
          "speaker_id": "1",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015671.1.4",
          "form": "아ㅎㅎ",
          "original_form": "아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015671.1.5",
          "form": "성격이 내성적?",
          "original_form": "성격이 내성적?",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015671.1.6",
          "form": "연예인하려면 조용하면안되긴하죠ㄹㄹ",
          "original_form": "연예인하려면 조용하면안되긴하죠ㄹㄹ",
          "speaker_id": "2",
          "time": "20210826 00:10"
        },
        {
          "id": "MDRW2100015671.1.7",
          "form": "성격이  커서 바꼇어요",
          "original_form": "성격이  커서 바꼇어요",
          "speaker_id": "1",
          "time": "20210826 00:12"
        },
        {
          "id": "MDRW2100015671.1.8",
          "form": "근데 ㅋㅋ 커보니 연예인는 못될상임 ㅋㅋㅋ",
          "original_form": "근데 ㅋㅋ 커보니 연예인는 못될상임 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:13"
        },
        {
          "id": "MDRW2100015671.1.9",
          "form": "ㅎㅎ 어릴때 한번쯤,그런생각하잠ㅎ아요ㅎ",
          "original_form": "ㅎㅎ 어릴때 한번쯤,그런생각하잠ㅎ아요ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015671.1.10",
          "form": "티비에 나오고 싶다고ㅎㅎㅎ 가수되고싶다고ㅎㅎㅎ",
          "original_form": "티비에 나오고 싶다고ㅎㅎㅎ 가수되고싶다고ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015671.1.11",
          "form": "원래는 내성적이셨던거에요?ㅎㅎㅎ",
          "original_form": "원래는 내성적이셨던거에요?ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:14"
        },
        {
          "id": "MDRW2100015671.1.12",
          "form": "네넹 ㅎㅎ 그랬엇죠",
          "original_form": "네넹 ㅎㅎ 그랬엇죠",
          "speaker_id": "1",
          "time": "20210826 00:16"
        },
        {
          "id": "MDRW2100015671.1.13",
          "form": "지금도 낯 잘가려욥 ㅋㅋ",
          "original_form": "지금도 낯 잘가려욥 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015671.1.14",
          "form": "저도 내성적임ㅎㅎㅎ",
          "original_form": "저도 내성적임ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015671.1.15",
          "form": "지금도 헤어샾 가면 쭈글이ㅎㅎㅎㅎㅎㅎ",
          "original_form": "지금도 헤어샾 가면 쭈글이ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:17"
        },
        {
          "id": "MDRW2100015671.1.16",
          "form": "근데 사회생활하면서 어쩔수없이 바뀐케이스에요ㅠ",
          "original_form": "근데 사회생활하면서 어쩔수없이 바뀐케이스에요ㅠ",
          "speaker_id": "2",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015671.1.17",
          "form": "그러시구낭  ~~",
          "original_form": "그러시구낭  ~~",
          "speaker_id": "1",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015671.1.18",
          "form": "슬기로운 의사생활에서 곰있잖아요ㅎ",
          "original_form": "슬기로운 의사생활에서 곰있잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015671.1.19",
          "form": "아네넵",
          "original_form": "아네넵",
          "speaker_id": "1",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015671.1.20",
          "form": "그보다 더 심해요ㅎ",
          "original_form": "그보다 더 심해요ㅎ",
          "speaker_id": "2",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015671.1.21",
          "form": "엘베 같이타면 어색하고",
          "original_form": "엘베 같이타면 어색하고",
          "speaker_id": "2",
          "time": "20210826 00:18"
        },
        {
          "id": "MDRW2100015671.1.22",
          "form": "친하지 않는 사람이랑 밥같이 먹으면 불편해요ㅎㅎ",
          "original_form": "친하지 않는 사람이랑 밥같이 먹으면 불편해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 00:19"
        },
        {
          "id": "MDRW2100015671.1.23",
          "form": "아항 근데 매력있는데",
          "original_form": "아항 근데 매력있는데",
