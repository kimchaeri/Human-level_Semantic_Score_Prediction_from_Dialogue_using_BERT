      "utterance": [
        {
          "id": "MDRW2100041542.1.1",
          "form": "비염 수술하는법도 있던데ㅜ",
          "original_form": "비염 수술하는법도 있던데ㅜ",
          "speaker_id": "1",
          "time": "20210906 09:52"
        },
        {
          "id": "MDRW2100041542.1.2",
          "form": "그렇죠ㅜㅜ",
          "original_form": "그렇죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 12:06"
        },
        {
          "id": "MDRW2100041542.1.3",
          "form": "비염수술하면 또 비염안생길까요?ㅜ",
          "original_form": "비염수술하면 또 비염안생길까요?ㅜ",
          "speaker_id": "2",
          "time": "20210906 12:06"
        },
        {
          "id": "MDRW2100041542.1.4",
          "form": "그렇지않을까요..??재발하면 넘 억울할것같네요...",
          "original_form": "그렇지않을까요..??재발하면 넘 억울할것같네요...",
          "speaker_id": "1",
          "time": "20210906 12:49"
        },
        {
          "id": "MDRW2100041542.1.5",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 12:49"
        },
        {
          "id": "MDRW2100041542.1.6",
          "form": "아휴 참 비염도 비염이지만. 요즘에는 마스크 때문에 턱드름이 ㅠㅠ",
          "original_form": "아휴 참 비염도 비염이지만. 요즘에는 마스크 때문에 턱드름이 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 12:50"
        },
        {
          "id": "MDRW2100041542.1.7",
          "form": "괜찮으신가요?",
          "original_form": "괜찮으신가요?",
          "speaker_id": "2",
          "time": "20210906 12:50"
        },
        {
          "id": "MDRW2100041542.1.8",
          "form": "저도 턱드름 심했었는데 요즘 싹 사라졌네여!!",
          "original_form": "저도 턱드름 심했었는데 요즘 싹 사라졌네여!!",
          "speaker_id": "1",
          "time": "20210906 12:51"
        },
        {
          "id": "MDRW2100041542.1.9",
          "form": "얼굴 닦는 패드 하나 바꿨는데 피부가 조아졌어욤ㅋㅋ",
          "original_form": "얼굴 닦는 패드 하나 바꿨는데 피부가 조아졌어욤ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:51"
        },
        {
          "id": "MDRW2100041542.1.10",
          "form": "오 대박 ㅋㅋ",
          "original_form": "오 대박 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:01"
        },
        {
          "id": "MDRW2100041542.1.11",
          "form": "추천좀 해주세요 ㅋ",
          "original_form": "추천좀 해주세요 ㅋ",
          "speaker_id": "2",
          "time": "20210906 13:01"
        },
        {
          "id": "MDRW2100041542.1.12",
          "form": "그게 뭐에요?",
          "original_form": "그게 뭐에요?",
          "speaker_id": "2",
          "time": "20210906 13:01"
        },
        {
          "id": "MDRW2100041542.1.13",
          "form": "유명한건데 아실수도..?",
          "original_form": "유명한건데 아실수도..?",
          "speaker_id": "1",
          "time": "20210906 13:03"
        },
        {
          "id": "MDRW2100041542.1.14",
          "form": "스트라이덱스요ㅋㅋㅋ",
          "original_form": "스트라이덱스요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:03"
        },
        {
          "id": "MDRW2100041542.1.15",
          "form": "처음 들었어요",
          "original_form": "처음 들었어요",
          "speaker_id": "2",
          "time": "20210906 13:21"
        },
        {
          "id": "MDRW2100041542.1.16",
          "form": "ㅋㅋㅋㅋ 검색해봤네요 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 검색해봤네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:21"
        },
        {
          "id": "MDRW2100041542.1.17",
          "form": "이거 저도 한번 써봐야되겠어요 ㅋㅋ",
          "original_form": "이거 저도 한번 써봐야되겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:22"
        },
        {
          "id": "MDRW2100041542.1.18",
          "form": "피부 평소에 예민하시면 일주일 한두번만 쓰시다가 괜찮다싶으면 늘려가세용ㅎㅎ",
          "original_form": "피부 평소에 예민하시면 일주일 한두번만 쓰시다가 괜찮다싶으면 늘려가세용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 13:22"
        },
        {
          "id": "MDRW2100041542.1.19",
          "form": "저는 강철피부라 빨간통으로 쓰네욬ㅋㅋㅋ",
          "original_form": "저는 강철피부라 빨간통으로 쓰네욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:22"
        },
        {
          "id": "MDRW2100041542.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ아 그러시군요 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 그러시군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:31"
        },
        {
          "id": "MDRW2100041542.1.21",
          "form": "저는 피부가 예민한 편이라 ㅋㅋ",
          "original_form": "저는 피부가 예민한 편이라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:31"
        },
        {
          "id": "MDRW2100041542.1.22",
          "form": "천천히 늘려가봐야되겠네요 ㅋㅋ",
          "original_form": "천천히 늘려가봐야되겠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:31"
        },
        {
          "id": "MDRW2100041542.1.23",
          "form": "ㅜㅜ마니 예민하시면 추천안드려요 흨..",
          "original_form": "ㅜㅜ마니 예민하시면 추천안드려요 흨..",
          "speaker_id": "1",
          "time": "20210906 13:38"
        },
        {
          "id": "MDRW2100041542.1.24",
          "form": "아하 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그런건가요 ㅋㅋ",
          "original_form": "아하 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그런건가요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 13:50"
        },
        {
          "id": "MDRW2100041542.1.25",
          "form": "ㅎㅎ혹시라도 피부 뒤집어지실까봐...걱정걱정",
          "original_form": "ㅎㅎ혹시라도 피부 뒤집어지실까봐...걱정걱정",
