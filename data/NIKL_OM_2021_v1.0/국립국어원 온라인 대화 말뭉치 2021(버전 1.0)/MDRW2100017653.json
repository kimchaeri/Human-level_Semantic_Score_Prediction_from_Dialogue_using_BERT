      "utterance": [
        {
          "id": "MDRW2100017653.1.1",
          "form": "저녁드셨나요?",
          "original_form": "저녁드셨나요?",
          "speaker_id": "1",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017653.1.2",
          "form": "네네 먹었어요",
          "original_form": "네네 먹었어요",
          "speaker_id": "2",
          "time": "20210826 20:34"
        },
        {
          "id": "MDRW2100017653.1.3",
          "form": "뭐드셨어요?",
          "original_form": "뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100017653.1.4",
          "form": "김치찌개 먹었어요",
          "original_form": "김치찌개 먹었어요",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017653.1.5",
          "form": "name1은 저녁 드셨나요?",
          "original_form": "&name1&은 저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210826 20:36"
        },
        {
          "id": "MDRW2100017653.1.6",
          "form": "네 먹었어요!",
          "original_form": "네 먹었어요!",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017653.1.7",
          "form": "어떤 거 드셨나요?",
          "original_form": "어떤 거 드셨나요?",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100017653.1.8",
          "form": "새우찜먹었어요",
          "original_form": "새우찜먹었어요",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017653.1.9",
          "form": "룸메가 백신을 맞아서 든든하게 먹을려고요",
          "original_form": "룸메가 백신을 맞아서 든든하게 먹을려고요",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017653.1.10",
          "form": "와 새우찜 대박",
          "original_form": "와 새우찜 대박",
          "speaker_id": "2",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017653.1.11",
          "form": "저 새우 좋아해요",
          "original_form": "저 새우 좋아해요",
          "speaker_id": "2",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017653.1.12",
          "form": "룸메 덕이려나요 ㅋㅋㅋ",
          "original_form": "룸메 덕이려나요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017653.1.13",
          "form": "앜ㅋㅋㅋㅋ덕이죠!",
          "original_form": "앜ㅋㅋㅋㅋ덕이죠!",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017653.1.14",
          "form": "새우 언제나 좋죠ㅠㅠ",
          "original_form": "새우 언제나 좋죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017653.1.15",
          "form": "뭘해먹어도 좋지않나요",
          "original_form": "뭘해먹어도 좋지않나요",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017653.1.16",
          "form": "알리오올리오 진짜 좋아해요",
          "original_form": "알리오올리오 진짜 좋아해요",
          "speaker_id": "2",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017653.1.17",
          "form": "근데 이건 새우 때문도 있고 마늘 때문도 있어요",
          "original_form": "근데 이건 새우 때문도 있고 마늘 때문도 있어요",
          "speaker_id": "2",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017653.1.18",
          "form": "헉 저도 알리오올리오 좋아해요",
          "original_form": "헉 저도 알리오올리오 좋아해요",
          "speaker_id": "1",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017653.1.19",
          "form": "오일파스타 너무 맛있죠",
          "original_form": "오일파스타 너무 맛있죠",
          "speaker_id": "1",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017653.1.20",
          "form": "간단하면서도요ㅠㅠㅠ",
          "original_form": "간단하면서도요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 20:41"
        },
        {
          "id": "MDRW2100017653.1.21",
          "form": "맞아요 근데 저는 그 맛을 못내겠더라고요",
          "original_form": "맞아요 근데 저는 그 맛을 못내겠더라고요",
          "speaker_id": "2",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017653.1.22",
          "form": "그냥 사서 먹어요 ㅋㅋㅋ",
          "original_form": "그냥 사서 먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 20:43"
        },
        {
          "id": "MDRW2100017653.1.23",
          "form": "집에서 내면 이상하게 맛이 덜나죠ㅋㅋㅋㅋㅋ",
          "original_form": "집에서 내면 이상하게 맛이 덜나죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017653.1.24",
          "form": "식당에서는 양파랑 굴소스로 어떤 조미료처럼 만들어쓴다고들 하는데",
          "original_form": "식당에서는 양파랑 굴소스로 어떤 조미료처럼 만들어쓴다고들 하는데",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017653.1.25",
          "form": "집에서 그러긴 안쉬우니까요ㅠ",
          "original_form": "집에서 그러긴 안쉬우니까요ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:44"
        }
      ]
    }
