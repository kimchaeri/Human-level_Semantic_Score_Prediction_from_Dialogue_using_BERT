          "speaker_id": "2",
          "time": "20210720 17:50"
        },
        {
          "id": "MDRW2100001032.1.3",
          "form": "오늘 32도인데",
          "original_form": "오늘 32도인데",
          "speaker_id": "2",
          "time": "20210720 17:50"
        },
        {
          "id": "MDRW2100001032.1.4",
          "form": "내일 36도래요",
          "original_form": "내일 36도래요",
          "speaker_id": "2",
          "time": "20210720 17:50"
        },
        {
          "id": "MDRW2100001032.1.5",
          "form": "진짜요?",
          "original_form": "진짜요?",
          "speaker_id": "1",
          "time": "20210720 17:50"
        },
        {
          "id": "MDRW2100001032.1.6",
          "form": "미쳤네요..",
          "original_form": "미쳤네요..",
          "speaker_id": "1",
          "time": "20210720 17:50"
        },
        {
          "id": "MDRW2100001032.1.7",
          "form": "사람을 녹이려고 작정했나봐요",
          "original_form": "사람을 녹이려고 작정했나봐요",
          "speaker_id": "2",
          "time": "20210720 17:50"
        },
        {
          "id": "MDRW2100001032.1.8",
          "form": "지구가",
          "original_form": "지구가",
          "speaker_id": "2",
          "time": "20210720 17:50"
        },
        {
          "id": "MDRW2100001032.1.9",
          "form": "비도 지멋대로 오잖아요",
          "original_form": "비도 지멋대로 오잖아요",
          "speaker_id": "1",
          "time": "20210720 17:50"
        },
        {
          "id": "MDRW2100001032.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210720 17:50"
        },
        {
          "id": "MDRW2100001032.1.11",
          "form": "우산을 맨날 들고다녀요",
          "original_form": "우산을 맨날 들고다녀요",
          "speaker_id": "1",
          "time": "20210720 17:50"
        },
        {
          "id": "MDRW2100001032.1.12",
          "form": "귀찮음",
          "original_form": "귀찮음",
          "speaker_id": "1",
          "time": "20210720 17:51"
        },
        {
          "id": "MDRW2100001032.1.13",
          "form": "기상청 날씨 맞춘적이 거의없어",
          "original_form": "기상청 날씨 맞춘적이 거의없어",
          "speaker_id": "2",
          "time": "20210720 17:51"
        },
        {
          "id": "MDRW2100001032.1.14",
          "form": "충격적인거",
          "original_form": "충격적인거",
          "speaker_id": "2",
          "time": "20210720 17:51"
        },
        {
          "id": "MDRW2100001032.1.15",
          "form": "뭔데요",
          "original_form": "뭔데요",
          "speaker_id": "1",
          "time": "20210720 17:51"
        },
        {
          "id": "MDRW2100001032.1.16",
          "form": "제 친구들은 어제 비안왔는데",
          "original_form": "제 친구들은 어제 비안왔는데",
          "speaker_id": "2",
          "time": "20210720 17:51"
        },
        {
          "id": "MDRW2100001032.1.17",
          "form": "지하철 타고 5분거리인곳에서는 폭우가 내림",
          "original_form": "지하철 타고 5분거리인곳에서는 폭우가 내림",
          "speaker_id": "2",
          "time": "20210720 17:51"
        },
        {
          "id": "MDRW2100001032.1.18",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210720 17:51"
        },
        {
          "id": "MDRW2100001032.1.19",
          "form": "평행세계에요",
