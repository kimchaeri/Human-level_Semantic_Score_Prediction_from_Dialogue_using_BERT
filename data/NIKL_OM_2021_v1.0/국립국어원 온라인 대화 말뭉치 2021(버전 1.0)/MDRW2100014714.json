      "utterance": [
        {
          "id": "MDRW2100014714.1.1",
          "form": "날씨가 많이 습하네요",
          "original_form": "날씨가 많이 습하네요",
          "speaker_id": "1",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014714.1.2",
          "form": "오늘은 비안와서 출근길에 편했어요",
          "original_form": "오늘은 비안와서 출근길에 편했어요",
          "speaker_id": "2",
          "time": "20210826 09:47"
        },
        {
          "id": "MDRW2100014714.1.3",
          "form": "날씨 좋으네요 ㅎㅎ",
          "original_form": "날씨 좋으네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 09:47"
        },
        {
          "id": "MDRW2100014714.1.4",
          "form": "네 오늘은 간만에 비가 안와서 쾌적한 기분이 들어요 ㅋㅋ",
          "original_form": "네 오늘은 간만에 비가 안와서 쾌적한 기분이 들어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:00"
        },
        {
          "id": "MDRW2100014714.1.5",
          "form": "비 피해 없으셧나용?",
          "original_form": "비 피해 없으셧나용?",
          "speaker_id": "2",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100014714.1.6",
          "form": "네 다행히 피해는 없었어요 ㅋㅋㅋ",
          "original_form": "네 다행히 피해는 없었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100014714.1.7",
          "form": "비 피해 있으셨어요?",
          "original_form": "비 피해 있으셨어요?",
          "speaker_id": "1",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100014714.1.8",
          "form": "다행이네요ㅎㅎ 여기도 별로 안와서 괜찮앗어요",
          "original_form": "다행이네요ㅎㅎ 여기도 별로 안와서 괜찮앗어요",
          "speaker_id": "2",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100014714.1.9",
          "form": "다른지역에는 비 피해 입은 곳도 있는것 같더라구요 ㅠㅠ",
          "original_form": "다른지역에는 비 피해 입은 곳도 있는것 같더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100014714.1.10",
          "form": "올해 생각보다 비가 많이 왔던 것 같아요",
          "original_form": "올해 생각보다 비가 많이 왔던 것 같아요",
          "speaker_id": "1",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100014714.1.11",
          "form": "다음주까진 비가 왓다갓다하니까 우산 꼭 챙겨다니세욥!",
          "original_form": "다음주까진 비가 왓다갓다하니까 우산 꼭 챙겨다니세욥!",
          "speaker_id": "2",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100014714.1.12",
          "form": "감사합니다 우산 꼭 챙겨다닐게요 ㅋㅋㅋㅋ",
          "original_form": "감사합니다 우산 꼭 챙겨다닐게요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100014714.1.13",
          "form": "날씨가 쨍하니 맑았다 흐렸다 해서 이제 진짜 우양산 사야겠어요 ㅋㅋㅋㅋㅋ",
          "original_form": "날씨가 쨍하니 맑았다 흐렸다 해서 이제 진짜 우양산 사야겠어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100014714.1.14",
          "form": "좋아하는 계절 잇으신가용?",
          "original_form": "좋아하는 계절 잇으신가용?",
          "speaker_id": "2",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100014714.1.15",
          "form": "저는 봄 좋아해요",
          "original_form": "저는 봄 좋아해요",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100014714.1.16",
          "form": "황사만 아니면ㅋㅋㅋㅋㅋ",
          "original_form": "황사만 아니면ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100014714.1.17",
          "form": "저는 더위에 쥐약이라 여름빼고 싹다 좋아요",
          "original_form": "저는 더위에 쥐약이라 여름빼고 싹다 좋아요",
          "speaker_id": "2",
          "time": "20210826 12:40"
        },
        {
          "id": "MDRW2100014714.1.18",
          "form": "ㅋㅋㅋㅋㅋ전 추위에 쥐약이라 겨울빼고 싹다 좋은데 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ전 추위에 쥐약이라 겨울빼고 싹다 좋은데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100014714.1.19",
          "form": "정반대네요 ㅋㅋㅋㅋㅋ",
          "original_form": "정반대네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:41"
        },
        {
          "id": "MDRW2100014714.1.20",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:42"
        },
        {
          "id": "MDRW2100014714.1.21",
          "form": "이렇게 반대일수가 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이렇게 반대일수가 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100014714.1.22",
          "form": "여긴 대프리카라서",
          "original_form": "여긴 대프리카라서",
          "speaker_id": "2",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100014714.1.23",
          "form": "더위 심각해요",
          "original_form": "더위 심각해요",
          "speaker_id": "2",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100014714.1.24",
          "form": "대구사시는구나",
          "original_form": "대구사시는구나",
          "speaker_id": "1",
          "time": "20210826 12:45"
        },
        {
          "id": "MDRW2100014714.1.25",
          "form": "대구 정말 더위 장난아니더라구요",
          "original_form": "대구 정말 더위 장난아니더라구요",
