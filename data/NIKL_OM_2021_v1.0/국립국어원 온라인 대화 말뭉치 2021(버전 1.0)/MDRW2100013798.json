      "utterance": [
        {
          "id": "MDRW2100013798.1.1",
          "form": "결혼 하셨나요? 전 미혼이에요",
          "original_form": "결혼 하셨나요? 전 미혼이에요",
          "speaker_id": "1",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100013798.1.2",
          "form": "저도 미혼이에요",
          "original_form": "저도 미혼이에요",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100013798.1.3",
          "form": "근데 얼마전 아프고 나서 몸 생각해서 다 해먹느라 힘들어요..ㅋㅋ",
          "original_form": "근데 얼마전 아프고 나서 몸 생각해서 다 해먹느라 힘들어요..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100013798.1.4",
          "form": "ㅋㅋ 요리!",
          "original_form": "ㅋㅋ 요리!",
          "speaker_id": "1",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100013798.1.5",
          "form": "본격적으로 해보니 쉽지않더라구요",
          "original_form": "본격적으로 해보니 쉽지않더라구요",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100013798.1.6",
          "form": "최근에 뭐해 드셨어요?",
          "original_form": "최근에 뭐해 드셨어요?",
          "speaker_id": "1",
          "time": "20210825 16:01"
        },
        {
          "id": "MDRW2100013798.1.7",
          "form": "장조림이요!",
          "original_form": "장조림이요!",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013798.1.8",
          "form": "보관하기 편하고 오래가서 ㅋㅋㅋ",
          "original_form": "보관하기 편하고 오래가서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013798.1.9",
          "form": "아 밥도둑! ㅠㅠ",
          "original_form": "아 밥도둑! ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013798.1.10",
          "form": "재료 뭐였어요? 돼지고기? 소고기? 닭고기?",
          "original_form": "재료 뭐였어요? 돼지고기? 소고기? 닭고기?",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013798.1.11",
          "form": "메추리알?",
          "original_form": "메추리알?",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013798.1.12",
          "form": "계란?",
          "original_form": "계란?",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013798.1.13",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013798.1.14",
          "form": "소고기+메추리알 장조림",
          "original_form": "소고기+메추리알 장조림",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013798.1.15",
          "form": "버터넣고 버터장조림밥 해서 무생채랑 먹었어",
          "original_form": "버터넣고 버터장조림밥 해서 무생채랑 먹었어",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013798.1.16",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013798.1.17",
          "form": "와 ㅠㅠㅠㅠ",
          "original_form": "와 ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013798.1.18",
          "form": "저는 요즘 잘 못해먹고 사먹네요",
          "original_form": "저는 요즘 잘 못해먹고 사먹네요",
          "speaker_id": "1",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013798.1.19",
          "form": "혼자 살면서 집안일 하는 거 정말 쉽지않죠ㅠ",
          "original_form": "혼자 살면서 집안일 하는 거 정말 쉽지않죠ㅠ",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013798.1.20",
          "form": "너무 안 건강하게 먹는 것 같아서 오늘은 샐러드라도 사먹으려고요",
          "original_form": "너무 안 건강하게 먹는 것 같아서 오늘은 샐러드라도 사먹으려고요",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013798.1.21",
          "form": "맞아요 그렇게라도 골고루 먹어줘야 해요 ㅋㅋ",
          "original_form": "맞아요 그렇게라도 골고루 먹어줘야 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013798.1.22",
          "form": "파리바게트 갈까 서브웨이 갈까 동네 새로 생긴 샐러드집 갈까 생각 중이에요",
          "original_form": "파리바게트 갈까 서브웨이 갈까 동네 새로 생긴 샐러드집 갈까 생각 중이에요",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013798.1.23",
          "form": "골라주세요! ㅋㅋ",
          "original_form": "골라주세요! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013798.1.24",
          "form": "새로 생긴 샐러드집 좋네요 ㅋㅋ",
          "original_form": "새로 생긴 샐러드집 좋네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013798.1.25",
          "form": "평소 집안일은 매일 하시는 편이에요?",
          "original_form": "평소 집안일은 매일 하시는 편이에요?",
          "speaker_id": "2",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013798.1.26",
          "form": "ㅋㅋㅋ 도전해보겠습니다",
          "original_form": "ㅋㅋㅋ 도전해보겠습니다",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013798.1.27",
          "form": "네 부지런히 해요",
          "original_form": "네 부지런히 해요",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013798.1.28",
          "form": "루틴이 있어요 ㅋㅋ",
          "original_form": "루틴이 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013798.1.29",
          "form": "오 부지런하시구나",
          "original_form": "오 부지런하시구나",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013798.1.30",
          "form": "전 빨래랑 청소만 간신히 해요",
          "original_form": "전 빨래랑 청소만 간신히 해요",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013798.1.31",
          "form": "화장실 깨끗하게 유지하기가 젤 어렵더라구요 ㅋㅋ",
          "original_form": "화장실 깨끗하게 유지하기가 젤 어렵더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:09"
        }
      ]
