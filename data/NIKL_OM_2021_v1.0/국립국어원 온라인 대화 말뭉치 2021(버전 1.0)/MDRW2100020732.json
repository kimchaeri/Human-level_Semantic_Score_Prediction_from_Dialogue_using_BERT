      "utterance": [
        {
          "id": "MDRW2100020732.1.1",
          "form": "저번에 무라벨 생수 산거 왔는데 넘 좋아요",
          "original_form": "저번에 무라벨 생수 산거 왔는데 넘 좋아요",
          "speaker_id": "2",
          "time": "20210828 11:49"
        },
        {
          "id": "MDRW2100020732.1.2",
          "form": "라벨 없는 생수가 나오는 시대라니..",
          "original_form": "라벨 없는 생수가 나오는 시대라니..",
          "speaker_id": "2",
          "time": "20210828 11:49"
        },
        {
          "id": "MDRW2100020732.1.3",
          "form": "그거 너무 편하죠 분리수거할때마다 라벨 벗ㅇ기는거 너무 귀찮아요 ㅜㅜ",
          "original_form": "그거 너무 편하죠 분리수거할때마다 라벨 벗ㅇ기는거 너무 귀찮아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 11:50"
        },
        {
          "id": "MDRW2100020732.1.4",
          "form": "맞아요 ㅎㅎ 그리고 저 어제 순대도 샀어요",
          "original_form": "맞아요 ㅎㅎ 그리고 저 어제 순대도 샀어요",
          "speaker_id": "2",
          "time": "20210828 11:51"
        },
        {
          "id": "MDRW2100020732.1.5",
          "form": "병천순대도 인터넷에 팔더라구요??",
          "original_form": "병천순대도 인터넷에 팔더라구요??",
          "speaker_id": "2",
          "time": "20210828 11:52"
        },
        {
          "id": "MDRW2100020732.1.6",
          "form": "요즘 인터넷에는 없는게 없더라구요 순대 떡볶이랑 먹고싶네요!",
          "original_form": "요즘 인터넷에는 없는게 없더라구요 순대 떡볶이랑 먹고싶네요!",
          "speaker_id": "1",
          "time": "20210828 11:52"
        },
        {
          "id": "MDRW2100020732.1.7",
          "form": "ㅋㅋㅋㅋ오 그거 좋은데요?",
          "original_form": "ㅋㅋㅋㅋ오 그거 좋은데요?",
          "speaker_id": "2",
          "time": "20210828 11:52"
        },
        {
          "id": "MDRW2100020732.1.8",
          "form": "전 순대국밥 만들어먹으려구요",
          "original_form": "전 순대국밥 만들어먹으려구요",
          "speaker_id": "2",
          "time": "20210828 11:53"
        },
        {
          "id": "MDRW2100020732.1.9",
          "form": "순대국밥 대박 육수를 어떻게 내시나요?",
          "original_form": "순대국밥 대박 육수를 어떻게 내시나요?",
          "speaker_id": "1",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100020732.1.10",
          "form": "육수는........ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "육수는........ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100020732.1.11",
          "form": "대기업의 맛으로 해결..",
          "original_form": "대기업의 맛으로 해결..",
          "speaker_id": "2",
          "time": "20210828 11:54"
        },
        {
          "id": "MDRW2100020732.1.12",
          "form": "아... ㅋㅋㅋ 시중에 파는 사골육수 쓰면되나요?",
          "original_form": "아... ㅋㅋㅋ 시중에 파는 사골육수 쓰면되나요?",
          "speaker_id": "1",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100020732.1.13",
          "form": "네 맞아요 한봉지에 천원정도 하는거 ㅋㅋ",
          "original_form": "네 맞아요 한봉지에 천원정도 하는거 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100020732.1.14",
          "form": "진짜 시중에 파는 국밥맛이 나요",
          "original_form": "진짜 시중에 파는 국밥맛이 나요",
          "speaker_id": "2",
          "time": "20210828 11:56"
        },
        {
          "id": "MDRW2100020732.1.15",
          "form": "와우 대박이네요 요즘엔 진짜 외식 안해도 되겠어요",
          "original_form": "와우 대박이네요 요즘엔 진짜 외식 안해도 되겠어요",
          "speaker_id": "1",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020732.1.16",
          "form": "그래도 남이 해준게 더 맛있어요...",
          "original_form": "그래도 남이 해준게 더 맛있어요...",
          "speaker_id": "2",
          "time": "20210828 11:58"
        },
        {
          "id": "MDRW2100020732.1.17",
          "form": "다 좋은데 설거지가... 귀찮네요",
          "original_form": "다 좋은데 설거지가... 귀찮네요",
          "speaker_id": "2",
          "time": "20210828 11:58"
        },
        {
          "id": "MDRW2100020732.1.18",
          "form": "아... 설거지... 저 사골 육수 기름기가 장난아니더라구요",
          "original_form": "아... 설거지... 저 사골 육수 기름기가 장난아니더라구요",
          "speaker_id": "1",
          "time": "20210828 11:59"
        },
        {
          "id": "MDRW2100020732.1.19",
          "form": "엥 그래요?",
          "original_form": "엥 그래요?",
          "speaker_id": "2",
          "time": "20210828 12:00"
        },
        {
          "id": "MDRW2100020732.1.20",
          "form": "맨날 남김없이 다 먹어서 몰랐네요",
          "original_form": "맨날 남김없이 다 먹어서 몰랐네요",
          "speaker_id": "2",
          "time": "20210828 12:00"
        },
        {
          "id": "MDRW2100020732.1.21",
          "form": "다 먹고나서 설거지 할때요 기름기가 많아서ㅜ",
          "original_form": "다 먹고나서 설거지 할때요 기름기가 많아서ㅜ",
          "speaker_id": "1",
          "time": "20210828 12:01"
        },
        {
          "id": "MDRW2100020732.1.22",
          "form": "항상 뜨거운물로 설거지해서 잘 몰랐네용",
          "original_form": "항상 뜨거운물로 설거지해서 잘 몰랐네용",
          "speaker_id": "2",
          "time": "20210828 12:02"
        }
      ]
    }
