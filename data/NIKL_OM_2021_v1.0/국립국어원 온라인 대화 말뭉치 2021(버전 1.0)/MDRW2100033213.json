      "utterance": [
        {
          "id": "MDRW2100033213.1.1",
          "form": "계모임 같은거하시나요",
          "original_form": "계모임 같은거하시나요",
          "speaker_id": "2",
          "time": "20210907 19:26"
        },
        {
          "id": "MDRW2100033213.1.2",
          "form": "고등학교 친구들과 하는 모임만 있어요",
          "original_form": "고등학교 친구들과 하는 모임만 있어요",
          "speaker_id": "1",
          "time": "20210907 19:29"
        },
        {
          "id": "MDRW2100033213.1.3",
          "form": "아 동창회 비슷한건가요",
          "original_form": "아 동창회 비슷한건가요",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100033213.1.4",
          "form": "그런가요 오래된 모임 이네요",
          "original_form": "그런가요 오래된 모임 이네요",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100033213.1.5",
          "form": "와 끈끈하겠네요",
          "original_form": "와 끈끈하겠네요",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100033213.1.6",
          "form": "가끔 놀러도가시나요",
          "original_form": "가끔 놀러도가시나요",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100033213.1.7",
          "form": "최근에 제주도를 다녀 왔어요",
          "original_form": "최근에 제주도를 다녀 왔어요",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100033213.1.8",
          "form": "어떤 모임을 하고 계신가요",
          "original_form": "어떤 모임을 하고 계신가요",
          "speaker_id": "1",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100033213.1.9",
          "form": "저는 특별한 모임은 없고",
          "original_form": "저는 특별한 모임은 없고",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100033213.1.10",
          "form": "예전에 일본어스터디같은건했어요",
          "original_form": "예전에 일본어스터디같은건했어요",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100033213.1.11",
          "form": "스터디 했던 분들 만나시나요",
          "original_form": "스터디 했던 분들 만나시나요",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100033213.1.12",
          "form": "아니요ㅋㅋ말그대로 공부하는 관계라",
          "original_form": "아니요ㅋㅋ말그대로 공부하는 관계라",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100033213.1.13",
          "form": "당시에도 업무적 연락밖에 안했네요",
          "original_form": "당시에도 업무적 연락밖에 안했네요",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100033213.1.14",
          "form": "친구끼리 제주도 너무 좋았겠어요",
          "original_form": "친구끼리 제주도 너무 좋았겠어요",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100033213.1.15",
          "form": "좋은 추억 많이된 여행 이었지요",
          "original_form": "좋은 추억 많이된 여행 이었지요",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100033213.1.16",
          "form": "나이들수록 친구끼리 모이기 쉽지않은데",
          "original_form": "나이들수록 친구끼리 모이기 쉽지않은데",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100033213.1.17",
          "form": "여행이라니 어디어디 가셨어요?",
          "original_form": "여행이라니 어디어디 가셨어요?",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100033213.1.18",
          "form": "천년의숲 과물노천탕 그리고 바닷가 를 갔어요",
          "original_form": "천년의숲 과물노천탕 그리고 바닷가 를 갔어요",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100033213.1.19",
          "form": "노천탕이 제주에있군요",
          "original_form": "노천탕이 제주에있군요",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100033213.1.20",
          "form": "너무좋을거같은데요",
          "original_form": "너무좋을거같은데요",
          "speaker_id": "2",
          "time": "20210907 20:44"
