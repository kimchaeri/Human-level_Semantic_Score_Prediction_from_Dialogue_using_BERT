      "utterance": [
        {
          "id": "MDRW2100036051.1.1",
          "form": "오오 동남아!",
          "original_form": "오오 동남아!",
          "speaker_id": "2",
          "time": "20210830 15:37"
        },
        {
          "id": "MDRW2100036051.1.2",
          "form": "저도 동남아도 좋아요",
          "original_form": "저도 동남아도 좋아요",
          "speaker_id": "2",
          "time": "20210830 15:37"
        },
        {
          "id": "MDRW2100036051.1.3",
          "form": "가성비 좋게 다녀올 수 있을거같아서요 ㅎㅎ",
          "original_form": "가성비 좋게 다녀올 수 있을거같아서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 15:37"
        },
        {
          "id": "MDRW2100036051.1.4",
          "form": "그죠 가성비좋게 레져도 하고 맛있는 과일도 먹고 그러고싶어요",
          "original_form": "그죠 가성비좋게 레져도 하고 맛있는 과일도 먹고 그러고싶어요",
          "speaker_id": "1",
          "time": "20210830 15:39"
        },
        {
          "id": "MDRW2100036051.1.5",
          "form": "일본 가신건 어디로 가셨었나요?",
          "original_form": "일본 가신건 어디로 가셨었나요?",
          "speaker_id": "1",
          "time": "20210830 15:39"
        },
        {
          "id": "MDRW2100036051.1.6",
          "form": "워낙 옛날이라 기억은 정확히 안나지만",
          "original_form": "워낙 옛날이라 기억은 정확히 안나지만",
          "speaker_id": "2",
          "time": "20210830 16:11"
        },
        {
          "id": "MDRW2100036051.1.7",
          "form": "홋카이도... 나가사키.. 이쪽 갔었어요!",
          "original_form": "홋카이도... 나가사키.. 이쪽 갔었어요!",
          "speaker_id": "2",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100036051.1.8",
          "form": "원래 도쿄 같은 도시를 가보고 싶었는데",
          "original_form": "원래 도쿄 같은 도시를 가보고 싶었는데",
          "speaker_id": "2",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100036051.1.9",
          "form": "시골만 다녀왔어요 ㅋㅋㅋㅋ",
          "original_form": "시골만 다녀왔어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:12"
        },
        {
          "id": "MDRW2100036051.1.10",
          "form": "그러셨구나 저는 홋카이도에 삿뽀로 꼭 한번 가보고싶어요ㅠ",
          "original_form": "그러셨구나 저는 홋카이도에 삿뽀로 꼭 한번 가보고싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100036051.1.11",
          "form": "제가 다녀온 곳 지역명이 잘 기억은 안나는데",
          "original_form": "제가 다녀온 곳 지역명이 잘 기억은 안나는데",
          "speaker_id": "2",
          "time": "20210830 16:18"
        },
        {
          "id": "MDRW2100036051.1.12",
          "form": "온천하는 게 되게 좋았어요!!!",
          "original_form": "온천하는 게 되게 좋았어요!!!",
          "speaker_id": "2",
          "time": "20210830 16:19"
        },
        {
          "id": "MDRW2100036051.1.13",
          "form": "삿뽀로... 거기 맥주가 맛있지 않던가요?!",
          "original_form": "삿뽀로... 거기 맥주가 맛있지 않던가요?!",
          "speaker_id": "2",
          "time": "20210830 16:19"
        },
        {
          "id": "MDRW2100036051.1.14",
          "form": "온천도 가셨구나! 거기 온천도 유명하죠",
          "original_form": "온천도 가셨구나! 거기 온천도 유명하죠",
          "speaker_id": "1",
          "time": "20210830 16:24"
        },
        {
          "id": "MDRW2100036051.1.15",
          "form": "맥주도 유명한데 눈축제가 유명해요",
          "original_form": "맥주도 유명한데 눈축제가 유명해요",
          "speaker_id": "1",
          "time": "20210830 16:24"
        },
        {
          "id": "MDRW2100036051.1.16",
          "form": "우와 그렇다면",
          "original_form": "우와 그렇다면",
          "speaker_id": "2",
          "time": "20210830 16:42"
        },
        {
          "id": "MDRW2100036051.1.17",
          "form": "겨울에 가야 제맛이겠군요!!!",
          "original_form": "겨울에 가야 제맛이겠군요!!!",
          "speaker_id": "2",
          "time": "20210830 16:42"
        },
        {
          "id": "MDRW2100036051.1.18",
          "form": "그렇죠!! 겨울에 가보고싶어요ㅠㅠ",
          "original_form": "그렇죠!! 겨울에 가보고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 16:43"
        },
        {
          "id": "MDRW2100036051.1.19",
          "form": "영화배경으로도 많이 나왔을걸요?삿뽀로",
          "original_form": "영화배경으로도 많이 나왔을걸요?삿뽀로",
          "speaker_id": "1",
          "time": "20210830 16:43"
        },
        {
          "id": "MDRW2100036051.1.20",
          "form": "오!!!",
          "original_form": "오!!!",
          "speaker_id": "2",
          "time": "20210830 16:44"
        },
        {
          "id": "MDRW2100036051.1.21",
          "form": "일본 특유의 겨울 분위기 느낌 있죠...",
          "original_form": "일본 특유의 겨울 분위기 느낌 있죠...",
          "speaker_id": "2",
          "time": "20210830 16:44"
        },
        {
          "id": "MDRW2100036051.1.22",
          "form": "일본 겨울 영화라고 하면",
          "original_form": "일본 겨울 영화라고 하면",
          "speaker_id": "2",
          "time": "20210830 16:45"
        },
        {
          "id": "MDRW2100036051.1.23",
          "form": "엄청 ㅋㅋㅋㅋㅋ 옛날 영화지만 설국 생각나네요... 원작이 소설인!",
          "original_form": "엄청 ㅋㅋㅋㅋㅋ 옛날 영화지만 설국 생각나네요... 원작이 소설인!",
          "speaker_id": "2",
          "time": "20210830 16:45"
        },
        {
          "id": "MDRW2100036051.1.24",
          "form": "그죠 일본 특유의 겨울 느낌이 있죠",
          "original_form": "그죠 일본 특유의 겨울 느낌이 있죠",
          "speaker_id": "1",
          "time": "20210830 16:46"
        },
        {
          "id": "MDRW2100036051.1.25",
          "form": "설국이라 제목만들어도 눈밭이 어울려요",
          "original_form": "설국이라 제목만들어도 눈밭이 어울려요",
          "speaker_id": "1",
          "time": "20210830 16:46"
        },
        {
          "id": "MDRW2100036051.1.26",
          "form": "저는 윤희에게 삿뽀로에서 찍은게 기억나요",
          "original_form": "저는 윤희에게 삿뽀로에서 찍은게 기억나요",
          "speaker_id": "1",
          "time": "20210830 16:47"
        },
        {
          "id": "MDRW2100036051.1.27",
          "form": "헉!",
          "original_form": "헉!",
          "speaker_id": "2",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100036051.1.28",
          "form": "윤희에게 배경이 삿뽀로였군요",
          "original_form": "윤희에게 배경이 삿뽀로였군요",
          "speaker_id": "2",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100036051.1.29",
          "form": "제가 좋아하는 영화 중 하나예요!",
          "original_form": "제가 좋아하는 영화 중 하나예요!",
          "speaker_id": "2",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100036051.1.30",
          "form": "저도 좋아하는 영화예요",
          "original_form": "저도 좋아하는 영화예요",
          "speaker_id": "1",
          "time": "20210830 17:36"
        },
        {
          "id": "MDRW2100036051.1.31",
          "form": "갑자기 중경삼림이 생각났는데",
          "original_form": "갑자기 중경삼림이 생각났는데",
          "speaker_id": "2",
          "time": "20210830 17:42"
        },
        {
          "id": "MDRW2100036051.1.32",
          "form": "홍콩 여행 가고 싶어지는 영화예요 ㅎㅎ",
          "original_form": "홍콩 여행 가고 싶어지는 영화예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 17:43"
        }
      ]
