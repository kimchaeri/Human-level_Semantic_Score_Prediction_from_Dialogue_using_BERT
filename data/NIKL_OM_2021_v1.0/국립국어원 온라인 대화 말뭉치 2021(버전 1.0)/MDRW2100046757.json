      "utterance": [
        {
          "id": "MDRW2100046757.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211104 17:44"
        },
        {
          "id": "MDRW2100046757.1.2",
          "form": "안녕하세요! 이제 곧 퇴근시간이네요!",
          "original_form": "안녕하세요! 이제 곧 퇴근시간이네요!",
          "speaker_id": "2",
          "time": "20211104 17:45"
        },
        {
          "id": "MDRW2100046757.1.3",
          "form": "네 그러네요 name2님들도 퇴근시간이 있나요?",
          "original_form": "네 그러네요 &name2&님들도 퇴근시간이 있나요?",
          "speaker_id": "1",
          "time": "20211104 17:45"
        },
        {
          "id": "MDRW2100046757.1.4",
          "form": "넵 그럼요 ㅋㅋ",
          "original_form": "넵 그럼요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 17:46"
        },
        {
          "id": "MDRW2100046757.1.5",
          "form": "저녁 메뉴는 정하셨나요?",
          "original_form": "저녁 메뉴는 정하셨나요?",
          "speaker_id": "2",
          "time": "20211104 17:46"
        },
        {
          "id": "MDRW2100046757.1.6",
          "form": "저 메뉴 추천좀 해주실래요",
          "original_form": "저 메뉴 추천좀 해주실래요",
          "speaker_id": "2",
          "time": "20211104 17:46"
        },
        {
          "id": "MDRW2100046757.1.7",
          "form": "전 어머니가 해주시는걸로..ㅋㅋ",
          "original_form": "전 어머니가 해주시는걸로..ㅋㅋ",
          "speaker_id": "1",
          "time": "20211104 17:47"
        },
        {
          "id": "MDRW2100046757.1.8",
          "form": "제가 짬뽕이 먹고싶으니 추천드려요 ㅎ",
          "original_form": "제가 짬뽕이 먹고싶으니 추천드려요 ㅎ",
          "speaker_id": "1",
          "time": "20211104 17:47"
        },
        {
          "id": "MDRW2100046757.1.9",
          "form": "어라 짬뽕...좋은데요??ㅋㅋㅋㅋ",
          "original_form": "어라 짬뽕...좋은데요??ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 17:47"
        },
        {
          "id": "MDRW2100046757.1.10",
          "form": "어머니가 해주시는 음식이 짱이죠!",
          "original_form": "어머니가 해주시는 음식이 짱이죠!",
          "speaker_id": "2",
          "time": "20211104 17:47"
        },
        {
          "id": "MDRW2100046757.1.11",
          "form": "어머니 음식 중에 뭐가 제일 맛나신가요",
          "original_form": "어머니 음식 중에 뭐가 제일 맛나신가요",
          "speaker_id": "2",
          "time": "20211104 17:48"
        },
        {
          "id": "MDRW2100046757.1.12",
          "form": "네 어머니가 요리를 잘하셔요 조리사시거든요",
          "original_form": "네 어머니가 요리를 잘하셔요 조리사시거든요",
          "speaker_id": "1",
          "time": "20211104 17:48"
        },
        {
          "id": "MDRW2100046757.1.13",
          "form": "잡채요~",
          "original_form": "잡채요~",
          "speaker_id": "1",
          "time": "20211104 17:48"
        },
        {
          "id": "MDRW2100046757.1.14",
          "form": "와 대박 잡채 은근히 간맞추기 어려운데!",
          "original_form": "와 대박 잡채 은근히 간맞추기 어려운데!",
          "speaker_id": "2",
          "time": "20211104 17:49"
        },
        {
          "id": "MDRW2100046757.1.15",
          "form": "요리 실력이 엄청나시겠네요..",
          "original_form": "요리 실력이 엄청나시겠네요..",
          "speaker_id": "2",
          "time": "20211104 17:49"
        },
        {
          "id": "MDRW2100046757.1.16",
          "form": "반찬투정 할 일이 없으셨겠는걸요",
          "original_form": "반찬투정 할 일이 없으셨겠는걸요",
          "speaker_id": "2",
          "time": "20211104 17:49"
        },
        {
          "id": "MDRW2100046757.1.17",
          "form": "네 맛있어서 많이 먹느라 꼭 운동을 합니다..ㅋ",
          "original_form": "네 맛있어서 많이 먹느라 꼭 운동을 합니다..ㅋ",
          "speaker_id": "1",
          "time": "20211104 17:49"
        },
        {
          "id": "MDRW2100046757.1.18",
          "form": "ㅋㅋㅋㅋㅋ앗 그런 단점이라면 환영이죠",
          "original_form": "ㅋㅋㅋㅋㅋ앗 그런 단점이라면 환영이죠",
          "speaker_id": "2",
          "time": "20211104 17:50"
        },
        {
          "id": "MDRW2100046757.1.19",
          "form": "운동도 꼬박꼬박 하시나요?",
          "original_form": "운동도 꼬박꼬박 하시나요?",
          "speaker_id": "2",
          "time": "20211104 17:51"
        },
        {
          "id": "MDRW2100046757.1.20",
          "form": "일주일에 5일정도는 꼭 하고있어요 저녁 먹구요",
          "original_form": "일주일에 5일정도는 꼭 하고있어요 저녁 먹구요",
          "speaker_id": "1",
          "time": "20211104 17:51"
        },
        {
          "id": "MDRW2100046757.1.21",
