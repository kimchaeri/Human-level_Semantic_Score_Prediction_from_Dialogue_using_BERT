      "utterance": [
        {
          "id": "MDRW2100029463.1.1",
          "form": "아침되니까 먹을 거...땡기네요",
          "original_form": "아침되니까 먹을 거...땡기네요",
          "speaker_id": "1",
          "time": "20210903 09:07"
        },
        {
          "id": "MDRW2100029463.1.2",
          "form": "그러네요..ㅎㅎ 아침 드셨나요??",
          "original_form": "그러네요..ㅎㅎ 아침 드셨나요??",
          "speaker_id": "2",
          "time": "20210903 10:37"
        },
        {
          "id": "MDRW2100029463.1.3",
          "form": "네! 점심도 먹고 집 왔답니다 드디어... 오리고기...",
          "original_form": "네! 점심도 먹고 집 왔답니다 드디어... 오리고기...",
          "speaker_id": "1",
          "time": "20210903 16:49"
        },
        {
          "id": "MDRW2100029463.1.4",
          "form": "오오 오리고기 맛있죠ㅎㅎ",
          "original_form": "오오 오리고기 맛있죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100029463.1.5",
          "form": "마라탕은 좋아하시나요??",
          "original_form": "마라탕은 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100029463.1.6",
          "form": "헉 네!",
          "original_form": "헉 네!",
          "speaker_id": "1",
          "time": "20210903 20:06"
        },
        {
          "id": "MDRW2100029463.1.7",
          "form": "마라탕 먹고 싶어오",
          "original_form": "마라탕 먹고 싶어오",
          "speaker_id": "1",
          "time": "20210903 20:07"
        },
        {
          "id": "MDRW2100029463.1.8",
          "form": "마라탕 너무 맛있어요ㅠㅠ",
          "original_form": "마라탕 너무 맛있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100029463.1.9",
          "form": "어떤 재료 가장 좋아하세요??",
          "original_form": "어떤 재료 가장 좋아하세요??",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100029463.1.10",
          "form": "감자 팽이버섯 중당... ㄴ다좋아요,,,",
          "original_form": "감자 팽이버섯 중당... ㄴ다좋아요,,,",
          "speaker_id": "1",
          "time": "20210904 01:18"
        },
        {
          "id": "MDRW2100029463.1.11",
          "form": "ㅎㅎㅎ 중당 못참죠 전 면종류는 조금씩 다 넣는 것 같아요",
          "original_form": "ㅎㅎㅎ 중당 못참죠 전 면종류는 조금씩 다 넣는 것 같아요",
          "speaker_id": "2",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100029463.1.12",
          "form": "고기도 꼭 넣습니다ㅎㅎ",
          "original_form": "고기도 꼭 넣습니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100029463.1.13",
          "form": "맞아요ㅠㅠ 소고기 넣으세요?",
          "original_form": "맞아요ㅠㅠ 소고기 넣으세요?",
          "speaker_id": "1",
          "time": "20210904 13:25"
        },
        {
          "id": "MDRW2100029463.1.14",
          "form": "네!! 소고기가 제일 무난 한 것 같아서 거의 소고기만 넣어요ㅎㅎ",
          "original_form": "네!! 소고기가 제일 무난 한 것 같아서 거의 소고기만 넣어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100029463.1.15",
          "form": "양고기 넣어본 적 있으세요?",
          "original_form": "양고기 넣어본 적 있으세요?",
          "speaker_id": "1",
          "time": "20210904 16:10"
        },
        {
          "id": "MDRW2100029463.1.16",
          "form": "아뇨 양고기는 잘 안맞을까봐 애초에 안넣어봫어요ㅎㅎ",
          "original_form": "아뇨 양고기는 잘 안맞을까봐 애초에 안넣어봫어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100029463.1.17",
          "form": "양고기로 드셔보셨어요??",
          "original_form": "양고기로 드셔보셨어요??",
          "speaker_id": "2",
          "time": "20210904 16:56"
        },
        {
          "id": "MDRW2100029463.1.18",
          "form": "저도 한 번도 안 먹어봤어요! 궁금하긴 허네용..",
          "original_form": "저도 한 번도 안 먹어봤어요! 궁금하긴 허네용..",
          "speaker_id": "1",
          "time": "20210904 17:34"
        },
        {
          "id": "MDRW2100029463.1.19",
          "form": "그러게요ㅎㅎ 양고기는 살짝 냄새날수도 있다고 해서 나중에 친구꺼 뺏어먹는 식으로 맛봐야겠어요ㅎㅎ",
          "original_form": "그러게요ㅎㅎ 양고기는 살짝 냄새날수도 있다고 해서 나중에 친구꺼 뺏어먹는 식으로 맛봐야겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:38"
        },
        {
          "id": "MDRW2100029463.1.20",
          "form": "ㅁㅋㅋㅌ양고기 넣는 친구를 못봤아요ㅠㅠ",
          "original_form": "ㅁㅋㅋㅌ양고기 넣는 친구를 못봤아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 21:00"
        },
        {
          "id": "MDRW2100029463.1.21",
          "form": "아 그렇긴하죠.. 추천하듯이 꼬득여서 먹어보는걸로..ㅎㅎ",
          "original_form": "아 그렇긴하죠.. 추천하듯이 꼬득여서 먹어보는걸로..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100029463.1.22",
          "form": "아 넘웃겨욬ㅌㅋㅋㅋㅋㅋㅋ 친구 보고 대신 먹어보라고 하는..",
          "original_form": "아 넘웃겨욬ㅌㅋㅋㅋㅋㅋㅋ 친구 보고 대신 먹어보라고 하는..",
          "speaker_id": "1",
          "time": "20210905 16:37"
        },
        {
          "id": "MDRW2100029463.1.23",
          "form": "저는 셔고기를 먹어야하니까요^^ㅎㅎㅎ",
          "original_form": "저는 셔고기를 먹어야하니까요^^ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 17:03"
        },
        {
          "id": "MDRW2100029463.1.24",
