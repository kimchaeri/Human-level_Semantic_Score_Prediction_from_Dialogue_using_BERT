      "utterance": [
        {
          "id": "MDRW2100034715.1.1",
          "form": "유튜브나 넷플릭스같은거 보세요?",
          "original_form": "유튜브나 넷플릭스같은거 보세요?",
          "speaker_id": "1",
          "time": "20210828 18:05"
        },
        {
          "id": "MDRW2100034715.1.2",
          "form": "네 많이보죠!",
          "original_form": "네 많이보죠!",
          "speaker_id": "2",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100034715.1.3",
          "form": "오 뭐보시나요",
          "original_form": "오 뭐보시나요",
          "speaker_id": "1",
          "time": "20210828 18:11"
        },
        {
          "id": "MDRW2100034715.1.4",
          "form": "유튜브는 덕질도 하고 요새 주식 관심있어서 주식관련영상도 봐요!",
          "original_form": "유튜브는 덕질도 하고 요새 주식 관심있어서 주식관련영상도 봐요!",
          "speaker_id": "2",
          "time": "20210828 18:12"
        },
        {
          "id": "MDRW2100034715.1.5",
          "form": "어떤거  보세요?",
          "original_form": "어떤거  보세요?",
          "speaker_id": "2",
          "time": "20210828 18:12"
        },
        {
          "id": "MDRW2100034715.1.6",
          "form": "오 주식보시는군요",
          "original_form": "오 주식보시는군요",
          "speaker_id": "1",
          "time": "20210828 18:12"
        },
        {
          "id": "MDRW2100034715.1.7",
          "form": "저는 먹방봐요 ㅋㅋㅋ",
          "original_form": "저는 먹방봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:12"
        },
        {
          "id": "MDRW2100034715.1.8",
          "form": "먹방도 좋죠! 어느분꺼보세요?",
          "original_form": "먹방도 좋죠! 어느분꺼보세요?",
          "speaker_id": "2",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034715.1.9",
          "form": "그때그때 다르긴한데 아마따라는 분이요!",
          "original_form": "그때그때 다르긴한데 아마따라는 분이요!",
          "speaker_id": "1",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034715.1.10",
          "form": "네 주식 해가지구 공부하려고 봐요!",
          "original_form": "네 주식 해가지구 공부하려고 봐요!",
          "speaker_id": "2",
          "time": "20210828 18:13"
        },
        {
          "id": "MDRW2100034715.1.11",
          "form": "아전 너무 어렵더라구요 주식은ㅋㅋㅋ",
          "original_form": "아전 너무 어렵더라구요 주식은ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:14"
        },
        {
          "id": "MDRW2100034715.1.12",
          "form": "주식 하고있지만 진짜 어려운거 같아요",
          "original_form": "주식 하고있지만 진짜 어려운거 같아요",
          "speaker_id": "2",
          "time": "20210828 18:14"
        },
        {
          "id": "MDRW2100034715.1.13",
          "form": "아마따요? 한번 찾아봐야겠네요",
          "original_form": "아마따요? 한번 찾아봐야겠네요",
          "speaker_id": "2",
          "time": "20210828 18:14"
        },
        {
          "id": "MDRW2100034715.1.14",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:15"
        },
        {
          "id": "MDRW2100034715.1.15",
          "form": "주식잘하는분들 신기해요",
          "original_form": "주식잘하는분들 신기해요",
          "speaker_id": "1",
          "time": "20210828 18:15"
        },
        {
          "id": "MDRW2100034715.1.16",
          "form": "그니까요 근데 그분들은  공부도 많이 하셔서 잘하시는거더라구요",
          "original_form": "그니까요 근데 그분들은  공부도 많이 하셔서 잘하시는거더라구요",
          "speaker_id": "2",
          "time": "20210828 18:16"
        },
        {
          "id": "MDRW2100034715.1.17",
          "form": "모를때는 감이고 운이라고 생각했는데 아니었더라구요ㅠ",
          "original_form": "모를때는 감이고 운이라고 생각했는데 아니었더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100034715.1.18",
          "form": "역시 뭐든 노력해야하나봐요 ㅜㅜ 저도 운인줄 알았는데",
          "original_form": "역시 뭐든 노력해야하나봐요 ㅜㅜ 저도 운인줄 알았는데",
          "speaker_id": "1",
          "time": "20210828 18:17"
        },
        {
          "id": "MDRW2100034715.1.19",
          "form": "그니까요 모든지 공부가 필요하더라구요ㅠ",
          "original_form": "그니까요 모든지 공부가 필요하더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210828 18:18"
        }
      ]
