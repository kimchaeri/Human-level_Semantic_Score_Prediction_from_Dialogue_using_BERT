      "utterance": [
        {
          "id": "MDRW2100044612.1.1",
          "form": "긍정님 최애음식은 먼가요",
          "original_form": "긍정님 최애음식은 먼가요",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044612.1.2",
          "form": "저는 쏘맥?ㅋㅋㅋㅋㅋㅋㅋㄲㅋㅋ",
          "original_form": "저는 쏘맥?ㅋㅋㅋㅋㅋㅋㅋㄲㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044612.1.3",
          "form": "치킨과 함께여ㅋㅋㅋ",
          "original_form": "치킨과 함께여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044612.1.4",
          "form": "아 맛잇겟다ㅜㅜ",
          "original_form": "아 맛잇겟다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044612.1.5",
          "form": "크...쏘맥 장인이세요?",
          "original_form": "크...쏘맥 장인이세요?",
          "speaker_id": "1",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044612.1.6",
          "form": "글엄요 한잔말아드리곱네요",
          "original_form": "글엄요 한잔말아드리곱네요",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044612.1.7",
          "form": "안주는 뭘로해드릴까여ㅋㅋ",
          "original_form": "안주는 뭘로해드릴까여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044612.1.8",
          "form": "ㅋㅋㅋㅋ 야채곱창이요",
          "original_form": "ㅋㅋㅋㅋ 야채곱창이요",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044612.1.9",
          "form": "님 비율 어케대요",
          "original_form": "님 비율 어케대요",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044612.1.10",
          "form": "와... 1대1.5가 제대로맛잇습니다",
          "original_form": "와... 1대1.5가 제대로맛잇습니다",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044612.1.11",
          "form": "곱창 흑흑 먹고싶다ㅋㅋㅋ",
          "original_form": "곱창 흑흑 먹고싶다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044612.1.12",
          "form": "한입에 톡 털어먹어야되요!ㅋㅋ",
          "original_form": "한입에 톡 털어먹어야되요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044612.1.13",
          "form": "뭐가 1인가요",
          "original_form": "뭐가 1인가요",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044612.1.14",
          "form": "소주여ㅋㅋㅋㅋ",
          "original_form": "소주여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044612.1.15",
          "form": "반대로 생각하는 먹짱이님은 제대로된 고수인정",
          "original_form": "반대로 생각하는 먹짱이님은 제대로된 고수인정",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044612.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044612.1.17",
          "form": "누가타주는거만 먹어서욬ㅋㅋㅋㅋ",
          "original_form": "누가타주는거만 먹어서욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044612.1.18",
          "form": "그 브랜드는 노상관 ?",
          "original_form": "그 브랜드는 노상관 ?",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044612.1.19",
          "form": "참이슬에 카스가 짱맛이더라구여",
          "original_form": "참이슬에 카스가 짱맛이더라구여",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044612.1.20",
          "form": "테라는 생각보다 연한듯?ㅋㅋ",
          "original_form": "테라는 생각보다 연한듯?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044612.1.21",
          "form": "ㅋㅋㅋ 그 조합 기억~~",
          "original_form": "ㅋㅋㅋ 그 조합 기억~~",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044612.1.22",
          "form": "시도할게여",
          "original_form": "시도할게여",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044612.1.23",
          "form": "네네 꼭 도전해보세요ㅎㅎㅎ\n아 불금이 다가오길 기다려봅니다",
          "original_form": "네네 꼭 도전해보세요ㅎㅎㅎ\n아 불금이 다가오길 기다려봅니다",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044612.1.24",
          "form": "불금에만 안주랑 한잔하거든여ㅋㅋㅋ",
          "original_form": "불금에만 안주랑 한잔하거든여ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044612.1.25",
          "form": "크... 적절하네여",
          "original_form": "크... 적절하네여",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044612.1.26",
          "form": "치킨 쏘맥 ㅋㅋㅋ",
          "original_form": "치킨 쏘맥 ㅋㅋㅋ",
