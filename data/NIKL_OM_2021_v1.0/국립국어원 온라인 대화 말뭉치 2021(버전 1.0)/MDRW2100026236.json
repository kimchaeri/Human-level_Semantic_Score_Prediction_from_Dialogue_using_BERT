      "utterance": [
        {
          "id": "MDRW2100026236.1.1",
          "form": "요즘 식이요법중이라서 그런가",
          "original_form": "요즘 식이요법중이라서 그런가",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026236.1.2",
          "form": "왜이렇게 다 맛있지",
          "original_form": "왜이렇게 다 맛있지",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026236.1.3",
          "form": "ㅋㅋㅋㅋㅋ가을이라 그럴지도",
          "original_form": "ㅋㅋㅋㅋㅋ가을이라 그럴지도",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026236.1.4",
          "form": "그것도 그렇다",
          "original_form": "그것도 그렇다",
          "speaker_id": "2",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026236.1.5",
          "form": "가을에 맛있는 식재료가 많이 나오잖아",
          "original_form": "가을에 맛있는 식재료가 많이 나오잖아",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026236.1.6",
          "form": "살찌는건 대박 재밌지",
          "original_form": "살찌는건 대박 재밌지",
          "speaker_id": "2",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026236.1.7",
          "form": "매일이 기대될정도야",
          "original_form": "매일이 기대될정도야",
          "speaker_id": "2",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026236.1.8",
          "form": "맞아ㅋㅋㅋㅋ말이 아니라 사람이 살 찌는 계절이야",
          "original_form": "맞아ㅋㅋㅋㅋ말이 아니라 사람이 살 찌는 계절이야",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026236.1.9",
          "form": "갑자기 육포 먹고싶다",
          "original_form": "갑자기 육포 먹고싶다",
          "speaker_id": "1",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026236.1.10",
          "form": "육포좋아하니",
          "original_form": "육포좋아하니",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100026236.1.11",
          "form": "난 꾸이맨좋아해",
          "original_form": "난 꾸이맨좋아해",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100026236.1.12",
          "form": "응 좋아하는데 가격이 너무 창렬이라 못 먹고 있어",
          "original_form": "응 좋아하는데 가격이 너무 창렬이라 못 먹고 있어",
          "speaker_id": "1",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026236.1.13",
          "form": "육포만드는 공정보니까",
          "original_form": "육포만드는 공정보니까",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026236.1.14",
          "form": "비싼게 이해는 되더라",
          "original_form": "비싼게 이해는 되더라",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026236.1.15",
          "form": "그건 그래 들어가는 고기 중량도 그렇고 말리는 과정도 빡세더라",
          "original_form": "그건 그래 들어가는 고기 중량도 그렇고 말리는 과정도 빡세더라",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026236.1.16",
          "form": "난 숏다리도 좋아해",
          "original_form": "난 숏다리도 좋아해",
          "speaker_id": "1",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100026236.1.17",
          "form": "난 턱을 아끼려고",
          "original_form": "난 턱을 아끼려고",
          "speaker_id": "2",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100026236.1.18",
          "form": "숏다리도 맛있긴해",
          "original_form": "숏다리도 맛있긴해",
          "speaker_id": "2",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026236.1.19",
          "form": "ㅋㅋㅋㅋ나도 요즘 딱딱한 거 줄이려고 노력중이야",
          "original_form": "ㅋㅋㅋㅋ나도 요즘 딱딱한 거 줄이려고 노력중이야",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026236.1.20",
          "form": "우리 서로 무슨 음식 생각하는지 맞추기 어때?",
          "original_form": "우리 서로 무슨 음식 생각하는지 맞추기 어때?",
          "speaker_id": "1",
          "time": "20210901 21:36"
        },
        {
          "id": "MDRW2100026236.1.21",
          "form": "좋아 재밌겠다",
          "original_form": "좋아 재밌겠다",
          "speaker_id": "2",
          "time": "20210901 21:36"
        }
      ]
