      "utterance": [
        {
          "id": "MDRW2100036629.1.1",
          "form": "새우 좋아하세요?",
          "original_form": "새우 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 02:44"
        },
        {
          "id": "MDRW2100036629.1.2",
          "form": "그 마트에서 1kg 대용량으로 파는 칵테일새우 그거 전 되게 좋아해요!",
          "original_form": "그 마트에서 1kg 대용량으로 파는 칵테일새우 그거 전 되게 좋아해요!",
          "speaker_id": "2",
          "time": "20210831 02:44"
        },
        {
          "id": "MDRW2100036629.1.3",
          "form": "네 저도요 대용량으로사서",
          "original_form": "네 저도요 대용량으로사서",
          "speaker_id": "1",
          "time": "20210831 02:44"
        },
        {
          "id": "MDRW2100036629.1.4",
          "form": "필요할때마다 꺼내쓰니 넘편하고 좋더라구여",
          "original_form": "필요할때마다 꺼내쓰니 넘편하고 좋더라구여",
          "speaker_id": "1",
          "time": "20210831 02:45"
        },
        {
          "id": "MDRW2100036629.1.5",
          "form": "맞아요 이걸로",
          "original_form": "맞아요 이걸로",
          "speaker_id": "2",
          "time": "20210831 02:45"
        },
        {
          "id": "MDRW2100036629.1.6",
          "form": "파스타해먹을때도 넣고",
          "original_form": "파스타해먹을때도 넣고",
          "speaker_id": "2",
          "time": "20210831 02:45"
        },
        {
          "id": "MDRW2100036629.1.7",
          "form": "맞아요 파스타많이해먹어요",
          "original_form": "맞아요 파스타많이해먹어요",
          "speaker_id": "1",
          "time": "20210831 02:45"
        },
        {
          "id": "MDRW2100036629.1.8",
          "form": "굴소스에 볶음밥 해먹을때도 넣고",
          "original_form": "굴소스에 볶음밥 해먹을때도 넣고",
          "speaker_id": "2",
          "time": "20210831 02:45"
        },
        {
          "id": "MDRW2100036629.1.9",
          "form": "좀 큰걸로 사면 튀겨서 칠리새우 해먹어요",
          "original_form": "좀 큰걸로 사면 튀겨서 칠리새우 해먹어요",
          "speaker_id": "2",
          "time": "20210831 02:46"
        },
        {
          "id": "MDRW2100036629.1.10",
          "form": "크 잘 아시네요",
          "original_form": "크 잘 아시네요",
          "speaker_id": "1",
          "time": "20210831 02:46"
        },
        {
          "id": "MDRW2100036629.1.11",
          "form": "요리좀하시나봐요",
          "original_form": "요리좀하시나봐요",
          "speaker_id": "1",
          "time": "20210831 02:46"
        },
        {
          "id": "MDRW2100036629.1.12",
          "form": "자세히 보시면",
          "original_form": "자세히 보시면",
          "speaker_id": "2",
          "time": "20210831 02:46"
        },
        {
          "id": "MDRW2100036629.1.13",
          "form": "다 시판 소스에요ㅋㅋㅋㅋㅋ",
          "original_form": "다 시판 소스에요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:46"
        },
        {
          "id": "MDRW2100036629.1.14",
          "form": "파스타소스... 칠리소스...굴소스...",
          "original_form": "파스타소스... 칠리소스...굴소스...",
          "speaker_id": "2",
          "time": "20210831 02:46"
        },
        {
          "id": "MDRW2100036629.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:46"
        },
        {
          "id": "MDRW2100036629.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ그래도 조리를거치잖아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그래도 조리를거치잖아요",
          "speaker_id": "1",
          "time": "20210831 02:46"
        },
        {
          "id": "MDRW2100036629.1.17",
          "form": "이것만 계~속 루틴으로 해드시면",
          "original_form": "이것만 계~속 루틴으로 해드시면",
          "speaker_id": "2",
          "time": "20210831 02:46"
        },
        {
          "id": "MDRW2100036629.1.18",
          "form": "레시피가 고정이 됩니다ㅋㅋㅋ",
          "original_form": "레시피가 고정이 됩니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:47"
        },
        {
          "id": "MDRW2100036629.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ레벨업하듯이여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ레벨업하듯이여",
          "speaker_id": "1",
          "time": "20210831 02:47"
        },
        {
          "id": "MDRW2100036629.1.20",
          "form": "새우하니까 감바스도 해먹고싶네요",
          "original_form": "새우하니까 감바스도 해먹고싶네요",
          "speaker_id": "1",
          "time": "20210831 02:47"
        },
        {
          "id": "MDRW2100036629.1.21",
          "form": "네네! 비슷한거죠",
          "original_form": "네네! 비슷한거죠",
          "speaker_id": "2",
          "time": "20210831 02:47"
        },
        {
          "id": "MDRW2100036629.1.22",
          "form": "아아아ㅠㅠㅠ",
          "original_form": "아아아ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 02:47"
        },
        {
          "id": "MDRW2100036629.1.23",
          "form": "너무 맛있죠.....",
          "original_form": "너무 맛있죠.....",
          "speaker_id": "2",
          "time": "20210831 02:47"
        },
        {
          "id": "MDRW2100036629.1.24",
          "form": "오일아까워서 자주못해먹어요ㅜ",
          "original_form": "오일아까워서 자주못해먹어요ㅜ",
          "speaker_id": "1",
          "time": "20210831 02:47"
        },
        {
          "id": "MDRW2100036629.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210831 02:47"
        },
        {
          "id": "MDRW2100036629.1.26",
          "form": "오일 비싼데ㅋㅋㅋㅋ",
          "original_form": "오일 비싼데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 02:47"
        },
        {
          "id": "MDRW2100036629.1.27",
          "form": "보통 요리하실때",
          "original_form": "보통 요리하실때",
          "speaker_id": "2",
          "time": "20210831 02:47"
        },
        {
          "id": "MDRW2100036629.1.28",
          "form": "그니까여 아주콸콸부어야되니까..ㅎㅎ",
          "original_form": "그니까여 아주콸콸부어야되니까..ㅎㅎ",
