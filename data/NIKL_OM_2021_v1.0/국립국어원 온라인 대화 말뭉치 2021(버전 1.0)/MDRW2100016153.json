      "utterance": [
        {
          "id": "MDRW2100016153.1.1",
          "form": "오늘 해 떳어요",
          "original_form": "오늘 해 떳어요",
          "speaker_id": "2",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016153.1.2",
          "form": "아주 오랜만에 보는 해여서 기분이 좋네요",
          "original_form": "아주 오랜만에 보는 해여서 기분이 좋네요",
          "speaker_id": "2",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016153.1.3",
          "form": "거기는 어떤가요?",
          "original_form": "거기는 어떤가요?",
          "speaker_id": "2",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016153.1.4",
          "form": "저도 오랜만에 해떠서 기분좋아서 물어봤어요",
          "original_form": "저도 오랜만에 해떠서 기분좋아서 물어봤어요",
          "speaker_id": "1",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016153.1.5",
          "form": "진짜 비가 징하게 와서 지겹던 참이거든요",
          "original_form": "진짜 비가 징하게 와서 지겹던 참이거든요",
          "speaker_id": "1",
          "time": "20210826 12:13"
        },
        {
          "id": "MDRW2100016153.1.6",
          "form": "맞아요 빨래도 안마르고",
          "original_form": "맞아요 빨래도 안마르고",
          "speaker_id": "2",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016153.1.7",
          "form": "오늘 빨래돌리고 나왔구만요",
          "original_form": "오늘 빨래돌리고 나왔구만요",
          "speaker_id": "2",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016153.1.8",
          "form": "오늘은 그럼 어떤 패션하고 나오셨나욧",
          "original_form": "오늘은 그럼 어떤 패션하고 나오셨나욧",
          "speaker_id": "2",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016153.1.9",
          "form": "오늘은 맨날 돌려입던 옷을 입었습니다",
          "original_form": "오늘은 맨날 돌려입던 옷을 입었습니다",
          "speaker_id": "1",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016153.1.10",
          "form": "가볍고 편하게",
          "original_form": "가볍고 편하게",
          "speaker_id": "1",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016153.1.11",
          "form": "극강의 효율 추구하시구먼요",
          "original_form": "극강의 효율 추구하시구먼요",
          "speaker_id": "2",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016153.1.12",
          "form": "그러면 비오는 날 vs 눈오는 날",
          "original_form": "그러면 비오는 날 vs 눈오는 날",
          "speaker_id": "2",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016153.1.13",
          "form": "아 어려운데",
          "original_form": "아 어려운데",
          "speaker_id": "1",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100016153.1.14",
          "form": "눈보다는 비가 낫지않나요",
          "original_form": "눈보다는 비가 낫지않나요",
          "speaker_id": "1",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100016153.1.15",
          "form": "눈오는 날 미끄러워서",
          "original_form": "눈오는 날 미끄러워서",
          "speaker_id": "1",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100016153.1.16",
          "form": "name2은 어떠세요",
          "original_form": "&name2&은 어떠세요",
          "speaker_id": "1",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016153.1.17",
          "form": "저도 비요",
          "original_form": "저도 비요",
          "speaker_id": "2",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016153.1.18",
          "form": "비가 지긋지긋해도",
          "original_form": "비가 지긋지긋해도",
          "speaker_id": "1",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016153.1.19",
          "form": "눈보다는 나은게 현실",
          "original_form": "눈보다는 나은게 현실",
          "speaker_id": "1",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016153.1.20",
          "form": "웹툰 플랫폼 중에 비오는 날 포인트 주는 곳이 있어서",
          "original_form": "웹툰 플랫폼 중에 비오는 날 포인트 주는 곳이 있어서",
          "speaker_id": "2",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016153.1.21",
          "form": "장마 아주 쏠쏠합니다",
          "original_form": "장마 아주 쏠쏠합니다",
          "speaker_id": "2",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016153.1.22",
          "form": "눈은 좀 쓰레기같어요",
          "original_form": "눈은 좀 쓰레기같어요",
          "speaker_id": "2",
          "time": "20210826 12:21"
        },
        {
          "id": "MDRW2100016153.1.23",
          "form": "스키장에서만 좋아요",
          "original_form": "스키장에서만 좋아요",
          "speaker_id": "2",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016153.1.24",
          "form": "스키장 가봤어요?",
          "original_form": "스키장 가봤어요?",
          "speaker_id": "1",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016153.1.25",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016153.1.26",
          "form": "가보셨나요?!!!",
          "original_form": "가보셨나요?!!!",
          "speaker_id": "2",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016153.1.27",
          "form": "올.. 가봤습니다",
          "original_form": "올.. 가봤습니다",
          "speaker_id": "1",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016153.1.28",
          "form": "근데 이번엔 코로나땨문에 못갔는데",
          "original_form": "근데 이번엔 코로나땨문에 못갔는데",
          "speaker_id": "2",
          "time": "20210826 12:22"
        },
        {
          "id": "MDRW2100016153.1.29",
          "form": "겨울 스키장에서 스키 타고",
          "original_form": "겨울 스키장에서 스키 타고",
          "speaker_id": "2",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016153.1.30",
          "form": "숙소 들가서 라면이 최고죠",
          "original_form": "숙소 들가서 라면이 최고죠",
