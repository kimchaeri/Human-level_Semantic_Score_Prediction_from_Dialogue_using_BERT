      "utterance": [
        {
          "id": "MDRW2100006343.1.1",
          "form": "사실 저도 뮤지컬을 직접 보러 가기보다는",
          "original_form": "사실 저도 뮤지컬을 직접 보러 가기보다는",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100006343.1.2",
          "form": "실황영상을 보는 편인거 같아요",
          "original_form": "실황영상을 보는 편인거 같아요",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100006343.1.3",
          "form": "아하! 클립 영상들을 보는 것도 즐겁죠",
          "original_form": "아하! 클립 영상들을 보는 것도 즐겁죠",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100006343.1.4",
          "form": "어떤 부분이 제일 흥미로우셨어요??",
          "original_form": "어떤 부분이 제일 흥미로우셨어요??",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100006343.1.5",
          "form": "일단 노래들이 너무 좋고",
          "original_form": "일단 노래들이 너무 좋고",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100006343.1.6",
          "form": "가사가 장난 아니에요...",
          "original_form": "가사가 장난 아니에요...",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006343.1.7",
          "form": "라임이나 말장난 등이 대단한 거 같더라고요",
          "original_form": "라임이나 말장난 등이 대단한 거 같더라고요",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100006343.1.8",
          "form": "번역 역시 직접 하신거죠?",
          "original_form": "번역 역시 직접 하신거죠?",
          "speaker_id": "1",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100006343.1.9",
          "form": "네 근데 위에 링크 건 블로그를 좀 참조하긴 했어요",
          "original_form": "네 근데 위에 링크 건 블로그를 좀 참조하긴 했어요",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100006343.1.10",
          "form": "가사에 숨은 의미나 그런걸 잘 설명해 두셨더라고요",
          "original_form": "가사에 숨은 의미나 그런걸 잘 설명해 두셨더라고요",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100006343.1.11",
          "form": "그런 숨은 의미를 알아채는게 뮤지컬의 묘미인 것 같아요",
          "original_form": "그런 숨은 의미를 알아채는게 뮤지컬의 묘미인 것 같아요",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100006343.1.12",
          "form": "name1님도 뮤지컬 좋아하시나요?",
          "original_form": "&name1&님도 뮤지컬 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006343.1.13",
          "form": "좋아해요!!",
          "original_form": "좋아해요!!",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100006343.1.14",
          "form": "위키드 캣츠 맘마미아 모두 좋아해요",
          "original_form": "위키드 캣츠 맘마미아 모두 좋아해요",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100006343.1.15",
          "form": "위키드",
          "original_form": "위키드",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100006343.1.16",
          "form": "저 아직 못봤어요...",
          "original_form": "저 아직 못봤어요...",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100006343.1.17",
          "form": "보고싶네요",
          "original_form": "보고싶네요",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100006343.1.18",
          "form": "위키드 아마 보시면 반할거에요",
          "original_form": "위키드 아마 보시면 반할거에요",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100006343.1.19",
          "form": "저는 미국가면 위키드 공연은 꼭 보고 오고 싶어요",
          "original_form": "저는 미국가면 위키드 공연은 꼭 보고 오고 싶어요",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100006343.1.20",
          "form": "저도 미국 가서 해밀턴 보는게 소원이에요 ㅋㅋㅋ",
          "original_form": "저도 미국 가서 해밀턴 보는게 소원이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100006343.1.21",
          "form": "우와 우리 미국은 정말 꼭 가야겠어요!",
          "original_form": "우와 우리 미국은 정말 꼭 가야겠어요!",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100006343.1.22",
          "form": "해밀턴에 정말 푹 빠지셨나봐요!",
          "original_form": "해밀턴에 정말 푹 빠지셨나봐요!",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100006343.1.23",
          "form": "그러게요ㅎㅎ",
          "original_form": "그러게요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100006343.1.24",
          "form": "영상으로만 보고 직접 본 적은 없어서...",
          "original_form": "영상으로만 보고 직접 본 적은 없어서...",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100006343.1.25",
          "form": "아마 직접 보시면 정말 뮤지컬과 사랑에 빠지게 되실거에요",
          "original_form": "아마 직접 보시면 정말 뮤지컬과 사랑에 빠지게 되실거에요",
          "speaker_id": "1",
          "time": "20210907 23:44"
        }
      ]
