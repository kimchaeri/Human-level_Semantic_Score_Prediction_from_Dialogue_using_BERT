      "utterance": [
        {
          "id": "MDRW2100001057.1.1",
          "form": "학교생활은 재밌나요?",
          "original_form": "학교생활은 재밌나요?",
          "speaker_id": "1",
          "time": "20210722 21:34"
        },
        {
          "id": "MDRW2100001057.1.2",
          "form": "재미있게 다니고있어요!",
          "original_form": "재미있게 다니고있어요!",
          "speaker_id": "2",
          "time": "20210722 21:34"
        },
        {
          "id": "MDRW2100001057.1.3",
          "form": "혹시 대학교..?",
          "original_form": "혹시 대학교..?",
          "speaker_id": "1",
          "time": "20210722 21:34"
        },
        {
          "id": "MDRW2100001057.1.4",
          "form": "지금은 방학이라 친구들끼리 자주 톡을 주고받고 있습니당",
          "original_form": "지금은 방학이라 친구들끼리 자주 톡을 주고받고 있습니당",
          "speaker_id": "2",
          "time": "20210722 21:34"
        },
        {
          "id": "MDRW2100001057.1.5",
          "form": "아뇨 중학교",
          "original_form": "아뇨 중학교",
          "speaker_id": "2",
          "time": "20210722 21:34"
        },
        {
          "id": "MDRW2100001057.1.6",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210722 21:34"
        },
        {
          "id": "MDRW2100001057.1.7",
          "form": "개학이 언제예요?",
          "original_form": "개학이 언제예요?",
          "speaker_id": "1",
          "time": "20210722 21:35"
        },
        {
          "id": "MDRW2100001057.1.8",
          "form": "23일입니다!",
          "original_form": "23일입니다!",
          "speaker_id": "2",
          "time": "20210722 21:35"
        },
        {
          "id": "MDRW2100001057.1.9",
          "form": "다들비슷하군요!",
          "original_form": "다들비슷하군요!",
          "speaker_id": "1",
          "time": "20210722 21:36"
        },
        {
          "id": "MDRW2100001057.1.10",
          "form": "월래 16일이였다가 대체공유일 지정되고 17일됬다가 학교 쌤들 백신 2차접종 때문에 23일에 바겼어요 ㅋㅋ",
          "original_form": "월래 16일이였다가 대체공유일 지정되고 17일됬다가 학교 쌤들 백신 2차접종 때문에 23일에 바겼어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210722 21:36"
        },
        {
          "id": "MDRW2100001057.1.11",
          "form": "오 좋네요 ㅋㅋㅋㅋ 방학은 길면좋죠 ㅎㅎ",
          "original_form": "오 좋네요 ㅋㅋㅋㅋ 방학은 길면좋죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210722 21:36"
        },
        {
          "id": "MDRW2100001057.1.12",
          "form": "여름방학기길면",
          "original_form": "여름방학기길면",
          "speaker_id": "2",
          "time": "20210722 21:36"
        },
        {
          "id": "MDRW2100001057.1.13",
          "form": "겨울방학 짧아져요ㅠㅠ",
          "original_form": "겨울방학 짧아져요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210722 21:37"
        },
        {
          "id": "MDRW2100001057.1.14",
          "form": "ㅠㅠㅛ 그건 슬프네요 ㅜ",
          "original_form": "ㅠㅠㅛ 그건 슬프네요 ㅜ",
          "speaker_id": "1",
          "time": "20210722 21:37"
        },
        {
          "id": "MDRW2100001057.1.15",
          "form": "코로나때문에 거의 온라인일껀데",
          "original_form": "코로나때문에 거의 온라인일껀데",
          "speaker_id": "1",
          "time": "20210722 21:37"
        },
        {
          "id": "MDRW2100001057.1.16",
          "form": "답답하겠어요 ㅜ",
          "original_form": "답답하겠어요 ㅜ",
          "speaker_id": "1",
          "time": "20210722 21:37"
        },
        {
          "id": "MDRW2100001057.1.17",
          "form": "다행이 요번에도 겨울방학이랑 봄방학이랑 연달아할거같아요",
          "original_form": "다행이 요번에도 겨울방학이랑 봄방학이랑 연달아할거같아요",
          "speaker_id": "2",
          "time": "20210722 21:37"
        },
        {
          "id": "MDRW2100001057.1.18",
          "form": "온라인인데 온라인이 아니거같아요 ㅋㅋ",
          "original_form": "온라인인데 온라인이 아니거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210722 21:37"
        },
        {
          "id": "MDRW2100001057.1.19",
          "form": "그냥 영상틀고 놀고있어요 ㅋㅋ",
          "original_form": "그냥 영상틀고 놀고있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210722 21:38"
        },
        {
          "id": "MDRW2100001057.1.20",
          "form": "옹ㅋㅋㅋ 온라인하면 집중이안되섴ㅋ",
          "original_form": "옹ㅋㅋㅋ 온라인하면 집중이안되섴ㅋ",
          "speaker_id": "1",
          "time": "20210722 21:38"
        },
        {
          "id": "MDRW2100001057.1.21",
          "form": "맞아요 ㅋㅋㅣㄱ",
          "original_form": "맞아요 ㅋㅋㅣㄱ",
          "speaker_id": "2",
          "time": "20210722 21:38"
        },
        {
          "id": "MDRW2100001057.1.22",
          "form": "저도 온라인은 녹화하고 틀어뫄서",
          "original_form": "저도 온라인은 녹화하고 틀어뫄서",
          "speaker_id": "1",
          "time": "20210722 21:38"
        },
        {
          "id": "MDRW2100001057.1.23",
          "form": "심지어 화상을 안해서 더그런거 같아요",
          "original_form": "심지어 화상을 안해서 더그런거 같아요",
          "speaker_id": "2",
          "time": "20210722 21:38"
        },
        {
          "id": "MDRW2100001057.1.24",
          "form": "안그러면 못돌려봐욬ㅋㅋ",
          "original_form": "안그러면 못돌려봐욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210722 21:38"
        },
        {
          "id": "MDRW2100001057.1.25",
          "form": "맞아요 화상이면 딴짓을 못하니까",
          "original_form": "맞아요 화상이면 딴짓을 못하니까",
          "speaker_id": "1",
          "time": "20210722 21:38"
        },
        {
          "id": "MDRW2100001057.1.26",
          "form": "저희는 그냥영상 올려주세요",
          "original_form": "저희는 그냥영상 올려주세요",
          "speaker_id": "2",
          "time": "20210722 21:39"
        },
        {
          "id": "MDRW2100001057.1.27",
          "form": "시험때 돌려보라고 ㅋ기",
          "original_form": "시험때 돌려보라고 ㅋ기",
          "speaker_id": "2",
          "time": "20210722 21:39"
        },
        {
          "id": "MDRW2100001057.1.28",
          "form": "ㅋㅋㅋㄱ 좋네요",
          "original_form": "ㅋㅋㅋㄱ 좋네요",
          "speaker_id": "1",
          "time": "20210722 21:39"
        },
        {
          "id": "MDRW2100001057.1.29",
          "form": "근데 체육은 온라인영상길이 심각했어요 ㅋㅋ",
          "original_form": "근데 체육은 온라인영상길이 심각했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210722 21:39"
        }
      ]
