      "utterance": [
        {
          "id": "MDRW2100022051.1.1",
          "form": "요즘은 서점에 많이 가",
          "original_form": "요즘은 서점에 많이 가",
          "speaker_id": "2",
          "time": "20210829 01:28"
        },
        {
          "id": "MDRW2100022051.1.2",
          "form": "너 원래 책 안 좋아하잖아",
          "original_form": "너 원래 책 안 좋아하잖아",
          "speaker_id": "1",
          "time": "20210829 01:28"
        },
        {
          "id": "MDRW2100022051.1.3",
          "form": "코로나 이후에 많이 가고 있어",
          "original_form": "코로나 이후에 많이 가고 있어",
          "speaker_id": "2",
          "time": "20210829 01:28"
        },
        {
          "id": "MDRW2100022051.1.4",
          "form": "심심해서 그러니",
          "original_form": "심심해서 그러니",
          "speaker_id": "1",
          "time": "20210829 01:28"
        },
        {
          "id": "MDRW2100022051.1.5",
          "form": "그런 이유도 있지",
          "original_form": "그런 이유도 있지",
          "speaker_id": "2",
          "time": "20210829 01:28"
        },
        {
          "id": "MDRW2100022051.1.6",
          "form": "또 다른 이유는 뭔데",
          "original_form": "또 다른 이유는 뭔데",
          "speaker_id": "1",
          "time": "20210829 01:28"
        },
        {
          "id": "MDRW2100022051.1.7",
          "form": "내가 여행을 좋아하잖아",
          "original_form": "내가 여행을 좋아하잖아",
          "speaker_id": "2",
          "time": "20210829 01:28"
        },
        {
          "id": "MDRW2100022051.1.8",
          "form": "그런데",
          "original_form": "그런데",
          "speaker_id": "1",
          "time": "20210829 01:28"
        },
        {
          "id": "MDRW2100022051.1.9",
          "form": "코로나로 인해 갈 수 없게 돼야지",
          "original_form": "코로나로 인해 갈 수 없게 돼야지",
          "speaker_id": "2",
          "time": "20210829 01:28"
        },
        {
          "id": "MDRW2100022051.1.10",
          "form": "그것이 책이랑 무슨 상관이야",
          "original_form": "그것이 책이랑 무슨 상관이야",
          "speaker_id": "1",
          "time": "20210829 01:28"
        },
        {
          "id": "MDRW2100022051.1.11",
          "form": "그래서 여행책을 보면서 대신 만족을 해",
          "original_form": "그래서 여행책을 보면서 대신 만족을 해",
          "speaker_id": "2",
          "time": "20210829 01:29"
        },
        {
          "id": "MDRW2100022051.1.12",
          "form": "아하 기행문을 보는구나",
          "original_form": "아하 기행문을 보는구나",
          "speaker_id": "1",
          "time": "20210829 01:29"
        },
        {
          "id": "MDRW2100022051.1.13",
          "form": "맞아 직접 여행가지 않아도 그 기분을 느낄 수 있어",
          "original_form": "맞아 직접 여행가지 않아도 그 기분을 느낄 수 있어",
          "speaker_id": "2",
          "time": "20210829 01:29"
