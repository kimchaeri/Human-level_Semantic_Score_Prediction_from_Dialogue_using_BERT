      "utterance": [
        {
          "id": "MDRW2100024484.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210831 11:18"
        },
        {
          "id": "MDRW2100024484.1.2",
          "form": "이상형이 어떤 사람이에요?",
          "original_form": "이상형이 어떤 사람이에요?",
          "speaker_id": "2",
          "time": "20210831 11:22"
        },
        {
          "id": "MDRW2100024484.1.3",
          "form": "귀여운 사람 좋아해요",
          "original_form": "귀여운 사람 좋아해요",
          "speaker_id": "1",
          "time": "20210831 11:23"
        },
        {
          "id": "MDRW2100024484.1.4",
          "form": "대신 키는 크코요",
          "original_form": "대신 키는 크코요",
          "speaker_id": "1",
          "time": "20210831 11:23"
        },
        {
          "id": "MDRW2100024484.1.5",
          "form": "크고요",
          "original_form": "크고요",
          "speaker_id": "1",
          "time": "20210831 11:23"
        },
        {
          "id": "MDRW2100024484.1.6",
          "form": "ㅋㅋㅋㅋ키크고 귀여운 사람이요?",
          "original_form": "ㅋㅋㅋㅋ키크고 귀여운 사람이요?",
          "speaker_id": "2",
          "time": "20210831 11:23"
        },
        {
          "id": "MDRW2100024484.1.7",
          "form": "그럼 키는 어느 정도면 크다고 생각해요?",
          "original_form": "그럼 키는 어느 정도면 크다고 생각해요?",
          "speaker_id": "2",
          "time": "20210831 11:23"
        },
        {
          "id": "MDRW2100024484.1.8",
          "form": "180이상은 됐으면 좋겠어요",
          "original_form": "180이상은 됐으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210831 11:26"
        },
        {
          "id": "MDRW2100024484.1.9",
          "form": "제가 딱 170이라서",
          "original_form": "제가 딱 170이라서",
          "speaker_id": "1",
          "time": "20210831 11:26"
        },
        {
          "id": "MDRW2100024484.1.10",
          "form": "저는 168인데도 남자는 180 정도를 생각하는데",
          "original_form": "저는 168인데도 남자는 180 정도를 생각하는데",
          "speaker_id": "2",
          "time": "20210831 11:28"
        },
        {
          "id": "MDRW2100024484.1.11",
          "form": "키 170이면 정말 그렇겠네요ㅎㅎ",
          "original_form": "키 170이면 정말 그렇겠네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 11:28"
        },
        {
          "id": "MDRW2100024484.1.12",
          "form": "설레는 키차이는 10센치 이상부터라는 말이 있죠",
          "original_form": "설레는 키차이는 10센치 이상부터라는 말이 있죠",
          "speaker_id": "1",
          "time": "20210831 11:30"
        },
        {
          "id": "MDRW2100024484.1.13",
          "form": "마지노선이네요",
          "original_form": "마지노선이네요",
          "speaker_id": "2",
          "time": "20210831 11:32"
        },
        {
          "id": "MDRW2100024484.1.14",
          "form": "이상이니까 딱 10센치 바래봅니다",
          "original_form": "이상이니까 딱 10센치 바래봅니다",
          "speaker_id": "1",
          "time": "20210831 11:32"
        },
        {
          "id": "MDRW2100024484.1.15",
          "form": "ㅋㅋㅋ 그래서 저도 연애가 쉽지 않네요",
          "original_form": "ㅋㅋㅋ 그래서 저도 연애가 쉽지 않네요",
          "speaker_id": "2",
          "time": "20210831 11:33"
        },
        {
          "id": "MDRW2100024484.1.16",
          "form": "사실 코로나라 사람 만나기 어려운 것도 있죠",
          "original_form": "사실 코로나라 사람 만나기 어려운 것도 있죠",
          "speaker_id": "2",
          "time": "20210831 11:33"
        },
        {
          "id": "MDRW2100024484.1.17",
          "form": "맞아요 코로나라 소개팅도 없죠",
          "original_form": "맞아요 코로나라 소개팅도 없죠",
          "speaker_id": "1",
          "time": "20210831 11:33"
        },
        {
          "id": "MDRW2100024484.1.18",
          "form": "소개팅 예전에는 좀 했었어요?",
          "original_form": "소개팅 예전에는 좀 했었어요?",
          "speaker_id": "2",
          "time": "20210831 11:34"
        },
        {
          "id": "MDRW2100024484.1.19",
          "form": "올해 스무살이라 소개팅 나갈 기회가 아예 없었어요",
          "original_form": "올해 스무살이라 소개팅 나갈 기회가 아예 없었어요",
          "speaker_id": "1",
          "time": "20210831 11:34"
        },
        {
          "id": "MDRW2100024484.1.20",
          "form": "그렇군요.. 전 어쩌다보니 소개팅을 안해봤네요",
          "original_form": "그렇군요.. 전 어쩌다보니 소개팅을 안해봤네요",
