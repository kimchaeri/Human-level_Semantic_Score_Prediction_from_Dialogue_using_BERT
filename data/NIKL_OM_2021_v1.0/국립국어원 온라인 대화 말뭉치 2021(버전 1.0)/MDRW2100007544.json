      "utterance": [
        {
          "id": "MDRW2100007544.1.1",
          "form": "싸이우ㅏㄹ드 부활하ㄴ대",
          "original_form": "싸이우ㅏㄹ드 부활하ㄴ대",
          "speaker_id": "1",
          "time": "20210822 03:09"
        },
        {
          "id": "MDRW2100007544.1.2",
          "form": "와 재밌겠다",
          "original_form": "와 재밌겠다",
          "speaker_id": "2",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007544.1.3",
          "form": "해봄??",
          "original_form": "해봄??",
          "speaker_id": "1",
          "time": "20210822 03:10"
        },
        {
          "id": "MDRW2100007544.1.4",
          "form": "조회는 해봤으",
          "original_form": "조회는 해봤으",
          "speaker_id": "2",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007544.1.5",
          "form": "조회??",
          "original_form": "조회??",
          "speaker_id": "1",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007544.1.6",
          "form": "벌써 서비스 시작했나??",
          "original_form": "벌써 서비스 시작했나??",
          "speaker_id": "1",
          "time": "20210822 03:11"
        },
        {
          "id": "MDRW2100007544.1.7",
          "form": "그거 막 아이디조회하고 정보맞는지 확인하고 하던디",
          "original_form": "그거 막 아이디조회하고 정보맞는지 확인하고 하던디",
          "speaker_id": "2",
          "time": "20210822 03:14"
        },
        {
          "id": "MDRW2100007544.1.8",
          "form": "그래??",
          "original_form": "그래??",
          "speaker_id": "1",
          "time": "20210822 03:14"
        },
        {
          "id": "MDRW2100007544.1.9",
          "form": "그전에 했던거 미리 삭제할수 있으려나",
          "original_form": "그전에 했던거 미리 삭제할수 있으려나",
          "speaker_id": "1",
          "time": "20210822 03:14"
        },
        {
          "id": "MDRW2100007544.1.10",
          "form": "다 비공개로 풀릴걸 아마",
          "original_form": "다 비공개로 풀릴걸 아마",
          "speaker_id": "2",
          "time": "20210822 03:14"
        },
        {
          "id": "MDRW2100007544.1.11",
          "form": "이거 삭제 안되면 아빠 싸이 큰일인데",
          "original_form": "이거 삭제 안되면 아빠 싸이 큰일인데",
          "speaker_id": "2",
          "time": "20210822 03:14"
        },
        {
          "id": "MDRW2100007544.1.12",
          "form": "아빠 왜??",
          "original_form": "아빠 왜??",
          "speaker_id": "1",
          "time": "20210822 03:15"
        },
        {
          "id": "MDRW2100007544.1.13",
          "form": "못볼사진 있어??",
          "original_form": "못볼사진 있어??",
          "speaker_id": "1",
          "time": "20210822 03:15"
        },
        {
          "id": "MDRW2100007544.1.14",
          "form": "아주많을걸",
          "original_form": "아주많을걸",
          "speaker_id": "2",
          "time": "20210822 03:15"
        },
        {
          "id": "MDRW2100007544.1.15",
          "form": "ㅎㅎ 궁금하다",
          "original_form": "ㅎㅎ 궁금하다",
          "speaker_id": "1",
          "time": "20210822 03:15"
        },
        {
          "id": "MDRW2100007544.1.16",
          "form": "노래 비지엠 그대로이려나",
          "original_form": "노래 비지엠 그대로이려나",
          "speaker_id": "1",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007544.1.17",
          "form": "뉴질랜드 추억도 많겠다",
          "original_form": "뉴질랜드 추억도 많겠다",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007544.1.18",
          "form": "어 나 근데 노래 있었는데 조회했을때 없다고 뜸 어이없어",
          "original_form": "어 나 근데 노래 있었는데 조회했을때 없다고 뜸 어이없어",
          "speaker_id": "2",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007544.1.19",
          "form": "뉴질랜드 갔다왔어??",
          "original_form": "뉴질랜드 갔다왔어??",
          "speaker_id": "1",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007544.1.20",
          "form": "좋았겠네",
          "original_form": "좋았겠네",
          "speaker_id": "1",
          "time": "20210822 03:16"
        },
        {
          "id": "MDRW2100007544.1.21",
          "form": "사진이 남는건 좋은거지",
          "original_form": "사진이 남는건 좋은거지",
          "speaker_id": "1",
          "time": "20210822 03:16"
        }
      ]
