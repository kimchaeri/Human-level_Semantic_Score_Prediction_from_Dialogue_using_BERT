      "utterance": [
        {
          "id": "MDRW2100033482.1.1",
          "form": "너 좀 특이한 알바 한 거 있어?",
          "original_form": "너 좀 특이한 알바 한 거 있어?",
          "speaker_id": "2",
          "time": "20210824 00:38"
        },
        {
          "id": "MDRW2100033482.1.2",
          "form": "나 예전에 어린이 테마파크에서 잏한적있어",
          "original_form": "나 예전에 어린이 테마파크에서 잏한적있어",
          "speaker_id": "1",
          "time": "20210824 00:38"
        },
        {
          "id": "MDRW2100033482.1.3",
          "form": "어린이 테마파크?",
          "original_form": "어린이 테마파크?",
          "speaker_id": "2",
          "time": "20210824 00:38"
        },
        {
          "id": "MDRW2100033482.1.4",
          "form": "그럼 서울에서 일을 한거야?",
          "original_form": "그럼 서울에서 일을 한거야?",
          "speaker_id": "2",
          "time": "20210824 00:38"
        },
        {
          "id": "MDRW2100033482.1.5",
          "form": "응 어린이들이 직업 체험을 해볼수 있는 곳이야",
          "original_form": "응 어린이들이 직업 체험을 해볼수 있는 곳이야",
          "speaker_id": "1",
          "time": "20210824 00:38"
        },
        {
          "id": "MDRW2100033482.1.6",
          "form": "아니아니 내가 사는 곳 근처에도 있었어",
          "original_form": "아니아니 내가 사는 곳 근처에도 있었어",
          "speaker_id": "1",
          "time": "20210824 00:39"
        },
        {
          "id": "MDRW2100033482.1.7",
          "form": "서울? 대단하두나.",
          "original_form": "서울? 대단하두나.",
          "speaker_id": "2",
          "time": "20210824 00:39"
        },
        {
          "id": "MDRW2100033482.1.8",
          "form": "아 날 농락한거구나?",
          "original_form": "아 날 농락한거구나?",
          "speaker_id": "2",
          "time": "20210824 00:39"
        },
        {
          "id": "MDRW2100033482.1.9",
          "form": "뮬론 버스를 타고 갔지만",
          "original_form": "뮬론 버스를 타고 갔지만",
          "speaker_id": "1",
          "time": "20210824 00:39"
        },
        {
          "id": "MDRW2100033482.1.10",
          "form": "짓궂구나.",
          "original_form": "짓궂구나.",
          "speaker_id": "2",
          "time": "20210824 00:39"
        },
        {
          "id": "MDRW2100033482.1.11",
          "form": "첫번째 질문에 대답한건데 오해했구나.",
          "original_form": "첫번째 질문에 대답한건데 오해했구나.",
          "speaker_id": "1",
          "time": "20210824 00:39"
        },
        {
          "id": "MDRW2100033482.1.12",
          "form": "그나저나 어린이들 직업체험을 도와준다니 신기하다.",
          "original_form": "그나저나 어린이들 직업체험을 도와준다니 신기하다.",
          "speaker_id": "2",
          "time": "20210824 00:39"
        },
        {
          "id": "MDRW2100033482.1.13",
          "form": "너는 어떤 직업을 해봤어?",
          "original_form": "너는 어떤 직업을 해봤어?",
          "speaker_id": "2",
          "time": "20210824 00:39"
        },
        {
          "id": "MDRW2100033482.1.14",
          "form": "응 어린이들은 정말 귀엽고 시끄러워",
          "original_form": "응 어린이들은 정말 귀엽고 시끄러워",
          "speaker_id": "1",
          "time": "20210824 00:39"
        },
        {
          "id": "MDRW2100033482.1.15",
          "form": "좋은거야?",
          "original_form": "좋은거야?",
          "speaker_id": "2",
          "time": "20210824 00:39"
        },
        {
          "id": "MDRW2100033482.1.16",
          "form": "나는 소방관을 해봤던게 기억나",
          "original_form": "나는 소방관을 해봤던게 기억나",
          "speaker_id": "1",
          "time": "20210824 00:39"
        },
        {
          "id": "MDRW2100033482.1.17",
          "form": "음 그래도 재미있는 알바였어",
          "original_form": "음 그래도 재미있는 알바였어",
          "speaker_id": "1",
          "time": "20210824 00:39"
        },
        {
          "id": "MDRW2100033482.1.18",
          "form": "우와. 실제로 너 소방서에 간 적은 있니?",
          "original_form": "우와. 실제로 너 소방서에 간 적은 있니?",
          "speaker_id": "2",
          "time": "20210824 00:40"
        },
        {
          "id": "MDRW2100033482.1.19",
          "form": "다음에는 놀이공원 알바도 한번 해보고싶었는데",
          "original_form": "다음에는 놀이공원 알바도 한번 해보고싶었는데",
          "speaker_id": "1",
          "time": "20210824 00:40"
        },
        {
          "id": "MDRW2100033482.1.20",
          "form": "너는 이런 활돌적인 알바는 관심없니?",
          "original_form": "너는 이런 활돌적인 알바는 관심없니?",
          "speaker_id": "1",
          "time": "20210824 00:40"
        },
        {
          "id": "MDRW2100033482.1.21",
          "form": "놀이동원 알바는 너무 힘들 거 같아.",
          "original_form": "놀이동원 알바는 너무 힘들 거 같아.",
          "speaker_id": "2",
          "time": "20210824 00:40"
        }
      ]
