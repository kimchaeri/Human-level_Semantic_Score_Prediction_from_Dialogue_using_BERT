      "utterance": [
        {
          "id": "MDRW2100026874.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026874.1.2",
          "form": "알바 해본적 있어요?",
          "original_form": "알바 해본적 있어요?",
          "speaker_id": "1",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026874.1.3",
          "form": "당연하죠!",
          "original_form": "당연하죠!",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026874.1.4",
          "form": "전 여러 개 해봤어요",
          "original_form": "전 여러 개 해봤어요",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026874.1.5",
          "form": "카페알바도 하고...",
          "original_form": "카페알바도 하고...",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026874.1.6",
          "form": "당신은요?",
          "original_form": "당신은요?",
          "speaker_id": "2",
          "time": "20210902 00:48"
        },
        {
          "id": "MDRW2100026874.1.7",
          "form": "저도 여러개 해봤어요 물류나 마트알바요!",
          "original_form": "저도 여러개 해봤어요 물류나 마트알바요!",
          "speaker_id": "1",
          "time": "20210902 00:50"
        },
        {
          "id": "MDRW2100026874.1.8",
          "form": "물류 어땠어요?",
          "original_form": "물류 어땠어요?",
          "speaker_id": "2",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026874.1.9",
          "form": "손목이 너무 아팠어요ㅠ",
          "original_form": "손목이 너무 아팠어요ㅠ",
          "speaker_id": "1",
          "time": "20210902 00:51"
        },
        {
          "id": "MDRW2100026874.1.10",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026874.1.11",
          "form": "돈은 많이 벌죠 그래도",
          "original_form": "돈은 많이 벌죠 그래도",
          "speaker_id": "2",
          "time": "20210902 00:52"
        },
        {
          "id": "MDRW2100026874.1.12",
          "form": "예전에는 돈 많이 줬다고 하더라고요",
          "original_form": "예전에는 돈 많이 줬다고 하더라고요",
          "speaker_id": "1",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026874.1.13",
          "form": "근데 저는 최저만 받았어요ㅠㅠ",
          "original_form": "근데 저는 최저만 받았어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 00:53"
        },
        {
          "id": "MDRW2100026874.1.14",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026874.1.15",
          "form": "마트알바는요?",
          "original_form": "마트알바는요?",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026874.1.16",
          "form": "판촉인가",
          "original_form": "판촉인가",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026874.1.17",
          "form": "아 맞아요 판촉 알바했었어요",
          "original_form": "아 맞아요 판촉 알바했었어요",
          "speaker_id": "1",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100026874.1.18",
          "form": "목소리 커야 한다고 알고 있어요",
          "original_form": "목소리 커야 한다고 알고 있어요",
          "speaker_id": "2",
          "time": "20210902 00:56"
        },
        {
          "id": "MDRW2100026874.1.19",
          "form": "목소리도 커야되는데 큰거보다 튼튼해야겠더라고요",
          "original_form": "목소리도 커야되는데 큰거보다 튼튼해야겠더라고요",
          "speaker_id": "1",
          "time": "20210902 00:59"
        },
        {
          "id": "MDRW2100026874.1.20",
          "form": "목소리가 커도 목이 나가면 할수가 없더라고요ㅠㅠ",
          "original_form": "목소리가 커도 목이 나가면 할수가 없더라고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 01:00"
        },
        {
          "id": "MDRW2100026874.1.21",
          "form": "허걱 그건 몰랐어요",
          "original_form": "허걱 그건 몰랐어요",
          "speaker_id": "2",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100026874.1.22",
          "form": "힘든 일이네요...",
          "original_form": "힘든 일이네요...",
          "speaker_id": "2",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100026874.1.23",
          "form": "전 피자 만드는 일도 했었어요",
          "original_form": "전 피자 만드는 일도 했었어요",
          "speaker_id": "2",
          "time": "20210902 01:03"
        },
        {
          "id": "MDRW2100026874.1.24",
          "form": "오 저도요",
          "original_form": "오 저도요",
          "speaker_id": "1",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100026874.1.25",
          "form": "도우부터 오븐들어가기까지 다 만드신거에요?",
          "original_form": "도우부터 오븐들어가기까지 다 만드신거에요?",
          "speaker_id": "1",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100026874.1.26",
          "form": "저는 피자라기 보단 피자빵이었어요ㅋㅋㅋ",
          "original_form": "저는 피자라기 보단 피자빵이었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100026874.1.27",
          "form": "네 밀가루부터 다 치댔습니다",
          "original_form": "네 밀가루부터 다 치댔습니다",
          "speaker_id": "2",
          "time": "20210902 09:53"
        },
        {
          "id": "MDRW2100026874.1.28",
          "form": "하루에 10시간씩 일해서 한달만에 그만 뒀어요 ㅋㅋ",
          "original_form": "하루에 10시간씩 일해서 한달만에 그만 뒀어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 09:53"
        }
      ]
