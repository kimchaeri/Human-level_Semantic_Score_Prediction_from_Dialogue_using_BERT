      "utterance": [
        {
          "id": "MDRW2100031157.1.1",
          "form": "요즘 중고거래가 유행이죵",
          "original_form": "요즘 중고거래가 유행이죵",
          "speaker_id": "1",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100031157.1.2",
          "form": "하시나요!?",
          "original_form": "하시나요!?",
          "speaker_id": "1",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100031157.1.3",
          "form": "아니요 저는 안해요 님은요?",
          "original_form": "아니요 저는 안해요 님은요?",
          "speaker_id": "2",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031157.1.4",
          "form": "엇 저는 예전부터 근근히 하고있었어요",
          "original_form": "엇 저는 예전부터 근근히 하고있었어요",
          "speaker_id": "1",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100031157.1.5",
          "form": "중고나라요!",
          "original_form": "중고나라요!",
          "speaker_id": "1",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100031157.1.6",
          "form": "당근은 잘안하구",
          "original_form": "당근은 잘안하구",
          "speaker_id": "1",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100031157.1.7",
          "form": "확실히 전국구가 조금더 싸요",
          "original_form": "확실히 전국구가 조금더 싸요",
          "speaker_id": "1",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100031157.1.8",
          "form": "사기꾼이 많긴한데ㅠ 고가거래는 잘안하는게 팁이죠,,ㅋㅋㅋㅠ",
          "original_form": "사기꾼이 많긴한데ㅠ 고가거래는 잘안하는게 팁이죠,,ㅋㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100031157.1.9",
          "form": "그렇죠~저는 당근 딱 한번만 해봤어요..",
          "original_form": "그렇죠~저는 당근 딱 한번만 해봤어요..",
          "speaker_id": "2",
          "time": "20210904 18:10"
        },
        {
          "id": "MDRW2100031157.1.10",
          "form": "사기꾼을 조심하는게 중요한것같아요ㅜ",
          "original_form": "사기꾼을 조심하는게 중요한것같아요ㅜ",
          "speaker_id": "2",
          "time": "20210904 18:11"
        },
        {
          "id": "MDRW2100031157.1.11",
          "form": "맞아요ㅠ 그래서 저는 더치트 이용해요",
          "original_form": "맞아요ㅠ 그래서 저는 더치트 이용해요",
          "speaker_id": "1",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100031157.1.12",
          "form": "더치트는 뭔가요?",
          "original_form": "더치트는 뭔가요?",
          "speaker_id": "2",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100031157.1.13",
          "form": "여기 번호나 계좌번호 치면 사기꾼인거 조금은 알수있거든요! 사기당한사람들이 사기당한 번호같은거 제보하고 모아놓는거예요",
          "original_form": "여기 번호나 계좌번호 치면 사기꾼인거 조금은 알수있거든요! 사기당한사람들이 사기당한 번호같은거 제보하고 모아놓는거예요",
          "speaker_id": "1",
          "time": "20210904 18:12"
        },
        {
          "id": "MDRW2100031157.1.14",
          "form": "막 한번호에 열명씩 사기당했다구 올라오기도해요",
          "original_form": "막 한번호에 열명씩 사기당했다구 올라오기도해요",
          "speaker_id": "1",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100031157.1.15",
          "form": "아 그렇군요...",
          "original_form": "아 그렇군요...",
          "speaker_id": "2",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100031157.1.16",
          "form": "그래도 그런게 있어서 다행이예요",
          "original_form": "그래도 그런게 있어서 다행이예요",
          "speaker_id": "2",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100031157.1.17",
          "form": "나중에 알아두세여 언젠가 쓸수두 있으니! 맞아요 생각해낸사람 좀 천재..ㅋㅋㅋ 되게많이 사기꾼 잡았대요",
          "original_form": "나중에 알아두세여 언젠가 쓸수두 있으니! 맞아요 생각해낸사람 좀 천재..ㅋㅋㅋ 되게많이 사기꾼 잡았대요",
          "speaker_id": "1",
          "time": "20210904 18:13"
        },
        {
          "id": "MDRW2100031157.1.18",
          "form": "사기 피해도 예방하고",
          "original_form": "사기 피해도 예방하고",
          "speaker_id": "1",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031157.1.19",
          "form": "이런데 전화번호 등록되면 T전화나 다른전화어플에 사기꾼이랴구ㅋㅋㅋ 뜬다네요ㅋㅋㅋ",
          "original_form": "이런데 전화번호 등록되면 T전화나 다른전화어플에 사기꾼이랴구ㅋㅋㅋ 뜬다네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:14"
        },
        {
          "id": "MDRW2100031157.1.20",
          "form": "아 그렇군요 많은걸 알려주셔서 감사해요",
          "original_form": "아 그렇군요 많은걸 알려주셔서 감사해요",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031157.1.21",
          "form": "어떤 물건들을 거래하시나요?",
          "original_form": "어떤 물건들을 거래하시나요?",
          "speaker_id": "2",
          "time": "20210904 18:15"
        },
        {
          "id": "MDRW2100031157.1.22",
          "form": "새거 많이해요 약간 책같은거 샀는데 안쓰거나 그런거",
          "original_form": "새거 많이해요 약간 책같은거 샀는데 안쓰거나 그런거",
          "speaker_id": "1",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100031157.1.23",
          "form": "많아가지고요ㅋㅋㅋㅠ",
          "original_form": "많아가지고요ㅋㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100031157.1.24",
          "form": "제가 사기도하구요",
          "original_form": "제가 사기도하구요",
          "speaker_id": "1",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100031157.1.25",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100031157.1.26",
          "form": "저는 이사갈때 가전제품 팔아봤어요 당근에서",
          "original_form": "저는 이사갈때 가전제품 팔아봤어요 당근에서",
          "speaker_id": "2",
          "time": "20210904 18:16"
        },
        {
          "id": "MDRW2100031157.1.27",
          "form": "오 ㅋㅋㅋ저는 가전제품은 낡을때까지 써가지고",
          "original_form": "오 ㅋㅋㅋ저는 가전제품은 낡을때까지 써가지고",
          "speaker_id": "1",
          "time": "20210904 18:17"
        },
        {
          "id": "MDRW2100031157.1.28",
          "form": "아 이벤트 같은거로 받은거는 저도 팔아봤어요",
          "original_form": "아 이벤트 같은거로 받은거는 저도 팔아봤어요",
          "speaker_id": "1",
          "time": "20210904 18:18"
        },
        {
          "id": "MDRW2100031157.1.29",
          "form": "한번!이요",
          "original_form": "한번!이요",
          "speaker_id": "1",
          "time": "20210904 18:18"
        },
        {
          "id": "MDRW2100031157.1.30",
          "form": "알뜰하시군요~~",
          "original_form": "알뜰하시군요~~",
          "speaker_id": "2",
          "time": "20210904 18:19"
        }
      ]
