      "utterance": [
        {
          "id": "MDRW2100019506.1.1",
          "form": "앜ㅋㅋㅋㅋ 전본다하고아직 안봣네요.",
          "original_form": "앜ㅋㅋㅋㅋ 전본다하고아직 안봣네요.",
          "speaker_id": "2",
          "time": "20210827 19:56"
        },
        {
          "id": "MDRW2100019506.1.2",
          "form": "ㅋㅋㅋ재밌어요ㅋㅋㅋㅋ근데 그래서 그동안은 세븐틴멤버들 모르니까",
          "original_form": "ㅋㅋㅋ재밌어요ㅋㅋㅋㅋ근데 그래서 그동안은 세븐틴멤버들 모르니까",
          "speaker_id": "1",
          "time": "20210827 19:57"
        },
        {
          "id": "MDRW2100019506.1.3",
          "form": "노래들어도 이목소리가 누군지몰랐는데",
          "original_form": "노래들어도 이목소리가 누군지몰랐는데",
          "speaker_id": "1",
          "time": "20210827 19:57"
        },
        {
          "id": "MDRW2100019506.1.4",
          "form": "이젠 대충알것같아서 무대좀 봤거든요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이젠 대충알것같아서 무대좀 봤거든요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100019506.1.5",
          "form": "근데 플레디스는 왤케 장발 아이돌을 하나씩 넣을까욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 플레디스는 왤케 장발 아이돌을 하나씩 넣을까욬ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100019506.1.6",
          "form": "아 ㅋㅋㅋㅋ ㄹㅇ",
          "original_form": "아 ㅋㅋㅋㅋ ㄹㅇ",
          "speaker_id": "2",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100019506.1.7",
          "form": "팬들 어울린다조아하는디 모를",
          "original_form": "팬들 어울린다조아하는디 모를",
          "speaker_id": "2",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100019506.1.8",
          "form": "진짜 제 감성 아니거든요....아니 왜 갑자기.장발...?",
          "original_form": "진짜 제 감성 아니거든요....아니 왜 갑자기.장발...?",
          "speaker_id": "1",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100019506.1.9",
          "form": "뉴이스트도 민기군이 장발이었죠?",
          "original_form": "뉴이스트도 민기군이 장발이었죠?",
          "speaker_id": "1",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019506.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ 네넨0ㄴ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 네넨0ㄴ",
          "speaker_id": "2",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019506.1.11",
          "form": "성수 취향이래요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "성수 취향이래요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019506.1.12",
          "form": "팀마다 장발 하나씩 넣는겤ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "팀마다 장발 하나씩 넣는겤ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:00"
        },
        {
          "id": "MDRW2100019506.1.13",
          "form": "어우 진짜 시러요",
          "original_form": "어우 진짜 시러요",
          "speaker_id": "2",
          "time": "20210827 20:00"
        },
        {
          "id": "MDRW2100019506.1.14",
          "form": "ㅋㅋ그쵸......",
          "original_form": "ㅋㅋ그쵸......",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019506.1.15",
          "form": "장발멤버들도 머리 자른게 훨씬낫더라고요",
          "original_form": "장발멤버들도 머리 자른게 훨씬낫더라고요",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019506.1.16",
          "form": "머리자르고 흑발하니가 이목구비 확살던데 ㅠ",
          "original_form": "머리자르고 흑발하니가 이목구비 확살던데 ㅠ",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019506.1.17",
          "form": "그니까요 익숙한거 해야자 먼 실험을 한다구",
          "original_form": "그니까요 익숙한거 해야자 먼 실험을 한다구",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100019506.1.18",
          "form": "성수머리에나 하지 실험",
          "original_form": "성수머리에나 하지 실험",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100019506.1.19",
          "form": "그러니까요 ㅋㅋㅋㅋ넘웃겨용",
          "original_form": "그러니까요 ㅋㅋㅋㅋ넘웃겨용",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100019506.1.20",
          "form": "남자아이돌은 걍 머리기르지말구",
          "original_form": "남자아이돌은 걍 머리기르지말구",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100019506.1.21",
          "form": "아 잘생긴!! 남돌은",
          "original_form": "아 잘생긴!! 남돌은",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100019506.1.22",
          "form": "그냥 깔끔한머리가최곤데...",
          "original_form": "그냥 깔끔한머리가최곤데...",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100019506.1.23",
          "form": "마자유  얼굴 망가뜨릴거면 이미 망가진 얼궇만 망가뜨려라!",
          "original_form": "마자유  얼굴 망가뜨릴거면 이미 망가진 얼궇만 망가뜨려라!",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100019506.1.24",
          "form": "ㅋㅋㅋ잘생긴사람은 뭔들 이긴하지만 그래도 흑발이 최고!!",
          "original_form": "ㅋㅋㅋ잘생긴사람은 뭔들 이긴하지만 그래도 흑발이 최고!!",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100019506.1.25",
          "form": "마자융",
          "original_form": "마자융",
          "speaker_id": "2",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100019506.1.26",
          "form": "체인지데이즈라는 프로그램 아시나요?",
          "original_form": "체인지데이즈라는 프로그램 아시나요?",
