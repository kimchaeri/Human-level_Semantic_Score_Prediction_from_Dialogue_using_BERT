      "utterance": [
        {
          "id": "MDRW2100016204.1.1",
          "form": "요즘 학교생활 어떠세요?",
          "original_form": "요즘 학교생활 어떠세요?",
          "speaker_id": "1",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016204.1.2",
          "form": "너무 재미있어요",
          "original_form": "너무 재미있어요",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016204.1.3",
          "form": "뭐하시는데요?",
          "original_form": "뭐하시는데요?",
          "speaker_id": "1",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016204.1.4",
          "form": "그쪽도 비대면하시나요?",
          "original_form": "그쪽도 비대면하시나요?",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016204.1.5",
          "form": "절친이 있으신가보네요?",
          "original_form": "절친이 있으신가보네요?",
          "speaker_id": "1",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016204.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016204.1.7",
          "form": "기숙사 같이 들어갔죠",
          "original_form": "기숙사 같이 들어갔죠",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016204.1.8",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016204.1.9",
          "form": "그 친구 놀리는 맛으로 살아요",
          "original_form": "그 친구 놀리는 맛으로 살아요",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016204.1.10",
          "form": "하루하루 짜릿해요",
          "original_form": "하루하루 짜릿해요",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016204.1.11",
          "form": "절친분이 계셔서 든든하고 행복하시겠어요",
          "original_form": "절친분이 계셔서 든든하고 행복하시겠어요",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016204.1.12",
          "form": "저보다 덤벙대는 애 처음봤거든요",
          "original_form": "저보다 덤벙대는 애 처음봤거든요",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016204.1.13",
          "form": "인성을 고쳐보실 생각은요?",
          "original_form": "인성을 고쳐보실 생각은요?",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016204.1.14",
          "form": "그럼요 아무렴요",
          "original_form": "그럼요 아무렴요",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016204.1.15",
          "form": "그 친구도 그렇게 생각할걸요?",
          "original_form": "그 친구도 그렇게 생각할걸요?",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016204.1.16",
          "form": "ㅇㅖ?",
          "original_form": "ㅇㅖ?",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016204.1.17",
          "form": "제 인성은 갓벽한데요",
          "original_form": "제 인성은 갓벽한데요",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016204.1.18",
          "form": "수강신청은 잘 하셨나요?",
          "original_form": "수강신청은 잘 하셨나요?",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016204.1.19",
          "form": "수강신청 잘 하셨",
          "original_form": "수강신청 잘 하셨",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016204.1.20",
          "form": "헐 제가 치고 있었는데",
          "original_form": "헐 제가 치고 있었는데",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100016204.1.21",
          "form": "ㅉ ㅣㅉ ㅣ뽕",
          "original_form": "ㅉ ㅣㅉ ㅣ뽕",
          "speaker_id": "1",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016204.1.22",
          "form": "저 하나 놓쳐서 다른거 잡았어요",
          "original_form": "저 하나 놓쳐서 다른거 잡았어요",
          "speaker_id": "2",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016204.1.23",
          "form": "아그게 뭐에요;;",
          "original_form": "아그게 뭐에요;;",
          "speaker_id": "2",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016204.1.24",
          "form": "몇학점 들으세요?",
          "original_form": "몇학점 들으세요?",
          "speaker_id": "2",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016204.1.25",
          "form": "17학점아요",
          "original_form": "17학점아요",
          "speaker_id": "1",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016204.1.26",
          "form": "오 ~~",
          "original_form": "오 ~~",
          "speaker_id": "2",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016204.1.27",
          "form": "전 20들어요",
          "original_form": "전 20들어요",
          "speaker_id": "2",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016204.1.28",
          "form": "우 ~~",
          "original_form": "우 ~~",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100016204.1.29",
          "form": "전공 만족도는 어느정도세요?",
          "original_form": "전공 만족도는 어느정도세요?",
          "speaker_id": "2",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100016204.1.30",
          "form": "음 10점만점에 7.5요",
          "original_form": "음 10점만점에 7.5요",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100016204.1.31",
          "form": "그쪽은요",
          "original_form": "그쪽은요",
          "speaker_id": "1",
          "time": "20210826 10:53"
        },
        {
          "id": "MDRW2100016204.1.32",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210826 10:54"
        },
        {
          "id": "MDRW2100016204.1.33",
          "form": "비슷해요 전 8이요",
          "original_form": "비슷해요 전 8이요",
          "speaker_id": "2",
          "time": "20210826 10:54"
        },
        {
          "id": "MDRW2100016204.1.34",
          "form": "동아리 하는 거 있으세옷?",
          "original_form": "동아리 하는 거 있으세옷?",
          "speaker_id": "2",
          "time": "20210826 10:54"
        },
        {
          "id": "MDRW2100016204.1.35",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016204.1.36",
          "form": "아 있엉ㅅ",
          "original_form": "아 있엉ㅅ",
          "speaker_id": "1",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016204.1.37",
          "form": "한남동에서 주로 만나서",
          "original_form": "한남동에서 주로 만나서",
          "speaker_id": "1",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016204.1.38",
          "form": "해요",
          "original_form": "해요",
          "speaker_id": "1",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016204.1.39",
          "form": "오 한남동",
          "original_form": "오 한남동",
          "speaker_id": "2",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016204.1.40",
          "form": "거기 동네 좋잖아요",
          "original_form": "거기 동네 좋잖아요",
          "speaker_id": "2",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016204.1.41",
          "form": "저도 거기서 동아리 했었어요",
          "original_form": "저도 거기서 동아리 했었어요",
          "speaker_id": "2",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016204.1.42",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016204.1.43",
          "form": "아 동아리가 아니라 스터디",
          "original_form": "아 동아리가 아니라 스터디",
          "speaker_id": "2",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016204.1.44",
          "form": "비슷하네요 저희",
          "original_form": "비슷하네요 저희",
          "speaker_id": "1",
