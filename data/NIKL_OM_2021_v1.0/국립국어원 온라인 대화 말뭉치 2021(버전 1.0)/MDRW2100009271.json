      "utterance": [
        {
          "id": "MDRW2100009271.1.1",
          "form": "아 네 재밌게 봤어요",
          "original_form": "아 네 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009271.1.2",
          "form": "헉 보셨구나!!",
          "original_form": "헉 보셨구나!!",
          "speaker_id": "1",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009271.1.3",
          "form": "저 이거 진짜 좋아해서",
          "original_form": "저 이거 진짜 좋아해서",
          "speaker_id": "1",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009271.1.4",
          "form": "다 본방사수 했어요!!",
          "original_form": "다 본방사수 했어요!!",
          "speaker_id": "1",
          "time": "20210822 22:27"
        },
        {
          "id": "MDRW2100009271.1.5",
          "form": "저희 남편도 엄청 챙겨보더라구요",
          "original_form": "저희 남편도 엄청 챙겨보더라구요",
          "speaker_id": "2",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009271.1.6",
          "form": "네 저희 아빠도 엄청 좋아해서",
          "original_form": "네 저희 아빠도 엄청 좋아해서",
          "speaker_id": "1",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009271.1.7",
          "form": "간만에 같이 잘 봤어요",
          "original_form": "간만에 같이 잘 봤어요",
          "speaker_id": "1",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009271.1.8",
          "form": "네 어느팀 응원하셨나요?",
          "original_form": "네 어느팀 응원하셨나요?",
          "speaker_id": "2",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009271.1.9",
          "form": "저는 다 좋은데 그 중에 udt랑 sdt랑 박군!!",
          "original_form": "저는 다 좋은데 그 중에 udt랑 sdt랑 박군!!",
          "speaker_id": "1",
          "time": "20210822 22:28"
        },
        {
          "id": "MDRW2100009271.1.10",
          "form": "아 박군",
          "original_form": "아 박군",
          "speaker_id": "2",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009271.1.11",
          "form": "저도 박군 좋아보이더라구요",
          "original_form": "저도 박군 좋아보이더라구요",
          "speaker_id": "2",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009271.1.12",
          "form": "네 미우새에서도 완전 약간",
          "original_form": "네 미우새에서도 완전 약간",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009271.1.13",
          "form": "좀 챙겨주고 싶은 스타일??",
          "original_form": "좀 챙겨주고 싶은 스타일??",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009271.1.14",
          "form": "좀 짠하죠",
          "original_form": "좀 짠하죠",
          "speaker_id": "2",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009271.1.15",
          "form": "네 엄청 열심히 살아서",
          "original_form": "네 엄청 열심히 살아서",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009271.1.16",
          "form": "잘 살았으면 싶은 사람같아요",
          "original_form": "잘 살았으면 싶은 사람같아요",
          "speaker_id": "1",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009271.1.17",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210822 22:29"
        },
        {
          "id": "MDRW2100009271.1.18",
          "form": "트롯신이 간다에서도 좀 그랬던 것 같네요",
          "original_form": "트롯신이 간다에서도 좀 그랬던 것 같네요",
          "speaker_id": "2",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100009271.1.19",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100009271.1.20",
          "form": "거기서도 완전 짠내",
          "original_form": "거기서도 완전 짠내",
          "speaker_id": "1",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100009271.1.21",
          "form": "그래서 다들 밀어주는 것 같아요",
          "original_form": "그래서 다들 밀어주는 것 같아요",
          "speaker_id": "1",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100009271.1.22",
          "form": "네 미우새에서도 자주 나오는 것 같던데요",
          "original_form": "네 미우새에서도 자주 나오는 것 같던데요",
          "speaker_id": "2",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100009271.1.23",
          "form": "네네 거의 매주 나오더라구요",
          "original_form": "네네 거의 매주 나오더라구요",
