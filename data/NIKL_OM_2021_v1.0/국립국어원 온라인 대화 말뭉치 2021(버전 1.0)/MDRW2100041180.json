      "utterance": [
        {
          "id": "MDRW2100041180.1.1",
          "form": "안녕하세요 name2님",
          "original_form": "안녕하세요 &name2&님",
          "speaker_id": "1",
          "time": "20210905 19:41"
        },
        {
          "id": "MDRW2100041180.1.2",
          "form": "네 안녕하세요",
          "original_form": "네 안녕하세요",
          "speaker_id": "2",
          "time": "20210905 19:41"
        },
        {
          "id": "MDRW2100041180.1.3",
          "form": "오늘 집안일 하셨나용?",
          "original_form": "오늘 집안일 하셨나용?",
          "speaker_id": "1",
          "time": "20210905 19:42"
        },
        {
          "id": "MDRW2100041180.1.4",
          "form": "청소만 했어요",
          "original_form": "청소만 했어요",
          "speaker_id": "2",
          "time": "20210905 19:43"
        },
        {
          "id": "MDRW2100041180.1.5",
          "form": "어떤 청소요? 저는 오늘 설거지만 하고 아무것도 안했어요! ㅋㅋㅋ",
          "original_form": "어떤 청소요? 저는 오늘 설거지만 하고 아무것도 안했어요! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:43"
        },
        {
          "id": "MDRW2100041180.1.6",
          "form": "굴러댕겨요",
          "original_form": "굴러댕겨요",
          "speaker_id": "1",
          "time": "20210905 19:44"
        },
        {
          "id": "MDRW2100041180.1.7",
          "form": "아침에 먼지만 좀 닦았어요ㅋㅋ",
          "original_form": "아침에 먼지만 좀 닦았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:47"
        },
        {
          "id": "MDRW2100041180.1.8",
          "form": "하기싫더라구요",
          "original_form": "하기싫더라구요",
          "speaker_id": "2",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100041180.1.9",
          "form": "저도 하기 싫어요ㅋㅋㅋㅋ 집안일은 왜이렇게 하기 싫을까요",
          "original_form": "저도 하기 싫어요ㅋㅋㅋㅋ 집안일은 왜이렇게 하기 싫을까요",
          "speaker_id": "1",
          "time": "20210905 19:48"
        },
        {
          "id": "MDRW2100041180.1.10",
          "form": "요즘 조금 부지런해져보자 생각하고 청소기를 매일 돌리고 있거든요? 오늘은 아니구요! 근데 먼지가..",
          "original_form": "요즘 조금 부지런해져보자 생각하고 청소기를 매일 돌리고 있거든요? 오늘은 아니구요! 근데 먼지가..",
          "speaker_id": "1",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100041180.1.11",
          "form": "매일 돌리는데 왜 똑같이 나오죠",
          "original_form": "매일 돌리는데 왜 똑같이 나오죠",
          "speaker_id": "1",
          "time": "20210905 19:49"
        },
        {
          "id": "MDRW2100041180.1.12",
          "form": "매일해도 티 안나니까 더 하기 싫어요ㅋㅋㅋㅋ",
          "original_form": "매일해도 티 안나니까 더 하기 싫어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100041180.1.13",
          "form": "대충 쓰는데만 치우고 있어요ㅎ",
          "original_form": "대충 쓰는데만 치우고 있어요ㅎ",
          "speaker_id": "2",
          "time": "20210905 19:50"
        },
        {
          "id": "MDRW2100041180.1.14",
          "form": "맞아요! ㅋㅋㅋ 좀 번쩍번쩍 광이라도 나면 좀 기분이 나을텐데",
          "original_form": "맞아요! ㅋㅋㅋ 좀 번쩍번쩍 광이라도 나면 좀 기분이 나을텐데",
          "speaker_id": "1",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100041180.1.15",
          "form": "집안일 중에 뭐가 제일 하기 싫으세요?",
          "original_form": "집안일 중에 뭐가 제일 하기 싫으세요?",
          "speaker_id": "1",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100041180.1.16",
          "form": "쓰레기 치우는게 제일 귀찮네요ㅋㅋ",
          "original_form": "쓰레기 치우는게 제일 귀찮네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:07"
        },
        {
          "id": "MDRW2100041180.1.17",
          "form": "음식물은 특히 좀...",
          "original_form": "음식물은 특히 좀...",
          "speaker_id": "2",
          "time": "20210905 20:07"
        },
        {
          "id": "MDRW2100041180.1.18",
          "form": "아 음식물 진짜..",
          "original_form": "아 음식물 진짜..",
          "speaker_id": "1",
          "time": "20210905 20:09"
        },
        {
          "id": "MDRW2100041180.1.19",
          "form": "썩지라도 않으면 좀 괜찮을텐데",
          "original_form": "썩지라도 않으면 좀 괜찮을텐데",
          "speaker_id": "1",
          "time": "20210905 20:09"
        },
        {
          "id": "MDRW2100041180.1.20",
          "form": "냄새나고 촉감도 변하고 ㅠ",
          "original_form": "냄새나고 촉감도 변하고 ㅠ",
          "speaker_id": "1",
          "time": "20210905 20:09"
        },
        {
          "id": "MDRW2100041180.1.21",
          "form": "맞아요ㅠㅠ 음쓰건조기도 번거롭더라구요",
          "original_form": "맞아요ㅠㅠ 음쓰건조기도 번거롭더라구요",
          "speaker_id": "2",
          "time": "20210905 20:11"
        },
        {
          "id": "MDRW2100041180.1.22",
          "form": "음쓰건조기 사용하고 계세요?",
          "original_form": "음쓰건조기 사용하고 계세요?",
          "speaker_id": "1",
          "time": "20210905 20:18"
        },
        {
          "id": "MDRW2100041180.1.23",
          "form": "예전에 샀던거요. 근데 거의 방치중이에요ㅋㅋ",
          "original_form": "예전에 샀던거요. 근데 거의 방치중이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:23"
        },
        {
          "id": "MDRW2100041180.1.24",
          "form": "ㅋㅋㅋ그러시군요! 저희 제출할까요? 주제는 아무거나 하셔도 다 좋아용",
          "original_form": "ㅋㅋㅋ그러시군요! 저희 제출할까요? 주제는 아무거나 하셔도 다 좋아용",
          "speaker_id": "1",
          "time": "20210905 20:23"
        }
      ]
    }
  ]
