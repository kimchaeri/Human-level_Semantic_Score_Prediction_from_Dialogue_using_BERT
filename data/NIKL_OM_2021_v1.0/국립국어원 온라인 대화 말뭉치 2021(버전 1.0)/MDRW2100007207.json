      "utterance": [
        {
          "id": "MDRW2100007207.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210822 00:51"
        },
        {
          "id": "MDRW2100007207.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007207.1.3",
          "form": "요즘 사고싶은 물건이 있나요??",
          "original_form": "요즘 사고싶은 물건이 있나요??",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007207.1.4",
          "form": "요즘은 새로 나온 갤럭시워치랑 플립이 사고 싶어요ㅋㅋㅋ",
          "original_form": "요즘은 새로 나온 갤럭시워치랑 플립이 사고 싶어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007207.1.5",
          "form": "바꿀때 되니까 되게 예쁘게 잘 냈더라고요...",
          "original_form": "바꿀때 되니까 되게 예쁘게 잘 냈더라고요...",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007207.1.6",
          "form": "아 저도요!!!",
          "original_form": "아 저도요!!!",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007207.1.7",
          "form": "진짜 플립 너무 예쁘지않나요 ㅠㅠㅠ 버즈도 케이스들 진짜 예쁘고",
          "original_form": "진짜 플립 너무 예쁘지않나요 ㅠㅠㅠ 버즈도 케이스들 진짜 예쁘고",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007207.1.8",
          "form": "진짜 핸드폰 무난하게 잘 쓰고 있어서 바꿀 생각 하나도 없었는데",
          "original_form": "진짜 핸드폰 무난하게 잘 쓰고 있어서 바꿀 생각 하나도 없었는데",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007207.1.9",
          "form": "계속 눈에 밟히는거 있죠",
          "original_form": "계속 눈에 밟히는거 있죠",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007207.1.10",
          "form": "진짜 너무 예쁜거 같아요",
          "original_form": "진짜 너무 예쁜거 같아요",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007207.1.11",
          "form": "저도 핸드폰 아직 괜찮은데 ㅋㅋㅋ큐ㅠㅠ 너무 가지고싶어요",
          "original_form": "저도 핸드폰 아직 괜찮은데 ㅋㅋㅋ큐ㅠㅠ 너무 가지고싶어요",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007207.1.12",
          "form": "저는 라벤더가 취향인데 name2님은 어떤색이 괜찮으셨나요?",
          "original_form": "저는 라벤더가 취향인데 &name2&님은 어떤색이 괜찮으셨나요?",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007207.1.13",
          "form": "저는 베이지색?? 그 색이 진짜 예쁘더라구요!",
          "original_form": "저는 베이지색?? 그 색이 진짜 예쁘더라구요!",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007207.1.14",
          "form": "버즈 2 나온것도 케이스들 너무 귀여워서 ㅠㅠㅠ 사고싳어요 ㅠㅠ",
          "original_form": "버즈 2 나온것도 케이스들 너무 귀여워서 ㅠㅠㅠ 사고싳어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100007207.1.15",
          "form": "그쵸ㅠ 삼성 공홈? 이었나 한정으로 기억하는데",
          "original_form": "그쵸ㅠ 삼성 공홈? 이었나 한정으로 기억하는데",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007207.1.16",
          "form": "시판 해줬으면 좋겠어요",
          "original_form": "시판 해줬으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007207.1.17",
          "form": "진짜 세트로 바로 장만합니다..",
          "original_form": "진짜 세트로 바로 장만합니다..",
          "speaker_id": "1",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007207.1.18",
          "form": "진짜요 ㅠㅜㅜ 삼성 제발 돈벌어줘....",
          "original_form": "진짜요 ㅠㅜㅜ 삼성 제발 돈벌어줘....",
          "speaker_id": "2",
          "time": "20210822 01:01"
        },
        {
          "id": "MDRW2100007207.1.19",
          "form": "사실 제트플립 사게 된 제일 큰 계기가ㅋㅋㅋㅋ",
          "original_form": "사실 제트플립 사게 된 제일 큰 계기가ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007207.1.20",
          "form": "야구보는데 세리머니로 계속 제트플립 세리머니를 하더라고요",
          "original_form": "야구보는데 세리머니로 계속 제트플립 세리머니를 하더라고요",
          "speaker_id": "1",
          "time": "20210822 01:03"
        },
        {
          "id": "MDRW2100007207.1.21",
          "form": "아아 ㅋㅋㅋㅋㅋㅋ 저 그거 들어본거같은데!",
          "original_form": "아아 ㅋㅋㅋㅋㅋㅋ 저 그거 들어본거같은데!",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007207.1.22",
          "form": "대감집 노비들이 저렇게 열심히 홍보하는데 대감집노비의 노비도 지갑 열어야죠...",
          "original_form": "대감집 노비들이 저렇게 열심히 홍보하는데 대감집노비의 노비도 지갑 열어야죠...",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007207.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋ 진짜 이재용씨 텍마머니....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋ 진짜 이재용씨 텍마머니....",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007207.1.24",
          "form": "재용씨 공장 좀 열심히 돌려봐요",
          "original_form": "재용씨 공장 좀 열심히 돌려봐요",
          "speaker_id": "1",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007207.1.25",
          "form": "라벤더 수량 부족하다고 계속 다른색 할 생각 없녜잖아",
          "original_form": "라벤더 수량 부족하다고 계속 다른색 할 생각 없녜잖아",
          "speaker_id": "1",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007207.1.26",
          "form": "아 부족하대요?!",
          "original_form": "아 부족하대요?!",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007207.1.27",
          "form": "대리점에 오늘 연락을 했는데",
          "original_form": "대리점에 오늘 연락을 했는데",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007207.1.28",
          "form": "라벤더 주문이 많아서",
          "original_form": "라벤더 주문이 많아서",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007207.1.29",
          "form": "다른색으로 유도하시더라고요",
          "original_form": "다른색으로 유도하시더라고요",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007207.1.30",
          "form": "재용씨... 눈치챙겨...",
          "original_form": "재용씨... 눈치챙겨...",
          "speaker_id": "2",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007207.1.31",
          "form": "라벤더 막뽑아내...",
          "original_form": "라벤더 막뽑아내...",
          "speaker_id": "2",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007207.1.32",
          "form": "라벤더 물량이 부족하묜",
          "original_form": "라벤더 물량이 부족하묜",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007207.1.33",
          "form": "사전예약으로 준다는 버즈2 물량은....",
          "original_form": "사전예약으로 준다는 버즈2 물량은....",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007207.1.34",
          "form": "진짜 재용씨 눈치챙겨",
          "original_form": "진짜 재용씨 눈치챙겨",
          "speaker_id": "1",
          "time": "20210822 01:09"
        },
        {
          "id": "MDRW2100007207.1.35",
          "form": "좀전에 라벤더 색 보고왔는데",
          "original_form": "좀전에 라벤더 색 보고왔는데",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007207.1.36",
          "form": "인기많을만하네요.....ㅠㅠ",
          "original_form": "인기많을만하네요.....ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007207.1.37",
          "form": "대감집 노비들 기다려라",
          "original_form": "대감집 노비들 기다려라",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007207.1.38",
          "form": "내가 제트플립 살게!",
          "original_form": "내가 제트플립 살게!",
          "speaker_id": "1",
          "time": "20210822 01:11"
        }
      ]
