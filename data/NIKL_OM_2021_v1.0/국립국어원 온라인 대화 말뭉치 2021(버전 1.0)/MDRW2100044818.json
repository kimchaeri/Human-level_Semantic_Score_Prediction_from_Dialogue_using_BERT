      "utterance": [
        {
          "id": "MDRW2100044818.1.1",
          "form": "난 짬뽕 좋아햐",
          "original_form": "난 짬뽕 좋아햐",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044818.1.2",
          "form": "국물 맛잇겟다..",
          "original_form": "국물 맛잇겟다..",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044818.1.3",
          "form": "탕수육 찍먹 부먹?",
          "original_form": "탕수육 찍먹 부먹?",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044818.1.4",
          "form": "찍먹",
          "original_form": "찍먹",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044818.1.5",
          "form": "볶아서 나오는 걱도 좋아함ㅋㅋ",
          "original_form": "볶아서 나오는 걱도 좋아함ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044818.1.6",
          "form": "혹시 부먹?",
          "original_form": "혹시 부먹?",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044818.1.7",
          "form": "나는 다먹",
          "original_form": "나는 다먹",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044818.1.8",
          "form": "부먹도 매력잇더라;",
          "original_form": "부먹도 매력잇더라;",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044818.1.9",
          "form": "사실 부먹을 더 많이먹는거같애",
          "original_form": "사실 부먹을 더 많이먹는거같애",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044818.1.10",
          "form": "다먹ㅋㅋㅋ 인정",
