          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044818.1.12",
          "form": "떡볶이랑 뭐랑 곁들여먹어",
          "original_form": "떡볶이랑 뭐랑 곁들여먹어",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044818.1.13",
          "form": "난 순대 필수",
          "original_form": "난 순대 필수",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044818.1.14",
          "form": "어머 그다음엔",
          "original_form": "어머 그다음엔",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044818.1.15",
          "form": "김말이?",
          "original_form": "김말이?",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044818.1.16",
          "form": "님은 뭐랑 먹어",
          "original_form": "님은 뭐랑 먹어",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044818.1.17",
          "form": "어머 나는 만두 오뎅 등등 튀김필수",
          "original_form": "어머 나는 만두 오뎅 등등 튀김필수",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044818.1.18",
          "form": "그리고 김밥",
          "original_form": "그리고 김밥",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044818.1.19",
          "form": "아 김밥 필수 ㅋㅋㅋㅋㅋ",
