      "utterance": [
        {
          "id": "MDRW2100007454.1.1",
          "form": "게임하는거잇나요",
          "original_form": "게임하는거잇나요",
          "speaker_id": "1",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007454.1.2",
          "form": "리듬게임은 이것저것해요",
          "original_form": "리듬게임은 이것저것해요",
          "speaker_id": "2",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007454.1.3",
          "form": "리듬게임 어떤거요?",
          "original_form": "리듬게임 어떤거요?",
          "speaker_id": "1",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007454.1.4",
          "form": "전 쿠킹덤",
          "original_form": "전 쿠킹덤",
          "speaker_id": "1",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007454.1.5",
          "form": "뱅드림도 조금하고 앙스타도 조금하고... 그래요",
          "original_form": "뱅드림도 조금하고 앙스타도 조금하고... 그래요",
          "speaker_id": "2",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007454.1.6",
          "form": "헉 저도 쿠킹덤해요",
          "original_form": "헉 저도 쿠킹덤해요",
          "speaker_id": "2",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007454.1.7",
          "form": "오 쿠킹덤 공통점이네요ㅎ",
          "original_form": "오 쿠킹덤 공통점이네요ㅎ",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007454.1.8",
          "form": "앙스타 인기진짜많던데",
          "original_form": "앙스타 인기진짜많던데",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007454.1.9",
          "form": "일러가 잘생겨서 그런가봐요",
          "original_form": "일러가 잘생겨서 그런가봐요",
          "speaker_id": "2",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007454.1.10",
          "form": "쿠킹덤은 요즘 안하고있어요",
          "original_form": "쿠킹덤은 요즘 안하고있어요",
          "speaker_id": "2",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007454.1.11",
          "form": "맞아요 저 가끔 봣는데 빨간머리 잘생겻더라구요",
          "original_form": "맞아요 저 가끔 봣는데 빨간머리 잘생겻더라구요",
          "speaker_id": "1",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007454.1.12",
          "form": "할게 별로 없더라구요",
          "original_form": "할게 별로 없더라구요",
          "speaker_id": "2",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007454.1.13",
          "form": "다들 빠져나가는 분위기에요",
          "original_form": "다들 빠져나가는 분위기에요",
          "speaker_id": "1",
          "time": "20210822 02:20"
        },
        {
          "id": "MDRW2100007454.1.14",
          "form": "바다요정때부터",
          "original_form": "바다요정때부터",
          "speaker_id": "1",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007454.1.15",
          "form": "가끔 접속해서 가챠 한번 돌리고... 계속 생산이랑 스테이지 깨는것만 반복이니까 재미가 없더라구요",
          "original_form": "가끔 접속해서 가챠 한번 돌리고... 계속 생산이랑 스테이지 깨는것만 반복이니까 재미가 없더라구요",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007454.1.16",
          "form": "ㅋㅋ맞아요 생산너무귀찮구아레나티켓도",
          "original_form": "ㅋㅋ맞아요 생산너무귀찮구아레나티켓도",
          "speaker_id": "1",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007454.1.17",
          "form": "너무많이주니까 쓰기도귀찮죠ㅠ에구",
          "original_form": "너무많이주니까 쓰기도귀찮죠ㅠ에구",
          "speaker_id": "1",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007454.1.18",
          "form": "왕국 꾸미기라도 하고있기는한데 발열이 너무 심해서 오래 못하겠더라구요ㅠ",
          "original_form": "왕국 꾸미기라도 하고있기는한데 발열이 너무 심해서 오래 못하겠더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210822 02:25"
        },
        {
          "id": "MDRW2100007454.1.19",
          "form": "ㅋㅋ저는왕꾸 아예안해요 데브 주가 쭉쭉 내려가던데 큰일이네요 진짜",
          "original_form": "ㅋㅋ저는왕꾸 아예안해요 데브 주가 쭉쭉 내려가던데 큰일이네요 진짜",
          "speaker_id": "1",
          "time": "20210822 02:26"
        },
        {
          "id": "MDRW2100007454.1.20",
          "form": "헐 진짜요? 하긴 제 주변사람들도 접더라구요 새로운 컨텐츠를 만들어도 돌아올지는 모르겠네요...",
          "original_form": "헐 진짜요? 하긴 제 주변사람들도 접더라구요 새로운 컨텐츠를 만들어도 돌아올지는 모르겠네요...",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007454.1.21",
          "form": "ㅋㅋ3월까지만해두 진짜 트렌드엿는데ㅠ",
          "original_form": "ㅋㅋ3월까지만해두 진짜 트렌드엿는데ㅠ",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007454.1.22",
          "form": "요즘은 무슨게임이 대세일지 모르겠어요",
          "original_form": "요즘은 무슨게임이 대세일지 모르겠어요",
          "speaker_id": "1",
          "time": "20210822 02:29"
        }
      ]
    }
  ]
