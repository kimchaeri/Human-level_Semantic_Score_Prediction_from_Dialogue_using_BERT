      "utterance": [
        {
          "id": "MDRW2100003085.1.1",
          "form": "난 민초파야",
          "original_form": "난 민초파야",
          "speaker_id": "1",
          "time": "20210820 18:26"
        },
        {
          "id": "MDRW2100003085.1.2",
          "form": "미튜",
          "original_form": "미튜",
          "speaker_id": "2",
          "time": "20210820 18:29"
        },
        {
          "id": "MDRW2100003085.1.3",
          "form": "반려동물 좋아해?",
          "original_form": "반려동물 좋아해?",
          "speaker_id": "2",
          "time": "20210820 18:29"
        },
        {
          "id": "MDRW2100003085.1.4",
          "form": "난 원래 동물들 되게 무서워했는데",
          "original_form": "난 원래 동물들 되게 무서워했는데",
          "speaker_id": "1",
          "time": "20210820 18:31"
        },
        {
          "id": "MDRW2100003085.1.5",
          "form": "요즘 친구집 강아지 보니까 너무 귀엽더라ㅎㅎ",
          "original_form": "요즘 친구집 강아지 보니까 너무 귀엽더라ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 18:31"
        },
        {
          "id": "MDRW2100003085.1.6",
          "form": "나도 한번도 안키워봐서 좋은지 모르겠어",
          "original_form": "나도 한번도 안키워봐서 좋은지 모르겠어",
          "speaker_id": "2",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100003085.1.7",
          "form": "짓고싶은 이름있어?",
          "original_form": "짓고싶은 이름있어?",
          "speaker_id": "2",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100003085.1.8",
          "form": "나는 name3!",
          "original_form": "나는 &name3&!",
          "speaker_id": "1",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003085.1.9",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "1",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003085.1.10",
          "form": "귀엽다",
          "original_form": "귀엽다",
          "speaker_id": "2",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003085.1.11",
          "form": "name4 로 해야지",
          "original_form": "&name4& 로 해야지",
          "speaker_id": "2",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003085.1.12",
          "form": "강아지라면",
          "original_form": "강아지라면",
          "speaker_id": "2",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003085.1.13",
          "form": "고양이는 무서울거같아",
          "original_form": "고양이는 무서울거같아",
          "speaker_id": "2",
          "time": "20210820 18:35"
        },
        {
          "id": "MDRW2100003085.1.14",
          "form": "난 고양이는 아직 무서워",
          "original_form": "난 고양이는 아직 무서워",
          "speaker_id": "1",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003085.1.15",
          "form": "친구 고양이가 나한테 하악질하는데 너무 무섭더라",
          "original_form": "친구 고양이가 나한테 하악질하는데 너무 무섭더라",
          "speaker_id": "1",
          "time": "20210820 18:37"
        },
        {
          "id": "MDRW2100003085.1.16",
          "form": "앵기는 맛도 없겠엉 애교가 있어야하는데",
          "original_form": "앵기는 맛도 없겠엉 애교가 있어야하는데",
          "speaker_id": "2",
          "time": "20210820 18:38"
        },
        {
          "id": "MDRW2100003085.1.17",
          "form": "가끔 앵기는 고양이들도 있던데",
          "original_form": "가끔 앵기는 고양이들도 있던데",
          "speaker_id": "1",
          "time": "20210820 18:42"
        },
        {
          "id": "MDRW2100003085.1.18",
          "form": "그런 애들 만나면 너무 좋아질 것 같아ㅋㅋㅋ",
          "original_form": "그런 애들 만나면 너무 좋아질 것 같아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:43"
        },
        {
          "id": "MDRW2100003085.1.19",
          "form": "그러면 대박",
          "original_form": "그러면 대박",
          "speaker_id": "2",
          "time": "20210820 18:43"
        },
        {
          "id": "MDRW2100003085.1.20",
          "form": "자랑하고 다닐듯",
          "original_form": "자랑하고 다닐듯",
          "speaker_id": "2",
          "time": "20210820 18:43"
        },
        {
          "id": "MDRW2100003085.1.21",
          "form": "맞아ㅎㅎ",
          "original_form": "맞아ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 18:46"
        },
        {
          "id": "MDRW2100003085.1.22",
          "form": "친구들 보면 사료값이나 병원비도 장난 아니더라",
          "original_form": "친구들 보면 사료값이나 병원비도 장난 아니더라",
          "speaker_id": "1",
          "time": "20210820 18:47"
        },
        {
          "id": "MDRW2100003085.1.23",
          "form": "책임감도 있어야하고",
          "original_form": "책임감도 있어야하고",
          "speaker_id": "2",
          "time": "20210820 18:48"
        },
        {
          "id": "MDRW2100003085.1.24",
          "form": "늙어서 버리는 사람들이 젤나쁨",
          "original_form": "늙어서 버리는 사람들이 젤나쁨",
          "speaker_id": "2",
          "time": "20210820 18:48"
        },
        {
          "id": "MDRW2100003085.1.25",
          "form": "맞아 가족이고 생명체인데 어떻게 그럴수가 있지",
          "original_form": "맞아 가족이고 생명체인데 어떻게 그럴수가 있지",
          "speaker_id": "1",
          "time": "20210820 18:53"
        }
      ]
    }
