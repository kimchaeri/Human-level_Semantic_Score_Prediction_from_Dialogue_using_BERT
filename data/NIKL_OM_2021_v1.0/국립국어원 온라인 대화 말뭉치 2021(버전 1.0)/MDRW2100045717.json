      "utterance": [
        {
          "id": "MDRW2100045717.1.1",
          "form": "좋아하는 음료는 있으신가요?",
          "original_form": "좋아하는 음료는 있으신가요?",
          "speaker_id": "1",
          "time": "20210908 05:57"
        },
        {
          "id": "MDRW2100045717.1.2",
          "form": "저는 탄산수 좋아해요",
          "original_form": "저는 탄산수 좋아해요",
          "speaker_id": "2",
          "time": "20210908 05:57"
        },
        {
          "id": "MDRW2100045717.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210908 05:57"
        },
        {
          "id": "MDRW2100045717.1.4",
          "form": "탄산수 맛있죠~",
          "original_form": "탄산수 맛있죠~",
          "speaker_id": "1",
          "time": "20210908 05:58"
        },
        {
          "id": "MDRW2100045717.1.5",
          "form": "저도 탄산수에 시럽 타먹는 거 좋아해요",
          "original_form": "저도 탄산수에 시럽 타먹는 거 좋아해요",
          "speaker_id": "1",
          "time": "20210908 05:58"
        },
        {
          "id": "MDRW2100045717.1.6",
          "form": "저는 그냥 향들어간 탄산수만 먹어요 ㅎㅎ",
          "original_form": "저는 그냥 향들어간 탄산수만 먹어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 05:59"
        },
        {
          "id": "MDRW2100045717.1.7",
          "form": "카페 음료는 어떤거 좋아하세요?",
          "original_form": "카페 음료는 어떤거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 06:00"
        },
        {
          "id": "MDRW2100045717.1.8",
          "form": "민초 좋아해요!",
          "original_form": "민초 좋아해요!",
          "speaker_id": "1",
          "time": "20210908 06:00"
        },
        {
          "id": "MDRW2100045717.1.9",
          "form": "민초파시군요",
          "original_form": "민초파시군요",
          "speaker_id": "2",
          "time": "20210908 06:01"
        },
        {
          "id": "MDRW2100045717.1.10",
          "form": "저는 반민초딘입니다.",
          "original_form": "저는 반민초딘입니다.",
          "speaker_id": "2",
          "time": "20210908 06:01"
        },
        {
          "id": "MDRW2100045717.1.11",
          "form": "아앗...",
          "original_form": "아앗...",
          "speaker_id": "1",
          "time": "20210908 06:02"
        },
        {
          "id": "MDRW2100045717.1.12",
          "form": "민초의 아름다움을 모르시다니...",
          "original_form": "민초의 아름다움을 모르시다니...",
          "speaker_id": "1",
          "time": "20210908 06:02"
        },
        {
          "id": "MDRW2100045717.1.13",
          "form": "있으면 먹는데 제돈주고는 안먹어요",
          "original_form": "있으면 먹는데 제돈주고는 안먹어요",
          "speaker_id": "2",
          "time": "20210908 06:02"
        },
        {
          "id": "MDRW2100045717.1.14",
          "form": "강경파는 아니라서",
          "original_form": "강경파는 아니라서",
          "speaker_id": "2",
          "time": "20210908 06:02"
        },
        {
          "id": "MDRW2100045717.1.15",
          "form": "그런데 민초 음료도 카페에서 많이 파나요?",
          "original_form": "그런데 민초 음료도 카페에서 많이 파나요?",
          "speaker_id": "2",
          "time": "20210908 06:03"
        },
        {
          "id": "MDRW2100045717.1.16",
          "form": "그래도 온건파이시니 자칫하면 넘어오시겠네요ㅋㅋ",
          "original_form": "그래도 온건파이시니 자칫하면 넘어오시겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:03"
        },
        {
          "id": "MDRW2100045717.1.17",
          "form": "저도 반민초였는데 먹다보니 정들었어요",
          "original_form": "저도 반민초였는데 먹다보니 정들었어요",
          "speaker_id": "1",
          "time": "20210908 06:03"
        },
        {
          "id": "MDRW2100045717.1.18",
          "form": "저는 아직까지는 다른 맛있는게 많아서 넘어가려면 시간이 걸릴것 같아요 ㅎㅎ",
          "original_form": "저는 아직까지는 다른 맛있는게 많아서 넘어가려면 시간이 걸릴것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 06:04"
        },
        {
          "id": "MDRW2100045717.1.19",
          "form": "선호하는 카페도 있으신가요?",
          "original_form": "선호하는 카페도 있으신가요?",
          "speaker_id": "2",
          "time": "20210908 06:04"
        },
        {
          "id": "MDRW2100045717.1.20",
          "form": "음 그냥 동네 싼 카페 자주가요",
          "original_form": "음 그냥 동네 싼 카페 자주가요",
          "speaker_id": "1",
          "time": "20210908 06:05"
        },
        {
          "id": "MDRW2100045717.1.21",
          "form": "커피없음 좀비라서...",
          "original_form": "커피없음 좀비라서...",
          "speaker_id": "1",
          "time": "20210908 06:05"
        },
        {
          "id": "MDRW2100045717.1.22",
          "form": "저는 커피를 안먹어서 사실 카페에 자주 안가는편이에요",
          "original_form": "저는 커피를 안먹어서 사실 카페에 자주 안가는편이에요",
          "speaker_id": "2",
          "time": "20210908 06:05"
        },
        {
          "id": "MDRW2100045717.1.23",
          "form": "오 그게 좋은 거죠ㅠㅠㅠ",
          "original_form": "오 그게 좋은 거죠ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:07"
        },
        {
          "id": "MDRW2100045717.1.24",
          "form": "카페인도 그냥 각성제지 체력보충제는 아니니까요",
          "original_form": "카페인도 그냥 각성제지 체력보충제는 아니니까요",
          "speaker_id": "1",
          "time": "20210908 06:07"
        },
        {
          "id": "MDRW2100045717.1.25",
          "form": "네 ㅠ 옛날에 시험기간에 카페인 음료 먹었다가 머리가 핑돌아서 쓰러질뻔한 경험이 있어서",
          "original_form": "네 ㅠ 옛날에 시험기간에 카페인 음료 먹었다가 머리가 핑돌아서 쓰러질뻔한 경험이 있어서",
          "speaker_id": "2",
          "time": "20210908 06:08"
        },
        {
          "id": "MDRW2100045717.1.26",
          "form": "그뒤로 카페인을 꺼리게되었어요",
          "original_form": "그뒤로 카페인을 꺼리게되었어요",
          "speaker_id": "2",
          "time": "20210908 06:09"
        },
        {
          "id": "MDRW2100045717.1.27",
          "form": "카페인이 안받나봐요ㅠㅠㅠ",
          "original_form": "카페인이 안받나봐요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:09"
        },
        {
          "id": "MDRW2100045717.1.28",
          "form": "저도 에너지 드링크는 어지럽더라고요",
          "original_form": "저도 에너지 드링크는 어지럽더라고요",
          "speaker_id": "1",
          "time": "20210908 06:10"
        },
        {
          "id": "MDRW2100045717.1.29",
          "form": "그래도 요즘은 백종원씨가 요리법을 잘 알려줘서",
          "original_form": "그래도 요즘은 백종원씨가 요리법을 잘 알려줘서",
          "speaker_id": "1",
          "time": "20210908 06:10"
        },
        {
          "id": "MDRW2100045717.1.30",
          "form": "조리해먹기도 좋은 거 같아요",
          "original_form": "조리해먹기도 좋은 거 같아요",
          "speaker_id": "1",
          "time": "20210908 06:10"
        }
      ]
