        {
          "id": "MDRW2100006387.1.1",
          "form": "그러게요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그러게요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
