        },
        {
          "id": "MDRW2100006387.1.2",
          "form": "다먹어놓고",
          "original_form": "다먹어놓고",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100006387.1.3",
          "form": "내 가장 긴 연애상대였는데",
          "original_form": "내 가장 긴 연애상대였는데",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100006387.1.4",
          "form": "나쁜넘 ㅜㅜ",
          "original_form": "나쁜넘 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100006387.1.5",
          "form": "진작에 말하지ㅋㅋ",
          "original_form": "진작에 말하지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100006387.1.6",
          "form": "순대국밥  저는 순대만 넣어달라 하는데",
          "original_form": "순대국밥  저는 순대만 넣어달라 하는데",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100006387.1.7",
          "form": "진정한순대국밥 ㅋㅋㅋㅋ",
          "original_form": "진정한순대국밥 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100006387.1.8",
          "form": "아....",
          "original_form": "아....",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100006387.1.9",
          "form": "저는 순대 말구",
          "original_form": "저는 순대 말구",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100006387.1.10",
          "form": "부속고기들로 먹어요ㅋㅋ",
          "original_form": "부속고기들로 먹어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100006387.1.11",
          "form": "특사이즈ㅋㅋ",
          "original_form": "특사이즈ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100006387.1.12",
          "form": "저와 반대시군여",
          "original_form": "저와 반대시군여",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100006387.1.13",
          "form": "호불호 갈리는메뉴",
          "original_form": "호불호 갈리는메뉴",
          "speaker_id": "2",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100006387.1.14",
          "form": "완전 맛있는데",
          "original_form": "완전 맛있는데{emoji:♡}",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100006387.1.15",
          "form": "대학때 친구가",
          "original_form": "대학때 친구가",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100006387.1.16",
          "form": "순대를 못먹어서",
          "original_form": "순대를 못먹어서",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100006387.1.17",
          "form": "완전 충격이었어요ㅋㅋ",
          "original_form": "완전 충격이었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100006387.1.18",
          "form": "순대 못먹는사람 꽤 있어요",
          "original_form": "순대 못먹는사람 꽤 있어요",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100006387.1.19",
          "form": "떡볶이에 순대랑 튀김 찍어먹으니까",
          "original_form": "떡볶이에 순대랑 튀김 찍어먹으니까",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100006387.1.20",
          "form": "못먹는건지 안먹는건지",
          "original_form": "못먹는건지 안먹는건지",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
