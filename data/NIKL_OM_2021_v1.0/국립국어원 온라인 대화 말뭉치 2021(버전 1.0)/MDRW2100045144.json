          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045144.1.9",
          "form": "완전 베테랑이시네요!",
          "original_form": "완전 베테랑이시네요!",
          "speaker_id": "1",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045144.1.10",
          "form": "시간이 금방 가요",
          "original_form": "시간이 금방 가요",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045144.1.11",
          "form": "연애시절이 그리워요",
          "original_form": "연애시절이 그리워요",
          "speaker_id": "2",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045144.1.12",
          "form": "소개팅하고싶어요ㅋㅋㅋ",
