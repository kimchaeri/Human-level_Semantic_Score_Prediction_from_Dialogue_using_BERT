      "utterance": [
        {
          "id": "MDRW2100034862.1.1",
          "form": "배고픈데",
          "original_form": "배고픈데",
          "speaker_id": "1",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100034862.1.2",
          "form": "음식점은 닫았고",
          "original_form": "음식점은 닫았고",
          "speaker_id": "1",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100034862.1.3",
          "form": "배달 음식 시킬지 고민된다",
          "original_form": "배달 음식 시킬지 고민된다",
          "speaker_id": "1",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100034862.1.4",
          "form": "먹고 싶은 거 있어?",
          "original_form": "먹고 싶은 거 있어?",
          "speaker_id": "2",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100034862.1.5",
          "form": "음 막",
          "original_form": "음 막",
          "speaker_id": "1",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100034862.1.6",
          "form": "뭐 먹고 싶다 하는",
          "original_form": "뭐 먹고 싶다 하는",
          "speaker_id": "1",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100034862.1.7",
          "form": "욕구는 별로 안 드는데",
          "original_form": "욕구는 별로 안 드는데",
          "speaker_id": "1",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100034862.1.8",
          "form": "평소 습관 때문인 듯 이것도",
          "original_form": "평소 습관 때문인 듯 이것도",
          "speaker_id": "1",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100034862.1.9",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100034862.1.10",
          "form": "진짜 이상해 느김이",
          "original_form": "진짜 이상해 느김이",
          "speaker_id": "1",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100034862.1.11",
          "form": "뭘 생각해도 그냥 그거 예전에 맛있게 먹엇엇지... 이 생각이고",
          "original_form": "뭘 생각해도 그냥 그거 예전에 맛있게 먹엇엇지... 이 생각이고",
          "speaker_id": "1",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100034862.1.12",
          "form": "진짜로 병원 가보는 게 오땨...",
          "original_form": "진짜로 병원 가보는 게 오땨...",
          "speaker_id": "2",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100034862.1.13",
          "form": "와 맛있겠다 당장 먹고 싶다 이게 아니라",
          "original_form": "와 맛있겠다 당장 먹고 싶다 이게 아니라",
          "speaker_id": "1",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100034862.1.14",
          "form": "막 암 걸린 거 아니겠지?????",
          "original_form": "막 암 걸린 거 아니겠지?????",
          "speaker_id": "1",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100034862.1.15",
          "form": "갑자기 이상한 생각이",
          "original_form": "갑자기 이상한 생각이",
          "speaker_id": "1",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100034862.1.16",
          "form": "암까지는 아니더라도",
          "original_form": "암까지는 아니더라도",
          "speaker_id": "2",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100034862.1.17",
          "form": "갑자기 식욕 떨어지면 질환일 가능성이 높다고 하길래...",
          "original_form": "갑자기 식욕 떨어지면 질환일 가능성이 높다고 하길래...",
          "speaker_id": "2",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100034862.1.18",
          "form": "월요일에 내과 한 번 다녀와...",
          "original_form": "월요일에 내과 한 번 다녀와...",
          "speaker_id": "2",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100034862.1.19",
          "form": "그래야겠다... 다리 상태도 볼 겸",
          "original_form": "그래야겠다... 다리 상태도 볼 겸",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034862.1.20",
          "form": "다리는 외과로 가지 않아?",
          "original_form": "다리는 외과로 가지 않아?",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034862.1.21",
          "form": "내과랑 외과랑 같이 하는 병원인가?",
          "original_form": "내과랑 외과랑 같이 하는 병원인가?",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100034862.1.22",
          "form": "화상 상처는 그냥 가정의학과 가서",
          "original_form": "화상 상처는 그냥 가정의학과 가서",
          "speaker_id": "1",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100034862.1.23",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100034862.1.24",
          "form": "아 내과 가라 했구나",
          "original_form": "아 내과 가라 했구나",
          "speaker_id": "1",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100034862.1.25",
          "form": "가정의학과 가서 식욕 관련 물어봐도 되지 않을까",
          "original_form": "가정의학과 가서 식욕 관련 물어봐도 되지 않을까",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100034862.1.26",
          "form": "그래 일단 가서 한번 말해보자",
          "original_form": "그래 일단 가서 한번 말해보자",
          "speaker_id": "2",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100034862.1.27",
          "form": "그래야겟다",
          "original_form": "그래야겟다",
          "speaker_id": "1",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100034862.1.28",
          "form": "하~",
          "original_form": "하~",
          "speaker_id": "1",
          "time": "20210828 21:27"
        }
      ]
    }
  ]
