      "utterance": [
        {
          "id": "MDRW2100034753.1.1",
          "form": "만화고기ㅋㅋ",
          "original_form": "만화고기ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100034753.1.2",
          "form": "해리포터 만찬때도요",
          "original_form": "해리포터 만찬때도요",
          "speaker_id": "2",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100034753.1.3",
          "form": "거기는 다 신기하고 맛있어보여요",
          "original_form": "거기는 다 신기하고 맛있어보여요",
          "speaker_id": "2",
          "time": "20210828 18:45"
        },
        {
          "id": "MDRW2100034753.1.4",
          "form": "지금 오상욱이 비빔면 7봉먹는게 나오네요",
          "original_form": "지금 오상욱이 비빔면 7봉먹는게 나오네요",
          "speaker_id": "2",
          "time": "20210828 18:45"
        },
        {
          "id": "MDRW2100034753.1.5",
          "form": "시상에...",
          "original_form": "시상에...",
          "speaker_id": "2",
          "time": "20210828 18:45"
        },
        {
          "id": "MDRW2100034753.1.6",
          "form": "아니 뭘 보고 계시나요",
          "original_form": "아니 뭘 보고 계시나요",
          "speaker_id": "1",
          "time": "20210828 18:46"
        },
        {
          "id": "MDRW2100034753.1.7",
          "form": "ㅋㅋㅋㅋ비빔면 3봉까진 저두 먹을 수 있을거 같아요",
          "original_form": "ㅋㅋㅋㅋ비빔면 3봉까진 저두 먹을 수 있을거 같아요",
          "speaker_id": "1",
          "time": "20210828 18:47"
        },
        {
          "id": "MDRW2100034753.1.8",
          "form": "나혼자산다요",
          "original_form": "나혼자산다요",
          "speaker_id": "2",
          "time": "20210828 18:47"
        },
        {
          "id": "MDRW2100034753.1.9",
          "form": "비빔면 1봉은 솔직히 넘적쥬",
          "original_form": "비빔면 1봉은 솔직히 넘적쥬",
          "speaker_id": "2",
          "time": "20210828 18:47"
        },
        {
          "id": "MDRW2100034753.1.10",
          "form": "약간 사리면 느낌이죠",
          "original_form": "약간 사리면 느낌이죠",
          "speaker_id": "1",
          "time": "20210828 18:47"
        },
        {
          "id": "MDRW2100034753.1.11",
          "form": "메인 먹으면서 사이드로 1봉 하세요..",
          "original_form": "메인 먹으면서 사이드로 1봉 하세요..",
          "speaker_id": "1",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100034753.1.12",
          "form": "오상욱씨라고 펜싱선수가 비빔면 7봉과 피자 한판을 둘이서 나눠먹네요",
          "original_form": "오상욱씨라고 펜싱선수가 비빔면 7봉과 피자 한판을 둘이서 나눠먹네요",
          "speaker_id": "2",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100034753.1.13",
          "form": "요런 양",
          "original_form": "요런 양",
          "speaker_id": "1",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100034753.1.14",
          "form": "역시 운동선수네요",
          "original_form": "역시 운동선수네요",
          "speaker_id": "1",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100034753.1.15",
          "form": "ㅋㅋ사이드 1봉 적절합니다",
          "original_form": "ㅋㅋ사이드 1봉 적절합니다",
          "speaker_id": "2",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100034753.1.16",
          "form": "활활 태우고 있나봅니다",
          "original_form": "활활 태우고 있나봅니다",
          "speaker_id": "1",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100034753.1.17",
          "form": "비빔면 맛있죠 느끼한 피자와 페어링",
          "original_form": "비빔면 맛있죠 느끼한 피자와 페어링",
          "speaker_id": "1",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100034753.1.18",
          "form": "이야...부럽네요 그의 소화력과 신진대사",
          "original_form": "이야...부럽네요 그의 소화력과 신진대사",
          "speaker_id": "2",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100034753.1.19",
          "form": "느끼한피자와 매콤 비빔면",
          "original_form": "느끼한피자와 매콤 비빔면",
          "speaker_id": "2",
          "time": "20210828 18:50"
        },
        {
          "id": "MDRW2100034753.1.20",
          "form": "조합 너무 좋은데 제가 그렇게 먹었다간 2시간 내 장선생과 화장실에서 심각헌 면담을 할 게 확실하네요",
          "original_form": "조합 너무 좋은데 제가 그렇게 먹었다간 2시간 내 장선생과 화장실에서 심각헌 면담을 할 게 확실하네요",
          "speaker_id": "1",
          "time": "20210828 18:51"
        },
        {
          "id": "MDRW2100034753.1.21",
          "form": "ㅋㅋ저도요 제 위장이 저를 용서치 않을거같아요",
          "original_form": "ㅋㅋ저도요 제 위장이 저를 용서치 않을거같아요",
          "speaker_id": "2",
          "time": "20210828 18:52"
        },
        {
          "id": "MDRW2100034753.1.22",
          "form": "누구는 치킨하고 불닭면을 같이 먹는데요",
          "original_form": "누구는 치킨하고 불닭면을 같이 먹는데요",
          "speaker_id": "2",
          "time": "20210828 18:52"
        },
        {
          "id": "MDRW2100034753.1.23",
          "form": "아.. 와우 튀김과 불닭면",
          "original_form": "아.. 와우 튀김과 불닭면",
          "speaker_id": "1",
          "time": "20210828 18:59"
        },
        {
          "id": "MDRW2100034753.1.24",
          "form": "장트라블타의 최고 콤비네이션이죠",
          "original_form": "장트라블타의 최고 콤비네이션이죠",
          "speaker_id": "1",
          "time": "20210828 18:59"
        },
        {
          "id": "MDRW2100034753.1.25",
          "form": "그니까요ㅋㅋ대단한분들",
          "original_form": "그니까요ㅋㅋ대단한분들",
          "speaker_id": "2",
          "time": "20210828 19:22"
        },
        {
          "id": "MDRW2100034753.1.26",
          "form": "저녁은 뭐 드셨어요?",
          "original_form": "저녁은 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034753.1.27",
          "form": "저 오늘 삼치구이랑 김치찌개 먹었어요!",
          "original_form": "저 오늘 삼치구이랑 김치찌개 먹었어요!",
          "speaker_id": "1",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100034753.1.28",
          "form": "오 삼치드셨군요!",
          "original_form": "오 삼치드셨군요!",
          "speaker_id": "2",
          "time": "20210828 20:05"
        },
        {
          "id": "MDRW2100034753.1.29",
          "form": "저는 토마토카레 먹었어요ㅎㅎ",
          "original_form": "저는 토마토카레 먹었어요ㅎㅎ",
