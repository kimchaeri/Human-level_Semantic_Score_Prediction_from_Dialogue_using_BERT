      "utterance": [
        {
          "id": "MDRW2100021882.1.1",
          "form": "얼마나 했나?",
          "original_form": "얼마나 했나?",
          "speaker_id": "1",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100021882.1.2",
          "form": "23갴ㅋㅌㅌㅌㅌㅌㅌ 어쩐지 힘들더라",
          "original_form": "23갴ㅋㅌㅌㅌㅌㅌㅌ 어쩐지 힘들더라",
          "speaker_id": "2",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100021882.1.3",
          "form": "ㅋㅋㅋㅋ힘드럭",
          "original_form": "ㅋㅋㅋㅋ힘드럭",
          "speaker_id": "1",
          "time": "20210828 23:35"
        },
        {
          "id": "MDRW2100021882.1.4",
          "form": "코로나 끝나면 배구보러 다닐거니?",
          "original_form": "코로나 끝나면 배구보러 다닐거니?",
          "speaker_id": "2",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021882.1.5",
          "form": "된다면?",
          "original_form": "된다면?",
          "speaker_id": "1",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021882.1.6",
          "form": "요새 야구도 찔끔찔끔 보는데",
          "original_form": "요새 야구도 찔끔찔끔 보는데",
          "speaker_id": "1",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100021882.1.7",
          "form": "못해서?",
          "original_form": "못해서?",
          "speaker_id": "2",
          "time": "20210828 23:38"
        },
        {
          "id": "MDRW2100021882.1.8",
          "form": "이걸 봐 말어 (묵음처리) 그 어딘가",
          "original_form": "이걸 봐 말어 (묵음처리) 그 어딘가",
          "speaker_id": "1",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100021882.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:40"
        },
        {
          "id": "MDRW2100021882.1.10",
          "form": "셉틴 발야구 볼래?",
          "original_form": "셉틴 발야구 볼래?",
          "speaker_id": "2",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100021882.1.11",
          "form": "ㅋㅋㅋㅌㅌㅌㅌ",
          "original_form": "ㅋㅋㅋㅌㅌㅌㅌ",
          "speaker_id": "2",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100021882.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ갑자기 이렇게?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ갑자기 이렇게?",
          "speaker_id": "1",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100021882.1.13",
          "form": "걔네는 잘하니?",
          "original_form": "걔네는 잘하니?",
          "speaker_id": "1",
          "time": "20210828 23:41"
        },
        {
          "id": "MDRW2100021882.1.14",
          "form": "아이템 써서 정정당당하진 않지만 그래서 웃겨",
          "original_form": "아이템 써서 정정당당하진 않지만 그래서 웃겨",
          "speaker_id": "2",
          "time": "20210828 23:42"
        },
        {
          "id": "MDRW2100021882.1.15",
          "form": "예능 스포츠구나ㅋㅋㅋ",
          "original_form": "예능 스포츠구나ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100021882.1.16",
          "form": "예능이죠",
          "original_form": "예능이죠",
          "speaker_id": "2",
          "time": "20210828 23:43"
        },
        {
          "id": "MDRW2100021882.1.17",
          "form": "런닝맨 야구 재밌었는데",
          "original_form": "런닝맨 야구 재밌었는데",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100021882.1.18",
          "form": "안타 한번에 23점을 내는 이광수 선수",
          "original_form": "안타 한번에 23점을 내는 이광수 선수",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100021882.1.19",
          "form": "23점이 어떻게 나왘ㅋㅋㅋㅋㅋㅋㅋ거기 야구도 이상해",
          "original_form": "23점이 어떻게 나왘ㅋㅋㅋㅋㅋㅋㅋ거기 야구도 이상해",
