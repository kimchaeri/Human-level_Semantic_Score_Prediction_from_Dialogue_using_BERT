      "utterance": [
        {
          "id": "MDRW2100017007.1.1",
          "form": "스포츠경기 관람 좋아하시나요?",
          "original_form": "스포츠경기 관람 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017007.1.2",
          "form": "이번에 코보컵 시간날때 보고 있어요",
          "original_form": "이번에 코보컵 시간날때 보고 있어요",
          "speaker_id": "1",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100017007.1.3",
          "form": "오 그러시구나",
          "original_form": "오 그러시구나",
          "speaker_id": "2",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100017007.1.4",
          "form": "이번 올림픽 배구경기보고 관심이 생겨서요",
          "original_form": "이번 올림픽 배구경기보고 관심이 생겨서요",
          "speaker_id": "1",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100017007.1.5",
          "form": "저도 이번 올림픽 보고",
          "original_form": "저도 이번 올림픽 보고",
          "speaker_id": "2",
          "time": "20210826 22:20"
        },
        {
          "id": "MDRW2100017007.1.6",
          "form": "여자배구에 대한 관심이 생겼어요",
          "original_form": "여자배구에 대한 관심이 생겼어요",
          "speaker_id": "2",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100017007.1.7",
          "form": "더 열심히 응원하고 관심 가져야겠다 생각했어요",
          "original_form": "더 열심히 응원하고 관심 가져야겠다 생각했어요",
          "speaker_id": "2",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100017007.1.8",
          "form": "그죠! 좀더 선수들이 좋은 대우를 받았으면 해서요",
          "original_form": "그죠! 좀더 선수들이 좋은 대우를 받았으면 해서요",
          "speaker_id": "1",
          "time": "20210826 22:22"
        },
        {
          "id": "MDRW2100017007.1.9",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100017007.1.10",
          "form": "남녀선수 연봉이나 지원 차이도 심하다고 해서",
          "original_form": "남녀선수 연봉이나 지원 차이도 심하다고 해서",
          "speaker_id": "2",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100017007.1.11",
          "form": "관심을 갖고 열심히 응원해야겠단 생각이 들더라고요",
          "original_form": "관심을 갖고 열심히 응원해야겠단 생각이 들더라고요",
          "speaker_id": "2",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100017007.1.12",
          "form": "아직 응원팀을 못골라서 경기전부 즐겁게 보고있어요",
          "original_form": "아직 응원팀을 못골라서 경기전부 즐겁게 보고있어요",
          "speaker_id": "1",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100017007.1.13",
          "form": "관람하고 계신가요?",
          "original_form": "관람하고 계신가요?",
          "speaker_id": "1",
          "time": "20210826 22:26"
        },
        {
          "id": "MDRW2100017007.1.14",
          "form": "저도 아직은 응원팀은 선택을 못했는데",
          "original_form": "저도 아직은 응원팀은 선택을 못했는데",
          "speaker_id": "2",
          "time": "20210826 22:27"
        },
        {
          "id": "MDRW2100017007.1.15",
          "form": "일단 경기 자체를 즐기며 관람하고 있어요",
          "original_form": "일단 경기 자체를 즐기며 관람하고 있어요",
          "speaker_id": "2",
          "time": "20210826 22:28"
        },
        {
          "id": "MDRW2100017007.1.16",
          "form": "응원하시는 분들 이야기를 들어보면 오히려 지금이 속편하고 재밌게 볼수있다고 하시더라구욕",
          "original_form": "응원하시는 분들 이야기를 들어보면 오히려 지금이 속편하고 재밌게 볼수있다고 하시더라구욕",
          "speaker_id": "1",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100017007.1.17",
          "form": "그래서 저도 즐기고있어요",
          "original_form": "그래서 저도 즐기고있어요",
          "speaker_id": "1",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100017007.1.18",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100017007.1.19",
          "form": "그럼 저도 즐기며 배구스포츠 관람해야겠네요",
          "original_form": "그럼 저도 즐기며 배구스포츠 관람해야겠네요",
          "speaker_id": "2",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100017007.1.20",
          "form": "배구말고는 보시는 스포츠 있으세요?",
          "original_form": "배구말고는 보시는 스포츠 있으세요?",
          "speaker_id": "1",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100017007.1.21",
          "form": "가끔 야구 봐요",
          "original_form": "가끔 야구 봐요",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100017007.1.22",
          "form": "야구 어느팀 좋아하세요?",
          "original_form": "야구 어느팀 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100017007.1.23",
          "form": "두산베어스요",
          "original_form": "두산베어스요",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100017007.1.24",
          "form": "예전엔 야구장 가서 관람하고 응원도 했는데",
          "original_form": "예전엔 야구장 가서 관람하고 응원도 했는데",
          "speaker_id": "2",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100017007.1.25",
          "form": "요즘은... 코로나때문에 힘드시겠네요",
          "original_form": "요즘은... 코로나때문에 힘드시겠네요",
          "speaker_id": "1",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100017007.1.26",
          "form": "네 코로나 이후엔 직관은 못하고 있죠",
          "original_form": "네 코로나 이후엔 직관은 못하고 있죠",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100017007.1.27",
          "form": "아 농구 경기장도 가까워서 몇번 관람하러 갔어요",
          "original_form": "아 농구 경기장도 가까워서 몇번 관람하러 갔어요",
          "speaker_id": "2",
          "time": "20210826 22:36"
        }
      ]
    }
