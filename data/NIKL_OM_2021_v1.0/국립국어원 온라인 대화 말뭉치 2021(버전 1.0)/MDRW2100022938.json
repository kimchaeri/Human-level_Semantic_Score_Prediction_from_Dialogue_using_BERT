      "utterance": [
        {
          "id": "MDRW2100022938.1.1",
          "form": "날씨가 오늘도 엄청 더웠어",
          "original_form": "날씨가 오늘도 엄청 더웠어",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022938.1.2",
          "form": "거긴어때",
          "original_form": "거긴어때",
          "speaker_id": "2",
          "time": "20210829 19:22"
        },
        {
          "id": "MDRW2100022938.1.3",
          "form": "여기도 볕이 꽤 쬐더라",
          "original_form": "여기도 볕이 꽤 쬐더라",
          "speaker_id": "1",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022938.1.4",
          "form": "비 안 오고 좋았어",
          "original_form": "비 안 오고 좋았어",
          "speaker_id": "1",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022938.1.5",
          "form": "우린 뒤늦게 바람이 센게",
          "original_form": "우린 뒤늦게 바람이 센게",
          "speaker_id": "2",
          "time": "20210829 19:23"
        },
        {
          "id": "MDRW2100022938.1.6",
          "form": "비올것같아",
          "original_form": "비올것같아",
          "speaker_id": "2",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022938.1.7",
          "form": "아 거긴 다시 비 와?",
          "original_form": "아 거긴 다시 비 와?",
          "speaker_id": "1",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022938.1.8",
          "form": "여긴 아직 비소식이 없네",
          "original_form": "여긴 아직 비소식이 없네",
          "speaker_id": "1",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022938.1.9",
          "form": "넌 비 오는 날 좋아해?",
          "original_form": "넌 비 오는 날 좋아해?",
          "speaker_id": "1",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022938.1.10",
          "form": "비가 다시올지도 몰라",
          "original_form": "비가 다시올지도 몰라",
          "speaker_id": "2",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022938.1.11",
          "form": "난 비오는날이 싫어",
          "original_form": "난 비오는날이 싫어",
          "speaker_id": "2",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022938.1.12",
          "form": "오 그럼 별로 안 좋겠네",
          "original_form": "오 그럼 별로 안 좋겠네",
          "speaker_id": "1",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022938.1.13",
          "form": "안타깝구나",
          "original_form": "안타깝구나",
          "speaker_id": "1",
          "time": "20210829 19:24"
        },
        {
          "id": "MDRW2100022938.1.14",
          "form": "나도 비는 별로 안 좋아해",
          "original_form": "나도 비는 별로 안 좋아해",
          "speaker_id": "1",
          "time": "20210829 19:25"
        },
        {
          "id": "MDRW2100022938.1.15",
          "form": "비오면 기운이 없엊ㅕ",
          "original_form": "비오면 기운이 없엊ㅕ",
          "speaker_id": "2",
          "time": "20210829 19:25"
        },
        {
          "id": "MDRW2100022938.1.16",
          "form": "습기도 짜증나고",
          "original_form": "습기도 짜증나고",
          "speaker_id": "2",
          "time": "20210829 19:25"
        },
        {
          "id": "MDRW2100022938.1.17",
          "form": "맞아 쾌적한 날씨가 좋아",
          "original_form": "맞아 쾌적한 날씨가 좋아",
          "speaker_id": "1",
          "time": "20210829 19:25"
        },
        {
          "id": "MDRW2100022938.1.18",
          "form": "나도 습한거 안 좋아해",
          "original_form": "나도 습한거 안 좋아해",
          "speaker_id": "1",
          "time": "20210829 19:25"
        },
        {
          "id": "MDRW2100022938.1.19",
          "form": "나는 맑은 날씨도 좋아하지만",
          "original_form": "나는 맑은 날씨도 좋아하지만",
          "speaker_id": "2",
          "time": "20210829 19:26"
        },
        {
          "id": "MDRW2100022938.1.20",
          "form": "바람 세게부는 날도 좋아",
          "original_form": "바람 세게부는 날도 좋아",
          "speaker_id": "2",
          "time": "20210829 19:26"
        },
        {
          "id": "MDRW2100022938.1.21",
          "form": "나도 왠지 바람 부는 날 좋아",
          "original_form": "나도 왠지 바람 부는 날 좋아",
          "speaker_id": "1",
          "time": "20210829 19:26"
        },
        {
          "id": "MDRW2100022938.1.22",
          "form": "걱정이 날아가는 것 같더라",
          "original_form": "걱정이 날아가는 것 같더라",
          "speaker_id": "1",
          "time": "20210829 19:26"
        },
        {
          "id": "MDRW2100022938.1.23",
          "form": "나이 먹으니까 눈오는건 싫더라",
          "original_form": "나이 먹으니까 눈오는건 싫더라",
          "speaker_id": "2",
          "time": "20210829 19:26"
        },
        {
          "id": "MDRW2100022938.1.24",
          "form": "아정말?",
          "original_form": "아정말?",
          "speaker_id": "1",
          "time": "20210829 19:26"
        },
        {
          "id": "MDRW2100022938.1.25",
          "form": "왜 눈 오는 게 싫어?",
          "original_form": "왜 눈 오는 게 싫어?",
          "speaker_id": "1",
          "time": "20210829 19:27"
        },
        {
          "id": "MDRW2100022938.1.26",
          "form": "다칠까봐 걱정돼서 싫어지더라고",
          "original_form": "다칠까봐 걱정돼서 싫어지더라고",
          "speaker_id": "2",
          "time": "20210829 19:27"
        }
      ]
    }
  ]
