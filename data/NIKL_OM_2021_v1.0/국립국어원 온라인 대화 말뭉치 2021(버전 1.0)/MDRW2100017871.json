      "utterance": [
        {
          "id": "MDRW2100017871.1.1",
          "form": "학교 다니세요?",
          "original_form": "학교 다니세요?",
          "speaker_id": "2",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017871.1.2",
          "form": "네네 회사랑 대학원 같이 다녀요",
          "original_form": "네네 회사랑 대학원 같이 다녀요",
          "speaker_id": "1",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017871.1.3",
          "form": "와 멋잇다",
          "original_form": "와 멋잇다",
          "speaker_id": "2",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017871.1.4",
          "form": "바쁘게 사시네요",
          "original_form": "바쁘게 사시네요",
          "speaker_id": "2",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017871.1.5",
          "form": "너무 힘들어요ㅠㅠ흑흑",
          "original_form": "너무 힘들어요ㅠㅠ흑흑",
          "speaker_id": "1",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017871.1.6",
          "form": "곧 개강인데 큰일났어요",
          "original_form": "곧 개강인데 큰일났어요",
          "speaker_id": "1",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100017871.1.7",
          "form": "코로나때문에 온라인수업인가요?",
          "original_form": "코로나때문에 온라인수업인가요?",
          "speaker_id": "2",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017871.1.8",
          "form": "네 대부분 온라인인데 단계따라 혼합이에요",
          "original_form": "네 대부분 온라인인데 단계따라 혼합이에요",
          "speaker_id": "1",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017871.1.9",
          "form": "코로나때문에 한동안은 온라인이겟네요 ㅠㅠ",
          "original_form": "코로나때문에 한동안은 온라인이겟네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017871.1.10",
          "form": "맞아요 저에겐 그래도 온라인이 나아서 다행이에요 이거 하나만큼은",
          "original_form": "맞아요 저에겐 그래도 온라인이 나아서 다행이에요 이거 하나만큼은",
          "speaker_id": "1",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100017871.1.11",
          "form": "회사랑 학교가 정반대 극과극이어서요 ㅎㅎ",
          "original_form": "회사랑 학교가 정반대 극과극이어서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100017871.1.12",
          "form": "아 지옥의 일정일뻔 햇네요 ㄷㄷ",
          "original_form": "아 지옥의 일정일뻔 햇네요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100017871.1.13",
          "form": "그쵸 그나마 딱 다행인 한가지는 이것",
          "original_form": "그쵸 그나마 딱 다행인 한가지는 이것",
          "speaker_id": "1",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100017871.1.14",
          "form": "이제 학교도 사실 꼭 오프라인일 필요가 없을 것 같아요",
          "original_form": "이제 학교도 사실 꼭 오프라인일 필요가 없을 것 같아요",
          "speaker_id": "1",
          "time": "20210826 22:16"
        },
        {
          "id": "MDRW2100017871.1.15",
          "form": "ㅋㅋㅋㅋ제가 너무 예전 사고방식으로 생각하고 잇엇나봐요",
          "original_form": "ㅋㅋㅋㅋ제가 너무 예전 사고방식으로 생각하고 잇엇나봐요",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100017871.1.16",
          "form": "전 시끌벅적한 개강분위기 이런게 너무 좋앗거든요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 시끌벅적한 개강분위기 이런게 너무 좋앗거든요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100017871.1.17",
          "form": "아 그렇군요 !! 그건 너무넘 좋죠 축제랑 ㅠㅠ",
          "original_form": "아 그렇군요 !! 그건 너무넘 좋죠 축제랑 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100017871.1.18",
          "form": "하지만 수업은 온라인으로 하고싶어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "하지만 수업은 온라인으로 하고싶어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100017871.1.19",
          "form": "시간절약  너무 멀어서ㅠ",
          "original_form": "시간절약 {emoji:♡} 너무 멀어서ㅠ",
