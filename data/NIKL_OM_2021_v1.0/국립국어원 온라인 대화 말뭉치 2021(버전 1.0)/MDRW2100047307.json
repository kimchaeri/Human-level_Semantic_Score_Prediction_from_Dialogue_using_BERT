      "utterance": [
        {
          "id": "MDRW2100047307.1.1",
          "form": "해도 티가 안나요 ㅋ",
          "original_form": "해도 티가 안나요 ㅋ",
          "speaker_id": "2",
          "time": "20210826 10:33"
        },
        {
          "id": "MDRW2100047307.1.2",
          "form": "좀있다 이디야 가려고요",
          "original_form": "좀있다 이디야 가려고요",
          "speaker_id": "1",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100047307.1.3",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100047307.1.4",
          "form": "쿠폰이 좀있는데",
          "original_form": "쿠폰이 좀있는데",
          "speaker_id": "1",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100047307.1.5",
          "form": "가서 먹고오려고요",
          "original_form": "가서 먹고오려고요",
          "speaker_id": "1",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100047307.1.6",
          "form": "오 조용한 카페타임 이신가요?",
          "original_form": "오 조용한 카페타임 이신가요?",
          "speaker_id": "2",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100047307.1.7",
          "form": "가까워요",
          "original_form": "가까워요",
          "speaker_id": "1",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100047307.1.8",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100047307.1.9",
          "form": "아, , 저는 어제까지인",
          "original_form": "아, , 저는 어제까지인",
          "speaker_id": "2",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100047307.1.10",
          "form": "여긴 무슨 레몬 플럼인가",
          "original_form": "여긴 무슨 레몬 플럼인가",
          "speaker_id": "1",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100047307.1.11",
          "form": "뭔가있는데",
          "original_form": "뭔가있는데",
          "speaker_id": "1",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100047307.1.12",
          "form": "이디야 깁콘 날렸어요 ㅜㅠ",
          "original_form": "이디야 깁콘 날렸어요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100047307.1.13",
          "form": "레몬티 같은건데",
          "original_form": "레몬티 같은건데",
          "speaker_id": "1",
          "time": "20210826 10:34"
        },
        {
          "id": "MDRW2100047307.1.14",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210826 10:35"
        },
        {
          "id": "MDRW2100047307.1.15",
          "form": "어째요 ㅜㅜ",
          "original_form": "어째요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 10:35"
        },
        {
          "id": "MDRW2100047307.1.16",
          "form": "토피넛라떼인가...",
          "original_form": "토피넛라떼인가...",
          "speaker_id": "2",
          "time": "20210826 10:35"
        },
        {
          "id": "MDRW2100047307.1.17",
          "form": "저도 그러게 생겨서 달려요",
          "original_form": "저도 그러게 생겨서 달려요",
          "speaker_id": "1",
          "time": "20210826 10:35"
        },
        {
          "id": "MDRW2100047307.1.18",
          "form": "아까워라",
          "original_form": "아까워라",
          "speaker_id": "1",
          "time": "20210826 10:35"
        },
        {
          "id": "MDRW2100047307.1.19",
          "form": "깜박하고있다가..",
          "original_form": "깜박하고있다가..",
          "speaker_id": "2",
          "time": "20210826 10:35"
        },
        {
          "id": "MDRW2100047307.1.20",
          "form": "앙 다시 안주니 속상해요",
          "original_form": "앙 다시 안주니 속상해요",
          "speaker_id": "1",
          "time": "20210826 10:35"
        },
        {
          "id": "MDRW2100047307.1.21",
          "form": "저도 그래요",
          "original_form": "저도 그래요",
          "speaker_id": "1",
          "time": "20210826 10:35"
        },
        {
          "id": "MDRW2100047307.1.22",
          "form": "부랴부랴 먹으러가고",
          "original_form": "부랴부랴 먹으러가고",
          "speaker_id": "1",
          "time": "20210826 10:35"
        },
        {
          "id": "MDRW2100047307.1.23",
          "form": "밤에 발견해서",
          "original_form": "밤에 발견해서",
          "speaker_id": "2",
          "time": "20210826 10:35"
        },
        {
          "id": "MDRW2100047307.1.24",
          "form": "11시까지하나싶었는데",
          "original_form": "11시까지하나싶었는데",
          "speaker_id": "2",
          "time": "20210826 10:35"
        },
        {
          "id": "MDRW2100047307.1.25",
          "form": "라스트오더 10시까지더라구요",
          "original_form": "라스트오더 10시까지더라구요",
          "speaker_id": "2",
          "time": "20210826 10:36"
        },
        {
          "id": "MDRW2100047307.1.26",
          "form": "제가 발견한시간이 10시반 ㅜㅠ",
          "original_form": "제가 발견한시간이 10시반 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210826 10:36"
        },
        {
          "id": "MDRW2100047307.1.27",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "1",
          "time": "20210826 10:36"
        },
        {
          "id": "MDRW2100047307.1.28",
          "form": "코로나라 그런가요?",
          "original_form": "코로나라 그런가요?",
          "speaker_id": "1",
          "time": "20210826 10:36"
        },
        {
          "id": "MDRW2100047307.1.29",
          "form": "어때요",
          "original_form": "어때요",
          "speaker_id": "1",
          "time": "20210826 10:36"
        },
        {
          "id": "MDRW2100047307.1.30",
          "form": "째요",
          "original_form": "째요",
          "speaker_id": "1",
          "time": "20210826 10:36"
        },
        {
          "id": "MDRW2100047307.1.31",
          "form": "그냥 언능언능쓰는게 좋은거같아요",
          "original_form": "그냥 언능언능쓰는게 좋은거같아요",
          "speaker_id": "2",
          "time": "20210826 10:36"
        },
        {
          "id": "MDRW2100047307.1.32",
          "form": "ㅜㅜ앙 듣기만해도 속터지네요",
          "original_form": "ㅜㅜ앙 듣기만해도 속터지네요",
          "speaker_id": "1",
          "time": "20210826 10:36"
        },
        {
          "id": "MDRW2100047307.1.33",
          "form": "그냥 휴지통으로 보냈어요 ㅜㅠ",
          "original_form": "그냥 휴지통으로 보냈어요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210826 10:36"
        },
        {
          "id": "MDRW2100047307.1.34",
          "form": "저도 치킨을 ㅜㅜ",
          "original_form": "저도 치킨을 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 10:37"
        },
        {
          "id": "MDRW2100047307.1.35",
          "form": "보냈어요",
          "original_form": "보냈어요",
          "speaker_id": "1",
          "time": "20210826 10:37"
        },
        {
          "id": "MDRW2100047307.1.36",
          "form": "아 이 맛난 것을",
          "original_form": "아 이 맛난 것을",
          "speaker_id": "1",
          "time": "20210826 10:37"
        }
      ]
    }
