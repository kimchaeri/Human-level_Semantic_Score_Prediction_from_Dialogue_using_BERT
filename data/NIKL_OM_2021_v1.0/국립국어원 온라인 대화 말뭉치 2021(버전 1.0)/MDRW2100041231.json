      "utterance": [
        {
          "id": "MDRW2100041231.1.1",
          "form": "오늘 날 선선하죠",
          "original_form": "오늘 날 선선하죠",
          "speaker_id": "2",
          "time": "20210905 20:39"
        },
        {
          "id": "MDRW2100041231.1.2",
          "form": "션해요",
          "original_form": "션해요",
          "speaker_id": "1",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100041231.1.3",
          "form": "저녁엔 추워요",
          "original_form": "저녁엔 추워요",
          "speaker_id": "1",
          "time": "20210905 20:40"
        },
        {
          "id": "MDRW2100041231.1.4",
          "form": "네네 저도 이제 긴팔 긴바지 꺼냈어요 ㅋㅋ하루만에",
          "original_form": "네네 저도 이제 긴팔 긴바지 꺼냈어요 ㅋㅋ하루만에",
          "speaker_id": "2",
          "time": "20210905 20:41"
        },
        {
          "id": "MDRW2100041231.1.5",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100041231.1.6",
          "form": "가을 좋아요 이러다 또 확 추워지겠져",
          "original_form": "가을 좋아요 이러다 또 확 추워지겠져",
          "speaker_id": "2",
          "time": "20210905 20:49"
        },
        {
          "id": "MDRW2100041231.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210905 20:49"
        },
        {
          "id": "MDRW2100041231.1.8",
          "form": "솜이불꺼냈네요",
          "original_form": "솜이불꺼냈네요",
          "speaker_id": "1",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100041231.1.9",
          "form": "오 솜이불 개시 하셨네여",
          "original_form": "오 솜이불 개시 하셨네여",
          "speaker_id": "2",
          "time": "20210905 20:51"
        },
        {
          "id": "MDRW2100041231.1.10",
          "form": "저는 인테리어할때 미리 꺼냈어서",
          "original_form": "저는 인테리어할때 미리 꺼냈어서",
          "speaker_id": "2",
          "time": "20210905 20:51"
        },
        {
          "id": "MDRW2100041231.1.11",
          "form": "덥게 지내며 흐회하다가 이제 좋아요",
          "original_form": "덥게 지내며 흐회하다가 이제 좋아요",
          "speaker_id": "2",
          "time": "20210905 20:51"
        },
        {
          "id": "MDRW2100041231.1.12",
          "form": "ㅎㅎ 추워서 꺼냈네요",
          "original_form": "ㅎㅎ 추워서 꺼냈네요",
          "speaker_id": "1",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100041231.1.13",
          "form": "오늘 따스하게 주무시겠어요",
          "original_form": "오늘 따스하게 주무시겠어요",
          "speaker_id": "2",
          "time": "20210905 20:52"
        },
        {
          "id": "MDRW2100041231.1.14",
          "form": "네~~비가오네요",
          "original_form": "네~~비가오네요",
          "speaker_id": "1",
          "time": "20210905 20:53"
        },
        {
          "id": "MDRW2100041231.1.15",
          "form": "엥 비와요 지금?",
          "original_form": "엥 비와요 지금?",
          "speaker_id": "2",
          "time": "20210905 20:55"
        },
        {
          "id": "MDRW2100041231.1.16",
          "form": "네..저녁부터 여기는비오네요",
          "original_form": "네..저녁부터 여기는비오네요",
          "speaker_id": "1",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100041231.1.17",
          "form": "낮엔엄청좋았는데",
          "original_form": "낮엔엄청좋았는데",
          "speaker_id": "1",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100041231.1.18",
          "form": "오호ㅠㅠ 여긴 그냥 흐리규 미스트 비",
          "original_form": "오호ㅠㅠ 여긴 그냥 흐리규 미스트 비",
          "speaker_id": "2",
          "time": "20210905 21:05"
        },
