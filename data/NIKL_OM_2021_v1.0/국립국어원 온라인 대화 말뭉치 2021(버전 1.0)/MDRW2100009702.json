      "utterance": [
        {
          "id": "MDRW2100009702.1.1",
          "form": "결혼했어요?",
          "original_form": "결혼했어요?",
          "speaker_id": "1",
          "time": "20210823 07:18"
        },
        {
          "id": "MDRW2100009702.1.2",
          "form": "아뇨 ㅋㅋ 대학생이에요",
          "original_form": "아뇨 ㅋㅋ 대학생이에요",
          "speaker_id": "2",
          "time": "20210823 09:50"
        },
        {
          "id": "MDRW2100009702.1.3",
          "form": "ㅂ부럽네요",
          "original_form": "ㅂ부럽네요",
          "speaker_id": "1",
          "time": "20210823 09:54"
        },
        {
          "id": "MDRW2100009702.1.4",
          "form": "결혼 하셨나요?",
          "original_form": "결혼 하셨나요?",
          "speaker_id": "2",
          "time": "20210823 09:56"
        },
        {
          "id": "MDRW2100009702.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210823 09:57"
        },
        {
          "id": "MDRW2100009702.1.6",
          "form": "헉 혹시 아이도 있으세요?",
          "original_form": "헉 혹시 아이도 있으세요?",
          "speaker_id": "2",
          "time": "20210823 11:33"
        },
        {
          "id": "MDRW2100009702.1.7",
          "form": "초딩이됴",
          "original_form": "초딩이됴",
          "speaker_id": "1",
          "time": "20210823 14:19"
        },
        {
          "id": "MDRW2100009702.1.8",
          "form": "아이고 귀엽겠어요",
          "original_form": "아이고 귀엽겠어요",
          "speaker_id": "2",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100009702.1.9",
          "form": "학교는 가나요",
          "original_form": "학교는 가나요",
          "speaker_id": "2",
          "time": "20210823 15:56"
        },
        {
          "id": "MDRW2100009702.1.10",
          "form": "방학이요",
          "original_form": "방학이요",
          "speaker_id": "1",
          "time": "20210823 17:09"
        },
        {
          "id": "MDRW2100009702.1.11",
          "form": "아하 아직 방학이구나",
          "original_form": "아하 아직 방학이구나",
          "speaker_id": "2",
          "time": "20210823 17:35"
        },
        {
          "id": "MDRW2100009702.1.12",
          "form": "개학하면 학교 가나요...? 코로나 때문에",
          "original_form": "개학하면 학교 가나요...? 코로나 때문에",
          "speaker_id": "2",
          "time": "20210823 17:35"
        },
        {
          "id": "MDRW2100009702.1.13",
          "form": "갈 듯요",
          "original_form": "갈 듯요",
          "speaker_id": "1",
          "time": "20210823 23:02"
        },
        {
          "id": "MDRW2100009702.1.14",
          "form": "여자앤가요 남자앤가요",
          "original_form": "여자앤가요 남자앤가요",
          "speaker_id": "2",
          "time": "20210823 23:27"
        },
