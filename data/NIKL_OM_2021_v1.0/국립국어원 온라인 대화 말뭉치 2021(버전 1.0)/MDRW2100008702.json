      "utterance": [
        {
          "id": "MDRW2100008702.1.1",
          "form": "오! 무릎보호대 저는 좀 효과없엇고",
          "original_form": "오! 무릎보호대 저는 좀 효과없엇고",
          "speaker_id": "1",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100008702.1.2",
          "form": "테이핑이 나았던 것 같아요!",
          "original_form": "테이핑이 나았던 것 같아요!",
          "speaker_id": "1",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100008702.1.3",
          "form": "근데 테이프는 땔 때 좀 아프긴해요 ㅋㅋㅋ",
          "original_form": "근데 테이프는 땔 때 좀 아프긴해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100008702.1.4",
          "form": "아하~~~",
          "original_form": "아하~~~",
          "speaker_id": "2",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100008702.1.5",
          "form": "손목은 효과좀 있나봐요",
          "original_form": "손목은 효과좀 있나봐요",
          "speaker_id": "1",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100008702.1.6",
          "form": "글쿤요,, 저도 참고할게요 ㅎㅎ",
          "original_form": "글쿤요,, 저도 참고할게요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:36"
        },
        {
          "id": "MDRW2100008702.1.7",
          "form": "손목은 확실히 잡아주는게 더 안정감있고 편하더라구요",
          "original_form": "손목은 확실히 잡아주는게 더 안정감있고 편하더라구요",
          "speaker_id": "2",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008702.1.8",
          "form": "다른 찍찍이같은 보호대말고 장갑처럼 쓰는게 낫더라구요",
          "original_form": "다른 찍찍이같은 보호대말고 장갑처럼 쓰는게 낫더라구요",
          "speaker_id": "2",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008702.1.9",
          "form": "오 참고하겠습니다 ㅋㅋㅋㅋ",
          "original_form": "오 참고하겠습니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:53"
        },
        {
          "id": "MDRW2100008702.1.10",
          "form": "나이먹다보니 확실히 건강관련된 아이템 끌리더라구요",
          "original_form": "나이먹다보니 확실히 건강관련된 아이템 끌리더라구요",
          "speaker_id": "1",
          "time": "20210822 17:53"
        },
        {
          "id": "MDRW2100008702.1.11",
          "form": "자세 잡아주는거나..",
          "original_form": "자세 잡아주는거나..",
          "speaker_id": "1",
          "time": "20210822 17:53"
        },
        {
          "id": "MDRW2100008702.1.12",
          "form": "맞아요,, 운동도 더 열심히 하게 돼요 ㅎㅎ",
          "original_form": "맞아요,, 운동도 더 열심히 하게 돼요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 18:07"
        },
        {
          "id": "MDRW2100008702.1.13",
          "form": "지금은 홈트하고있는데 코로나 좀 잠잠해지면 헬스장에 가고싶네요,,",
          "original_form": "지금은 홈트하고있는데 코로나 좀 잠잠해지면 헬스장에 가고싶네요,,",
          "speaker_id": "2",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008702.1.14",
          "form": "혹시 폼롤러같은거도 쓰시나요?",
          "original_form": "혹시 폼롤러같은거도 쓰시나요?",
          "speaker_id": "1",
          "time": "20210822 19:05"
        },
        {
          "id": "MDRW2100008702.1.15",
          "form": "그게 그리 좋다던데",
          "original_form": "그게 그리 좋다던데",
          "speaker_id": "1",
          "time": "20210822 19:05"
        },
        {
          "id": "MDRW2100008702.1.16",
          "form": "저는 폼롤러는 없고 예전에 요가할 때 산 코어가 있어요!",
          "original_form": "저는 폼롤러는 없고 예전에 요가할 때 산 코어가 있어요!",
          "speaker_id": "2",
          "time": "20210822 19:07"
        },
        {
          "id": "MDRW2100008702.1.17",
          "form": "폼롤러보다 길이는 짧은 원통기둥이에요",
          "original_form": "폼롤러보다 길이는 짧은 원통기둥이에요",
          "speaker_id": "2",
          "time": "20210822 19:07"
        },
        {
          "id": "MDRW2100008702.1.18",
          "form": "엄청 딱딱해요..ㅋㅋㅋㅋㅋ",
          "original_form": "엄청 딱딱해요..ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:07"
        },
        {
          "id": "MDRW2100008702.1.19",
          "form": "이건 딱딱해서 자주 못쓰겠더라구요 폼롤러를 하나 살까봐요,,",
          "original_form": "이건 딱딱해서 자주 못쓰겠더라구요 폼롤러를 하나 살까봐요,,",
          "speaker_id": "2",
          "time": "20210822 19:08"
        },
        {
          "id": "MDRW2100008702.1.20",
          "form": "오 그건 요가용인가봐요!! 요가도 장비빨,,",
          "original_form": "오 그건 요가용인가봐요!! 요가도 장비빨,,",
          "speaker_id": "1",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008702.1.21",
          "form": "폼롤러는 몸풀때 되게 좋다던데 안마기랑은 또 다르려나요",
          "original_form": "폼롤러는 몸풀때 되게 좋다던데 안마기랑은 또 다르려나요",
          "speaker_id": "1",
          "time": "20210822 19:12"
        },
        {
          "id": "MDRW2100008702.1.22",
          "form": "그럴거같아요! 저도 안마기쓰면서 이거 전자파는 괜찮나 걱정되더라구요",
          "original_form": "그럴거같아요! 저도 안마기쓰면서 이거 전자파는 괜찮나 걱정되더라구요",
          "speaker_id": "2",
          "time": "20210822 19:20"
        },
        {
          "id": "MDRW2100008702.1.23",
          "form": "하루종일 붙잡고있는건 아니니깐 괜찮을거라고 생각하고있어요 ㅋㅋㅋ",
          "original_form": "하루종일 붙잡고있는건 아니니깐 괜찮을거라고 생각하고있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:21"
        },
        {
          "id": "MDRW2100008702.1.24",
          "form": "아 전자파! 그건 생각도 못했네요",
          "original_form": "아 전자파! 그건 생각도 못했네요",
          "speaker_id": "1",
          "time": "20210822 19:25"
        },
        {
          "id": "MDRW2100008702.1.25",
          "form": "사람 쓰라고 만든거니까 괜찮지 않을까요? 전자레인지도 괜찮다던데",
          "original_form": "사람 쓰라고 만든거니까 괜찮지 않을까요? 전자레인지도 괜찮다던데",
          "speaker_id": "1",
          "time": "20210822 19:26"
        },
        {
          "id": "MDRW2100008702.1.26",
          "form": "그럼 다행이네요~~~",
          "original_form": "그럼 다행이네요~~~",
          "speaker_id": "2",
          "time": "20210822 19:39"
        },
        {
          "id": "MDRW2100008702.1.27",
          "form": "ㅎ내일 출근이라니 믿기지않네요,,",
          "original_form": "ㅎ내일 출근이라니 믿기지않네요,,",
          "speaker_id": "2",
          "time": "20210822 19:40"
        },
        {
          "id": "MDRW2100008702.1.28",
          "form": "대신 출근해주는 로봇같은거 있으면 사고싶어요,,",
          "original_form": "대신 출근해주는 로봇같은거 있으면 사고싶어요,,",
          "speaker_id": "1",
          "time": "20210822 19:47"
        }
      ]
