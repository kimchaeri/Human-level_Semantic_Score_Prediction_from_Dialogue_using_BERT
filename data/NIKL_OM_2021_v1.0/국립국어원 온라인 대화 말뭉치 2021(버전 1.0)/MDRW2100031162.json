      "utterance": [
        {
          "id": "MDRW2100031162.1.1",
          "form": "폴더블 폰 사고싶어요",
          "original_form": "폴더블 폰 사고싶어요",
          "speaker_id": "1",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100031162.1.2",
          "form": "플립 3",
          "original_form": "플립 3",
          "speaker_id": "1",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100031162.1.3",
          "form": "언팩 행사는 뭐지",
          "original_form": "언팩 행사는 뭐지",
          "speaker_id": "1",
          "time": "20210904 17:42"
        },
        {
          "id": "MDRW2100031162.1.4",
          "form": "언박싱 하는것처럼 언패키징아닐까요 갤럭시 폴더 +갤럭시워치",
          "original_form": "언박싱 하는것처럼 언패키징아닐까요 갤럭시 폴더 +갤럭시워치",
          "speaker_id": "2",
          "time": "20210904 17:43"
        },
        {
          "id": "MDRW2100031162.1.5",
          "form": "아아 구렇균요 다 같이",
          "original_form": "아아 구렇균요 다 같이",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100031162.1.6",
          "form": "이번에 동생이 삼성 입사해서",
          "original_form": "이번에 동생이 삼성 입사해서",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100031162.1.7",
          "form": "임직원 할인 된댔는데",
          "original_form": "임직원 할인 된댔는데",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100031162.1.8",
          "form": "얼마안돼서 치웠어요 ㅋㅋ",
          "original_form": "얼마안돼서 치웠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100031162.1.9",
          "form": "그만두었다구요? ㅋ",
          "original_form": "그만두었다구요? ㅋ",
          "speaker_id": "2",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100031162.1.10",
          "form": "아뇨아뇨 사는걸 치웠어요",
          "original_form": "아뇨아뇨 사는걸 치웠어요",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100031162.1.11",
          "form": "입사는 해야죠 ㅋㅋ힘들게 들어가서 ㅋㅋ",
          "original_form": "입사는 해야죠 ㅋㅋ힘들게 들어가서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100031162.1.12",
          "form": "얼마 할인이 안돼서",
          "original_form": "얼마 할인이 안돼서",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100031162.1.13",
          "form": "제가 헷갈리게 썼네요 글을 ㅋㅋ",
          "original_form": "제가 헷갈리게 썼네요 글을 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100031162.1.14",
          "form": "물량이 없는것 같아요. 인기모델이라",
          "original_form": "물량이 없는것 같아요. 인기모델이라",
          "speaker_id": "2",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100031162.1.15",
          "form": "그런가요ㅠㅠ 저는 일년 약정이남아서ㅠㅠ",
          "original_form": "그런가요ㅠㅠ 저는 일년 약정이남아서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100031162.1.16",
          "form": "그냥 아이폰 갈까ㅠㅠ",
          "original_form": "그냥 아이폰 갈까ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100031162.1.17",
          "form": "갤럭시 ㅇ워치누 예쁘더라구요",
          "original_form": "갤럭시 ㅇ워치누 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100031162.1.18",
          "form": "그툐ㅠㅠ 여태는 애플워치 사고팠는데",
          "original_form": "그툐ㅠㅠ 여태는 애플워치 사고팠는데",
          "speaker_id": "1",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100031162.1.19",
          "form": "갤럭시 핏 쓰다보니 워치 더 갖고파요",
          "original_form": "갤럭시 핏 쓰다보니 워치 더 갖고파요",
          "speaker_id": "1",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100031162.1.20",
          "form": "이번에 나온건 예쁜것 같더라구요",
          "original_form": "이번에 나온건 예쁜것 같더라구요",
          "speaker_id": "2",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031162.1.21",
          "form": "그툐 이번에 디자인이 이뻐요",
          "original_form": "그툐 이번에 디자인이 이뻐요",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031162.1.22",
          "form": "열일했네요 갤럭시",
          "original_form": "열일했네요 갤럭시",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031162.1.23",
          "form": "체지방측정도 되고 심전도? 뭐이것도 된다는것 같아요",
          "original_form": "체지방측정도 되고 심전도? 뭐이것도 된다는것 같아요",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031162.1.24",
          "form": "네네 백싱 부작용도 잡아냈다면서요",
          "original_form": "네네 백싱 부작용도 잡아냈다면서요",
          "speaker_id": "1",
          "time": "20210904 17:49"
        }
      ]
    }
