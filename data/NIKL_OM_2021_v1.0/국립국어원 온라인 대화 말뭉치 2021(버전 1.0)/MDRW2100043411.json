      "utterance": [
        {
          "id": "MDRW2100043411.1.1",
          "form": "혹시 알쓸범잡 보셨나요?",
          "original_form": "혹시 알쓸범잡 보셨나요?",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043411.1.2",
          "form": "네 헉",
          "original_form": "네 헉",
          "speaker_id": "2",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043411.1.3",
          "form": "저그거 본방으로 다 챙겨봤어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저그거 본방으로 다 챙겨봤어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043411.1.4",
          "form": "박지선 교수님 좋어해서요",
          "original_form": "박지선 교수님 좋어해서요",
          "speaker_id": "2",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043411.1.5",
          "form": "오 저도요ㅋㅋ 진짜 박지선 교수님 나오는 유퀴즈도 봤었어요",
          "original_form": "오 저도요ㅋㅋ 진짜 박지선 교수님 나오는 유퀴즈도 봤었어요",
          "speaker_id": "1",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100043411.1.6",
          "form": "진짜 멋지셔요..",
          "original_form": "진짜 멋지셔요..",
          "speaker_id": "1",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100043411.1.7",
          "form": "자도요 ㅋㅋㅋㅋ",
          "original_form": "자도요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043411.1.8",
          "form": "혹시.그럼 그알저알두 보세요?",
          "original_form": "혹시.그럼 그알저알두 보세요?",
          "speaker_id": "2",
          "time": "20210907 19:01"
        },
        {
          "id": "MDRW2100043411.1.9",
          "form": "그건 아직 안봤어요!",
          "original_form": "그건 아직 안봤어요!",
          "speaker_id": "1",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100043411.1.10",
          "form": "괜찮나요??",
          "original_form": "괜찮나요??",
          "speaker_id": "1",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043411.1.11",
          "form": "네 박지선님 나와서 그알 뒷이야기 하시는데 재미나요",
          "original_form": "네 박지선님 나와서 그알 뒷이야기 하시는데 재미나요",
          "speaker_id": "2",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100043411.1.12",
          "form": "이따 밤에 당장 봐야겠어요 ...",
          "original_form": "이따 밤에 당장 봐야겠어요 ...",
          "speaker_id": "1",
          "time": "20210907 19:08"
        },
        {
          "id": "MDRW2100043411.1.13",
          "form": "ㅋㅋㅋㅋ이거저거 서브 내용이 많은데 그알 좋아하시면 재미있을 곳 같아요",
          "original_form": "ㅋㅋㅋㅋ이거저거 서브 내용이 많은데 그알 좋아하시면 재미있을 곳 같아요",
          "speaker_id": "2",
          "time": "20210907 19:09"
        },
        {
          "id": "MDRW2100043411.1.14",
          "form": "그알 좋아하죠.. 요즘 매주는 못챙겨보지만요..",
          "original_form": "그알 좋아하죠.. 요즘 매주는 못챙겨보지만요..",
          "speaker_id": "1",
          "time": "20210907 19:11"
        },
        {
          "id": "MDRW2100043411.1.15",
          "form": "범죄 프로 좋아하시면 표리부동도 추천드려요 ㅋㅋ 케이비에스에서 하는데 이수정 교수님하구 표창원 박사 나와요 ㅋㅋㅋ",
          "original_form": "범죄 프로 좋아하시면 표리부동도 추천드려요 ㅋㅋ 케이비에스에서 하는데 이수정 교수님하구 표창원 박사 나와요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:12"
        },
        {
          "id": "MDRW2100043411.1.16",
          "form": "오 저그거 챙겨봐요 ㅋㅋ",
          "original_form": "오 저그거 챙겨봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043411.1.17",
          "form": "요즘 범죄프로그램 예전보다 많아져서 넘 좋아요.. 챙겨볼게 많아졌어요!",
          "original_form": "요즘 범죄프로그램 예전보다 많아져서 넘 좋아요.. 챙겨볼게 많아졌어요!",
          "speaker_id": "1",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100043411.1.18",
          "form": "ㅋㅋㅋㅋ 저두요 저 당혹사랑 꼬꼬무도 챙겨봤어요 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 저두요 저 당혹사랑 꼬꼬무도 챙겨봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043411.1.19",
          "form": "요즘 좀 유행인 것 같아요",
          "original_form": "요즘 좀 유행인 것 같아요",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100043411.1.20",
          "form": "오오 맞아요 .. 역시.. 다 챙겨보시는군요 !!",
          "original_form": "오오 맞아요 .. 역시.. 다 챙겨보시는군요 !!",
          "speaker_id": "1",
          "time": "20210907 19:16"
        }
      ]
    }
