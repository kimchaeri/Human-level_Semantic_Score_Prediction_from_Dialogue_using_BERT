      "utterance": [
        {
          "id": "MDRW2100002071.1.1",
          "form": "봉사 다녀보셨어요?",
          "original_form": "봉사 다녀보셨어요?",
          "speaker_id": "1",
          "time": "20210819 23:19"
        },
        {
          "id": "MDRW2100002071.1.2",
          "form": "급식이때 아동센터 봉사 6년정도 했어요",
          "original_form": "급식이때 아동센터 봉사 6년정도 했어요",
          "speaker_id": "2",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100002071.1.3",
          "form": "와 대단하네요",
          "original_form": "와 대단하네요",
          "speaker_id": "1",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100002071.1.4",
          "form": "마음먹고 직접 움직인다는게 엄청난거같아요",
          "original_form": "마음먹고 직접 움직인다는게 엄청난거같아요",
          "speaker_id": "1",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100002071.1.5",
          "form": "전 요즘은 그냥 기부금으로 ..선행을 ㅠㅠ",
          "original_form": "전 요즘은 그냥 기부금으로 ..선행을 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 23:21"
        },
        {
          "id": "MDRW2100002071.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210819 23:21"
        },
        {
          "id": "MDRW2100002071.1.7",
          "form": "봉사를 다닌 다는 것이 쉬운일은 아니죠..",
          "original_form": "봉사를 다닌 다는 것이 쉬운일은 아니죠..",
          "speaker_id": "2",
          "time": "20210819 23:21"
        },
        {
          "id": "MDRW2100002071.1.8",
          "form": "기부금도 대단하세요! 진짜루요",
          "original_form": "기부금도 대단하세요! 진짜루요",
          "speaker_id": "2",
          "time": "20210819 23:21"
        },
        {
          "id": "MDRW2100002071.1.9",
          "form": "그렇게 생각해주시면 감사하죠 ㅎㅎ",
          "original_form": "그렇게 생각해주시면 감사하죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 23:22"
        },
        {
          "id": "MDRW2100002071.1.10",
          "form": "기회가된다면 봉사도 다니고싶어요",
          "original_form": "기회가된다면 봉사도 다니고싶어요",
          "speaker_id": "1",
          "time": "20210819 23:22"
        },
        {
          "id": "MDRW2100002071.1.11",
          "form": "꼭 거창하지 않아도 작은 것 부터 하면 되니까 부담가지시지 마세요",
          "original_form": "꼭 거창하지 않아도 작은 것 부터 하면 되니까 부담가지시지 마세요",
          "speaker_id": "2",
          "time": "20210819 23:22"
        },
        {
          "id": "MDRW2100002071.1.12",
          "form": "동호회같은거 나가보신 적 있으세요?",
          "original_form": "동호회같은거 나가보신 적 있으세요?",
          "speaker_id": "2",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100002071.1.13",
          "form": "저는 아직 없어요",
          "original_form": "저는 아직 없어요",
          "speaker_id": "2",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100002071.1.14",
          "form": "동호회라 .. 전에 배드민턴은 나가봣어요 ㅎㅎ",
          "original_form": "동호회라 .. 전에 배드민턴은 나가봣어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100002071.1.15",
          "form": "활동적이시네요",
          "original_form": "활동적이시네요",
          "speaker_id": "2",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100002071.1.16",
          "form": "배드민턴 재밌잖아요",
          "original_form": "배드민턴 재밌잖아요",
          "speaker_id": "2",
          "time": "20210819 23:23"
        },
        {
          "id": "MDRW2100002071.1.17",
          "form": "네넹 ㅋㅋ 할수있는 운동이 넘적어서",
          "original_form": "네넹 ㅋㅋ 할수있는 운동이 넘적어서",
          "speaker_id": "1",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002071.1.18",
          "form": "친구따라 가봣는데 재밌었어요 ㅎ",
          "original_form": "친구따라 가봣는데 재밌었어요 ㅎ",
          "speaker_id": "1",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002071.1.19",
          "form": "그럴거 같아요 배드민턴은 공을 주고 받는 게 매력있더라구요",
          "original_form": "그럴거 같아요 배드민턴은 공을 주고 받는 게 매력있더라구요",
          "speaker_id": "2",
          "time": "20210819 23:25"
        },
        {
          "id": "MDRW2100002071.1.20",
          "form": "가볍게 툭툭쳐서 넘기는게 재밌어요",
          "original_form": "가볍게 툭툭쳐서 넘기는게 재밌어요",
          "speaker_id": "2",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002071.1.21",
          "form": "맞아용 ㅎㅎ 동호회 종류도 엄청많던데",
          "original_form": "맞아용 ㅎㅎ 동호회 종류도 엄청많던데",
          "speaker_id": "1",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002071.1.22",
          "form": "코로나가 좀풀려야 도전해볼텐데요 ㅠ",
          "original_form": "코로나가 좀풀려야 도전해볼텐데요 ㅠ",
          "speaker_id": "1",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002071.1.23",
          "form": "그러니까요ㅠ",
          "original_form": "그러니까요ㅠ",
          "speaker_id": "2",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002071.1.24",
          "form": "코로나 때문에 제한이 너무 많이 걸려요",
          "original_form": "코로나 때문에 제한이 너무 많이 걸려요",
          "speaker_id": "2",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002071.1.25",
          "form": "가족모임도 못하니 뭐... ㅠㅠ으휴 큰일이에여",
          "original_form": "가족모임도 못하니 뭐... ㅠㅠ으휴 큰일이에여",
          "speaker_id": "1",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002071.1.26",
          "form": "그러니까요..",
          "original_form": "그러니까요..",
          "speaker_id": "2",
          "time": "20210819 23:27"
        }
      ]
