      "utterance": [
        {
          "id": "MDRW2100015780.1.1",
          "form": "좋아하는 게임 있으세요?",
          "original_form": "좋아하는 게임 있으세요?",
          "speaker_id": "1",
          "time": "20210826 00:58"
        },
        {
          "id": "MDRW2100015780.1.2",
          "form": "음 아니요 게임은 별로 안좋아해요!",
          "original_form": "음 아니요 게임은 별로 안좋아해요!",
          "speaker_id": "2",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015780.1.3",
          "form": "헉 정말요?",
          "original_form": "헉 정말요?",
          "speaker_id": "1",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015780.1.4",
          "form": "보는 것도 안좋아하시나요?",
          "original_form": "보는 것도 안좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015780.1.5",
          "form": "네네 잘 못하기도하고 좀 쉽게 질리는편이에요",
          "original_form": "네네 잘 못하기도하고 좀 쉽게 질리는편이에요",
          "speaker_id": "2",
          "time": "20210826 01:01"
        },
        {
          "id": "MDRW2100015780.1.6",
          "form": "네네 ㅠㅠ 가끔 하긴해요 ㅋㅋㅋ",
          "original_form": "네네 ㅠㅠ 가끔 하긴해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:01"
        },
        {
          "id": "MDRW2100015780.1.7",
          "form": "스타듀밸리 아세요?",
          "original_form": "스타듀밸리 아세요?",
          "speaker_id": "2",
          "time": "20210826 01:01"
        },
        {
          "id": "MDRW2100015780.1.8",
          "form": "들어봤어요! 그거 하시나요?",
          "original_form": "들어봤어요! 그거 하시나요?",
          "speaker_id": "1",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015780.1.9",
          "form": "요즘은 안하는데 예전에 꽤 중독되서 했어요 ㅋㅋㅋㅋ",
          "original_form": "요즘은 안하는데 예전에 꽤 중독되서 했어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:04"
        },
        {
          "id": "MDRW2100015780.1.10",
          "form": "농장관리하는건데 귀여워요",
          "original_form": "농장관리하는건데 귀여워요",
          "speaker_id": "2",
          "time": "20210826 01:04"
        },
        {
          "id": "MDRW2100015780.1.11",
          "form": "좋아하는 게임 있으세요?",
          "original_form": "좋아하는 게임 있으세요?",
          "speaker_id": "2",
          "time": "20210826 01:04"
        },
        {
          "id": "MDRW2100015780.1.12",
          "form": "전 특별히 좋아하는 게임은 없어요",
          "original_form": "전 특별히 좋아하는 게임은 없어요",
          "speaker_id": "1",
          "time": "20210826 01:05"
        },
        {
          "id": "MDRW2100015780.1.13",
          "form": "변덕이 심해서요ㅋㅋㅋㅋ",
          "original_form": "변덕이 심해서요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:05"
        },
        {
          "id": "MDRW2100015780.1.14",
          "form": "전에는 배그 좋아했고",
          "original_form": "전에는 배그 좋아했고",
          "speaker_id": "1",
          "time": "20210826 01:05"
        },
        {
          "id": "MDRW2100015780.1.15",
          "form": "최근엔 동숲 포켓캠프 자주 했어요",
          "original_form": "최근엔 동숲 포켓캠프 자주 했어요",
          "speaker_id": "1",
          "time": "20210826 01:05"
        },
        {
          "id": "MDRW2100015780.1.16",
          "form": "오오 배그는 한번 해보고싶더라구요",
          "original_form": "오오 배그는 한번 해보고싶더라구요",
          "speaker_id": "2",
          "time": "20210826 01:05"
        },
        {
          "id": "MDRW2100015780.1.17",
          "form": "동숲도 궁금하긴해요 귀엽던데",
          "original_form": "동숲도 궁금하긴해요 귀엽던데",
          "speaker_id": "2",
          "time": "20210826 01:06"
        },
        {
          "id": "MDRW2100015780.1.18",
          "form": "그쵸 동숲 귀여워요",
          "original_form": "그쵸 동숲 귀여워요",
          "speaker_id": "1",
          "time": "20210826 01:07"
        },
        {
          "id": "MDRW2100015780.1.19",
          "form": "근데 어떤 분은 배그하면 어지럽다고 하더라구요",
          "original_form": "근데 어떤 분은 배그하면 어지럽다고 하더라구요",
          "speaker_id": "1",
          "time": "20210826 01:07"
        },
        {
          "id": "MDRW2100015780.1.20",
          "form": "아아 화면 막 움직여서 그런걸까요",
          "original_form": "아아 화면 막 움직여서 그런걸까요",
          "speaker_id": "2",
          "time": "20210826 01:09"
        },
        {
          "id": "MDRW2100015780.1.21",
          "form": "카트 해보셧어요?",
          "original_form": "카트 해보셧어요?",
          "speaker_id": "2",
          "time": "20210826 01:28"
        },
        {
          "id": "MDRW2100015780.1.22",
          "form": "어릴 때 다 해보지 않나요?ㅌㅋㅋㅋ 추억이네요",
          "original_form": "어릴 때 다 해보지 않나요?ㅌㅋㅋㅋ 추억이네요",
          "speaker_id": "1",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100015780.1.23",
          "form": "ㅋㅋㅋㅋㅋ그쵸 몇년전에 오랜만에 해봤는데 많이바뀌엇더라구요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ그쵸 몇년전에 오랜만에 해봤는데 많이바뀌엇더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100015780.1.24",
          "form": "맵과 기능이 다양해졋어요 ㅋㅋㅋㅋ",
          "original_form": "맵과 기능이 다양해졋어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 11:57"
        },
        {
          "id": "MDRW2100015780.1.25",
          "form": "헋 더 재밌겠네요",
          "original_form": "헋 더 재밌겠네요",
          "speaker_id": "1",
          "time": "20210826 12:00"
