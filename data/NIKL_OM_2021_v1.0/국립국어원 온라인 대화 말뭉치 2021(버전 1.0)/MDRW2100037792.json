      "utterance": [
        {
          "id": "MDRW2100037792.1.1",
          "form": "저녁드셨나요?",
          "original_form": "저녁드셨나요?",
          "speaker_id": "2",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037792.1.2",
          "form": "찬 맥주에 따땃한 튀김..",
          "original_form": "찬 맥주에 따땃한 튀김..",
          "speaker_id": "1",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100037792.1.3",
          "form": "저 아직요 ㅜㅜ 드셨어요?",
          "original_form": "저 아직요 ㅜㅜ 드셨어요?",
          "speaker_id": "1",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100037792.1.4",
          "form": "저도아직,,",
          "original_form": "저도아직,,",
          "speaker_id": "2",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100037792.1.5",
          "form": "어떤거드실예정이세요?!",
          "original_form": "어떤거드실예정이세요?!",
          "speaker_id": "2",
          "time": "20210901 18:27"
        },
        {
          "id": "MDRW2100037792.1.6",
          "form": "먹다남은 부대찌개를..",
          "original_form": "먹다남은 부대찌개를..",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100037792.1.7",
          "form": "저는 국수 먹을것같아요 ㅋㅋㅋ",
          "original_form": "저는 국수 먹을것같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100037792.1.8",
          "form": "배달음식 좋아하세요?!",
          "original_form": "배달음식 좋아하세요?!",
          "speaker_id": "2",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100037792.1.9",
          "form": "오 국수 잔치국수요?",
          "original_form": "오 국수 잔치국수요?",
          "speaker_id": "1",
          "time": "20210901 18:37"
        },
        {
          "id": "MDRW2100037792.1.10",
          "form": "저 배달음식 스레기 너무나와서 좀 꺼려지더라구요 환경운동가는 아니지만..",
          "original_form": "저 배달음식 스레기 너무나와서 좀 꺼려지더라구요 환경운동가는 아니지만..",
          "speaker_id": "1",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037792.1.11",
          "form": "네 잔치국수!!",
          "original_form": "네 잔치국수!!",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037792.1.12",
          "form": "어 맞아요 스레기 너무많이 나오죠ㅠ",
          "original_form": "어 맞아요 스레기 너무많이 나오죠ㅠ",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100037792.1.13",
          "form": "잔치국수에 김이랑 김치랑 넣어먹으면 대존맛이져..",
          "original_form": "잔치국수에 김이랑 김치랑 넣어먹으면 대존맛이져..",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100037792.1.14",
          "form": "그맛은 못참죠",
          "original_form": "그맛은 못참죠",
          "speaker_id": "2",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100037792.1.15",
          "form": "밥먹고왓습니다.. 남은 부찌에 밥말아먹엇어요",
          "original_form": "밥먹고왓습니다.. 남은 부찌에 밥말아먹엇어요",
          "speaker_id": "1",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100037792.1.16",
          "form": "아 얼큰하고 맛있죠 부찌",
          "original_form": "아 얼큰하고 맛있죠 부찌",
          "speaker_id": "2",
          "time": "20210901 19:20"
        },
        {
          "id": "MDRW2100037792.1.17",
          "form": "ㅋㅋㅋㅋ 그 대한미국놈 아시나요 부찌 중독된 미국인",
          "original_form": "ㅋㅋㅋㅋ 그 대한미국놈 아시나요 부찌 중독된 미국인",
          "speaker_id": "1",
          "time": "20210901 19:22"
        },
        {
          "id": "MDRW2100037792.1.18",
          "form": "ㅋㄱㅋㅋㄱㅋㅋㄱ부찌에 미친사람이잖아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㄱㅋㅋㄱㅋㅋㄱ부찌에 미친사람이잖아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:23"
        },
