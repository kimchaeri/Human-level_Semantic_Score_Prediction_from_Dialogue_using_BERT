      "utterance": [
        {
          "id": "MDRW2100046609.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20211102 00:55"
        },
        {
          "id": "MDRW2100046609.1.2",
          "form": "안녕하세요! 주말에 할로윈 잘 보내셨나요!",
          "original_form": "안녕하세요! 주말에 할로윈 잘 보내셨나요!",
          "speaker_id": "2",
          "time": "20211102 00:58"
        },
        {
          "id": "MDRW2100046609.1.3",
          "form": "응 너는?",
          "original_form": "응 너는?",
          "speaker_id": "1",
          "time": "20211102 00:58"
        },
        {
          "id": "MDRW2100046609.1.4",
          "form": "주말엔 그냥 책읽고 넷플릭스 보면서 쉬었죠",
          "original_form": "주말엔 그냥 책읽고 넷플릭스 보면서 쉬었죠",
          "speaker_id": "2",
          "time": "20211102 00:59"
        },
        {
          "id": "MDRW2100046609.1.5",
          "form": "그러니?",
          "original_form": "그러니?",
          "speaker_id": "1",
          "time": "20211101 13:01"
        },
        {
          "id": "MDRW2100046609.1.6",
          "form": "난 그냥 편하게 지냈어.",
          "original_form": "난 그냥 편하게 지냈어.",
          "speaker_id": "1",
          "time": "20211101 13:02"
        },
        {
          "id": "MDRW2100046609.1.7",
          "form": "주말에 취미생활같은거 안하시나요",
          "original_form": "주말에 취미생활같은거 안하시나요",
          "speaker_id": "2",
          "time": "20211101 13:04"
        },
        {
          "id": "MDRW2100046609.1.8",
          "form": "넹 그냥 일하고 쉬죠.",
          "original_form": "넹 그냥 일하고 쉬죠.",
          "speaker_id": "1",
          "time": "20211101 13:04"
        },
        {
          "id": "MDRW2100046609.1.9",
          "form": "오 주말에도 일하시나봐요",
          "original_form": "오 주말에도 일하시나봐요",
          "speaker_id": "2",
          "time": "20211101 13:04"
        },
        {
          "id": "MDRW2100046609.1.10",
          "form": "여기 관리인이라서요 에휴 시골이에요.",
          "original_form": "여기 관리인이라서요 에휴 시골이에요.",
          "speaker_id": "1",
          "time": "20211101 13:05"
        },
        {
          "id": "MDRW2100046609.1.11",
          "form": "시골이면 공기는 좋겠어요",
          "original_form": "시골이면 공기는 좋겠어요",
          "speaker_id": "2",
          "time": "20211101 13:12"
        },
        {
          "id": "MDRW2100046609.1.12",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20211101 13:13"
        },
        {
          "id": "MDRW2100046609.1.13",
          "form": "주말까지 일하시면 언제 쉬세요",
          "original_form": "주말까지 일하시면 언제 쉬세요",
          "speaker_id": "2",
          "time": "20211101 13:13"
        },
        {
          "id": "MDRW2100046609.1.14",
          "form": "공기는 엄청 좋죠.",
          "original_form": "공기는 엄청 좋죠.",
          "speaker_id": "1",
          "time": "20211101 13:13"
        },
        {
          "id": "MDRW2100046609.1.15",
          "form": "그냥 틈틈이 쉬어요.",
          "original_form": "그냥 틈틈이 쉬어요.",
          "speaker_id": "1",
          "time": "20211101 13:14"
        },
        {
          "id": "MDRW2100046609.1.16",
          "form": "잠깐만요. 혹시 요원이셔요?",
          "original_form": "잠깐만요. 혹시 요원이셔요?",
          "speaker_id": "1",
          "time": "20211101 13:14"
        },
        {
          "id": "MDRW2100046609.1.17",
          "form": "일하면서 중간중간 여유 찾아 쉬시길..",
          "original_form": "일하면서 중간중간 여유 찾아 쉬시길..",
          "speaker_id": "2",
          "time": "20211101 13:14"
        },
        {
          "id": "MDRW2100046609.1.18",
          "form": "네넹 고마워요.",
          "original_form": "네넹 고마워요.",
          "speaker_id": "1",
