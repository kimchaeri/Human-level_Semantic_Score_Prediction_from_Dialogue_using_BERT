      "utterance": [
        {
          "id": "MDRW2100000523.1.1",
          "form": "요즘 급 날씨 선선해 진거 같지 않아??",
          "original_form": "요즘 급 날씨 선선해 진거 같지 않아??",
          "speaker_id": "1",
          "time": "20210819 16:35"
        },
        {
          "id": "MDRW2100000523.1.2",
          "form": "오늘 깜짝 놀랐어",
          "original_form": "오늘 깜짝 놀랐어",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000523.1.3",
          "form": "밤엔 춥다니까",
          "original_form": "밤엔 춥다니까",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000523.1.4",
          "form": "어 오늘 애들 어린이집 데려다 주는데",
          "original_form": "어 오늘 애들 어린이집 데려다 주는데",
          "speaker_id": "1",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000523.1.5",
          "form": "에어컨 안켜도 될정도로 선선해 졌더라???",
          "original_form": "에어컨 안켜도 될정도로 선선해 졌더라???",
          "speaker_id": "1",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000523.1.6",
          "form": "어어",
          "original_form": "어어",
          "speaker_id": "2",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000523.1.7",
          "form": "그리고 바람이 슝슝",
          "original_form": "그리고 바람이 슝슝",
          "speaker_id": "2",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000523.1.8",
          "form": "휴.. 우리집은 그래도 아직 에어컨틀어.",
          "original_form": "휴.. 우리집은 그래도 아직 에어컨틀어.",
          "speaker_id": "1",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000523.1.9",
          "form": "남편때문에???",
          "original_form": "남편때문에???",
          "speaker_id": "2",
          "time": "20210819 16:50"
        },
        {
          "id": "MDRW2100000523.1.10",
          "form": "아님 습기??",
          "original_form": "아님 습기??",
          "speaker_id": "2",
          "time": "20210819 16:50"
        },
        {
          "id": "MDRW2100000523.1.11",
          "form": "둘다.ㅋㅋㅋㅋ",
          "original_form": "둘다.ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000523.1.12",
          "form": "남편이 습기랑 벌레 극혐함.",
          "original_form": "남편이 습기랑 벌레 극혐함.",
          "speaker_id": "1",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000523.1.13",
          "form": "아무래도 큰 도로변으로 창나서.. 먼지고 많고.",
          "original_form": "아무래도 큰 도로변으로 창나서.. 먼지고 많고.",
          "speaker_id": "1",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000523.1.14",
          "form": "습기는 근데 진짜 치명적이야",
          "original_form": "습기는 근데 진짜 치명적이야",
          "speaker_id": "2",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000523.1.15",
          "form": "애 셋이잖아^^",
          "original_form": "애 셋이잖아^^",
          "speaker_id": "2",
          "time": "20210819 17:32"
        },
        {
          "id": "MDRW2100000523.1.16",
          "form": "그래서 오늘 환기하고 또 가동했지.. 에어컨..",
          "original_form": "그래서 오늘 환기하고 또 가동했지.. 에어컨..",
          "speaker_id": "1",
          "time": "20210819 21:17"
        },
        {
          "id": "MDRW2100000523.1.17",
          "form": "잘했어",
          "original_form": "잘했어",
          "speaker_id": "2",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100000523.1.18",
          "form": "습기 이즈 베리 임포턴트",
          "original_form": "습기 이즈 베리 임포턴트",
          "speaker_id": "2",
          "time": "20210819 23:55"
        },
        {
          "id": "MDRW2100000523.1.19",
          "form": "휴. 오늘은 다시 너무 더운데?",
          "original_form": "휴. 오늘은 다시 너무 더운데?",
          "speaker_id": "1",
          "time": "20210820 08:28"
        },
        {
          "id": "MDRW2100000523.1.20",
          "form": "거긴 좀 선선해??",
          "original_form": "거긴 좀 선선해??",
          "speaker_id": "1",
          "time": "20210820 08:28"
        },
        {
          "id": "MDRW2100000523.1.21",
          "form": "어???",
          "original_form": "어???",
          "speaker_id": "2",
          "time": "20210820 09:32"
        },
        {
          "id": "MDRW2100000523.1.22",
          "form": "여기 지금 좀 난 추워",
          "original_form": "여기 지금 좀 난 추워",
          "speaker_id": "2",
          "time": "20210820 09:32"
        },
        {
          "id": "MDRW2100000523.1.23",
          "form": "휴.. 대한민국이 이렇게 넓다..어딘 춥고 어딘 덥고..",
          "original_form": "휴.. 대한민국이 이렇게 넓다..어딘 춥고 어딘 덥고..",
          "speaker_id": "1",
          "time": "20210820 09:33"
        },
        {
          "id": "MDRW2100000523.1.24",
          "form": "그래도 미세먼지는 요즘 쫌 덜한듯?",
          "original_form": "그래도 미세먼지는 요즘 쫌 덜한듯?",
          "speaker_id": "1",
          "time": "20210820 09:33"
        },
        {
          "id": "MDRW2100000523.1.25",
          "form": "진짜!!!",
          "original_form": "진짜!!!",
          "speaker_id": "2",
          "time": "20210820 09:40"
        },
        {
          "id": "MDRW2100000523.1.26",
          "form": "미세먼지 계속 좋더라??",
          "original_form": "미세먼지 계속 좋더라??",
          "speaker_id": "2",
          "time": "20210820 09:40"
        },
        {
          "id": "MDRW2100000523.1.27",
          "form": "코로나 동안 미먼은 고민 안한듯",
          "original_form": "코로나 동안 미먼은 고민 안한듯",
          "speaker_id": "2",
          "time": "20210820 09:40"
        },
        {
          "id": "MDRW2100000523.1.28",
          "form": "그래?? 봄에는 좀 심한거 같더니..",
          "original_form": "그래?? 봄에는 좀 심한거 같더니..",
          "speaker_id": "1",
          "time": "20210820 09:41"
        },
        {
          "id": "MDRW2100000523.1.29",
          "form": "가을에는 좀 괜찮아진듯??? 여긴그래.",
          "original_form": "가을에는 좀 괜찮아진듯??? 여긴그래.",
          "speaker_id": "1",
          "time": "20210820 09:42"
        },
        {
          "id": "MDRW2100000523.1.30",
          "form": "여기는 늘",
          "original_form": "여기는 늘",
          "speaker_id": "2",
          "time": "20210820 12:11"
        },
        {
          "id": "MDRW2100000523.1.31",
          "form": "안좋은 느낌ㅜㅜ",
          "original_form": "안좋은 느낌ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 12:11"
        },
        {
          "id": "MDRW2100000523.1.32",
          "form": "아니 가을 장마 온다네???",
          "original_form": "아니 가을 장마 온다네???",
          "speaker_id": "1",
          "time": "20210820 12:24"
        },
        {
          "id": "MDRW2100000523.1.33",
          "form": "어???",
          "original_form": "어???",
          "speaker_id": "2",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100000523.1.34",
          "form": "장마라고????",
          "original_form": "장마라고????",
          "speaker_id": "2",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100000523.1.35",
          "form": "어어...",
          "original_form": "어어...",
          "speaker_id": "1",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100000523.1.36",
          "form": "정말 난해한 날씨다",
          "original_form": "정말 난해한 날씨다",
          "speaker_id": "2",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100000523.1.37",
          "form": "가을장마온대..",
          "original_form": "가을장마온대..",
          "speaker_id": "1",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100000523.1.38",
          "form": "올해 5.6월도 장마인줄",
          "original_form": "올해 5.6월도 장마인줄",
          "speaker_id": "2",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100000523.1.39",
          "form": "정말 기상 이변아니야??",
          "original_form": "정말 기상 이변아니야??",
          "speaker_id": "1",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100000523.1.40",
          "form": "7월에는 장마 없고",
          "original_form": "7월에는 장마 없고",
          "speaker_id": "2",
          "time": "20210820 12:37"
        },
        {
          "id": "MDRW2100000523.1.41",
          "form": "기상 이변 맞는듯",
          "original_form": "기상 이변 맞는듯",
          "speaker_id": "2",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100000523.1.42",
          "form": "어딘 산불나고..",
          "original_form": "어딘 산불나고..",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
        {
          "id": "MDRW2100000523.1.43",
          "form": "온도가 막 50도라 그러고...",
          "original_form": "온도가 막 50도라 그러고...",
          "speaker_id": "1",
          "time": "20210820 12:38"
        },
