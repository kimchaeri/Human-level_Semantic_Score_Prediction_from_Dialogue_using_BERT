      "utterance": [
        {
          "id": "MDRW2100009379.1.1",
          "form": "오늘 시원하네~",
          "original_form": "오늘 시원하네~",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009379.1.2",
          "form": "나 에어컨 안켜고 선풍기 돌리는데~",
          "original_form": "나 에어컨 안켜고 선풍기 돌리는데~",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009379.1.3",
          "form": "아직까지 밖에서 노는 애가 있네;;",
          "original_form": "아직까지 밖에서 노는 애가 있네;;",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009379.1.4",
          "form": "헉 아직 논다고?",
          "original_form": "헉 아직 논다고?",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009379.1.5",
          "form": "그아이도 에너지 장난아니구나ㅋ",
          "original_form": "그아이도 에너지 장난아니구나ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009379.1.6",
          "form": "그러게 오늘 시원해",
          "original_form": "그러게 오늘 시원해",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009379.1.7",
          "form": "좋다ㅋ",
          "original_form": "좋다ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009379.1.8",
          "form": "어른들이랑 같이 소리 나네~",
          "original_form": "어른들이랑 같이 소리 나네~",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009379.1.9",
          "form": "근데 내일 태풍온대ㅜ",
          "original_form": "근데 내일 태풍온대ㅜ",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009379.1.10",
          "form": "저녁에 대구 지나간다는말이 있더라",
          "original_form": "저녁에 대구 지나간다는말이 있더라",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009379.1.11",
          "form": "여름에 뭔가 장마도 짧고 그렇디~",
          "original_form": "여름에 뭔가 장마도 짧고 그렇디~",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009379.1.12",
          "form": "진짜??",
          "original_form": "진짜??",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009379.1.13",
          "form": "문닫아야되나??",
          "original_form": "문닫아야되나??",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009379.1.14",
          "form": "자다가 일어나는거 늠 싫다 ㅋㅋ",
          "original_form": "자다가 일어나는거 늠 싫다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009379.1.15",
          "form": "응 그러니깐",
          "original_form": "응 그러니깐",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009379.1.16",
          "form": "태풍 지나고 나면 가을오겠다~",
          "original_form": "태풍 지나고 나면 가을오겠다~",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009379.1.17",
          "form": "무사히 조심히 잘 지나갔으면 좋겠네",
          "original_form": "무사히 조심히 잘 지나갔으면 좋겠네",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009379.1.18",
          "form": "시간이 너무 빠른거 같네~",
          "original_form": "시간이 너무 빠른거 같네~",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009379.1.19",
          "form": "응 지금도 보니 가을날씨인듯",
          "original_form": "응 지금도 보니 가을날씨인듯",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009379.1.20",
          "form": "맞아. 이번에는 그렇게 막 덥지는 않았던것같은데",
          "original_form": "맞아. 이번에는 그렇게 막 덥지는 않았던것같은데",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009379.1.21",
          "form": "벌써 가을이 다가온다.ㅋ",
          "original_form": "벌써 가을이 다가온다.ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009379.1.22",
          "form": "그니깐~이러다가 이제 한계절만 있는거 아님 ㅋㅋㅋ",
          "original_form": "그니깐~이러다가 이제 한계절만 있는거 아님 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100009379.1.23",
          "form": "겨울되면 좋겠다 ㅋㅋㅋ패딩하나면 충분함 ㅋㅋㅋㅋ",
          "original_form": "겨울되면 좋겠다 ㅋㅋㅋ패딩하나면 충분함 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009379.1.24",
          "form": "ㅋㅋ아 웃겨ㅋㅋ",
          "original_form": "ㅋㅋ아 웃겨ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009379.1.25",
          "form": "가을도 잠깐이더라ㅋ",
          "original_form": "가을도 잠깐이더라ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009379.1.26",
          "form": "그니깐~우린 벌써 겨울옷 다 나왔음 ㅋㅋㅋ",
          "original_form": "그니깐~우린 벌써 겨울옷 다 나왔음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009379.1.27",
          "form": "아 진짜?ㅋㅋㅋ",
          "original_form": "아 진짜?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009379.1.28",
          "form": "겨울옷 구경가야겠네ㅋㅋ",
          "original_form": "겨울옷 구경가야겠네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009379.1.29",
          "form": "얼마전꺼지 보기만 햐도 덥더만~",
          "original_form": "얼마전꺼지 보기만 햐도 덥더만~",
          "speaker_id": "2",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009379.1.30",
          "form": "그러니깐ㅋㅋ빠르다ㅋㅋ",
          "original_form": "그러니깐ㅋㅋ빠르다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009379.1.31",
          "form": "이젠 다들 긴팔 사간다 ㅋㅋㅋ",
          "original_form": "이젠 다들 긴팔 사간다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:09"
        }
      ]
    }
