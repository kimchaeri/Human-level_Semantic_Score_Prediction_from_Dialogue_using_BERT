      "utterance": [
        {
          "id": "MDRW2100015418.1.1",
          "form": "월매막걸리가",
          "original_form": "월매막걸리가",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015418.1.2",
          "form": "편의점에팔던가요?",
          "original_form": "편의점에팔던가요?",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015418.1.3",
          "form": "왜본기억이없죠??!",
          "original_form": "왜본기억이없죠??!",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015418.1.4",
          "form": "ㅋㅋㅋ넹 씨유랑 지에스 세븐일레븐 다 팔요",
          "original_form": "ㅋㅋㅋ넹 씨유랑 지에스 세븐일레븐 다 팔요",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015418.1.5",
          "form": "아요",
          "original_form": "아요",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015418.1.6",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015418.1.7",
          "form": "몰랐네요",
          "original_form": "몰랐네요",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015418.1.8",
          "form": "밤막걸리는",
          "original_form": "밤막걸리는",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015418.1.9",
          "form": "자주본것같은데말이죠",
          "original_form": "자주본것같은데말이죠",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015418.1.10",
          "form": "ㅋㅋㅋ밤막걸리 특이해서 가끔 괜찮더라고요 ㅎㅎ",
          "original_form": "ㅋㅋㅋ밤막걸리 특이해서 가끔 괜찮더라고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:16"
        },
        {
          "id": "MDRW2100015418.1.11",
          "form": "ㅋㅋㅋ저는",
          "original_form": "ㅋㅋㅋ저는",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015418.1.12",
          "form": "하이볼을 좋아한답니다",
          "original_form": "하이볼을 좋아한답니다",
          "speaker_id": "1",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015418.1.13",
          "form": "오.. 어떤술로 하는거요??",
          "original_form": "오.. 어떤술로 하는거요??",
          "speaker_id": "2",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015418.1.14",
          "form": "위스키에 토닉웓너나 탄산수",
          "original_form": "위스키에 토닉웓너나 탄산수",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015418.1.15",
          "form": "등등넣는거예요ㅋㅋㅋ",
          "original_form": "등등넣는거예요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015418.1.16",
          "form": "넹 ㅋㅋㅋ위스키 어쩐거로 드세요??ㅋㅋㅋ",
          "original_form": "넹 ㅋㅋㅋ위스키 어쩐거로 드세요??ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015418.1.17",
          "form": "사실 타먹진않고",
          "original_form": "사실 타먹진않고",
          "speaker_id": "1",
          "time": "20210825 23:50"
        },
        {
          "id": "MDRW2100015418.1.18",
          "form": "가게에 파는걸 먹는거라서요ㅋㅋㅋㅋㅋ",
          "original_form": "가게에 파는걸 먹는거라서요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015418.1.19",
          "form": "달달하니 알쓰에게딱이죠",
          "original_form": "달달하니 알쓰에게딱이죠",
          "speaker_id": "1",
          "time": "20210825 23:51"
        },
        {
          "id": "MDRW2100015418.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ 오.. 저도 꼭 먹어봐야겠어요. \r\n상상이 안가네요 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 오.. 저도 꼭 먹어봐야겠어요. \r\n상상이 안가네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:52"
        },
        {
          "id": "MDRW2100015418.1.21",
          "form": "네네ㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100015418.1.22",
          "form": "엄청 달고맛있어요!",
          "original_form": "엄청 달고맛있어요!",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100015418.1.23",
          "form": "오 달군요 ㅋㅋ\r\n동생한테 무슨맛이냐니까 그렇게 안닿다고 하더라고요 ㅋㅋㅋ 업체마다 레시피가 다 다른가봐요 ㅋㅋ",
          "original_form": "오 달군요 ㅋㅋ\r\n동생한테 무슨맛이냐니까 그렇게 안닿다고 하더라고요 ㅋㅋㅋ 업체마다 레시피가 다 다른가봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100015418.1.24",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100015418.1.25",
          "form": "안단걸드셨나보네요",
          "original_form": "안단걸드셨나보네요",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100015418.1.26",
          "form": "그게 농도에따라",
          "original_form": "그게 농도에따라",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100015418.1.27",
          "form": "조금타면 안달기도한데",
          "original_form": "조금타면 안달기도한데",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100015418.1.28",
          "form": "전달게먹어요ㅋㅋㅋㅋ",
          "original_form": "전달게먹어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:59"
        },
        {
          "id": "MDRW2100015418.1.29",
          "form": ". 저는 최근에 애플사이다 맥주를 마셔보고 적당한 당도의 술은 꽤 괜찮겟구나 싶엇어요 ㅋㅋ\r\n칵테일 먹고싶네요..",
          "original_form": ". 저는 최근에 애플사이다 맥주를 마셔보고 적당한 당도의 술은 꽤 괜찮겟구나 싶엇어요 ㅋㅋ\r\n칵테일 먹고싶네요..",
          "speaker_id": "2",
          "time": "20210826 17:46"
        },
        {
          "id": "MDRW2100015418.1.30",
