      "utterance": [
        {
          "id": "MDRW2100002672.1.1",
          "form": "요즘 운동같은건 해?",
          "original_form": "요즘 운동같은건 해?",
          "speaker_id": "1",
          "time": "20210820 14:39"
        },
        {
          "id": "MDRW2100002672.1.2",
          "form": "아니 전혀",
          "original_form": "아니 전혀",
          "speaker_id": "2",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002672.1.3",
          "form": "맨날 뒹굴거려",
          "original_form": "맨날 뒹굴거려",
          "speaker_id": "2",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002672.1.4",
          "form": "너는 운동 해?",
          "original_form": "너는 운동 해?",
          "speaker_id": "2",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002672.1.5",
          "form": "나도 거의 안해",
          "original_form": "나도 거의 안해",
          "speaker_id": "1",
          "time": "20210820 14:40"
        },
        {
          "id": "MDRW2100002672.1.6",
          "form": "솔직히 움직이는거 귀찮아",
          "original_form": "솔직히 움직이는거 귀찮아",
          "speaker_id": "2",
          "time": "20210820 14:41"
        },
        {
          "id": "MDRW2100002672.1.7",
          "form": "근데 맨날 먹기만 하고 뒹굴거리면 살 안쪄??",
          "original_form": "근데 맨날 먹기만 하고 뒹굴거리면 살 안쪄??",
          "speaker_id": "1",
          "time": "20210820 14:41"
        },
        {
          "id": "MDRW2100002672.1.8",
          "form": "체중을 안재서 모르겠는데",
          "original_form": "체중을 안재서 모르겠는데",
          "speaker_id": "2",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002672.1.9",
          "form": "아마 찌지 않을까?",
          "original_form": "아마 찌지 않을까?",
          "speaker_id": "2",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002672.1.10",
          "form": "지금 재보는거 추천~",
          "original_form": "지금 재보는거 추천~",
          "speaker_id": "1",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002672.1.11",
          "form": "쪘으면 몸무게에 충격받아서 집에서라도 운동하자",
          "original_form": "쪘으면 몸무게에 충격받아서 집에서라도 운동하자",
          "speaker_id": "1",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002672.1.12",
          "form": "하하.. 거절할게",
          "original_form": "하하.. 거절할게",
          "speaker_id": "2",
          "time": "20210820 14:45"
        },
        {
          "id": "MDRW2100002672.1.13",
          "form": "넌 다이어트 같은거 해본적 있어?",
          "original_form": "넌 다이어트 같은거 해본적 있어?",
          "speaker_id": "1",
          "time": "20210820 14:46"
        },
        {
          "id": "MDRW2100002672.1.14",
          "form": "아니 없어",
          "original_form": "아니 없어",
          "speaker_id": "2",
          "time": "20210820 14:47"
        },
        {
          "id": "MDRW2100002672.1.15",
          "form": "너는 있어?",
          "original_form": "너는 있어?",
          "speaker_id": "2",
          "time": "20210820 14:47"
        },
        {
          "id": "MDRW2100002672.1.16",
          "form": "난 있을턱이 없지",
          "original_form": "난 있을턱이 없지",
          "speaker_id": "1",
          "time": "20210820 14:55"
        },
        {
          "id": "MDRW2100002672.1.17",
          "form": "애초에 난 몸무게가 미달이야",
          "original_form": "애초에 난 몸무게가 미달이야",
          "speaker_id": "1",
          "time": "20210820 14:55"
        },
        {
          "id": "MDRW2100002672.1.18",
          "form": "아하... 몰랐네",
          "original_form": "아하... 몰랐네",
          "speaker_id": "2",
          "time": "20210820 14:57"
        },
        {
          "id": "MDRW2100002672.1.19",
          "form": "난 정상체중이야",
          "original_form": "난 정상체중이야",
          "speaker_id": "2",
          "time": "20210820 14:57"
        },
        {
          "id": "MDRW2100002672.1.20",
          "form": "근데 최근들어 배달 음식 시켜먹고 그러니까 살이 찌긴 찌더라",
          "original_form": "근데 최근들어 배달 음식 시켜먹고 그러니까 살이 찌긴 찌더라",
          "speaker_id": "1",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002672.1.21",
          "form": "너무 많이 먹은것 같기도해",
          "original_form": "너무 많이 먹은것 같기도해",
          "speaker_id": "1",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002672.1.22",
          "form": "미달이면 괜찮아",
          "original_form": "미달이면 괜찮아",
          "speaker_id": "2",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002672.1.23",
          "form": "쪄도 돼",
          "original_form": "쪄도 돼",
          "speaker_id": "2",
          "time": "20210820 14:59"
        },
        {
          "id": "MDRW2100002672.1.24",
          "form": "근데 팔은 여전히 나뭇가지 수준이야",
          "original_form": "근데 팔은 여전히 나뭇가지 수준이야",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002672.1.25",
          "form": "대체 어디가 찌는건지도 모르겠어",
          "original_form": "대체 어디가 찌는건지도 모르겠어",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002672.1.26",
          "form": "보통은 배 아닌가?",
          "original_form": "보통은 배 아닌가?",
          "speaker_id": "2",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002672.1.27",
          "form": "배는 그대로 더라..",
          "original_form": "배는 그대로 더라..",
          "speaker_id": "1",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002672.1.28",
          "form": "그럼 어디지",
          "original_form": "그럼 어디지",
          "speaker_id": "2",
          "time": "20210820 15:03"
        },
        {
          "id": "MDRW2100002672.1.29",
          "form": "아니면 안찐거 아니야?",
          "original_form": "아니면 안찐거 아니야?",
          "speaker_id": "2",
          "time": "20210820 15:03"
        },
        {
          "id": "MDRW2100002672.1.30",
          "form": "아니야 분명 몸무게는 늘었어",
          "original_form": "아니야 분명 몸무게는 늘었어",
          "speaker_id": "1",
          "time": "20210820 15:03"
        },
        {
          "id": "MDRW2100002672.1.31",
          "form": "근데 어딘지를 모르겠어",
          "original_form": "근데 어딘지를 모르겠어",
          "speaker_id": "1",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002672.1.32",
          "form": "음.. 신기하네",
          "original_form": "음.. 신기하네",
          "speaker_id": "2",
          "time": "20210820 15:04"
        }
      ]
    }
  ]
