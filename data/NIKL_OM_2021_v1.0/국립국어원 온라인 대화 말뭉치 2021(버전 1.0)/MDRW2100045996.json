        {
          "id": "MDRW2100045996.1.1",
          "form": "나는 찰떡 아이스크림을 좋아해",
          "original_form": "나는 찰떡 아이스크림을 좋아해",
          "speaker_id": "2",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045996.1.2",
          "form": "그게 뭐지",
          "original_form": "그게 뭐지",
          "speaker_id": "1",
          "time": "20210908 09:26"
        },
        {
          "id": "MDRW2100045996.1.3",
          "form": "롯데에서 나온 거야",
