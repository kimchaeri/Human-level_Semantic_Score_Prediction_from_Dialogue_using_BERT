      "utterance": [
        {
          "id": "MDRW2100022377.1.1",
          "form": "안녕하세요 반갑습니다",
          "original_form": "안녕하세요 반갑습니다",
          "speaker_id": "1",
          "time": "20210829 13:15"
        },
        {
          "id": "MDRW2100022377.1.2",
          "form": "안녕하세요.. 비 안오나요?",
          "original_form": "안녕하세요.. 비 안오나요?",
          "speaker_id": "2",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100022377.1.3",
          "form": "여기는 다행이 안오네요",
          "original_form": "여기는 다행이 안오네요",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022377.1.4",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022377.1.5",
          "form": "여기도 안와요... 오늘 내일은 그냥 흐리고 말듯..",
          "original_form": "여기도 안와요... 오늘 내일은 그냥 흐리고 말듯..",
          "speaker_id": "2",
          "time": "20210829 16:49"
        },
        {
          "id": "MDRW2100022377.1.6",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022377.1.7",
          "form": "비소식이 왜이리 많은지",
          "original_form": "비소식이 왜이리 많은지",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022377.1.8",
          "form": "그만 왔으면 좋겠네요",
          "original_form": "그만 왔으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210829 16:53"
        },
        {
          "id": "MDRW2100022377.1.9",
          "form": "징글맞아요..",
          "original_form": "징글맞아요..",
          "speaker_id": "2",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022377.1.10",
          "form": "장마 같아요",
          "original_form": "장마 같아요",
          "speaker_id": "2",
          "time": "20210829 16:56"
        },
        {
          "id": "MDRW2100022377.1.11",
          "form": "그러게요  한국이 언제부터 가을장마가 있었는지",
          "original_form": "그러게요  한국이 언제부터 가을장마가 있었는지",
          "speaker_id": "1",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100022377.1.12",
          "form": "최근엔 기후가 완전 변한거 같아요",
          "original_form": "최근엔 기후가 완전 변한거 같아요",
          "speaker_id": "2",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022377.1.13",
          "form": "가을 장마가 낯설지가 않아요",
          "original_form": "가을 장마가 낯설지가 않아요",
          "speaker_id": "2",
          "time": "20210829 17:57"
        },
        {
          "id": "MDRW2100022377.1.14",
          "form": "그러게요 전세계적인 문제 같네요",
          "original_form": "그러게요 전세계적인 문제 같네요",
          "speaker_id": "1",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100022377.1.15",
          "form": "그나마 태풍은 덜오는거 같아서 좋네요",
          "original_form": "그나마 태풍은 덜오는거 같아서 좋네요",
          "speaker_id": "2",
          "time": "20210829 18:01"
        },
        {
          "id": "MDRW2100022377.1.16",
          "form": "태풍도 오더라도 잘 피해갔으면 해요 농가 피해가 커서요",
          "original_form": "태풍도 오더라도 잘 피해갔으면 해요 농가 피해가 커서요",
          "speaker_id": "1",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100022377.1.17",
          "form": "근데 최근에 태풍피해도 거의 없었던듯..",
          "original_form": "근데 최근에 태풍피해도 거의 없었던듯..",
          "speaker_id": "2",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100022377.1.18",
          "form": "그래요",
          "original_form": "그래요",
          "speaker_id": "1",
          "time": "20210829 18:45"
        }
      ]
