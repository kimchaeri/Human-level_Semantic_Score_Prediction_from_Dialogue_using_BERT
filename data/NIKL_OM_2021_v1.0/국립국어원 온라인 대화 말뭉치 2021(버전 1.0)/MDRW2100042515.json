      "utterance": [
        {
          "id": "MDRW2100042515.1.1",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042515.1.2",
          "form": "그 드라마 진짜 재밌더라고요",
          "original_form": "그 드라마 진짜 재밌더라고요",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042515.1.3",
          "form": "맴이 찡했터요",
          "original_form": "맴이 찡했터요",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042515.1.4",
          "form": "강하늘 배우도 나오고요!",
          "original_form": "강하늘 배우도 나오고요!",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042515.1.5",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042515.1.6",
          "form": "네 그림도 잔짜 예쁘고요",
          "original_form": "네 그림도 잔짜 예쁘고요",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042515.1.7",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042515.1.8",
          "form": "배경이 진짜 좋았죠 보보경심려~!",
          "original_form": "배경이 진짜 좋았죠 보보경심려~!",
          "speaker_id": "1",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042515.1.9",
          "form": "강하늘도 멋지고요",
          "original_form": "강하늘도 멋지고요",
          "speaker_id": "2",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042515.1.10",
          "form": "name2님 사극을 즐겨보시나요?",
          "original_form": "&name2&님 사극을 즐겨보시나요?",
          "speaker_id": "1",
          "time": "20210906 22:36"
        },
        {
          "id": "MDRW2100042515.1.11",
          "form": "에 좋아해요 나의나라도 좋아해요 가을님은요",
          "original_form": "에 좋아해요 나의나라도 좋아해요 가을님은요",
          "speaker_id": "2",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042515.1.12",
          "form": "나의나라 재밌죠!",
          "original_form": "나의나라 재밌죠!",
          "speaker_id": "1",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042515.1.13",
          "form": "저는 처ㅣ근에 사극을 좋아하기 시작해서 본 게 몇 작품 안되네요ㅜㅜ",
          "original_form": "저는 처ㅣ근에 사극을 좋아하기 시작해서 본 게 몇 작품 안되네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042515.1.14",
          "form": "저는 장혁배우를 좋아해서 나의나라도 너무 좋게 봤답니다....",
          "original_form": "저는 장혁배우를 좋아해서 나의나라도 너무 좋게 봤답니다....",
          "speaker_id": "1",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042515.1.15",
          "form": "연가력만 좋으묜 사극은 정말 재밌어요",
          "original_form": "연가력만 좋으묜 사극은 정말 재밌어요",
          "speaker_id": "2",
          "time": "20210906 22:37"
        },
        {
          "id": "MDRW2100042515.1.16",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042515.1.17",
          "form": "사극은 진짜 연기력이 중요한 거 같아요!ㅜㅜ",
          "original_form": "사극은 진짜 연기력이 중요한 거 같아요!ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042515.1.18",
          "form": "장혁 멋지고 연기 짱이에요",
          "original_form": "장혁 멋지고 연기 짱이에요",
          "speaker_id": "2",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042515.1.19",
          "form": "장혁배우 나온 작품중에서 재밌게 버신 게 있나요?",
          "original_form": "장혁배우 나온 작품중에서 재밌게 버신 게 있나요?",
          "speaker_id": "1",
          "time": "20210906 22:38"
        },
        {
          "id": "MDRW2100042515.1.20",
          "form": "장혁 최고의 ㄷ작품은 고맙습니다 같아요",
          "original_form": "장혁 최고의 ㄷ작품은 고맙습니다 같아요",
          "speaker_id": "2",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042515.1.21",
          "form": "아하! 전 그걸 못봐서요ㅜㅜ",
          "original_form": "아하! 전 그걸 못봐서요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042515.1.22",
          "form": "장혁배우 나온 것 중에서 운명처럼 널 사랑해도 재밋답니다!",
          "original_form": "장혁배우 나온 것 중에서 운명처럼 널 사랑해도 재밋답니다!",
          "speaker_id": "1",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042515.1.23",
          "form": "ㅜㅜ 꼭 보세요",
          "original_form": "ㅜㅜ 꼭 보세요",
          "speaker_id": "2",
          "time": "20210906 22:39"
        },
        {
          "id": "MDRW2100042515.1.24",
          "form": "보고싶네요 정말 ㅜㅜ",
          "original_form": "보고싶네요 정말 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 22:40"
        },
        {
          "id": "MDRW2100042515.1.25",
          "form": "혹시 장혁나온 진짜사나이편도 보셨나요?",
          "original_form": "혹시 장혁나온 진짜사나이편도 보셨나요?",
          "speaker_id": "1",
          "time": "20210906 22:40"
        },
        {
          "id": "MDRW2100042515.1.26",
          "form": "전 그걸보고 좋아하게 됐답니다 ㅎ.ㅎ",
          "original_form": "전 그걸보고 좋아하게 됐답니다 ㅎ.ㅎ",
