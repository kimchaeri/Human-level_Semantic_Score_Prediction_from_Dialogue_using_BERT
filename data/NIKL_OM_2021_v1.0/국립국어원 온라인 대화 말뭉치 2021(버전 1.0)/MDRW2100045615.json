      "utterance": [
        {
          "id": "MDRW2100045615.1.1",
          "form": "아침메뉴는 정하셨나요?",
          "original_form": "아침메뉴는 정하셨나요?",
          "speaker_id": "1",
          "time": "20210908 06:50"
        },
        {
          "id": "MDRW2100045615.1.2",
          "form": "아직 정하지 못했어요",
          "original_form": "아직 정하지 못했어요",
          "speaker_id": "2",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045615.1.3",
          "form": "메뉴 정하셨나요",
          "original_form": "메뉴 정하셨나요",
          "speaker_id": "2",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045615.1.4",
          "form": "저는 던킨 도넛먹으려구요",
          "original_form": "저는 던킨 도넛먹으려구요",
          "speaker_id": "1",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045615.1.5",
          "form": "어제 미니도넛 사왔거든요",
          "original_form": "어제 미니도넛 사왔거든요",
          "speaker_id": "1",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045615.1.6",
          "form": "미니도넛 드셔보셨어요?",
          "original_form": "미니도넛 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100045615.1.7",
          "form": "전 냉장고에 있는 호박죽 먹어야 겠어요",
          "original_form": "전 냉장고에 있는 호박죽 먹어야 겠어요",
          "speaker_id": "2",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100045615.1.8",
          "form": "호박죽 맛나곘어요",
          "original_form": "호박죽 맛나곘어요",
          "speaker_id": "1",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100045615.1.9",
          "form": "미니도넛은 못봤던것 같네요",
          "original_form": "미니도넛은 못봤던것 같네요",
          "speaker_id": "2",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100045615.1.10",
          "form": "요즘같이 쓰리쓸쓸할땐",
          "original_form": "요즘같이 쓰리쓸쓸할땐",
          "speaker_id": "1",
          "time": "20210908 07:02"
        },
        {
          "id": "MDRW2100045615.1.11",
          "form": "뜨끈한 죽도 좋죠",
          "original_form": "뜨끈한 죽도 좋죠",
          "speaker_id": "1",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045615.1.12",
          "form": "죽이라서 아침에 부담없이 먹을수가 있죠",
          "original_form": "죽이라서 아침에 부담없이 먹을수가 있죠",
          "speaker_id": "2",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045615.1.13",
          "form": "미니도넛 맛있겠어요",
          "original_form": "미니도넛 맛있겠어요",
          "speaker_id": "2",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100045615.1.14",
          "form": "속도 편하고 아침메뉴로 딱이네요",
          "original_form": "속도 편하고 아침메뉴로 딱이네요",
          "speaker_id": "1",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100045615.1.15",
          "form": "미니도넛은 마블땜인지",
          "original_form": "미니도넛은 마블땜인지",
          "speaker_id": "1",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100045615.1.16",
          "form": "손바닥만하게 미니도넛6개 있더라구요",
          "original_form": "손바닥만하게 미니도넛6개 있더라구요",
          "speaker_id": "1",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100045615.1.17",
          "form": "커피랑 같이 먹어도 좋을것 같네요",
          "original_form": "커피랑 같이 먹어도 좋을것 같네요",
          "speaker_id": "2",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100045615.1.18",
          "form": "네 안그래도 커피랑 먹으려구요",
          "original_form": "네 안그래도 커피랑 먹으려구요",
          "speaker_id": "1",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045615.1.19",
          "form": "그래도 도넛이니 비쌀줄알았는데",
          "original_form": "그래도 도넛이니 비쌀줄알았는데",
          "speaker_id": "1",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045615.1.20",
          "form": "3300원이더라구요",
          "original_form": "3300원이더라구요",
          "speaker_id": "1",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045615.1.21",
          "form": "가격 괜찮죠?",
          "original_form": "가격 괜찮죠?",
          "speaker_id": "1",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045615.1.22",
          "form": "파바가면 도넛 몇개집으면 만원 후딱인데 ㅎ",
          "original_form": "파바가면 도넛 몇개집으면 만원 후딱인데 ㅎ",
          "speaker_id": "1",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045615.1.23",
          "form": "가격이 저렴하네요",
          "original_form": "가격이 저렴하네요",
          "speaker_id": "2",
          "time": "20210908 07:05"
        },
        {
          "id": "MDRW2100045615.1.24",
          "form": "거기에 해피앱에서 천원 쿠폰줘서",
          "original_form": "거기에 해피앱에서 천원 쿠폰줘서",
          "speaker_id": "1",
          "time": "20210908 07:06"
        },
        {
          "id": "MDRW2100045615.1.25",
          "form": "2300원에 사왔어요 ㅎ",
          "original_form": "2300원에 사왔어요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 07:06"
        },
        {
          "id": "MDRW2100045615.1.26",
          "form": "아 완전 대박 찬스네요",
          "original_form": "아 완전 대박 찬스네요",
          "speaker_id": "2",
          "time": "20210908 07:06"
        },
        {
          "id": "MDRW2100045615.1.27",
          "form": "네 저도 비쌀줄알고 천원쿠폰이길래",
          "original_form": "네 저도 비쌀줄알고 천원쿠폰이길래",
          "speaker_id": "1",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100045615.1.28",
          "form": "머 이런 쿠폰을...했는데",
          "original_form": "머 이런 쿠폰을...했는데",
          "speaker_id": "1",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100045615.1.29",
          "form": "괜찮더라구요 ㅎ",
          "original_form": "괜찮더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100045615.1.30",
          "form": "사오니 아이들이 2개씩 후딱 헤치우네요 ㅎ",
          "original_form": "사오니 아이들이 2개씩 후딱 헤치우네요 ㅎ",
          "speaker_id": "1",
          "time": "20210908 07:07"
        },
        {
          "id": "MDRW2100045615.1.31",
          "form": "사이즈도 자그마하니 저희 아이들이 먹기에 딱좋은 사이즈더라구요",
          "original_form": "사이즈도 자그마하니 저희 아이들이 먹기에 딱좋은 사이즈더라구요",
          "speaker_id": "1",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100045615.1.32",
          "form": "저도 쿠폰 찾아서 방문해봐야 겠어요",
          "original_form": "저도 쿠폰 찾아서 방문해봐야 겠어요",
          "speaker_id": "2",
          "time": "20210908 07:08"
        },
        {
          "id": "MDRW2100045615.1.33",
          "form": "아이들도 좋아하는 맛인가봐요",
          "original_form": "아이들도 좋아하는 맛인가봐요",
          "speaker_id": "2",
          "time": "20210908 07:08"
        }
      ]
