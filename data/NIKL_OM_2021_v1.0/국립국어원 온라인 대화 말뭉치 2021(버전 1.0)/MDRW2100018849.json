      "utterance": [
        {
          "id": "MDRW2100018849.1.1",
          "form": "안녕하세요.",
          "original_form": "안녕하세요.",
          "speaker_id": "1",
          "time": "20210827 12:51"
        },
        {
          "id": "MDRW2100018849.1.2",
          "form": "오늘 정말 가을 같네요.",
          "original_form": "오늘 정말 가을 같네요.",
          "speaker_id": "1",
          "time": "20210827 12:51"
        },
        {
          "id": "MDRW2100018849.1.3",
          "form": "네 날씨 너무좋아요.. 하늘도 청명하구!",
          "original_form": "네 날씨 너무좋아요.. 하늘도 청명하구!",
          "speaker_id": "2",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100018849.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100018849.1.5",
          "form": "요새 비가 계속 와서 힘들었는데",
          "original_form": "요새 비가 계속 와서 힘들었는데",
          "speaker_id": "1",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100018849.1.6",
          "form": "날씨 맑아서 기분도 좋네요",
          "original_form": "날씨 맑아서 기분도 좋네요",
          "speaker_id": "1",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100018849.1.7",
          "form": "맞아요! 가을 좋아하시나봐요??",
          "original_form": "맞아요! 가을 좋아하시나봐요??",
          "speaker_id": "2",
          "time": "20210827 12:54"
        },
        {
          "id": "MDRW2100018849.1.8",
          "form": "네 저는 가을겨울이 좋더라구요",
          "original_form": "네 저는 가을겨울이 좋더라구요",
          "speaker_id": "1",
          "time": "20210827 12:56"
        },
        {
          "id": "MDRW2100018849.1.9",
          "form": "옷을 좋아해서",
          "original_form": "옷을 좋아해서",
          "speaker_id": "1",
          "time": "20210827 12:56"
        },
        {
          "id": "MDRW2100018849.1.10",
          "form": "껴입는 걸 특히 좋아하거든요",
          "original_form": "껴입는 걸 특히 좋아하거든요",
          "speaker_id": "1",
          "time": "20210827 12:56"
        },
        {
          "id": "MDRW2100018849.1.11",
          "form": "아 패션에 관심이 많으신가보군요!!",
          "original_form": "아 패션에 관심이 많으신가보군요!!",
          "speaker_id": "2",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100018849.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 13:00"
        },
        {
          "id": "MDRW2100018849.1.13",
          "form": "거의 맨날 눈팅만 하지만요",
          "original_form": "거의 맨날 눈팅만 하지만요",
          "speaker_id": "1",
          "time": "20210827 13:00"
        },
        {
          "id": "MDRW2100018849.1.14",
          "form": "아 ㅋㅋㅋ 지금 이시국이라 입으러갈데가 없긴해요 ㅠ",
          "original_form": "아 ㅋㅋㅋ 지금 이시국이라 입으러갈데가 없긴해요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 13:02"
        },
        {
          "id": "MDRW2100018849.1.15",
          "form": "그래도 전 절기바뀔때마다 쇼핑하러가는거 좋아해요",
          "original_form": "그래도 전 절기바뀔때마다 쇼핑하러가는거 좋아해요",
          "speaker_id": "2",
          "time": "20210827 13:02"
        },
        {
          "id": "MDRW2100018849.1.16",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210827 13:03"
        },
        {
          "id": "MDRW2100018849.1.17",
          "form": "나중에 입을 거 알지만",
          "original_form": "나중에 입을 거 알지만",
          "speaker_id": "1",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018849.1.18",
          "form": "품절 될까 봐",
          "original_form": "품절 될까 봐",
          "speaker_id": "1",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018849.1.19",
          "form": "사게 되더라고요",
          "original_form": "사게 되더라고요",
          "speaker_id": "1",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018849.1.20",
          "form": "맞아요! 옷구매는 타이밍",
          "original_form": "맞아요! 옷구매는 타이밍",
          "speaker_id": "2",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018849.1.21",
          "form": "맘에드는거있으면 사야해요 아니면 없어지더라구요",
          "original_form": "맘에드는거있으면 사야해요 아니면 없어지더라구요",
          "speaker_id": "2",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018849.1.22",
          "form": "맞아요 방금도",
          "original_form": "맞아요 방금도",
          "speaker_id": "1",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018849.1.23",
          "form": "사려고 한 트위드 자켓 품절이라",
          "original_form": "사려고 한 트위드 자켓 품절이라",
          "speaker_id": "1",
          "time": "20210827 13:28"
        },
        {
          "id": "MDRW2100018849.1.24",
          "form": "넘 눈물이 나네요",
          "original_form": "넘 눈물이 나네요",
          "speaker_id": "1",
          "time": "20210827 13:29"
        },
        {
          "id": "MDRW2100018849.1.25",
          "form": "아정말요??ㅜㅜ 세일기간에는 품절이 잦아서 슬퍼요",
          "original_form": "아정말요??ㅜㅜ 세일기간에는 품절이 잦아서 슬퍼요",
          "speaker_id": "2",
          "time": "20210827 14:34"
        },
        {
          "id": "MDRW2100018849.1.26",
