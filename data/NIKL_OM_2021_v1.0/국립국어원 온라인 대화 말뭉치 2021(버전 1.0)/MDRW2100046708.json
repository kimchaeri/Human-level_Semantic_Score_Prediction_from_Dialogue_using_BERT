        {
          "id": "MDRW2100046708.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211105 00:37"
        },
        {
          "id": "MDRW2100046708.1.3",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20211105 00:37"
        },
        {
          "id": "MDRW2100046708.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20211105 00:37"
        },
        {
          "id": "MDRW2100046708.1.5",
          "form": "스벅 좋아해요!",
          "original_form": "스벅 좋아해요!",
          "speaker_id": "1",
          "time": "20211105 00:37"
        },
        {
          "id": "MDRW2100046708.1.6",
          "form": "비싼 입맛이시네요 ㅎ",
          "original_form": "비싼 입맛이시네요 ㅎ",
          "speaker_id": "2",
          "time": "20211105 00:38"
        },
        {
          "id": "MDRW2100046708.1.7",
          "form": "스벅에서 가장 좋아하는 메뉴가 뭐에용?",
          "original_form": "스벅에서 가장 좋아하는 메뉴가 뭐에용?",
          "speaker_id": "2",
          "time": "20211105 00:39"
        },
        {
          "id": "MDRW2100046708.1.8",
          "form": "아메리카노",
          "original_form": "아메리카노",
          "speaker_id": "1",
          "time": "20211104 13:10"
        },
        {
          "id": "MDRW2100046708.1.9",
          "form": "카페모카요!",
          "original_form": "카페모카요!",
          "speaker_id": "1",
          "time": "20211104 13:10"
        },
        {
          "id": "MDRW2100046708.1.10",
          "form": "오 스벅에서 카페모카는 한 번도 안 먹어봤는데",
          "original_form": "오 스벅에서 카페모카는 한 번도 안 먹어봤는데",
          "speaker_id": "2",
          "time": "20211104 13:39"
        },
        {
          "id": "MDRW2100046708.1.11",
          "form": "맛있나요 ㅎㅎㅎ",
          "original_form": "맛있나요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 13:39"
        },
        {
          "id": "MDRW2100046708.1.12",
          "form": "넹 근데 저도 주로 아메리카노 먹어요",
          "original_form": "넹 근데 저도 주로 아메리카노 먹어요",
          "speaker_id": "1",
          "time": "20211104 13:39"
        },
        {
          "id": "MDRW2100046708.1.13",
          "form": "아하 ㅋㅋㅋㅋ 님은 커피",
          "original_form": "아하 ㅋㅋㅋㅋ 님은 커피",
          "speaker_id": "2",
          "time": "20211104 13:41"
        },
        {
          "id": "MDRW2100046708.1.14",
          "form": "신맛나는거 좋아하세여?",
          "original_form": "신맛나는거 좋아하세여?",
          "speaker_id": "2",
          "time": "20211104 13:41"
        },
        {
          "id": "MDRW2100046708.1.15",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20211104 13:41"
        },
        {
          "id": "MDRW2100046708.1.16",
          "form": "커피향 구수하게 나는게 좋아여",
          "original_form": "커피향 구수하게 나는게 좋아여",
          "speaker_id": "1",
          "time": "20211104 13:41"
        },
        {
          "id": "MDRW2100046708.1.17",
          "form": "오 저도",
          "original_form": "오 저도",
          "speaker_id": "2",
          "time": "20211104 13:41"
        },
        {
          "id": "MDRW2100046708.1.18",
          "form": "산미는 별로",
          "original_form": "산미는 별로",
          "speaker_id": "1",
          "time": "20211104 13:41"
        },
        {
          "id": "MDRW2100046708.1.19",
          "form": "산미 강한거 싫어요",
          "original_form": "산미 강한거 싫어요",
          "speaker_id": "2",
          "time": "20211104 13:41"
        },
        {
          "id": "MDRW2100046708.1.20",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20211104 13:41"
        },
        {
          "id": "MDRW2100046708.1.21",
          "form": "스벅이 좀 구수한거같아욬ㅋㅋㅋㅋ",
          "original_form": "스벅이 좀 구수한거같아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 13:41"
        },
        {
          "id": "MDRW2100046708.1.22",
          "form": "커피빈이 시고..",
          "original_form": "커피빈이 시고..",
          "speaker_id": "2",
          "time": "20211104 13:41"
        },
        {
          "id": "MDRW2100046708.1.23",
          "form": "커피빈은 안먹어봐서 모르겠는데 스벅이 제 취향",
          "original_form": "커피빈은 안먹어봐서 모르겠는데 스벅이 제 취향",
          "speaker_id": "1",
          "time": "20211104 13:43"
        },
        {
          "id": "MDRW2100046708.1.24",
          "form": "지점마다 다른건진 모르겠는데 제 입에는 커피빈이 프랜차이즈 카페 중에 제일 산미가 셌어요 ㅠ\r\n\r\n",
