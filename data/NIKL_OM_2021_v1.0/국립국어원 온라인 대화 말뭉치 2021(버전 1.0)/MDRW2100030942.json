      "utterance": [
        {
          "id": "MDRW2100030942.1.1",
          "form": "어제오늘 간만에 해가 났네요. 거긴 어떤가요?",
          "original_form": "어제오늘 간만에 해가 났네요. 거긴 어떤가요?",
          "speaker_id": "1",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100030942.1.2",
          "form": "여기도 해떳어요!",
          "original_form": "여기도 해떳어요!",
          "speaker_id": "2",
          "time": "20210904 14:33"
        },
        {
          "id": "MDRW2100030942.1.3",
          "form": "너무 오랜만에 보는거같아요 ㅠㅠ",
          "original_form": "너무 오랜만에 보는거같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 14:33"
        },
        {
          "id": "MDRW2100030942.1.4",
          "form": "그러게 말입니다",
          "original_form": "그러게 말입니다",
          "speaker_id": "1",
          "time": "20210904 14:35"
        },
        {
          "id": "MDRW2100030942.1.5",
          "form": "거의 2주만이지요",
          "original_form": "거의 2주만이지요",
          "speaker_id": "1",
          "time": "20210904 14:35"
        },
        {
          "id": "MDRW2100030942.1.6",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "2",
          "time": "20210904 14:41"
        },
        {
          "id": "MDRW2100030942.1.7",
          "form": "금요일부터 일요일까지 해가 나서 정말 기분좋은 주말을 보냈었스니다",
          "original_form": "금요일부터 일요일까지 해가 나서 정말 기분좋은 주말을 보냈었스니다",
          "speaker_id": "1",
          "time": "20210906 06:23"
        },
        {
          "id": "MDRW2100030942.1.8",
          "form": "그런데 내일 비소식이 또 있더군요",
          "original_form": "그런데 내일 비소식이 또 있더군요",
          "speaker_id": "1",
          "time": "20210906 06:23"
        },
        {
          "id": "MDRW2100030942.1.9",
          "form": "장마는 끝난 거였으면 좋겠습니다",
          "original_form": "장마는 끝난 거였으면 좋겠습니다",
          "speaker_id": "1",
          "time": "20210906 06:23"
        },
        {
          "id": "MDRW2100030942.1.10",
          "form": "저 백신맞으러가는 9월 9일도 비소식있던데 안왔으면 좋겠네요 ㅠㅠ",
          "original_form": "저 백신맞으러가는 9월 9일도 비소식있던데 안왔으면 좋겠네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 11:16"
        },
        {
          "id": "MDRW2100030942.1.11",
          "form": "비가 너무 자주 오네요",
          "original_form": "비가 너무 자주 오네요",
          "speaker_id": "1",
          "time": "20210906 14:09"
        },
        {
          "id": "MDRW2100030942.1.12",
          "form": "백신 잘 맞아야 할텐데요",
          "original_form": "백신 잘 맞아야 할텐데요",
          "speaker_id": "1",
          "time": "20210906 14:09"
        },
        {
          "id": "MDRW2100030942.1.13",
          "form": "환절기이기도 하니 감기 조심해야 합니다",
          "original_form": "환절기이기도 하니 감기 조심해야 합니다",
          "speaker_id": "1",
          "time": "20210906 14:09"
        },
        {
          "id": "MDRW2100030942.1.14",
          "form": "컨디션이 안좋으면 백신을 못 맞을 수도 있다고 하더군요",
          "original_form": "컨디션이 안좋으면 백신을 못 맞을 수도 있다고 하더군요",
          "speaker_id": "1",
          "time": "20210906 14:09"
        },
        {
          "id": "MDRW2100030942.1.15",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "2",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100030942.1.16",
          "form": "요즘같은 기온차 크고",
          "original_form": "요즘같은 기온차 크고",
          "speaker_id": "2",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100030942.1.17",
          "form": "비도 오는 이때 컨디션 조절 잘해야죠",
          "original_form": "비도 오는 이때 컨디션 조절 잘해야죠",
          "speaker_id": "2",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100030942.1.18",
          "form": "한겨울보다 환절기에 감기환자들이 많으니 조심해야지요",
          "original_form": "한겨울보다 환절기에 감기환자들이 많으니 조심해야지요",
          "speaker_id": "1",
          "time": "20210906 14:18"
        },
        {
          "id": "MDRW2100030942.1.19",
          "form": "가벼운 걸칠 옷도 필요하지요",
          "original_form": "가벼운 걸칠 옷도 필요하지요",
          "speaker_id": "1",
          "time": "20210906 14:19"
        },
        {
          "id": "MDRW2100030942.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210906 14:33"
        },
        {
          "id": "MDRW2100030942.1.21",
          "form": "아침에 산책나갔는데 쌀쌀했답니다",
          "original_form": "아침에 산책나갔는데 쌀쌀했답니다",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100030942.1.22",
          "form": "내일부터는 얇은 점퍼 챙길 생각입니다",
          "original_form": "내일부터는 얇은 점퍼 챙길 생각입니다",
          "speaker_id": "1",
          "time": "20210906 14:40"
        },
        {
          "id": "MDRW2100030942.1.23",
          "form": "저도 슬슬 가디건이나 겉옷좀 구비해둘려고요",
          "original_form": "저도 슬슬 가디건이나 겉옷좀 구비해둘려고요",
          "speaker_id": "2",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100030942.1.24",
          "form": "날씨가 쌀쌀해지니..",
          "original_form": "날씨가 쌀쌀해지니..",
          "speaker_id": "2",
          "time": "20210906 14:44"
        },
        {
          "id": "MDRW2100030942.1.25",
          "form": "백신 맞기전에 건강 관리 잘 해야하니까요",
          "original_form": "백신 맞기전에 건강 관리 잘 해야하니까요",
          "speaker_id": "1",
          "time": "20210906 17:05"
        }
      ]
    }
