      "utterance": [
        {
          "id": "MDRW2100027667.1.1",
          "form": "최근에영화관 갔음?",
          "original_form": "최근에영화관 갔음?",
          "speaker_id": "1",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100027667.1.2",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100027667.1.3",
          "form": "1년 넘게 안갔을듯",
          "original_form": "1년 넘게 안갔을듯",
          "speaker_id": "2",
          "time": "20210902 14:06"
        },
        {
          "id": "MDRW2100027667.1.4",
          "form": "나도 코로나 때문에 안감 ㅠ",
          "original_form": "나도 코로나 때문에 안감 ㅠ",
          "speaker_id": "1",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027667.1.5",
          "form": "요번에 블랙위도우 볼랴고 고민하긴햇ㅅ서",
          "original_form": "요번에 블랙위도우 볼랴고 고민하긴햇ㅅ서",
          "speaker_id": "1",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027667.1.6",
          "form": "맞아 볼까 했는데",
          "original_form": "맞아 볼까 했는데",
          "speaker_id": "2",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027667.1.7",
          "form": "찝찝해서 안갔음",
          "original_form": "찝찝해서 안갔음",
          "speaker_id": "2",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027667.1.8",
          "form": "근디또 의외로 카페보다 안전한거같기도하고",
          "original_form": "근디또 의외로 카페보다 안전한거같기도하고",
          "speaker_id": "1",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027667.1.9",
          "form": "말을 안하재녀",
          "original_form": "말을 안하재녀",
          "speaker_id": "1",
          "time": "20210902 14:07"
        },
        {
          "id": "MDRW2100027667.1.10",
          "form": "그렇긴 함 ㅋㅋㅋ",
          "original_form": "그렇긴 함 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027667.1.11",
          "form": "음료취식만 가능하대",
          "original_form": "음료취식만 가능하대",
          "speaker_id": "1",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027667.1.12",
          "form": "사람 별로 없으면 괜찮을 것 같기도",
          "original_form": "사람 별로 없으면 괜찮을 것 같기도",
          "speaker_id": "2",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027667.1.13",
          "form": "스파이더맨은 보러가려구..",
          "original_form": "스파이더맨은 보러가려구..",
          "speaker_id": "2",
          "time": "20210902 14:08"
        },
        {
          "id": "MDRW2100027667.1.14",
          "form": "마자 그건 무조건 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "마자 그건 무조건 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027667.1.15",
          "form": "예고 장난아니돋제",
          "original_form": "예고 장난아니돋제",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027667.1.16",
          "form": "어엌ㅋㅋㅋ예고보니까 가서 봐야제",
          "original_form": "어엌ㅋㅋㅋ예고보니까 가서 봐야제",
          "speaker_id": "2",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027667.1.17",
          "form": "마블의리 지키러간다ㅠ",
          "original_form": "마블의리 지키러간다ㅠ",
          "speaker_id": "2",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027667.1.18",
          "form": "그땐 팝콘 먹을수잇음좋것다",
          "original_form": "그땐 팝콘 먹을수잇음좋것다",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027667.1.19",
          "form": "맛잇지...",
          "original_form": "맛잇지...",
          "speaker_id": "2",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027667.1.20",
          "form": "cgv에 팔던 오징어도 맛있었는데",
          "original_form": "cgv에 팔던 오징어도 맛있었는데",
          "speaker_id": "2",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027667.1.21",
          "form": "음료가능하명 팝콘도...먹게해주지",
          "original_form": "음료가능하명 팝콘도...먹게해주지",
          "speaker_id": "1",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027667.1.22",
          "form": "아 버터오징어 존맛탱",
          "original_form": "아 버터오징어 존맛탱",
          "speaker_id": "1",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027667.1.23",
          "form": "ㅋㅋㅋ촉촉 쫠깃",
          "original_form": "ㅋㅋㅋ촉촉 쫠깃",
          "speaker_id": "2",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027667.1.24",
          "form": "먹고싶네...",
          "original_form": "먹고싶네...",
          "speaker_id": "2",
          "time": "20210902 14:10"
        },
        {
          "id": "MDRW2100027667.1.25",
          "form": "향기 장난아닌데 ㅜㅜ",
          "original_form": "향기 장난아닌데 ㅜㅜ",
