      "utterance": [
        {
          "id": "MDRW2100012809.1.1",
          "form": "공연 보는거 좋아하세요?",
          "original_form": "공연 보는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 01:44"
        },
        {
          "id": "MDRW2100012809.1.2",
          "form": "좋아하는데 지방이라 기회가 없었어요ㅠ",
          "original_form": "좋아하는데 지방이라 기회가 없었어요ㅠ",
          "speaker_id": "2",
          "time": "20210825 01:47"
        },
        {
          "id": "MDRW2100012809.1.3",
          "form": "name1님은 자주보러 다니셨나요?",
          "original_form": "&name1&님은 자주보러 다니셨나요?",
          "speaker_id": "2",
          "time": "20210825 01:47"
        },
        {
          "id": "MDRW2100012809.1.4",
          "form": "저도 지방살아요",
          "original_form": "저도 지방살아요",
          "speaker_id": "1",
          "time": "20210825 01:47"
        },
        {
          "id": "MDRW2100012809.1.5",
          "form": "요즘은 아니고 예전에 아이유랑 해외 연예인 내한 콘서트 다녔어요~",
          "original_form": "요즘은 아니고 예전에 아이유랑 해외 연예인 내한 콘서트 다녔어요~",
          "speaker_id": "1",
          "time": "20210825 01:48"
        },
        {
          "id": "MDRW2100012809.1.6",
          "form": "와 아이유! 진짜 티켓팅 엄두도 안나요ㅠ",
          "original_form": "와 아이유! 진짜 티켓팅 엄두도 안나요ㅠ",
          "speaker_id": "2",
          "time": "20210825 01:52"
        },
        {
          "id": "MDRW2100012809.1.7",
          "form": "와 내한콘서트도 가보면 만족도 장난아니라고 하더라고요",
          "original_form": "와 내한콘서트도 가보면 만족도 장난아니라고 하더라고요",
          "speaker_id": "2",
          "time": "20210825 01:52"
        },
        {
          "id": "MDRW2100012809.1.8",
          "form": "비싼 값을 하더라고요",
          "original_form": "비싼 값을 하더라고요",
          "speaker_id": "1",
          "time": "20210825 01:53"
        },
        {
          "id": "MDRW2100012809.1.9",
          "form": "자본의 맛 제대로",
          "original_form": "자본의 맛 제대로",
          "speaker_id": "1",
          "time": "20210825 01:53"
        },
        {
          "id": "MDRW2100012809.1.10",
          "form": "뮤지컬 보신적 있으세요?",
          "original_form": "뮤지컬 보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210825 01:53"
        },
        {
          "id": "MDRW2100012809.1.11",
          "form": "오 없어유..",
          "original_form": "오 없어유..",
          "speaker_id": "2",
          "time": "20210825 01:54"
        },
        {
          "id": "MDRW2100012809.1.12",
          "form": "물어보시는거 다 없어서 머쓱한",
          "original_form": "물어보시는거 다 없어서 머쓱한",
          "speaker_id": "2",
          "time": "20210825 01:54"
        },
        {
          "id": "MDRW2100012809.1.13",
          "form": "자본의 맛이 그리좋군여...",
          "original_form": "자본의 맛이 그리좋군여...",
          "speaker_id": "2",
          "time": "20210825 01:54"
        },
        {
          "id": "MDRW2100012809.1.14",
          "form": "저도 없어요ㅋㅋ",
          "original_form": "저도 없어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:54"
        },
        {
          "id": "MDRW2100012809.1.15",
          "form": "뮤지컬은 어떤거 보셯",
          "original_form": "뮤지컬은 어떤거 보셯",
          "speaker_id": "2",
          "time": "20210825 01:54"
        },
        {
          "id": "MDRW2100012809.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210825 01:54"
        },
        {
          "id": "MDRW2100012809.1.17",
          "form": "연극 본적 있으신가요?",
          "original_form": "연극 본적 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 01:54"
        },
        {
          "id": "MDRW2100012809.1.18",
          "form": "뭔가 콘서트는 현장감이 장난아니더라고용",
          "original_form": "뭔가 콘서트는 현장감이 장난아니더라고용",
          "speaker_id": "2",
          "time": "20210825 01:54"
        },
        {
          "id": "MDRW2100012809.1.19",
          "form": "없다고..말한다면..",
          "original_form": "없다고..말한다면..",
          "speaker_id": "2",
          "time": "20210825 01:54"
        },
        {
          "id": "MDRW2100012809.1.20",
          "form": "대홧거리가 넘 안생기죠ㅠㅠ",
          "original_form": "대홧거리가 넘 안생기죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 01:54"
        },
        {
          "id": "MDRW2100012809.1.21",
          "form": "연극은 혜화 갔을때 보려다 마땅한게 없어서 안봤었어유!",
          "original_form": "연극은 혜화 갔을때 보려다 마땅한게 없어서 안봤었어유!",
          "speaker_id": "2",
          "time": "20210825 01:55"
        },
        {
          "id": "MDRW2100012809.1.22",
          "form": "만들면 되죠ㅎㅎ",
          "original_form": "만들면 되죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 01:55"
        },
        {
          "id": "MDRW2100012809.1.23",
          "form": "전시회 보신적있으신가요?",
          "original_form": "전시회 보신적있으신가요?",
          "speaker_id": "1",
          "time": "20210825 01:55"
        },
        {
          "id": "MDRW2100012809.1.24",
          "form": "네 전에 상상마당 가서 사진전? 본 적 있어요",
          "original_form": "네 전에 상상마당 가서 사진전? 본 적 있어요",
          "speaker_id": "2",
          "time": "20210825 01:56"
        },
        {
          "id": "MDRW2100012809.1.25",
          "form": "가끔 서울가면 박물관이나 미술관 가보는거 좋아해요!",
          "original_form": "가끔 서울가면 박물관이나 미술관 가보는거 좋아해요!",
          "speaker_id": "2",
          "time": "20210825 01:56"
        },
        {
          "id": "MDRW2100012809.1.26",
          "form": "name1님은 문화생활 자주 하시나봐요!",
          "original_form": "&name1&님은 문화생활 자주 하시나봐요!",
          "speaker_id": "2",
          "time": "20210825 01:56"
        },
        {
          "id": "MDRW2100012809.1.27",
          "form": "과거형이에요",
          "original_form": "과거형이에요",
          "speaker_id": "1",
          "time": "20210825 01:57"
        },
        {
          "id": "MDRW2100012809.1.28",
          "form": "했었어요ㅋㅋ",
          "original_form": "했었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:57"
        }
      ]
    }
  ]
