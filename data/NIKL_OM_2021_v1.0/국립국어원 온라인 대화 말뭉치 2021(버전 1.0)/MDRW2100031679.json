      "utterance": [
        {
          "id": "MDRW2100031679.1.1",
          "form": "이번올림픽은 보셨나요?!",
          "original_form": "이번올림픽은 보셨나요?!",
          "speaker_id": "1",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100031679.1.2",
          "form": "네 잘봤어요",
          "original_form": "네 잘봤어요",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100031679.1.3",
          "form": "재밌었어요",
          "original_form": "재밌었어요",
          "speaker_id": "2",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031679.1.4",
          "form": "그쵸!!",
          "original_form": "그쵸!!",
          "speaker_id": "1",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031679.1.5",
          "form": "배구도 4강신화갔어요!!",
          "original_form": "배구도 4강신화갔어요!!",
          "speaker_id": "1",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031679.1.6",
          "form": "그러네요 4강신화네요 ㅎㅎ",
          "original_form": "그러네요 4강신화네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031679.1.7",
          "form": "국내브이리그도 가보고싶어요!",
          "original_form": "국내브이리그도 가보고싶어요!",
          "speaker_id": "1",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031679.1.8",
          "form": "가보신적있으세요?!",
          "original_form": "가보신적있으세요?!",
          "speaker_id": "1",
          "time": "20210904 23:32"
        },
        {
          "id": "MDRW2100031679.1.9",
          "form": "아뇨 ㅎ 저는 가본적이 없는데 가보고싶네요?",
          "original_form": "아뇨 ㅎ 저는 가본적이 없는데 가보고싶네요?",
          "speaker_id": "2",
          "time": "20210904 23:32"
