      "utterance": [
        {
          "id": "MDRW2100034488.1.1",
          "form": "네 전 쌀국수 먹었어요",
          "original_form": "네 전 쌀국수 먹었어요",
          "speaker_id": "2",
          "time": "20210828 13:55"
        },
        {
          "id": "MDRW2100034488.1.2",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210828 13:55"
        },
        {
          "id": "MDRW2100034488.1.3",
          "form": "저는 라면이요ㅎㅎ",
          "original_form": "저는 라면이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 18:19"
        },
        {
          "id": "MDRW2100034488.1.4",
          "form": "어젯밤에 먹방 영상보는데 라면이 너무 먹고 싶어서ㅋㅋㅋ",
          "original_form": "어젯밤에 먹방 영상보는데 라면이 너무 먹고 싶어서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:19"
        },
        {
          "id": "MDRW2100034488.1.5",
          "form": "저도 먹방 영상 볼 때는 특히 라면이 엄청 땡기더라고요ㅋㅋ",
          "original_form": "저도 먹방 영상 볼 때는 특히 라면이 엄청 땡기더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:52"
        },
        {
          "id": "MDRW2100034488.1.6",
          "form": "아는 맛이라 그런 것 같아요",
          "original_form": "아는 맛이라 그런 것 같아요",
          "speaker_id": "2",
          "time": "20210828 18:52"
        },
        {
          "id": "MDRW2100034488.1.7",
          "form": "맞아요 아는 맛이면 더 먹고 싶어지는ㅠㅠ",
          "original_form": "맞아요 아는 맛이면 더 먹고 싶어지는ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 01:11"
        },
        {
          "id": "MDRW2100034488.1.8",
          "form": "저녁으로 어떤거 드셨나용?",
          "original_form": "저녁으로 어떤거 드셨나용?",
          "speaker_id": "1",
          "time": "20210829 01:11"
        },
        {
          "id": "MDRW2100034488.1.9",
          "form": "미역국이요",
          "original_form": "미역국이요",
          "speaker_id": "2",
          "time": "20210829 08:39"
        },
        {
          "id": "MDRW2100034488.1.10",
          "form": "name1 님은 뭐 드셨어요?",
          "original_form": "&name1& 님은 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210829 08:40"
        },
        {
          "id": "MDRW2100034488.1.11",
          "form": "저는 어제 냉면 먹었어요!",
          "original_form": "저는 어제 냉면 먹었어요!",
          "speaker_id": "1",
          "time": "20210829 19:41"
        },
        {
          "id": "MDRW2100034488.1.12",
          "form": "방금은 치킨 먹었는데 name2님은 저녁 드셨나요?",
          "original_form": "방금은 치킨 먹었는데 &name2&님은 저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210829 19:41"
        },
        {
          "id": "MDRW2100034488.1.13",
          "form": "네 전 집밥 먹었어요",
          "original_form": "네 전 집밥 먹었어요",
          "speaker_id": "2",
          "time": "20210829 20:49"
        },
        {
          "id": "MDRW2100034488.1.14",
          "form": "무슨 치킨 드셨어요?",
          "original_form": "무슨 치킨 드셨어요?",
          "speaker_id": "2",
          "time": "20210829 20:49"
        },
        {
          "id": "MDRW2100034488.1.15",
          "form": "시장에 맛있다는 치킨 집 있다길래 사왔는데",
          "original_form": "시장에 맛있다는 치킨 집 있다길래 사왔는데",
          "speaker_id": "1",
          "time": "20210830 13:18"
        },
        {
          "id": "MDRW2100034488.1.16",
          "form": "왕천파닭이네요ㅎㅎ",
          "original_form": "왕천파닭이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 13:19"
        },
        {
          "id": "MDRW2100034488.1.17",
          "form": "파닭 맛있겠네요",
          "original_form": "파닭 맛있겠네요",
          "speaker_id": "2",
          "time": "20210830 15:15"
        },
        {
          "id": "MDRW2100034488.1.18",
          "form": "오랜만에 파절임이 먹고 싶어졌어요",
          "original_form": "오랜만에 파절임이 먹고 싶어졌어요",
          "speaker_id": "2",
          "time": "20210830 15:15"
        },
        {
          "id": "MDRW2100034488.1.19",
          "form": "파절임 맛있죠ㅎㅎ",
          "original_form": "파절임 맛있죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100034488.1.20",
          "form": "좋아하는 반찬 있으신가요?",
          "original_form": "좋아하는 반찬 있으신가요?",
          "speaker_id": "1",
          "time": "20210831 00:30"
        },
        {
          "id": "MDRW2100034488.1.21",
          "form": "가지볶음이요! 계속 먹어도 안 질리고 맛있어요",
          "original_form": "가지볶음이요! 계속 먹어도 안 질리고 맛있어요",
          "speaker_id": "2",
          "time": "20210831 09:33"
        },
        {
          "id": "MDRW2100034488.1.22",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210831 09:33"
        },
        {
          "id": "MDRW2100034488.1.23",
          "form": "저는 미역줄기볶음이요ㅎㅎ 이걸루 맨날 밥먹을 수 있어요",
          "original_form": "저는 미역줄기볶음이요ㅎㅎ 이걸루 맨날 밥먹을 수 있어요",
          "speaker_id": "1",
          "time": "20210901 19:55"
        },
        {
          "id": "MDRW2100034488.1.24",
          "form": "ㅋㅋㅋㅋ 엄청 좋아하시나 보네요 ! 미역국도 좋아하세요?",
          "original_form": "ㅋㅋㅋㅋ 엄청 좋아하시나 보네요 ! 미역국도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 19:58"
        },
        {
          "id": "MDRW2100034488.1.25",
          "form": "미역국도 좋아요 하지만 미역 줄기만의 꼬득꼬득함은 못이겨욧ㅋㅋㅋㅋㅋ",
          "original_form": "미역국도 좋아요 하지만 미역 줄기만의 꼬득꼬득함은 못이겨욧ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:59"
        },
        {
          "id": "MDRW2100034488.1.26",
          "form": "그렇죠ㅋㅋㅋ 고사리나물이나 고구마줄기 같은 것도 좋아하세요?",
          "original_form": "그렇죠ㅋㅋㅋ 고사리나물이나 고구마줄기 같은 것도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 07:54"
        },
        {
          "id": "MDRW2100034488.1.27",
          "form": "네! 좋아해요 어떻게 아셨대",
          "original_form": "네! 좋아해요 어떻게 아셨대",
