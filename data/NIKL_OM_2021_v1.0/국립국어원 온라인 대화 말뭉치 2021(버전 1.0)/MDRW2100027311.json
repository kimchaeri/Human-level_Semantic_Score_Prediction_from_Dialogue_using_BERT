      "utterance": [
        {
          "id": "MDRW2100027311.1.1",
          "form": "전에 같이먹은분도",
          "original_form": "전에 같이먹은분도",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100027311.1.2",
          "form": "영상은 봤는데 정리를 안 함",
          "original_form": "영상은 봤는데 정리를 안 함",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100027311.1.3",
          "form": "맛집많은 곳 가고싶다",
          "original_form": "맛집많은 곳 가고싶다",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100027311.1.4",
          "form": "내껄로 대체해",
          "original_form": "내껄로 대체해",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100027311.1.5",
          "form": "귀찮을때 ㅋㅋㅋ",
          "original_form": "귀찮을때 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100027311.1.6",
          "form": "좋은 생각입니다",
          "original_form": "좋은 생각입니다",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100027311.1.7",
          "form": "텀블러 좋음",
          "original_form": "텀블러 좋음",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100027311.1.8",
          "form": "전에 본 하얀 텀블러",
          "original_form": "전에 본 하얀 텀블러",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100027311.1.9",
          "form": "유채꽃그려져있는거",
          "original_form": "유채꽃그려져있는거",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100027311.1.10",
          "form": "여행지에서 사왔나요",
          "original_form": "여행지에서 사왔나요",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100027311.1.11",
          "form": "그거 제주도 갔을때 삼",
          "original_form": "그거 제주도 갔을때 삼",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100027311.1.12",
          "form": "역시 제주도",
          "original_form": "역시 제주도",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100027311.1.13",
          "form": "매우 만족",
          "original_form": "매우 만족",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100027311.1.14",
          "form": "디자인 존예",
          "original_form": "디자인 존예",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100027311.1.15",
          "form": "성늠도 좋음",
          "original_form": "성늠도 좋음",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100027311.1.16",
          "form": "여행지에서 텀블러 사오는 것도 도움 된다",
          "original_form": "여행지에서 텀블러 사오는 것도 도움 된다",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100027311.1.17",
          "form": "그때제주해녀 볼펜도 샀음",
          "original_form": "그때제주해녀 볼펜도 샀음",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100027311.1.18",
          "form": "해녀 볼 때는 무슨 해녀가 그래서 있나요",
          "original_form": "해녀 볼 때는 무슨 해녀가 그래서 있나요",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100027311.1.19",
          "form": "때타서 어두운걸로 또 사러 가고싶다",
          "original_form": "때타서 어두운걸로 또 사러 가고싶다",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100027311.1.20",
          "form": "해녀가 그려져 있나요",
          "original_form": "해녀가 그려져 있나요",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100027311.1.21",
          "form": "볼펜꼭지에 해녀모형있는거ㄱ0",
          "original_form": "볼펜꼭지에 해녀모형있는거ㄱ0",
          "speaker_id": "2",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100027311.1.22",
          "form": "사왔었음",
          "original_form": "사왔었음",
          "speaker_id": "2",
