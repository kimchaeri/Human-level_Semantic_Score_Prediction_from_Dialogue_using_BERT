        {
          "id": "MDRW2100044747.1.2",
          "form": "파산이겠는데?",
          "original_form": "파산이겠는데?",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044747.1.3",
          "form": "근데 걔가 주장한",
          "original_form": "근데 걔가 주장한",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044747.1.4",
          "form": "무슨 여자 관련 루머가",
          "original_form": "무슨 여자 관련 루머가",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044747.1.5",
          "form": "진짜 입만 턴거였나봐",
          "original_form": "진짜 입만 턴거였나봐",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044747.1.6",
          "form": "증거 제시 못해서 패소로 완전히 기울었다는데",
          "original_form": "증거 제시 못해서 패소로 완전히 기울었다는데",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100044747.1.7",
          "form": "wow...",
          "original_form": "wow...",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044747.1.8",
          "form": "그런데",
          "original_form": "그런데",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044747.1.9",
          "form": "갑질까지 터지니까",
          "original_form": "갑질까지 터지니까",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044747.1.10",
          "form": "옹호론자들마자",
          "original_form": "옹호론자들마자",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044747.1.11",
          "form": "와우 그건아니지",
          "original_form": "와우 그건아니지",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044747.1.12",
          "form": "이러다가",
          "original_form": "이러다가",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044747.1.13",
          "form": "눈물의 ㅁㅁ쇼를 하면서",
          "original_form": "눈물의 ㅁㅁ쇼를 하면서",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044747.1.14",
          "form": "자기 잠정 은퇴한다고?",
          "original_form": "자기 잠정 은퇴한다고?",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044747.1.15",
          "form": "ㅋㅋㅋㅋㅋ아니 이ㅔ 감빵가셔야죠",
          "original_form": "ㅋㅋㅋㅋㅋ아니 이ㅔ 감빵가셔야죠",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044747.1.16",
          "form": "무슨소리하는거야",
          "original_form": "무슨소리하는거야",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044747.1.17",
          "form": "은퇴ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
