      "utterance": [
        {
          "id": "MDRW2100019089.1.1",
          "form": "가고싶은여행지가있으신가요?",
          "original_form": "가고싶은여행지가있으신가요?",
          "speaker_id": "1",
          "time": "20210827 15:11"
        },
        {
          "id": "MDRW2100019089.1.2",
          "form": "저는 제주도요!",
          "original_form": "저는 제주도요!",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019089.1.3",
          "form": "제주도 많이 가보셨겠쥬?",
          "original_form": "제주도 많이 가보셨겠쥬?",
          "speaker_id": "2",
          "time": "20210827 15:13"
        },
        {
          "id": "MDRW2100019089.1.4",
          "form": "많이라기보단근데 가도 여행은아니라ㅜ",
          "original_form": "많이라기보단근데 가도 여행은아니라ㅜ",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019089.1.5",
          "form": "나도 막상 안가본곳이많을듯!",
          "original_form": "나도 막상 안가본곳이많을듯!",
          "speaker_id": "1",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019089.1.6",
          "form": "섭지코지 가보고싶어유",
          "original_form": "섭지코지 가보고싶어유",
          "speaker_id": "2",
          "time": "20210827 15:15"
        },
        {
          "id": "MDRW2100019089.1.7",
          "form": "제주도 맛집 투어도 재밌겠다!!",
          "original_form": "제주도 맛집 투어도 재밌겠다!!",
          "speaker_id": "2",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019089.1.8",
          "form": "뭐한디야",
          "original_form": "뭐한디야",
          "speaker_id": "1",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019089.1.9",
          "form": "전에 동생엄마나이렇게갈땐 경주가 다 짜고 먹을꺼 카페까지 다짜서 난그양감",
          "original_form": "전에 동생엄마나이렇게갈땐 경주가 다 짜고 먹을꺼 카페까지 다짜서 난그양감",
          "speaker_id": "1",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019089.1.10",
          "form": "ㅋㅋ오 경주 너무 좋아",
          "original_form": "ㅋㅋ오 경주 너무 좋아",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019089.1.11",
          "form": "그럼 따라가기만하면되잖아",
          "original_form": "그럼 따라가기만하면되잖아",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019089.1.12",
          "form": "마자 ㅋㅋㅋㅋ걘그런거 좋아하는지는모르겟지맏",
          "original_form": "마자 ㅋㅋㅋㅋ걘그런거 좋아하는지는모르겟지맏",
          "speaker_id": "1",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100019089.1.13",
          "form": "계획세우는걸 좋아하는 사람도 있긴 하더라 ㅋㅋㅋ",
          "original_form": "계획세우는걸 좋아하는 사람도 있긴 하더라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:23"
        },
        {
          "id": "MDRW2100019089.1.14",
          "form": "부럽다 ㅋㅋㅋㅋ 난 못해 ㅠ그래서 여행지갓다돌아와서 후회하는스타일이야",
          "original_form": "부럽다 ㅋㅋㅋㅋ 난 못해 ㅠ그래서 여행지갓다돌아와서 후회하는스타일이야",
          "speaker_id": "1",
          "time": "20210827 15:25"
        },
        {
          "id": "MDRW2100019089.1.15",
          "form": "ㅋㅋㅋ꼼꼼스타일과 함께 가자 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ꼼꼼스타일과 함께 가자 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:25"
        },
        {
          "id": "MDRW2100019089.1.16",
          "form": "너가 그렇게 되도록!",
          "original_form": "너가 그렇게 되도록!",
          "speaker_id": "1",
          "time": "20210827 15:25"
        },
        {
          "id": "MDRW2100019089.1.17",
          "form": "ㅋㅋㅋㅋ저요?",
          "original_form": "ㅋㅋㅋㅋ저요?",
          "speaker_id": "2",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100019089.1.18",
          "form": "그냥 두루뭉술하게 보고 가자 ㅋㅋㅋ",
          "original_form": "그냥 두루뭉술하게 보고 가자 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100019089.1.19",
          "form": "맛집만 알아보고가자 ㅋㅋ로컬맛집으로",
          "original_form": "맛집만 알아보고가자 ㅋㅋ로컬맛집으로",
          "speaker_id": "1",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019089.1.20",
          "form": "여행은.식도락이쥬",
          "original_form": "여행은.식도락이쥬",
          "speaker_id": "1",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019089.1.21",
          "form": "그렇쥬!! 먹는게 남는거쥬!!!!!",
          "original_form": "그렇쥬!! 먹는게 남는거쥬!!!!!",
          "speaker_id": "2",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019089.1.22",
          "form": "여행지에 기억에남은음식은",
          "original_form": "여행지에 기억에남은음식은",
          "speaker_id": "1",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019089.1.23",
          "form": "난 달팽이요리!! 넘 충격적 비주얼이라서 ㅋㅋㅋㅋ",
          "original_form": "난 달팽이요리!! 넘 충격적 비주얼이라서 ㅋㅋㅋㅋ",
