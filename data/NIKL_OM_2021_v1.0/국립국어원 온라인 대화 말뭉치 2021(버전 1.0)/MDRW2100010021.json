      "utterance": [
        {
          "id": "MDRW2100010021.1.1",
          "form": "온라인 커뮤니티 뭐해?",
          "original_form": "온라인 커뮤니티 뭐해?",
          "speaker_id": "1",
          "time": "20210823 13:17"
        },
        {
          "id": "MDRW2100010021.1.2",
          "form": "sns도 포함되는건가?",
          "original_form": "sns도 포함되는건가?",
          "speaker_id": "2",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100010021.1.3",
          "form": "아닝ㅎ",
          "original_form": "아닝ㅎ",
          "speaker_id": "1",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100010021.1.4",
          "form": "그럼 딱히 하는건없는거같아",
          "original_form": "그럼 딱히 하는건없는거같아",
          "speaker_id": "2",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100010021.1.5",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100010021.1.6",
          "form": "그렇구나 난 뽐뿌해",
          "original_form": "그렇구나 난 뽐뿌해",
          "speaker_id": "1",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100010021.1.7",
          "form": "알아?",
          "original_form": "알아?",
          "speaker_id": "1",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100010021.1.8",
          "form": "아니아니 무슨 커뮤니티야?",
          "original_form": "아니아니 무슨 커뮤니티야?",
          "speaker_id": "2",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100010021.1.9",
          "form": "인터넷에 저렴하게 올라오는 상품들이 올라와",
          "original_form": "인터넷에 저렴하게 올라오는 상품들이 올라와",
          "speaker_id": "1",
          "time": "20210823 13:20"
        },
        {
          "id": "MDRW2100010021.1.10",
          "form": "와 신기하다 뭐뭐샀어?",
          "original_form": "와 신기하다 뭐뭐샀어?",
          "speaker_id": "2",
          "time": "20210823 13:31"
        },
        {
          "id": "MDRW2100010021.1.11",
          "form": "어제는 먹을거를 샀엉",
          "original_form": "어제는 먹을거를 샀엉",
          "speaker_id": "1",
          "time": "20210823 13:31"
        },
        {
          "id": "MDRW2100010021.1.12",
          "form": "먹을것도있구나! 우와",
          "original_form": "먹을것도있구나! 우와",
          "speaker_id": "2",
          "time": "20210823 13:40"
        },
        {
          "id": "MDRW2100010021.1.13",
          "form": "응 일상생활에 필요한게 올라왕",
          "original_form": "응 일상생활에 필요한게 올라왕",
          "speaker_id": "1",
          "time": "20210823 14:00"
        },
        {
          "id": "MDRW2100010021.1.14",
          "form": "젤 좋았던건뭐였어?",
          "original_form": "젤 좋았던건뭐였어?",
          "speaker_id": "2",
          "time": "20210823 23:34"
        },
        {
          "id": "MDRW2100010021.1.15",
          "form": "생활용품! 실제 사용 후기도 댓글로 써줘 좋아",
          "original_form": "생활용품! 실제 사용 후기도 댓글로 써줘 좋아",
          "speaker_id": "1",
