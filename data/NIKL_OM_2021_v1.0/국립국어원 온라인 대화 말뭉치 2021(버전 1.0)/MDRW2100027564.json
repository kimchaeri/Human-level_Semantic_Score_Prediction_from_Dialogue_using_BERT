      "utterance": [
        {
          "id": "MDRW2100027564.1.1",
          "form": "설거지 하려는데 고양이가 자꾸 울어",
          "original_form": "설거지 하려는데 고양이가 자꾸 울어",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027564.1.2",
          "form": "귀여워",
          "original_form": "귀여워",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027564.1.3",
          "form": "바로 옆에 있어?",
          "original_form": "바로 옆에 있어?",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027564.1.4",
          "form": "싱크대 기웃기웃거리는 중",
          "original_form": "싱크대 기웃기웃거리는 중",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027564.1.5",
          "form": "귀엽다.",
          "original_form": "귀엽다.",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027564.1.6",
          "form": "님만 보나요",
          "original_form": "님만 보나요",
          "speaker_id": "2",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027564.1.7",
          "form": "님도 보세요",
          "original_form": "님도 보세요",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027564.1.8",
          "form": "애기 또 저리 가서 우네",
          "original_form": "애기 또 저리 가서 우네",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027564.1.9",
          "form": "하............애기",
          "original_form": "하............애기",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027564.1.10",
          "form": "놀아달라고 우는 건가",
          "original_form": "놀아달라고 우는 건가",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027564.1.11",
          "form": "모르겠음...",
          "original_form": "모르겠음...",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027564.1.12",
          "form": "근데 안 놀아준지 좀 돼서",
          "original_form": "근데 안 놀아준지 좀 돼서",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027564.1.13",
          "form": "그런 걸지도",
          "original_form": "그런 걸지도",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027564.1.14",
          "form": "적폐집사",
          "original_form": "적폐집사",
          "speaker_id": "2",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027564.1.15",
          "form": "설거지해야돼",
          "original_form": "설거지해야돼",
          "speaker_id": "1",
          "time": "20210902 13:37"
        },
        {
          "id": "MDRW2100027564.1.16",
          "form": "가서대신놀아주고싶다",
          "original_form": "가서대신놀아주고싶다",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027564.1.17",
          "form": "고영 장난감 잔뜩 사가야지.",
          "original_form": "고영 장난감 잔뜩 사가야지.",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027564.1.18",
          "form": "웃겨... 이제 숨바꼭질 하자네",
          "original_form": "웃겨... 이제 숨바꼭질 하자네",
          "speaker_id": "1",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027564.1.19",
          "form": "ㄱㅇㅇ",
          "original_form": "ㄱㅇㅇ",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027564.1.20",
          "form": "애기...",
          "original_form": "애기...",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027564.1.21",
          "form": "빤히 쳐다보는 거 귀여워...",
          "original_form": "빤히 쳐다보는 거 귀여워...",
