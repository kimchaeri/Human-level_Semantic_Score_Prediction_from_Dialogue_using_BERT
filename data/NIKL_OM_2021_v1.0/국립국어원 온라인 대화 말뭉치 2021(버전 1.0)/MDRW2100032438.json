      "utterance": [
        {
          "id": "MDRW2100032438.1.1",
          "form": "저는 허리가 조금 안 좋아요",
          "original_form": "저는 허리가 조금 안 좋아요",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100032438.1.2",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100032438.1.3",
          "form": "허리가 왜 안좋으실까요",
          "original_form": "허리가 왜 안좋으실까요",
          "speaker_id": "1",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100032438.1.4",
          "form": "앉아서 오랫동안 이래서 그런가 봐요",
          "original_form": "앉아서 오랫동안 이래서 그런가 봐요",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100032438.1.5",
          "form": "보통 직장인들이 허리가 안좋더라구요",
          "original_form": "보통 직장인들이 허리가 안좋더라구요",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032438.1.6",
          "form": "저도 뭐 좋진않네요",
          "original_form": "저도 뭐 좋진않네요",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032438.1.7",
          "form": "어디 안 좋은 데는 없나요",
          "original_form": "어디 안 좋은 데는 없나요",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032438.1.8",
          "form": "저는 목이 좀 불편해요",
          "original_form": "저는 목이 좀 불편해요",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032438.1.9",
          "form": "사고를 당한 적이 있나요",
          "original_form": "사고를 당한 적이 있나요",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032438.1.10",
          "form": "몇년전에 버스 급정거할때 넘어 진 적이 있어요",
          "original_form": "몇년전에 버스 급정거할때 넘어 진 적이 있어요",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032438.1.11",
          "form": "아마 그때 긴장해서 굳었나봐요",
          "original_form": "아마 그때 긴장해서 굳었나봐요",
          "speaker_id": "2",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100032438.1.12",
          "form": "그때 치료를 잘 했어야했는데 말이지요",
          "original_form": "그때 치료를 잘 했어야했는데 말이지요",
          "speaker_id": "1",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100032438.1.13",
          "form": "지금이라도 해 보시는게 어때요",
          "original_form": "지금이라도 해 보시는게 어때요",
          "speaker_id": "2",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100032438.1.14",
          "form": "한의원에 가보아야 겠어요 비올때 뻐근해요",
          "original_form": "한의원에 가보아야 겠어요 비올때 뻐근해요",
          "speaker_id": "1",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100032438.1.15",
          "form": "정기 검진도 한 번씩 받아 봐야 돼요",
          "original_form": "정기 검진도 한 번씩 받아 봐야 돼요",
          "speaker_id": "2",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100032438.1.16",
          "form": "미리 검진해서 확인하는게 가장 낫지요",
          "original_form": "미리 검진해서 확인하는게 가장 낫지요",
          "speaker_id": "1",
          "time": "20210905 22:08"
        }
      ]
