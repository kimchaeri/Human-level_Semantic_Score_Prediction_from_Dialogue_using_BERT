      "utterance": [
        {
          "id": "MDRW2100040698.1.1",
          "form": "계속 그렇게 돈을 들고 다니겠네 걔는",
          "original_form": "계속 그렇게 돈을 들고 다니겠네 걔는",
          "speaker_id": "2",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100040698.1.2",
          "form": "친구들도 이제 계속 사달라고 하니",
          "original_form": "친구들도 이제 계속 사달라고 하니",
          "speaker_id": "2",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100040698.1.3",
          "form": "근데 들어보니 친구도 그냥 자기아들한테 너무주는건 아닌것같다구 일단 카드에 충전식으로 2천원인가 해줬다하대",
          "original_form": "근데 들어보니 친구도 그냥 자기아들한테 너무주는건 아닌것같다구 일단 카드에 충전식으로 2천원인가 해줬다하대",
          "speaker_id": "1",
          "time": "20210905 00:14"
        },
        {
          "id": "MDRW2100040698.1.4",
          "form": "놀이터에서 놀다가 물도 그애는 편의점물 사먹더란다",
          "original_form": "놀이터에서 놀다가 물도 그애는 편의점물 사먹더란다",
          "speaker_id": "1",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100040698.1.5",
          "form": "얼음사고 물사고",
          "original_form": "얼음사고 물사고",
          "speaker_id": "1",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100040698.1.6",
          "form": "어이쿠",
          "original_form": "어이쿠",
          "speaker_id": "2",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100040698.1.7",
          "form": "편의점물 이라니.....",
          "original_form": "편의점물 이라니.....",
          "speaker_id": "2",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100040698.1.8",
          "form": "애들이 아직은 어려서",
          "original_form": "애들이 아직은 어려서",
          "speaker_id": "1",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100040698.1.9",
          "form": "애가 벌써 그렇게 돈을 막 써서 어떡하노",
          "original_form": "애가 벌써 그렇게 돈을 막 써서 어떡하노",
          "speaker_id": "2",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100040698.1.10",
          "form": "돈개념이없잖아",
          "original_form": "돈개념이없잖아",
          "speaker_id": "1",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100040698.1.11",
          "form": "그러니깐",
          "original_form": "그러니깐",
          "speaker_id": "1",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100040698.1.12",
          "form": "그 집 돈 많은가 보네",
          "original_form": "그 집 돈 많은가 보네",
          "speaker_id": "2",
          "time": "20210905 00:15"
        },
        {
          "id": "MDRW2100040698.1.13",
          "form": "그래서 얘기했는데 응",
          "original_form": "그래서 얘기했는데 응",
          "speaker_id": "1",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040698.1.14",
          "form": "그런가보더라",
          "original_form": "그런가보더라",
          "speaker_id": "1",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040698.1.15",
          "form": "아 또 돈이 진짜 많으면 뭐....",
          "original_form": "아 또 돈이 진짜 많으면 뭐....",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040698.1.16",
          "form": "쩝....",
          "original_form": "쩝....",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040698.1.17",
          "form": "돈 많은집인듯.",
          "original_form": "돈 많은집인듯.",
          "speaker_id": "1",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040698.1.18",
          "form": "그렇네...",
          "original_form": "그렇네...",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040698.1.19",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040698.1.20",
          "form": "할말이 없군",
          "original_form": "할말이 없군",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040698.1.21",
          "form": "씀씀이가 다르니",
          "original_form": "씀씀이가 다르니",
          "speaker_id": "2",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040698.1.22",
          "form": "그러이",
          "original_form": "그러이",
          "speaker_id": "1",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040698.1.23",
          "form": "난감하겠더라",
          "original_form": "난감하겠더라",
          "speaker_id": "1",
          "time": "20210905 00:16"
        },
        {
          "id": "MDRW2100040698.1.24",
          "form": "돈많은 집이라하니",
