      "utterance": [
        {
          "id": "MDRW2100040537.1.1",
          "form": "소속사가 나빴네",
          "original_form": "소속사가 나빴네",
          "speaker_id": "2",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100040537.1.2",
          "form": "다음 앨범나오면 사긴할텐데 안타깝고",
          "original_form": "다음 앨범나오면 사긴할텐데 안타깝고",
          "speaker_id": "1",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100040537.1.3",
          "form": "(。ŏ_ŏ)。",
          "original_form": "(。ŏ_ŏ)。",
          "speaker_id": "2",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100040537.1.4",
          "form": "근데 본인은 행복한듯 그럼 된거지",
          "original_form": "근데 본인은 행복한듯 그럼 된거지",
          "speaker_id": "1",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100040537.1.5",
          "form": "그래도 응원해줭",
          "original_form": "그래도 응원해줭",
          "speaker_id": "2",
          "time": "20210904 22:24"
        },
        {
          "id": "MDRW2100040537.1.6",
          "form": "노력의 방향이 잘못된건가",
          "original_form": "노력의 방향이 잘못된건가",
          "speaker_id": "1",
          "time": "20210904 22:25"
        },
        {
          "id": "MDRW2100040537.1.7",
          "form": "곧 끝나는거 아님? 계약?",
          "original_form": "곧 끝나는거 아님? 계약?",
          "speaker_id": "2",
          "time": "20210904 22:25"
        },
        {
          "id": "MDRW2100040537.1.8",
          "form": "찐으로 안타깝다",
          "original_form": "찐으로 안타깝다",
          "speaker_id": "1",
          "time": "20210904 22:25"
        },
        {
          "id": "MDRW2100040537.1.9",
          "form": "ㄷ타임라인이 멈춤",
          "original_form": "ㄷ타임라인이 멈춤",
          "speaker_id": "1",
          "time": "20210904 22:25"
        },
        {
          "id": "MDRW2100040537.1.10",
          "form": "하는게 있는데도?",
          "original_form": "하는게 있는데도?",
          "speaker_id": "2",
          "time": "20210904 22:26"
        },
        {
          "id": "MDRW2100040537.1.11",
          "form": "또는 일상이야기함ㅋㅋ 팬들이ㅋㅋ",
          "original_form": "또는 일상이야기함ㅋㅋ 팬들이ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:26"
        },
        {
          "id": "MDRW2100040537.1.12",
          "form": "저런",
          "original_form": "저런",
          "speaker_id": "2",
          "time": "20210904 22:26"
        },
        {
          "id": "MDRW2100040537.1.13",
          "form": "3년만에 처음",
          "original_form": "3년만에 처음",
          "speaker_id": "1",
          "time": "20210904 22:26"
        },
        {
          "id": "MDRW2100040537.1.14",
          "form": "다른 구독계를 파보자",
          "original_form": "다른 구독계를 파보자",
          "speaker_id": "2",
          "time": "20210904 22:26"
        },
        {
          "id": "MDRW2100040537.1.15",
          "form": "ㅜㅜ 내 애정이...ㅜㅠ",
          "original_form": "ㅜㅜ 내 애정이...ㅜㅠ",
          "speaker_id": "1",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100040537.1.16",
          "form": "그렇게 휴식기도 있는거임",
          "original_form": "그렇게 휴식기도 있는거임",
          "speaker_id": "2",
          "time": "20210904 22:27"
        },
        {
          "id": "MDRW2100040537.1.17",
          "form": "ㅡㅜㅠ 슬픔",
          "original_form": "ㅡㅜㅠ 슬픔",
