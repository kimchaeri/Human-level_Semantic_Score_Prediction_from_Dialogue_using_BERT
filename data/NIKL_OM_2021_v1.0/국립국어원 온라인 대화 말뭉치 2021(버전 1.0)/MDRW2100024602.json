      "utterance": [
        {
          "id": "MDRW2100024602.1.1",
          "form": "비가 많이 오네요",
          "original_form": "비가 많이 오네요",
          "speaker_id": "1",
          "time": "20210831 15:17"
        },
        {
          "id": "MDRW2100024602.1.2",
          "form": "그러게요 이젠 제법 추워서 그만 왔으면 좋겠는데...",
          "original_form": "그러게요 이젠 제법 추워서 그만 왔으면 좋겠는데...",
          "speaker_id": "2",
          "time": "20210831 15:19"
        },
        {
          "id": "MDRW2100024602.1.3",
          "form": "언제까지 오려나 ㅜ",
          "original_form": "언제까지 오려나 ㅜ",
          "speaker_id": "1",
          "time": "20210831 15:19"
        },
        {
          "id": "MDRW2100024602.1.4",
          "form": "해가 그리워요",
          "original_form": "해가 그리워요",
          "speaker_id": "2",
          "time": "20210831 15:20"
        },
        {
          "id": "MDRW2100024602.1.5",
          "form": "언제 더웠나 모르겠네요 ㅎ",
          "original_form": "언제 더웠나 모르겠네요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100024602.1.6",
          "form": "올해 더위 안그래도 심했는데 추위도 최저점 찍게 생겼어요",
          "original_form": "올해 더위 안그래도 심했는데 추위도 최저점 찍게 생겼어요",
          "speaker_id": "2",
          "time": "20210831 15:27"
        },
        {
          "id": "MDRW2100024602.1.7",
          "form": "신발젖을정도로 오는게 싫네요 ㅜ",
          "original_form": "신발젖을정도로 오는게 싫네요 ㅜ",
          "speaker_id": "1",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100024602.1.8",
          "form": "이럴때는 샌들이 편하고 좋은데 샌들 신기가 참 그렇죠",
          "original_form": "이럴때는 샌들이 편하고 좋은데 샌들 신기가 참 그렇죠",
          "speaker_id": "2",
          "time": "20210831 20:56"
        },
        {
          "id": "MDRW2100024602.1.9",
          "form": "맞아요 꿉꿉하니",
          "original_form": "맞아요 꿉꿉하니",
          "speaker_id": "1",
          "time": "20210831 20:58"
        },
        {
          "id": "MDRW2100024602.1.10",
          "form": "나갈 일이 아예 없었으면 좋겠어요",
          "original_form": "나갈 일이 아예 없었으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210831 20:58"
        },
        {
          "id": "MDRW2100024602.1.11",
          "form": "어느계절 좋아하시나요?",
          "original_form": "어느계절 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210831 21:05"
        },
        {
          "id": "MDRW2100024602.1.12",
          "form": "저는 겨울 제일 좋아해요",
          "original_form": "저는 겨울 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210901 13:47"
        },
        {
          "id": "MDRW2100024602.1.13",
          "form": "겨울스포츠하시나요?",
          "original_form": "겨울스포츠하시나요?",
          "speaker_id": "1",
          "time": "20210901 13:58"
        },
        {
          "id": "MDRW2100024602.1.14",
          "form": "스노보드 조금 탔었어요",
          "original_form": "스노보드 조금 탔었어요",
          "speaker_id": "2",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100024602.1.15",
          "form": "추위는 잘 견디시나봐요?",
          "original_form": "추위는 잘 견디시나봐요?",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100024602.1.16",
          "form": "옷만 껴입으면 버틸만하죠ㅋㅋ",
          "original_form": "옷만 껴입으면 버틸만하죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:03"
        },
        {
          "id": "MDRW2100024602.1.17",
          "form": "저는 차라리 여름파라서 ㅎ",
          "original_form": "저는 차라리 여름파라서 ㅎ",
          "speaker_id": "1",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100024602.1.18",
          "form": "추위가 젤싫어요",
          "original_form": "추위가 젤싫어요",
          "speaker_id": "1",
          "time": "20210901 14:05"
        },
        {
          "id": "MDRW2100024602.1.19",
          "form": "여름은 습한게 너무 힘들어요",
          "original_form": "여름은 습한게 너무 힘들어요",
          "speaker_id": "2",
          "time": "20210901 14:06"
        }
      ]
    }
