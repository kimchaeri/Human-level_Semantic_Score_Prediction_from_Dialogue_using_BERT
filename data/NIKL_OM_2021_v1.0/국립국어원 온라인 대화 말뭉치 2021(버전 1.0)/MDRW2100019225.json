      "utterance": [
        {
          "id": "MDRW2100019225.1.1",
          "form": "넷플릭스 보시나요?",
          "original_form": "넷플릭스 보시나요?",
          "speaker_id": "2",
          "time": "20210830 01:07"
        },
        {
          "id": "MDRW2100019225.1.2",
          "form": "가끔 봐요",
          "original_form": "가끔 봐요",
          "speaker_id": "1",
          "time": "20210830 07:12"
        },
        {
          "id": "MDRW2100019225.1.3",
          "form": "자주보시나요?",
          "original_form": "자주보시나요?",
          "speaker_id": "1",
          "time": "20210830 07:12"
        },
        {
          "id": "MDRW2100019225.1.4",
          "form": "전 넷플릭스 안 봐요 ㅎㅎ 웨이브 ㅋㅋ",
          "original_form": "전 넷플릭스 안 봐요 ㅎㅎ 웨이브 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:47"
        },
        {
          "id": "MDRW2100019225.1.5",
          "form": "무슨 컨텐츠 시청하시나요,?",
          "original_form": "무슨 컨텐츠 시청하시나요,?",
          "speaker_id": "1",
          "time": "20210830 15:56"
        },
        {
          "id": "MDRW2100019225.1.6",
          "form": "시간 맞춰 못 보니 드라마나 예능 이요",
          "original_form": "시간 맞춰 못 보니 드라마나 예능 이요",
          "speaker_id": "2",
          "time": "20210830 17:19"
        },
        {
          "id": "MDRW2100019225.1.7",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "1",
          "time": "20210830 18:31"
        },
        {
          "id": "MDRW2100019225.1.8",
          "form": "요즘은 무슨 프로보세요?",
          "original_form": "요즘은 무슨 프로보세요?",
          "speaker_id": "1",
          "time": "20210830 18:31"
        },
        {
          "id": "MDRW2100019225.1.9",
          "form": "나혼자산다 미우새 슬의생이요 ㅎㅎ",
          "original_form": "나혼자산다 미우새 슬의생이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100019225.1.10",
          "form": "아하 제 아내랑 같네요",
          "original_form": "아하 제 아내랑 같네요",
          "speaker_id": "1",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100019225.1.11",
          "form": "제 아내도 나혼자산다랑 슬의생보던데",
          "original_form": "제 아내도 나혼자산다랑 슬의생보던데",
          "speaker_id": "1",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100019225.1.12",
          "form": "저는 유튜브로 맛있는 녀석들 클립보는거 좋아하는데",
          "original_form": "저는 유튜브로 맛있는 녀석들 클립보는거 좋아하는데",
          "speaker_id": "1",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100019225.1.13",
          "form": "먹방 같은거 보세요?",
          "original_form": "먹방 같은거 보세요?",
          "speaker_id": "1",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100019225.1.14",
          "form": "그런건 잘 안 봐요 ㅎㅎㅎ",
          "original_form": "그런건 잘 안 봐요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 19:14"
        },
        {
          "id": "MDRW2100019225.1.15",
          "form": "ㅎㅎ 저는 맛있는 녀석들 먹방만 봐요",
          "original_form": "ㅎㅎ 저는 맛있는 녀석들 먹방만 봐요",
          "speaker_id": "1",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100019225.1.16",
          "form": "넷이 너무 귀여고 케미도 좋아서",
          "original_form": "넷이 너무 귀여고 케미도 좋아서",
          "speaker_id": "1",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100019225.1.17",
          "form": "그런데 최근에 김준현 하차해서 조금 아쉬워요",
          "original_form": "그런데 최근에 김준현 하차해서 조금 아쉬워요",
          "speaker_id": "1",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100019225.1.18",
          "form": "아 왜 하차했어요?",
          "original_form": "아 왜 하차했어요?",
          "speaker_id": "2",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100019225.1.19",
          "form": "글쎄요 그건 잘 모르겠어요",
          "original_form": "글쎄요 그건 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210830 21:45"
        }
      ]
