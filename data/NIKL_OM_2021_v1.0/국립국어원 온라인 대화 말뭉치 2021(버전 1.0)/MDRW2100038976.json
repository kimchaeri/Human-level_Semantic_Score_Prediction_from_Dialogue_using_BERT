      "utterance": [
        {
          "id": "MDRW2100038976.1.1",
          "form": "현장에서 증인신청과 심문을",
          "original_form": "현장에서 증인신청과 심문을",
          "speaker_id": "2",
          "time": "20210902 23:58"
        },
        {
          "id": "MDRW2100038976.1.2",
          "form": "그러게 정말 대단하네 어떻게 알앗지",
          "original_form": "그러게 정말 대단하네 어떻게 알앗지",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100038976.1.3",
          "form": "저기에도 숨은 사연이 있네",
          "original_form": "저기에도 숨은 사연이 있네",
          "speaker_id": "2",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100038976.1.4",
          "form": "그러게 진범이 진술안할까봐 증인신청을 안했다니",
          "original_form": "그러게 진범이 진술안할까봐 증인신청을 안했다니",
          "speaker_id": "1",
          "time": "20210902 23:59"
        },
        {
          "id": "MDRW2100038976.1.5",
          "form": "재판도 심리가 많을듯",
          "original_form": "재판도 심리가 많을듯",
          "speaker_id": "2",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100038976.1.6",
          "form": "응 근데 한문제 풀고 애기를 너무 많이 하네",
          "original_form": "응 근데 한문제 풀고 애기를 너무 많이 하네",
          "speaker_id": "1",
          "time": "20210903 00:00"
        },
        {
          "id": "MDRW2100038976.1.7",
          "form": "그러게 언제 문제를 풀지",
          "original_form": "그러게 언제 문제를 풀지",
          "speaker_id": "2",
          "time": "20210903 00:01"
        },
        {
          "id": "MDRW2100038976.1.8",
          "form": "범죄를 저질르면 편하게 못사네",
          "original_form": "범죄를 저질르면 편하게 못사네",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100038976.1.9",
          "form": "17년이나 흐르고 고백",
          "original_form": "17년이나 흐르고 고백",
          "speaker_id": "2",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100038976.1.10",
          "form": "그러게 억울하게 옥살이하신 분들 많겠다",
          "original_form": "그러게 억울하게 옥살이하신 분들 많겠다",
          "speaker_id": "1",
          "time": "20210903 00:02"
        },
        {
          "id": "MDRW2100038976.1.11",
          "form": "용서의 힘이란다",
          "original_form": "용서의 힘이란다",
          "speaker_id": "2",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100038976.1.12",
          "form": "15억 국가배상한다네 누명으로 옥살이를 했으니",
          "original_form": "15억 국가배상한다네 누명으로 옥살이를 했으니",
          "speaker_id": "1",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100038976.1.13",
          "form": "15억도 적다 17년인데",
          "original_form": "15억도 적다 17년인데",
          "speaker_id": "2",
          "time": "20210903 00:03"
        },
        {
          "id": "MDRW2100038976.1.14",
          "form": "그렇긴 하지 그래서 정신적배상도 청구할꺼라네",
          "original_form": "그렇긴 하지 그래서 정신적배상도 청구할꺼라네",
