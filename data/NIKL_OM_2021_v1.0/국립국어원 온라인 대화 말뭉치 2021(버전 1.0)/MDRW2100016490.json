      "utterance": [
        {
          "id": "MDRW2100016490.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋ걔들은 담음날 봐도 얘기할 애들임 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ걔들은 담음날 봐도 얘기할 애들임 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016490.1.2",
          "form": "이제 빠마는 젱 빡쎈거로 말면 안되는 나이 ㅋㅋㅋㅋㅋ",
          "original_form": "이제 빠마는 젱 빡쎈거로 말면 안되는 나이 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016490.1.3",
          "form": "우리 첫찌도 이제 내가 하지말라캐여 ㅋㅋㅋ",
          "original_form": "우리 첫찌도 이제 내가 하지말라캐여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:13"
        },
        {
          "id": "MDRW2100016490.1.4",
          "form": "아 니가 파마 직접 하지 말라고??",
          "original_form": "아 니가 파마 직접 하지 말라고??",
          "speaker_id": "1",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100016490.1.5",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:45"
        },
        {
          "id": "MDRW2100016490.1.6",
          "form": "그래 그만클 말아줬음 됐다",
          "original_form": "그래 그만클 말아줬음 됐다",
          "speaker_id": "1",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100016490.1.7",
          "form": "우리 name3 계속 머리 풀어달라고 ㅋㅋㅋㄱ 멋 부릴 나이 ㅋㅋ",
          "original_form": "우리 &name3& 계속 머리 풀어달라고 ㅋㅋㅋㄱ 멋 부릴 나이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100016490.1.8",
          "form": "내가하면 아줌마파마되니깐 ㅋㅋㅋㅋ이젠 지도 자연스러움을 추구해여 ㅋㅋㅋㅋ",
          "original_form": "내가하면 아줌마파마되니깐 ㅋㅋㅋㅋ이젠 지도 자연스러움을 추구해여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100016490.1.9",
          "form": "밑에만 자연스러운웨이브 ㅋㅋㅋㅋ",
          "original_form": "밑에만 자연스러운웨이브 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100016490.1.10",
          "form": "방학시작할때 했었음 지금쯤 이쁠껀데~",
          "original_form": "방학시작할때 했었음 지금쯤 이쁠껀데~",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016490.1.11",
          "form": "아빠가 욕심이 과했음 ㅋㅋ",
          "original_form": "아빠가 욕심이 과했음 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016490.1.12",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016490.1.13",
          "form": "풀릴까봐",
          "original_form": "풀릴까봐",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016490.1.14",
          "form": "언니네 name3은 생머리라 금방 풀릴꺼같은데~",
          "original_form": "언니네 &name3&은 생머리라 금방 풀릴꺼같은데~",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016490.1.15",
          "form": "애를 백종원을 만들어놓음",
          "original_form": "애를 백종원을 만들어놓음",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016490.1.16",
          "form": "3일정도만 지남 자연스러워 질텐데여 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "3일정도만 지남 자연스러워 질텐데여 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016490.1.17",
          "form": "어 안그래도 아저씨가 그래서 더 곱슬하게 말았어",
          "original_form": "어 안그래도 아저씨가 그래서 더 곱슬하게 말았어",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016490.1.18",
          "form": "그래서 일주일만 참으라고",
          "original_form": "그래서 일주일만 참으라고",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016490.1.19",
          "form": "아 백종원 빵터진다 진짜 ㅋㅋㅋ",
          "original_form": "아 백종원 빵터진다 진짜 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016490.1.20",
          "form": "이수도 오늘 머리 파마 한다고",
          "original_form": "이수도 오늘 머리 파마 한다고",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016490.1.21",
          "form": "원 보고 내일 되면 name4도 머리 똑같아 질 테니 피어 돼가지고",
          "original_form": "원 보고 내일 되면 &name4&도 머리 똑같아 질 테니 피어 돼가지고",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016490.1.22",
          "form": "얼굴뽀얘가 이쁠꺼같은데 ㅋㅋㅋㅋ",
          "original_form": "얼굴뽀얘가 이쁠꺼같은데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016490.1.23",
          "form": "원 보고 내일 되면 name4도 머리 똑같아 질 테니",
          "original_form": "원 보고 내일 되면 &name4&도 머리 똑같아 질 테니",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016490.1.24",
          "form": "하루만 혼자 다니라고",
          "original_form": "하루만 혼자 다니라고",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016490.1.25",
          "form": "내일은 곱슬하게 둘이 다니면 된다고",
          "original_form": "내일은 곱슬하게 둘이 다니면 된다고",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016490.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 빵터진다 진짜 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 빵터진다 진짜 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016490.1.27",
          "form": "해놨더니 막 웃음",
          "original_form": "해놨더니 막 웃음",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016490.1.28",
          "form": "지도 지머리가 웃긴 걸 아는 거지",
          "original_form": "지도 지머리가 웃긴 걸 아는 거지",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016490.1.29",
          "form": "name4랑은 진짜 영혼의 단짝이다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "&name4&랑은 진짜 영혼의 단짝이다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016490.1.30",
          "form": "그니까 꼽 슬머리도 같이 함",
          "original_form": "그니까 꼽 슬머리도 같이 함",
          "speaker_id": "1",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100016490.1.31",
          "form": "name4언니한테 머리 얘기했더니 안그래도 name4도 자꾸 풀려서",
          "original_form": "&name4&언니한테 머리 얘기했더니 안그래도 &name4&도 자꾸 풀려서",
          "speaker_id": "1",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100016490.1.32",
          "form": "내 지금 은행서 혼자 낄낄거리고있어요 ㅋㅋㅋㅋㅋㅋ아 배아파 ㅋㅋㅋㅋ",
          "original_form": "내 지금 은행서 혼자 낄낄거리고있어요 ㅋㅋㅋㅋㅋㅋ아 배아파 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:49"
        },
        {
          "id": "MDRW2100016490.1.33",
