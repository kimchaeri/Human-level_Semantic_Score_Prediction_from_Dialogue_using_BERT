      "utterance": [
        {
          "id": "MDRW2100024636.1.1",
          "form": "오늘은 연달아서 나의 여행연대기3탄까지 해줄게",
          "original_form": "오늘은 연달아서 나의 여행연대기3탄까지 해줄게",
          "speaker_id": "2",
          "time": "20210831 16:25"
        },
        {
          "id": "MDRW2100024636.1.2",
          "form": "유학은 얼마나 갔다온거야?",
          "original_form": "유학은 얼마나 갔다온거야?",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100024636.1.3",
          "form": "2년밖에 안 있었엉ㅋㅋ",
          "original_form": "2년밖에 안 있었엉ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100024636.1.4",
          "form": "2년 동안 디자인스쿨 다닌거?",
          "original_form": "2년 동안 디자인스쿨 다닌거?",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100024636.1.5",
          "form": "아니 6개월 어학원 다니고 6개월 놀고 1년 디자인스쿨 다님",
          "original_form": "아니 6개월 어학원 다니고 6개월 놀고 1년 디자인스쿨 다님",
          "speaker_id": "2",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100024636.1.6",
          "form": "6개월동안 여행 다녔어?",
          "original_form": "6개월동안 여행 다녔어?",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100024636.1.7",
          "form": "아니... 딱히 여행보다는 스위스 아웃렛 가고",
          "original_form": "아니... 딱히 여행보다는 스위스 아웃렛 가고",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100024636.1.8",
          "form": "어디지 섬 다녀오고",
          "original_form": "어디지 섬 다녀오고",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100024636.1.9",
          "form": "베네치아 다녀오고",
          "original_form": "베네치아 다녀오고",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100024636.1.10",
          "form": "뭐 많이했네 좋았겠다",
          "original_form": "뭐 많이했네 좋았겠다",
          "speaker_id": "1",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100024636.1.11",
          "form": "무슨 섬 갔을때가 제일 좋았어",
          "original_form": "무슨 섬 갔을때가 제일 좋았어",
          "speaker_id": "2",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100024636.1.12",
          "form": "절벽에 레스토랑에서 식사하고",
          "original_form": "절벽에 레스토랑에서 식사하고",
          "speaker_id": "2",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100024636.1.13",
          "form": "아 나 거기 어딘지 알것같은데",
          "original_form": "아 나 거기 어딘지 알것같은데",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100024636.1.14",
          "form": "그치? 근데 오래되서 기억은 잘 안남ㅠ",
          "original_form": "그치? 근데 오래되서 기억은 잘 안남ㅠ",
          "speaker_id": "2",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100024636.1.15",
          "form": "그래도 기억은 다 하고있나보네",
          "original_form": "그래도 기억은 다 하고있나보네",
          "speaker_id": "1",
          "time": "20210908 01:31"
        },
        {
          "id": "MDRW2100024636.1.16",
          "form": "바다랑 경치 뭐 그런거는 다 기억에 있지!!",
          "original_form": "바다랑 경치 뭐 그런거는 다 기억에 있지!!",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100024636.1.17",
          "form": "베네치아는 어땠어?",
          "original_form": "베네치아는 어땠어?",
          "speaker_id": "1",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100024636.1.18",
          "form": "베네치아 좋은데 습하고 비둘기 많고ㅋㅋ",
          "original_form": "베네치아 좋은데 습하고 비둘기 많고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:32"
        },
        {
          "id": "MDRW2100024636.1.19",
          "form": "비둘기? 그런데 비둘기가있어?",
          "original_form": "비둘기? 그런데 비둘기가있어?",
          "speaker_id": "1",
          "time": "20210908 01:33"
        },
        {
          "id": "MDRW2100024636.1.20",
          "form": "응 신기하게 광장에 비둘기 천지더라!!",
          "original_form": "응 신기하게 광장에 비둘기 천지더라!!",
          "speaker_id": "2",
          "time": "20210908 01:33"
        }
      ]
