      "utterance": [
        {
          "id": "MDRW2100001941.1.1",
          "form": "티켓팅 할때 정말 긴장되더라",
          "original_form": "티켓팅 할때 정말 긴장되더라",
          "speaker_id": "1",
          "time": "20210819 22:23"
        },
        {
          "id": "MDRW2100001941.1.2",
          "form": "마우스 광클 미친듯이 해",
          "original_form": "마우스 광클 미친듯이 해",
          "speaker_id": "1",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100001941.1.3",
          "form": "진짜 근데 이선좌 한 3번 떴으면 그 티켓팅은 망한거야.",
          "original_form": "진짜 근데 이선좌 한 3번 떴으면 그 티켓팅은 망한거야.",
          "speaker_id": "2",
          "time": "20210819 22:25"
        },
        {
          "id": "MDRW2100001941.1.4",
          "form": "가망이 없어",
          "original_form": "가망이 없어",
          "speaker_id": "2",
          "time": "20210819 22:25"
        },
        {
          "id": "MDRW2100001941.1.5",
          "form": "맞아 맞아 공감돼",
          "original_form": "맞아 맞아 공감돼",
          "speaker_id": "1",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001941.1.6",
          "form": "바로 취소표 생각나더라",
          "original_form": "바로 취소표 생각나더라",
          "speaker_id": "1",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001941.1.7",
          "form": "취소표 뜨는 시간에 대기 타잖아",
          "original_form": "취소표 뜨는 시간에 대기 타잖아",
          "speaker_id": "1",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001941.1.8",
          "form": "응응 그게 사이트마다 달랐던거 같은데 새벽두시였나? 친구 표 잡으러 같이 광클했던 기억이나네",
          "original_form": "응응 그게 사이트마다 달랐던거 같은데 새벽두시였나? 친구 표 잡으러 같이 광클했던 기억이나네",
          "speaker_id": "2",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001941.1.9",
          "form": "콘서트는 어때? 취소표가 풀려?",
          "original_form": "콘서트는 어때? 취소표가 풀려?",
          "speaker_id": "2",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001941.1.10",
          "form": "보통 인터파크에서 많이 하지 않아?",
          "original_form": "보통 인터파크에서 많이 하지 않아?",
          "speaker_id": "1",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001941.1.11",
          "form": "뮤지컬 티켓 잡아줄때 인터파크에서 많이 했었어",
          "original_form": "뮤지컬 티켓 잡아줄때 인터파크에서 많이 했었어",
          "speaker_id": "1",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001941.1.12",
          "form": "어우 취소표도 하늘의 별따기야",
          "original_form": "어우 취소표도 하늘의 별따기야",
          "speaker_id": "1",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001941.1.13",
          "form": "새벽두시는 아마 예사였던것 같아.",
          "original_form": "새벽두시는 아마 예사였던것 같아.",
          "speaker_id": "2",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001941.1.14",
          "form": "예사도 티켓팅 자주 하더라",
          "original_form": "예사도 티켓팅 자주 하더라",
          "speaker_id": "1",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001941.1.15",
          "form": "인터파크는 두시 반 이후",
          "original_form": "인터파크는 두시 반 이후",
          "speaker_id": "1",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001941.1.16",
          "form": "콘서트는 취소 하는 사람이 별로 없어서 더 어렵나?",
          "original_form": "콘서트는 취소 하는 사람이 별로 없어서 더 어렵나?",
          "speaker_id": "2",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001941.1.17",
          "form": "근데 그것도 정시에 하는건 아니더라",
          "original_form": "근데 그것도 정시에 하는건 아니더라",
          "speaker_id": "1",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001941.1.18",
          "form": "응 어렵고 매크로가 휩쓸어",
          "original_form": "응 어렵고 매크로가 휩쓸어",
          "speaker_id": "1",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001941.1.19",
          "form": "컴퓨터 잘 되는 pc방 가야해",
          "original_form": "컴퓨터 잘 되는 pc방 가야해",
          "speaker_id": "1",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100001941.1.20",
          "form": "아 맞아 그 시간 즈음~에 하니까 새로고침 하면서 기다리지",
          "original_form": "아 맞아 그 시간 즈음~에 하니까 새로고침 하면서 기다리지",
          "speaker_id": "2",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100001941.1.21",
          "form": "와우...근데 매크로 업자가 파는게 팔려???",
          "original_form": "와우...근데 매크로 업자가 파는게 팔려???",
          "speaker_id": "2",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100001941.1.22",
          "form": "보통 플미 붙이잖아.",
          "original_form": "보통 플미 붙이잖아.",
          "speaker_id": "2",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100001941.1.23",
          "form": "취소표도 업자가 쓸어가는걸로 알아",
          "original_form": "취소표도 업자가 쓸어가는걸로 알아",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001941.1.24",
          "form": "그걸 뚫어야지",
          "original_form": "그걸 뚫어야지",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001941.1.25",
          "form": "진짜 슬퍼",
          "original_form": "진짜 슬퍼",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001941.1.26",
          "form": "헐....근데 플미 붙인 표, 신고하면 표 취소 안 돼?",
          "original_form": "헐....근데 플미 붙인 표, 신고하면 표 취소 안 돼?",
          "speaker_id": "2",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100001941.1.27",
          "form": "프리미엄 붙은 티켓 잡긴 어려울걸",
          "original_form": "프리미엄 붙은 티켓 잡긴 어려울걸",
          "speaker_id": "1",
          "time": "20210819 22:36"
        },
        {
          "id": "MDRW2100001941.1.28",
          "form": "물론 트위터같은 sns에서 잡는 사람도 있더라",
          "original_form": "물론 트위터같은 sns에서 잡는 사람도 있더라",
          "speaker_id": "1",
          "time": "20210819 22:37"
        },
        {
          "id": "MDRW2100001941.1.29",
          "form": "뮤지컬 티켓팅도 앞자리나 원하는 좋은 자리는 술수 썼을걸",
          "original_form": "뮤지컬 티켓팅도 앞자리나 원하는 좋은 자리는 술수 썼을걸",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001941.1.30",
          "form": "근데 요즘은 관리를 많이 하긴 하더라 공지도 내고",
          "original_form": "근데 요즘은 관리를 많이 하긴 하더라 공지도 내고",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001941.1.31",
          "form": "헐....그랬을까? 매크로일까?",
          "original_form": "헐....그랬을까? 매크로일까?",
          "speaker_id": "2",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001941.1.32",
          "form": "그렇지 않을까",
          "original_form": "그렇지 않을까",
          "speaker_id": "1",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001941.1.33",
          "form": "근데 옛날에 인디밴드계열 공연도 좀 다녔는데 맨날 유명 찍사분이 맨앞열 가있으시긴 하더라.",
          "original_form": "근데 옛날에 인디밴드계열 공연도 좀 다녔는데 맨날 유명 찍사분이 맨앞열 가있으시긴 하더라.",
          "speaker_id": "2",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001941.1.34",
          "form": "아무리 신의 손이라도 맨 앞자리는 무리야",
          "original_form": "아무리 신의 손이라도 맨 앞자리는 무리야",
          "speaker_id": "1",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001941.1.35",
          "form": "티켓팅 꽤 빡셌는데도 매번 맨 앞자리셔서 놀라워했는데",
          "original_form": "티켓팅 꽤 빡셌는데도 매번 맨 앞자리셔서 놀라워했는데",
          "speaker_id": "2",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001941.1.36",
          "form": "슬프다 다들 그러고 있었을까...",
          "original_form": "슬프다 다들 그러고 있었을까...",
          "speaker_id": "2",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001941.1.37",
          "form": "따로 플미 티켓을 사시거나 매크로 쓰지 않을까?",
          "original_form": "따로 플미 티켓을 사시거나 매크로 쓰지 않을까?",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001941.1.38",
          "form": "그래도 그냥 아니라고 믿을래 어차피 내 인생에 맨앞자리는 없을 것 같으니까...ㅠㅠ",
          "original_form": "그래도 그냥 아니라고 믿을래 어차피 내 인생에 맨앞자리는 없을 것 같으니까...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001941.1.39",
          "form": "근데 서버 빠른 컴퓨터는 매크로도 이겨내",
          "original_form": "근데 서버 빠른 컴퓨터는 매크로도 이겨내",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001941.1.40",
          "form": "매크로는 서버 먹통이면 구동 불가거든",
          "original_form": "매크로는 서버 먹통이면 구동 불가거든",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001941.1.41",
          "form": "컴퓨터 프로그램 이론상이지만...!",
          "original_form": "컴퓨터 프로그램 이론상이지만...!",
          "speaker_id": "1",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001941.1.42",
          "form": "난 그 현장에 갈 수 있는 걸로 족해",
          "original_form": "난 그 현장에 갈 수 있는 걸로 족해",
          "speaker_id": "1",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001941.1.43",
          "form": "나두나두. 그저 내가 앉을 자리 하나 있는게 정말 다행이다~",
          "original_form": "나두나두. 그저 내가 앉을 자리 하나 있는게 정말 다행이다~",
          "speaker_id": "2",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001941.1.44",
          "form": "내가 못 간 공연이 레전드일수 있으니깐 ㅋㅋㅋㅋ",
