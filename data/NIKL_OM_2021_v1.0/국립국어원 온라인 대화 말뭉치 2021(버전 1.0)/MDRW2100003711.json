      "utterance": [
        {
          "id": "MDRW2100003711.1.1",
          "form": "주말인데 밀린 집안일 하셨나용ㅎㅎ",
          "original_form": "주말인데 밀린 집안일 하셨나용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:06"
        },
        {
          "id": "MDRW2100003711.1.2",
          "form": "아니요ㅎㅎ 전 전업이라ㅋ주말평일 구분이 없어요ㅎ",
          "original_form": "아니요ㅎㅎ 전 전업이라ㅋ주말평일 구분이 없어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 11:08"
        },
        {
          "id": "MDRW2100003711.1.3",
          "form": "아! ㅋㅋㅠ 이미 다 끝내셨겠네요",
          "original_form": "아! ㅋㅋㅠ 이미 다 끝내셨겠네요",
          "speaker_id": "1",
          "time": "20210821 11:11"
        },
        {
          "id": "MDRW2100003711.1.4",
          "form": "전 이제 일어나서 해야하는데",
          "original_form": "전 이제 일어나서 해야하는데",
          "speaker_id": "1",
          "time": "20210821 11:11"
        },
        {
          "id": "MDRW2100003711.1.5",
          "form": "대체 집안일은 왜 해도해도 끝이없는지",
          "original_form": "대체 집안일은 왜 해도해도 끝이없는지",
          "speaker_id": "1",
          "time": "20210821 11:11"
        },
        {
          "id": "MDRW2100003711.1.6",
          "form": "맞아요ㅡㅠ끝이없는게 집안일같아요",
          "original_form": "맞아요ㅡㅠ끝이없는게 집안일같아요",
          "speaker_id": "2",
          "time": "20210821 11:21"
        },
        {
          "id": "MDRW2100003711.1.7",
          "form": "밀대 밀어야지 생각만 해놓고 30분째 누워있어요",
          "original_form": "밀대 밀어야지 생각만 해놓고 30분째 누워있어요",
          "speaker_id": "1",
          "time": "20210821 11:21"
        },
        {
          "id": "MDRW2100003711.1.8",
          "form": "밀대 밀고 청소기 돌리거든요ㅠ",
          "original_form": "밀대 밀고 청소기 돌리거든요ㅠ",
          "speaker_id": "1",
          "time": "20210821 11:22"
        },
        {
          "id": "MDRW2100003711.1.9",
          "form": "일단 주말이니 쉬고ㅎㅎ",
          "original_form": "일단 주말이니 쉬고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 11:28"
        },
        {
          "id": "MDRW2100003711.1.10",
          "form": "ㅋㅋ쉴만큼 쉬고 12시 지나서 해야죠ㅜㅜ",
          "original_form": "ㅋㅋ쉴만큼 쉬고 12시 지나서 해야죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 11:28"
        },
        {
          "id": "MDRW2100003711.1.11",
          "form": "몰아서 하면 그나마 좀 낫더라구요",
          "original_form": "몰아서 하면 그나마 좀 낫더라구요",
          "speaker_id": "1",
          "time": "20210821 11:29"
        },
        {
          "id": "MDRW2100003711.1.12",
          "form": "설거지빼고",
          "original_form": "설거지빼고",
          "speaker_id": "1",
          "time": "20210821 11:29"
        },
        {
          "id": "MDRW2100003711.1.13",
          "form": "맞아요~  저도 몰아서 하는 스타일이에요ㅋㅋ",
          "original_form": "맞아요~  저도 몰아서 하는 스타일이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 11:32"
        },
        {
          "id": "MDRW2100003711.1.14",
          "form": "전업이신분도 똑같군요ㅋㅋㅋㅋ",
          "original_form": "전업이신분도 똑같군요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 11:32"
        },
        {
          "id": "MDRW2100003711.1.15",
          "form": "제일 귀찮은게 방 닦는거예요",
          "original_form": "제일 귀찮은게 방 닦는거예요",
          "speaker_id": "1",
          "time": "20210821 11:33"
        },
        {
          "id": "MDRW2100003711.1.16",
          "form": "무선물청소기 쓰는데도 귀찮네요",
          "original_form": "무선물청소기 쓰는데도 귀찮네요",
          "speaker_id": "1",
          "time": "20210821 11:33"
        },
        {
          "id": "MDRW2100003711.1.17",
          "form": "저도 그건 신랑시켜요ㅋ",
          "original_form": "저도 그건 신랑시켜요ㅋ",
          "speaker_id": "2",
          "time": "20210821 11:33"
        },
        {
          "id": "MDRW2100003711.1.18",
          "form": "주중에는 밀기만",
          "original_form": "주중에는 밀기만",
          "speaker_id": "2",
          "time": "20210821 11:33"
        },
        {
          "id": "MDRW2100003711.1.19",
          "form": "저도 누가 대신해줬으면 좋겠어요ㅜㅜ",
          "original_form": "저도 누가 대신해줬으면 좋겠어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003711.1.20",
          "form": "로봇청소기 들여도 별로라서",
          "original_form": "로봇청소기 들여도 별로라서",
          "speaker_id": "1",
          "time": "20210821 11:35"
        },
        {
          "id": "MDRW2100003711.1.21",
          "form": "결국 제가 다시하게 되더라구요",
          "original_form": "결국 제가 다시하게 되더라구요",
          "speaker_id": "1",
          "time": "20210821 11:35"
        }
      ]
