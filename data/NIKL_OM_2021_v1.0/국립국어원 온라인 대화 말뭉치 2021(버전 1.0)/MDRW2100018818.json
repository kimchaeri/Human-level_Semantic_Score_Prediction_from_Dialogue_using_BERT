      "utterance": [
        {
          "id": "MDRW2100018818.1.1",
          "form": "누님은 알바 뭐해봤어요",
          "original_form": "누님은 알바 뭐해봤어요",
          "speaker_id": "2",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018818.1.2",
          "form": "화가 날 때도 그런 효과가 있나요...",
          "original_form": "화가 날 때도 그런 효과가 있나요...",
          "speaker_id": "1",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018818.1.3",
          "form": "알바보다는...",
          "original_form": "알바보다는...",
          "speaker_id": "1",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018818.1.4",
          "form": "알바는 해본적이 없어요 그러고보니",
          "original_form": "알바는 해본적이 없어요 그러고보니",
          "speaker_id": "1",
          "time": "20210827 12:28"
        },
        {
          "id": "MDRW2100018818.1.5",
          "form": "봉사를 했지...",
          "original_form": "봉사를 했지...",
          "speaker_id": "1",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018818.1.6",
          "form": "업무얘기 끝나면 봉사썰 풀어야겠다",
          "original_form": "업무얘기 끝나면 봉사썰 풀어야겠다",
          "speaker_id": "1",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018818.1.7",
          "form": "무급으로 혹사당하셨나보네요",
          "original_form": "무급으로 혹사당하셨나보네요",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018818.1.8",
          "form": "저는 알바는 진짜 많이해봤어요",
          "original_form": "저는 알바는 진짜 많이해봤어요",
          "speaker_id": "2",
          "time": "20210827 12:29"
        },
        {
          "id": "MDRW2100018818.1.9",
          "form": "아뇨 무급은 아닌데.. ㅋㅋㅋ",
          "original_form": "아뇨 무급은 아닌데.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018818.1.10",
          "form": "어떤거 해봤어요?",
          "original_form": "어떤거 해봤어요?",
          "speaker_id": "1",
          "time": "20210827 12:30"
        },
        {
          "id": "MDRW2100018818.1.11",
          "form": "제일 윗 글을 못봤네. 뭐 화나는 일 있어요?",
          "original_form": "제일 윗 글을 못봤네. 뭐 화나는 일 있어요?",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018818.1.12",
          "form": "편의점 택배 포장 설거지 홀서빙 카페 독서실 스터디카페 청소",
          "original_form": "편의점 택배 포장 설거지 홀서빙 카페 독서실 스터디카페 청소",
          "speaker_id": "2",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018818.1.13",
          "form": "또 뭐 있을텐데 기억이 안나네여",
          "original_form": "또 뭐 있을텐데 기억이 안나네여",
          "speaker_id": "2",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018818.1.14",
          "form": "일할 때는 화나는 일이 꽤 생기죠ㅋㅋㅋㅋㅋ",
          "original_form": "일할 때는 화나는 일이 꽤 생기죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018818.1.15",
          "form": "우와...어떤게 제일 낫어요?",
          "original_form": "우와...어떤게 제일 낫어요?",
          "speaker_id": "1",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018818.1.16",
          "form": "역시 쏘세지가 나오는 홀서빙?",
          "original_form": "역시 쏘세지가 나오는 홀서빙?",
          "speaker_id": "1",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018818.1.17",
          "form": "쏘세지는 홀서빙 못해봤어요ㅋㅋㅋㅋ",
          "original_form": "쏘세지는 홀서빙 못해봤어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018818.1.18",
          "form": "쏘세지는 설거지",
          "original_form": "쏘세지는 설거지",
          "speaker_id": "2",
          "time": "20210827 12:40"
        },
        {
          "id": "MDRW2100018818.1.19",
          "form": "아하...",
          "original_form": "아하...",
          "speaker_id": "1",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100018818.1.20",
          "form": "포장알바가 좋았죠",
          "original_form": "포장알바가 좋았죠",
          "speaker_id": "2",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100018818.1.21",
          "form": "여성 쇼핑몰 포장인데",
          "original_form": "여성 쇼핑몰 포장인데",
          "speaker_id": "2",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100018818.1.22",
          "form": "ㅋㅋㅋㅋ쏠쏠했나요?",
          "original_form": "ㅋㅋㅋㅋ쏠쏠했나요?",
          "speaker_id": "1",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100018818.1.23",
          "form": "그냥 단순반복이었는데",
          "original_form": "그냥 단순반복이었는데",
          "speaker_id": "2",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100018818.1.24",
          "form": "실장님 이하 직원들이 전부 미인이셨...",
          "original_form": "실장님 이하 직원들이 전부 미인이셨...",
          "speaker_id": "2",
          "time": "20210827 12:41"
        },
        {
          "id": "MDRW2100018818.1.25",
          "form": "어허허",
          "original_form": "어허허",
          "speaker_id": "2",
          "time": "20210827 12:42"
        },
        {
          "id": "MDRW2100018818.1.26",
          "form": "엇...저 답장 안갔나요 ?",
          "original_form": "엇...저 답장 안갔나요 ?",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100018818.1.27",
          "form": "청일점얘기했는데..?",
          "original_form": "청일점얘기했는데..?",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100018818.1.28",
          "form": "안왔어요!",
          "original_form": "안왔어요!",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100018818.1.29",
          "form": "청일점이라 온갖 고된일은 다했지만",
          "original_form": "청일점이라 온갖 고된일은 다했지만",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100018818.1.30",
          "form": "모델 누님들이 엄청 이쁘셔서 견뎌냈죠 허허",
          "original_form": "모델 누님들이 엄청 이쁘셔서 견뎌냈죠 허허",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100018818.1.31",
          "form": "ㅜㅜ 톡 보냈는데도 없어지는 경우가 꽤 되네요",
          "original_form": "ㅜㅜ 톡 보냈는데도 없어지는 경우가 꽤 되네요",
          "speaker_id": "1",
          "time": "20210827 23:35"
        }
      ]
