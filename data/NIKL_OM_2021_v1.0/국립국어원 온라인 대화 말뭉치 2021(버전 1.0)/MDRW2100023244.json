      "utterance": [
        {
          "id": "MDRW2100023244.1.1",
          "form": "그리고 지금",
          "original_form": "그리고 지금",
          "speaker_id": "1",
          "time": "20210829 23:16"
        },
        {
          "id": "MDRW2100023244.1.2",
          "form": "한밤중에 깨서",
          "original_form": "한밤중에 깨서",
          "speaker_id": "1",
          "time": "20210829 23:16"
        },
        {
          "id": "MDRW2100023244.1.3",
          "form": "지금 당장 악신을 만나야겠다고;;;",
          "original_form": "지금 당장 악신을 만나야겠다고;;;",
          "speaker_id": "1",
          "time": "20210829 23:16"
        },
        {
          "id": "MDRW2100023244.1.4",
          "form": "아니 직장인이라며.....",
          "original_form": "아니 직장인이라며.....",
          "speaker_id": "1",
          "time": "20210829 23:16"
        },
        {
          "id": "MDRW2100023244.1.5",
          "form": "왜???",
          "original_form": "왜???",
          "speaker_id": "2",
          "time": "20210829 23:16"
        },
        {
          "id": "MDRW2100023244.1.6",
          "form": "아니 진짜",
          "original_form": "아니 진짜",
          "speaker_id": "2",
          "time": "20210829 23:16"
        },
        {
          "id": "MDRW2100023244.1.7",
          "form": "몰라 이유는 안알려줌",
          "original_form": "몰라 이유는 안알려줌",
          "speaker_id": "1",
          "time": "20210829 23:16"
        },
        {
          "id": "MDRW2100023244.1.8",
          "form": "여주한테 정 안붙는게",
          "original_form": "여주한테 정 안붙는게",
          "speaker_id": "2",
          "time": "20210829 23:16"
        },
        {
          "id": "MDRW2100023244.1.9",
          "form": "남주들은 커뮤거든?",
          "original_form": "남주들은 커뮤거든?",
          "speaker_id": "2",
          "time": "20210829 23:16"
        },
        {
          "id": "MDRW2100023244.1.10",
          "form": "여주는 걍 거기 맞춰서",
          "original_form": "여주는 걍 거기 맞춰서",
          "speaker_id": "2",
          "time": "20210829 23:16"
        },
        {
          "id": "MDRW2100023244.1.11",
          "form": "적당히 끼워만든 티나...",
          "original_form": "적당히 끼워만든 티나...",
          "speaker_id": "2",
          "time": "20210829 23:17"
        },
        {
          "id": "MDRW2100023244.1.12",
          "form": "ㅇㅇ 여주 캐릭터리티가",
          "original_form": "ㅇㅇ 여주 캐릭터리티가",
          "speaker_id": "1",
          "time": "20210829 23:17"
        },
        {
          "id": "MDRW2100023244.1.13",
          "form": "이랬다가 저랬다가 하니까",
          "original_form": "이랬다가 저랬다가 하니까",
          "speaker_id": "1",
          "time": "20210829 23:17"
        },
        {
          "id": "MDRW2100023244.1.14",
          "form": "뭐... 어쩌란건지...",
          "original_form": "뭐... 어쩌란건지...",
          "speaker_id": "1",
          "time": "20210829 23:17"
        },
        {
          "id": "MDRW2100023244.1.15",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210829 23:17"
        },
        {
          "id": "MDRW2100023244.1.16",
          "form": "그런 결심을 하게 하려면",
          "original_form": "그런 결심을 하게 하려면",
          "speaker_id": "2",
          "time": "20210829 23:17"
        },
        {
          "id": "MDRW2100023244.1.17",
          "form": "자기 설정값때문에 고통스러워하는 애가 있거나",
          "original_form": "자기 설정값때문에 고통스러워하는 애가 있거나",
          "speaker_id": "2",
          "time": "20210829 23:18"
        },
        {
          "id": "MDRW2100023244.1.18",
          "form": "그런거라도 있어야지",
          "original_form": "그런거라도 있어야지",
          "speaker_id": "2",
          "time": "20210829 23:18"
        },
        {
          "id": "MDRW2100023244.1.19",
          "form": "ㅎㅏ",
          "original_form": "ㅎㅏ",
          "speaker_id": "2",
          "time": "20210829 23:18"
        },
        {
          "id": "MDRW2100023244.1.20",
          "form": "난 여성향겜이",
          "original_form": "난 여성향겜이",
          "speaker_id": "2",
          "time": "20210829 23:18"
        },
        {
          "id": "MDRW2100023244.1.21",
          "form": "여주 홀대하는 티나면",
          "original_form": "여주 홀대하는 티나면",
          "speaker_id": "2",
          "time": "20210829 23:18"
        },
        {
          "id": "MDRW2100023244.1.22",
          "form": "이입 못하는데",
          "original_form": "이입 못하는데",
          "speaker_id": "2",
          "time": "20210829 23:18"
        },
        {
          "id": "MDRW2100023244.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:19"
        },
        {
          "id": "MDRW2100023244.1.24",
          "form": "외압에 굴하지 않기엔",
          "original_form": "외압에 굴하지 않기엔",
          "speaker_id": "1",
          "time": "20210829 23:19"
        },
        {
          "id": "MDRW2100023244.1.25",
          "form": "직장인은 너무 많은 풍파를 겪엇는데...",
          "original_form": "직장인은 너무 많은 풍파를 겪엇는데...",
          "speaker_id": "1",
          "time": "20210829 23:19"
        },
        {
          "id": "MDRW2100023244.1.26",
          "form": "ㅇㅇ애초에 직장인이란 설정은",
          "original_form": "ㅇㅇ애초에 직장인이란 설정은",
          "speaker_id": "2",
          "time": "20210829 23:19"
        },
        {
          "id": "MDRW2100023244.1.27",
          "form": "왜 집어넣은거야",
          "original_form": "왜 집어넣은거야",
          "speaker_id": "2",
          "time": "20210829 23:19"
        },
        {
          "id": "MDRW2100023244.1.28",
          "form": "걍 트위터에서",
          "original_form": "걍 트위터에서",
          "speaker_id": "2",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023244.1.29",
          "form": "직장에 잡혔어요 아이고도비",
          "original_form": "직장에 잡혔어요 아이고도비",
          "speaker_id": "2",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023244.1.30",
          "form": "이런것만 보고",
          "original_form": "이런것만 보고",
          "speaker_id": "2",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023244.1.31",
          "form": "짠것같음",
          "original_form": "짠것같음",
          "speaker_id": "2",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023244.1.32",
          "form": "ㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023244.1.33",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023244.1.34",
          "form": "낡고 지친 직장인 드립만 본듯....",
          "original_form": "낡고 지친 직장인 드립만 본듯....",
          "speaker_id": "1",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023244.1.35",
          "form": "ㅇㅇ대학원생이 등장하는것도",
          "original_form": "ㅇㅇ대학원생이 등장하는것도",
          "speaker_id": "2",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023244.1.36",
          "form": "비슷한 맥락으로 보여",
          "original_form": "비슷한 맥락으로 보여",
          "speaker_id": "2",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100023244.1.37",
          "form": "그리고 제목이 신도 야근을 하나요 인 이유는",
          "original_form": "그리고 제목이 신도 야근을 하나요 인 이유는",
          "speaker_id": "2",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023244.1.38",
          "form": "전혀 모르겠어",
          "original_form": "전혀 모르겠어",
          "speaker_id": "2",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100023244.1.39",
          "form": "그런 제목일거면",
          "original_form": "그런 제목일거면",
          "speaker_id": "2",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100023244.1.40",
          "form": "1. 신이 됐어도 일이 너무 많다. 아 그지같다",
          "original_form": "1. 신이 됐어도 일이 너무 많다. 아 그지같다",
          "speaker_id": "2",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100023244.1.41",
          "form": "2. ㅇㅏ 직장생활 엿같다 신 되면 야근이라도 안할까?",
          "original_form": "2. ㅇㅏ 직장생활 엿같다 신 되면 야근이라도 안할까?",
          "speaker_id": "2",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100023244.1.42",
          "form": "뭐 이런",
          "original_form": "뭐 이런",
          "speaker_id": "2",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100023244.1.43",
          "form": "맥락이랄게 있어야하는게",
          "original_form": "맥락이랄게 있어야하는게",
          "speaker_id": "2",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100023244.1.44",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "2",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100023244.1.45",
          "form": "ㅇㅇ 그니까",
          "original_form": "ㅇㅇ 그니까",
          "speaker_id": "1",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100023244.1.46",
          "form": "대부분",
          "original_form": "대부분",
          "speaker_id": "1",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100023244.1.47",
          "form": "2로 생각하지",
          "original_form": "2로 생각하지",
          "speaker_id": "1",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100023244.1.48",
          "form": "2인줄 알고 덥썩 수락했더니 사실 1이었다",
          "original_form": "2인줄 알고 덥썩 수락했더니 사실 1이었다",
          "speaker_id": "1",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100023244.1.49",
          "form": "이정도로 가벼운 스토리를 생각하지",
          "original_form": "이정도로 가벼운 스토리를 생각하지",
          "speaker_id": "1",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100023244.1.50",
          "form": "ㅇㅇ솔직히",
          "original_form": "ㅇㅇ솔직히",
          "speaker_id": "2",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100023244.1.51",
          "form": "나도 그랬음",
          "original_form": "나도 그랬음",
          "speaker_id": "2",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100023244.1.52",
          "form": "현실과 가상 사이에서 존재를 고민하는 내용이 4챕 내내 나오길 바라는 사람이",
          "original_form": "현실과 가상 사이에서 존재를 고민하는 내용이 4챕 내내 나오길 바라는 사람이",
          "speaker_id": "1",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100023244.1.53",
          "form": "어딨냐규...",
          "original_form": "어딨냐규...",
          "speaker_id": "1",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100023244.1.54",
          "form": "ㅇㅇ그럴거면 제목이라도 좀",
          "original_form": "ㅇㅇ그럴거면 제목이라도 좀",
          "speaker_id": "2",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100023244.1.55",
          "form": "무거웠어야지",
          "original_form": "무거웠어야지",
          "speaker_id": "2",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100023244.1.56",
          "form": "제목은 거의 라노벨이야",
          "original_form": "제목은 거의 라노벨이야",
          "speaker_id": "2",
          "time": "20210829 23:24"
        },
        {
          "id": "MDRW2100023244.1.57",
          "form": "난 그래서 처음엔",
          "original_form": "난 그래서 처음엔",
          "speaker_id": "2",
          "time": "20210829 23:25"
        },
        {
          "id": "MDRW2100023244.1.58",
          "form": "직장인인데 갑자기 신으로 이직당했다?!",
          "original_form": "직장인인데 갑자기 신으로 이직당했다?!",
          "speaker_id": "2",
          "time": "20210829 23:25"
        },
        {
          "id": "MDRW2100023244.1.59",
          "form": "근데 전임자의 인수인계도 없고 부하직원들은 비협조적?!",
          "original_form": "근데 전임자의 인수인계도 없고 부하직원들은 비협조적?!",
          "speaker_id": "2",
          "time": "20210829 23:25"
        },
        {
          "id": "MDRW2100023244.1.60",
          "form": "이런 스토릴줄",
          "original_form": "이런 스토릴줄",
          "speaker_id": "2",
          "time": "20210829 23:25"
        },
        {
          "id": "MDRW2100023244.1.61",
          "form": "나도 ㅋㅋㅋㅋㅋㅋ 걍 가벼운 일상개그 연애물인줄 알았지",
          "original_form": "나도 ㅋㅋㅋㅋㅋㅋ 걍 가벼운 일상개그 연애물인줄 알았지",
          "speaker_id": "1",
          "time": "20210829 23:26"
        },
        {
          "id": "MDRW2100023244.1.62",
          "form": "갑자기 스릴러 오컬트 게임이 될 줄은...",
          "original_form": "갑자기 스릴러 오컬트 게임이 될 줄은...",
          "speaker_id": "1",
          "time": "20210829 23:26"
        },
        {
          "id": "MDRW2100023244.1.63",
          "form": "ㄹㅇ....",
          "original_form": "ㄹㅇ....",
          "speaker_id": "2",
          "time": "20210829 23:26"
        },
        {
          "id": "MDRW2100023244.1.64",
          "form": "오늘부터 원신 데이터 사전다운로드 가능함",
          "original_form": "오늘부터 원신 데이터 사전다운로드 가능함",
          "speaker_id": "1",
