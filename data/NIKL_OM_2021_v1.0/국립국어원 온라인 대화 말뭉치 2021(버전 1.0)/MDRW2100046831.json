      "utterance": [
        {
          "id": "MDRW2100046831.1.1",
          "form": "안녕하십니까",
          "original_form": "안녕하십니까",
          "speaker_id": "1",
          "time": "20211104 21:37"
        },
        {
          "id": "MDRW2100046831.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211104 21:37"
        },
        {
          "id": "MDRW2100046831.1.3",
          "form": "요즘 즐기고 계시는 취미가 있으신가요?",
          "original_form": "요즘 즐기고 계시는 취미가 있으신가요?",
          "speaker_id": "2",
          "time": "20211104 21:38"
        },
        {
          "id": "MDRW2100046831.1.4",
          "form": "취미라고 해야할진 모르겠는데 코인이라고 업비트에서 조금씩하고있습니다",
          "original_form": "취미라고 해야할진 모르겠는데 코인이라고 업비트에서 조금씩하고있습니다",
          "speaker_id": "1",
          "time": "20211104 21:39"
        },
        {
          "id": "MDRW2100046831.1.5",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20211104 21:39"
        },
        {
          "id": "MDRW2100046831.1.6",
          "form": "비트코인 말씀하시는거죠 ㅎㅎ",
          "original_form": "비트코인 말씀하시는거죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 21:39"
        },
        {
          "id": "MDRW2100046831.1.7",
          "form": "네 가장많이 알려진게 비트코인입니다!",
          "original_form": "네 가장많이 알려진게 비트코인입니다!",
          "speaker_id": "1",
          "time": "20211104 21:39"
        },
        {
          "id": "MDRW2100046831.1.8",
          "form": "지금도 장이 열려있나요??",
          "original_form": "지금도 장이 열려있나요??",
          "speaker_id": "2",
          "time": "20211104 21:40"
        },
        {
          "id": "MDRW2100046831.1.9",
          "form": "네 24시간내내 열려서 진짜 너무빠지면 피폐해질정도인것같구요\r\n그래서 중심을 잘잡아야하는것같습니다",
          "original_form": "네 24시간내내 열려서 진짜 너무빠지면 피폐해질정도인것같구요\r\n그래서 중심을 잘잡아야하는것같습니다",
          "speaker_id": "1",
          "time": "20211104 21:40"
        },
        {
          "id": "MDRW2100046831.1.10",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20211104 21:40"
        },
        {
          "id": "MDRW2100046831.1.11",
          "form": "워낙 월급만으로는살기가팍팍하다보니..ㅎㅎ",
          "original_form": "워낙 월급만으로는살기가팍팍하다보니..ㅎㅎ",
          "speaker_id": "1",
          "time": "20211104 21:40"
        },
        {
          "id": "MDRW2100046831.1.12",
          "form": "24시간 장이 열려있군요...;;;; 정말",
          "original_form": "24시간 장이 열려있군요...;;;; 정말",
          "speaker_id": "2",
          "time": "20211104 21:40"
        },
        {
          "id": "MDRW2100046831.1.13",
          "form": "여기에 투자 많이 하시는 분들은",
          "original_form": "여기에 투자 많이 하시는 분들은",
          "speaker_id": "2",
          "time": "20211104 21:41"
        },
        {
          "id": "MDRW2100046831.1.14",
          "form": "거의 잠도 못자시겠네요....ㅠㅠㅠ",
          "original_form": "거의 잠도 못자시겠네요....ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20211104 21:41"
        },
        {
          "id": "MDRW2100046831.1.15",
          "form": "아 하긴",
          "original_form": "아 하긴",
          "speaker_id": "2",
          "time": "20211104 21:41"
        },
        {
          "id": "MDRW2100046831.1.16",
          "form": "그렇죠 ㅠㅠㅠ",
          "original_form": "그렇죠 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20211104 21:41"
        },
        {
          "id": "MDRW2100046831.1.17",
          "form": "물가는오르고",
          "original_form": "물가는오르고",
          "speaker_id": "2",
          "time": "20211104 21:41"
        },
        {
          "id": "MDRW2100046831.1.18",
          "form": "월급은 안오르고ㅎ",
          "original_form": "월급은 안오르고ㅎ",
          "speaker_id": "2",
          "time": "20211104 21:41"
        },
        {
          "id": "MDRW2100046831.1.19",
          "form": "네 거의 컴퓨터앞에서잘거에요\r\n전 큰돈은아니라 그냥 조금씩 용돈벌이나 한다는마음으로하고있습니다",
          "original_form": "네 거의 컴퓨터앞에서잘거에요\r\n전 큰돈은아니라 그냥 조금씩 용돈벌이나 한다는마음으로하고있습니다",
          "speaker_id": "1",
          "time": "20211104 21:41"
        },
        {
          "id": "MDRW2100046831.1.20",
          "form": "맞습니다 그래서 제 주변에서도 주식 코인같은걸 많이들하시더라구여",
          "original_form": "맞습니다 그래서 제 주변에서도 주식 코인같은걸 많이들하시더라구여",
          "speaker_id": "1",
          "time": "20211104 21:42"
        },
        {
          "id": "MDRW2100046831.1.21",
          "form": "뭔가 비트코인에",
          "original_form": "뭔가 비트코인에",
          "speaker_id": "2",
          "time": "20211104 21:44"
        },
        {
          "id": "MDRW2100046831.1.22",
          "form": "큰 돈 투자하면 정말 일상생활이",
          "original_form": "큰 돈 투자하면 정말 일상생활이",
          "speaker_id": "2",
          "time": "20211104 21:44"
        },
        {
          "id": "MDRW2100046831.1.23",
          "form": "불가할 거 같은 느낌 ㅜㅜ",
          "original_form": "불가할 거 같은 느낌 ㅜㅜ",
          "speaker_id": "2",
          "time": "20211104 21:44"
        },
        {
          "id": "MDRW2100046831.1.24",
          "form": "님같은",
          "original_form": "님같은",
          "speaker_id": "2",
          "time": "20211104 21:44"
        },
        {
          "id": "MDRW2100046831.1.25",
          "form": "마인드로 해야되는거같아요...",
          "original_form": "마인드로 해야되는거같아요...",
          "speaker_id": "2",
          "time": "20211104 21:45"
        },
        {
          "id": "MDRW2100046831.1.26",
          "form": "그래도 초조할 거 같은데",
          "original_form": "그래도 초조할 거 같은데",
          "speaker_id": "2",
          "time": "20211104 21:45"
        },
        {
          "id": "MDRW2100046831.1.27",
          "form": "네 확실히요; 저도 그런적이 있었어서요 사실",
          "original_form": "네 확실히요; 저도 그런적이 있었어서요 사실",
          "speaker_id": "1",
          "time": "20211104 21:45"
        },
        {
          "id": "MDRW2100046831.1.28",
          "form": "그때 크게 데이고 이젠 소액으로 밥값이나 벌자 마인드로..ㅎㅎ",
          "original_form": "그때 크게 데이고 이젠 소액으로 밥값이나 벌자 마인드로..ㅎㅎ",
          "speaker_id": "1",
          "time": "20211104 21:45"
        },
        {
          "id": "MDRW2100046831.1.29",
          "form": "아하 ㅠㅠ",
          "original_form": "아하 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211104 21:46"
        },
        {
          "id": "MDRW2100046831.1.30",
          "form": "코인 떡상해서",
          "original_form": "코인 떡상해서",
          "speaker_id": "2",
          "time": "20211104 21:46"
        },
        {
          "id": "MDRW2100046831.1.31",
          "form": "님 부자 되셨으면 ㅎㅎㅎ",
          "original_form": "님 부자 되셨으면 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 21:46"
        },
        {
          "id": "MDRW2100046831.1.32",
          "form": "그래도 건강 챙겨가면서",
          "original_form": "그래도 건강 챙겨가면서",
          "speaker_id": "2",
          "time": "20211104 21:46"
        },
        {
          "id": "MDRW2100046831.1.33",
          "form": "하세용",
          "original_form": "하세용",
          "speaker_id": "2",
          "time": "20211104 21:46"
        },
        {
          "id": "MDRW2100046831.1.34",
          "form": "말씀감사합니다 님도 건강하세요",
          "original_form": "말씀감사합니다 님도 건강하세요",
          "speaker_id": "1",
          "time": "20211104 21:46"
        },
        {
          "id": "MDRW2100046831.1.35",
          "form": "네네 다 잘먹고 잘살자는 마음에서 하는 일들이니",
          "original_form": "네네 다 잘먹고 잘살자는 마음에서 하는 일들이니",
          "speaker_id": "2",
          "time": "20211104 21:47"
        },
        {
          "id": "MDRW2100046831.1.36",
