      "utterance": [
        {
          "id": "MDRW2100005213.1.1",
          "form": "여행 좋아하세요?ㅎㅎ",
          "original_form": "여행 좋아하세요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100005213.1.2",
          "form": "너무 좋아해요",
          "original_form": "너무 좋아해요",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005213.1.3",
          "form": "기억에 남는 여행 있으세요?",
          "original_form": "기억에 남는 여행 있으세요?",
          "speaker_id": "1",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005213.1.4",
          "form": "저두 넘 좋아해요 음.. 최근에 간 제주도가 기억에 남네요!",
          "original_form": "저두 넘 좋아해요 음.. 최근에 간 제주도가 기억에 남네요!",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005213.1.5",
          "form": "최근에 다녀오셨어요?",
          "original_form": "최근에 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100005213.1.6",
          "form": "네 저번달에요!",
          "original_form": "네 저번달에요!",
          "speaker_id": "2",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005213.1.7",
          "form": "제주도에 이제 사람많죠?",
          "original_form": "제주도에 이제 사람많죠?",
          "speaker_id": "1",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005213.1.8",
          "form": "네~ ㅠㅠ 마스크 꼭꼭 쓰고 조심히다녔어요",
          "original_form": "네~ ㅠㅠ 마스크 꼭꼭 쓰고 조심히다녔어요",
          "speaker_id": "2",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005213.1.9",
          "form": "작년에 제주도 갔다왔어요",
          "original_form": "작년에 제주도 갔다왔어요",
          "speaker_id": "1",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100005213.1.10",
          "form": "제주도 넘 좋은것 같아요~",
          "original_form": "제주도 넘 좋은것 같아요~",
          "speaker_id": "2",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005213.1.11",
          "form": "어디가보셨어요?",
          "original_form": "어디가보셨어요?",
          "speaker_id": "1",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100005213.1.12",
          "form": "아르떼 다녀오셨어요?",
          "original_form": "아르떼 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005213.1.13",
          "form": "음 이곳저곳 돌아다녔는데.. 딱 한군데가 안떠오르네요 ㅋㅋㅋ",
          "original_form": "음 이곳저곳 돌아다녔는데.. 딱 한군데가 안떠오르네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005213.1.14",
          "form": "앗 아니요 못갔어요 ㅠㅠ 비오면 가려했는데 날씨가 계속 맑더라구요!ㅋㅋ",
          "original_form": "앗 아니요 못갔어요 ㅠㅠ 비오면 가려했는데 날씨가 계속 맑더라구요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100005213.1.15",
          "form": "날좋으면 밖에서 놀아야죠 ㅎㅎ",
          "original_form": "날좋으면 밖에서 놀아야죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005213.1.16",
          "form": "제주도는 진짜 하늘이 반이상 해주는거같아요",
          "original_form": "제주도는 진짜 하늘이 반이상 해주는거같아요",
          "speaker_id": "1",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005213.1.17",
          "form": "사진도 여행할때도",
          "original_form": "사진도 여행할때도",
          "speaker_id": "1",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100005213.1.18",
          "form": "마자요~ ㅠㅠ 사진은 잘나와서 넘좋았어요! ㅎㅎ 날씨가 좋아서 바닷가를 많이 갔어요~",
          "original_form": "마자요~ ㅠㅠ 사진은 잘나와서 넘좋았어요! ㅎㅎ 날씨가 좋아서 바닷가를 많이 갔어요~",
          "speaker_id": "2",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100005213.1.19",
          "form": "협재?",
          "original_form": "협재?",
          "speaker_id": "1",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005213.1.20",
          "form": "해수욕장은요?",
          "original_form": "해수욕장은요?",
          "speaker_id": "1",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005213.1.21",
          "form": "거기 갔어요! 넘이쁘더라구요 ㅠㅠ",
          "original_form": "거기 갔어요! 넘이쁘더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 21:24"
        },
        {
          "id": "MDRW2100005213.1.22",
          "form": "근데 막상 .. 물놀이는 못하고.. 바다가 보이는 카페에서만 즐겼네용ㅋㅋ",
          "original_form": "근데 막상 .. 물놀이는 못하고.. 바다가 보이는 카페에서만 즐겼네용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005213.1.23",
          "form": "글쵸 거긴 진짜 반짝반짝 빛나는 느낌이더라구요",
          "original_form": "글쵸 거긴 진짜 반짝반짝 빛나는 느낌이더라구요",
          "speaker_id": "1",
          "time": "20210821 21:25"
        },
        {
          "id": "MDRW2100005213.1.24",
          "form": "넹 ㅠ진짜 넘이뻤어요~ 또 가고싶네요",
          "original_form": "넹 ㅠ진짜 넘이뻤어요~ 또 가고싶네요",
          "speaker_id": "2",
          "time": "20210821 21:26"
        }
      ]
    }
  ]
