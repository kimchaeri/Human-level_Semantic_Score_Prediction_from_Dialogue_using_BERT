      "utterance": [
        {
          "id": "MDRW2100040914.1.1",
          "form": "저도 집순이에요ㅎ",
          "original_form": "저도 집순이에요ㅎ",
          "speaker_id": "1",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100040914.1.2",
          "form": "요즘 재밌게 보시는거 있나요~?",
          "original_form": "요즘 재밌게 보시는거 있나요~?",
          "speaker_id": "1",
          "time": "20210905 12:15"
        },
        {
          "id": "MDRW2100040914.1.3",
          "form": "ㅎㅎ저는 파워집순이.....ㅎㅎ",
          "original_form": "ㅎㅎ저는 파워집순이.....ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 12:15"
        },
        {
          "id": "MDRW2100040914.1.4",
          "form": "음 영화좋아해서",
          "original_form": "음 영화좋아해서",
          "speaker_id": "2",
          "time": "20210905 12:15"
        },
        {
          "id": "MDRW2100040914.1.5",
          "form": "영화주로많이봐요",
          "original_form": "영화주로많이봐요",
          "speaker_id": "2",
          "time": "20210905 12:15"
        },
        {
          "id": "MDRW2100040914.1.6",
          "form": "저도 출퇴근 아니면 잘 안나가는 ㅋㅋ",
          "original_form": "저도 출퇴근 아니면 잘 안나가는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:15"
        },
        {
          "id": "MDRW2100040914.1.7",
          "form": "오 무슨영화 보셨어요~?",
          "original_form": "오 무슨영화 보셨어요~?",
          "speaker_id": "1",
          "time": "20210905 12:15"
        },
        {
          "id": "MDRW2100040914.1.8",
          "form": "ㅋㅋ출근하는순간 방전이에여ㅜ",
          "original_form": "ㅋㅋ출근하는순간 방전이에여ㅜ",
          "speaker_id": "2",
          "time": "20210905 12:16"
        },
        {
          "id": "MDRW2100040914.1.9",
          "form": "샹치 볼까생각중인데",
          "original_form": "샹치 볼까생각중인데",
          "speaker_id": "2",
          "time": "20210905 12:16"
        },
        {
          "id": "MDRW2100040914.1.10",
          "form": "오늘은 못나가겟어용",
          "original_form": "오늘은 못나가겟어용",
          "speaker_id": "2",
          "time": "20210905 12:16"
        },
        {
          "id": "MDRW2100040914.1.11",
          "form": "맞아요 퇴근하고 오면 널부러져요 ㄷㄷ",
          "original_form": "맞아요 퇴근하고 오면 널부러져요 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210905 12:17"
        },
        {
          "id": "MDRW2100040914.1.12",
          "form": "헉 저도 오늘 샹치보려고 진짜 오랜만에 약속잡았는데",
          "original_form": "헉 저도 오늘 샹치보려고 진짜 오랜만에 약속잡았는데",
          "speaker_id": "1",
          "time": "20210905 12:17"
        },
        {
          "id": "MDRW2100040914.1.13",
          "form": "취소했어요 ㅎ어제 출근하고 피곤해서",
          "original_form": "취소했어요 ㅎ어제 출근하고 피곤해서",
          "speaker_id": "1",
          "time": "20210905 12:17"
        },
        {
          "id": "MDRW2100040914.1.14",
          "form": "앗ㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:18"
        },
        {
          "id": "MDRW2100040914.1.15",
          "form": "이해합니다",
          "original_form": "이해합니다",
          "speaker_id": "2",
          "time": "20210905 12:18"
        },
        {
          "id": "MDRW2100040914.1.16",
          "form": "당연한거에요 흐흐",
          "original_form": "당연한거에요 흐흐",
          "speaker_id": "2",
          "time": "20210905 12:18"
        },
        {
          "id": "MDRW2100040914.1.17",
          "form": "요즘 주로 뭐보세용",
          "original_form": "요즘 주로 뭐보세용",
          "speaker_id": "2",
          "time": "20210905 12:18"
        },
        {
          "id": "MDRW2100040914.1.18",
          "form": "ㅋㅋ그쵸",
          "original_form": "ㅋㅋ그쵸",
          "speaker_id": "1",
          "time": "20210905 12:45"
        },
        {
          "id": "MDRW2100040914.1.19",
          "form": "요즘 티비프로는 식스센스요",
          "original_form": "요즘 티비프로는 식스센스요",
          "speaker_id": "1",
          "time": "20210905 12:45"
        },
        {
          "id": "MDRW2100040914.1.20",
          "form": "심야괴담회랑요",
          "original_form": "심야괴담회랑요",
          "speaker_id": "1",
          "time": "20210905 12:45"
        },
        {
          "id": "MDRW2100040914.1.21",
          "form": "오 저는 놀뭐 런닝맨 유퀴즈!!",
          "original_form": "오 저는 놀뭐 런닝맨 유퀴즈!!",
          "speaker_id": "2",
          "time": "20210905 12:48"
        },
        {
          "id": "MDRW2100040914.1.22",
          "form": "ㅋ 아 런닝맨 올만에 봤는데 재밌더라고요 이광수 나갔다해서 봤거든요",
          "original_form": "ㅋ 아 런닝맨 올만에 봤는데 재밌더라고요 이광수 나갔다해서 봤거든요",
          "speaker_id": "1",
          "time": "20210905 15:19"
        },
        {
          "id": "MDRW2100040914.1.23",
          "form": "광수나간날 올해 젤 많이 울엇어유ㅋㅋㅋ",
          "original_form": "광수나간날 올해 젤 많이 울엇어유ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 09:50"
        },
        {
          "id": "MDRW2100040914.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 09:51"
        },
        {
          "id": "MDRW2100040914.1.25",
          "form": "아 정말요? 런닝맨 팬이셨나봐요ㅠ",
          "original_form": "아 정말요? 런닝맨 팬이셨나봐요ㅠ",
          "speaker_id": "1",
          "time": "20210906 11:06"
        },
        {
          "id": "MDRW2100040914.1.26",
          "form": "ㅠㅠ팬인줄 그날 알앗어요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅠㅠ팬인줄 그날 알앗어요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:21"
        },
        {
          "id": "MDRW2100040914.1.27",
          "form": "광수런닝맨에 진심이엿나바여ㅋㅋㅋㅋㄲ",
          "original_form": "광수런닝맨에 진심이엿나바여ㅋㅋㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210906 11:21"
        },
        {
          "id": "MDRW2100040914.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ그러셨구나",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그러셨구나",
          "speaker_id": "1",
          "time": "20210906 11:58"
        },
        {
          "id": "MDRW2100040914.1.29",
          "form": "저는 한도안 안보다가 최근에 재밌었다길래 봤거든요",
          "original_form": "저는 한도안 안보다가 최근에 재밌었다길래 봤거든요",
          "speaker_id": "1",
          "time": "20210906 11:58"
        },
        {
          "id": "MDRW2100040914.1.30",
          "form": "유튜브가 있어서 편해요",
          "original_form": "유튜브가 있어서 편해요",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100040914.1.31",
