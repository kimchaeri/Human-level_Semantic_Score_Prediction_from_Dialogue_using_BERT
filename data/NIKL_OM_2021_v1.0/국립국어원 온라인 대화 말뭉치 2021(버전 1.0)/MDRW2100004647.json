      "utterance": [
        {
          "id": "MDRW2100004647.1.1",
          "form": "내 앞에서 책을 10권을 뽑고 사라지면... 좀 슬퍼져",
          "original_form": "내 앞에서 책을 10권을 뽑고 사라지면... 좀 슬퍼져",
          "speaker_id": "1",
          "time": "20210821 17:42"
        },
        {
          "id": "MDRW2100004647.1.2",
          "form": "성악설ㅋㅋㅋㅋ",
          "original_form": "성악설ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:43"
        },
        {
          "id": "MDRW2100004647.1.3",
          "form": "주제가 바꼈네",
          "original_form": "주제가 바꼈네",
          "speaker_id": "1",
          "time": "20210821 17:45"
        },
        {
          "id": "MDRW2100004647.1.4",
          "form": "게임 모임이 제일 즐겁지",
          "original_form": "게임 모임이 제일 즐겁지",
          "speaker_id": "1",
          "time": "20210821 17:45"
        },
        {
          "id": "MDRW2100004647.1.5",
          "form": "다들 혼자서 최애를 부르짖는 곳... 아주 즐거워",
          "original_form": "다들 혼자서 최애를 부르짖는 곳... 아주 즐거워",
          "speaker_id": "1",
          "time": "20210821 17:46"
        },
        {
          "id": "MDRW2100004647.1.6",
          "form": "걍 우리 모임이자나",
          "original_form": "걍 우리 모임이자나",
          "speaker_id": "2",
          "time": "20210821 17:47"
        },
        {
          "id": "MDRW2100004647.1.7",
          "form": "진실을 말하다니",
          "original_form": "진실을 말하다니",
          "speaker_id": "1",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004647.1.8",
          "form": "푸헹헹",
          "original_form": "푸헹헹",
          "speaker_id": "2",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004647.1.9",
          "form": "하지만.... 최애는 예쁘단 말이야",
          "original_form": "하지만.... 최애는 예쁘단 말이야",
          "speaker_id": "1",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004647.1.10",
          "form": "전자 피규어는... 아름답다",
          "original_form": "전자 피규어는... 아름답다",
          "speaker_id": "1",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004647.1.11",
          "form": "최애앓이 모임이",
          "original_form": "최애앓이 모임이",
          "speaker_id": "2",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004647.1.12",
          "form": "정말 집단독백현장 그자체",
          "original_form": "정말 집단독백현장 그자체",
          "speaker_id": "2",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004647.1.13",
          "form": "갈틱폰을 하면 제일 잘 보이지",
          "original_form": "갈틱폰을 하면 제일 잘 보이지",
          "speaker_id": "1",
          "time": "20210821 17:52"
        },
        {
          "id": "MDRW2100004647.1.14",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋ모이면 하는 1순위 갈틱폰",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋ모이면 하는 1순위 갈틱폰",
          "speaker_id": "2",
          "time": "20210821 17:53"
        },
        {
          "id": "MDRW2100004647.1.15",
          "form": "아냐 요즘엔 모아보새지",
          "original_form": "아냐 요즘엔 모아보새지",
          "speaker_id": "1",
          "time": "20210821 17:54"
        },
        {
          "id": "MDRW2100004647.1.16",
          "form": "모임도 유행이 있다",
          "original_form": "모임도 유행이 있다",
          "speaker_id": "2",
          "time": "20210821 17:54"
        },
        {
          "id": "MDRW2100004647.1.17",
          "form": "온라인으로 할 수 있는게 많아져서 다행이지",
          "original_form": "온라인으로 할 수 있는게 많아져서 다행이지",
          "speaker_id": "1",
          "time": "20210821 18:00"
        },
        {
          "id": "MDRW2100004647.1.18",
          "form": "아니었음 곰팡이처럼.... 눌러붙었을 것",
