      "utterance": [
        {
          "id": "MDRW2100046741.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211104 13:05"
        },
        {
          "id": "MDRW2100046741.1.2",
          "form": "안녕하세요! 점심 드셨나요?",
          "original_form": "안녕하세요! 점심 드셨나요?",
          "speaker_id": "2",
          "time": "20211104 13:21"
        },
        {
          "id": "MDRW2100046741.1.3",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "1",
          "time": "20211104 13:23"
        },
        {
          "id": "MDRW2100046741.1.4",
          "form": "집에 있는 나물과 식은 밥으로 비벼 먹었어요",
          "original_form": "집에 있는 나물과 식은 밥으로 비벼 먹었어요",
          "speaker_id": "1",
          "time": "20211104 13:23"
        },
        {
          "id": "MDRW2100046741.1.5",
          "form": "어찌나 맛없던지",
          "original_form": "어찌나 맛없던지",
          "speaker_id": "1",
          "time": "20211104 13:24"
        },
        {
          "id": "MDRW2100046741.1.6",
          "form": "역시  비빔밥은 고추장이 생명이네요",
          "original_form": "역시  비빔밥은 고추장이 생명이네요",
          "speaker_id": "1",
          "time": "20211104 13:24"
        },
        {
          "id": "MDRW2100046741.1.7",
          "form": "점심 하셨나용",
          "original_form": "점심 하셨나용",
          "speaker_id": "1",
          "time": "20211104 13:27"
        },
        {
          "id": "MDRW2100046741.1.8",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20211104 13:33"
        },
        {
          "id": "MDRW2100046741.1.9",
          "form": "헉! 어떻게 비빔밥을 고추장 없이 드셨어요?",
          "original_form": "헉! 어떻게 비빔밥을 고추장 없이 드셨어요?",
          "speaker_id": "2",
          "time": "20211104 13:33"
        },
        {
          "id": "MDRW2100046741.1.10",
          "form": "장이 생명이잖아요 ㅎㅎ",
          "original_form": "장이 생명이잖아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 13:33"
        },
        {
          "id": "MDRW2100046741.1.11",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20211104 13:33"
        },
        {
          "id": "MDRW2100046741.1.12",
          "form": "저도 점심 먹고 왔어요 ㅎㅎ",
          "original_form": "저도 점심 먹고 왔어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 13:33"
        },
        {
          "id": "MDRW2100046741.1.13",
          "form": "저는 덮밥 먹었어요!",
          "original_form": "저는 덮밥 먹었어요!",
          "speaker_id": "2",
          "time": "20211104 13:34"
        },
        {
          "id": "MDRW2100046741.1.14",
          "form": "어떤 덮밥인가요",
          "original_form": "어떤 덮밥인가요",
          "speaker_id": "1",
          "time": "20211104 13:34"
        },
        {
          "id": "MDRW2100046741.1.15",
          "form": "장어 덮밥인가요",
          "original_form": "장어 덮밥인가요",
          "speaker_id": "1",
          "time": "20211104 13:35"
        },
        {
          "id": "MDRW2100046741.1.16",
          "form": "아뇨 ㅎㅎ",
          "original_form": "아뇨 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 13:39"
        },
        {
          "id": "MDRW2100046741.1.17",
          "form": "장어 덮밥이었으면 좋았겠지만",
          "original_form": "장어 덮밥이었으면 좋았겠지만",
          "speaker_id": "2",
          "time": "20211104 13:39"
        },
        {
          "id": "MDRW2100046741.1.18",
          "form": "대창 덮밥 먹었어요!",
          "original_form": "대창 덮밥 먹었어요!",
          "speaker_id": "2",
          "time": "20211104 13:39"
        },
        {
          "id": "MDRW2100046741.1.19",
          "form": "저는 치아가 좋지못해서  대창 막창 못먹네요ㅜㅜ",
          "original_form": "저는 치아가 좋지못해서  대창 막창 못먹네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20211104 13:40"
        },
        {
          "id": "MDRW2100046741.1.20",
          "form": "헉 치아가 안좋으시군요 ㅠㅠ",
          "original_form": "헉 치아가 안좋으시군요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211104 13:54"
        },
        {
          "id": "MDRW2100046741.1.21",
          "form": "저도 자주 먹지는 않아요 ㅋㅋㅋ",
          "original_form": "저도 자주 먹지는 않아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 13:54"
        },
        {
          "id": "MDRW2100046741.1.22",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20211104 13:57"
        },
        {
          "id": "MDRW2100046741.1.23",
          "form": "내장이 부드러우면 좋을텐데 아싑네요",
          "original_form": "내장이 부드러우면 좋을텐데 아싑네요",
          "speaker_id": "1",
          "time": "20211104 13:57"
        },
        {
          "id": "MDRW2100046741.1.24",
          "form": "건강이 최고입니다 아프고나니깐  모든 게 부질없더라구요",
          "original_form": "건강이 최고입니다 아프고나니깐  모든 게 부질없더라구요",
          "speaker_id": "1",
          "time": "20211104 14:07"
        },
        {
          "id": "MDRW2100046741.1.25",
          "form": "건강이 정말 최고지요 ㅠㅠ",
          "original_form": "건강이 정말 최고지요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211104 14:09"
        },
        {
          "id": "MDRW2100046741.1.26",
          "form": "이제는 괜찮으신거예요~?",
          "original_form": "이제는 괜찮으신거예요~?",
          "speaker_id": "2",
          "time": "20211104 14:10"
        },
        {
          "id": "MDRW2100046741.1.27",
          "form": "넵 완치는 아니더라도 일상생활은 겨우 하네요",
          "original_form": "넵 완치는 아니더라도 일상생활은 겨우 하네요",
          "speaker_id": "1",
          "time": "20211104 14:10"
        },
        {
          "id": "MDRW2100046741.1.28",
          "form": "아이구 ㅠㅠ 얼른 완치돼서 건강하게 지내셨으면 좋겠네요!",
          "original_form": "아이구 ㅠㅠ 얼른 완치돼서 건강하게 지내셨으면 좋겠네요!",
          "speaker_id": "2",
          "time": "20211104 14:11"
        }
      ]
    }
