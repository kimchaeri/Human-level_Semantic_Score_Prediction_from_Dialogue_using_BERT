        {
          "id": "MDRW2100047309.1.1",
          "form": "파세요",
          "original_form": "파세요",
          "speaker_id": "2",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100047309.1.2",
          "form": "구글플레이 금액권이죠?",
          "original_form": "구글플레이 금액권이죠?",
          "speaker_id": "2",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100047309.1.3",
          "form": "저도 예전에 5000원짜리받아서 중나에",
          "original_form": "저도 예전에 5000원짜리받아서 중나에",
          "speaker_id": "2",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100047309.1.4",
          "form": "올리니 금방사거던데요",
          "original_form": "올리니 금방사거던데요",
          "speaker_id": "2",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100047309.1.5",
          "form": "그게",
          "original_form": "그게",
          "speaker_id": "1",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100047309.1.6",
          "form": "파는지 모르고",
          "original_form": "파는지 모르고",
          "speaker_id": "1",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100047309.1.7",
          "form": "등록을 했어요 ㅜㅜ",
          "original_form": "등록을 했어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100047309.1.8",
          "form": "무려 2만원인데 ㅜㅜ",
          "original_form": "무려 2만원인데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100047309.1.9",
          "form": "이불을 팍찾어요 ㅜㅜ",
          "original_form": "이불을 팍찾어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100047309.1.10",
          "form": "아 지금생각해도 슬프네요",
          "original_form": "아 지금생각해도 슬프네요",
          "speaker_id": "1",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100047309.1.11",
          "form": "곧 사라지는데",
          "original_form": "곧 사라지는데",
          "speaker_id": "1",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100047309.1.12",
          "form": "게임도 안하고",
          "original_form": "게임도 안하고",
          "speaker_id": "1",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100047309.1.13",
          "form": "물건도 못사고",
          "original_form": "물건도 못사고",
          "speaker_id": "1",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100047309.1.14",
          "form": "책이나 아무거나 막 사려고요",
          "original_form": "책이나 아무거나 막 사려고요",
          "speaker_id": "1",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100047309.1.15",
          "form": "이북 ㅜㅜ",
          "original_form": "이북 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 10:47"
        },
        {
          "id": "MDRW2100047309.1.16",
          "form": "뭘사야 하나요? ㅋㅋ",
          "original_form": "뭘사야 하나요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100047309.1.17",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100047309.1.18",
          "form": "등록을 ㅜㅠ",
          "original_form": "등록을 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100047309.1.19",
          "form": "네 전 바보 입니다",
          "original_form": "네 전 바보 입니다",
          "speaker_id": "1",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100047309.1.20",
          "form": "게임하는사람들한테 유용하던데요",
          "original_form": "게임하는사람들한테 유용하던데요",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100047309.1.21",
          "form": "그때의 나를 말리고 싶어요",
          "original_form": "그때의 나를 말리고 싶어요",
          "speaker_id": "1",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100047309.1.22",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100047309.1.23",
          "form": "ㅋㅋㅋ 할수없죠",
          "original_form": "ㅋㅋㅋ 할수없죠",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100047309.1.24",
          "form": "전 게임은 스벅각 베라각 밖에 몰라요 ㅋㅋ",
          "original_form": "전 게임은 스벅각 베라각 밖에 몰라요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100047309.1.25",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100047309.1.26",
          "form": "마음의 양식용으로 주문을",
          "original_form": "마음의 양식용으로 주문을",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100047309.1.27",
          "form": "마구잡이로 통크게 책 사겠습니다 ㅋㅋ",
          "original_form": "마구잡이로 통크게 책 사겠습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100047309.1.28",
          "form": "아님 아들책이라도 사는게 ㅎㅎ",
          "original_form": "아님 아들책이라도 사는게 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100047309.1.29",
          "form": "아!!!사서 읽고 당근에 판매요 ㅋㅋㅋ",
          "original_form": "아!!!사서 읽고 당근에 판매요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100047309.1.30",
          "form": "아들이 살게 없데요",
          "original_form": "아들이 살게 없데요",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100047309.1.31",
          "form": "이북밖에 안돼요",
          "original_form": "이북밖에 안돼요",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100047309.1.32",
          "form": "아 이런일이 못팔아요",
          "original_form": "아 이런일이 못팔아요",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100047309.1.33",
          "form": "아 이북 ㅜㅠ",
          "original_form": "아 이북 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100047309.1.34",
          "form": "진짜 쓸때 없죠 ㅋㅋ",
          "original_form": "진짜 쓸때 없죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100047309.1.35",
          "form": "마음의 양식용으로 생각정리하셔야겠네요",
          "original_form": "마음의 양식용으로 생각정리하셔야겠네요",
          "speaker_id": "2",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100047309.1.36",
          "form": "이러며 배워갑니다 ㅎㅎ",
          "original_form": "이러며 배워갑니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100047309.1.37",
          "form": "저도 맨날팔아서 ㅋㅋ",
          "original_form": "저도 맨날팔아서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100047309.1.38",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100047309.1.39",
          "form": "요새 좀",
          "original_form": "요새 좀",
          "speaker_id": "1",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100047309.1.40",
