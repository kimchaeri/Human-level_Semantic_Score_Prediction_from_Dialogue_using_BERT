      "utterance": [
        {
          "id": "MDRW2100044727.1.1",
          "form": "솔직히",
          "original_form": "솔직히",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044727.1.2",
          "form": "사무직 업무는 원래 단순반복이라",
          "original_form": "사무직 업무는 원래 단순반복이라",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044727.1.3",
          "form": "나중가면 내 일 다 기계가하는거 아닌가몰라 ㅋㅋㅋ",
          "original_form": "나중가면 내 일 다 기계가하는거 아닌가몰라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100044727.1.4",
          "form": "ㅋㅋㅋ 가능할듯",
          "original_form": "ㅋㅋㅋ 가능할듯",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044727.1.5",
          "form": "약간 기본 패턴만 주고",
          "original_form": "약간 기본 패턴만 주고",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044727.1.6",
          "form": "혹시 변칙이 있으면 가끔 관리자가 처리하고",
          "original_form": "혹시 변칙이 있으면 가끔 관리자가 처리하고",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044727.1.7",
          "form": "그정도만 되어도 훨씬 일 줄어들고 인간도 줄고..",
          "original_form": "그정도만 되어도 훨씬 일 줄어들고 인간도 줄고..",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100044727.1.8",
          "form": "그치...ㅋㅋㅋㅋㅋ",
          "original_form": "그치...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044727.1.9",
          "form": "그래서 사실 출생아는 좀 줄어드는게 맞다고 생각함",
          "original_form": "그래서 사실 출생아는 좀 줄어드는게 맞다고 생각함",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044727.1.10",
          "form": "어차피 일자리가 없다...",
          "original_form": "어차피 일자리가 없다...",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044727.1.11",
          "form": "ㅋㅋㅋㅋ 맞아..",
          "original_form": "ㅋㅋㅋㅋ 맞아..",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044727.1.12",
          "form": "글고 대학이랑 취업문 좀 넓어져야지..",
          "original_form": "글고 대학이랑 취업문 좀 넓어져야지..",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044727.1.13",
          "form": "사람 많아봤자 우리처럼",
          "original_form": "사람 많아봤자 우리처럼",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044727.1.14",
          "form": "애들이 뭔 고생이여",
          "original_form": "애들이 뭔 고생이여",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044727.1.15",
          "form": "취준하느라 고생만 할거...",
          "original_form": "취준하느라 고생만 할거...",
          "speaker_id": "2",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044727.1.16",
          "form": "ㅇㅇ..",
          "original_form": "ㅇㅇ..",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100044727.1.17",
          "form": "취준생은 진심으로 바란다",
          "original_form": "취준생은 진심으로 바란다",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044727.1.18",
          "form": "타노스 데려와라",
          "original_form": "타노스 데려와라",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044727.1.19",
          "form": "반만 날려보내자",
          "original_form": "반만 날려보내자",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044727.1.20",
          "form": "ㅋㅋㅋㅋ 극단적이잖아 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 극단적이잖아 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100044727.1.21",
          "form": "ㅋㅋ큐ㅠㅠ 반 나가면 반이 차지하던 자리 남잖아..",
          "original_form": "ㅋㅋ큐ㅠㅠ 반 나가면 반이 차지하던 자리 남잖아..",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044727.1.22",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044727.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아무리 그래도 그렇지 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아무리 그래도 그렇지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044727.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ 가끔.. 타노스는 천재다 그런 생각을 해...(대체",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 가끔.. 타노스는 천재다 그런 생각을 해...(대체",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044727.1.25",
          "form": "그렇지..참아야지...흑흑...",
          "original_form": "그렇지..참아야지...흑흑...",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044727.1.26",
          "form": "맞아 이성을 챙기자 취준생이여...",
          "original_form": "맞아 이성을 챙기자 취준생이여...",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044727.1.27",
          "form": "베이비붐세대 은퇴는 내 취업 이후일텐데ㅠ",
          "original_form": "베이비붐세대 은퇴는 내 취업 이후일텐데ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044727.1.28",
          "form": "허엉 백수로 살고 싶습니다 선생님....",
          "original_form": "허엉 백수로 살고 싶습니다 선생님....",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
