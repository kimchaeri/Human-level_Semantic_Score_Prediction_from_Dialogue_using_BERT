      "utterance": [
        {
          "id": "MDRW2100020747.1.1",
          "form": "점심뭐먹오?",
          "original_form": "점심뭐먹오?",
          "speaker_id": "1",
          "time": "20210828 12:00"
        },
        {
          "id": "MDRW2100020747.1.2",
          "form": "풀무원 들기름 메밀 막국수!",
          "original_form": "풀무원 들기름 메밀 막국수!",
          "speaker_id": "2",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020747.1.3",
          "form": "먹어본댜",
          "original_form": "먹어본댜",
          "speaker_id": "2",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020747.1.4",
          "form": "마싯나???!",
          "original_form": "마싯나???!",
          "speaker_id": "1",
          "time": "20210828 14:06"
        },
        {
          "id": "MDRW2100020747.1.5",
          "form": "흠",
          "original_form": "흠",
          "speaker_id": "2",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020747.1.6",
          "form": "맛은 있는데",
          "original_form": "맛은 있는데",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020747.1.7",
          "form": "내 취향은 아닌 것 같아",
          "original_form": "내 취향은 아닌 것 같아",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020747.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020747.1.9",
          "form": "나는 그냥 막국수가 더 맛있어",
          "original_form": "나는 그냥 막국수가 더 맛있어",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020747.1.10",
          "form": "양념장 있는 겅",
          "original_form": "양념장 있는 겅",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020747.1.11",
          "form": "그랴..?ㅠㅠ집에 냉면양념장 있지않오!?그거라도 넣어먹지ㅠㅠ",
          "original_form": "그랴..?ㅠㅠ집에 냉면양념장 있지않오!?그거라도 넣어먹지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100020747.1.12",
          "form": "마쟈",
          "original_form": "마쟈",
          "speaker_id": "2",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100020747.1.13",
          "form": "엄마는 넣어 먹었당",
          "original_form": "엄마는 넣어 먹었당",
          "speaker_id": "2",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100020747.1.14",
          "form": "뭔가 넣어먹기에는 먹다보면 끝나있었옹..",
          "original_form": "뭔가 넣어먹기에는 먹다보면 끝나있었옹..",
          "speaker_id": "2",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100020747.1.15",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100020747.1.16",
          "form": "악 양이 또 작구나..?",
          "original_form": "악 양이 또 작구나..?",
          "speaker_id": "1",
          "time": "20210828 19:20"
        },
        {
          "id": "MDRW2100020747.1.17",
          "form": "애매한 양인가보군...",
          "original_form": "애매한 양인가보군...",
          "speaker_id": "1",
          "time": "20210828 19:21"
        },
        {
          "id": "MDRW2100020747.1.18",
          "form": "웅 적당햇...",
          "original_form": "웅 적당햇...",
          "speaker_id": "2",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100020747.1.19",
          "form": "ㄹㅋㅋㅋㅋ",
          "original_form": "ㄹㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100020747.1.20",
          "form": "ㅋㅋㅋㅋㅋ나도 곧 머거야지...내거만 남앗지..?",
          "original_form": "ㅋㅋㅋㅋㅋ나도 곧 머거야지...내거만 남앗지..?",
          "speaker_id": "1",
          "time": "20210828 19:48"
        },
        {
          "id": "MDRW2100020747.1.21",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "2",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100020747.1.22",
          "form": "낼 아침으로 머거봥",
          "original_form": "낼 아침으로 머거봥",
          "speaker_id": "2",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100020747.1.23",
          "form": "그르까ㅋㅋㅋ근데 내일 아침 또 마음바뀔수도 이써..",
          "original_form": "그르까ㅋㅋㅋ근데 내일 아침 또 마음바뀔수도 이써..",
