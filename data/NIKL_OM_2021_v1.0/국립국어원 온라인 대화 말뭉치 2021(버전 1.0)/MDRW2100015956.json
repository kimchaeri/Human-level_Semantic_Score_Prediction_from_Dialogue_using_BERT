      "utterance": [
        {
          "id": "MDRW2100015956.1.1",
          "form": "요즘은 날씨가 많이 선선해져서 잘 때 이불이 없으면 춥더라구용",
          "original_form": "요즘은 날씨가 많이 선선해져서 잘 때 이불이 없으면 춥더라구용",
          "speaker_id": "2",
          "time": "20210826 03:45"
        },
        {
          "id": "MDRW2100015956.1.2",
          "form": "맞아요 감기조심하세요",
          "original_form": "맞아요 감기조심하세요",
          "speaker_id": "1",
          "time": "20210826 03:45"
        },
        {
          "id": "MDRW2100015956.1.3",
          "form": "그러고보니 이번 여름에는 여름감기 없이 지나갔어요",
          "original_form": "그러고보니 이번 여름에는 여름감기 없이 지나갔어요",
          "speaker_id": "2",
          "time": "20210826 03:45"
        },
        {
          "id": "MDRW2100015956.1.4",
          "form": "항상 일어나면 코찔찔이었는데",
          "original_form": "항상 일어나면 코찔찔이었는데",
          "speaker_id": "2",
          "time": "20210826 03:46"
        },
        {
          "id": "MDRW2100015956.1.5",
          "form": "저도 비염 있어서 콧물달고살아요ㅋㅋ",
          "original_form": "저도 비염 있어서 콧물달고살아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 03:46"
        },
        {
          "id": "MDRW2100015956.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 비염은 현대인의 기본 질환 수준인 것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 비염은 현대인의 기본 질환 수준인 것 같아요",
          "speaker_id": "2",
          "time": "20210826 03:46"
        },
        {
          "id": "MDRW2100015956.1.7",
          "form": "가을옷 꺼내셨나요?",
          "original_form": "가을옷 꺼내셨나요?",
          "speaker_id": "1",
          "time": "20210826 03:46"
        },
        {
          "id": "MDRW2100015956.1.8",
          "form": "저도 만성비염이라 ㅠㅠ 특히 봄되면 콧물이 멈추지않아요..",
          "original_form": "저도 만성비염이라 ㅠㅠ 특히 봄되면 콧물이 멈추지않아요..",
          "speaker_id": "2",
          "time": "20210826 03:47"
        },
        {
          "id": "MDRW2100015956.1.9",
          "form": "아직은 여름옷만 입고있어요",
          "original_form": "아직은 여름옷만 입고있어요",
          "speaker_id": "2",
          "time": "20210826 03:47"
        },
        {
          "id": "MDRW2100015956.1.10",
          "form": "대신 새로 사는 옷은 가을옷만 사게되는 것 같아요 ㅎㅎ",
          "original_form": "대신 새로 사는 옷은 가을옷만 사게되는 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 03:47"
        },
        {
          "id": "MDRW2100015956.1.11",
          "form": "하 미세먼지 미치죠",
          "original_form": "하 미세먼지 미치죠",
          "speaker_id": "1",
          "time": "20210826 03:47"
        },
        {
          "id": "MDRW2100015956.1.12",
          "form": "맞아요 미세먼지랑 꽃가루 ㅠㅠ 그래도 요즘은 마스크 쓰거 다녀서 밖에서는 오히려 예전보다 증상이 덜해요 ㅋㅋㅋ",
          "original_form": "맞아요 미세먼지랑 꽃가루 ㅠㅠ 그래도 요즘은 마스크 쓰거 다녀서 밖에서는 오히려 예전보다 증상이 덜해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 03:48"
        },
        {
          "id": "MDRW2100015956.1.13",
          "form": "혹시 안졸리는 알레르기약이나 비염약 드셔보셨나요?",
          "original_form": "혹시 안졸리는 알레르기약이나 비염약 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210826 03:48"
        },
        {
          "id": "MDRW2100015956.1.14",
          "form": "저는 졸리는 것 밖에 못먹어봤는데 확실히 일상생활에 지장이 크더라구요ㅠㅠ",
          "original_form": "저는 졸리는 것 밖에 못먹어봤는데 확실히 일상생활에 지장이 크더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 03:49"
        },
        {
          "id": "MDRW2100015956.1.15",
          "form": "아니요 그냥 비염약 자체가 잠와요",
          "original_form": "아니요 그냥 비염약 자체가 잠와요",
          "speaker_id": "1",
          "time": "20210826 03:49"
        },
        {
          "id": "MDRW2100015956.1.16",
          "form": "비염 수술 해보셨나요?",
          "original_form": "비염 수술 해보셨나요?",
          "speaker_id": "1",
          "time": "20210826 03:49"
        },
        {
          "id": "MDRW2100015956.1.17",
          "form": "저는 수술까지는 생각 안해봤는데 친구가 비염수술했어요",
          "original_form": "저는 수술까지는 생각 안해봤는데 친구가 비염수술했어요",
          "speaker_id": "2",
          "time": "20210826 03:50"
        },
        {
          "id": "MDRW2100015956.1.18",
          "form": "하기 전보다 더 낫다고 하더라구요",
          "original_form": "하기 전보다 더 낫다고 하더라구요",
          "speaker_id": "2",
          "time": "20210826 03:50"
        },
        {
          "id": "MDRW2100015956.1.19",
          "form": "좋아하는 계절 있으세요?",
          "original_form": "좋아하는 계절 있으세요?",
          "speaker_id": "1",
          "time": "20210826 03:51"
        },
        {
          "id": "MDRW2100015956.1.20",
          "form": "저는 가을이 제일 좋아요~",
          "original_form": "저는 가을이 제일 좋아요~",
          "speaker_id": "2",
          "time": "20210826 03:52"
        },
        {
          "id": "MDRW2100015956.1.21",
          "form": "선선하고 하늘도 파래서요!",
          "original_form": "선선하고 하늘도 파래서요!",
          "speaker_id": "2",
          "time": "20210826 03:52"
        }
      ]
    }
  ]
