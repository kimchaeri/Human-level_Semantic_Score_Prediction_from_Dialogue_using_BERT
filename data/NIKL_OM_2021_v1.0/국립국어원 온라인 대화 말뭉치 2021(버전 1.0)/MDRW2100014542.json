      "utterance": [
        {
          "id": "MDRW2100014542.1.1",
          "form": "난 화장품으로는 크게 효과 느낀적이 별로 없는듯",
          "original_form": "난 화장품으로는 크게 효과 느낀적이 별로 없는듯",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014542.1.2",
          "form": "리뉴얼 전에 아크 앰플은 쫌 효과 봤었는데",
          "original_form": "리뉴얼 전에 아크 앰플은 쫌 효과 봤었는데",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014542.1.3",
          "form": "비타민앰플 잘 맞나보다",
          "original_form": "비타민앰플 잘 맞나보다",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014542.1.4",
          "form": "그른가ㅋㅋ 간혹 비타민 앰플 따갑다는 후기 많이 봤는데 난 그런건 없네ㅋㅋ",
          "original_form": "그른가ㅋㅋ 간혹 비타민 앰플 따갑다는 후기 많이 봤는데 난 그런건 없네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014542.1.5",
          "form": "좋구만 ㅋㅋㅋ",
          "original_form": "좋구만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014542.1.6",
          "form": "아 피부과 한번 맛들이니까 또 관리받고싶다ㅋㅋㅋ",
          "original_form": "아 피부과 한번 맛들이니까 또 관리받고싶다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014542.1.7",
          "form": "다들 그럴거여 ㅋㅋㅋㅋ 돈이 좋지",
          "original_form": "다들 그럴거여 ㅋㅋㅋㅋ 돈이 좋지",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014542.1.8",
          "form": "그니까 그 좋은돈 벌고 싶은데 능력도 없고 건강도 없네ㅋㅋㅋㅋ",
          "original_form": "그니까 그 좋은돈 벌고 싶은데 능력도 없고 건강도 없네ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014542.1.9",
          "form": "돈벌어도 피부과 손떨려서 가겠나 ㅋㅋㅋ",
          "original_form": "돈벌어도 피부과 손떨려서 가겠나 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014542.1.10",
          "form": "그건 그르타ㅋㅋㅋ 비싸긴 비싸지",
          "original_form": "그건 그르타ㅋㅋㅋ 비싸긴 비싸지",
          "speaker_id": "1",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014542.1.11",
          "form": "머리자르고 싶다 ㅋㅋㅋ",
          "original_form": "머리자르고 싶다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014542.1.12",
          "form": "요즘 머리 자르는데 얼마징",
          "original_form": "요즘 머리 자르는데 얼마징",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014542.1.13",
          "form": "글쎄 나 자를때 얼마였드라 기억이 ㅋㅋㅋㅋㅋ",
          "original_form": "글쎄 나 자를때 얼마였드라 기억이 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
