      "utterance": [
        {
          "id": "MDRW2100012557.1.1",
          "form": "백신 주사 맞으셨나요?",
          "original_form": "백신 주사 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100012557.1.2",
          "form": "네 이번 주말에 2차 맞을 예정이예요",
          "original_form": "네 이번 주말에 2차 맞을 예정이예요",
          "speaker_id": "2",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100012557.1.3",
          "form": "저는 2주 후에 1차 예정이에요. 뭐 맞으셨어요?",
          "original_form": "저는 2주 후에 1차 예정이에요. 뭐 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100012557.1.4",
          "form": "대답을 보냈다고 생각했는데 에러가 났나봐요",
          "original_form": "대답을 보냈다고 생각했는데 에러가 났나봐요",
          "speaker_id": "2",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100012557.1.5",
          "form": "저는 어제 화이자 2차접종 했어요",
          "original_form": "저는 어제 화이자 2차접종 했어요",
          "speaker_id": "2",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100012557.1.6",
          "form": "어떠신가요?",
          "original_form": "어떠신가요?",
          "speaker_id": "1",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100012557.1.7",
          "form": "음 괜찮은 것 같았느뉘ㅣ",
          "original_form": "음 괜찮은 것 같았느뉘ㅣ",
          "speaker_id": "2",
          "time": "20210904 13:39"
        },
        {
          "id": "MDRW2100012557.1.8",
          "form": "는데",
          "original_form": "는데",
          "speaker_id": "2",
          "time": "20210904 13:39"
        },
        {
          "id": "MDRW2100012557.1.9",
          "form": "24시간 지나고나니 몸살기가 좀...",
          "original_form": "24시간 지나고나니 몸살기가 좀...",
          "speaker_id": "2",
          "time": "20210904 13:39"
        },
        {
          "id": "MDRW2100012557.1.10",
          "form": "약 먹고 쉬셔야 하겠네요ㅠ",
          "original_form": "약 먹고 쉬셔야 하겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210904 13:40"
        },
        {
          "id": "MDRW2100012557.1.11",
          "form": "안그래도 약 먹고 뒹굴거리고 있어요ㅎㅎ",
          "original_form": "안그래도 약 먹고 뒹굴거리고 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 13:41"
        },
        {
          "id": "MDRW2100012557.1.12",
          "form": "저도 걱정은 좀 되는데.. 빨리 맞고 싶네요 ㅎㅎ",
          "original_form": "저도 걱정은 좀 되는데.. 빨리 맞고 싶네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:45"
        },
        {
          "id": "MDRW2100012557.1.13",
          "form": "아 아직 1차접종 안하셨다고 했죠?",
          "original_form": "아 아직 1차접종 안하셨다고 했죠?",
          "speaker_id": "2",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100012557.1.14",
          "form": "화이자나 모더나 중에 뭔지 결정됐나요?",
          "original_form": "화이자나 모더나 중에 뭔지 결정됐나요?",
          "speaker_id": "2",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100012557.1.15",
          "form": "화이자 또는 모더나라고 써 있네요ㅎㅎ",
          "original_form": "화이자 또는 모더나라고 써 있네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:47"
        },
        {
          "id": "MDRW2100012557.1.16",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210904 13:47"
        },
        {
          "id": "MDRW2100012557.1.17",
          "form": "모더나가 예방효과는 더 좋다고 하던데",
          "original_form": "모더나가 예방효과는 더 좋다고 하던데",
          "speaker_id": "2",
          "time": "20210904 13:47"
        },
        {
          "id": "MDRW2100012557.1.18",
          "form": "근데 면역반응은 더 심하대요ㅜ",
          "original_form": "근데 면역반응은 더 심하대요ㅜ",
          "speaker_id": "2",
          "time": "20210904 13:47"
        },
        {
          "id": "MDRW2100012557.1.19",
          "form": "으 뭐가 됐든 팔만 좀 아프다 지나갔음 좋겠네요ㅠ",
          "original_form": "으 뭐가 됐든 팔만 좀 아프다 지나갔음 좋겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210904 13:56"
        },
        {
          "id": "MDRW2100012557.1.20",
          "form": "네네 무사히 지나가길",
          "original_form": "네네 무사히 지나가길",
          "speaker_id": "2",
          "time": "20210904 13:57"
        }
      ]
