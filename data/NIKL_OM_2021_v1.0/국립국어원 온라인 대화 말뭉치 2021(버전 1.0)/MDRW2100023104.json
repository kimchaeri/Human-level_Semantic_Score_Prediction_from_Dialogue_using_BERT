      "utterance": [
        {
          "id": "MDRW2100023104.1.1",
          "form": "ㅎㅎname1은 알바 많이해보셨을것같애요ㅎ",
          "original_form": "ㅎㅎ&name1&은 알바 많이해보셨을것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100023104.1.2",
          "form": "음 많이 했죠 ㅋ",
          "original_form": "음 많이 했죠 ㅋ",
          "speaker_id": "1",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023104.1.3",
          "form": "오ㅎ 정답이네여ㅎㅎ",
          "original_form": "오ㅎ 정답이네여ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 21:45"
        },
        {
          "id": "MDRW2100023104.1.4",
          "form": "학교 다닐때  편의점 알바는 제일 기본이고 ㅎ",
          "original_form": "학교 다닐때  편의점 알바는 제일 기본이고 ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100023104.1.5",
          "form": "저 중3때부터 알바 했는데  그땐 피아노 학원에서",
          "original_form": "저 중3때부터 알바 했는데  그땐 피아노 학원에서",
          "speaker_id": "1",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100023104.1.6",
          "form": "유치원~ 초딩  이론수업이랑  피아놀 가르쳤어요",
          "original_form": "유치원~ 초딩  이론수업이랑  피아놀 가르쳤어요",
          "speaker_id": "1",
          "time": "20210829 21:46"
        },
        {
          "id": "MDRW2100023104.1.7",
          "form": "원장님이 ㅋㅋㅋ 약간 날라리셨어요\r\n어링애들은 제가 다 맡앗던거 같아요",
          "original_form": "원장님이 ㅋㅋㅋ 약간 날라리셨어요\r\n어링애들은 제가 다 맡앗던거 같아요",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023104.1.8",
          "form": "우와ㅎ",
          "original_form": "우와ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023104.1.9",
          "form": "엄청 많이하셨군요ㅎ",
          "original_form": "엄청 많이하셨군요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023104.1.10",
          "form": "아 시골이긴했는대 원생수 120명 이엿어요",
          "original_form": "아 시골이긴했는대 원생수 120명 이엿어요",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023104.1.11",
          "form": "피아노 잘하셨으니깐 그쪽으로 알바 하셨군요ㅎ",
          "original_form": "피아노 잘하셨으니깐 그쪽으로 알바 하셨군요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023104.1.12",
          "form": "우와 엄청 많네요ㅎ",
          "original_form": "우와 엄청 많네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023104.1.13",
          "form": "너무 원장이 양아치였네요ㅎ",
          "original_form": "너무 원장이 양아치였네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023104.1.14",
          "form": "근데 그중 90퍼를 제가 맡아서",
          "original_form": "근데 그중 90퍼를 제가 맡아서",
          "speaker_id": "1",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023104.1.15",
          "form": "방학때 8시에가면  2시까지 1차 (초딩)\r\n4~6시까지 (유치부)",
          "original_form": "방학때 8시에가면  2시까지 1차 (초딩)\r\n4~6시까지 (유치부)",
          "speaker_id": "1",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023104.1.16",
          "form": "읔 근데 30만원 +  제 학원값은  공짜엿죠",
          "original_form": "읔 근데 30만원 +  제 학원값은  공짜엿죠",
          "speaker_id": "1",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023104.1.17",
          "form": "우와ㅎ",
          "original_form": "우와ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023104.1.18",
          "form": "근데 아 원생수 많아서 지금 생각해보면",
          "original_form": "근데 아 원생수 많아서 지금 생각해보면",
          "speaker_id": "1",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023104.1.19",
          "form": "너무 짜네요ㅎ",
          "original_form": "너무 짜네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023104.1.20",
          "form": "신고감이엿음 ㅋㅋ",
          "original_form": "신고감이엿음 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023104.1.21",
          "form": "글죠ㅎ",
          "original_form": "글죠ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023104.1.22",
          "form": "그래도 그때부터 다르셨군요ㅎ",
          "original_form": "그래도 그때부터 다르셨군요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023104.1.23",
          "form": "학원비를 아끼시면서 부수입도 챙기셨네요ㅎ",
          "original_form": "학원비를 아끼시면서 부수입도 챙기셨네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023104.1.24",
          "form": "아 전 집안이좀 어려워서",
          "original_form": "아 전 집안이좀 어려워서",
          "speaker_id": "1",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023104.1.25",
          "form": "스스로 하기 됏어요 ㅅㅎ",
          "original_form": "스스로 하기 됏어요 ㅅㅎ",
          "speaker_id": "1",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023104.1.26",
          "form": "아아ㅠ",
          "original_form": "아아ㅠ",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023104.1.27",
          "form": "그러셨구나ㅠ",
          "original_form": "그러셨구나ㅠ",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023104.1.28",
          "form": "아빠가 사고로 다치셔서 엄마가 실질적 가장이셧고",
          "original_form": "아빠가 사고로 다치셔서 엄마가 실질적 가장이셧고",
          "speaker_id": "1",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023104.1.29",
          "form": "에고에고ㅠ",
          "original_form": "에고에고ㅠ",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100023104.1.30",
          "form": "고생많으셨겟네요ㅍ",
          "original_form": "고생많으셨겟네요ㅍ",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023104.1.31",
          "form": "저도 그맘이해해요ㅠ",
          "original_form": "저도 그맘이해해요ㅠ",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023104.1.32",
          "form": "전 스스로 알바 +장학금 타서 대학갓구요",
          "original_form": "전 스스로 알바 +장학금 타서 대학갓구요",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023104.1.33",
          "form": "우와...",
          "original_form": "우와...",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100023104.1.34",
          "form": "대단하시다 정말",
          "original_form": "대단하시다 정말",
          "speaker_id": "2",
          "time": "20210829 21:51"
        }
      ]
