      "utterance": [
        {
          "id": "MDRW2100019597.1.1",
          "form": "정리 잘 하시나요?",
          "original_form": "정리 잘 하시나요?",
          "speaker_id": "1",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019597.1.2",
          "form": "약간 강박이 있어요 ㅋㅋㅋㅋ",
          "original_form": "약간 강박이 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100019597.1.3",
          "form": "물건은 제자리에",
          "original_form": "물건은 제자리에",
          "speaker_id": "2",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100019597.1.4",
          "form": "약간 작정하고 치우는 타입인데 쓰던 물건은 늘 그 자리에 있어야해요 ㅎ",
          "original_form": "약간 작정하고 치우는 타입인데 쓰던 물건은 늘 그 자리에 있어야해요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100019597.1.5",
          "form": "정리하는 거 좋아하세요?",
          "original_form": "정리하는 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100019597.1.6",
          "form": "네 좋아해요 잘은 못해도요",
          "original_form": "네 좋아해요 잘은 못해도요",
          "speaker_id": "1",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100019597.1.7",
          "form": "정리하는 거 좋아하시면 기본적으로 늘 깔끔할 것 같아요!",
          "original_form": "정리하는 거 좋아하시면 기본적으로 늘 깔끔할 것 같아요!",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019597.1.8",
          "form": "여름옷 정리 하셨나요?",
          "original_form": "여름옷 정리 하셨나요?",
          "speaker_id": "2",
          "time": "20210827 21:14"
        },
        {
          "id": "MDRW2100019597.1.9",
          "form": "부족하지만 깔끔하려고 노력은 해요",
          "original_form": "부족하지만 깔끔하려고 노력은 해요",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019597.1.10",
          "form": "아직이요 슬슬해야죠",
          "original_form": "아직이요 슬슬해야죠",
          "speaker_id": "1",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100019597.1.11",
          "form": "장마가 얼른 끝나야 빨래해서 다 넣을텐데",
          "original_form": "장마가 얼른 끝나야 빨래해서 다 넣을텐데",
          "speaker_id": "2",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100019597.1.12",
          "form": "날씨가 참 안 도와주네요",
          "original_form": "날씨가 참 안 도와주네요",
          "speaker_id": "2",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100019597.1.13",
          "form": "맞아요 비가 계속 예보에 들어서 있어서 빨래하기 불편해요",
          "original_form": "맞아요 비가 계속 예보에 들어서 있어서 빨래하기 불편해요",
          "speaker_id": "1",
          "time": "20210827 21:25"
        },
        {
          "id": "MDRW2100019597.1.14",
          "form": "이런 시기에는 건조기도 소용 없어서 더 그렇죠ㅠ",
          "original_form": "이런 시기에는 건조기도 소용 없어서 더 그렇죠ㅠ",
          "speaker_id": "2",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100019597.1.15",
          "form": "대청소는 1년에 몇 번 정도 하세요?",
          "original_form": "대청소는 1년에 몇 번 정도 하세요?",
          "speaker_id": "2",
          "time": "20210827 21:30"
        },
        {
          "id": "MDRW2100019597.1.16",
          "form": "2번 정도해요. 봄가을 날씨 좋을 때요",
          "original_form": "2번 정도해요. 봄가을 날씨 좋을 때요",
          "speaker_id": "1",
          "time": "20210827 21:31"
        },
        {
          "id": "MDRW2100019597.1.17",
          "form": "요즘 청소도구가 잘 나와서 대청소도 생각보다 금방 끝나는 것 같아요",
          "original_form": "요즘 청소도구가 잘 나와서 대청소도 생각보다 금방 끝나는 것 같아요",
          "speaker_id": "2",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100019597.1.18",
          "form": "맞아요 간편한게 참 많아요",
          "original_form": "맞아요 간편한게 참 많아요",
          "speaker_id": "1",
          "time": "20210827 21:38"
        },
        {
          "id": "MDRW2100019597.1.19",
          "form": "간편해서 하나씩 사다 보니까 너무 많이 산 것 같네요",
          "original_form": "간편해서 하나씩 사다 보니까 너무 많이 산 것 같네요",
          "speaker_id": "2",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019597.1.20",
          "form": "언제 다 쓰나 싶어요 ㅎ",
          "original_form": "언제 다 쓰나 싶어요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 21:42"
        },
        {
          "id": "MDRW2100019597.1.21",
          "form": "쟁여도 금방 쓰게 되더라구요",
          "original_form": "쟁여도 금방 쓰게 되더라구요",
          "speaker_id": "1",
          "time": "20210827 21:47"
        }
      ]
