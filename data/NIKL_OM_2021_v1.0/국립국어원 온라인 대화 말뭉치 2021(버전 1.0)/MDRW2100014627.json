      "utterance": [
        {
          "id": "MDRW2100014627.1.1",
          "form": "식사는 하셨어요?!",
          "original_form": "식사는 하셨어요?!",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014627.1.2",
          "form": "지금 퇴근중이예요 ㅠ",
          "original_form": "지금 퇴근중이예요 ㅠ",
          "speaker_id": "2",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014627.1.3",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014627.1.4",
          "form": "그럼 아직 ㅠㅠㅠㅠㅠ?",
          "original_form": "그럼 아직 ㅠㅠㅠㅠㅠ?",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014627.1.5",
          "form": "네 지금 집에 거의 도착해가요 ㅠㅠㅠㅠㅠㅠㅜㅜㅜㅜㅜㅜㅠ",
          "original_form": "네 지금 집에 거의 도착해가요 ㅠㅠㅠㅠㅠㅠㅜㅜㅜㅜㅜㅜㅠ",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014627.1.6",
          "form": "저녁식사 하셨어요???",
          "original_form": "저녁식사 하셨어요???",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014627.1.7",
          "form": "네 저는 비빔국수..!",
          "original_form": "네 저는 비빔국수..!",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014627.1.8",
          "form": "너무 매워서 속이 쓰려요",
          "original_form": "너무 매워서 속이 쓰려요",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014627.1.9",
          "form": "비빔국수 존맛탱이죠",
          "original_form": "비빔국수 존맛탱이죠",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014627.1.10",
          "form": "많이 맵게드셨나보네 ㅠㅜ",
          "original_form": "많이 맵게드셨나보네 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014627.1.11",
          "form": "팔도비빔면 소스를 사서 써 봤는데",
          "original_form": "팔도비빔면 소스를 사서 써 봤는데",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014627.1.12",
          "form": "너무 많이 넣었나봐요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무 많이 넣었나봐요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:06"
        },
        {
          "id": "MDRW2100014627.1.13",
          "form": "팔도네넴띤 소스 은근 맵죠",
          "original_form": "팔도네넴띤 소스 은근 맵죠",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014627.1.14",
          "form": "ㅠㅜㅜㅜㅜㅜㅠㅜㅠ",
          "original_form": "ㅠㅜㅜㅜㅜㅜㅠㅜㅠ",
          "speaker_id": "2",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014627.1.15",
          "form": "네넴띤 귀염",
          "original_form": "네넴띤 귀염",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014627.1.16",
          "form": "네 매운데 만능이에요",
          "original_form": "네 매운데 만능이에요",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014627.1.17",
          "form": "이거랑 섞으면 다 맛있는 것 같아요",
          "original_form": "이거랑 섞으면 다 맛있는 것 같아요",
          "speaker_id": "1",
          "time": "20210825 20:13"
        },
        {
          "id": "MDRW2100014627.1.18",
          "form": "물없는요리의 만능소스네여",
          "original_form": "물없는요리의 만능소스네여",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014627.1.19",
          "form": "라면스프는 국물요리 만능이구",
          "original_form": "라면스프는 국물요리 만능이구",
          "speaker_id": "2",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014627.1.20",
          "form": "크흐 라면스프",
          "original_form": "크흐 라면스프",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014627.1.21",
          "form": "맞아요 진짜 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 진짜 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014627.1.22",
          "form": "맛이 없을 수가 없는!",
          "original_form": "맛이 없을 수가 없는!",
          "speaker_id": "1",
          "time": "20210825 20:19"
