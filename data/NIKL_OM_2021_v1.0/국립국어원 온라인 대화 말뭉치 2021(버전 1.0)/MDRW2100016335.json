      "utterance": [
        {
          "id": "MDRW2100016335.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100016335.1.2",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "1",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100016335.1.3",
          "form": "아니요 종교는 없어요 ㅋㅋ",
          "original_form": "아니요 종교는 없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016335.1.4",
          "form": "종교 잇으세요?",
          "original_form": "종교 잇으세요?",
          "speaker_id": "2",
          "time": "20210826 12:16"
        },
        {
          "id": "MDRW2100016335.1.5",
          "form": "아니요저도없어요",
          "original_form": "아니요저도없어요",
          "speaker_id": "1",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016335.1.6",
          "form": "나중에 종교믿을 생각은 있으셔요?",
          "original_form": "나중에 종교믿을 생각은 있으셔요?",
          "speaker_id": "2",
          "time": "20210826 12:19"
        },
        {
          "id": "MDRW2100016335.1.7",
          "form": "지금은없어요! 그치만 마음이 힘들거나 기대고싶을곳이필요할때 긍정적 기능은 한다고생각해요",
          "original_form": "지금은없어요! 그치만 마음이 힘들거나 기대고싶을곳이필요할때 긍정적 기능은 한다고생각해요",
          "speaker_id": "1",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016335.1.8",
          "form": "제가 종교를 가질일이없었으면 좋겠어요ㅎ",
          "original_form": "제가 종교를 가질일이없었으면 좋겠어요ㅎ",
          "speaker_id": "1",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100016335.1.9",
          "form": "네 ㅋㅋㅋㅋㅋㅋ그건그래요",
          "original_form": "네 ㅋㅋㅋㅋㅋㅋ그건그래요",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016335.1.10",
          "form": "저도 지금은 없는데",
          "original_form": "저도 지금은 없는데",
          "speaker_id": "2",
          "time": "20210826 13:07"
        },
        {
          "id": "MDRW2100016335.1.11",
          "form": "나이들어서는",
          "original_form": "나이들어서는",
          "speaker_id": "2",
          "time": "20210826 13:07"
        },
        {
          "id": "MDRW2100016335.1.12",
          "form": "좋을것같아요",
          "original_form": "좋을것같아요",
          "speaker_id": "2",
          "time": "20210826 13:07"
        },
        {
          "id": "MDRW2100016335.1.13",
          "form": "코로나시대에 종교모임 어떻게생각하세요?",
          "original_form": "코로나시대에 종교모임 어떻게생각하세요?",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100016335.1.14",
          "form": "학교도 온라인수업을하는 시국에 방역수칙도지키지않고 오프라인 예배를 강요하는 집단은 범법자라고생각이들어요.",
          "original_form": "학교도 온라인수업을하는 시국에 방역수칙도지키지않고 오프라인 예배를 강요하는 집단은 범법자라고생각이들어요.",
          "speaker_id": "1",
          "time": "20210901 20:15"
        },
        {
          "id": "MDRW2100016335.1.15",
          "form": "님은 어떻게 생각하세요?",
          "original_form": "님은 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210901 20:16"
        },
        {
          "id": "MDRW2100016335.1.16",
          "form": "집단감염이 일어난곳이 한두군데가아닌데 솔직히 이단이나 기독교나 다른게 뭔지모르겠어요",
          "original_form": "집단감염이 일어난곳이 한두군데가아닌데 솔직히 이단이나 기독교나 다른게 뭔지모르겠어요",
          "speaker_id": "1",
          "time": "20210901 20:17"
        },
        {
          "id": "MDRW2100016335.1.17",
          "form": "네 저도 그렇게생각해요ㅜ",
          "original_form": "네 저도 그렇게생각해요ㅜ",
          "speaker_id": "2",
          "time": "20210901 20:18"
        },
        {
          "id": "MDRW2100016335.1.18",
          "form": "왜그렇게 모이려고하는지ㅜ",
          "original_form": "왜그렇게 모이려고하는지ㅜ",
          "speaker_id": "2",
          "time": "20210901 20:18"
        },
        {
          "id": "MDRW2100016335.1.19",
          "form": "돈때문이겠죠? ㅎ",
          "original_form": "돈때문이겠죠? ㅎ",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100016335.1.20",
          "form": "그런거겠죠ㅜ진짜 노이해입니다.ㅜ",
          "original_form": "그런거겠죠ㅜ진짜 노이해입니다.ㅜ",
          "speaker_id": "2",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100016335.1.21",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210901 20:22"
        },
        {
          "id": "MDRW2100016335.1.22",
          "form": "아는 지인은 그래도 안나가고",
          "original_form": "아는 지인은 그래도 안나가고",
          "speaker_id": "2",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100016335.1.23",
          "form": "집에서만하더라구요.",
          "original_form": "집에서만하더라구요.",
          "speaker_id": "2",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100016335.1.24",
          "form": "나가는사람들때문에 다른교인들이 욕먹어요",
          "original_form": "나가는사람들때문에 다른교인들이 욕먹어요",
          "speaker_id": "2",
          "time": "20210901 20:23"
        },
        {
          "id": "MDRW2100016335.1.25",
          "form": "순수하게 믿음 있는사람들은 온라인으로",
          "original_form": "순수하게 믿음 있는사람들은 온라인으로",
          "speaker_id": "1",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100016335.1.26",
          "form": "참가하는것같아요",
          "original_form": "참가하는것같아요",
          "speaker_id": "1",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100016335.1.27",
          "form": "네 맞아요ㅋ",
          "original_form": "네 맞아요ㅋ",
          "speaker_id": "2",
          "time": "20210901 20:24"
        }
      ]
    }
  ]
