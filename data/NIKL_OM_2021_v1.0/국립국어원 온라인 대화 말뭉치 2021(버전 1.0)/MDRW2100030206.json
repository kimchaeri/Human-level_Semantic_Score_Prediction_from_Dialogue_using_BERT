        },
        {
          "id": "MDRW2100030206.1.2",
          "form": "수학여행 좋지 ㅋㅋ 난 제주도 갓엇어",
          "original_form": "수학여행 좋지 ㅋㅋ 난 제주도 갓엇어",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030206.1.3",
          "form": "수학여행으로 경주 가본적있어?",
          "original_form": "수학여행으로 경주 가본적있어?",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030206.1.4",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030206.1.5",
          "form": "재밌겠다",
          "original_form": "재밌겠다",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030206.1.6",
          "form": "경주 가봣어 초등학교때",
          "original_form": "경주 가봣어 초등학교때",
          "speaker_id": "2",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030206.1.7",
          "form": "나도 그랬어",
          "original_form": "나도 그랬어",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030206.1.8",
          "form": "근데 끔찍하지않았어?",
          "original_form": "근데 끔찍하지않았어?",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100030206.1.9",
          "form": "더운 날씨에 밖에있기 힘들지 ㅎㅎ",
          "original_form": "더운 날씨에 밖에있기 힘들지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030206.1.10",
          "form": "아침 7시에 일어나서 밤 11시까지 돌아다니잖아",
          "original_form": "아침 7시에 일어나서 밤 11시까지 돌아다니잖아",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030206.1.11",
          "form": "그래도 볼건 많잖아",
          "original_form": "그래도 볼건 많잖아",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030206.1.12",
          "form": "나 피곤해서 쓰러질뻔했어",
          "original_form": "나 피곤해서 쓰러질뻔했어",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030206.1.13",
          "form": "그렇긴한데 재미없잖아",
          "original_form": "그렇긴한데 재미없잖아",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030206.1.14",
          "form": "여행을 힐링여행이 짱이지",
          "original_form": "여행을 힐링여행이 짱이지",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030206.1.15",
          "form": "안그래?",
          "original_form": "안그래?",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030206.1.16",
          "form": "글쎄..",
          "original_form": "글쎄..",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030206.1.17",
          "form": "맛있는거 먹고 뭐 폭포같은거 보구",
          "original_form": "맛있는거 먹고 뭐 폭포같은거 보구",
          "speaker_id": "1",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030206.1.18",
          "form": "역사를 아는것도 중요하다구 봐",
          "original_form": "역사를 아는것도 중요하다구 봐",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100030206.1.19",
          "form": "그건 교과서로 보면 돼",
          "original_form": "그건 교과서로 보면 돼",
          "speaker_id": "1",
          "time": "20210903 22:57"
        }
