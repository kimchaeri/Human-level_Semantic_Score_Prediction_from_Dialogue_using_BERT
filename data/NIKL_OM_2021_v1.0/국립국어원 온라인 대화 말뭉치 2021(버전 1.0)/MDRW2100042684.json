        },
        {
          "id": "MDRW2100042684.1.3",
          "form": "우리 연구단에는 과자가 많아",
          "original_form": "우리 연구단에는 과자가 많아",
          "speaker_id": "2",
          "time": "20210907 01:18"
        },
        {
          "id": "MDRW2100042684.1.4",
          "form": "좋은 회사 네",
          "original_form": "좋은 회사 네",
          "speaker_id": "1",
          "time": "20210907 01:18"
        },
        {
          "id": "MDRW2100042684.1.5",
          "form": "매주 금요일 마다 이마트에서 사서 도착해",
          "original_form": "매주 금요일 마다 이마트에서 사서 도착해",
          "speaker_id": "2",
          "time": "20210907 01:18"
        },
        {
          "id": "MDRW2100042684.1.6",
          "form": "주로 어떤 과자니",
          "original_form": "주로 어떤 과자니",
          "speaker_id": "1",
          "time": "20210907 01:18"
        },
        {
          "id": "MDRW2100042684.1.7",
          "form": "오뜨 가 맨날 포함되어 있어",
          "original_form": "오뜨 가 맨날 포함되어 있어",
          "speaker_id": "2",
          "time": "20210907 01:19"
        },
        {
          "id": "MDRW2100042684.1.8",
          "form": "다른 맛있는 건 없나",
          "original_form": "다른 맛있는 건 없나",
          "speaker_id": "1",
          "time": "20210907 01:19"
        },
        {
          "id": "MDRW2100042684.1.9",
          "form": "오예스도 인기가 많아",
          "original_form": "오예스도 인기가 많아",
          "speaker_id": "2",
          "time": "20210907 01:19"
        },
        {
          "id": "MDRW2100042684.1.10",
          "form": "너는 오에스 싫어하잖아",
          "original_form": "너는 오에스 싫어하잖아",
          "speaker_id": "1",
          "time": "20210907 01:19"
        },
        {
          "id": "MDRW2100042684.1.11",
          "form": "나는 엄마 손 파이을 좋아해",
          "original_form": "나는 엄마 손 파이을 좋아해",
          "speaker_id": "2",
          "time": "20210907 01:19"
        },
        {
          "id": "MDRW2100042684.1.12",
          "form": "초코맛 다른건 없니",
          "original_form": "초코맛 다른건 없니",
          "speaker_id": "1",
          "time": "20210907 01:20"
        },
        {
          "id": "MDRW2100042684.1.13",
          "form": "초코송이가 있더라",
          "original_form": "초코송이가 있더라",
          "speaker_id": "2",
          "time": "20210907 01:20"
        },
        {
          "id": "MDRW2100042684.1.14",
          "form": "그건 별로네",
          "original_form": "그건 별로네",
          "speaker_id": "1",
          "time": "20210907 01:20"
        },
        {
          "id": "MDRW2100042684.1.15",
          "form": "젤리도 맛이 있어",
          "original_form": "젤리도 맛이 있어",
          "speaker_id": "2",
