      "utterance": [
        {
          "id": "MDRW2100041620.1.1",
          "form": "육전이요?",
          "original_form": "육전이요?",
          "speaker_id": "2",
          "time": "20210906 13:16"
        },
        {
          "id": "MDRW2100041620.1.2",
          "form": "네 육전요!",
          "original_form": "네 육전요!",
          "speaker_id": "1",
          "time": "20210906 13:19"
        },
        {
          "id": "MDRW2100041620.1.3",
          "form": "손 많이 가지 않아요?",
          "original_form": "손 많이 가지 않아요?",
          "speaker_id": "2",
          "time": "20210906 13:19"
        },
        {
          "id": "MDRW2100041620.1.4",
          "form": "그냥 소고기 부채살 사서 밀가루 계란물 입히면 끝이에요!",
          "original_form": "그냥 소고기 부채살 사서 밀가루 계란물 입히면 끝이에요!",
          "speaker_id": "1",
          "time": "20210906 13:19"
        },
        {
          "id": "MDRW2100041620.1.5",
          "form": "대단하신데요",
          "original_form": "대단하신데요",
          "speaker_id": "2",
          "time": "20210906 13:19"
        },
        {
          "id": "MDRW2100041620.1.6",
          "form": "요즘은 냉동으로 불고기보다 도톰하고 일정하게 썰어서 팔더라고요",
          "original_form": "요즘은 냉동으로 불고기보다 도톰하고 일정하게 썰어서 팔더라고요",
          "speaker_id": "1",
          "time": "20210906 13:19"
        },
        {
          "id": "MDRW2100041620.1.7",
          "form": "오 그런거 사서하면 쉽나요?",
          "original_form": "오 그런거 사서하면 쉽나요?",
          "speaker_id": "2",
          "time": "20210906 13:20"
        },
        {
          "id": "MDRW2100041620.1.8",
          "form": "불고기거리로 하는거보다 훨씬요!",
          "original_form": "불고기거리로 하는거보다 훨씬요!",
          "speaker_id": "1",
          "time": "20210906 13:20"
        },
        {
          "id": "MDRW2100041620.1.9",
          "form": "맞아요 불고기거린 너무 얇아서ㅠㅠ",
          "original_form": "맞아요 불고기거린 너무 얇아서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 13:20"
        },
        {
          "id": "MDRW2100041620.1.10",
          "form": "네 고기가 막 다 찢어지고 처치곤란ㅋㅋ",
          "original_form": "네 고기가 막 다 찢어지고 처치곤란ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:20"
        },
        {
          "id": "MDRW2100041620.1.11",
          "form": "아 육전 먹고싶네요 동그랑땡도 ㅎㅎ",
          "original_form": "아 육전 먹고싶네요 동그랑땡도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 13:20"
        },
        {
          "id": "MDRW2100041620.1.12",
          "form": "맞아요 그게 제일 맛있는 거 같아요",
          "original_form": "맞아요 그게 제일 맛있는 거 같아요",
          "speaker_id": "1",
          "time": "20210906 13:20"
        },
        {
          "id": "MDRW2100041620.1.13",
          "form": "고기니까요",
          "original_form": "고기니까요",
          "speaker_id": "2",
