      "utterance": [
        {
          "id": "MDRW2100022686.1.1",
          "form": "저는 해산물 잘 못먹어요",
          "original_form": "저는 해산물 잘 못먹어요",
          "speaker_id": "2",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022686.1.2",
          "form": "비링거 못먹어서요 ㅠㅠ",
          "original_form": "비링거 못먹어서요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022686.1.3",
          "form": "어머 그래요?",
          "original_form": "어머 그래요?",
          "speaker_id": "1",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022686.1.4",
          "form": "그럼 조개 같은 것도요?",
          "original_form": "그럼 조개 같은 것도요?",
          "speaker_id": "1",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022686.1.5",
          "form": "근대 회는 그래두 괜찮아서 ㅋㅋㅋㅋ",
          "original_form": "근대 회는 그래두 괜찮아서 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022686.1.6",
          "form": "조개는 조개구이먹어요!",
          "original_form": "조개는 조개구이먹어요!",
          "speaker_id": "2",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022686.1.7",
          "form": "다행이네요 회 알러지 있는 분도 있더라구요",
          "original_form": "다행이네요 회 알러지 있는 분도 있더라구요",
          "speaker_id": "1",
          "time": "20210829 16:59"
        },
        {
          "id": "MDRW2100022686.1.8",
          "form": "조개구이 무한리필 가고 싶어요 ㅜㅜ",
          "original_form": "조개구이 무한리필 가고 싶어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022686.1.9",
          "form": "크 맛잇겟다 ㅠㅠㅠ",
          "original_form": "크 맛잇겟다 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022686.1.10",
          "form": "오이도에는 치즈도 올려준다던데 ..",
          "original_form": "오이도에는 치즈도 올려준다던데 ..",
          "speaker_id": "1",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022686.1.11",
          "form": "치즈는 추가하면되지않아요!?",
          "original_form": "치즈는 추가하면되지않아요!?",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022686.1.12",
          "form": "매번 치즈먹은거같응뎈ㅋㅋㅋ",
          "original_form": "매번 치즈먹은거같응뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022686.1.13",
          "form": "아 그래요? 잘 몰라요 조개구이는 ㅜㅜ",
          "original_form": "아 그래요? 잘 몰라요 조개구이는 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 17:00"
        },
        {
          "id": "MDRW2100022686.1.14",
          "form": "무한리필 집에서도 추가하면 치즈 무한리필이에요?",
          "original_form": "무한리필 집에서도 추가하면 치즈 무한리필이에요?",
          "speaker_id": "1",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022686.1.15",
          "form": "저도 그냥 바닷가 놀러갈때먹구그래요 ㅋㅋㅋㅋ",
          "original_form": "저도 그냥 바닷가 놀러갈때먹구그래요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022686.1.16",
          "form": "흠 그건 가게마다 다를거같긴해욬ㅋㅋㅋㅋ",
          "original_form": "흠 그건 가게마다 다를거같긴해욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022686.1.17",
          "form": "저도 끽해야 굴 구워먹은 게 다네요..",
          "original_form": "저도 끽해야 굴 구워먹은 게 다네요..",
          "speaker_id": "1",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022686.1.18",
          "form": "포항갔을때 먹은거기억나네요",
          "original_form": "포항갔을때 먹은거기억나네요",
          "speaker_id": "2",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022686.1.19",
          "form": "호호 불면서 칼로 슥 집어넣고 쓱!",
          "original_form": "호호 불면서 칼로 슥 집어넣고 쓱!",
          "speaker_id": "1",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022686.1.20",
          "form": "거기는 수제비 무한리필이엇어요!!",
          "original_form": "거기는 수제비 무한리필이엇어요!!",
          "speaker_id": "2",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022686.1.21",
          "form": "아주잘어울렷어요",
          "original_form": "아주잘어울렷어요",
          "speaker_id": "2",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022686.1.22",
          "form": "그리고 노로바이러스 걸리면 ...ㅎ 다시 생각 안나죠",
          "original_form": "그리고 노로바이러스 걸리면 ...ㅎ 다시 생각 안나죠",
          "speaker_id": "1",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022686.1.23",
          "form": "오 수제비 ㅋㅋ 칼국수 이런거 무한리필 서비스 많이 하더라구요",
          "original_form": "오 수제비 ㅋㅋ 칼국수 이런거 무한리필 서비스 많이 하더라구요",
          "speaker_id": "1",
          "time": "20210829 17:01"
        },
        {
          "id": "MDRW2100022686.1.24",
          "form": "ㅋㅋㅋㅋ맛아요 겨울에 먹으면 딱",
          "original_form": "ㅋㅋㅋㅋ맛아요 겨울에 먹으면 딱",
          "speaker_id": "2",
          "time": "20210829 17:02"
        },
        {
          "id": "MDRW2100022686.1.25",
          "form": "굴이 노로바이러스 많이걸리죠",
          "original_form": "굴이 노로바이러스 많이걸리죠",
          "speaker_id": "2",
          "time": "20210829 17:02"
        },
        {
          "id": "MDRW2100022686.1.26",
          "form": "굴도 드세요??",
          "original_form": "굴도 드세요??",
          "speaker_id": "2",
          "time": "20210829 17:02"
        },
        {
          "id": "MDRW2100022686.1.27",
          "form": "저희 가족은 명절에 모이면",
          "original_form": "저희 가족은 명절에 모이면",
          "speaker_id": "1",
          "time": "20210829 17:02"
        },
        {
          "id": "MDRW2100022686.1.28",
          "form": "마당에서 연탄 피워서 굴 구워 먹어요",
          "original_form": "마당에서 연탄 피워서 굴 구워 먹어요",
          "speaker_id": "1",
          "time": "20210829 17:02"
        }
      ]
