      "utterance": [
        {
          "id": "MDRW2100025313.1.1",
          "form": "그거 창귀 가사에도",
          "original_form": "그거 창귀 가사에도",
          "speaker_id": "2",
          "time": "20210901 09:05"
        },
        {
          "id": "MDRW2100025313.1.2",
          "form": "그 특히 귀신이 말할때 원숭이같은게 고개 갸웃거리면서",
          "original_form": "그 특히 귀신이 말할때 원숭이같은게 고개 갸웃거리면서",
          "speaker_id": "1",
          "time": "20210901 09:05"
        },
        {
          "id": "MDRW2100025313.1.3",
          "form": "물귀신 얘기 나왔던거같은데",
          "original_form": "물귀신 얘기 나왔던거같은데",
          "speaker_id": "2",
          "time": "20210901 09:05"
        },
        {
          "id": "MDRW2100025313.1.4",
          "form": "수살귀ㅇㅇㅇ",
          "original_form": "수살귀ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 09:05"
        },
        {
          "id": "MDRW2100025313.1.5",
          "form": "웃는거 넘무서워",
          "original_form": "웃는거 넘무서워",
          "speaker_id": "1",
          "time": "20210901 09:05"
        },
        {
          "id": "MDRW2100025313.1.6",
          "form": "히익",
          "original_form": "히익",
          "speaker_id": "1",
          "time": "20210901 09:05"
        },
        {
          "id": "MDRW2100025313.1.7",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210901 09:05"
        },
        {
          "id": "MDRW2100025313.1.8",
          "form": "고개 툭툭 떨어트리는거ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "고개 툭툭 떨어트리는거ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:06"
        },
        {
          "id": "MDRW2100025313.1.9",
          "form": "그런게있었나",
          "original_form": "그런게있었나",
          "speaker_id": "1",
          "time": "20210901 09:06"
        },
        {
          "id": "MDRW2100025313.1.10",
          "form": "ㅇㅇㅇㅇ 중간에",
          "original_form": "ㅇㅇㅇㅇ 중간에",
          "speaker_id": "2",
          "time": "20210901 09:06"
        },
        {
          "id": "MDRW2100025313.1.11",
          "form": "오늘 다시한번봐야겠네",
          "original_form": "오늘 다시한번봐야겠네",
          "speaker_id": "1",
          "time": "20210901 09:06"
        },
        {
          "id": "MDRW2100025313.1.12",
          "form": "마지막 클라이맥스 가기 전에",
          "original_form": "마지막 클라이맥스 가기 전에",
          "speaker_id": "2",
          "time": "20210901 09:06"
        },
        {
          "id": "MDRW2100025313.1.13",
          "form": "빌드업할때",
          "original_form": "빌드업할때",
          "speaker_id": "2",
          "time": "20210901 09:06"
        },
        {
          "id": "MDRW2100025313.1.14",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 09:06"
        },
        {
          "id": "MDRW2100025313.1.15",
          "form": "아예 물귀신이라고 얘기해?",
          "original_form": "아예 물귀신이라고 얘기해?",
          "speaker_id": "1",
          "time": "20210901 09:06"
        },
        {
          "id": "MDRW2100025313.1.16",
          "form": "나무사이에는 웅신님이 연못바닥에는 수살귀에",
          "original_form": "나무사이에는 웅신님이 연못바닥에는 수살귀에",
          "speaker_id": "2",
          "time": "20210901 09:06"
        },
        {
          "id": "MDRW2100025313.1.17",
          "form": "오.....",
          "original_form": "오.....",
          "speaker_id": "1",
          "time": "20210901 09:06"
        },
        {
          "id": "MDRW2100025313.1.18",
          "form": "벽공 너머에는 불사조가 ㅇㅇㅇㅇ에는 도깨비가 였나",
          "original_form": "벽공 너머에는 불사조가 ㅇㅇㅇㅇ에는 도깨비가 였나",
          "speaker_id": "2",
          "time": "20210901 09:06"
        },
        {
          "id": "MDRW2100025313.1.19",
          "form": "수살귀=물귀신",
          "original_form": "수살귀=물귀신",
          "speaker_id": "2",
          "time": "20210901 09:06"
        },
        {
          "id": "MDRW2100025313.1.20",
          "form": "웅신님도 귀신인가",
          "original_form": "웅신님도 귀신인가",
          "speaker_id": "1",
          "time": "20210901 09:06"
        },
        {
          "id": "MDRW2100025313.1.21",
          "form": "곰귀신인데",
          "original_form": "곰귀신인데",
          "speaker_id": "2",
          "time": "20210901 09:06"
        },
        {
          "id": "MDRW2100025313.1.22",
          "form": "악귀 악신이라고 단정은",
          "original_form": "악귀 악신이라고 단정은",
          "speaker_id": "2",
          "time": "20210901 09:07"
        },
        {
          "id": "MDRW2100025313.1.23",
          "form": "못지을거같아여",
          "original_form": "못지을거같아여",
          "speaker_id": "2",
          "time": "20210901 09:07"
        },
        {
          "id": "MDRW2100025313.1.24",
          "form": "불사조도 악귀 느낌은 아니고",
          "original_form": "불사조도 악귀 느낌은 아니고",
          "speaker_id": "2",
          "time": "20210901 09:07"
        },
        {
          "id": "MDRW2100025313.1.25",
          "form": "아하... 하긴 그 귀신이 우리나라에서는",
          "original_form": "아하... 하긴 그 귀신이 우리나라에서는",
          "speaker_id": "1",
          "time": "20210901 09:07"
        },
        {
          "id": "MDRW2100025313.1.26",
          "form": "절대악 절대선도 아니라던데",
          "original_form": "절대악 절대선도 아니라던데",
          "speaker_id": "1",
          "time": "20210901 09:07"
        },
        {
          "id": "MDRW2100025313.1.27",
          "form": "도깨비도 원래 선함 악함 이런 구분이 안되는 존재고",
          "original_form": "도깨비도 원래 선함 악함 이런 구분이 안되는 존재고",
          "speaker_id": "2",
          "time": "20210901 09:07"
        },
        {
          "id": "MDRW2100025313.1.28",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210901 09:07"
        },
        {
          "id": "MDRW2100025313.1.29",
          "form": "아마 일부러",
          "original_form": "아마 일부러",
          "speaker_id": "2",
          "time": "20210901 09:07"
        },
        {
          "id": "MDRW2100025313.1.30",
          "form": "그 뮤비나오는게 딱 창귀라서그렇지",
          "original_form": "그 뮤비나오는게 딱 창귀라서그렇지",
          "speaker_id": "1",
          "time": "20210901 09:07"
        },
        {
          "id": "MDRW2100025313.1.31",
          "form": "선한 신 악한 신 구분 안되게",
          "original_form": "선한 신 악한 신 구분 안되게",
          "speaker_id": "2",
          "time": "20210901 09:07"
        },
        {
          "id": "MDRW2100025313.1.32",
          "form": "아마 그렇게나쁜친구들은 아닐것...",
          "original_form": "아마 그렇게나쁜친구들은 아닐것...",
          "speaker_id": "1",
          "time": "20210901 09:07"
        },
        {
          "id": "MDRW2100025313.1.33",
          "form": "다 섞어둔거 아닐까 하는 저의 뇌피셜...",
          "original_form": "다 섞어둔거 아닐까 하는 저의 뇌피셜...",
          "speaker_id": "2",
          "time": "20210901 09:08"
        },
        {
          "id": "MDRW2100025313.1.34",
          "form": "저도 그래서 안예은 뮤비들다찾아보려하는데",
          "original_form": "저도 그래서 안예은 뮤비들다찾아보려하는데",
          "speaker_id": "1",
          "time": "20210901 09:08"
        },
        {
          "id": "MDRW2100025313.1.35",
          "form": "저도이런 토속적인 귀신얘기나 이런거좋아해서",
          "original_form": "저도이런 토속적인 귀신얘기나 이런거좋아해서",
          "speaker_id": "1",
          "time": "20210901 09:08"
        },
        {
          "id": "MDRW2100025313.1.36",
          "form": "근데 능소화뮤비는 아직 못보겠어",
          "original_form": "근데 능소화뮤비는 아직 못보겠어",
          "speaker_id": "1",
          "time": "20210901 09:09"
        }
      ]
    }
  ]
