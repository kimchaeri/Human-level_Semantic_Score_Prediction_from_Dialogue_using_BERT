      "utterance": [
        {
          "id": "MDRW2100030293.1.1",
          "form": "친구가 맨날 추천하는데 들은 척도 안 했거든요 ㅎ.ㅎ",
          "original_form": "친구가 맨날 추천하는데 들은 척도 안 했거든요 ㅎ.ㅎ",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030293.1.2",
          "form": "도망치치는건~~ 이거재밌어요 ㅎㅎ",
          "original_form": "도망치치는건~~ 이거재밌어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030293.1.3",
          "form": "시마랑 캐릿터는 엄청다른데 ㅋㅋㅋ",
          "original_form": "시마랑 캐릿터는 엄청다른데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030293.1.4",
          "form": "그리고 보시고나서...sp도 봐주세요...",
          "original_form": "그리고 보시고나서...sp도 봐주세요...",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030293.1.5",
          "form": "우왁 그건 또 다른 드라마인가요?!?!?!?",
          "original_form": "우왁 그건 또 다른 드라마인가요?!?!?!?",
          "speaker_id": "1",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030293.1.6",
          "form": "아 스페셜이요!",
          "original_form": "아 스페셜이요!",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030293.1.7",
          "form": "음 뭐지...본작이있고..",
          "original_form": "음 뭐지...본작이있고..",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030293.1.8",
          "form": "그거의 스페셜..?",
          "original_form": "그거의 스페셜..?",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030293.1.9",
          "form": "그 왓챠에는 없는데",
          "original_form": "그 왓챠에는 없는데",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030293.1.10",
          "form": "도라마코리아라구 일드 정식 수입해서 보는 사이트랑 어플있어요!",
          "original_form": "도라마코리아라구 일드 정식 수입해서 보는 사이트랑 어플있어요!",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030293.1.11",
          "form": "헉!!!!!!!!! 햄님 완전 척척박사같아요",
          "original_form": "헉!!!!!!!!! 햄님 완전 척척박사같아요",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030293.1.12",
          "form": "저 오늘 마침 백신도 맞았으니",
          "original_form": "저 오늘 마침 백신도 맞았으니",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030293.1.13",
          "form": "내일 달려뷰겠어요",
          "original_form": "내일 달려뷰겠어요",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030293.1.14",
          "form": "광고보는대신 무료로 보싱수잇습니다!!",
          "original_form": "광고보는대신 무료로 보싱수잇습니다!!",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030293.1.15",
          "form": "합법사이트구용..",
          "original_form": "합법사이트구용..",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030293.1.16",
          "form": "헐 너무 좋은 사이트잖아요옹!?!?!",
          "original_form": "헐 너무 좋은 사이트잖아요옹!?!?!",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030293.1.17",
          "form": "우왕",
          "original_form": "우왕",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030293.1.18",
          "form": "그쵸 ㅋㅋㅋ!!",
          "original_form": "그쵸 ㅋㅋㅋ!!",
          "speaker_id": "2",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030293.1.19",
          "form": "햄님 미우에서 제일 좋아하는 에피소드 모이용!",
          "original_form": "햄님 미우에서 제일 좋아하는 에피소드 모이용!",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030293.1.20",
          "form": "뭐예여!",
          "original_form": "뭐예여!",
          "speaker_id": "1",
          "time": "20210903 23:35"
        },
        {
          "id": "MDRW2100030293.1.21",
          "form": "저는 4편...아오이케 토코..",
          "original_form": "저는 4편...아오이케 토코..",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030293.1.22",
          "form": ".....ㅠㅠㅠㅠㅠ",
          "original_form": ".....ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030293.1.23",
          "form": "오마이갓 저랑 같으셔요",
          "original_form": "오마이갓 저랑 같으셔요",
          "speaker_id": "1",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030293.1.24",
          "form": "저 그것만 10번 봤잖아요ㅠㅠㅠㅠ",
          "original_form": "저 그것만 10번 봤잖아요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 23:36"
        },
        {
          "id": "MDRW2100030293.1.25",
          "form": "진짜ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "진짜ㅠㅠㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030293.1.26",
          "form": "그 사람이 자기 트위터에 올린 글 읽는 부분에서",
          "original_form": "그 사람이 자기 트위터에 올린 글 읽는 부분에서",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030293.1.27",
          "form": "펑펑 울었어요",
          "original_form": "펑펑 울었어요",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030293.1.28",
          "form": "진짜로.........저두요......",
          "original_form": "진짜로.........저두요......",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030293.1.29",
          "form": "너무...너무.....",
          "original_form": "너무...너무.....",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030293.1.30",
          "form": "다 좋아하긴하는데 아오이케토코 에피는 진짜...",
          "original_form": "다 좋아하긴하는데 아오이케토코 에피는 진짜...",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030293.1.31",
          "form": "밀리언달러 걸...ㅠㅠ",
          "original_form": "밀리언달러 걸...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030293.1.32",
          "form": "힝 그니까요 저 첨에 보면서 에 머야 노잼이네",
          "original_form": "힝 그니까요 저 첨에 보면서 에 머야 노잼이네",
          "speaker_id": "1",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030293.1.33",
          "form": "이러다가 버스씬 나오는 순간부코",
          "original_form": "이러다가 버스씬 나오는 순간부코",
          "speaker_id": "1",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030293.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030293.1.35",
          "form": "울었잖아요 흑흑",
          "original_form": "울었잖아요 흑흑",
          "speaker_id": "1",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030293.1.36",
          "form": "진짜.........",
          "original_form": "진짜.........",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030293.1.37",
          "form": "저 그거랑 그 시마 과거 얘기 나오는 편도 좋아해요",
          "original_form": "저 그거랑 그 시마 과거 얘기 나오는 편도 좋아해요",
          "speaker_id": "1",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030293.1.38",
          "form": "헉 마자요 저두",
          "original_form": "헉 마자요 저두",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030293.1.39",
          "form": "저 그거 마지막에",
          "original_form": "저 그거 마지막에",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030293.1.40",
          "form": "옥상에서 이부키가",
          "original_form": "옥상에서 이부키가",
          "speaker_id": "2",
          "time": "20210903 23:38"
        },
        {
          "id": "MDRW2100030293.1.41",
          "form": "자기 생명선은 길다고할때",
          "original_form": "자기 생명선은 길다고할때",
          "speaker_id": "2",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030293.1.42",
          "form": "너무 좋았어요..",
          "original_form": "너무 좋았어요..",
