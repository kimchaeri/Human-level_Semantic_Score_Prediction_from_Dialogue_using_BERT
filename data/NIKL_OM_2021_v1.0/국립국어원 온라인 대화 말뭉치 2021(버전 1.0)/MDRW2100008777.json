      "utterance": [
        {
          "id": "MDRW2100008777.1.1",
          "form": "전 캄보디아 좋더라구요",
          "original_form": "전 캄보디아 좋더라구요",
          "speaker_id": "2",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008777.1.2",
          "form": "앙코르와트 넘 좋았어요 일출 완전 이쁘고",
          "original_form": "앙코르와트 넘 좋았어요 일출 완전 이쁘고",
          "speaker_id": "2",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008777.1.3",
          "form": "아 캄보디아 어릴때 가봤어요~~~!!",
          "original_form": "아 캄보디아 어릴때 가봤어요~~~!!",
          "speaker_id": "1",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008777.1.4",
          "form": "오 어릴때ㅎㅎ 어떠셨나요?",
          "original_form": "오 어릴때ㅎㅎ 어떠셨나요?",
          "speaker_id": "2",
          "time": "20210822 18:17"
        },
        {
          "id": "MDRW2100008777.1.5",
          "form": "이뻤던거 같아요",
          "original_form": "이뻤던거 같아요",
          "speaker_id": "1",
          "time": "20210822 18:30"
        },
        {
          "id": "MDRW2100008777.1.6",
          "form": "전 현지가이드 따로 초빙해서 친구랑 둘이갔거든요",
          "original_form": "전 현지가이드 따로 초빙해서 친구랑 둘이갔거든요",
          "speaker_id": "2",
          "time": "20210822 18:32"
        },
        {
          "id": "MDRW2100008777.1.7",
          "form": "편하고 좋더라구요",
          "original_form": "편하고 좋더라구요",
          "speaker_id": "2",
          "time": "20210822 18:32"
        },
        {
          "id": "MDRW2100008777.1.8",
          "form": "오 현지가이드 신기하네요",
          "original_form": "오 현지가이드 신기하네요",
          "speaker_id": "1",
          "time": "20210822 18:34"
        },
        {
          "id": "MDRW2100008777.1.9",
          "form": "한국말도 잘하더라구요ㅎ",
          "original_form": "한국말도 잘하더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210822 18:35"
        },
        {
          "id": "MDRW2100008777.1.10",
          "form": "가족단위로도 많이 오던데요",
          "original_form": "가족단위로도 많이 오던데요",
          "speaker_id": "2",
          "time": "20210822 18:35"
        },
        {
          "id": "MDRW2100008777.1.11",
          "form": "오 그런가요 참고",
          "original_form": "오 그런가요 참고",
          "speaker_id": "1",
          "time": "20210822 20:48"
        },
        {
          "id": "MDRW2100008777.1.12",
          "form": "근데 코로나땜에 갈수나 있을런지 모르겠네요ㅎ",
          "original_form": "근데 코로나땜에 갈수나 있을런지 모르겠네요ㅎ",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100008777.1.13",
          "form": "기회되면 한번더 가고싶더라구요",
          "original_form": "기회되면 한번더 가고싶더라구요",
          "speaker_id": "2",
          "time": "20210822 21:27"
        },
        {
          "id": "MDRW2100008777.1.14",
          "form": "갈 수 있을거에요",
          "original_form": "갈 수 있을거에요",
          "speaker_id": "1",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100008777.1.15",
          "form": "몇년후에나  ㅎㅎ",
          "original_form": "몇년후에나  ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100008777.1.16",
          "form": "울릉도는 맛집이나 유명한음식이 뭐예요?",
          "original_form": "울릉도는 맛집이나 유명한음식이 뭐예요?",
          "speaker_id": "2",
          "time": "20210822 21:31"
        },
        {
          "id": "MDRW2100008777.1.17",
          "form": "그 명이나물이랑 따개비? 엿도 유명하죠",
          "original_form": "그 명이나물이랑 따개비? 엿도 유명하죠",
          "speaker_id": "1",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100008777.1.18",
          "form": "아 명이나물ㅎ 맛있죠",
          "original_form": "아 명이나물ㅎ 맛있죠",
          "speaker_id": "2",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100008777.1.19",
          "form": "따개비는 뭔지모르겠네요",
          "original_form": "따개비는 뭔지모르겠네요",
          "speaker_id": "2",
          "time": "20210822 22:33"
        },
        {
          "id": "MDRW2100008777.1.20",
          "form": "음~ 그 고둥같은거요",
          "original_form": "음~ 그 고둥같은거요",
          "speaker_id": "1",
          "time": "20210822 22:34"
        },
        {
          "id": "MDRW2100008777.1.21",
          "form": "아ㅎㅎ 맛있겠네요",
          "original_form": "아ㅎㅎ 맛있겠네요",
          "speaker_id": "2",
          "time": "20210822 22:35"
        }
