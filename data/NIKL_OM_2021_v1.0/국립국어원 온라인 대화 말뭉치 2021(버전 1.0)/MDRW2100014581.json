      "utterance": [
        {
          "id": "MDRW2100014581.1.1",
          "form": "운동 좋아하시나요?",
          "original_form": "운동 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014581.1.2",
          "form": "네 좋아해요 ㅋㅋ 좋아하세요??",
          "original_form": "네 좋아해요 ㅋㅋ 좋아하세요??",
          "speaker_id": "2",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014581.1.3",
          "form": "운동하면서 쾌감까지 느끼진 않지만 요즘 좀 좋아하고 있는 중 같아요",
          "original_form": "운동하면서 쾌감까지 느끼진 않지만 요즘 좀 좋아하고 있는 중 같아요",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014581.1.4",
          "form": "어떤 운동 좋아하세요?",
          "original_form": "어떤 운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014581.1.5",
          "form": "헬스하다가 수영이요",
          "original_form": "헬스하다가 수영이요",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014581.1.6",
          "form": "근데 시국이 이래서 수영은 관뒀어요",
          "original_form": "근데 시국이 이래서 수영은 관뒀어요",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014581.1.7",
          "form": "저도 수영 다녔는데 코로나 이후로 그만뒀어요ㅠㅠ",
          "original_form": "저도 수영 다녔는데 코로나 이후로 그만뒀어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014581.1.8",
          "form": "아무래도 탈의실이 불안해서요",
          "original_form": "아무래도 탈의실이 불안해서요",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014581.1.9",
          "form": "맞아요 진짜 수영 저도 엄청 좋아하는데",
          "original_form": "맞아요 진짜 수영 저도 엄청 좋아하는데",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014581.1.10",
          "form": "마스크를 못껴서",
          "original_form": "마스크를 못껴서",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014581.1.11",
          "form": "불안하죠",
          "original_form": "불안하죠",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014581.1.12",
          "form": "물 속에서는 뭐 전파 안 된다고는 하는데 불안하긴 해요",
          "original_form": "물 속에서는 뭐 전파 안 된다고는 하는데 불안하긴 해요",
          "speaker_id": "1",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014581.1.13",
          "form": "아 그래요??",
          "original_form": "아 그래요??",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014581.1.14",
          "form": "사실 하고싶어도 시설들이 다 문 닫았더라고요",
          "original_form": "사실 하고싶어도 시설들이 다 문 닫았더라고요",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014581.1.15",
          "form": "어디는 하고 어디는 닫고...",
          "original_form": "어디는 하고 어디는 닫고...",
          "speaker_id": "1",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014581.1.16",
          "form": "저희동네는 다 닫아서 ㅠ",
          "original_form": "저희동네는 다 닫아서 ㅠ",
          "speaker_id": "2",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014581.1.17",
          "form": "전 불안해서 못 하디만 괜찮아지면 제일 먼저 하고 싶어요",
          "original_form": "전 불안해서 못 하디만 괜찮아지면 제일 먼저 하고 싶어요",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014581.1.18",
          "form": "수영 얼마나 하셨어요??",
          "original_form": "수영 얼마나 하셨어요??",
          "speaker_id": "2",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014581.1.19",
          "form": "최근에는 몇개월 못 했어요-",
          "original_form": "최근에는 몇개월 못 했어요-",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014581.1.20",
          "form": "어렸을 때붜 계속 해오던 거라ㅠ",
          "original_form": "어렸을 때붜 계속 해오던 거라ㅠ",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014581.1.21",
          "form": "와 대박이네요 저는 1년밖에 안해서 초보인데",
          "original_form": "와 대박이네요 저는 1년밖에 안해서 초보인데",
          "speaker_id": "2",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014581.1.22",
          "form": "할때 너무 재미있었어요",
          "original_form": "할때 너무 재미있었어요",
          "speaker_id": "2",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014581.1.23",
          "form": "1년하셨으면 그래도 다 하실 수 있는 거 아니에요?",
          "original_form": "1년하셨으면 그래도 다 하실 수 있는 거 아니에요?",
          "speaker_id": "1",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014581.1.24",
          "form": "전 다이빙이 너무 무서워요",
          "original_form": "전 다이빙이 너무 무서워요",
          "speaker_id": "1",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014581.1.25",
          "form": "중급 막 가려할때 코로나 터져서 ㅠㅠ",
          "original_form": "중급 막 가려할때 코로나 터져서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014581.1.26",
          "form": "다이빙이 진짜 너무 멋있어보여요",
          "original_form": "다이빙이 진짜 너무 멋있어보여요",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014581.1.27",
          "form": "막 도구쓰고",
          "original_form": "막 도구쓰고",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014581.1.28",
          "form": "헉 딱 터지다니ㅠㅠ",
          "original_form": "헉 딱 터지다니ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 20:37"
        }
      ]
    }
