      "utterance": [
        {
          "id": "MDRW2100013021.1.1",
          "form": "평소에 어떤걸 자주 드세요",
          "original_form": "평소에 어떤걸 자주 드세요",
          "speaker_id": "1",
          "time": "20210825 11:10"
        },
        {
          "id": "MDRW2100013021.1.2",
          "form": "저는 집에서 자주 먹는 편이에요",
          "original_form": "저는 집에서 자주 먹는 편이에요",
          "speaker_id": "2",
          "time": "20210825 11:20"
        },
        {
          "id": "MDRW2100013021.1.3",
          "form": "요리 좋아하시나요??",
          "original_form": "요리 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210825 11:24"
        },
        {
          "id": "MDRW2100013021.1.4",
          "form": "네! 좋아해요",
          "original_form": "네! 좋아해요",
          "speaker_id": "2",
          "time": "20210825 12:01"
        },
        {
          "id": "MDRW2100013021.1.5",
          "form": "그중에서도 간단하게 팬 하나로 만드는 요리를 좋아해요",
          "original_form": "그중에서도 간단하게 팬 하나로 만드는 요리를 좋아해요",
          "speaker_id": "2",
          "time": "20210825 12:01"
        },
        {
          "id": "MDRW2100013021.1.6",
          "form": "오 어떤 요리 하시나요??",
          "original_form": "오 어떤 요리 하시나요??",
          "speaker_id": "1",
          "time": "20210825 14:50"
        },
        {
          "id": "MDRW2100013021.1.7",
          "form": "원풋파스타를 가장 즐겨해요",
          "original_form": "원풋파스타를 가장 즐겨해요",
          "speaker_id": "2",
          "time": "20210825 15:17"
        },
        {
          "id": "MDRW2100013021.1.8",
          "form": "다 한 프라이팬에 넣고 요리해서 쉽고 설거지도 덜 나와요",
          "original_form": "다 한 프라이팬에 넣고 요리해서 쉽고 설거지도 덜 나와요",
          "speaker_id": "2",
          "time": "20210825 15:18"
        },
        {
          "id": "MDRW2100013021.1.9",
          "form": "오오 파스타 !! 맛있을거같아요 !!",
          "original_form": "오오 파스타 !! 맛있을거같아요 !!",
          "speaker_id": "1",
          "time": "20210825 16:28"
        },
        {
          "id": "MDRW2100013021.1.10",
          "form": "조리법이 간단해서 추천드려요",
          "original_form": "조리법이 간단해서 추천드려요",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013021.1.11",
          "form": "name1님은 요리 좋아하시나요?",
          "original_form": "&name1&님은 요리 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013021.1.12",
          "form": "요리를 레시피 보고 따라하는 수준이예요..!",
          "original_form": "요리를 레시피 보고 따라하는 수준이예요..!",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100013021.1.13",
          "form": "그래도 맛은 기본은 하는거 같아요 ㅋㅋ",
          "original_form": "그래도 맛은 기본은 하는거 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:35"
        },
        {
          "id": "MDRW2100013021.1.14",
          "form": "오! 그렇군요",
          "original_form": "오! 그렇군요",
          "speaker_id": "2",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100013021.1.15",
          "form": "최근에 하신 요리가 있나요?",
          "original_form": "최근에 하신 요리가 있나요?",
          "speaker_id": "2",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100013021.1.16",
          "form": "미역국에 계란말이용..! 완전 초보자라서 ㅎㅎ",
          "original_form": "미역국에 계란말이용..! 완전 초보자라서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:33"
        },
        {
          "id": "MDRW2100013021.1.17",
          "form": "정말 맛있었을 것 같아요! 직접하신 요리가 더 맛있게 느껴질때가 많죠",
          "original_form": "정말 맛있었을 것 같아요! 직접하신 요리가 더 맛있게 느껴질때가 많죠",
          "speaker_id": "2",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100013021.1.18",
          "form": "둘이 조합도 잘어울리네요 ㅎㅎ",
          "original_form": "둘이 조합도 잘어울리네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100013021.1.19",
          "form": "배달도 질려서 이제 만들어 먹을려고해요 !!",
          "original_form": "배달도 질려서 이제 만들어 먹을려고해요 !!",
