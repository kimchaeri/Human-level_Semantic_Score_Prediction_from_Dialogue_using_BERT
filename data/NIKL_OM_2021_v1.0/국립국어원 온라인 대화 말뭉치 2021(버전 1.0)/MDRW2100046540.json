      "utterance": [
        {
          "id": "MDRW2100046540.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211027 15:47"
        },
        {
          "id": "MDRW2100046540.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211027 15:48"
        },
        {
          "id": "MDRW2100046540.1.3",
          "form": "운전 하시나요?",
          "original_form": "운전 하시나요?",
          "speaker_id": "2",
          "time": "20211027 15:48"
        },
        {
          "id": "MDRW2100046540.1.4",
          "form": "반가워요",
          "original_form": "반가워요",
          "speaker_id": "1",
          "time": "20211027 15:48"
        },
        {
          "id": "MDRW2100046540.1.5",
          "form": "네 그럼요",
          "original_form": "네 그럼요",
          "speaker_id": "1",
          "time": "20211027 15:48"
        },
        {
          "id": "MDRW2100046540.1.6",
          "form": "전 운전 시작한지 이제 4개월 차 되는",
          "original_form": "전 운전 시작한지 이제 4개월 차 되는",
          "speaker_id": "2",
          "time": "20211027 15:49"
        },
        {
          "id": "MDRW2100046540.1.7",
          "form": "운전경력만 20년이에요",
          "original_form": "운전경력만 20년이에요",
          "speaker_id": "1",
          "time": "20211027 15:49"
        },
        {
          "id": "MDRW2100046540.1.8",
          "form": "앗 그러신가요",
          "original_form": "앗 그러신가요",
          "speaker_id": "1",
          "time": "20211027 15:49"
        },
        {
          "id": "MDRW2100046540.1.9",
          "form": "초보운전자입니다 ㅎㅎ",
          "original_form": "초보운전자입니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211027 15:49"
        },
        {
          "id": "MDRW2100046540.1.10",
          "form": "와 운전경력만 20년차시라니",
          "original_form": "와 운전경력만 20년차시라니",
          "speaker_id": "2",
          "time": "20211027 15:49"
        },
        {
          "id": "MDRW2100046540.1.11",
          "form": "엄청난 대선배네요",
          "original_form": "엄청난 대선배네요",
          "speaker_id": "2",
          "time": "20211027 15:49"
        },
        {
          "id": "MDRW2100046540.1.12",
          "form": "복잡한 도로와 골목길이 좀 긴장되시겠네요",
          "original_form": "복잡한 도로와 골목길이 좀 긴장되시겠네요",
          "speaker_id": "1",
          "time": "20211027 15:50"
        },
        {
          "id": "MDRW2100046540.1.13",
          "form": "네 ㅠ",
          "original_form": "네 ㅠ",
          "speaker_id": "2",
          "time": "20211027 15:50"
        },
        {
          "id": "MDRW2100046540.1.14",
          "form": "주차도 아직 후진주차밖에 못한답니다 ㅎㅎ",
          "original_form": "주차도 아직 후진주차밖에 못한답니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211027 15:50"
        },
        {
          "id": "MDRW2100046540.1.15",
          "form": "시간이 약입니당 ㅋㅋ",
          "original_form": "시간이 약입니당 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211027 15:50"
        },
        {
          "id": "MDRW2100046540.1.16",
          "form": "시간이 지나면 나아지겠죠? ㅋㅋ",
          "original_form": "시간이 지나면 나아지겠죠? ㅋㅋ",
          "speaker_id": "2",
          "time": "20211027 15:50"
        },
        {
          "id": "MDRW2100046540.1.17",
          "form": "그럼요 다 됩니다",
          "original_form": "그럼요 다 됩니다",
          "speaker_id": "1",
          "time": "20211027 15:50"
        },
        {
          "id": "MDRW2100046540.1.18",
          "form": "좋아하시는 드라이브 코스 있으신가요?",
          "original_form": "좋아하시는 드라이브 코스 있으신가요?",
          "speaker_id": "2",
          "time": "20211027 15:51"
        },
        {
          "id": "MDRW2100046540.1.19",
          "form": "저는 한강 좋아해요",
          "original_form": "저는 한강 좋아해요",
          "speaker_id": "1",
          "time": "20211027 15:51"
        },
        {
          "id": "MDRW2100046540.1.20",
          "form": "서울 사시는구나~!",
          "original_form": "서울 사시는구나~!",
          "speaker_id": "2",
          "time": "20211027 15:51"
        },
        {
          "id": "MDRW2100046540.1.21",
          "form": "드라이브라기보다는 한강에 차 세워놓고 누워있기요",
          "original_form": "드라이브라기보다는 한강에 차 세워놓고 누워있기요",
          "speaker_id": "1",
          "time": "20211027 15:51"
        },
        {
          "id": "MDRW2100046540.1.22",
          "form": "와 좋네요 ㅎㅎ",
          "original_form": "와 좋네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211027 15:51"
        },
        {
          "id": "MDRW2100046540.1.23",
          "form": "저는 아직 서울로는 한번도 끌고나가본적이 없는 쌩초보여서요 ㅋㅋㅋ",
          "original_form": "저는 아직 서울로는 한번도 끌고나가본적이 없는 쌩초보여서요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211027 15:51"
        },
        {
          "id": "MDRW2100046540.1.24",
          "form": "시간이 다 지나면 웃으실거에요 ㅋ",
          "original_form": "시간이 다 지나면 웃으실거에요 ㅋ",
          "speaker_id": "1",
          "time": "20211027 15:52"
        },
        {
          "id": "MDRW2100046540.1.25",
          "form": "열심히 연습해볼게요! ㅋㅋ",
          "original_form": "열심히 연습해볼게요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20211027 15:52"
        }
      ]
    }
