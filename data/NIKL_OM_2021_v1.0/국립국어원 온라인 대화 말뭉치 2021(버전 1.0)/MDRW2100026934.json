      "utterance": [
        {
          "id": "MDRW2100026934.1.1",
          "form": "저 당근마켓에서",
          "original_form": "저 당근마켓에서",
          "speaker_id": "1",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100026934.1.2",
          "form": "냉장고를 사려고요",
          "original_form": "냉장고를 사려고요",
          "speaker_id": "1",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100026934.1.3",
          "form": "와우...",
          "original_form": "와우...",
          "speaker_id": "2",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100026934.1.4",
          "form": "저는 ..뭘할지 모르겠는데",
          "original_form": "저는 ..뭘할지 모르겠는데",
          "speaker_id": "2",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100026934.1.5",
          "form": "오타쿠짓하겠죠",
          "original_form": "오타쿠짓하겠죠",
          "speaker_id": "2",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100026934.1.6",
          "form": "그건 맨날하는거구여",
          "original_form": "그건 맨날하는거구여",
          "speaker_id": "1",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100026934.1.7",
          "form": "스루가야에서 못쓰니까",
          "original_form": "스루가야에서 못쓰니까",
          "speaker_id": "2",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100026934.1.8",
          "form": "좀  .. 특별한...",
          "original_form": "좀  .. 특별한...",
          "speaker_id": "1",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100026934.1.9",
          "form": "아야",
          "original_form": "아야",
          "speaker_id": "2",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100026934.1.10",
          "form": "살살때리세요",
          "original_form": "살살때리세요",
          "speaker_id": "2",
          "time": "20210902 01:27"
        },
        {
          "id": "MDRW2100026934.1.11",
          "form": "ㅋㅋㅋ하 감자는",
          "original_form": "ㅋㅋㅋ하 감자는",
          "speaker_id": "1",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100026934.1.12",
          "form": "리디북스에서 사지않을까싶은...",
          "original_form": "리디북스에서 사지않을까싶은...",
          "speaker_id": "2",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100026934.1.13",
          "form": "아닐수도",
          "original_form": "아닐수도",
          "speaker_id": "2",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100026934.1.14",
          "form": "뼈도 없어서 이천유ㅓㄴ 비싸지지도",
          "original_form": "뼈도 없어서 이천유ㅓㄴ 비싸지지도",
          "speaker_id": "1",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100026934.1.15",
          "form": "않는데",
          "original_form": "않는데",
          "speaker_id": "1",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100026934.1.16",
          "form": "........",
          "original_form": "........",
          "speaker_id": "2",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100026934.1.17",
          "form": "감자의상품가치가",
          "original_form": "감자의상품가치가",
          "speaker_id": "2",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100026934.1.18",
          "form": "2000원비싸진거죠",
          "original_form": "2000원비싸진거죠",
          "speaker_id": "2",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100026934.1.19",
          "form": "쌩감자에서",
          "original_form": "쌩감자에서",
          "speaker_id": "2",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100026934.1.20",
          "form": "으깬감자로",
          "original_form": "으깬감자로",
          "speaker_id": "2",
          "time": "20210902 01:28"
        },
        {
          "id": "MDRW2100026934.1.21",
          "form": "보관날짜가 줄어드니까",
          "original_form": "보관날짜가 줄어드니까",
          "speaker_id": "1",
          "time": "20210902 01:29"
        },
        {
          "id": "MDRW2100026934.1.22",
          "form": "오히려 가격이 낮아져야하지않나",
          "original_form": "오히려 가격이 낮아져야하지않나",
          "speaker_id": "1",
          "time": "20210902 01:29"
        },
        {
          "id": "MDRW2100026934.1.23",
          "form": "싶네여",
          "original_form": "싶네여",
          "speaker_id": "1",
          "time": "20210902 01:29"
        },
        {
          "id": "MDRW2100026934.1.24",
          "form": "으깨서 간한 감자인거죠",
          "original_form": "으깨서 간한 감자인거죠",
          "speaker_id": "2",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100026934.1.25",
          "form": "상품가치 업",
          "original_form": "상품가치 업",
          "speaker_id": "2",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100026934.1.26",
          "form": "저 편의점에서 쇼 핑 할건데요",
          "original_form": "저 편의점에서 쇼 핑 할건데요",
          "speaker_id": "2",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100026934.1.27",
          "form": "내릴 아침에 일찍일어난다면",
          "original_form": "내릴 아침에 일찍일어난다면",
          "speaker_id": "2",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100026934.1.28",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100026934.1.29",
          "form": "가능성잇나요?",
          "original_form": "가능성잇나요?",
          "speaker_id": "1",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100026934.1.30",
          "form": "점심시간까지 멀쩡하게 먹을수있는게 머가 잇을까요",
          "original_form": "점심시간까지 멀쩡하게 먹을수있는게 머가 잇을까요",
          "speaker_id": "2",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100026934.1.31",
          "form": "......",
          "original_form": "......",
          "speaker_id": "2",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100026934.1.32",
          "form": "그만때리라구요",
          "original_form": "그만때리라구요",
          "speaker_id": "2",
          "time": "20210902 01:30"
        },
        {
          "id": "MDRW2100026934.1.33",
          "form": "알겟어요...율지마시구",
          "original_form": "알겟어요...율지마시구",
          "speaker_id": "1",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100026934.1.34",
          "form": "음...과자 어때용",
          "original_form": "음...과자 어때용",
          "speaker_id": "1",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100026934.1.35",
          "form": "계란빵같은",
          "original_form": "계란빵같은",
          "speaker_id": "1",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100026934.1.36",
          "form": "한국인은 밥심이라면서",
          "original_form": "한국인은 밥심이라면서",
          "speaker_id": "2",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100026934.1.37",
          "form": "과자를",
          "original_form": "과자를",
          "speaker_id": "2",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100026934.1.38",
          "form": "오잉",
          "original_form": "오잉",
          "speaker_id": "2",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100026934.1.39",
          "form": "계란빵 과자도 있나요",
          "original_form": "계란빵 과자도 있나요",
          "speaker_id": "2",
          "time": "20210902 01:31"
        },
        {
          "id": "MDRW2100026934.1.40",
          "form": "아 계란과자",
          "original_form": "아 계란과자",
          "speaker_id": "1",
          "time": "20210902 01:31"
        },
        {
