      "utterance": [
        {
          "id": "MDRW2100006062.1.1",
          "form": "저녁드셨나요?",
          "original_form": "저녁드셨나요?",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100006062.1.2",
          "form": "먹었어요^^",
          "original_form": "먹었어요^^",
          "speaker_id": "2",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100006062.1.3",
          "form": "맛난거드셨어요?",
          "original_form": "맛난거드셨어요?",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100006062.1.4",
          "form": "함박스테이크랑 김치찌개요",
          "original_form": "함박스테이크랑 김치찌개요",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100006062.1.5",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100006062.1.6",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100006062.1.7",
          "form": "된장찌개에 그냥 반찬해서 먹었습니다 ㅋㅋㅋ",
          "original_form": "된장찌개에 그냥 반찬해서 먹었습니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100006062.1.8",
          "form": "김 김치 그런기본반찬",
          "original_form": "김 김치 그런기본반찬",
          "speaker_id": "1",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100006062.1.9",
          "form": "전 그런 평범한 식단 좋아해요",
          "original_form": "전 그런 평범한 식단 좋아해요",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100006062.1.10",
          "form": "함박스테이크 안먹은지 엄청 오래됐는데",
          "original_form": "함박스테이크 안먹은지 엄청 오래됐는데",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100006062.1.11",
          "form": "김치찌개랑 조합이 괜찮나요?",
          "original_form": "김치찌개랑 조합이 괜찮나요?",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100006062.1.12",
          "form": "네~ 그냥 집밥이라 ㅋㅋㅋ",
          "original_form": "네~ 그냥 집밥이라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100006062.1.13",
          "form": "전 보통 어머니가 해주시는거 먹어서",
          "original_form": "전 보통 어머니가 해주시는거 먹어서",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100006062.1.14",
          "form": "집에와서 있는대로 먹어요 ㅋㅋ",
          "original_form": "집에와서 있는대로 먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100006062.1.15",
          "form": "저도 냉장고 기본 반찬에 냉동식품 추가 뭐 이런식",
          "original_form": "저도 냉장고 기본 반찬에 냉동식품 추가 뭐 이런식",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100006062.1.16",
          "form": "내일은 치킨을 시켜야겠어요 ㅋㅋ",
          "original_form": "내일은 치킨을 시켜야겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100006062.1.17",
          "form": "치킨은 어떤거 주로 드세요?",
          "original_form": "치킨은 어떤거 주로 드세요?",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100006062.1.18",
          "form": "교촌자주먹다가",
          "original_form": "교촌자주먹다가",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100006062.1.19",
          "form": "요즘엔 푸라닭 자주먹고",
          "original_form": "요즘엔 푸라닭 자주먹고",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100006062.1.20",
          "form": "오~ 푸리닭 맛있죠?",
          "original_form": "오~ 푸리닭 맛있죠?",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100006062.1.21",
          "form": "네 맛있더라구요 ㅋㅋ",
          "original_form": "네 맛있더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:19"
        }
