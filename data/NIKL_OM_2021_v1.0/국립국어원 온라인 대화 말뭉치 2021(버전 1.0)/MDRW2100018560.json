      "utterance": [
        {
          "id": "MDRW2100018560.1.1",
          "form": "이번 올림픽때 재밌게 본 경기 있으신가요",
          "original_form": "이번 올림픽때 재밌게 본 경기 있으신가요",
          "speaker_id": "2",
          "time": "20210827 08:22"
        },
        {
          "id": "MDRW2100018560.1.2",
          "form": "다 재밌었는데 특히나 양궁이요ㅋㅋㅋㅋ",
          "original_form": "다 재밌었는데 특히나 양궁이요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:22"
        },
        {
          "id": "MDRW2100018560.1.3",
          "form": "양궁은 올림픽때마다 관심갖긴했지만",
          "original_form": "양궁은 올림픽때마다 관심갖긴했지만",
          "speaker_id": "1",
          "time": "20210827 08:22"
        },
        {
          "id": "MDRW2100018560.1.4",
          "form": "아 양궁 엄청 재밌었죠",
          "original_form": "아 양궁 엄청 재밌었죠",
          "speaker_id": "2",
          "time": "20210827 08:23"
        },
        {
          "id": "MDRW2100018560.1.5",
          "form": "심장박동  중계가 돼서 더 박진감이 ㅎㅎ",
          "original_form": "심장박동  중계가 돼서 더 박진감이 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 08:23"
        },
        {
          "id": "MDRW2100018560.1.6",
          "form": "다시보기만 진짜 백번 한 거같아요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "다시보기만 진짜 백번 한 거같아요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:23"
        },
        {
          "id": "MDRW2100018560.1.7",
          "form": "ㅎㅎ 우리나라 양궁선수들 진짜 대단해요",
          "original_form": "ㅎㅎ 우리나라 양궁선수들 진짜 대단해요",
          "speaker_id": "2",
          "time": "20210827 08:24"
        },
        {
          "id": "MDRW2100018560.1.8",
          "form": "그렇게 바람이 세게 부는데 텐텐",
          "original_form": "그렇게 바람이 세게 부는데 텐텐",
          "speaker_id": "2",
          "time": "20210827 08:24"
        },
        {
          "id": "MDRW2100018560.1.9",
          "form": "우리가 주몽의 나라다ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "우리가 주몽의 나라다ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:25"
        },
        {
          "id": "MDRW2100018560.1.10",
          "form": "양궁말고 재밌게 본 경기 있으세요?",
          "original_form": "양궁말고 재밌게 본 경기 있으세요?",
          "speaker_id": "1",
          "time": "20210827 08:25"
        },
        {
          "id": "MDRW2100018560.1.11",
          "form": "이번에 배구도 되게 핫했는데!",
          "original_form": "이번에 배구도 되게 핫했는데!",
          "speaker_id": "1",
          "time": "20210827 08:25"
        },
        {
          "id": "MDRW2100018560.1.12",
          "form": "배구요!",
          "original_form": "배구요!",
          "speaker_id": "2",
          "time": "20210827 08:25"
        },
        {
          "id": "MDRW2100018560.1.13",
          "form": "ㅎㅎ 연경신을 절절이 체감했어요",
          "original_form": "ㅎㅎ 연경신을 절절이 체감했어요",
          "speaker_id": "2",
          "time": "20210827 08:25"
        },
        {
          "id": "MDRW2100018560.1.14",
          "form": "진짜 제 맘속에 금메달입니다",
          "original_form": "진짜 제 맘속에 금메달입니다",
          "speaker_id": "2",
          "time": "20210827 08:26"
        },
        {
          "id": "MDRW2100018560.1.15",
          "form": "배구 원래 좋아하세요?",
          "original_form": "배구 원래 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 08:27"
        },
        {
          "id": "MDRW2100018560.1.16",
          "form": "아뇨 룰 1도 몰라요ㅋㅋㅋ",
          "original_form": "아뇨 룰 1도 몰라요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:27"
        },
        {
          "id": "MDRW2100018560.1.17",
          "form": "로테이션으로 공격하고 뭐. .  너무 어려워서",
          "original_form": "로테이션으로 공격하고 뭐. .  너무 어려워서",
          "speaker_id": "1",
          "time": "20210827 08:27"
        },
        {
          "id": "MDRW2100018560.1.18",
          "form": "ㅎㅎ 저와 같으시네요",
          "original_form": "ㅎㅎ 저와 같으시네요",
          "speaker_id": "2",
          "time": "20210827 08:27"
        },
        {
          "id": "MDRW2100018560.1.19",
          "form": "해설들으면서 스파이크!!! 이거랑 선수들 이름만 외웠어요ㅋㅋㅋㅋ",
          "original_form": "해설들으면서 스파이크!!! 이거랑 선수들 이름만 외웠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:28"
        },
        {
          "id": "MDRW2100018560.1.20",
          "form": "룰 몰라도 재밌는 배구ㅎㅎ",
          "original_form": "룰 몰라도 재밌는 배구ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 08:28"
        },
        {
          "id": "MDRW2100018560.1.21",
          "form": "선수들 하나하나 다 정들었어요",
          "original_form": "선수들 하나하나 다 정들었어요",
          "speaker_id": "2",
          "time": "20210827 08:28"
        },
        {
          "id": "MDRW2100018560.1.22",
          "form": "우상혁 높이뛰기 보셨나요",
          "original_form": "우상혁 높이뛰기 보셨나요",
          "speaker_id": "2",
          "time": "20210827 08:29"
        },
        {
          "id": "MDRW2100018560.1.23",
          "form": "와... 장난 아니었죠",
          "original_form": "와... 장난 아니었죠",
          "speaker_id": "1",
          "time": "20210827 08:29"
        },
        {
          "id": "MDRW2100018560.1.24",
          "form": "우상혁선수는 마인드도 대단했잖아요!",
          "original_form": "우상혁선수는 마인드도 대단했잖아요!",
          "speaker_id": "1",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100018560.1.25",
          "form": "너무 씩씩하게 웃는데 그표정이 너무 좋더라고요!",
          "original_form": "너무 씩씩하게 웃는데 그표정이 너무 좋더라고요!",
          "speaker_id": "2",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100018560.1.26",
          "form": "정말 팬 됐어요",
          "original_form": "정말 팬 됐어요",
          "speaker_id": "2",
