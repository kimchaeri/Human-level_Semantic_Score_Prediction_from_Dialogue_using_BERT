      "utterance": [
        {
          "id": "MDRW2100047614.1.1",
          "form": "안녕하새요~",
          "original_form": "안녕하새요~",
          "speaker_id": "2",
          "time": "20210827 11:54"
        },
        {
          "id": "MDRW2100047614.1.2",
          "form": "아침드셨어요?!",
          "original_form": "아침드셨어요?!",
          "speaker_id": "2",
          "time": "20210827 11:54"
        },
        {
          "id": "MDRW2100047614.1.3",
          "form": "아침 안먹고 아점 먹으려구요 !",
          "original_form": "아침 안먹고 아점 먹으려구요 !",
          "speaker_id": "1",
          "time": "20210827 12:02"
        },
        {
          "id": "MDRW2100047614.1.4",
          "form": "아침 드셨나용",
          "original_form": "아침 드셨나용",
          "speaker_id": "1",
          "time": "20210827 12:02"
        },
        {
          "id": "MDRW2100047614.1.5",
          "form": "저는 지금 인나서 ㅎㅎ..",
          "original_form": "저는 지금 인나서 ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210827 12:04"
        },
        {
          "id": "MDRW2100047614.1.6",
          "form": "이제 점심 먹어야죠",
          "original_form": "이제 점심 먹어야죠",
          "speaker_id": "2",
          "time": "20210827 12:04"
        },
        {
          "id": "MDRW2100047614.1.7",
          "form": "저도 일어난지 얼마 안됐어요 ...ㅎㅎ",
          "original_form": "저도 일어난지 얼마 안됐어요 ...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100047614.1.8",
          "form": "점심 어떤거 드실 예정인가요 !",
          "original_form": "점심 어떤거 드실 예정인가요 !",
          "speaker_id": "1",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100047614.1.9",
          "form": "다이어트한다고 맨날 똑같이먹어서 ㅎ..",
          "original_form": "다이어트한다고 맨날 똑같이먹어서 ㅎ..",
          "speaker_id": "2",
          "time": "20210827 12:16"
        },
        {
          "id": "MDRW2100047614.1.10",
          "form": "단호박에 닭가슴살이요 ..",
          "original_form": "단호박에 닭가슴살이요 ..",
          "speaker_id": "2",
          "time": "20210827 12:16"
        },
        {
          "id": "MDRW2100047614.1.11",
          "form": "아앗.. 맛있는데ㅜ매일 먹으면 좀 그렇죠",
          "original_form": "아앗.. 맛있는데ㅜ매일 먹으면 좀 그렇죠",
          "speaker_id": "1",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100047614.1.12",
          "form": "맞아요 ㅠㅠ 요즘 삶이 밍숭맹숭..",
          "original_form": "맞아요 ㅠㅠ 요즘 삶이 밍숭맹숭..",
          "speaker_id": "2",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100047614.1.13",
          "form": "저는 다이어트하면 불닭 떡볶이 이런게 너무 땡기더라구요..",
          "original_form": "저는 다이어트하면 불닭 떡볶이 이런게 너무 땡기더라구요..",
          "speaker_id": "1",
          "time": "20210827 17:50"
        },
        {
          "id": "MDRW2100047614.1.14",
          "form": "치팅데이도 가끔 가지시나요?",
          "original_form": "치팅데이도 가끔 가지시나요?",
          "speaker_id": "1",
          "time": "20210827 17:50"
        },
        {
          "id": "MDRW2100047614.1.15",
          "form": "얼마전애 가졌어요 ㅎㅎ",
          "original_form": "얼마전애 가졌어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100047614.1.16",
          "form": "맛난 타코를 먹고왔습니다 !!",
          "original_form": "맛난 타코를 먹고왔습니다 !!",
          "speaker_id": "2",
          "time": "20210827 19:12"
        },
        {
          "id": "MDRW2100047614.1.17",
          "form": "타코 맛있었겠어요 !!",
          "original_form": "타코 맛있었겠어요 !!",
          "speaker_id": "1",
          "time": "20210827 19:23"
        },
        {
          "id": "MDRW2100047614.1.18",
          "form": "타코 먹고싶네요 가득싸서..",
          "original_form": "타코 먹고싶네요 가득싸서..",
          "speaker_id": "1",
          "time": "20210827 19:24"
        },
        {
          "id": "MDRW2100047614.1.19",
          "form": "입한가득 ㅠㅠ 소스가 진짜 너무 맛있더라고요",
          "original_form": "입한가득 ㅠㅠ 소스가 진짜 너무 맛있더라고요",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100047614.1.20",
          "form": "군데 지금은 다른것들 먹고싶어지고 막..ㅎ",
          "original_form": "군데 지금은 다른것들 먹고싶어지고 막..ㅎ",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100047614.1.21",
          "form": "진짜루요 ㅠㅠ 다이어트하면 먹고싶은 음식 매번 달라지잖아요..!",
          "original_form": "진짜루요 ㅠㅠ 다이어트하면 먹고싶은 음식 매번 달라지잖아요..!",
          "speaker_id": "1",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100047614.1.22",
          "form": "뭐가 제일 땡기시나요",
          "original_form": "뭐가 제일 땡기시나요",
          "speaker_id": "1",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100047614.1.23",
          "form": "저 라면이요 ㅠㅠ",
          "original_form": "저 라면이요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 21:01"
        },
        {
          "id": "MDRW2100047614.1.24",
          "form": "그 국물이 어찌나 맛나보이던지 ㅠㅠㅠ",
          "original_form": "그 국물이 어찌나 맛나보이던지 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 21:01"
        },
        {
          "id": "MDRW2100047614.1.25",
          "form": "와 .. 맞아요.. 라면 얼큰함 굿이죠 ..ㅠㅠ",
          "original_form": "와 .. 맞아요.. 라면 얼큰함 굿이죠 ..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 21:04"
        }
      ]
