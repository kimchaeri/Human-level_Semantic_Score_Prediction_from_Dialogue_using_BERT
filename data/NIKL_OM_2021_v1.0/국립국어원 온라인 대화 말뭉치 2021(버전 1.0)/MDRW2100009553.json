      "utterance": [
        {
          "id": "MDRW2100009553.1.1",
          "form": "광자매요ㅋㅋ",
          "original_form": "광자매요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:31"
        },
        {
          "id": "MDRW2100009553.1.2",
          "form": "주말드라마 ㅋㅋㅋ 예능은 놀면뭐하니 ㅋㅋ",
          "original_form": "주말드라마 ㅋㅋㅋ 예능은 놀면뭐하니 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:34"
        },
        {
          "id": "MDRW2100009553.1.3",
          "form": "광자매 한지 오래된거같은데",
          "original_form": "광자매 한지 오래된거같은데",
          "speaker_id": "1",
          "time": "20210823 00:34"
        },
        {
          "id": "MDRW2100009553.1.4",
          "form": "끝날때 안됐어요?",
          "original_form": "끝날때 안됐어요?",
          "speaker_id": "1",
          "time": "20210823 00:34"
        },
        {
          "id": "MDRW2100009553.1.5",
          "form": "그건 모르겠는데 아직도 해요 ㅋㅋ",
          "original_form": "그건 모르겠는데 아직도 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:35"
        },
        {
          "id": "MDRW2100009553.1.6",
          "form": "가족드라마라 길게하긴 하더라구요",
          "original_form": "가족드라마라 길게하긴 하더라구요",
          "speaker_id": "1",
          "time": "20210823 00:35"
        },
        {
          "id": "MDRW2100009553.1.7",
          "form": "전유퀴즈 슬의생 전참시 요런거 봐요",
          "original_form": "전유퀴즈 슬의생 전참시 요런거 봐요",
          "speaker_id": "1",
          "time": "20210823 00:36"
        },
        {
          "id": "MDRW2100009553.1.8",
          "form": "내용상 금방 끝날거 같진 않았어요 ㅋㅋ 저도 첨부터 본게 아니라 ㅋ",
          "original_form": "내용상 금방 끝날거 같진 않았어요 ㅋㅋ 저도 첨부터 본게 아니라 ㅋ",
          "speaker_id": "2",
          "time": "20210823 00:36"
        },
        {
          "id": "MDRW2100009553.1.9",
          "form": "오 전참시 재밌는데 요즘 잘 못봤어여ㅠ",
          "original_form": "오 전참시 재밌는데 요즘 잘 못봤어여ㅠ",
          "speaker_id": "2",
          "time": "20210823 00:36"
        },
        {
          "id": "MDRW2100009553.1.10",
          "form": "천뚱나올때가 잼나더라구요ㅎ",
          "original_form": "천뚱나올때가 잼나더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210823 00:38"
        },
        {
          "id": "MDRW2100009553.1.11",
          "form": "마자요 ㅋㅋ 먹방 한다고 하던데 얼마나 잘 먹고 깔끔하게 먹는지",
          "original_form": "마자요 ㅋㅋ 먹방 한다고 하던데 얼마나 잘 먹고 깔끔하게 먹는지",
          "speaker_id": "2",
          "time": "20210823 00:38"
        },
        {
          "id": "MDRW2100009553.1.12",
          "form": "진짜 맛있게먹더라구요",
          "original_form": "진짜 맛있게먹더라구요",
          "speaker_id": "1",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100009553.1.13",
          "form": "천뚱 본가 방송 하던거 봤는데 차림음식들 보고 와",
          "original_form": "천뚱 본가 방송 하던거 봤는데 차림음식들 보고 와",
          "speaker_id": "2",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100009553.1.14",
          "form": "저게 다들어가나 했는데 순삭",
          "original_form": "저게 다들어가나 했는데 순삭",
          "speaker_id": "1",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100009553.1.15",
          "form": "입안이 태평양",
          "original_form": "입안이 태평양",
          "speaker_id": "2",
          "time": "20210823 00:40"
        },
        {
          "id": "MDRW2100009553.1.16",
          "form": "많이 들어가는데 깔끔하게 먹던데요ㅎㅎ",
          "original_form": "많이 들어가는데 깔끔하게 먹던데요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 00:41"
        },
        {
          "id": "MDRW2100009553.1.17",
          "form": "그니까요 ㅋㅋ 안이 엄청 넓은듯해요",
          "original_form": "그니까요 ㅋㅋ 안이 엄청 넓은듯해요",
          "speaker_id": "2",
          "time": "20210823 00:41"
        }
      ]
