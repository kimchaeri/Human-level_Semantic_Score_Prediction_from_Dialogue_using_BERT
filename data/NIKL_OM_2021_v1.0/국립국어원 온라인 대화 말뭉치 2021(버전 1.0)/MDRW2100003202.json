      "utterance": [
        {
          "id": "MDRW2100003202.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 20:26"
        },
        {
          "id": "MDRW2100003202.1.2",
          "form": "네 안녕하세요 ㅎㅎ",
          "original_form": "네 안녕하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 20:27"
        },
        {
          "id": "MDRW2100003202.1.3",
          "form": "보시는 책 있으세요?",
          "original_form": "보시는 책 있으세요?",
          "speaker_id": "1",
          "time": "20210820 20:28"
        },
        {
          "id": "MDRW2100003202.1.4",
          "form": "전 그냥 카카오 페이지 소설이나 웹툰봐요",
          "original_form": "전 그냥 카카오 페이지 소설이나 웹툰봐요",
          "speaker_id": "2",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003202.1.5",
          "form": "무슨책 읽으세요?",
          "original_form": "무슨책 읽으세요?",
          "speaker_id": "2",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003202.1.6",
          "form": "가끔 철학책보고",
          "original_form": "가끔 철학책보고",
          "speaker_id": "1",
          "time": "20210820 20:32"
        },
        {
          "id": "MDRW2100003202.1.7",
          "form": "최근엔 만화책 사서보는데 좋더라고요",
          "original_form": "최근엔 만화책 사서보는데 좋더라고요",
          "speaker_id": "1",
          "time": "20210820 20:32"
        },
        {
          "id": "MDRW2100003202.1.8",
          "form": "철학책 와 전 안봐봤어요",
          "original_form": "철학책 와 전 안봐봤어요",
          "speaker_id": "2",
          "time": "20210820 20:33"
        },
        {
          "id": "MDRW2100003202.1.9",
          "form": "저희집도 만화책 요새 엄청사요",
          "original_form": "저희집도 만화책 요새 엄청사요",
          "speaker_id": "2",
          "time": "20210820 20:33"
        },
        {
          "id": "MDRW2100003202.1.10",
          "form": "딸이 엄청 샀어요",
          "original_form": "딸이 엄청 샀어요",
          "speaker_id": "2",
          "time": "20210820 20:33"
        },
        {
          "id": "MDRW2100003202.1.11",
          "form": "바니스타스 수기 귀멸의 칼날 어느날 공주가 되었다",
          "original_form": "바니스타스 수기 귀멸의 칼날 어느날 공주가 되었다",
          "speaker_id": "2",
          "time": "20210820 20:34"
        },
        {
          "id": "MDRW2100003202.1.12",
          "form": "문호스트레이독스",
          "original_form": "문호스트레이독스",
          "speaker_id": "2",
          "time": "20210820 20:34"
        },
        {
          "id": "MDRW2100003202.1.13",
          "form": "딸 나이가 어떻게 되시나요?",
          "original_form": "딸 나이가 어떻게 되시나요?",
          "speaker_id": "1",
          "time": "20210820 20:34"
        },
        {
          "id": "MDRW2100003202.1.14",
          "form": "엄청샀네요 허리가 휘었어요 ㅋㅋ",
          "original_form": "엄청샀네요 허리가 휘었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:34"
        },
        {
          "id": "MDRW2100003202.1.15",
          "form": "초6이요",
          "original_form": "초6이요",
          "speaker_id": "2",
          "time": "20210820 20:34"
        },
        {
          "id": "MDRW2100003202.1.16",
          "form": "웹툰을 책으로 만든건 가격이 다 12000원이 넘더라구요",
          "original_form": "웹툰을 책으로 만든건 가격이 다 12000원이 넘더라구요",
          "speaker_id": "2",
          "time": "20210820 20:35"
        },
        {
          "id": "MDRW2100003202.1.17",
          "form": "보는 만화는 뭐예요?",
          "original_form": "보는 만화는 뭐예요?",
          "speaker_id": "2",
          "time": "20210820 20:35"
        },
        {
          "id": "MDRW2100003202.1.18",
          "form": "저는 드래곤볼이랑 귀멸의 칼날 샀는데",
          "original_form": "저는 드래곤볼이랑 귀멸의 칼날 샀는데",
          "speaker_id": "1",
          "time": "20210820 20:37"
        },
        {
          "id": "MDRW2100003202.1.19",
          "form": "권수가 많아서 생각보다 비싸네요...",
          "original_form": "권수가 많아서 생각보다 비싸네요...",
          "speaker_id": "1",
          "time": "20210820 20:37"
        },
        {
          "id": "MDRW2100003202.1.20",
          "form": "오저 어렸을때",
          "original_form": "오저 어렸을때",
          "speaker_id": "2",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003202.1.21",
          "form": "비디오 첨산날 첨본게",
          "original_form": "비디오 첨산날 첨본게",
          "speaker_id": "2",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003202.1.22",
          "form": "드래곤볼이예요",
          "original_form": "드래곤볼이예요",
          "speaker_id": "2",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003202.1.23",
          "form": "그후로 진짜 재밌게 봤어요",
          "original_form": "그후로 진짜 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003202.1.24",
          "form": "비써죠 계속나와서",
          "original_form": "비써죠 계속나와서",
          "speaker_id": "2",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003202.1.25",
          "form": "귀멸의 칼날 애니도 2기 기다려요",
          "original_form": "귀멸의 칼날 애니도 2기 기다려요",
          "speaker_id": "2",
          "time": "20210820 20:39"
        },
        {
          "id": "MDRW2100003202.1.26",
          "form": "정말 잼나요",
          "original_form": "정말 잼나요",
          "speaker_id": "2",
          "time": "20210820 20:39"
        },
        {
          "id": "MDRW2100003202.1.27",
          "form": "원펀맨 만화책도 있으신가요?",
          "original_form": "원펀맨 만화책도 있으신가요?",
          "speaker_id": "2",
          "time": "20210820 20:40"
        },
        {
          "id": "MDRW2100003202.1.28",
          "form": "나루토도 있으신가요?",
          "original_form": "나루토도 있으신가요?",
          "speaker_id": "2",
          "time": "20210820 20:40"
        },
        {
          "id": "MDRW2100003202.1.29",
          "form": "요샌 이북만 사요",
          "original_form": "요샌 이북만 사요",
          "speaker_id": "2",
          "time": "20210820 20:41"
        },
        {
          "id": "MDRW2100003202.1.30",
          "form": "이북을 무분별하게 사서 도대체 몇권인지 안읽은게 더 많아서 반성중요",
          "original_form": "이북을 무분별하게 사서 도대체 몇권인지 안읽은게 더 많아서 반성중요",
          "speaker_id": "2",
          "time": "20210820 20:43"
        },
        {
          "id": "MDRW2100003202.1.31",
          "form": "저는 원펀맨이랑 나루토는 애니로만 봤는데",
          "original_form": "저는 원펀맨이랑 나루토는 애니로만 봤는데",
          "speaker_id": "1",
          "time": "20210820 20:44"
        },
        {
          "id": "MDRW2100003202.1.32",
          "form": "만화책이 재질때문에 그런가 보는재미가 있더라구요",
          "original_form": "만화책이 재질때문에 그런가 보는재미가 있더라구요",
          "speaker_id": "1",
          "time": "20210820 20:44"
        },
        {
          "id": "MDRW2100003202.1.33",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210820 20:45"
        },
        {
          "id": "MDRW2100003202.1.34",
          "form": "전블리치도 광팬이였어요 ㅋㅋ",
          "original_form": "전블리치도 광팬이였어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:45"
        },
        {
          "id": "MDRW2100003202.1.35",
          "form": "이북이 사기만하고 안보게돼요",
          "original_form": "이북이 사기만하고 안보게돼요",
          "speaker_id": "2",
          "time": "20210820 20:45"
        },
        {
          "id": "MDRW2100003202.1.36",
          "form": "만화책이북으로 사고 그냥 모셔놨네요",
          "original_form": "만화책이북으로 사고 그냥 모셔놨네요",
          "speaker_id": "2",
          "time": "20210820 20:45"
        },
        {
          "id": "MDRW2100003202.1.37",
          "form": "저희딸은 요새 베스트셀러도 읽고 저보다 나아요 ㅎㅎ",
          "original_form": "저희딸은 요새 베스트셀러도 읽고 저보다 나아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 20:46"
        },
        {
          "id": "MDRW2100003202.1.38",
          "form": "전 주식이나 타로책 좀봤어요",
          "original_form": "전 주식이나 타로책 좀봤어요",
          "speaker_id": "2",
          "time": "20210820 20:46"
        },
        {
          "id": "MDRW2100003202.1.39",
          "form": "소장은 로맨스 소설요",
          "original_form": "소장은 로맨스 소설요",
          "speaker_id": "2",
          "time": "20210820 20:46"
        },
        {
          "id": "MDRW2100003202.1.40",
          "form": "주식책 사시고 수익좀 있으셨어요?",
          "original_form": "주식책 사시고 수익좀 있으셨어요?",
          "speaker_id": "1",
          "time": "20210820 20:49"
        },
        {
          "id": "MDRW2100003202.1.41",
          "form": "공모주요 ㅋㅋ수익 소소하지만좋아요",
          "original_form": "공모주요 ㅋㅋ수익 소소하지만좋아요",
          "speaker_id": "2",
          "time": "20210820 20:50"
        },
        {
          "id": "MDRW2100003202.1.42",
          "form": "오늘도 5개팔아서 ㅋㅋ10만원 더벌었어요 ㅋㅋ",
          "original_form": "오늘도 5개팔아서 ㅋㅋ10만원 더벌었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:50"
        },
        {
          "id": "MDRW2100003202.1.43",
          "form": "타로책은 봤는데 기억이 안나요",
          "original_form": "타로책은 봤는데 기억이 안나요",
          "speaker_id": "2",
          "time": "20210820 20:51"
        },
        {
          "id": "MDRW2100003202.1.44",
          "form": "몇번 읽었는데도 기억이ㅜ안너네요",
          "original_form": "몇번 읽었는데도 기억이ㅜ안너네요",
          "speaker_id": "2",
          "time": "20210820 20:51"
        },
        {
          "id": "MDRW2100003202.1.45",
          "form": "요 타로가 책마다 내용이 틀려요",
          "original_form": "요 타로가 책마다 내용이 틀려요",
          "speaker_id": "2",
          "time": "20210820 20:51"
        }
      ]
