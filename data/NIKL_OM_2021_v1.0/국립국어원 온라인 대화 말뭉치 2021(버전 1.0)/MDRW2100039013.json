      "utterance": [
        {
          "id": "MDRW2100039013.1.1",
          "form": "오늘 뭐 드셨나요?!",
          "original_form": "오늘 뭐 드셨나요?!",
          "speaker_id": "1",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100039013.1.2",
          "form": "점심에는 그냥 백반 먹었어요",
          "original_form": "점심에는 그냥 백반 먹었어요",
          "speaker_id": "2",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100039013.1.3",
          "form": "음~ 백반 맛있죠",
          "original_form": "음~ 백반 맛있죠",
          "speaker_id": "1",
          "time": "20210903 16:29"
        },
        {
          "id": "MDRW2100039013.1.4",
          "form": "name1님은 어떤 거 드셨어요?",
          "original_form": "&name1&님은 어떤 거 드셨어요?",
          "speaker_id": "2",
          "time": "20210903 16:31"
        },
        {
          "id": "MDRW2100039013.1.5",
          "form": "저는 오늘~ 찜닭 먹고 나왔습니다 ㅎㅎ",
          "original_form": "저는 오늘~ 찜닭 먹고 나왔습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100039013.1.6",
          "form": "찜닭 좋아하시나요?",
          "original_form": "찜닭 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210904 14:18"
        },
        {
          "id": "MDRW2100039013.1.7",
          "form": "찜닭 맛있죠ㅋㅋ 어디꺼 드셨어요? 아니면 홈메이드?",
          "original_form": "찜닭 맛있죠ㅋㅋ 어디꺼 드셨어요? 아니면 홈메이드?",
          "speaker_id": "2",
          "time": "20210904 14:27"
        },
        {
          "id": "MDRW2100039013.1.8",
          "form": "저 두찜 시켜 먹었습니다!",
          "original_form": "저 두찜 시켜 먹었습니다!",
          "speaker_id": "1",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100039013.1.9",
          "form": "무난히 먹기에 제일 좋은 거 같아요 ㅎㅎ",
          "original_form": "무난히 먹기에 제일 좋은 거 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 14:30"
        },
        {
          "id": "MDRW2100039013.1.10",
          "form": "저희 동네에도 있는지 찾아봐야겠어요 ㅎㅎ",
          "original_form": "저희 동네에도 있는지 찾아봐야겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 14:35"
        },
        {
          "id": "MDRW2100039013.1.11",
          "form": "저번에 네고왕에도 나왔었던 걸로 기억해요!",
          "original_form": "저번에 네고왕에도 나왔었던 걸로 기억해요!",
          "speaker_id": "1",
          "time": "20210904 14:35"
        },
        {
          "id": "MDRW2100039013.1.12",
          "form": "근데 자취할 때는 좀 부담되는 가격이라",
          "original_form": "근데 자취할 때는 좀 부담되는 가격이라",
          "speaker_id": "1",
          "time": "20210904 14:35"
        },
        {
          "id": "MDRW2100039013.1.13",
          "form": "혼자 먹긴 좀 많은 양인가요?",
          "original_form": "혼자 먹긴 좀 많은 양인가요?",
          "speaker_id": "2",
          "time": "20210904 14:50"
        },
        {
          "id": "MDRW2100039013.1.14",
          "form": "두 마리라 혼자는 정말 많아요",
          "original_form": "두 마리라 혼자는 정말 많아요",
          "speaker_id": "1",
          "time": "20210904 14:51"
        },
        {
          "id": "MDRW2100039013.1.15",
          "form": "두마리나 ㅋㅋ 많긴 많네요",
          "original_form": "두마리나 ㅋㅋ 많긴 많네요",
          "speaker_id": "2",
          "time": "20210904 15:16"
        },
        {
          "id": "MDRW2100039013.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 근데 맛 다르게 가능해서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 근데 맛 다르게 가능해서",
          "speaker_id": "1",
