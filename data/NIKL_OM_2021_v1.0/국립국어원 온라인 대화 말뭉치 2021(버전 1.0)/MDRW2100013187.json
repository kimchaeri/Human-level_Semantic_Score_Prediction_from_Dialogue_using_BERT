        {
          "id": "MDRW2100013187.1.1",
          "form": "공연 좋아하세요?",
          "original_form": "공연 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 13:19"
        },
        {
          "id": "MDRW2100013187.1.2",
          "form": "공연 본 적은 많이 없어요",
          "original_form": "공연 본 적은 많이 없어요",
          "speaker_id": "2",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100013187.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210825 13:21"
        },
        {
          "id": "MDRW2100013187.1.4",
          "form": "저두요 ㅋㅋㅋ 콘서트 가보신 적 있으세요?",
          "original_form": "저두요 ㅋㅋㅋ 콘서트 가보신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100013187.1.5",
          "form": "아뇨 예전에 아이돌 좋아할 때도 못 가봤어요",
          "original_form": "아뇨 예전에 아이돌 좋아할 때도 못 가봤어요",
          "speaker_id": "2",
          "time": "20210825 13:22"
        },
        {
          "id": "MDRW2100013187.1.6",
          "form": "헐 티켓팅이 항상 어렵긴 하죠...",
          "original_form": "헐 티켓팅이 항상 어렵긴 하죠...",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013187.1.7",
          "form": "인기많은 아이돌이엇나봐요!",
          "original_form": "인기많은 아이돌이엇나봐요!",
          "speaker_id": "1",
          "time": "20210825 13:23"
        },
        {
          "id": "MDRW2100013187.1.8",
          "form": "맞아요 그것도 그렇고 어렸을 때는 학생이었어서 못 갔어요",
          "original_form": "맞아요 그것도 그렇고 어렸을 때는 학생이었어서 못 갔어요",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100013187.1.9",
          "form": "name1은 콘서트 가보셨어요?",
          "original_form": "&name1&은 콘서트 가보셨어요?",
          "speaker_id": "2",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100013187.1.10",
          "form": "아뇨 ㅋㅋㅋ  저도 못가봤어요",
          "original_form": "아뇨 ㅋㅋㅋ  저도 못가봤어요",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100013187.1.11",
          "form": "아이유나 태연 콘서트가보고싶긴한데",
          "original_form": "아이유나 태연 콘서트가보고싶긴한데",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100013187.1.12",
          "form": "티켓팅 장난아니게 어려울 것 같아서 ㅜㅜ",
          "original_form": "티켓팅 장난아니게 어려울 것 같아서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 13:27"
        },
        {
          "id": "MDRW2100013187.1.13",
          "form": "헐 저도요",
          "original_form": "헐 저도요",
          "speaker_id": "2",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100013187.1.14",
          "form": "죽기 전에 꼭 아이유랑 태연 콘서트 가보고 싶어요",
          "original_form": "죽기 전에 꼭 아이유랑 태연 콘서트 가보고 싶어요",
          "speaker_id": "2",
          "time": "20210825 13:28"
        },
        {
          "id": "MDRW2100013187.1.15",
          "form": "ㅋㅋㅋㅋ 아이유 노래에 사람들이 불빛반짝반짝하는게 넘 예쁘더라구요",
          "original_form": "ㅋㅋㅋㅋ 아이유 노래에 사람들이 불빛반짝반짝하는게 넘 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013187.1.16",
          "form": "ㅋㅋㅋ맞아요 저도 떼창 잘할 자신 있는데!!",
          "original_form": "ㅋㅋㅋ맞아요 저도 떼창 잘할 자신 있는데!!",
          "speaker_id": "2",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013187.1.17",
          "form": "ㅋㅋㅋㅋ한국인은 떼창이죠!!",
          "original_form": "ㅋㅋㅋㅋ한국인은 떼창이죠!!",
          "speaker_id": "1",
          "time": "20210825 13:29"
        },
        {
          "id": "MDRW2100013187.1.18",
          "form": "최근에 영화는 뭐보셨어요?",
          "original_form": "최근에 영화는 뭐보셨어요?",
          "speaker_id": "1",
          "time": "20210825 13:30"
        },
        {
          "id": "MDRW2100013187.1.19",
          "form": "블랙위도우요",
          "original_form": "블랙위도우요",
          "speaker_id": "2",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100013187.1.20",
          "form": "영화값이 비싸서 자주 보기도 힘들더라고요",
