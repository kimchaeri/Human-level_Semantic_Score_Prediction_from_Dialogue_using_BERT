        },
        {
          "id": "MDRW2100016175.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210826 10:39"
        },
        {
          "id": "MDRW2100016175.1.3",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 10:44"
        },
        {
          "id": "MDRW2100016175.1.4",
          "form": "아니요.. ㅋㅋㅋㅋㅋ 다이어트 때문에 하고는 해요",
          "original_form": "아니요.. ㅋㅋㅋㅋㅋ 다이어트 때문에 하고는 해요",
          "speaker_id": "2",
          "time": "20210826 10:52"
        },
        {
          "id": "MDRW2100016175.1.5",
          "form": "오어떤운동이요",
          "original_form": "오어떤운동이요",
          "speaker_id": "1",
          "time": "20210826 10:56"
        },
        {
          "id": "MDRW2100016175.1.6",
          "form": "요즘엔 유튜브보며 홈트레이닝해요!",
          "original_form": "요즘엔 유튜브보며 홈트레이닝해요!",
          "speaker_id": "2",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016175.1.7",
          "form": "예전엔 필라테스 몇개월 했어요",
          "original_form": "예전엔 필라테스 몇개월 했어요",
          "speaker_id": "2",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016175.1.8",
          "form": "오 필테 재밌나요",
          "original_form": "오 필테 재밌나요",
          "speaker_id": "1",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016175.1.9",
          "form": "네네 재밌더라구요! 자세교정도되구요",
          "original_form": "네네 재밌더라구요! 자세교정도되구요",
          "speaker_id": "2",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016175.1.10",
          "form": "운동 어떤거하세요??",
          "original_form": "운동 어떤거하세요??",
          "speaker_id": "2",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016175.1.11",
          "form": "홈트 해보려구요",
          "original_form": "홈트 해보려구요",
          "speaker_id": "1",
          "time": "20210826 11:29"
        },
        {
          "id": "MDRW2100016175.1.12",
          "form": "아아 그렇군요 다이어트하시나요?",
          "original_form": "아아 그렇군요 다이어트하시나요?",
          "speaker_id": "2",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100016175.1.13",
          "form": "아뇨 다이어트는 안합니다ㅎㅎ",
          "original_form": "아뇨 다이어트는 안합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 12:03"
        },
        {
          "id": "MDRW2100016175.1.14",
          "form": "건강때문에 하시는군요!",
          "original_form": "건강때문에 하시는군요!",
          "speaker_id": "2",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100016175.1.15",
          "form": "따로 건강을 위해 챙기능거 있으세여?",
          "original_form": "따로 건강을 위해 챙기능거 있으세여?",
          "speaker_id": "2",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100016175.1.16",
          "form": "비타민 챙겨먹어요!",
          "original_form": "비타민 챙겨먹어요!",
          "speaker_id": "1",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016175.1.17",
          "form": "오 그렇군요 저두 영양제 챙겨먹어요!",
          "original_form": "오 그렇군요 저두 영양제 챙겨먹어요!",
          "speaker_id": "2",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016175.1.18",
          "form": "비타민만 드시나요?",
          "original_form": "비타민만 드시나요?",
          "speaker_id": "2",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100016175.1.19",
