      "utterance": [
        {
          "id": "MDRW2100002164.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 00:11"
        },
        {
          "id": "MDRW2100002164.1.2",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "2",
          "time": "20210820 00:13"
        },
        {
          "id": "MDRW2100002164.1.3",
          "form": "유튜브나 넷플릭스 좋아하세요?",
          "original_form": "유튜브나 넷플릭스 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 00:14"
        },
        {
          "id": "MDRW2100002164.1.4",
          "form": "유튜브 좋아해요!",
          "original_form": "유튜브 좋아해요!",
          "speaker_id": "2",
          "time": "20210820 00:14"
        },
        {
          "id": "MDRW2100002164.1.5",
          "form": "좋아하는 유튜브 있으세요,'",
          "original_form": "좋아하는 유튜브 있으세요,'",
          "speaker_id": "2",
          "time": "20210820 00:14"
        },
        {
          "id": "MDRW2100002164.1.6",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210820 00:14"
        },
        {
          "id": "MDRW2100002164.1.7",
          "form": "저는 여행을 좋아해서 빠니보틀 보구 있어요ㅎ",
          "original_form": "저는 여행을 좋아해서 빠니보틀 보구 있어요ㅎ",
          "speaker_id": "1",
          "time": "20210820 00:15"
        },
        {
          "id": "MDRW2100002164.1.8",
          "form": "좋아하는 채널 있나요?",
          "original_form": "좋아하는 채널 있나요?",
          "speaker_id": "1",
          "time": "20210820 00:15"
        },
        {
          "id": "MDRW2100002164.1.9",
          "form": "우아한사파리요",
          "original_form": "우아한사파리요",
          "speaker_id": "2",
          "time": "20210820 00:21"
        },
        {
          "id": "MDRW2100002164.1.10",
          "form": "유기견구조하고 하는 채널이예요",
          "original_form": "유기견구조하고 하는 채널이예요",
          "speaker_id": "2",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002164.1.11",
          "form": "아 저도 유기견 관심 많은데 한번 봐야겠네요~",
          "original_form": "아 저도 유기견 관심 많은데 한번 봐야겠네요~",
          "speaker_id": "1",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002164.1.12",
          "form": "유투브는 컨텐츠가 엄청 다양해서 여러가지를\r\n경험해볼수 있어서 좋아요!",
          "original_form": "유투브는 컨텐츠가 엄청 다양해서 여러가지를\r\n경험해볼수 있어서 좋아요!",
          "speaker_id": "1",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002164.1.13",
          "form": "맞아요!! 저는 역사장르도 좋아해서  별별역사이야기도 구독했어요!!",
          "original_form": "맞아요!! 저는 역사장르도 좋아해서  별별역사이야기도 구독했어요!!",
          "speaker_id": "2",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002164.1.14",
          "form": "와ㅎㅎ네 역사도 좋아요",
          "original_form": "와ㅎㅎ네 역사도 좋아요",
          "speaker_id": "1",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002164.1.15",
          "form": "조승연 채널도 좋아해요 역사 많이 다뤄줘서",
          "original_form": "조승연 채널도 좋아해요 역사 많이 다뤄줘서",
          "speaker_id": "1",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002164.1.16",
          "form": "오호 한 번  시청해보고 구독이랑 좋아요 눌러야겠어용",
          "original_form": "오호 한 번  시청해보고 구독이랑 좋아요 눌러야겠어용",
          "speaker_id": "2",
          "time": "20210820 00:40"
        },
        {
          "id": "MDRW2100002164.1.17",
          "form": "네ㅎ 조승연 진짜 좋은내용 많이 하더라구요",
          "original_form": "네ㅎ 조승연 진짜 좋은내용 많이 하더라구요",
          "speaker_id": "1",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002164.1.18",
          "form": "요샌 코로나로 집에 잇을땐 넷플릭스도\r\n자주 보게되요~",
          "original_form": "요샌 코로나로 집에 잇을땐 넷플릭스도\r\n자주 보게되요~",
          "speaker_id": "1",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002164.1.19",
          "form": "주로 어떤거보세요?",
          "original_form": "주로 어떤거보세요?",
          "speaker_id": "2",
          "time": "20210820 00:44"
        },
        {
          "id": "MDRW2100002164.1.20",
          "form": "저는 승리호 이후로 안본지 오래예요",
          "original_form": "저는 승리호 이후로 안본지 오래예요",
          "speaker_id": "2",
          "time": "20210820 00:44"
        },
        {
          "id": "MDRW2100002164.1.21",
          "form": "저는 영국이나 프랑스영화 좋아해요ㅎ",
          "original_form": "저는 영국이나 프랑스영화 좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210820 00:44"
        },
        {
          "id": "MDRW2100002164.1.22",
          "form": "승리호 신기해서 두번 봣어요ㅎ",
          "original_form": "승리호 신기해서 두번 봣어요ㅎ",
          "speaker_id": "1",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002164.1.23",
          "form": "국내에 우주영화라니",
          "original_form": "국내에 우주영화라니",
          "speaker_id": "1",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002164.1.24",
          "form": "오오 맞아요  저도 많이 봤어용!",
          "original_form": "오오 맞아요  저도 많이 봤어용!",
          "speaker_id": "2",
          "time": "20210820 09:24"
        },
        {
          "id": "MDRW2100002164.1.25",
          "form": "두세번 봤어용",
          "original_form": "두세번 봤어용",
          "speaker_id": "2",
