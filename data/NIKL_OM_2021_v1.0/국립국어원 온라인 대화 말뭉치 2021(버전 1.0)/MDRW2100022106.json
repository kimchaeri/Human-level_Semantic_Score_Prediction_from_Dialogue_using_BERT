      "utterance": [
        {
          "id": "MDRW2100022106.1.1",
          "form": "건주기 사는 거 어때요??",
          "original_form": "건주기 사는 거 어때요??",
          "speaker_id": "1",
          "time": "20210901 07:17"
        },
        {
          "id": "MDRW2100022106.1.2",
          "form": "건조기?",
          "original_form": "건조기?",
          "speaker_id": "2",
          "time": "20210901 07:17"
        },
        {
          "id": "MDRW2100022106.1.3",
          "form": "나도 그 생각하고 있는데",
          "original_form": "나도 그 생각하고 있는데",
          "speaker_id": "2",
          "time": "20210901 07:17"
        },
        {
          "id": "MDRW2100022106.1.4",
          "form": "지금 빨래가 장난 아니다",
          "original_form": "지금 빨래가 장난 아니다",
          "speaker_id": "2",
          "time": "20210901 07:17"
        },
        {
          "id": "MDRW2100022106.1.5",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210901 07:17"
        },
        {
          "id": "MDRW2100022106.1.6",
          "form": "호우주의보 내릴 정도니 빨래 마를 틈이 없네",
          "original_form": "호우주의보 내릴 정도니 빨래 마를 틈이 없네",
          "speaker_id": "2",
          "time": "20210901 07:18"
        },
        {
          "id": "MDRW2100022106.1.7",
          "form": "건조기가 필요해요",
          "original_form": "건조기가 필요해요",
          "speaker_id": "1",
          "time": "20210901 07:18"
        },
        {
          "id": "MDRW2100022106.1.8",
          "form": "이럴때 있으면 좋은 건 맞지만",
          "original_form": "이럴때 있으면 좋은 건 맞지만",
          "speaker_id": "2",
          "time": "20210901 07:18"
        },
        {
          "id": "MDRW2100022106.1.9",
          "form": "사람들일아 대회해보니 장단점이 있더라고",
          "original_form": "사람들일아 대회해보니 장단점이 있더라고",
          "speaker_id": "2",
          "time": "20210901 07:18"
        },
        {
          "id": "MDRW2100022106.1.10",
          "form": "어떤거호?",
          "original_form": "어떤거호?",
          "speaker_id": "1",
          "time": "20210901 07:18"
        },
        {
          "id": "MDRW2100022106.1.11",
          "form": "자리를 차지하고.. 옷이 망가진대",
          "original_form": "자리를 차지하고.. 옷이 망가진대",
          "speaker_id": "2",
          "time": "20210901 07:19"
        },
        {
          "id": "MDRW2100022106.1.12",
          "form": "그렇긴 한데 옷이 안마르니까요",
          "original_form": "그렇긴 한데 옷이 안마르니까요",
          "speaker_id": "1",
          "time": "20210901 07:20"
        },
        {
          "id": "MDRW2100022106.1.13",
          "form": "조금 지나면 더 졿은 성능의 건조기가 나오지 않을까?",
          "original_form": "조금 지나면 더 졿은 성능의 건조기가 나오지 않을까?",
          "speaker_id": "2",
          "time": "20210901 07:20"
        },
        {
          "id": "MDRW2100022106.1.14",
          "form": "언제까지 기다려야 할 까요?",
          "original_form": "언제까지 기다려야 할 까요?",
          "speaker_id": "1",
          "time": "20210901 07:22"
        },
        {
          "id": "MDRW2100022106.1.15",
          "form": "집안일을 다 기계로 하려고 하면 안돼",
          "original_form": "집안일을 다 기계로 하려고 하면 안돼",
          "speaker_id": "2",
          "time": "20210901 07:23"
        },
        {
          "id": "MDRW2100022106.1.16",
          "form": "빨래 건조기 설거지 식기세척기 청소 로봋청소기 쓰면",
          "original_form": "빨래 건조기 설거지 식기세척기 청소 로봋청소기 쓰면",
          "speaker_id": "2",
          "time": "20210901 07:23"
        },
        {
          "id": "MDRW2100022106.1.17",
          "form": "할만한 집인일이 없지",
          "original_form": "할만한 집인일이 없지",
          "speaker_id": "2",
          "time": "20210901 07:23"
        },
        {
          "id": "MDRW2100022106.1.18",
          "form": "굳이 사람 손을 쓸 필요가",
          "original_form": "굳이 사람 손을 쓸 필요가",
          "speaker_id": "1",
          "time": "20210901 07:24"
        }
      ]
