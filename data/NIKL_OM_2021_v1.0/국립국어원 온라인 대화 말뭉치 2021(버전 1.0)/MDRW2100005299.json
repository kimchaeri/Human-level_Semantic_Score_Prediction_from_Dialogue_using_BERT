      "utterance": [
        {
          "id": "MDRW2100005299.1.1",
          "form": "즐겨듣는 노래가 있나요",
          "original_form": "즐겨듣는 노래가 있나요",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005299.1.2",
          "form": "제가 옛날에 멈춰살아서... 이승기노래 아이유노래 돌려들어요",
          "original_form": "제가 옛날에 멈춰살아서... 이승기노래 아이유노래 돌려들어요",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005299.1.3",
          "form": "좋아하는 가수나 노래?",
          "original_form": "좋아하는 가수나 노래?",
          "speaker_id": "2",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005299.1.4",
          "form": "이승기노래?",
          "original_form": "이승기노래?",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005299.1.5",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005299.1.6",
          "form": "나랑결혼해줄래 이거요?",
          "original_form": "나랑결혼해줄래 이거요?",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005299.1.7",
          "form": "너무 옛날인데요...ㅠ",
          "original_form": "너무 옛날인데요...ㅠ",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005299.1.8",
          "form": "아앗..",
          "original_form": "아앗..",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005299.1.9",
          "form": "숲 인가 그거 앨범이랑",
          "original_form": "숲 인가 그거 앨범이랑",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005299.1.10",
          "form": "...나랑결혼해줄래도 가끔들어요",
          "original_form": "...나랑결혼해줄래도 가끔들어요",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005299.1.11",
          "form": "제가 너무 올드했네요 핫..",
          "original_form": "제가 너무 올드했네요 핫..",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005299.1.12",
          "form": "누난 내여자니까",
          "original_form": "누난 내여자니까",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005299.1.13",
          "form": "만 빼고 다 듣고있어요",
          "original_form": "만 빼고 다 듣고있어요",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005299.1.14",
          "form": "이것도 사실 노래자체는 좋은데 감성이안맞아서 ㅍ",
          "original_form": "이것도 사실 노래자체는 좋은데 감성이안맞아서 ㅍ",
          "speaker_id": "2",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005299.1.15",
          "form": "어떤 느낌인지 알거같아요ㅎ",
          "original_form": "어떤 느낌인지 알거같아요ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005299.1.16",
          "form": "아이유씨 노래는 어떤거 들어요?",
          "original_form": "아이유씨 노래는 어떤거 들어요?",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005299.1.17",
          "form": "아이유 노래는 거의 다 돌려듣는데",
          "original_form": "아이유 노래는 거의 다 돌려듣는데",
          "speaker_id": "2",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005299.1.18",
          "form": "밤편지 좋아해서 좀더 자주들어요",
          "original_form": "밤편지 좋아해서 좀더 자주들어요",
          "speaker_id": "2",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005299.1.19",
          "form": "그 노래 좋더라구요",
          "original_form": "그 노래 좋더라구요",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005299.1.20",
          "form": "특히 요즘 같은 때 듣기 좋은 거 같아요",
          "original_form": "특히 요즘 같은 때 듣기 좋은 거 같아요",
          "speaker_id": "1",
          "time": "20210821 21:54"
        },
        {
          "id": "MDRW2100005299.1.21",
          "form": "맞아요 늦여름밤을 주제로한게 아닌가싶을정도로",
          "original_form": "맞아요 늦여름밤을 주제로한게 아닌가싶을정도로",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005299.1.22",
          "form": "좋아하는 가수나 노래 있나요??",
          "original_form": "좋아하는 가수나 노래 있나요??",
          "speaker_id": "2",
          "time": "20210821 21:55"
        },
        {
          "id": "MDRW2100005299.1.23",
          "form": "저 악뮤 좋아해요",
          "original_form": "저 악뮤 좋아해요",
          "speaker_id": "1",
          "time": "20210821 21:55"
        }
      ]
    }
