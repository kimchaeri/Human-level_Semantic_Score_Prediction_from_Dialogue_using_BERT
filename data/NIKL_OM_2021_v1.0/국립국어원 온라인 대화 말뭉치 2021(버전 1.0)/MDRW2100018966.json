      "utterance": [
        {
          "id": "MDRW2100018966.1.1",
          "form": "애들은 성적표 받아왔어?",
          "original_form": "애들은 성적표 받아왔어?",
          "speaker_id": "2",
          "time": "20210827 14:36"
        },
        {
          "id": "MDRW2100018966.1.2",
          "form": "ㅇㅇ 방금 가져왔어",
          "original_form": "ㅇㅇ 방금 가져왔어",
          "speaker_id": "1",
          "time": "20210827 14:36"
        },
        {
          "id": "MDRW2100018966.1.3",
          "form": "잘 나왔어? ㅎㅎ",
          "original_form": "잘 나왔어? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:36"
        },
        {
          "id": "MDRW2100018966.1.4",
          "form": "전체적으로 잘했는데 잘함이 하나 있네.. ㅋ",
          "original_form": "전체적으로 잘했는데 잘함이 하나 있네.. ㅋ",
          "speaker_id": "1",
          "time": "20210827 14:37"
        },
        {
          "id": "MDRW2100018966.1.5",
          "form": "다른 건 다 매우잘함이고?",
          "original_form": "다른 건 다 매우잘함이고?",
          "speaker_id": "2",
          "time": "20210827 14:37"
        },
        {
          "id": "MDRW2100018966.1.6",
          "form": "ㅇㅇ ㅎㅎ 잘했는데도 하나가 잘함이라 아쉽네",
          "original_form": "ㅇㅇ ㅎㅎ 잘했는데도 하나가 잘함이라 아쉽네",
          "speaker_id": "1",
          "time": "20210827 14:37"
        },
        {
          "id": "MDRW2100018966.1.7",
          "form": "욕심이 끝이 없지 ㅋㅋ 어떤 과목이 잘함이야?",
          "original_form": "욕심이 끝이 없지 ㅋㅋ 어떤 과목이 잘함이야?",
          "speaker_id": "2",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018966.1.8",
          "form": "큰아들은 국어, 작은 아들은 사회",
          "original_form": "큰아들은 국어, 작은 아들은 사회",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018966.1.9",
          "form": "ㅎㅎ 보나마다 다 쓰는 거였구만",
          "original_form": "ㅎㅎ 보나마다 다 쓰는 거였구만",
          "speaker_id": "2",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018966.1.10",
          "form": "그런 것 같아 ㅋㅋ",
          "original_form": "그런 것 같아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018966.1.11",
          "form": "너무 아쉬워하지마 그것도 잘한건데 ㅎㅎ",
          "original_form": "너무 아쉬워하지마 그것도 잘한건데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100018966.1.12",
          "form": "ㅇㅇ 잘했다고 칭찬해줬어 ㅎㅎ",
          "original_form": "ㅇㅇ 잘했다고 칭찬해줬어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100018966.1.13",
          "form": "나름 고생해서 얻은 점수인데 무한칭찬해줘야지",
          "original_form": "나름 고생해서 얻은 점수인데 무한칭찬해줘야지",
