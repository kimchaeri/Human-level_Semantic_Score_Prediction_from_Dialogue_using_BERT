      "utterance": [
        {
          "id": "MDRW2100044487.1.1",
          "form": "마라탕 중간이나 매운맛이요!!!",
          "original_form": "마라탕 중간이나 매운맛이요!!!",
          "speaker_id": "1",
          "time": "20210907 22:52"
        },
        {
          "id": "MDRW2100044487.1.2",
          "form": "저는 중간맛 자주 먹어요 ㅋㅋ",
          "original_form": "저는 중간맛 자주 먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044487.1.3",
          "form": "중간맛이 무난무난",
          "original_form": "중간맛이 무난무난",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044487.1.4",
          "form": "적당히 자극적인것같아요!!",
          "original_form": "적당히 자극적인것같아요!!",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044487.1.5",
          "form": "맞아요 저는 매운건 잘먹는데 그 마라 마비되는 매운맛은 안좋아해서 중간맛으로 먹어요!",
          "original_form": "맞아요 저는 매운건 잘먹는데 그 마라 마비되는 매운맛은 안좋아해서 중간맛으로 먹어요!",
          "speaker_id": "2",
          "time": "20210907 23:00"
        },
        {
          "id": "MDRW2100044487.1.6",
          "form": "맞아요 마라만의 매운맛이 있어요",
          "original_form": "맞아요 마라만의 매운맛이 있어요",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044487.1.7",
          "form": "마비되는 맛ㅋㅋ",
          "original_form": "마비되는 맛ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044487.1.8",
          "form": "그리고 뭔가모르게",
          "original_form": "그리고 뭔가모르게",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044487.1.9",
          "form": "매운거에 비해 속이 많이쓰려요",
          "original_form": "매운거에 비해 속이 많이쓰려요",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044487.1.10",
          "form": "그쵸 그 매운맛이 엽떡불닭과는 다른 매운맛이에요 ㅋㅋ",
          "original_form": "그쵸 그 매운맛이 엽떡불닭과는 다른 매운맛이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044487.1.11",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044487.1.12",
          "form": "진짜 속을 깎는것같은 매운맛이에요",
          "original_form": "진짜 속을 깎는것같은 매운맛이에요",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044487.1.13",
          "form": "그래서 맛있는데 자주는 못먹게 되는 것 같아요 ㅠㅠ",
          "original_form": "그래서 맛있는데 자주는 못먹게 되는 것 같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044487.1.14",
          "form": "맞아요ㅠㅠ 건강에 최악인 맛",
          "original_form": "맞아요ㅠㅠ 건강에 최악인 맛",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044487.1.15",
          "form": "라면보다 몸에 안좋다고 하더라구요",
          "original_form": "라면보다 몸에 안좋다고 하더라구요",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044487.1.16",
          "form": "그쵸 ㅠㅠ 만드는거 보니까 .. 몸에 안좋을수밖에 없더라구요",
          "original_form": "그쵸 ㅠㅠ 만드는거 보니까 .. 몸에 안좋을수밖에 없더라구요",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100044487.1.17",
          "form": "만드는거보신적 있나요?!",
          "original_form": "만드는거보신적 있나요?!",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044487.1.18",
          "form": "인터넷에 떠서 봤는데 무슨 기름..? 같은게 엄청 들어가더라구요",
          "original_form": "인터넷에 떠서 봤는데 무슨 기름..? 같은게 엄청 들어가더라구요",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044487.1.19",
          "form": "맞아요 마라소스보면",
          "original_form": "맞아요 마라소스보면",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044487.1.20",
          "form": "기름에 절여져있긴하더라구요",
          "original_form": "기름에 절여져있긴하더라구요",
          "speaker_id": "1",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044487.1.21",
          "form": "기름에 절여진 마라...",
          "original_form": "기름에 절여진 마라...",
          "speaker_id": "1",
          "time": "20210907 23:25"
        }
      ]
