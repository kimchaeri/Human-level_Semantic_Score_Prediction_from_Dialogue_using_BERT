        },
        {
          "id": "MDRW2100006611.1.3",
          "form": "혹시 취업하셨나요",
          "original_form": "혹시 취업하셨나요",
          "speaker_id": "1",
          "time": "20210908 03:15"
        },
        {
          "id": "MDRW2100006611.1.4",
          "form": "아니요 아직 학생이에요ㅠㅠ",
          "original_form": "아니요 아직 학생이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100006611.1.5",
          "form": "아하 그렇군요 저도 대학생이에요",
          "original_form": "아하 그렇군요 저도 대학생이에요",
          "speaker_id": "1",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100006611.1.6",
          "form": "그럼 알바해보신적있나요",
          "original_form": "그럼 알바해보신적있나요",
          "speaker_id": "1",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100006611.1.7",
          "form": "알바는 다양하게 많이 해봤죠!!",
          "original_form": "알바는 다양하게 많이 해봤죠!!",
          "speaker_id": "2",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100006611.1.8",
          "form": "어떤 아르바이트 해보셨나요?",
          "original_form": "어떤 아르바이트 해보셨나요?",
          "speaker_id": "1",
          "time": "20210908 03:16"
        },
        {
          "id": "MDRW2100006611.1.9",
          "form": "저 식당도 해보고 관공서도 해보고",
          "original_form": "저 식당도 해보고 관공서도 해보고",
          "speaker_id": "2",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100006611.1.10",
          "form": "이것저것 다 해봤어요",
          "original_form": "이것저것 다 해봤어요",
          "speaker_id": "2",
          "time": "20210908 03:18"
        },
        {
          "id": "MDRW2100006611.1.11",
          "form": "학교에서하는 근로는 해보셨나요",
          "original_form": "학교에서하는 근로는 해보셨나요",
          "speaker_id": "1",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100006611.1.12",
          "form": "아 저 과사 이런건 못해봤어요",
          "original_form": "아 저 과사 이런건 못해봤어요",
          "speaker_id": "2",
          "time": "20210908 03:19"
        },
        {
          "id": "MDRW2100006611.1.13",
          "form": "학교 식당에서만 일해봤다는....ㅠ",
          "original_form": "학교 식당에서만 일해봤다는....ㅠ",
          "speaker_id": "2",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100006611.1.14",
          "form": "아하 학교 식당은 알바개념인건가요",
          "original_form": "아하 학교 식당은 알바개념인건가요",
          "speaker_id": "1",
          "time": "20210908 03:20"
        },
        {
          "id": "MDRW2100006611.1.15",
          "form": "근로 개념이긴한데 전혀 편하지 않더라고요 ㅋㅋㅋㅋ",
          "original_form": "근로 개념이긴한데 전혀 편하지 않더라고요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:21"
        },
        {
          "id": "MDRW2100006611.1.16",
          "form": "일반식당도 힘들텐데 학교니까 더 힘들었겠네요",
