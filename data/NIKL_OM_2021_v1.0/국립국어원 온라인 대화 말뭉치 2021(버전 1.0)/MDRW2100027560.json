      "utterance": [
        {
          "id": "MDRW2100027560.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027560.1.2",
          "form": "안녕하세요! 가장 최근에는 어떤 물건 사셨어요?",
          "original_form": "안녕하세요! 가장 최근에는 어떤 물건 사셨어요?",
          "speaker_id": "2",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027560.1.3",
          "form": "신.세.계.마트에서 소불고기",
          "original_form": "신.세.계.마트에서 소불고기",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027560.1.4",
          "form": "샀어용",
          "original_form": "샀어용",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027560.1.5",
          "form": "우와 맛있겠네요. 오늘 저녁에 먹을건가봐요!",
          "original_form": "우와 맛있겠네요. 오늘 저녁에 먹을건가봐요!",
          "speaker_id": "2",
          "time": "20210902 13:38"
        },
        {
          "id": "MDRW2100027560.1.6",
          "form": "ㅋㅋ아뇨 신세.계",
          "original_form": "ㅋㅋ아뇨 신세.계",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027560.1.7",
          "form": "쿠폰 잘못 뿌려서",
          "original_form": "쿠폰 잘못 뿌려서",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027560.1.8",
          "form": "줍줍",
          "original_form": "줍줍",
          "speaker_id": "1",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100027560.1.9",
          "form": "오... 전 쿠폰도 잘 못줍고 할인도 잘 못받는데",
          "original_form": "오... 전 쿠폰도 잘 못줍고 할인도 잘 못받는데",
          "speaker_id": "2",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100027560.1.10",
          "form": "능력자ㅎㅎ",
          "original_form": "능력자ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100027560.1.11",
          "form": "친구덕에ㅋㅋ",
          "original_form": "친구덕에ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100027560.1.12",
          "form": "근데 잘못 뿌린거는 맞아서",
          "original_form": "근데 잘못 뿌린거는 맞아서",
          "speaker_id": "1",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100027560.1.13",
          "form": "취소당할까봐",
          "original_form": "취소당할까봐",
          "speaker_id": "1",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100027560.1.14",
          "form": "덕분에 맛있는 소불고기 먹으니 완전 이득이네요 즐겨요 이기분ㅎㅎㅎ",
          "original_form": "덕분에 맛있는 소불고기 먹으니 완전 이득이네요 즐겨요 이기분ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:47"
        },
        {
          "id": "MDRW2100027560.1.15",
          "form": "ㅋㅋ와야 일단. .",
          "original_form": "ㅋㅋ와야 일단. .",
          "speaker_id": "1",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100027560.1.16",
          "form": "네 오면 맛있게 드시기 바랍니다ㅎㅎ",
          "original_form": "네 오면 맛있게 드시기 바랍니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100027560.1.17",
          "form": "넵ㅋㅋ",
          "original_form": "넵ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100027560.1.18",
          "form": "뭐 사셨나요 최근에",
          "original_form": "뭐 사셨나요 최근에",
          "speaker_id": "1",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100027560.1.19",
          "form": "전 좀 재미없는거 샀어요 모니터 받침대요ㅎㅎ",
          "original_form": "전 좀 재미없는거 샀어요 모니터 받침대요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027560.1.20",
          "form": "음 재미가 없긴하네요",
          "original_form": "음 재미가 없긴하네요",
          "speaker_id": "1",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100027560.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027560.1.22",
          "form": "그래도 필요하니까 사셨겠죠",
          "original_form": "그래도 필요하니까 사셨겠죠",
          "speaker_id": "1",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100027560.1.23",
          "form": "네ㅎㅎ 두꺼운 책 올려놓고 쓰다가 그냥 하나 사는게 낫겠다 싶어서ㅋㅋㅋ",
          "original_form": "네ㅎㅎ 두꺼운 책 올려놓고 쓰다가 그냥 하나 사는게 낫겠다 싶어서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:02"
        },
        {
          "id": "MDRW2100027560.1.24",
          "form": "ㅋㅋ그럼 옳은 소비인듯 합니다",
          "original_form": "ㅋㅋ그럼 옳은 소비인듯 합니다",
          "speaker_id": "1",
