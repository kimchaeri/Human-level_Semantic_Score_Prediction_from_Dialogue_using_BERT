      "utterance": [
        {
          "id": "MDRW2100018022.1.1",
          "form": "어디 가보셨어요",
          "original_form": "어디 가보셨어요",
          "speaker_id": "2",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100018022.1.2",
          "form": "전 라오스 다시 가보고 싶어요 ㅋㅋㅋ",
          "original_form": "전 라오스 다시 가보고 싶어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100018022.1.3",
          "form": "라오스도 진짜 좋다고 들었어여! 전 안가봤는데",
          "original_form": "라오스도 진짜 좋다고 들었어여! 전 안가봤는데",
          "speaker_id": "2",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100018022.1.4",
          "form": "네 너무 좋앗어요 ㅋㅋㅋㅋ",
          "original_form": "네 너무 좋앗어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100018022.1.5",
          "form": "어디가 특히 좋았어요? 다음에 라오스 꼭 가려고요",
          "original_form": "어디가 특히 좋았어요? 다음에 라오스 꼭 가려고요",
          "speaker_id": "2",
          "time": "20210827 08:18"
        },
        {
          "id": "MDRW2100018022.1.6",
          "form": "저는 루앙프라방이 너무 좋앗어요",
          "original_form": "저는 루앙프라방이 너무 좋앗어요",
          "speaker_id": "1",
          "time": "20210827 12:01"
        },
        {
          "id": "MDRW2100018022.1.7",
          "form": "오 사진 찾아보니까 진짜 멋있는 도시네요",
          "original_form": "오 사진 찾아보니까 진짜 멋있는 도시네요",
          "speaker_id": "2",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100018022.1.8",
          "form": "네 추천해요!",
          "original_form": "네 추천해요!",
          "speaker_id": "1",
          "time": "20210827 12:11"
        },
        {
          "id": "MDRW2100018022.1.9",
          "form": "name2은 제일 좋았던 여행지가 어디신가요?",
          "original_form": "&name2&은 제일 좋았던 여행지가 어디신가요?",
          "speaker_id": "1",
          "time": "20210827 12:11"
        },
        {
          "id": "MDRW2100018022.1.10",
          "form": "저는 도쿄 부근에 하코네를 다녀왔었는데",
          "original_form": "저는 도쿄 부근에 하코네를 다녀왔었는데",
          "speaker_id": "2",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018022.1.11",
          "form": "좀 고즈넉하면서도 화산 볼 수 있고 해서 신기하더라구요",
          "original_form": "좀 고즈넉하면서도 화산 볼 수 있고 해서 신기하더라구요",
          "speaker_id": "2",
          "time": "20210827 12:13"
        },
        {
          "id": "MDRW2100018022.1.12",
          "form": "하코네 좋다는 얘기 들어봣어요",
          "original_form": "하코네 좋다는 얘기 들어봣어요",
          "speaker_id": "1",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018022.1.13",
          "form": "언제다녀오셧어요?",
          "original_form": "언제다녀오셧어요?",
          "speaker_id": "1",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018022.1.14",
          "form": "2019년 초에 다녀왔어요ㅎㅎ",
          "original_form": "2019년 초에 다녀왔어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:16"
        },
        {
          "id": "MDRW2100018022.1.15",
          "form": "전 도쿄도 한번도 안가봐서 코로나 끝나면 가보고 싶어요 ㅎㅎ",
          "original_form": "전 도쿄도 한번도 안가봐서 코로나 끝나면 가보고 싶어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:17"
        },
        {
          "id": "MDRW2100018022.1.16",
          "form": "저두 도쿄 디즈니랜드 너무 가고싶어요ㅎㅎ",
          "original_form": "저두 도쿄 디즈니랜드 너무 가고싶어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100018022.1.17",
          "form": "뭔가 서울이랑 비슷한 느낌이라 되게 특이한 여행지는 아니라는 생각이 들었는데",
          "original_form": "뭔가 서울이랑 비슷한 느낌이라 되게 특이한 여행지는 아니라는 생각이 들었는데",
          "speaker_id": "2",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100018022.1.18",
          "form": "그래도 여행다니기는 엄청 편했어요",
          "original_form": "그래도 여행다니기는 엄청 편했어요",
          "speaker_id": "2",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018022.1.19",
          "form": "맞아요 일본이 여행다니기는 참 편하죠",
          "original_form": "맞아요 일본이 여행다니기는 참 편하죠",
          "speaker_id": "1",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100018022.1.20",
          "form": "유니버셜 스튜디오도 가고싶어요 ㅠㅠㅎㅎ",
          "original_form": "유니버셜 스튜디오도 가고싶어요 ㅠㅠㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100018022.1.21",
          "form": "아 유니버셜도 맞아오 ㅠ가고싶어요ㅠ",
          "original_form": "아 유니버셜도 맞아오 ㅠ가고싶어요ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:31"
        },
        {
          "id": "MDRW2100018022.1.22",
          "form": "마리오 랜드인가 그게 문열었대서 너무 궁금해요",
          "original_form": "마리오 랜드인가 그게 문열었대서 너무 궁금해요",
          "speaker_id": "2",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018022.1.23",
          "form": "맞아요 ㅠㅠ 해리포터 테마파크도 또 가고싶어요",
          "original_form": "맞아요 ㅠㅠ 해리포터 테마파크도 또 가고싶어요",
          "speaker_id": "1",
          "time": "20210827 12:32"
        },
        {
          "id": "MDRW2100018022.1.24",
          "form": "아 이미 다녀오셨군요ㅠㅠ 해리포터 얼마전에 정주행 끝내서 저두 너무 가고싶은 곳이에요",
          "original_form": "아 이미 다녀오셨군요ㅠㅠ 해리포터 얼마전에 정주행 끝내서 저두 너무 가고싶은 곳이에요",
          "speaker_id": "2",
          "time": "20210827 13:01"
        },
        {
          "id": "MDRW2100018022.1.25",
          "form": "네 저도 너무 가고싶어요 ㅠㅠ",
          "original_form": "네 저도 너무 가고싶어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018022.1.26",
          "form": "도쿄도 한번도 안가봐서 도쿄도 꼭 가보고싶어요 코로나끝나면",
          "original_form": "도쿄도 한번도 안가봐서 도쿄도 꼭 가보고싶어요 코로나끝나면",
          "speaker_id": "1",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018022.1.27",
          "form": "도쿄는 근데 진짜 한국어 간판도 많이 생기고 해서",
          "original_form": "도쿄는 근데 진짜 한국어 간판도 많이 생기고 해서",
          "speaker_id": "2",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018022.1.28",
          "form": "명동같은 느낌 나기도 했어요ㅋㅋㅋ",
          "original_form": "명동같은 느낌 나기도 했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:27"
        },
        {
          "id": "MDRW2100018022.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ오사카나 후쿠오카도 한국어간판 많더라구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ오사카나 후쿠오카도 한국어간판 많더라구요",
          "speaker_id": "1",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018022.1.30",
          "form": "그나마 홋카이도는 덜햇던 것 같은데 ㅋㅋㅋㅋ",
          "original_form": "그나마 홋카이도는 덜햇던 것 같은데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:36"
        },
        {
          "id": "MDRW2100018022.1.31",
          "form": "다른 도시들도 그렇군요ㅋㅋㅋㅋ 저는 도쿄밖에 안가봐서 다른 도시들도 궁금하네요",
          "original_form": "다른 도시들도 그렇군요ㅋㅋㅋㅋ 저는 도쿄밖에 안가봐서 다른 도시들도 궁금하네요",
          "speaker_id": "2",
          "time": "20210827 13:37"
        },
        {
          "id": "MDRW2100018022.1.32",
          "form": "홋카이도도 가보고싶고",
          "original_form": "홋카이도도 가보고싶고",
