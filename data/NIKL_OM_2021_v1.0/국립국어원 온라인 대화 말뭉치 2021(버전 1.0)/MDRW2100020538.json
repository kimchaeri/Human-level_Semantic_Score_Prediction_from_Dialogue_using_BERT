      "utterance": [
        {
          "id": "MDRW2100020538.1.1",
          "form": "한번 도전해보겠어요",
          "original_form": "한번 도전해보겠어요",
          "speaker_id": "2",
          "time": "20210828 09:26"
        },
        {
          "id": "MDRW2100020538.1.2",
          "form": "제가 크림들어간 라면에 크게 실패한 적이 있어서",
          "original_form": "제가 크림들어간 라면에 크게 실패한 적이 있어서",
          "speaker_id": "2",
          "time": "20210828 09:26"
        },
        {
          "id": "MDRW2100020538.1.3",
          "form": "음 맛있을거에요 저 느끼한거 싫어하는데",
          "original_form": "음 맛있을거에요 저 느끼한거 싫어하는데",
          "speaker_id": "1",
          "time": "20210828 09:26"
        },
        {
          "id": "MDRW2100020538.1.4",
          "form": "불닭에도 선입견을 씌웠네요",
          "original_form": "불닭에도 선입견을 씌웠네요",
          "speaker_id": "2",
          "time": "20210828 09:26"
        },
        {
          "id": "MDRW2100020538.1.5",
          "form": "이건 괜찮더라구요",
          "original_form": "이건 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210828 09:26"
        },
        {
          "id": "MDRW2100020538.1.6",
          "form": "크림들어간 라면?",
          "original_form": "크림들어간 라면?",
          "speaker_id": "1",
          "time": "20210828 09:27"
        },
        {
          "id": "MDRW2100020538.1.7",
          "form": "뭐가있죠?",
          "original_form": "뭐가있죠?",
          "speaker_id": "1",
          "time": "20210828 09:27"
        },
        {
          "id": "MDRW2100020538.1.8",
          "form": "네 까르보나라면이었나",
          "original_form": "네 까르보나라면이었나",
          "speaker_id": "2",
          "time": "20210828 09:27"
        },
        {
          "id": "MDRW2100020538.1.9",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "1",
          "time": "20210828 09:27"
        },
        {
          "id": "MDRW2100020538.1.10",
          "form": "이름부터",
          "original_form": "이름부터",
          "speaker_id": "1",
          "time": "20210828 09:27"
        },
        {
          "id": "MDRW2100020538.1.11",
          "form": "뭔가 특이한 라면이 있었어요",
          "original_form": "뭔가 특이한 라면이 있었어요",
          "speaker_id": "2",
          "time": "20210828 09:27"
        },
        {
          "id": "MDRW2100020538.1.12",
          "form": "저 사실 까르보나라를 안좋아해서",
          "original_form": "저 사실 까르보나라를 안좋아해서",
          "speaker_id": "1",
          "time": "20210828 09:27"
        },
        {
          "id": "MDRW2100020538.1.13",
          "form": "느끼한걸 못먹겠더라구요",
          "original_form": "느끼한걸 못먹겠더라구요",
          "speaker_id": "1",
          "time": "20210828 09:27"
        },
        {
          "id": "MDRW2100020538.1.14",
          "form": "그럼 토마토파이신가요",
          "original_form": "그럼 토마토파이신가요",
          "speaker_id": "2",
          "time": "20210828 09:27"
        },
        {
          "id": "MDRW2100020538.1.15",
          "form": "알리오올리오?",
          "original_form": "알리오올리오?",
          "speaker_id": "2",
          "time": "20210828 09:27"
        },
        {
          "id": "MDRW2100020538.1.16",
          "form": "알리오올리오 새우빼고",
          "original_form": "알리오올리오 새우빼고",
          "speaker_id": "1",
          "time": "20210828 09:28"
        },
        {
          "id": "MDRW2100020538.1.17",
          "form": "한거 좋아해요",
          "original_form": "한거 좋아해요",
          "speaker_id": "1",
          "time": "20210828 09:28"
        },
        {
          "id": "MDRW2100020538.1.18",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "2",
          "time": "20210828 09:28"
        },
        {
          "id": "MDRW2100020538.1.19",
          "form": "마늘잔뜩 넣어서",
          "original_form": "마늘잔뜩 넣어서",
          "speaker_id": "1",
          "time": "20210828 09:28"
        },
        {
          "id": "MDRW2100020538.1.20",
          "form": "저도 친구랑 파스타집가면",
          "original_form": "저도 친구랑 파스타집가면",
          "speaker_id": "2",
          "time": "20210828 09:28"
        },
        {
          "id": "MDRW2100020538.1.21",
          "form": "새우들어간거 다 뺀답니다",
          "original_form": "새우들어간거 다 뺀답니다",
          "speaker_id": "2",
          "time": "20210828 09:28"
        },
        {
          "id": "MDRW2100020538.1.22",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "1",
          "time": "20210828 09:28"
        },
        {
          "id": "MDRW2100020538.1.23",
          "form": "주문전에 빼달라고하시나요",
          "original_form": "주문전에 빼달라고하시나요",
          "speaker_id": "1",
          "time": "20210828 09:28"
        },
        {
          "id": "MDRW2100020538.1.24",
          "form": "새우넣은거 다 빼면 한두개 남더라구요 ㅎㅎ..",
          "original_form": "새우넣은거 다 빼면 한두개 남더라구요 ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210828 09:28"
        },
        {
          "id": "MDRW2100020538.1.25",
          "form": "앗ㅋㅋㅋㅋㅋ",
          "original_form": "앗ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 09:29"
        },
        {
          "id": "MDRW2100020538.1.26",
          "form": "아녀 웬만하면 놓치시더라구요",
          "original_form": "아녀 웬만하면 놓치시더라구요",
          "speaker_id": "2",
          "time": "20210828 09:29"
        },
        {
          "id": "MDRW2100020538.1.27",
          "form": "알리오 올리오는",
