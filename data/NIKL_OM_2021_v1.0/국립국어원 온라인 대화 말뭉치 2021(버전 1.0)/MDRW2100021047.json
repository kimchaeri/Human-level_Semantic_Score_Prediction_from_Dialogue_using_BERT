      "utterance": [
        {
          "id": "MDRW2100021047.1.1",
          "form": "앞으로는 B뿐아니라 A한테도 선의를 베풀지 않을지도 모르고",
          "original_form": "앞으로는 B뿐아니라 A한테도 선의를 베풀지 않을지도 모르고",
          "speaker_id": "2",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100021047.1.2",
          "form": "이렇게 우리 사회가 각박해지고..",
          "original_form": "이렇게 우리 사회가 각박해지고..",
          "speaker_id": "2",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100021047.1.3",
          "form": "흑흑 괜찮아요 안베풀어도 되니까",
          "original_form": "흑흑 괜찮아요 안베풀어도 되니까",
          "speaker_id": "1",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021047.1.4",
          "form": "C가 스스로 닦게만 해주세요..",
          "original_form": "C가 스스로 닦게만 해주세요..",
          "speaker_id": "1",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021047.1.5",
          "form": "ㅠㅜ 이런일이 비일비재하면 스트레스일거같아요",
          "original_form": "ㅠㅜ 이런일이 비일비재하면 스트레스일거같아요",
          "speaker_id": "2",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021047.1.6",
          "form": "맨날 C쪽에 문의해서 하라고 (=니들도 나를 닦아줘)한단 말이에요 흑흑",
          "original_form": "맨날 C쪽에 문의해서 하라고 (=니들도 나를 닦아줘)한단 말이에요 흑흑",
          "speaker_id": "1",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100021047.1.7",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100021047.1.8",
          "form": "진짜 재수..",
          "original_form": "진짜 재수..",
          "speaker_id": "2",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100021047.1.9",
          "form": "다행히 한분기에 한 번 정도에요",
          "original_form": "다행히 한분기에 한 번 정도에요",
          "speaker_id": "1",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100021047.1.10",
          "form": "이번분기는.. 지났으니까... ^^",
          "original_form": "이번분기는.. 지났으니까... ^^",
          "speaker_id": "1",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100021047.1.11",
          "form": "ㅋㅋㅋ심지어 닦는 방법도 딴데 물어보고 해달라니",
          "original_form": "ㅋㅋㅋ심지어 닦는 방법도 딴데 물어보고 해달라니",
          "speaker_id": "2",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100021047.1.12",
          "form": "진짜 스톤키즈네요ㅋㅋ",
          "original_form": "진짜 스톤키즈네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100021047.1.13",
          "form": "양심이라는게 없나요",
          "original_form": "양심이라는게 없나요",
          "speaker_id": "2",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100021047.1.14",
          "form": "ㅋㅋ일을 일단 못해요",
          "original_form": "ㅋㅋ일을 일단 못해요",
          "speaker_id": "1",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100021047.1.15",
          "form": "거기에 양심도 없어요!",
          "original_form": "거기에 양심도 없어요!",
          "speaker_id": "1",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100021047.1.16",
          "form": "스톤키즠ㅋㅋ",
          "original_form": "스톤키즠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100021047.1.17",
          "form": "ㅋㅋㅋㅋ와 돈내고 직장다시십쇼..",
          "original_form": "ㅋㅋㅋㅋ와 돈내고 직장다시십쇼..",
          "speaker_id": "2",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100021047.1.18",
          "form": "맞아요 도대체!!! 그돈을 날 주든지",
          "original_form": "맞아요 도대체!!! 그돈을 날 주든지",
          "speaker_id": "1",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100021047.1.19",
          "form": "뭐든 돈주고 시켜라 스톤키즈..",
          "original_form": "뭐든 돈주고 시켜라 스톤키즈..",
          "speaker_id": "2",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100021047.1.20",
          "form": "약간저런게 야비한거보다 그 .. 수치심이 없다고 하나요ㅋㅋ",
          "original_form": "약간저런게 야비한거보다 그 .. 수치심이 없다고 하나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100021047.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 15:32"
        },
        {
          "id": "MDRW2100021047.1.22",
          "form": "딱히 계략을 세우거나 악당 우두머리 느낌도 아니에요",
          "original_form": "딱히 계략을 세우거나 악당 우두머리 느낌도 아니에요",
          "speaker_id": "1",
          "time": "20210828 15:32"
        },
        {
          "id": "MDRW2100021047.1.23",
          "form": "그냥.. 부끄러움이 없다 내가 귀찮지 않을 수 있다면",
          "original_form": "그냥.. 부끄러움이 없다 내가 귀찮지 않을 수 있다면",
          "speaker_id": "1",
          "time": "20210828 15:33"
        },
        {
          "id": "MDRW2100021047.1.24",
          "form": "고인물이신가요",
          "original_form": "고인물이신가요",
          "speaker_id": "2",
          "time": "20210828 15:33"
        },
        {
          "id": "MDRW2100021047.1.25",
          "form": "이거에요",
          "original_form": "이거에요",
          "speaker_id": "1",
          "time": "20210828 15:33"
        },
        {
          "id": "MDRW2100021047.1.26",
          "form": "아 맞아요 좀 고였죠..",
          "original_form": "아 맞아요 좀 고였죠..",
          "speaker_id": "1",
          "time": "20210828 15:33"
        },
        {
          "id": "MDRW2100021047.1.27",
          "form": "정확하시네요 ㅜ",
          "original_form": "정확하시네요 ㅜ",
          "speaker_id": "1",
          "time": "20210828 15:33"
        },
        {
          "id": "MDRW2100021047.1.28",
          "form": "ㅋㅋㅋ 귀찮음과 수치심을 바꾸신",
          "original_form": "ㅋㅋㅋ 귀찮음과 수치심을 바꾸신",
          "speaker_id": "2",
          "time": "20210828 15:33"
        },
        {
          "id": "MDRW2100021047.1.29",
          "form": "고인분들이 많이들 그러시죠",
          "original_form": "고인분들이 많이들 그러시죠",
          "speaker_id": "2",
          "time": "20210828 15:33"
        },
        {
          "id": "MDRW2100021047.1.30",
          "form": "여기도 고인물이신 분들 숨쉬듯이 멍 소리 많이 하시거든요ㅋㅋ",
          "original_form": "여기도 고인물이신 분들 숨쉬듯이 멍 소리 많이 하시거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:35"
        },
        {
          "id": "MDRW2100021047.1.31",
          "form": "흑흑 1인분까진 바라지도 않으니 0.5라도 해주세요",
          "original_form": "흑흑 1인분까진 바라지도 않으니 0.5라도 해주세요",
          "speaker_id": "1",
          "time": "20210828 15:35"
