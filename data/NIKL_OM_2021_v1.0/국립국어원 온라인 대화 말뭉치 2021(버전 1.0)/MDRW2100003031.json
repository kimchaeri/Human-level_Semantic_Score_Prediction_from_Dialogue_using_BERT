      "utterance": [
        {
          "id": "MDRW2100003031.1.1",
          "form": "저는 내장말고는 다 잘먹어요",
          "original_form": "저는 내장말고는 다 잘먹어요",
          "speaker_id": "2",
          "time": "20210820 17:41"
        },
        {
          "id": "MDRW2100003031.1.2",
          "form": "내장도 맛있어요 ㅠ",
          "original_form": "내장도 맛있어요 ㅠ",
          "speaker_id": "1",
          "time": "20210820 17:41"
        },
        {
          "id": "MDRW2100003031.1.3",
          "form": "최애 음식있나요",
          "original_form": "최애 음식있나요",
          "speaker_id": "1",
          "time": "20210820 17:41"
        },
        {
          "id": "MDRW2100003031.1.4",
          "form": "고기는 하루에 1끼 이상먹어요",
          "original_form": "고기는 하루에 1끼 이상먹어요",
          "speaker_id": "2",
          "time": "20210820 17:42"
        },
        {
          "id": "MDRW2100003031.1.5",
          "form": "오 저희남편도요 ㅋㅋ",
          "original_form": "오 저희남편도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003031.1.6",
          "form": "고기쟁이예요 완전",
          "original_form": "고기쟁이예요 완전",
          "speaker_id": "1",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003031.1.7",
          "form": "아이들 몇살이예요?",
          "original_form": "아이들 몇살이예요?",
          "speaker_id": "2",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003031.1.8",
          "form": "맞벌이라 이유식 사먹였는데\r\n이제 해먹여야되서ㅠㅠ",
          "original_form": "맞벌이라 이유식 사먹였는데\r\n이제 해먹여야되서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003031.1.9",
          "form": "아 저는 아직 애기가없어요",
          "original_form": "아 저는 아직 애기가없어요",
          "speaker_id": "1",
          "time": "20210820 17:47"
        },
        {
          "id": "MDRW2100003031.1.10",
          "form": "아 네~ 오늘은 뭐먹기로했어요?",
          "original_form": "아 네~ 오늘은 뭐먹기로했어요?",
          "speaker_id": "2",
          "time": "20210820 17:48"
        },
        {
          "id": "MDRW2100003031.1.11",
          "form": "오늘 저녁은 회 배달시켜먹으려구요",
          "original_form": "오늘 저녁은 회 배달시켜먹으려구요",
          "speaker_id": "1",
          "time": "20210820 17:49"
        },
        {
          "id": "MDRW2100003031.1.12",
          "form": "마음품님은요?",
          "original_form": "마음품님은요?",
          "speaker_id": "1",
          "time": "20210820 17:49"
        },
        {
          "id": "MDRW2100003031.1.13",
          "form": "오 회 좋죠! 8월초에 강원도에서 하도많이 먹어서",
          "original_form": "오 회 좋죠! 8월초에 강원도에서 하도많이 먹어서",
          "speaker_id": "2",
          "time": "20210820 17:50"
        },
        {
          "id": "MDRW2100003031.1.14",
          "form": "오늘은 닭강정먹으려고요",
          "original_form": "오늘은 닭강정먹으려고요",
          "speaker_id": "2",
          "time": "20210820 17:50"
        },
        {
          "id": "MDRW2100003031.1.15",
          "form": "오 닭강정 좋죠 ㅠ",
          "original_form": "오 닭강정 좋죠 ㅠ",
          "speaker_id": "1",
          "time": "20210820 17:51"
        },
        {
          "id": "MDRW2100003031.1.16",
          "form": "여긴 경기도인데 닭강정맛집이썽요",
          "original_form": "여긴 경기도인데 닭강정맛집이썽요",
          "speaker_id": "1",
          "time": "20210820 17:51"
        },
        {
          "id": "MDRW2100003031.1.17",
          "form": "여기는대전이라",
          "original_form": "여기는대전이라",
          "speaker_id": "2",
          "time": "20210820 17:52"
        },
        {
          "id": "MDRW2100003031.1.18",
          "form": "그냥저냥 다 맛있어요ㅋㅋ",
          "original_form": "그냥저냥 다 맛있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:52"
        },
        {
          "id": "MDRW2100003031.1.19",
          "form": "대전오면 다들 성심당밖에 모른데요ㅋㅋ",
          "original_form": "대전오면 다들 성심당밖에 모른데요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 17:52"
        },
        {
          "id": "MDRW2100003031.1.20",
          "form": "오 대전 한번인가밖에",
          "original_form": "오 대전 한번인가밖에",
          "speaker_id": "1",
          "time": "20210820 17:52"
        },
        {
          "id": "MDRW2100003031.1.21",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:52"
        },
        {
          "id": "MDRW2100003031.1.22",
          "form": "저도 성심당은 가봤네여",
          "original_form": "저도 성심당은 가봤네여",
          "speaker_id": "1",
          "time": "20210820 17:53"
        }
      ]
