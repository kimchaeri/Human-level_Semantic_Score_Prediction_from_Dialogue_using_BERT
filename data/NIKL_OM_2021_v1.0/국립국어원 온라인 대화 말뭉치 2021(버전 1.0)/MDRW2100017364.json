      "utterance": [
        {
          "id": "MDRW2100017364.1.1",
          "form": "혼자 사시나요?",
          "original_form": "혼자 사시나요?",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017364.1.2",
          "form": "네 자취요",
          "original_form": "네 자취요",
          "speaker_id": "2",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100017364.1.3",
          "form": "자취하면 좋은점이 잇나요?!",
          "original_form": "자취하면 좋은점이 잇나요?!",
          "speaker_id": "1",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100017364.1.4",
          "form": "혼자산다는 점이죠!",
          "original_form": "혼자산다는 점이죠!",
          "speaker_id": "2",
          "time": "20210826 18:58"
        },
        {
          "id": "MDRW2100017364.1.5",
          "form": "재가혼자잇는걸 좋아해서",
          "original_form": "재가혼자잇는걸 좋아해서",
          "speaker_id": "2",
          "time": "20210826 18:58"
        },
        {
          "id": "MDRW2100017364.1.6",
          "form": "누가 잔소리안하고",
          "original_form": "누가 잔소리안하고",
          "speaker_id": "2",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017364.1.7",
          "form": "그지만 별롭니다",
          "original_form": "그지만 별롭니다",
          "speaker_id": "2",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017364.1.8",
          "form": "ㅋㅋㅋ헐 저도요! 혼자있는거 좋아하고 잔소리없는거좋아요",
          "original_form": "ㅋㅋㅋ헐 저도요! 혼자있는거 좋아하고 잔소리없는거좋아요",
          "speaker_id": "1",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017364.1.9",
          "form": "근데 청소가 너무 귀찮아요ㅠ 요리두",
          "original_form": "근데 청소가 너무 귀찮아요ㅠ 요리두",
          "speaker_id": "1",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017364.1.10",
          "form": "걍누가다해줫음좋겟어요",
          "original_form": "걍누가다해줫음좋겟어요",
          "speaker_id": "2",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017364.1.11",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017364.1.12",
          "form": "돈많으면 자취하며  가정부 두면 딱인데",
          "original_form": "돈많으면 자취하며  가정부 두면 딱인데",
          "speaker_id": "2",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017364.1.13",
          "form": "그쵸그쵸 그리구 청소는 귀찮긴하지만 집은 좀더 넓엇음좋겟어요 ㅋㅋㅋㅋ",
          "original_form": "그쵸그쵸 그리구 청소는 귀찮긴하지만 집은 좀더 넓엇음좋겟어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017364.1.14",
          "form": "ㄹㅇ... 좁아요 항상 언제나",
          "original_form": "ㄹㅇ... 좁아요 항상 언제나",
          "speaker_id": "2",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017364.1.15",
          "form": "로봇청소기 있어요?",
          "original_form": "로봇청소기 있어요?",
          "speaker_id": "2",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017364.1.16",
          "form": "아니요 없어요 근데 로봇고용해도 집에 빈자리가없어서 로봇이 못돌아다닐것같아요 ㅋㅋㅋㅋ ㅠ",
          "original_form": "아니요 없어요 근데 로봇고용해도 집에 빈자리가없어서 로봇이 못돌아다닐것같아요 ㅋㅋㅋㅋ ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017364.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ 저두...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저두...",
          "speaker_id": "2",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017364.1.18",
          "form": "집안일 꿀팁 잇어요?",
          "original_form": "집안일 꿀팁 잇어요?",
          "speaker_id": "2",
          "time": "20210826 19:07"
        },
        {
          "id": "MDRW2100017364.1.19",
          "form": "바로바로 치우자...? 그순간에 안치우면 이제 영영안치우게되는것같아요",
          "original_form": "바로바로 치우자...? 그순간에 안치우면 이제 영영안치우게되는것같아요",
          "speaker_id": "1",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017364.1.20",
          "form": "오... 진짜그렇겠어요",
          "original_form": "오... 진짜그렇겠어요",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017364.1.21",
          "form": "바로치우세요?",
          "original_form": "바로치우세요?",
          "speaker_id": "2",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017364.1.22",
          "form": "그러려고 노력하는 편이에요 특히 쓰레기들나오는거는 더더욱!!",
          "original_form": "그러려고 노력하는 편이에요 특히 쓰레기들나오는거는 더더욱!!",
          "speaker_id": "1",
          "time": "20210826 19:13"
        }
      ]
    }
