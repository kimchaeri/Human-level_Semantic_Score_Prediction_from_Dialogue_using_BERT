      "utterance": [
        {
          "id": "MDRW2100024961.1.1",
          "form": "좋아하는 아이스크림 있어?",
          "original_form": "좋아하는 아이스크림 있어?",
          "speaker_id": "1",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100024961.1.2",
          "form": "베라 뉴치케 아니",
          "original_form": "베라 뉴치케 아니",
          "speaker_id": "2",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100024961.1.3",
          "form": "레인보우샤베트도 좋아해",
          "original_form": "레인보우샤베트도 좋아해",
          "speaker_id": "2",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100024961.1.4",
          "form": "당연하지 무조건 필수",
          "original_form": "당연하지 무조건 필수",
          "speaker_id": "1",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100024961.1.5",
          "form": "미안 샤베트 배척",
          "original_form": "미안 샤베트 배척",
          "speaker_id": "1",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100024961.1.6",
          "form": "하 샤베트로 깔끔하게 해줘야",
          "original_form": "하 샤베트로 깔끔하게 해줘야",
          "speaker_id": "2",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100024961.1.7",
          "form": "계속 먹을수있다구",
          "original_form": "계속 먹을수있다구",
          "speaker_id": "2",
          "time": "20210831 21:26"
        },
        {
          "id": "MDRW2100024961.1.8",
          "form": "무슨소리야 민초로 입 깔끔 가능해",
          "original_form": "무슨소리야 민초로 입 깔끔 가능해",
          "speaker_id": "1",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100024961.1.9",
          "form": "혹시 민초는 조아하니?",
          "original_form": "혹시 민초는 조아하니?",
          "speaker_id": "1",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100024961.1.10",
          "form": "민초 세입 먹으면...",
          "original_form": "민초 세입 먹으면...",
          "speaker_id": "2",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100024961.1.11",
          "form": "할당량 끝난다 ㅋㅋ",
          "original_form": "할당량 끝난다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100024961.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ이럴수가",
          "original_form": "ㅋㅋㅋㅋㅋㅋ이럴수가",
          "speaker_id": "1",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100024961.1.13",
          "form": "투게더는 좋아하니?",
          "original_form": "투게더는 좋아하니?",
          "speaker_id": "1",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100024961.1.14",
          "form": "ㅋㅋㅋ투게더 싫어하는 사람 누구?",
          "original_form": "ㅋㅋㅋ투게더 싫어하는 사람 누구?",
          "speaker_id": "2",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100024961.1.15",
          "form": "아직 못봤다",
          "original_form": "아직 못봤다",
          "speaker_id": "2",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024961.1.16",
          "form": "투게더 한톧 순삭 가능",
          "original_form": "투게더 한톧 순삭 가능",
          "speaker_id": "2",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024961.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ하긴 투게더 제일 기본이지",
          "original_form": "ㅋㅋㅋㅋㅋㅋ하긴 투게더 제일 기본이지",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024961.1.18",
          "form": "슈퍼 아이스크림중에 사먹는거잇어?",
          "original_form": "슈퍼 아이스크림중에 사먹는거잇어?",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100024961.1.19",
          "form": "음 요즘엔 빵빠레",
          "original_form": "음 요즘엔 빵빠레",
          "speaker_id": "2",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100024961.1.20",
          "form": "좀 사먹었지",
          "original_form": "좀 사먹었지",
          "speaker_id": "2",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100024961.1.21",
          "form": "요즘 슈퍼에서 사먹는거 있나?",
          "original_form": "요즘 슈퍼에서 사먹는거 있나?",
          "speaker_id": "2",
          "time": "20210831 21:30"
        },
        {
          "id": "MDRW2100024961.1.22",
          "form": "크 빵빠레 최고",
          "original_form": "크 빵빠레 최고",
          "speaker_id": "1",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100024961.1.23",
          "form": "냔 잘 안사먹는데 냉장고에있으면 먹어",
          "original_form": "냔 잘 안사먹는데 냉장고에있으면 먹어",
          "speaker_id": "1",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100024961.1.24",
          "form": "옥동자 최든에 먹엇따",
          "original_form": "옥동자 최든에 먹엇따",
          "speaker_id": "1",
          "time": "20210831 21:31"
        },
        {
          "id": "MDRW2100024961.1.25",
          "form": "옥동자 오랜만이네",
          "original_form": "옥동자 오랜만이네",
          "speaker_id": "2",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024961.1.26",
          "form": "예전에 와일드바디 진짜 많이 먹었는데",
          "original_form": "예전에 와일드바디 진짜 많이 먹었는데",
          "speaker_id": "2",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024961.1.27",
          "form": "빵빠레는 잘 안보여ㅠ",
          "original_form": "빵빠레는 잘 안보여ㅠ",
          "speaker_id": "1",
          "time": "20210831 21:32"
        },
        {
          "id": "MDRW2100024961.1.28",
          "form": "와 추억이다",
          "original_form": "와 추억이다",
          "speaker_id": "1",
          "time": "20210831 21:32"
        }
      ]
    }
  ]
