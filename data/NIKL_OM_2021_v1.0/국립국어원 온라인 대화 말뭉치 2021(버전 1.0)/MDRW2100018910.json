        {
          "id": "MDRW2100018910.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 13:31"
        },
        {
          "id": "MDRW2100018910.1.2",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "2",
          "time": "20210827 13:38"
        },
        {
          "id": "MDRW2100018910.1.3",
          "form": "게임좋아하죠",
          "original_form": "게임좋아하죠",
          "speaker_id": "2",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018910.1.4",
          "form": "어떤게임 하시나요?",
          "original_form": "어떤게임 하시나요?",
          "speaker_id": "1",
          "time": "20210827 13:39"
        },
        {
          "id": "MDRW2100018910.1.5",
          "form": "최근엔 주로 플스합니다!",
          "original_form": "최근엔 주로 플스합니다!",
          "speaker_id": "2",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018910.1.6",
          "form": "플스5인가요?",
          "original_form": "플스5인가요?",
          "speaker_id": "1",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018910.1.7",
          "form": "아뇨ㅋㅋ 예전에 사놓은 플스4입니다",
          "original_form": "아뇨ㅋㅋ 예전에 사놓은 플스4입니다",
          "speaker_id": "2",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018910.1.8",
          "form": "아 그러시구나 플스로 어떤 타이틀 주로하세요?",
          "original_form": "아 그러시구나 플스로 어떤 타이틀 주로하세요?",
          "speaker_id": "1",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018910.1.9",
          "form": "요즘엔 gta4 스토리 깨고잇어욥",
          "original_form": "요즘엔 gta4 스토리 깨고잇어욥",
          "speaker_id": "2",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018910.1.10",
          "form": "저도 gta4 했었는데!",
          "original_form": "저도 gta4 했었는데!",
          "speaker_id": "1",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018910.1.11",
          "form": "속시원하고 좋죠",
          "original_form": "속시원하고 좋죠",
          "speaker_id": "1",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018910.1.12",
          "form": "ㅋㅋㅋㅋ네 재밌어요ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ네 재밌어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018910.1.13",
          "form": "범죄로 돈 한참 벌고계시겠네요",
          "original_form": "범죄로 돈 한참 벌고계시겠네요",
          "speaker_id": "1",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018910.1.14",
          "form": "예전에 산안드레스도 했거든요",
          "original_form": "예전에 산안드레스도 했거든요",
          "speaker_id": "2",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018910.1.15",
          "form": "산안드레스 명작이죠",
          "original_form": "산안드레스 명작이죠",
          "speaker_id": "1",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018910.1.16",
          "form": "ㅋㅋㅋㅋ근데 어제 스토리 다 깨서",
          "original_form": "ㅋㅋㅋㅋ근데 어제 스토리 다 깨서",
          "speaker_id": "2",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018910.1.17",
          "form": "다른거하려고요",
          "original_form": "다른거하려고요",
          "speaker_id": "2",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018910.1.18",
          "form": "타이틀 많이 아시나요?",
          "original_form": "타이틀 많이 아시나요?",
          "speaker_id": "2",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018910.1.19",
          "form": "엔딩보셨구나 어떤엔딩 나셨어요?",
          "original_form": "엔딩보셨구나 어떤엔딩 나셨어요?",
          "speaker_id": "1",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018910.1.20",
          "form": "많이는 아니고 유명한것들 몇개 해봤어요",
          "original_form": "많이는 아니고 유명한것들 몇개 해봤어요",
          "speaker_id": "1",
          "time": "20210827 13:46"
        },
        {
          "id": "MDRW2100018910.1.21",
          "form": "그냥 서로 담배피고 헤어지던데요",
          "original_form": "그냥 서로 담배피고 헤어지던데요",
