      "utterance": [
        {
          "id": "MDRW2100035842.1.1",
          "form": "아침 드셨나용?",
          "original_form": "아침 드셨나용?",
          "speaker_id": "2",
          "time": "20210830 08:03"
        },
        {
          "id": "MDRW2100035842.1.2",
          "form": "아직이요! 아침 드셨나요?",
          "original_form": "아직이요! 아침 드셨나요?",
          "speaker_id": "1",
          "time": "20210830 08:04"
        },
        {
          "id": "MDRW2100035842.1.3",
          "form": "저도 아직이요!",
          "original_form": "저도 아직이요!",
          "speaker_id": "2",
          "time": "20210830 08:12"
        },
        {
          "id": "MDRW2100035842.1.4",
          "form": "늦잠자서 늦었어요ㅠ",
          "original_form": "늦잠자서 늦었어요ㅠ",
          "speaker_id": "2",
          "time": "20210830 08:12"
        },
        {
          "id": "MDRW2100035842.1.5",
          "form": "앗....ㅠㅠ 아침먹을 시간 없으셨겠네요",
          "original_form": "앗....ㅠㅠ 아침먹을 시간 없으셨겠네요",
          "speaker_id": "1",
          "time": "20210830 08:13"
        },
        {
          "id": "MDRW2100035842.1.6",
          "form": "넵ㅠ 엄마가 쥬스 갈아주셔서 이따 마시려구요!",
          "original_form": "넵ㅠ 엄마가 쥬스 갈아주셔서 이따 마시려구요!",
          "speaker_id": "2",
          "time": "20210830 08:15"
        },
        {
          "id": "MDRW2100035842.1.7",
          "form": "아침 어떤거 드세요?",
          "original_form": "아침 어떤거 드세요?",
          "speaker_id": "2",
          "time": "20210830 08:15"
        },
        {
          "id": "MDRW2100035842.1.8",
          "form": "저는 시리얼먹을거 같아요 대충 속 채운다는 느낌으로...",
          "original_form": "저는 시리얼먹을거 같아요 대충 속 채운다는 느낌으로...",
          "speaker_id": "1",
          "time": "20210830 08:28"
        },
        {
          "id": "MDRW2100035842.1.9",
          "form": "아하! 아침에 간단히 먹는게 편하긴 하죠",
          "original_form": "아하! 아침에 간단히 먹는게 편하긴 하죠",
          "speaker_id": "2",
          "time": "20210830 08:34"
        },
        {
          "id": "MDRW2100035842.1.10",
          "form": "점심은 밖에서 드세요?",
          "original_form": "점심은 밖에서 드세요?",
          "speaker_id": "1",
          "time": "20210830 08:37"
        },
        {
          "id": "MDRW2100035842.1.11",
          "form": "집에서 먹어요!",
          "original_form": "집에서 먹어요!",
          "speaker_id": "2",
          "time": "20210830 08:44"
        },
        {
          "id": "MDRW2100035842.1.12",
          "form": "맛있는게 먹고 싶네요ㅋㅋ",
          "original_form": "맛있는게 먹고 싶네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 08:44"
        },
        {
          "id": "MDRW2100035842.1.13",
          "form": "저는 닭갈비 먹을 예정이예요",
          "original_form": "저는 닭갈비 먹을 예정이예요",
          "speaker_id": "1",
          "time": "20210830 08:48"
        },
        {
          "id": "MDRW2100035842.1.14",
          "form": "든든하게 먹으려구요",
          "original_form": "든든하게 먹으려구요",
          "speaker_id": "1",
          "time": "20210830 08:48"
        },
        {
          "id": "MDRW2100035842.1.15",
          "form": "오!!",
          "original_form": "오!!",
          "speaker_id": "2",
          "time": "20210830 08:49"
        },
        {
          "id": "MDRW2100035842.1.16",
          "form": "맛있는거 드시네요!!",
          "original_form": "맛있는거 드시네요!!",
          "speaker_id": "2",
          "time": "20210830 08:49"
        },
        {
          "id": "MDRW2100035842.1.17",
          "form": "점심을 제일 든든하게 챙기게돼요ㅋㅋㅋ",
          "original_form": "점심을 제일 든든하게 챙기게돼요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 08:50"
        },
        {
          "id": "MDRW2100035842.1.18",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:01"
        },
        {
          "id": "MDRW2100035842.1.19",
          "form": "그래야 저녁도 덜먹게 되더라구요ㅋㅋ",
          "original_form": "그래야 저녁도 덜먹게 되더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:01"
        }
      ]
