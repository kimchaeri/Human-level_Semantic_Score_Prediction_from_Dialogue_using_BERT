      "utterance": [
        {
          "id": "MDRW2100036173.1.1",
          "form": "이번에 몇 과목 듣는다고 했지?",
          "original_form": "이번에 몇 과목 듣는다고 했지?",
          "speaker_id": "1",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100036173.1.2",
          "form": "나 8?",
          "original_form": "나 8?",
          "speaker_id": "2",
          "time": "20210830 17:58"
        },
        {
          "id": "MDRW2100036173.1.3",
          "form": "짱 많다잉...",
          "original_form": "짱 많다잉...",
          "speaker_id": "1",
          "time": "20210830 17:59"
        },
        {
          "id": "MDRW2100036173.1.4",
          "form": "2학점짜리는 일주일에 2시간 수업 해?",
          "original_form": "2학점짜리는 일주일에 2시간 수업 해?",
          "speaker_id": "1",
          "time": "20210830 17:59"
        },
        {
          "id": "MDRW2100036173.1.5",
          "form": "2학점 5개?",
          "original_form": "2학점 5개?",
          "speaker_id": "2",
          "time": "20210830 18:40"
        },
        {
          "id": "MDRW2100036173.1.6",
          "form": "그건 2시간 수업이야?",
          "original_form": "그건 2시간 수업이야?",
          "speaker_id": "1",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100036173.1.7",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100036173.1.8",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100036173.1.9",
          "form": "난 3학점만 들어서 궁금했으",
          "original_form": "난 3학점만 들어서 궁금했으",
          "speaker_id": "1",
          "time": "20210830 18:41"
        },
        {
          "id": "MDRW2100036173.1.10",
          "form": "민지야 우리도 수업 공지가 하나 둘 씩 뜨고 있어",
          "original_form": "민지야 우리도 수업 공지가 하나 둘 씩 뜨고 있어",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100036173.1.11",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100036173.1.12",
          "form": "과제 공지도 하나 떴어",
          "original_form": "과제 공지도 하나 떴어",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100036173.1.13",
          "form": "수업도 안 들었는데!",
          "original_form": "수업도 안 들었는데!",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100036173.1.14",
          "form": "과제 공지를 주는 게 어딨냐고!",
          "original_form": "과제 공지를 주는 게 어딨냐고!",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100036173.1.15",
          "form": "아니 에바야!!",
          "original_form": "아니 에바야!!",
          "speaker_id": "2",
          "time": "20210830 18:47"
        },
        {
          "id": "MDRW2100036173.1.16",
          "form": "나도 그렇게 생각해...",
          "original_form": "나도 그렇게 생각해...",
          "speaker_id": "1",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100036173.1.17",
          "form": "이게 뭐라고... 너무 떨린다ㅋㅋㅋㅋ",
          "original_form": "이게 뭐라고... 너무 떨린다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100036173.1.18",
          "form": "성적 1학기 보다 잘 받아야 해...",
          "original_form": "성적 1학기 보다 잘 받아야 해...",
          "speaker_id": "1",
          "time": "20210830 18:49"
        },
        {
          "id": "MDRW2100036173.1.19",
          "form": "나두..",
          "original_form": "나두..",
          "speaker_id": "2",
          "time": "20210830 18:52"
        },
        {
          "id": "MDRW2100036173.1.20",
          "form": "이번에 진짜 3.5 넘길 거야...",
          "original_form": "이번에 진짜 3.5 넘길 거야...",
          "speaker_id": "1",
          "time": "20210830 18:53"
        },
        {
          "id": "MDRW2100036173.1.21",
          "form": "A 3개는 받을 거야...",
          "original_form": "A 3개는 받을 거야...",
          "speaker_id": "1",
          "time": "20210830 18:54"
        },
        {
          "id": "MDRW2100036173.1.22",
          "form": "오오오 좋다!",
          "original_form": "오오오 좋다!",
          "speaker_id": "2",
          "time": "20210830 19:04"
        },
        {
          "id": "MDRW2100036173.1.23",
          "form": "파이팅하자",
          "original_form": "파이팅하자",
          "speaker_id": "1",
          "time": "20210830 19:05"
        },
        {
          "id": "MDRW2100036173.1.24",
          "form": "이번에 공부 열심히 해보자",
          "original_form": "이번에 공부 열심히 해보자",
          "speaker_id": "1",
          "time": "20210830 19:05"
        }
