      "utterance": [
        {
          "id": "MDRW2100043711.1.1",
          "form": "가을되니까 비염이 도져서 콧물줄줄이네여",
          "original_form": "가을되니까 비염이 도져서 콧물줄줄이네여",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043711.1.2",
          "form": "알러지있으신가여",
          "original_form": "알러지있으신가여",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043711.1.3",
          "form": "저도 비염 있어요",
          "original_form": "저도 비염 있어요",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043711.1.4",
          "form": "요즘 눈이 간질간질 ㅠㅠ",
          "original_form": "요즘 눈이 간질간질 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043711.1.5",
          "form": "ㅜㅜ그쳐 선선한건 넘좋은데 비염은 최악이네요",
          "original_form": "ㅜㅜ그쳐 선선한건 넘좋은데 비염은 최악이네요",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043711.1.6",
          "form": "지르텍 미리 사놨어요 얼마전에 ㅋㅋ",
          "original_form": "지르텍 미리 사놨어요 얼마전에 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043711.1.7",
          "form": "저도 조만간 사러가야겠어요 ㅎㅎㅜ",
          "original_form": "저도 조만간 사러가야겠어요 ㅎㅎㅜ",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043711.1.8",
          "form": "운동 하시는거있으세요",
          "original_form": "운동 하시는거있으세요",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043711.1.9",
          "form": "코로나이후로 살이 붙어서 홈트하려고요 이제..",
          "original_form": "코로나이후로 살이 붙어서 홈트하려고요 이제..",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043711.1.10",
          "form": "저도..ㅎㅎ..근데 홈트 넘하기 힘들더라구요 의지갖기가 흑흑",
          "original_form": "저도..ㅎㅎ..근데 홈트 넘하기 힘들더라구요 의지갖기가 흑흑",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043711.1.11",
          "form": "유튜버보고하실건가여",
          "original_form": "유튜버보고하실건가여",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043711.1.12",
          "form": "네네 땅끄부부인가 구독만 해놨어요 ㅋㅋㅋ",
          "original_form": "네네 땅끄부부인가 구독만 해놨어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043711.1.13",
          "form": "과연 언제 시작할지..",
          "original_form": "과연 언제 시작할지..",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043711.1.14",
          "form": "아..저도..그분들거봤는데 ㅎㅎ..도저히...따라갈수가",
          "original_form": "아..저도..그분들거봤는데 ㅎㅎ..도저히...따라갈수가",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043711.1.15",
          "form": "그분들 표정이 너무 온화해서 할수있을것만 같은 착각이 들어요",
          "original_form": "그분들 표정이 너무 온화해서 할수있을것만 같은 착각이 들어요",
          "speaker_id": "2",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043711.1.16",
          "form": "ㅋㅋㅋㅋㅋ표정 넘편안하시지만 자세는 그렇지않고",
          "original_form": "ㅋㅋㅋㅋㅋ표정 넘편안하시지만 자세는 그렇지않고",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043711.1.17",
          "form": "ㅋㅋㅋ 그러게요 엄청 힘들어보이는 자세..",
          "original_form": "ㅋㅋㅋ 그러게요 엄청 힘들어보이는 자세..",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043711.1.18",
          "form": "초도 어찌나 분같은지..",
          "original_form": "초도 어찌나 분같은지..",
          "speaker_id": "1",
          "time": "20210907 20:17"
        }
