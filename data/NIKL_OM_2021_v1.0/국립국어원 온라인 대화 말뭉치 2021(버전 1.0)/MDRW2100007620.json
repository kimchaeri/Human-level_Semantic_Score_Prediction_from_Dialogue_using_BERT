        {
          "id": "MDRW2100007620.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210822 07:51"
        },
        {
          "id": "MDRW2100007620.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210822 07:51"
        },
        {
          "id": "MDRW2100007620.1.3",
          "form": "키우는 반려동물이 있나요?",
          "original_form": "키우는 반려동물이 있나요?",
          "speaker_id": "2",
          "time": "20210822 07:51"
        },
        {
          "id": "MDRW2100007620.1.4",
          "form": "아니요 없어요ㅎㅎ 2724님은?",
          "original_form": "아니요 없어요ㅎㅎ 2724님은?",
          "speaker_id": "1",
          "time": "20210822 08:24"
        },
        {
          "id": "MDRW2100007620.1.5",
          "form": "저도 없어요",
          "original_form": "저도 없어요",
          "speaker_id": "2",
          "time": "20210822 09:19"
        },
        {
          "id": "MDRW2100007620.1.6",
          "form": "고양이 키우고싶어요",
          "original_form": "고양이 키우고싶어요",
          "speaker_id": "2",
          "time": "20210822 09:19"
        },
        {
          "id": "MDRW2100007620.1.7",
          "form": "오 고양이 귀엽겠어요",
          "original_form": "오 고양이 귀엽겠어요",
          "speaker_id": "1",
          "time": "20210822 09:20"
        },
        {
          "id": "MDRW2100007620.1.8",
          "form": "귀엽죠 고양이 손도 많이 안가고",
          "original_form": "귀엽죠 고양이 손도 많이 안가고",
          "speaker_id": "2",
          "time": "20210822 09:23"
        },
        {
          "id": "MDRW2100007620.1.9",
          "form": "네 똑똑하죠",
          "original_form": "네 똑똑하죠",
          "speaker_id": "1",
          "time": "20210822 09:30"
        },
        {
          "id": "MDRW2100007620.1.10",
          "form": "털날림만 아니면 키우고싶어요",
          "original_form": "털날림만 아니면 키우고싶어요",
          "speaker_id": "2",
          "time": "20210822 09:31"
        },
        {
          "id": "MDRW2100007620.1.11",
          "form": "털은 어쩔수가...",
          "original_form": "털은 어쩔수가...",
          "speaker_id": "1",
          "time": "20210822 09:33"
        },
        {
          "id": "MDRW2100007620.1.12",
          "form": "뿜어져나온다고하더라구요",
          "original_form": "뿜어져나온다고하더라구요",
          "speaker_id": "2",
          "time": "20210822 09:35"
        },
        {
          "id": "MDRW2100007620.1.13",
          "form": "개도 털날리는데 고양이는 정말..",
          "original_form": "개도 털날리는데 고양이는 정말..",
          "speaker_id": "2",
          "time": "20210822 09:35"
        },
        {
          "id": "MDRW2100007620.1.14",
          "form": "아 안키우고 싶네요",
          "original_form": "아 안키우고 싶네요",
          "speaker_id": "1",
          "time": "20210822 09:42"
        },
        {
          "id": "MDRW2100007620.1.15",
          "form": "저도 그래서 유튜브에서 동영상보는걸로 만족하고있어요",
          "original_form": "저도 그래서 유튜브에서 동영상보는걸로 만족하고있어요",
          "speaker_id": "2",
          "time": "20210822 09:43"
        },
        {
          "id": "MDRW2100007620.1.16",
          "form": "ㅎㅎ그것도 충분한듯요",
          "original_form": "ㅎㅎ그것도 충분한듯요",
          "speaker_id": "1",
