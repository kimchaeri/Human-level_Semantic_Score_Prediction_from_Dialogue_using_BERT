      "utterance": [
        {
          "id": "MDRW2100011736.1.1",
          "form": "딸은 잘 지내나요?",
          "original_form": "딸은 잘 지내나요?",
          "speaker_id": "2",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100011736.1.2",
          "form": "요샌 어떤가요",
          "original_form": "요샌 어떤가요",
          "speaker_id": "2",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100011736.1.3",
          "form": "오늘 코가 찡찡거린다고 연락왔던데",
          "original_form": "오늘 코가 찡찡거린다고 연락왔던데",
          "speaker_id": "1",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100011736.1.4",
          "form": "코가요? 감기인가요?",
          "original_form": "코가요? 감기인가요?",
          "speaker_id": "2",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100011736.1.5",
          "form": "요새 비가 많이 오고 날이 차서 그런가봐요ㅠㅠ",
          "original_form": "요새 비가 많이 오고 날이 차서 그런가봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100011736.1.6",
          "form": "아침마다 고인 물에서 첨벙첨벙대더니",
          "original_form": "아침마다 고인 물에서 첨벙첨벙대더니",
          "speaker_id": "1",
          "time": "20210824 15:41"
        },
        {
          "id": "MDRW2100011736.1.7",
          "form": "그래서 그런게 아닐까",
          "original_form": "그래서 그런게 아닐까",
          "speaker_id": "1",
          "time": "20210824 15:41"
        },
        {
          "id": "MDRW2100011736.1.8",
          "form": "첨벙이는 모습 생각하니까 귀여운데요ㅋㅋ",
          "original_form": "첨벙이는 모습 생각하니까 귀여운데요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:42"
        },
        {
          "id": "MDRW2100011736.1.9",
          "form": "하지만 콧물이 나온다니ㅠㅠ조심해야겠네요",
          "original_form": "하지만 콧물이 나온다니ㅠㅠ조심해야겠네요",
          "speaker_id": "2",
          "time": "20210824 15:42"
        },
        {
          "id": "MDRW2100011736.1.10",
          "form": "글체...",
          "original_form": "글체...",
          "speaker_id": "1",
          "time": "20210824 15:47"
        },
        {
          "id": "MDRW2100011736.1.11",
          "form": "요새 감기로 열나도 병원에선 잘 안 받아주니까 그것도 걱정이야",
          "original_form": "요새 감기로 열나도 병원에선 잘 안 받아주니까 그것도 걱정이야",
          "speaker_id": "1",
          "time": "20210824 15:47"
        },
        {
          "id": "MDRW2100011736.1.12",
          "form": "아 맞네요 열나면 코로나 검사하러 가야하잖아요",
          "original_form": "아 맞네요 열나면 코로나 검사하러 가야하잖아요",
          "speaker_id": "2",
          "time": "20210824 15:48"
        },
        {
          "id": "MDRW2100011736.1.13",
          "form": "어디간데도 없는데 또 병원에 사정해야하나ㅠ",
          "original_form": "어디간데도 없는데 또 병원에 사정해야하나ㅠ",
          "speaker_id": "1",
          "time": "20210824 15:49"
        },
        {
          "id": "MDRW2100011736.1.14",
          "form": "전에도 열나서 병원 진료 못 봤었죠ㅠㅠ",
          "original_form": "전에도 열나서 병원 진료 못 봤었죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 15:50"
        },
        {
          "id": "MDRW2100011736.1.15",
          "form": "응....결국 검사하고 음성받고 진료봤지",
          "original_form": "응....결국 검사하고 음성받고 진료봤지",
          "speaker_id": "1",
          "time": "20210824 15:50"
        },
        {
          "id": "MDRW2100011736.1.16",
          "form": "일단 얼집에서오면 상태보고 진로보러 가야겠다",
          "original_form": "일단 얼집에서오면 상태보고 진로보러 가야겠다",
          "speaker_id": "1",
          "time": "20210824 15:51"
        },
        {
          "id": "MDRW2100011736.1.17",
          "form": "열만 나지마라. 괜찮을거예요",
          "original_form": "열만 나지마라. 괜찮을거예요",
          "speaker_id": "2",
          "time": "20210824 15:53"
        },
        {
          "id": "MDRW2100011736.1.18",
          "form": "아들은 좀 어떤가요",
          "original_form": "아들은 좀 어떤가요",
          "speaker_id": "2",
          "time": "20210824 15:54"
        },
        {
          "id": "MDRW2100011736.1.19",
          "form": "글쓰기 연습한다고 들었는데 잘 하고 있나요?",
          "original_form": "글쓰기 연습한다고 들었는데 잘 하고 있나요?",
          "speaker_id": "2",
          "time": "20210824 15:55"
        },
        {
          "id": "MDRW2100011736.1.20",
          "form": "매일 쓰곤 있지",
          "original_form": "매일 쓰곤 있지",
          "speaker_id": "1",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011736.1.21",
          "form": "어때요? 많이 늘었어요?",
          "original_form": "어때요? 많이 늘었어요?",
          "speaker_id": "2",
          "time": "20210824 15:56"
        },
        {
          "id": "MDRW2100011736.1.22",
          "form": "삐뚤삐뚤하더니 요샌 많이 늘었다",
          "original_form": "삐뚤삐뚤하더니 요샌 많이 늘었다",
          "speaker_id": "1",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011736.1.23",
          "form": "전에 볼 때는 한두글자는 못 알아보겠던데ㅋㅋㅋ",
          "original_form": "전에 볼 때는 한두글자는 못 알아보겠던데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:58"
        },
        {
          "id": "MDRW2100011736.1.24",
          "form": "그 정도는 아니었거든!",
          "original_form": "그 정도는 아니었거든!",
          "speaker_id": "1",
          "time": "20210824 15:58"
        },
        {
          "id": "MDRW2100011736.1.25",
          "form": "줄 맞춰서도 잘 쓴다..",
          "original_form": "줄 맞춰서도 잘 쓴다..",
          "speaker_id": "1",
          "time": "20210824 15:59"
        },
        {
          "id": "MDRW2100011736.1.26",
          "form": "아침에는비가 마니와서 장화신고갔는데",
          "original_form": "아침에는비가 마니와서 장화신고갔는데",
          "speaker_id": "1",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011736.1.27",
          "form": "지금은 그쳐서 또투덜되겠네",
          "original_form": "지금은 그쳐서 또투덜되겠네",
          "speaker_id": "1",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011736.1.28",
          "form": "ㅋㅋㅋ투덜쟁이네",
          "original_form": "ㅋㅋㅋ투덜쟁이네",
          "speaker_id": "2",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011736.1.29",
          "form": "오늘도 한 투정했네요",
          "original_form": "오늘도 한 투정했네요",
          "speaker_id": "2",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011736.1.30",
          "form": "ㅋㅋ사춘기가 벌써 ㅋ",
          "original_form": "ㅋㅋ사춘기가 벌써 ㅋ",
          "speaker_id": "1",
          "time": "20210824 16:02"
        },
        {
          "id": "MDRW2100011736.1.31",
          "form": "아직 어리지 않나요ㅋㅋㅋ아니 벌써 사춘기라니",
          "original_form": "아직 어리지 않나요ㅋㅋㅋ아니 벌써 사춘기라니",
          "speaker_id": "2",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011736.1.32",
          "form": "요즘은 일찍온다더라",
          "original_form": "요즘은 일찍온다더라",
          "speaker_id": "1",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011736.1.33",
          "form": "그래도 주위얘기들어보니",
          "original_form": "그래도 주위얘기들어보니",
          "speaker_id": "1",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011736.1.34",
          "form": "울애는 착한거더라",
          "original_form": "울애는 착한거더라",
          "speaker_id": "1",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011736.1.35",
          "form": "요새 어린 애들이 무섭죠ㅋㅋㅋ",
          "original_form": "요새 어린 애들이 무섭죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011736.1.36",
          "form": "하긴 아들 표정이 뚱하긴 해도 욕하는건 못 본거 같아요",
          "original_form": "하긴 아들 표정이 뚱하긴 해도 욕하는건 못 본거 같아요",
          "speaker_id": "2",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011736.1.37",
          "form": "ㅇㅇ 이정도면 착한거라니깐",
          "original_form": "ㅇㅇ 이정도면 착한거라니깐",
          "speaker_id": "1",
          "time": "20210824 16:12"
        },
        {
          "id": "MDRW2100011736.1.38",
          "form": "요즘애들 무서버",
          "original_form": "요즘애들 무서버",
          "speaker_id": "1",
          "time": "20210824 16:12"
        },
        {
          "id": "MDRW2100011736.1.39",
          "form": "결혼안하고 나랑살거래",
          "original_form": "결혼안하고 나랑살거래",
          "speaker_id": "1",
          "time": "20210824 16:12"
        },
        {
          "id": "MDRW2100011736.1.40",
          "form": "ㅋㅋㅋㅋ그거 무서운데요",
          "original_form": "ㅋㅋㅋㅋ그거 무서운데요",
          "speaker_id": "2",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011736.1.41",
          "form": "얼른 여자친구부터 소개시켜줘야겠네요ㅋㅋ",
          "original_form": "얼른 여자친구부터 소개시켜줘야겠네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011736.1.42",
          "form": "ㅋㅋ 나는괜찮다 델꼬살지뭐 ㅋ",
          "original_form": "ㅋㅋ 나는괜찮다 델꼬살지뭐 ㅋ",
          "speaker_id": "1",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011736.1.43",
          "form": "여자친구가 힘들끼라~",
          "original_form": "여자친구가 힘들끼라~",
          "speaker_id": "1",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011736.1.44",
          "form": "에이형이라 ㅋ",
          "original_form": "에이형이라 ㅋ",
          "speaker_id": "1",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011736.1.45",
          "form": "감당 가능한가요",
          "original_form": "감당 가능한가요",
          "speaker_id": "2",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100011736.1.46",
          "form": "나이 더 먹으면 나이먹은만큼 더 할텐데ㅋㅋㅋ",
          "original_form": "나이 더 먹으면 나이먹은만큼 더 할텐데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100011736.1.47",
          "form": "ㅋ내가 감당할수있지 다른 여자들은 힘들수도 ㅋ",
          "original_form": "ㅋ내가 감당할수있지 다른 여자들은 힘들수도 ㅋ",
          "speaker_id": "1",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100011736.1.48",
          "form": "맞네 나이무면 더 심해질수도 ㅋ",
          "original_form": "맞네 나이무면 더 심해질수도 ㅋ",
          "speaker_id": "1",
          "time": "20210824 16:15"
        },
        {
          "id": "MDRW2100011736.1.49",
          "form": "ㅋㅋㅋㅋ몇 년 뒤에 제발 나가라고 하소연 할 지도",
          "original_form": "ㅋㅋㅋㅋ몇 년 뒤에 제발 나가라고 하소연 할 지도",
          "speaker_id": "2",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100011736.1.50",
          "form": "몰라요",
          "original_form": "몰라요",
          "speaker_id": "2",
          "time": "20210824 16:16"
        },
        {
          "id": "MDRW2100011736.1.51",
          "form": "맞다 이아랑 몬살겠던데 ㅋ\r\n지금도 나가라하고싶을때 한번씩있더라",
          "original_form": "맞다 이아랑 몬살겠던데 ㅋ\r\n지금도 나가라하고싶을때 한번씩있더라",
          "speaker_id": "1",
          "time": "20210824 16:19"
        }
      ]
