      "utterance": [
        {
          "id": "MDRW2100031984.1.1",
          "form": "날씨가 많이 쌀쌀하네요 ㅠ",
          "original_form": "날씨가 많이 쌀쌀하네요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 10:19"
        },
        {
          "id": "MDRW2100031984.1.2",
          "form": "네, 이젠 가을이 되어가네요",
          "original_form": "네, 이젠 가을이 되어가네요",
          "speaker_id": "2",
          "time": "20210905 10:40"
        },
        {
          "id": "MDRW2100031984.1.3",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100031984.1.4",
          "form": "가을이라니 너무 좋네요",
          "original_form": "가을이라니 너무 좋네요",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100031984.1.5",
          "form": "가을에 꼭 하는게 있으신가요?",
          "original_form": "가을에 꼭 하는게 있으신가요?",
          "speaker_id": "1",
          "time": "20210905 12:08"
        },
        {
          "id": "MDRW2100031984.1.6",
          "form": "딱히 특별히 하는건 없어요",
          "original_form": "딱히 특별히 하는건 없어요",
          "speaker_id": "2",
          "time": "20210905 12:54"
        },
        {
          "id": "MDRW2100031984.1.7",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100031984.1.8",
          "form": "저는 그래도 단풍구경이나 지역축제를 갔던 것 같아요",
          "original_form": "저는 그래도 단풍구경이나 지역축제를 갔던 것 같아요",
          "speaker_id": "1",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100031984.1.9",
          "form": "오~ 가을에만 볼수있으니 더욱 좋네요",
          "original_form": "오~ 가을에만 볼수있으니 더욱 좋네요",
          "speaker_id": "2",
          "time": "20210905 17:19"
        },
        {
          "id": "MDRW2100031984.1.10",
          "form": "맞아요 ㅎㅎ",
          "original_form": "맞아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 19:23"
        },
        {
          "id": "MDRW2100031984.1.11",
          "form": "가을에만 하는 전어 축제나 단풍축제 등 가고싶네요 ㅎㅎ",
          "original_form": "가을에만 하는 전어 축제나 단풍축제 등 가고싶네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 19:23"
        },
        {
          "id": "MDRW2100031984.1.12",
          "form": "오늘 여기는 쌀쌀해서 긴바지와 7부 윗옷을 입었어요",
          "original_form": "오늘 여기는 쌀쌀해서 긴바지와 7부 윗옷을 입었어요",
          "speaker_id": "1",
          "time": "20210905 19:24"
        },
        {
          "id": "MDRW2100031984.1.13",
          "form": "옷차림은 어떠셨나요??",
          "original_form": "옷차림은 어떠셨나요??",
          "speaker_id": "1",
          "time": "20210905 19:24"
        },
        {
          "id": "MDRW2100031984.1.14",
          "form": "반팔아니면 긴팔에 긴바지,반바지요",
          "original_form": "반팔아니면 긴팔에 긴바지,반바지요",
          "speaker_id": "2",
          "time": "20210905 20:04"
        },
        {
          "id": "MDRW2100031984.1.15",
          "form": "그렇죠 요즘은 ㄱㅇ런날씨가 돼버렸네요 ㅎㅎ",
          "original_form": "그렇죠 요즘은 ㄱㅇ런날씨가 돼버렸네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100031984.1.16",
          "form": "내일도 비가 온다는데",
          "original_form": "내일도 비가 온다는데",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100031984.1.17",
          "form": "내일의 날씨는 어떤가요?",
          "original_form": "내일의 날씨는 어떤가요?",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100031984.1.18",
          "form": "요기도 비가 온다고 해네요 이번주는 주간날씨봤는데 3일은 비가 올듯해요",
          "original_form": "요기도 비가 온다고 해네요 이번주는 주간날씨봤는데 3일은 비가 올듯해요",
          "speaker_id": "2",
          "time": "20210906 00:40"
        },
        {
          "id": "MDRW2100031984.1.19",
          "form": "와 그렇군요",
          "original_form": "와 그렇군요",
          "speaker_id": "1",
          "time": "20210906 11:22"
        },
        {
          "id": "MDRW2100031984.1.20",
          "form": "여기도 수요일까지 비가안다고 쓰여있는데",
          "original_form": "여기도 수요일까지 비가안다고 쓰여있는데",
          "speaker_id": "1",
          "time": "20210906 11:22"
        },
        {
          "id": "MDRW2100031984.1.21",
          "form": "또 지금은 비가 안오네요 ㅎㅎ",
          "original_form": "또 지금은 비가 안오네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 11:22"
        },
        {
          "id": "MDRW2100031984.1.22",
          "form": "하루종인 온다고 했는데 말이에요",
          "original_form": "하루종인 온다고 했는데 말이에요",
          "speaker_id": "1",
          "time": "20210906 11:23"
        },
        {
          "id": "MDRW2100031984.1.23",
          "form": "요긴 비안오다가 잠깐왔어요~",
          "original_form": "요긴 비안오다가 잠깐왔어요~",
          "speaker_id": "2",
          "time": "20210906 16:28"
        },
        {
          "id": "MDRW2100031984.1.24",
          "form": "여기도 그런것같아요 ㅎㅎ 잠깐 오더라구요",
          "original_form": "여기도 그런것같아요 ㅎㅎ 잠깐 오더라구요",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100031984.1.25",
          "form": "기상청이 요즘 잘 맞네요",
          "original_form": "기상청이 요즘 잘 맞네요",
          "speaker_id": "1",
          "time": "20210906 17:54"
        },
        {
          "id": "MDRW2100031984.1.26",
          "form": "가을비가 내리고 쌀쌀해졌는데 막상 내일.다시 덥다고 하더라구요",
          "original_form": "가을비가 내리고 쌀쌀해졌는데 막상 내일.다시 덥다고 하더라구요",
          "speaker_id": "1",
          "time": "20210906 17:54"
        }
      ]
    }
  ]
