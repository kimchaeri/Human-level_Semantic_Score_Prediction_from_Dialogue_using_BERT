      "utterance": [
        {
          "id": "MDRW2100017408.1.1",
          "form": "오늘 뭐 드셨어요",
          "original_form": "오늘 뭐 드셨어요",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017408.1.2",
          "form": "닭볶음탕이요 ㅋㅋㅋ",
          "original_form": "닭볶음탕이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017408.1.3",
          "form": "뭐드셧어요?",
          "original_form": "뭐드셧어요?",
          "speaker_id": "2",
          "time": "20210826 19:17"
        },
        {
          "id": "MDRW2100017408.1.4",
          "form": "저는 곧 낙지볶음 먹을거같아요",
          "original_form": "저는 곧 낙지볶음 먹을거같아요",
          "speaker_id": "1",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017408.1.5",
          "form": "낙지볶음 맛잇겟네요",
          "original_form": "낙지볶음 맛잇겟네요",
          "speaker_id": "2",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017408.1.6",
          "form": "밥 볶아먹나요?",
          "original_form": "밥 볶아먹나요?",
          "speaker_id": "2",
          "time": "20210826 19:38"
        },
        {
          "id": "MDRW2100017408.1.7",
          "form": "비벼먹을거에요 ㅋㅋ",
          "original_form": "비벼먹을거에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017408.1.8",
          "form": "닭볶음탕 직접 하셨나요",
          "original_form": "닭볶음탕 직접 하셨나요",
          "speaker_id": "1",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100017408.1.9",
          "form": "아니요 배달이요 ㅋㅋㅋㅋㅋ",
          "original_form": "아니요 배달이요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017408.1.10",
          "form": "낙지볶음은 요리하신거예요?",
          "original_form": "낙지볶음은 요리하신거예요?",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100017408.1.11",
          "form": "아 저도 포장이요 ㅋㅋㅋ",
          "original_form": "아 저도 포장이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017408.1.12",
          "form": "가족들 식사중인데 누워있어요 ㅋㅋ",
          "original_form": "가족들 식사중인데 누워있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100017408.1.13",
          "form": "가족들은 다른메뉴 드시는 거예요?",
          "original_form": "가족들은 다른메뉴 드시는 거예요?",
          "speaker_id": "2",
          "time": "20210826 19:42"
        },
        {
          "id": "MDRW2100017408.1.14",
          "form": "아니요 메인 낙지볶음ㅋㅋㅋ",
          "original_form": "아니요 메인 낙지볶음ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017408.1.15",
          "form": "이제다먹었어요",
          "original_form": "이제다먹었어요",
          "speaker_id": "1",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017408.1.16",
          "form": "밥도 비벼드셧나요 ㅋㅋ",
          "original_form": "밥도 비벼드셧나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:57"
        },
        {
          "id": "MDRW2100017408.1.17",
          "form": "네 그럣습니다 ㅋㅋ 닭볶음탕 잘 드셨나요",
          "original_form": "네 그럣습니다 ㅋㅋ 닭볶음탕 잘 드셨나요",
          "speaker_id": "1",
