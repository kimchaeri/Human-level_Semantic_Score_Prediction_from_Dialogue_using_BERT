      "utterance": [
        {
          "id": "MDRW2100012830.1.1",
          "form": "네 생각보다 과일향이 강하고 순하더라고요",
          "original_form": "네 생각보다 과일향이 강하고 순하더라고요",
          "speaker_id": "2",
          "time": "20210825 02:09"
        },
        {
          "id": "MDRW2100012830.1.2",
          "form": "name1님은 마셔보셨나요",
          "original_form": "&name1&님은 마셔보셨나요",
          "speaker_id": "2",
          "time": "20210825 02:10"
        },
        {
          "id": "MDRW2100012830.1.3",
          "form": "아니요ㅠ 뭔가 거부감 들었어요",
          "original_form": "아니요ㅠ 뭔가 거부감 들었어요",
          "speaker_id": "1",
          "time": "20210825 02:10"
        },
        {
          "id": "MDRW2100012830.1.4",
          "form": "곰표에서 팝콘인가? 그것도 나왓더라구요",
          "original_form": "곰표에서 팝콘인가? 그것도 나왓더라구요",
          "speaker_id": "1",
          "time": "20210825 02:10"
        },
        {
          "id": "MDRW2100012830.1.5",
          "form": "아 콜래보 중엔 곰표가 그나마 여기저기 잘어울리는 것 같아요",
          "original_form": "아 콜래보 중엔 곰표가 그나마 여기저기 잘어울리는 것 같아요",
          "speaker_id": "2",
          "time": "20210825 02:12"
        },
        {
          "id": "MDRW2100012830.1.6",
          "form": "오 팝콘도요?",
          "original_form": "오 팝콘도요?",
          "speaker_id": "2",
          "time": "20210825 02:12"
        },
        {
          "id": "MDRW2100012830.1.7",
          "form": "어 그건 맞아요",
          "original_form": "어 그건 맞아요",
          "speaker_id": "1",
          "time": "20210825 02:12"
        },
        {
          "id": "MDRW2100012830.1.8",
          "form": "무슨 모나미 말표 등등 나오던데 적응이 안돼요 ㅠ",
          "original_form": "무슨 모나미 말표 등등 나오던데 적응이 안돼요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 02:12"
        },
        {
          "id": "MDRW2100012830.1.9",
          "form": "곰표 맥주는 맛있다고 들었는데 궁금하네요 ㅎㅎ",
          "original_form": "곰표 맥주는 맛있다고 들었는데 궁금하네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 02:12"
        },
        {
          "id": "MDRW2100012830.1.10",
          "form": "술 잘 드시나요?",
          "original_form": "술 잘 드시나요?",
          "speaker_id": "2",
          "time": "20210825 02:13"
        },
        {
          "id": "MDRW2100012830.1.11",
          "form": "진한 맛 좋아하면 취향 아닐테고",
          "original_form": "진한 맛 좋아하면 취향 아닐테고",
          "speaker_id": "2",
          "time": "20210825 02:14"
        },
        {
          "id": "MDRW2100012830.1.12",
          "form": "잘 못먹는 편이에요 ㅎㅎ",
          "original_form": "잘 못먹는 편이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 02:14"
        },
        {
          "id": "MDRW2100012830.1.13",
          "form": "그럼 맥주중에선 입맛에 맞으실 거예요",
          "original_form": "그럼 맥주중에선 입맛에 맞으실 거예요",
          "speaker_id": "2",
          "time": "20210825 02:14"
        },
        {
          "id": "MDRW2100012830.1.14",
          "form": "과일향 날 줄응 몰랐는데 구미가 당기네요",
          "original_form": "과일향 날 줄응 몰랐는데 구미가 당기네요",
          "speaker_id": "1",
          "time": "20210825 02:14"
        },
        {
          "id": "MDRW2100012830.1.15",
          "form": "망고링고 마시다 곰표 마시면 비슷한 계열인게 느껴져요",
          "original_form": "망고링고 마시다 곰표 마시면 비슷한 계열인게 느껴져요",
          "speaker_id": "2",
          "time": "20210825 02:14"
        },
        {
          "id": "MDRW2100012830.1.16",
          "form": "헐 그래요? 더 솔깃하네요",
          "original_form": "헐 그래요? 더 솔깃하네요",
          "speaker_id": "1",
          "time": "20210825 02:15"
        },
        {
          "id": "MDRW2100012830.1.17",
          "form": "혹시 콜라보제품들 논란 있던거 어떻게 생각하세요?",
          "original_form": "혹시 콜라보제품들 논란 있던거 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210825 02:15"
        },
        {
          "id": "MDRW2100012830.1.18",
          "form": "망고링고정도로 순하진 않아도요ㅎ 연달아 마시기 거부감이 없더라고요",
          "original_form": "망고링고정도로 순하진 않아도요ㅎ 연달아 마시기 거부감이 없더라고요",
          "speaker_id": "2",
          "time": "20210825 02:15"
        },
        {
          "id": "MDRW2100012830.1.19",
          "form": "아 모나미는 좀 자제했으면 하죠",
          "original_form": "아 모나미는 좀 자제했으면 하죠",
          "speaker_id": "2",
          "time": "20210825 02:15"
        },
        {
          "id": "MDRW2100012830.1.20",
          "form": "ㅌㅋㅋㅋ 근데 모나미랑 삼성 콜라보 해서",
          "original_form": "ㅌㅋㅋㅋ 근데 모나미랑 삼성 콜라보 해서",
          "speaker_id": "1",
          "time": "20210825 02:16"
        },
        {
          "id": "MDRW2100012830.1.21",
          "form": "S펜 만든건 너무 예쁘게 잘 나왔더라구요",
          "original_form": "S펜 만든건 너무 예쁘게 잘 나왔더라구요",
          "speaker_id": "1",
          "time": "20210825 02:16"
        },
        {
          "id": "MDRW2100012830.1.22",
          "form": "그나마 곰표는 식품쪽에서 식품쪽이니 괜찮고",
          "original_form": "그나마 곰표는 식품쪽에서 식품쪽이니 괜찮고",
          "speaker_id": "2",
          "time": "20210825 02:16"
        },
        {
          "id": "MDRW2100012830.1.23",
          "form": "모나미도 식품 말고 다른데 콜래보는 좋아요ㅎ",
          "original_form": "모나미도 식품 말고 다른데 콜래보는 좋아요ㅎ",
          "speaker_id": "2",
          "time": "20210825 02:17"
        },
        {
          "id": "MDRW2100012830.1.24",
          "form": "그쵸 말표는 좀 ㅌㅋ 그랬어요...",
          "original_form": "그쵸 말표는 좀 ㅌㅋ 그랬어요...",
          "speaker_id": "1",
          "time": "20210825 02:17"
        },
        {
          "id": "MDRW2100012830.1.25",
          "form": "단지 식품쪽 콜래보는 조심스러워서요ㅎ",
          "original_form": "단지 식품쪽 콜래보는 조심스러워서요ㅎ",
          "speaker_id": "2",
          "time": "20210825 02:17"
        }
      ]
    }
