      "utterance": [
        {
          "id": "MDRW2100042038.1.1",
          "form": "수영구만 주는건가 뭐지",
          "original_form": "수영구만 주는건가 뭐지",
          "speaker_id": "2",
          "time": "20210906 19:03"
        },
        {
          "id": "MDRW2100042038.1.2",
          "form": "그런가봐오!!!",
          "original_form": "그런가봐오!!!",
          "speaker_id": "1",
          "time": "20210906 19:03"
        },
        {
          "id": "MDRW2100042038.1.3",
          "form": "장만볼건가요",
          "original_form": "장만볼건가요",
          "speaker_id": "1",
          "time": "20210906 19:03"
        },
        {
          "id": "MDRW2100042038.1.4",
          "form": "네네 장 보는데 다 쓸것갘아요",
          "original_form": "네네 장 보는데 다 쓸것갘아요",
          "speaker_id": "2",
          "time": "20210906 19:04"
        },
        {
          "id": "MDRW2100042038.1.5",
          "form": "뭐사먹을거예요!",
          "original_form": "뭐사먹을거예요!",
          "speaker_id": "1",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100042038.1.6",
          "form": "무슨반찬",
          "original_form": "무슨반찬",
          "speaker_id": "1",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100042038.1.7",
          "form": "개구리반찬",
          "original_form": "개구리반찬",
          "speaker_id": "1",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100042038.1.8",
          "form": "음 요거트 같은거 사올거같아요",
          "original_form": "음 요거트 같은거 사올거같아요",
          "speaker_id": "2",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100042038.1.9",
          "form": "요거트좋죵 건강하구좋아용",
          "original_form": "요거트좋죵 건강하구좋아용",
          "speaker_id": "1",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100042038.1.10",
          "form": "기분좋아지는 맛",
          "original_form": "기분좋아지는 맛",
          "speaker_id": "2",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100042038.1.11",
          "form": "과일도 사고요",
          "original_form": "과일도 사고요",
          "speaker_id": "2",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100042038.1.12",
          "form": "과일은무슨과일쓰",
          "original_form": "과일은무슨과일쓰",
          "speaker_id": "1",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100042038.1.13",
          "form": "요즘 뭐가 제철인가요",
          "original_form": "요즘 뭐가 제철인가요",
          "speaker_id": "2",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100042038.1.14",
          "form": "포도?",
          "original_form": "포도?",
          "speaker_id": "2",
          "time": "20210906 19:06"
        },
        {
          "id": "MDRW2100042038.1.15",
          "form": "포도맛이쓰",
          "original_form": "포도맛이쓰",
          "speaker_id": "1",
          "time": "20210906 19:08"
        },
        {
          "id": "MDRW2100042038.1.16",
          "form": "강추",
          "original_form": "강추",
          "speaker_id": "1",
          "time": "20210906 19:08"
        },
        {
          "id": "MDRW2100042038.1.17",
          "form": "포도졸아 샤인머스캣두",
          "original_form": "포도졸아 샤인머스캣두",
          "speaker_id": "2",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100042038.1.18",
          "form": "샤인머스캣아직못먹어봣슴돠",
          "original_form": "샤인머스캣아직못먹어봣슴돠",
          "speaker_id": "1",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100042038.1.19",
          "form": "드셔보세요 정말 맛나요",
          "original_form": "드셔보세요 정말 맛나요",
          "speaker_id": "2",
