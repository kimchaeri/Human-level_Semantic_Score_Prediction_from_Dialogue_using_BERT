        },
        {
          "id": "MDRW2100003526.1.2",
          "form": "엄마~ 주말에 백화점 가야할듯요~",
          "original_form": "엄마~ 주말에 백화점 가야할듯요~",
          "speaker_id": "2",
          "time": "20210821 00:24"
        },
        {
          "id": "MDRW2100003526.1.3",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "1",
          "time": "20210821 00:24"
        },
        {
          "id": "MDRW2100003526.1.4",
          "form": "선글라스는 껴보고 사느게 좋을 거 같아서요~",
          "original_form": "선글라스는 껴보고 사느게 좋을 거 같아서요~",
          "speaker_id": "2",
          "time": "20210821 00:25"
        },
        {
          "id": "MDRW2100003526.1.5",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "1",
          "time": "20210821 00:25"
        },
        {
          "id": "MDRW2100003526.1.6",
          "form": "백화점까지는 못 갈거 같은데...",
          "original_form": "백화점까지는 못 갈거 같은데...",
          "speaker_id": "1",
          "time": "20210821 00:25"
        },
        {
          "id": "MDRW2100003526.1.7",
          "form": "왜요? 그냥 껴보는 것만 하는 건데~",
          "original_form": "왜요? 그냥 껴보는 것만 하는 건데~",
          "speaker_id": "2",
          "time": "20210821 00:25"
        },
        {
          "id": "MDRW2100003526.1.8",
          "form": "아니 2001 아울렛?",
          "original_form": "아니 2001 아울렛?",
          "speaker_id": "2",
          "time": "20210821 00:26"
        },
        {
          "id": "MDRW2100003526.1.9",
          "form": "알았어~",
          "original_form": "알았어~",
          "speaker_id": "1",
          "time": "20210821 00:26"
        },
        {
          "id": "MDRW2100003526.1.10",
          "form": "선글라스는 얼굴형에 맞는 거랑",
          "original_form": "선글라스는 얼굴형에 맞는 거랑",
          "speaker_id": "2",
          "time": "20210821 00:26"
        },
        {
          "id": "MDRW2100003526.1.11",
          "form": "알 색깔도 눈에 맞는거 사야 한대요",
          "original_form": "알 색깔도 눈에 맞는거 사야 한대요",
          "speaker_id": "2",
          "time": "20210821 00:26"
        },
        {
          "id": "MDRW2100003526.1.12",
          "form": "그래~ 언제갈껀데?",
          "original_form": "그래~ 언제갈껀데?",
          "speaker_id": "1",
          "time": "20210821 00:27"
        },
        {
          "id": "MDRW2100003526.1.13",
          "form": "훔,,, 토욜에 잠깐?",
          "original_form": "훔,,, 토욜에 잠깐?",
          "speaker_id": "2",
          "time": "20210821 00:27"
        },
        {
          "id": "MDRW2100003526.1.14",
          "form": "몇시에?",
          "original_form": "몇시에?",
          "speaker_id": "1",
          "time": "20210821 00:27"
        },
        {
          "id": "MDRW2100003526.1.15",
          "form": "그냥 일찍 오전에 가서 보고 점심 먹고 올까요?",
          "original_form": "그냥 일찍 오전에 가서 보고 점심 먹고 올까요?",
          "speaker_id": "2",
          "time": "20210821 00:27"
        },
        {
          "id": "MDRW2100003526.1.16",
          "form": "좋은 생각이네~",
          "original_form": "좋은 생각이네~",
          "speaker_id": "1",
          "time": "20210821 00:27"
        },
        {
          "id": "MDRW2100003526.1.17",
          "form": "밥은 엄마가 사줄게",
          "original_form": "밥은 엄마가 사줄게",
          "speaker_id": "1",
          "time": "20210821 00:27"
        },
        {
          "id": "MDRW2100003526.1.18",
          "form": "오예~~ 맛있는 거 먹어야겠다~~ㅎㅎ",
