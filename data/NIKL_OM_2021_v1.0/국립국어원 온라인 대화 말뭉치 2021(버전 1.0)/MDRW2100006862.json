      "utterance": [
        {
          "id": "MDRW2100006862.1.1",
          "form": "결혼은 언제쯤 하셨나요?",
          "original_form": "결혼은 언제쯤 하셨나요?",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100006862.1.2",
          "form": "8년전이요",
          "original_form": "8년전이요",
          "speaker_id": "1",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100006862.1.3",
          "form": "오 오래되셨네요!",
          "original_form": "오 오래되셨네요!",
          "speaker_id": "2",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100006862.1.4",
          "form": "저는 아직 미혼이에요",
          "original_form": "저는 아직 미혼이에요",
          "speaker_id": "2",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100006862.1.5",
          "form": "가족이 생기시니까 할일이 참 많으실껏 같아요",
          "original_form": "가족이 생기시니까 할일이 참 많으실껏 같아요",
          "speaker_id": "2",
          "time": "20210908 10:58"
        },
        {
          "id": "MDRW2100006862.1.6",
          "form": "네 그렇죠 짐도 늘고 할일도 늘고",
          "original_form": "네 그렇죠 짐도 늘고 할일도 늘고",
          "speaker_id": "1",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100006862.1.7",
          "form": "집안일도 좀 많아졌나요?",
          "original_form": "집안일도 좀 많아졌나요?",
          "speaker_id": "2",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100006862.1.8",
          "form": "네 빨래도 옷정리 목욕 양치 밥..끝도없죠",
          "original_form": "네 빨래도 옷정리 목욕 양치 밥..끝도없죠",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100006862.1.9",
          "form": "ㅠㅠ 그쳐 바쁘시겠어요 저는 일단 그런게 뭔가 당연하면서 힘든거 같아요",
          "original_form": "ㅠㅠ 그쳐 바쁘시겠어요 저는 일단 그런게 뭔가 당연하면서 힘든거 같아요",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100006862.1.10",
          "form": "집안일이라는게,, ㅠ",
          "original_form": "집안일이라는게,, ㅠ",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100006862.1.11",
          "form": "맞아요 티 안나고",
          "original_form": "맞아요 티 안나고",
          "speaker_id": "1",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100006862.1.12",
          "form": "아 그런 점도 있네요 티가 안난다는 점",
          "original_form": "아 그런 점도 있네요 티가 안난다는 점",
          "speaker_id": "2",
          "time": "20210908 11:03"
        },
        {
          "id": "MDRW2100006862.1.13",
          "form": "하는건 힘든데 ㅎㅎ",
          "original_form": "하는건 힘든데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 11:09"
        },
        {
          "id": "MDRW2100006862.1.14",
          "form": "ㅠㅠ 진짜 힘든데 티안나는거 만큼 힘든건 없을 거 같기도 해요",
          "original_form": "ㅠㅠ 진짜 힘든데 티안나는거 만큼 힘든건 없을 거 같기도 해요",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006862.1.15",
          "form": "그만큼 집안일 안하려고 미루는 저의 심리가 이런 이윤가봐여 ㅠㅋㅋ",
          "original_form": "그만큼 집안일 안하려고 미루는 저의 심리가 이런 이윤가봐여 ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006862.1.16",
          "form": "하기싫죠 가장 싫은건요??",
          "original_form": "하기싫죠 가장 싫은건요??",
          "speaker_id": "1",
          "time": "20210908 11:10"
        },
        {
          "id": "MDRW2100006862.1.17",
          "form": "빨래가 그렇습니다",
          "original_form": "빨래가 그렇습니다",
          "speaker_id": "2",
          "time": "20210908 11:11"
        },
        {
          "id": "MDRW2100006862.1.18",
          "form": "아 건조기 사용하시나요",
          "original_form": "아 건조기 사용하시나요",
          "speaker_id": "1",
          "time": "20210908 11:16"
        }
      ]
