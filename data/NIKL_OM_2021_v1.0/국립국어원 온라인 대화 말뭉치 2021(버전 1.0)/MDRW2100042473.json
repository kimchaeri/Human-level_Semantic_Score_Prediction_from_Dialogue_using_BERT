      "utterance": [
        {
          "id": "MDRW2100042473.1.1",
          "form": "사이드 메뉴도 시켰니?",
          "original_form": "사이드 메뉴도 시켰니?",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042473.1.2",
          "form": "아니 집에서 직접해먹었어",
          "original_form": "아니 집에서 직접해먹었어",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042473.1.3",
          "form": "아 배달해먹은 게 아니었구나",
          "original_form": "아 배달해먹은 게 아니었구나",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042473.1.4",
          "form": "배달했다늠 줄",
          "original_form": "배달했다늠 줄",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042473.1.5",
          "form": "이번에",
          "original_form": "이번에",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042473.1.6",
          "form": "김장할거라고",
          "original_form": "김장할거라고",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042473.1.7",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042473.1.8",
          "form": "수육해먹었는데",
          "original_form": "수육해먹었는데",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042473.1.9",
          "form": "웃긴건 김치는 아직도야",
          "original_form": "웃긴건 김치는 아직도야",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042473.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042473.1.11",
          "form": "설레발 조리법 아니냐고",
          "original_form": "설레발 조리법 아니냐고",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042473.1.12",
          "form": "김치를 만들 예정이니",
          "original_form": "김치를 만들 예정이니",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042473.1.13",
          "form": "돼지 먼저 먹자!",
          "original_form": "돼지 먼저 먹자!",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042473.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042473.1.15",
          "form": "진짜웃겨서 원",
          "original_form": "진짜웃겨서 원",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042473.1.16",
          "form": "새김치도 없는데 뭐랑 먹었어",
          "original_form": "새김치도 없는데 뭐랑 먹었어",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042473.1.17",
          "form": "묵은지?",
          "original_form": "묵은지?",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042473.1.18",
          "form": "응 그리고 간장 와사비",
          "original_form": "응 그리고 간장 와사비",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042473.1.19",
          "form": "ㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042473.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042473.1.21",
          "form": "은근 색다르고 맛있어",
          "original_form": "은근 색다르고 맛있어",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042473.1.22",
          "form": "약간 일식 느낌 나겠는걸",
          "original_form": "약간 일식 느낌 나겠는걸",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042473.1.23",
          "form": "깔끔할 것 같아",
          "original_form": "깔끔할 것 같아",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042473.1.24",
          "form": "난 쌈장판데",
          "original_form": "난 쌈장판데",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042473.1.25",
          "form": "쌈싸먹는것도 맛있지",
          "original_form": "쌈싸먹는것도 맛있지",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042473.1.26",
          "form": "다음엔 간장 해먹어볼게",
          "original_form": "다음엔 간장 해먹어볼게",
          "speaker_id": "2",
          "time": "20210906 22:24"
        }
      ]
