      "utterance": [
        {
          "id": "MDRW2100005811.1.1",
          "form": "최근에 머 갖고싶은거잇음?",
          "original_form": "최근에 머 갖고싶은거잇음?",
          "speaker_id": "1",
          "time": "20210907 13:12"
        },
        {
          "id": "MDRW2100005811.1.2",
          "form": "화장품, 옷, 스위치 게임 정도? 사람 만나는게 가장 좋지만",
          "original_form": "화장품, 옷, 스위치 게임 정도? 사람 만나는게 가장 좋지만",
          "speaker_id": "2",
          "time": "20210907 13:13"
        },
        {
          "id": "MDRW2100005811.1.3",
          "form": "아 요즘은",
          "original_form": "아 요즘은",
          "speaker_id": "2",
          "time": "20210907 13:13"
        },
        {
          "id": "MDRW2100005811.1.4",
          "form": "목걸이, 팔찌, 반지도",
          "original_form": "목걸이, 팔찌, 반지도",
          "speaker_id": "2",
          "time": "20210907 13:13"
        },
        {
          "id": "MDRW2100005811.1.5",
          "form": "관심가고있음 악세서리",
          "original_form": "관심가고있음 악세서리",
          "speaker_id": "2",
          "time": "20210907 13:14"
        },
        {
          "id": "MDRW2100005811.1.6",
          "form": "그보다 님 오늘 생일아님?",
          "original_form": "그보다 님 오늘 생일아님?",
          "speaker_id": "2",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100005811.1.7",
          "form": "맞음",
          "original_form": "맞음",
          "speaker_id": "1",
          "time": "20210907 13:15"
        },
        {
          "id": "MDRW2100005811.1.8",
          "form": "거기 카페 뭐 없음?",
          "original_form": "거기 카페 뭐 없음?",
          "speaker_id": "2",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100005811.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ괜찮아  무리안해도됨",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ괜찮아  무리안해도됨",
          "speaker_id": "1",
          "time": "20210907 13:17"
        },
        {
          "id": "MDRW2100005811.1.10",
          "form": "무리안하니까",
          "original_form": "무리안하니까",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100005811.1.11",
          "form": "이거 카펜",
          "original_form": "이거 카펜",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100005811.1.12",
          "form": "말해주셈",
          "original_form": "말해주셈",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100005811.1.13",
          "form": "중고거래 개꿀..",
          "original_form": "중고거래 개꿀..",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100005811.1.14",
          "form": "아 근데 나 향수팔려고 올린거 아무도 안사가",
          "original_form": "아 근데 나 향수팔려고 올린거 아무도 안사가",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100005811.1.15",
          "form": "중고거래 자주하긴하는데",
          "original_form": "중고거래 자주하긴하는데",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100005811.1.16",
          "form": "요즘은 파는쪽이",
          "original_form": "요즘은 파는쪽이",
          "speaker_id": "2",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100005811.1.17",
          "form": "비율이 압도적이라",
          "original_form": "비율이 압도적이라",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100005811.1.18",
          "form": "파는거 난 진심 귀찮아서못해",
          "original_form": "파는거 난 진심 귀찮아서못해",
          "speaker_id": "1",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100005811.1.19",
          "form": "싸게 올리려고 경쟁도 많이하고 그래서 더 그런듯",
          "original_form": "싸게 올리려고 경쟁도 많이하고 그래서 더 그런듯",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100005811.1.20",
          "form": "나도 사실",
          "original_form": "나도 사실",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100005811.1.21",
          "form": "귀찮아서",
          "original_form": "귀찮아서",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100005811.1.22",
          "form": "가격을 너무 빨리내림",
          "original_form": "가격을 너무 빨리내림",
          "speaker_id": "2",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100005811.1.23",
          "form": "가격제안 풀어놨으니까 제발 제안하세요",
          "original_form": "가격제안 풀어놨으니까 제발 제안하세요",
          "speaker_id": "1",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100005811.1.24",
          "form": "향수 사놓고 안쓰는거 개임 ㅠ",
          "original_form": "향수 사놓고 안쓰는거 {censored}개임 ㅠ",
          "speaker_id": "1",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100005811.1.25",
          "form": "사람들 제안도 잘 안함ㅋㅋ",
          "original_form": "사람들 제안도 잘 안함ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100005811.1.26",
          "form": "정해진 가격 좋아해서",
          "original_form": "정해진 가격 좋아해서",
          "speaker_id": "2",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100005811.1.27",
          "form": "올려논것ㅅ도 겁나쌈",
          "original_form": "올려논것ㅅ도 겁나쌈",
          "speaker_id": "1",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100005811.1.28",
          "form": "싸야 사가기는 하니까",
          "original_form": "싸야 사가기는 하니까",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100005811.1.29",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "1",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100005811.1.30",
          "form": "아 근데 향수사고싶다",
          "original_form": "아 근데 향수사고싶다",
          "speaker_id": "1",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100005811.1.31",
          "form": "향수 사니 좋드라",
          "original_form": "향수 사니 좋드라",
          "speaker_id": "2",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100005811.1.32",
          "form": "생일선물로 받아서 이득임 히히",
          "original_form": "생일선물로 받아서 이득임 히히",
          "speaker_id": "1",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100005811.1.33",
          "form": "나는 향수 사도 한두가지만 쓰는거 좋아함",
          "original_form": "나는 향수 사도 한두가지만 쓰는거 좋아함",
          "speaker_id": "2",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100005811.1.34",
          "form": "저래야하는데....",
          "original_form": "저래야하는데....",
          "speaker_id": "1",
          "time": "20210907 13:29"
        },
        {
          "id": "MDRW2100005811.1.35",
          "form": "나 디스커버리세트 샀잖어 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "나 디스커버리세트 샀잖어 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:29"
        },
        {
          "id": "MDRW2100005811.1.36",
          "form": "그냥 향 자체를 여러개보다 한가지 고집하는편",
          "original_form": "그냥 향 자체를 여러개보다 한가지 고집하는편",
          "speaker_id": "2",
          "time": "20210907 13:29"
        },
        {
          "id": "MDRW2100005811.1.37",
          "form": "아니 받았잖아 ㅋㅋㅋ",
          "original_form": "아니 받았잖아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:29"
        },
        {
          "id": "MDRW2100005811.1.38",
          "form": "뭘 구매해서 써도 그렇지만",
          "original_form": "뭘 구매해서 써도 그렇지만",
          "speaker_id": "2",
          "time": "20210907 13:29"
        },
        {
          "id": "MDRW2100005811.1.39",
          "form": "아냐 갠차넠 ㅋㅋㅋㅋㅋ",
          "original_form": "아냐 갠차넠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:32"
        },
        {
          "id": "MDRW2100005811.1.40",
          "form": "내가 찾아보면됨",
          "original_form": "내가 찾아보면됨",
