      "utterance": [
        {
          "id": "MDRW2100043668.1.1",
          "form": "저도요!",
          "original_form": "저도요!",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043668.1.2",
          "form": "마침 해물도 일는데 해먹을껄 그랬어요",
          "original_form": "마침 해물도 일는데 해먹을껄 그랬어요",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043668.1.3",
          "form": "오오 해물 파전 !!!",
          "original_form": "오오 해물 파전 !!!",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043668.1.4",
          "form": "전 뒤집는거 잘하세요?",
          "original_form": "전 뒤집는거 잘하세요?",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043668.1.5",
          "form": "아뇨 3번에 1번은 실패하는거 같어요",
          "original_form": "아뇨 3번에 1번은 실패하는거 같어요",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043668.1.6",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043668.1.7",
          "form": "ㅋㅋㅋ 저는 아에 시도도 못하겟어요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 저는 아에 시도도 못하겟어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043668.1.8",
          "form": "해물에 어떤거 넣으세요?",
          "original_form": "해물에 어떤거 넣으세요?",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043668.1.9",
          "form": "저는 only 오징어파입니다..!",
          "original_form": "저는 only 오징어파입니다..!",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043668.1.10",
          "form": "해물믹스말고 오징어만 사서 넣어요",
          "original_form": "해물믹스말고 오징어만 사서 넣어요",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043668.1.11",
          "form": "우아 오징어 ㅠㅠㅠ",
          "original_form": "우아 오징어 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043668.1.12",
          "form": "말린 오징어도 좋아하세요?",
          "original_form": "말린 오징어도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043668.1.13",
          "form": "완전 좋아해요..",
          "original_form": "완전 좋아해요..",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043668.1.14",
          "form": "한치 드셔보셨엉ㅅ?",
          "original_form": "한치 드셔보셨엉ㅅ?",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043668.1.15",
          "form": "아뇨!! 한치가 생선인가요?",
          "original_form": "아뇨!! 한치가 생선인가요?",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043668.1.16",
          "form": "네! 오징여랑 비슷한건데 말린거 구워먹을때",
          "original_form": "네! 오징여랑 비슷한건데 말린거 구워먹을때",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043668.1.17",
          "form": "오징어보다 더 맛있엉ㅅ!",
          "original_form": "오징어보다 더 맛있엉ㅅ!",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043668.1.18",
          "form": "기회되시묜 뀩 드셔뽀세요",
          "original_form": "기회되시묜 뀩 드셔뽀세요",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100043668.1.19",
          "form": "오오 들어본거같아요 !! 소스에 찍어먹나요?",
          "original_form": "오오 들어본거같아요 !! 소스에 찍어먹나요?",
          "speaker_id": "1",
          "time": "20210907 20:04"
        },
        {
          "id": "MDRW2100043668.1.20",
          "form": "네 마요네즈에 찍어먹어요!",
          "original_form": "네 마요네즈에 찍어먹어요!",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043668.1.21",
          "form": "오징어랑 진짜 비슷해요!!",
          "original_form": "오징어랑 진짜 비슷해요!!",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043668.1.22",
          "form": "오오 마요네즈 ㅠㅠㅜㅜ 오징어 마요네즈 조합 너무 좋아요 ㅠ",
          "original_form": "오오 마요네즈 ㅠㅠㅜㅜ 오징어 마요네즈 조합 너무 좋아요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043668.1.23",
          "form": "저두요 못먹은지 너무 오래돼서ㅠ먹고파요",
          "original_form": "저두요 못먹은지 너무 오래돼서ㅠ먹고파요",
