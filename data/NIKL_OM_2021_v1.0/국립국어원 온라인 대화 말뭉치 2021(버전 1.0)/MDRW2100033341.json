          "form": "1일 3젤",
          "original_form": "1일 3젤",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100033341.1.31",
          "form": "저 엄청 쳐묵했서용",
          "original_form": "저 엄청 쳐묵했서용",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100033341.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 3젤 인졍 #",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 3젤 인졍 #",
          "speaker_id": "1",
          "time": "20210907 17:40"
        },
        {
          "id": "MDRW2100033341.1.33",
          "form": "어디 젤 조아하셧나요",
          "original_form": "어디 젤 조아하셧나요",
          "speaker_id": "1",
          "time": "20210907 17:40"
        },
        {
          "id": "MDRW2100033341.1.34",
          "form": "오래되서 기억도 안나는데",
          "original_form": "오래되서 기억도 안나는데",
          "speaker_id": "2",
          "time": "20210907 17:40"
        },
        {
          "id": "MDRW2100033341.1.35",
          "form": "거기 트래비분수 쪽에서",
          "original_form": "거기 트래비분수 쪽에서",
          "speaker_id": "2",
          "time": "20210907 17:40"
        },
        {
          "id": "MDRW2100033341.1.36",
          "form": "가까웟는데",
          "original_form": "가까웟는데",
          "speaker_id": "2",
          "time": "20210907 17:40"
        },
        {
          "id": "MDRW2100033341.1.37",
          "form": "기억이 안나유",
          "original_form": "기억이 안나유",
          "speaker_id": "2",
          "time": "20210907 17:40"
        },
        {
          "id": "MDRW2100033341.1.38",
          "form": "제수준에 맞는 2유로짜리",
          "original_form": "제수준에 맞는 2유로짜리",
          "speaker_id": "2",
          "time": "20210907 17:41"
        },
        {
          "id": "MDRW2100033341.1.39",
          "form": "였나?",
          "original_form": "였나?",
          "speaker_id": "2",
          "time": "20210907 17:41"
        },
        {
          "id": "MDRW2100033341.1.40",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:41"
        },
        {
          "id": "MDRW2100033341.1.41",
          "form": "엌ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "엌ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:41"
        },
        {
          "id": "MDRW2100033341.1.42",
          "form": "저도 얼마엿는지 기억이안나넨요",
          "original_form": "저도 얼마엿는지 기억이안나넨요",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
          "id": "MDRW2100033341.1.43",
          "form": "무슨맛",
          "original_form": "무슨맛",
          "speaker_id": "1",
          "time": "20210907 17:42"
        },
        {
