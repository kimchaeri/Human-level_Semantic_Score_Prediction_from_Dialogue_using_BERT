          "original_form": "개꿀",
          "speaker_id": "2",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100033341.1.22",
          "form": "좋았겠네요",
          "original_form": "좋았겠네요",
          "speaker_id": "2",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100033341.1.23",
          "form": "젤라또 사묵고싶네요",
          "original_form": "젤라또 사묵고싶네요",
          "speaker_id": "2",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100033341.1.24",
          "form": "그졍..",
          "original_form": "그졍..",
          "speaker_id": "1",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100033341.1.25",
          "form": "저도젤라또..",
          "original_form": "저도젤라또..",
          "speaker_id": "1",
          "time": "20210907 17:38"
        },
        {
          "id": "MDRW2100033341.1.26",
