      "utterance": [
        {
          "id": "MDRW2100042555.1.1",
          "form": "공연 내내 배우들 동선이",
          "original_form": "공연 내내 배우들 동선이",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042555.1.2",
          "form": "객석 옆 통로인...",
          "original_form": "객석 옆 통로인...",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042555.1.3",
          "form": "잠깐 들러서 앉았다 가는 수준이 아니라",
          "original_form": "잠깐 들러서 앉았다 가는 수준이 아니라",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042555.1.4",
          "form": "위험햇져..",
          "original_form": "위험햇져..",
          "speaker_id": "2",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042555.1.5",
          "form": "wow.....",
          "original_form": "wow.....",
          "speaker_id": "1",
          "time": "20210906 22:49"
        },
        {
          "id": "MDRW2100042555.1.6",
          "form": "아니 관심이없어서몰랐는데",
          "original_form": "아니 관심이없어서몰랐는데",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042555.1.7",
          "form": "어메이징하네",
          "original_form": "어메이징하네",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042555.1.8",
          "form": "아니 제가 그 위대한쇼맨을",
          "original_form": "아니 제가 그 위대한쇼맨을",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042555.1.9",
          "form": "별로안좋아해서",
          "original_form": "별로안좋아해서",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042555.1.10",
          "form": "코멧석인가.. 그렇게 불렀던듯",
          "original_form": "코멧석인가.. 그렇게 불렀던듯",
          "speaker_id": "2",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042555.1.11",
          "form": "관심을 안가졌는데",
          "original_form": "관심을 안가졌는데",
          "speaker_id": "1",
          "time": "20210906 22:50"
        },
        {
          "id": "MDRW2100042555.1.12",
          "form": "저도요...",
          "original_form": "저도요...",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042555.1.13",
          "form": "정말 놀라운 구조네여",
          "original_form": "정말 놀라운 구조네여",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042555.1.14",
          "form": "그중에 넘버 하나는 좋아하지만....",
          "original_form": "그중에 넘버 하나는 좋아하지만....",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042555.1.15",
          "form": "그게노래는잘나와서",
          "original_form": "그게노래는잘나와서",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042555.1.16",
          "form": "작품 자체는 한번도 본적없는데",
          "original_form": "작품 자체는 한번도 본적없는데",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042555.1.17",
          "form": "ㅇㅇㅇㅇ...",
          "original_form": "ㅇㅇㅇㅇ...",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042555.1.18",
          "form": "노래보면서 좋아하는사람이 많다고 하던데",
          "original_form": "노래보면서 좋아하는사람이 많다고 하던데",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042555.1.19",
          "form": "내용이 빻았다고",
          "original_form": "내용이 빻았다고",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042555.1.20",
          "form": "넘버는 조아서 재수없어...",
          "original_form": "넘버는 조아서 재수없어...",
          "speaker_id": "2",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042555.1.21",
          "form": "근데심각하게 빻았더라고",
          "original_form": "근데심각하게 빻았더라고",
          "speaker_id": "1",
          "time": "20210906 22:51"
        },
        {
          "id": "MDRW2100042555.1.22",
          "form": "ㅇㅇ.....",
          "original_form": "ㅇㅇ.....",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042555.1.23",
          "form": "미화할게 없어서ㅋㅋㅋㅋㅋㅋㅋ.....",
          "original_form": "미화할게 없어서ㅋㅋㅋㅋㅋㅋㅋ.....",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042555.1.24",
          "form": "그딴 걸....",
          "original_form": "그딴 걸....",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042555.1.25",
          "form": "아 짱나",
          "original_form": "아 짱나",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042555.1.26",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042555.1.27",
          "form": "암튼 그런사람들을 서커스 돈벌이로 데려다놓고 노예처럼 학대한사람을",
          "original_form": "암튼 그런사람들을 서커스 돈벌이로 데려다놓고 노예처럼 학대한사람을",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042555.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그것도 그사람들이 용서하고 다시 데려오는구조",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그것도 그사람들이 용서하고 다시 데려오는구조",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042555.1.29",
          "form": "와웅 어느섬나라 마인드가 생각나는군요",
          "original_form": "와웅 어느섬나라 마인드가 생각나는군요",
          "speaker_id": "1",
          "time": "20210906 22:54"
        },
        {
          "id": "MDRW2100042555.1.30",
          "form": "ㄹㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㄹㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042555.1.31",
          "form": "학대의 피해자를",
          "original_form": "학대의 피해자를",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042555.1.32",
          "form": "불완전한 우리지만 우리도 잘 하는 게 있어! 이건 우리가 원한 일이야!",
          "original_form": "불완전한 우리지만 우리도 잘 하는 게 있어! 이건 우리가 원한 일이야!",
          "speaker_id": "2",
          "time": "20210906 22:55"
        },
        {
          "id": "MDRW2100042555.1.33",
          "form": "라고 미화시킬 생각을 하는 것 자체가",
          "original_form": "라고 미화시킬 생각을 하는 것 자체가",
          "speaker_id": "2",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042555.1.34",
          "form": "우우",
          "original_form": "우우",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042555.1.35",
          "form": "대체 무슨... 정신머리지?",
          "original_form": "대체 무슨... 정신머리지?",
          "speaker_id": "2",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042555.1.36",
          "form": "멍멍이소리",
          "original_form": "멍멍이소리",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042555.1.37",
          "form": "야 여기 멍멍이소리 좀 안나게 해라!!!!",
          "original_form": "야 여기 멍멍이소리 좀 안나게 해라!!!!",
          "speaker_id": "1",
          "time": "20210906 22:56"
        },
        {
          "id": "MDRW2100042555.1.38",
          "form": "근데 이멍멍이소리를 뮤지컬로 만들어서 가져올생각을",
          "original_form": "근데 이멍멍이소리를 뮤지컬로 만들어서 가져올생각을",
          "speaker_id": "1",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042555.1.39",
          "form": "왜하지",
          "original_form": "왜하지",
          "speaker_id": "1",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042555.1.40",
          "form": "자국내비판도꽤셌던걸로 기억하는데",
          "original_form": "자국내비판도꽤셌던걸로 기억하는데",
          "speaker_id": "1",
          "time": "20210906 22:57"
        },
        {
          "id": "MDRW2100042555.1.41",
          "form": "일단 연출을 해놓으면",
          "original_form": "일단 연출을 해놓으면",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042555.1.42",
          "form": "비주얼적으로 화려하니까...?",
          "original_form": "비주얼적으로 화려하니까...?",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042555.1.43",
          "form": "서커스 소재니...",
          "original_form": "서커스 소재니...",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042555.1.44",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋ진짜",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋ진짜",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042555.1.45",
          "form": "넘버도 잘 뽑혀서....",
          "original_form": "넘버도 잘 뽑혀서....",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042555.1.46",
          "form": "차라리 새로 만들어라....",
          "original_form": "차라리 새로 만들어라....",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042555.1.47",
          "form": "더 포기가 안됐나봄",
          "original_form": "더 포기가 안됐나봄",
          "speaker_id": "2",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042555.1.48",
          "form": "재연까지했던데",
          "original_form": "재연까지했던데",
          "speaker_id": "1",
          "time": "20210906 22:58"
        },
        {
          "id": "MDRW2100042555.1.49",
          "form": "어우 꼴보기싫어...",
          "original_form": "어우 꼴보기싫어...",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042555.1.50",
          "form": "하지만",
          "original_form": "하지만",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100042555.1.51",
          "form": "디아더사이드는",
          "original_form": "디아더사이드는",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042555.1.52",
          "form": "좋긴했어",
          "original_form": "좋긴했어",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042555.1.53",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042555.1.54",
          "form": "저도 노래만들었을땐",
          "original_form": "저도 노래만들었을땐",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042555.1.55",
          "form": "좋다고생각해여....",
          "original_form": "좋다고생각해여....",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042555.1.56",
          "form": "라라랜드 음감이라던가 그러던데",
          "original_form": "라라랜드 음감이라던가 그러던데",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100042555.1.57",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210906 23:02"
        },
        {
          "id": "MDRW2100042555.1.58",
