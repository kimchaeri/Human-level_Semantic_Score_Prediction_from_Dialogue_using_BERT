      "utterance": [
        {
          "id": "MDRW2100024553.1.1",
          "form": "혹시 책 좋아하시나요??",
          "original_form": "혹시 책 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210831 14:00"
        },
        {
          "id": "MDRW2100024553.1.2",
          "form": "책 요새 강제로 전공서적 엄청 읽고 있어요",
          "original_form": "책 요새 강제로 전공서적 엄청 읽고 있어요",
          "speaker_id": "2",
          "time": "20210831 15:24"
        },
        {
          "id": "MDRW2100024553.1.3",
          "form": "오늘 집에 가면 프로타고라스가 도착해잇을거에요..",
          "original_form": "오늘 집에 가면 프로타고라스가 도착해잇을거에요..",
          "speaker_id": "2",
          "time": "20210831 15:24"
        },
        {
          "id": "MDRW2100024553.1.4",
          "form": "사람 이름인가요......?",
          "original_form": "사람 이름인가요......?",
          "speaker_id": "1",
          "time": "20210831 17:54"
        },
        {
          "id": "MDRW2100024553.1.5",
          "form": "그분이 누구시죠....?",
          "original_form": "그분이 누구시죠....?",
          "speaker_id": "1",
          "time": "20210831 17:54"
        },
        {
          "id": "MDRW2100024553.1.6",
          "form": "책 제목인데 사람이름이기도 해요,..",
          "original_form": "책 제목인데 사람이름이기도 해요,..",
          "speaker_id": "2",
          "time": "20210831 18:08"
        },
        {
          "id": "MDRW2100024553.1.7",
          "form": "플라톤이 쓴 책이랍니다..",
          "original_form": "플라톤이 쓴 책이랍니다..",
          "speaker_id": "2",
          "time": "20210831 18:08"
        },
        {
          "id": "MDRW2100024553.1.8",
          "form": "헉...철학과신가요??",
          "original_form": "헉...철학과신가요??",
          "speaker_id": "1",
          "time": "20210831 21:55"
        },
        {
          "id": "MDRW2100024553.1.9",
          "form": "네...ㅋ쿠ㅜㅜㅜㅜㅜ 행복하고 괴롭습니다..",
          "original_form": "네...ㅋ쿠ㅜㅜㅜㅜㅜ 행복하고 괴롭습니다..",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100024553.1.10",
          "form": "플라톤은 재미없는거같아요",
          "original_form": "플라톤은 재미없는거같아요",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100024553.1.11",
          "form": "대단하시네요 어려울 거 같은데!!",
          "original_form": "대단하시네요 어려울 거 같은데!!",
          "speaker_id": "1",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100024553.1.12",
          "form": "서양철학을 배우는건가요??",
          "original_form": "서양철학을 배우는건가요??",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100024553.1.13",
          "form": "네 ㅋㅋㅋㅋㅋ ㅜㅜ 즐겁고 괴로운 길",
          "original_form": "네 ㅋㅋㅋㅋㅋ ㅜㅜ 즐겁고 괴로운 길",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100024553.1.14",
          "form": "철학책 중에 일반인이 읽을만한 재밌는 책도 있나요??",
          "original_form": "철학책 중에 일반인이 읽을만한 재밌는 책도 있나요??",
          "speaker_id": "1",
          "time": "20210902 13:49"
        },
        {
          "id": "MDRW2100024553.1.15",
          "form": "음....ㅡ없는거같아요 ㅋㅋㅋㅋ 아니면 아예 해설서나 개론서는 좀 재미잇을수도잇는데",
          "original_form": "음....ㅡ없는거같아요 ㅋㅋㅋㅋ 아니면 아예 해설서나 개론서는 좀 재미잇을수도잇는데",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100024553.1.16",
          "form": "관심잇는분야잇으세요?",
          "original_form": "관심잇는분야잇으세요?",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100024553.1.17",
          "form": "염세주의 이런거 궁금해욬ㅋㅋㅋㅋ",
          "original_form": "염세주의 이런거 궁금해욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100024553.1.18",
          "form": "헉 염세주읰ㅋㅋㅋㅋㅋ 본격적이신데요?",
          "original_form": "헉 염세주읰ㅋㅋㅋㅋㅋ 본격적이신데요?",
          "speaker_id": "2",
          "time": "20210903 07:44"
        },
        {
          "id": "MDRW2100024553.1.19",
          "form": "쇼펜하우어가 유명한데 글을 쉽게 쓰는지는 모르겟네요",
          "original_form": "쇼펜하우어가 유명한데 글을 쉽게 쓰는지는 모르겟네요",
          "speaker_id": "2",
          "time": "20210903 07:44"
        },
        {
          "id": "MDRW2100024553.1.20",
          "form": "쇼펜하우어 개론서 추천합니다",
          "original_form": "쇼펜하우어 개론서 추천합니다",
          "speaker_id": "2",
