        },
        {
          "id": "MDRW2100044811.1.2",
          "form": "그리고 어머니께 뺏기기",
          "original_form": "그리고 어머니께 뺏기기",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044811.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044811.1.4",
          "form": "어머 죠타로 씨두 참~",
          "original_form": "어머 죠타로 씨두 참~",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044811.1.5",
          "form": "ㅠ ㅠ ㅠ 어머니...그만두세요(ㅈㄴ울기",
          "original_form": "ㅠ ㅠ ㅠ 어머니...그만두세요(ㅈㄴ울기",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044811.1.6",
          "form": "진짜 웃겨",
          "original_form": "진짜 웃겨",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044811.1.7",
          "form": "맞아 4부의...꼬인족보도 너무 어이없음",
          "original_form": "맞아 4부의...꼬인족보도 너무 어이없음",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044811.1.8",
          "form": "맞아 아니",
          "original_form": "맞아 아니",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044811.1.9",
          "form": "죠셉 의문의 ㄱ새ㄲ였음을....",
          "original_form": "죠셉 의문의 ㄱ새ㄲ였음을....",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044811.1.10",
          "form": "난 60 먹은",
          "original_form": "난 60 먹은",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044811.1.11",
          "form": "할배가",
          "original_form": "할배가",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044811.1.12",
          "form": "아니...",
          "original_form": "아니...",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044811.1.13",
          "form": "(이마짚)",
          "original_form": "(이마짚)",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044811.1.14",
          "form": "진짜....치매인데다가 지팡이 짚고 겨우겨우 다니는데",
          "original_form": "진짜....치매인데다가 지팡이 짚고 겨우겨우 다니는데",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044811.1.15",
          "form": "ㄹㅇ 강강쥐여서 깜좍놀랫자너요",
          "original_form": "ㄹㅇ 강강쥐여서 깜좍놀랫자너요",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044811.1.16",
          "form": "아니 집에 아내가 있는디요",
          "original_form": "아니 집에 아내가 있는디요",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044811.1.17",
          "form": "진짜...........................................30년전에 그랬음 그냥 이혼이었음",
          "original_form": "진짜...........................................30년전에 그랬음 그냥 이혼이었음",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044811.1.18",
          "form": "아내가 내조 열심히 하는 와중에 두집살림 해버림;",
          "original_form": "아내가 내조 열심히 하는 와중에 두집살림 해버림;",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044811.1.19",
          "form": "매우 당황....",
          "original_form": "매우 당황....",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044811.1.20",
          "form": "열받어..",
          "original_form": "열받어..",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044811.1.21",
          "form": "솔직히 죠스케가 그거 용서해준것도....................................보살인거임",
          "original_form": "솔직히 죠스케가 그거 용서해준것도....................................보살인거임",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044811.1.22",
          "form": "나같음 얼굴도 안봤음 그저 열받기만",
          "original_form": "나같음 얼굴도 안봤음 그저 열받기만",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044811.1.23",
          "form": "근데 나는 그거 처음 봤을때",
          "original_form": "근데 나는 그거 처음 봤을때",
          "speaker_id": "1",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044811.1.24",
          "form": "응ㅇ응응",
          "original_form": "응ㅇ응응",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044811.1.25",
          "form": "어떻게 진짜 할아버지 애 아닌거 아니냐",
          "original_form": "어떻게 진짜 할아버지 애 아닌거 아니냐",
