      "utterance": [
        {
          "id": "MDRW2100033984.1.1",
          "form": "놀면 뭐하니요!",
          "original_form": "놀면 뭐하니요!",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100033984.1.2",
          "form": "호! 놀면뭐하니 지난주꺼 재미있더라구요",
          "original_form": "호! 놀면뭐하니 지난주꺼 재미있더라구요",
          "speaker_id": "1",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100033984.1.3",
          "form": "거기에 나온 노래가 계속 생각나요 ㅋㅋ",
          "original_form": "거기에 나온 노래가 계속 생각나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100033984.1.4",
          "form": "모이자~ 모오이이자~~ 우리 함께 달리자~",
          "original_form": "모이자~ 모오이이자~~ 우리 함께 달리자~",
          "speaker_id": "1",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100033984.1.5",
          "form": "아 저번주가 올림픽특집이었나요",
          "original_form": "아 저번주가 올림픽특집이었나요",
          "speaker_id": "2",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100033984.1.6",
          "form": "네!! ㅋㅋ 신유빈 선수도 나오고 김연자 선생님도 나오시고 ㅋㅋ",
          "original_form": "네!! ㅋㅋ 신유빈 선수도 나오고 김연자 선생님도 나오시고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:06"
        },
        {
          "id": "MDRW2100033984.1.7",
          "form": "ㅋㅋㅋ맞아요 오랜만에 정준하 하하 보니 좋더라구요",
          "original_form": "ㅋㅋㅋ맞아요 오랜만에 정준하 하하 보니 좋더라구요",
          "speaker_id": "2",
          "time": "20210825 18:10"
        },
        {
          "id": "MDRW2100033984.1.8",
          "form": "아 ㅋㅋㅋㅋ 맞아욬ㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ 맞아욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100033984.1.9",
          "form": "담주 예고 보셨어요?",
          "original_form": "담주 예고 보셨어요?",
          "speaker_id": "1",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100033984.1.10",
          "form": "아뇽 ㅠㅠ 담주도 이어서 나오는거죠?",
          "original_form": "아뇽 ㅠㅠ 담주도 이어서 나오는거죠?",
          "speaker_id": "2",
          "time": "20210825 18:13"
        },
        {
          "id": "MDRW2100033984.1.11",
          "form": "그건 아닌거같아요!",
          "original_form": "그건 아닌거같아요!",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100033984.1.12",
          "form": "이번편에 끝나지 않았나요??",
          "original_form": "이번편에 끝나지 않았나요??",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100033984.1.13",
          "form": "신유빈선수편?",
          "original_form": "신유빈선수편?",
          "speaker_id": "1",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100033984.1.14",
          "form": "아아 그래용? 끝내용이",
          "original_form": "아아 그래용? 끝내용이",
          "speaker_id": "2",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100033984.1.15",
          "form": "잘기억이 안나네요 ㅠ",
          "original_form": "잘기억이 안나네요 ㅠ",
          "speaker_id": "2",
          "time": "20210825 18:14"
        },
        {
          "id": "MDRW2100033984.1.16",
          "form": "저도 끝까지는 안봐서ㅜㅜ",
          "original_form": "저도 끝까지는 안봐서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100033984.1.17",
          "form": "담주에 하하랑 정준하가 또 나오긴하던데요!!",
          "original_form": "담주에 하하랑 정준하가 또 나오긴하던데요!!",
          "speaker_id": "1",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100033984.1.18",
          "form": "예고보셨어여??",
          "original_form": "예고보셨어여??",
          "speaker_id": "1",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100033984.1.19",
          "form": "네네 제2의무도라고들 하더라구요 ㅎㅎ",
          "original_form": "네네 제2의무도라고들 하더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100033984.1.20",
          "form": "완전이요 ㅋㅋㅋㅋ",
          "original_form": "완전이요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100033984.1.21",
          "form": "저 무한도전 보는줄 알았잖아요ㅠㅠㅠ",
          "original_form": "저 무한도전 보는줄 알았잖아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100033984.1.22",
          "form": "예전에 무한뉴스하던거처럼 ㅋㅋㅋ",
          "original_form": "예전에 무한뉴스하던거처럼 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:31"
        },
