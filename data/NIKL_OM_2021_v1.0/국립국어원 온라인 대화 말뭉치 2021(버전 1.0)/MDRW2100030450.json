      "utterance": [
        {
          "id": "MDRW2100030450.1.1",
          "form": "연애는 잘하고 계신가요",
          "original_form": "연애는 잘하고 계신가요",
          "speaker_id": "1",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100030450.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이런",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ이런",
          "speaker_id": "2",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100030450.1.3",
          "form": "아뇨 솔로입니다",
          "original_form": "아뇨 솔로입니다",
          "speaker_id": "2",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100030450.1.4",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210904 01:16"
        },
        {
          "id": "MDRW2100030450.1.5",
          "form": "저는 기혼입니다 ㅎㅎ",
          "original_form": "저는 기혼입니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 01:17"
        },
        {
          "id": "MDRW2100030450.1.6",
          "form": "아 ㅋㅋ 결혼생활은 좋으신가용?",
          "original_form": "아 ㅋㅋ 결혼생활은 좋으신가용?",
          "speaker_id": "2",
          "time": "20210904 01:18"
        },
        {
          "id": "MDRW2100030450.1.7",
          "form": "결혼 추천하세요?",
          "original_form": "결혼 추천하세요?",
          "speaker_id": "2",
          "time": "20210904 01:18"
        },
        {
          "id": "MDRW2100030450.1.8",
          "form": "네 저는 추천입니다",
          "original_form": "네 저는 추천입니다",
          "speaker_id": "1",
          "time": "20210904 01:19"
        },
        {
          "id": "MDRW2100030450.1.9",
          "form": "이유는요??",
          "original_form": "이유는요??",
          "speaker_id": "2",
          "time": "20210904 01:20"
        },
        {
          "id": "MDRW2100030450.1.10",
          "form": "음.. 결혼하신지 얼마되셨는지.. 실례가 안된다면 ㅎㅎ",
          "original_form": "음.. 결혼하신지 얼마되셨는지.. 실례가 안된다면 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 01:20"
        },
        {
          "id": "MDRW2100030450.1.11",
          "form": "1년정도요 ㅎㅎ",
          "original_form": "1년정도요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 01:21"
        },
        {
          "id": "MDRW2100030450.1.12",
          "form": "안정적이에요 ㅋㅋ",
          "original_form": "안정적이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:21"
        },
        {
          "id": "MDRW2100030450.1.13",
          "form": "좋으실때같은데요? 1년이면 ㅎㅎ",
          "original_form": "좋으실때같은데요? 1년이면 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 01:22"
        },
        {
          "id": "MDRW2100030450.1.14",
          "form": "네 좋죠 ㅎㅎ",
          "original_form": "네 좋죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030450.1.15",
          "form": "근데 둘이",
          "original_form": "근데 둘이",
          "speaker_id": "1",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030450.1.16",
          "form": "성향 맞는게 중요하죠ㅋ ㄱ",
          "original_form": "성향 맞는게 중요하죠ㅋ ㄱ",
          "speaker_id": "1",
          "time": "20210904 01:24"
        },
        {
          "id": "MDRW2100030450.1.17",
          "form": "그렇죠 ㅎㅎ",
          "original_form": "그렇죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 01:26"
        },
        {
          "id": "MDRW2100030450.1.18",
          "form": "연애 쉰지는 오래 되셨나요",
          "original_form": "연애 쉰지는 오래 되셨나요",
          "speaker_id": "1",
          "time": "20210904 01:35"
        },
        {
          "id": "MDRW2100030450.1.19",
          "form": "음 오래는 아닌거같은데",
          "original_form": "음 오래는 아닌거같은데",
          "speaker_id": "2",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100030450.1.20",
          "form": "오래는 얼마 만큼 일까요",
          "original_form": "오래는 얼마 만큼 일까요",
          "speaker_id": "2",
          "time": "20210904 01:37"
        },
        {
          "id": "MDRW2100030450.1.21",
          "form": "한 1년쯤!",
          "original_form": "한 1년쯤!",
          "speaker_id": "1",
          "time": "20210904 05:17"
        },
        {
          "id": "MDRW2100030450.1.22",
          "form": "오래 안됐네용",
          "original_form": "오래 안됐네용",
          "speaker_id": "2",
          "time": "20210904 12:39"
        },
        {
          "id": "MDRW2100030450.1.23",
          "form": "ㅋㅋ 그럼 뭐 곧 다시 하시겠어요",
          "original_form": "ㅋㅋ 그럼 뭐 곧 다시 하시겠어요",
