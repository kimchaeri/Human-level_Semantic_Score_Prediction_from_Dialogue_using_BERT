      "utterance": [
        {
          "id": "MDRW2100037550.1.1",
          "form": "비 엄청오죠!",
          "original_form": "비 엄청오죠!",
          "speaker_id": "1",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100037550.1.2",
          "form": "지금은 안오는데 어제 엄청 많이왔어요...",
          "original_form": "지금은 안오는데 어제 엄청 많이왔어요...",
          "speaker_id": "2",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100037550.1.3",
          "form": "지금도 비오시나요?",
          "original_form": "지금도 비오시나요?",
          "speaker_id": "2",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100037550.1.4",
          "form": "오전에는 엄청 내리더니 이제는 그쳤네요",
          "original_form": "오전에는 엄청 내리더니 이제는 그쳤네요",
          "speaker_id": "1",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100037550.1.5",
          "form": "시원해요",
          "original_form": "시원해요",
          "speaker_id": "1",
          "time": "20210901 18:54"
        },
        {
          "id": "MDRW2100037550.1.6",
          "form": "그쵸 ..아침에 너무 추워서 놀랐어요",
          "original_form": "그쵸 ..아침에 너무 추워서 놀랐어요",
          "speaker_id": "2",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100037550.1.7",
          "form": "이불 바꿔야겠더라구요 ..ㅜㅜ",
          "original_form": "이불 바꿔야겠더라구요 ..ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100037550.1.8",
          "form": "맞아요 ㅠ",
          "original_form": "맞아요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100037550.1.9",
          "form": "감기걸리기 딱좋은 날씨..",
          "original_form": "감기걸리기 딱좋은 날씨..",
          "speaker_id": "1",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100037550.1.10",
          "form": "이시국에 감기걸리면 ㅠㅍ",
          "original_form": "이시국에 감기걸리면 ㅠㅍ",
          "speaker_id": "1",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100037550.1.11",
          "form": "감기걸려서 열이라도 나면 ..진짜 ...",
          "original_form": "감기걸려서 열이라도 나면 ..진짜 ...",
          "speaker_id": "2",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100037550.1.12",
          "form": "조심해야겠어요 ㅠㅠ",
          "original_form": "조심해야겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100037550.1.13",
          "form": "이제 장마 다가고나면 일교차 심해질텐데",
          "original_form": "이제 장마 다가고나면 일교차 심해질텐데",
          "speaker_id": "1",
          "time": "20210901 19:07"
        },
        {
          "id": "MDRW2100037550.1.14",
          "form": "외투를 하나 장만해야할까봐요",
          "original_form": "외투를 하나 장만해야할까봐요",
          "speaker_id": "1",
          "time": "20210901 19:07"
        },
        {
          "id": "MDRW2100037550.1.15",
          "form": "진짜 겉옷이제 입어야할 것 같아요",
          "original_form": "진짜 겉옷이제 입어야할 것 같아요",
          "speaker_id": "2",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037550.1.16",
          "form": "아침저녁으로는 넘 춥더라구요 바람도 쌀쌀하고..",
          "original_form": "아침저녁으로는 넘 춥더라구요 바람도 쌀쌀하고..",
          "speaker_id": "2",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037550.1.17",
          "form": "아침에 추워서 일어나는게 싫더라고요 ㅋㅋㅋ",
          "original_form": "아침에 추워서 일어나는게 싫더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100037550.1.18",
          "form": "이제 전기장판의 계절이 점점..",
          "original_form": "이제 전기장판의 계절이 점점..",
          "speaker_id": "1",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100037550.1.19",
          "form": "겨울이 점점 오고있네요...",
          "original_form": "겨울이 점점 오고있네요...",
          "speaker_id": "2",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100037550.1.20",
          "form": "오늘도 추워서 아침에 깼어요 ..ㅎㅎ",
          "original_form": "오늘도 추워서 아침에 깼어요 ..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100037550.1.21",
          "form": "겨울 좋아하시나요?",
          "original_form": "겨울 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210903 08:29"
        },
        {
          "id": "MDRW2100037550.1.22",
          "form": "음 따뜻한옷입는건 좋아하는데 눈오는 날 외출하는건 별로안좋아해요...",
          "original_form": "음 따뜻한옷입는건 좋아하는데 눈오는 날 외출하는건 별로안좋아해요...",
          "speaker_id": "2",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100037550.1.23",
          "form": "미끄러워서요?",
          "original_form": "미끄러워서요?",
          "speaker_id": "1",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100037550.1.24",
          "form": "네네 .. 미끄럽고 까맣게 축축한 눈 밟는 것도 싫네요 ㅠㅠ",
          "original_form": "네네 .. 미끄럽고 까맣게 축축한 눈 밟는 것도 싫네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 19:27"
        },
        {
          "id": "MDRW2100037550.1.25",
          "form": "맞져 ㅠㅠ 눈은 보는건 예쁜데..",
          "original_form": "맞져 ㅠㅠ 눈은 보는건 예쁜데..",
          "speaker_id": "1",
          "time": "20210903 21:10"
        }
      ]
    }
