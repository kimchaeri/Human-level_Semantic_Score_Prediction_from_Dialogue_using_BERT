        },
        {
          "id": "MDRW2100046655.1.3",
          "form": "치킨 vs 피자 어떤걸 좋아하시나요?",
          "original_form": "치킨 vs 피자 어떤걸 좋아하시나요?",
          "speaker_id": "2",
          "time": "20211104 12:02"
        },
        {
          "id": "MDRW2100046655.1.4",
          "form": "피자요",
          "original_form": "피자요",
          "speaker_id": "1",
          "time": "20211104 12:03"
        },
        {
          "id": "MDRW2100046655.1.5",
          "form": "피자 좋아해요",
          "original_form": "피자 좋아해요",
          "speaker_id": "1",
          "time": "20211104 12:03"
        },
        {
          "id": "MDRW2100046655.1.6",
          "form": "헉 저두요",
          "original_form": "헉 저두요",
          "speaker_id": "2",
          "time": "20211104 12:03"
        },
        {
          "id": "MDRW2100046655.1.7",
          "form": "무조건 피자파",
          "original_form": "무조건 피자파",
          "speaker_id": "2",
          "time": "20211104 12:03"
        },
        {
          "id": "MDRW2100046655.1.8",
          "form": "배고픈가요?",
          "original_form": "배고픈가요?",
          "speaker_id": "1",
          "time": "20211104 12:05"
        },
        {
          "id": "MDRW2100046655.1.9",
          "form": "최애 피자는 뭐예요?",
          "original_form": "최애 피자는 뭐예요?",
          "speaker_id": "2",
          "time": "20211104 12:05"
        },
        {
          "id": "MDRW2100046655.1.10",
          "form": "네.. 배고파요",
          "original_form": "네.. 배고파요",
          "speaker_id": "2",
          "time": "20211104 12:05"
        },
        {
          "id": "MDRW2100046655.1.11",
          "form": "코스트코 피자죠",
          "original_form": "코스트코 피자죠",
          "speaker_id": "1",
          "time": "20211104 12:05"
        },
        {
          "id": "MDRW2100046655.1.12",
          "form": "저 코스트코 피자 못먹어봣는데",
          "original_form": "저 코스트코 피자 못먹어봣는데",
          "speaker_id": "2",
          "time": "20211104 12:06"
        },
        {
          "id": "MDRW2100046655.1.13",
          "form": "그게 그렇게 맛있어요?",
          "original_form": "그게 그렇게 맛있어요?",
          "speaker_id": "2",
          "time": "20211104 12:06"
        },
        {
          "id": "MDRW2100046655.1.14",
          "form": "사실 저도 안먹어봤어요",
          "original_form": "사실 저도 안먹어봤어요",
          "speaker_id": "1",
          "time": "20211104 12:06"
        },
        {
          "id": "MDRW2100046655.1.15",
          "form": "음? ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "음? ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 12:08"
        },
        {
          "id": "MDRW2100046655.1.16",
          "form": "전 도미노 포테이토가 최애구요",
          "original_form": "전 도미노 포테이토가 최애구요",
          "speaker_id": "2",
          "time": "20211104 12:08"
        },
        {
          "id": "MDRW2100046655.1.17",
          "form": "파파존스 올미트도 좋아해요 ㅋㅋ",
          "original_form": "파파존스 올미트도 좋아해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 12:08"
        },
        {
          "id": "MDRW2100046655.1.18",
          "form": "전 요즘 냉동피자 먹고있어요",
          "original_form": "전 요즘 냉동피자 먹고있어요",
          "speaker_id": "1",
          "time": "20211104 07:08"
        },
        {
          "id": "MDRW2100046655.1.19",
          "form": "에어프라이어에 데워먹어니 무지맛있어요",
          "original_form": "에어프라이어에 데워먹어니 무지맛있어요",
          "speaker_id": "1",
          "time": "20211104 07:08"
        },
        {
          "id": "MDRW2100046655.1.20",
          "form": "똑똑똑",
          "original_form": "똑똑똑",
          "speaker_id": "1",
          "time": "20211104 08:28"
        },
        {
          "id": "MDRW2100046655.1.21",
          "form": "오 피자도 에어프라이어에 데워먹어요?",
