      "utterance": [
        {
          "id": "MDRW2100029236.1.1",
          "form": "흰둥이씨는 최근에 뭐 사셨어요?",
          "original_form": "흰둥이씨는 최근에 뭐 사셨어요?",
          "speaker_id": "2",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029236.1.2",
          "form": "저는 노트북 샀어요!",
          "original_form": "저는 노트북 샀어요!",
          "speaker_id": "1",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029236.1.3",
          "form": "퇴사 기념으로..",
          "original_form": "퇴사 기념으로..",
          "speaker_id": "1",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029236.1.4",
          "form": "어 잘하셨어요 !",
          "original_form": "어 잘하셨어요 !",
          "speaker_id": "2",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029236.1.5",
          "form": "나 자신에서 선물 좋죠..",
          "original_form": "나 자신에서 선물 좋죠..",
          "speaker_id": "2",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029236.1.6",
          "form": "자신에게...",
          "original_form": "자신에게...",
          "speaker_id": "2",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029236.1.7",
          "form": "지금 아니면 또 미룰 것 같아서요",
          "original_form": "지금 아니면 또 미룰 것 같아서요",
          "speaker_id": "1",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029236.1.8",
          "form": "맞아요. 노트북 이런건",
          "original_form": "맞아요. 노트북 이런건",
          "speaker_id": "2",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029236.1.9",
          "form": "자잘한 건 잘 사는데",
          "original_form": "자잘한 건 잘 사는데",
          "speaker_id": "1",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029236.1.10",
          "form": "좀 큰거는ㅋㅋ",
          "original_form": "좀 큰거는ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029236.1.11",
          "form": "또 미루죠.",
          "original_form": "또 미루죠.",
          "speaker_id": "2",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029236.1.12",
          "form": "저는 미루다가 시기 놓치는게",
          "original_form": "저는 미루다가 시기 놓치는게",
          "speaker_id": "2",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029236.1.13",
          "form": "코트...",
          "original_form": "코트...",
          "speaker_id": "2",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029236.1.14",
          "form": "근데 조금 후회하는 중이예요",
          "original_form": "근데 조금 후회하는 중이예요",
          "speaker_id": "1",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029236.1.15",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029236.1.16",
          "form": "코트는 여름에 사는거죠",
          "original_form": "코트는 여름에 사는거죠",
          "speaker_id": "1",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029236.1.17",
          "form": "노트북만 사면 되는 줄 알았는데",
          "original_form": "노트북만 사면 되는 줄 알았는데",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029236.1.18",
          "form": "여름이 싸죠..",
          "original_form": "여름이 싸죠..",
          "speaker_id": "2",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029236.1.19",
          "form": "노트북 거치대가 필요하고",
          "original_form": "노트북 거치대가 필요하고",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029236.1.20",
          "form": "아이고..",
          "original_form": "아이고..",
          "speaker_id": "2",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029236.1.21",
          "form": "무선 마우스 무선 키보드",
          "original_form": "무선 마우스 무선 키보드",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029236.1.22",
          "form": "젠더도 사고",
          "original_form": "젠더도 사고",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029236.1.23",
          "form": "맥북인가요..",
          "original_form": "맥북인가요..",
          "speaker_id": "2",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029236.1.24",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029236.1.25",
          "form": "죽겠어요ㅋㅋ",
          "original_form": "죽겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:35"
        },
        {
          "id": "MDRW2100029236.1.26",
          "form": "윈도 최적화 인간인데",
          "original_form": "윈도 최적화 인간인데",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029236.1.27",
          "form": "전 그래서 핸드폰도 삼성 아님 엘지 써요..",
          "original_form": "전 그래서 핸드폰도 삼성 아님 엘지 써요..",
          "speaker_id": "2",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029236.1.28",
          "form": "신문물에 빠졌어요",
          "original_form": "신문물에 빠졌어요",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029236.1.29",
          "form": "저도 안드로이드가 좋긴하더라구요",
          "original_form": "저도 안드로이드가 좋긴하더라구요",
          "speaker_id": "1",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029236.1.30",
          "form": "저는 아직 대학생이라 안드로이드를 써도 괜찮은데",
          "original_form": "저는 아직 대학생이라 안드로이드를 써도 괜찮은데",
          "speaker_id": "2",
          "time": "20210903 00:36"
        },
        {
          "id": "MDRW2100029236.1.31",
          "form": "막 취업하면 다 갈아탄다고..",
          "original_form": "막 취업하면 다 갈아탄다고..",
          "speaker_id": "2",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029236.1.32",
          "form": "아 그런가요?ㅋㅋ",
          "original_form": "아 그런가요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029236.1.33",
          "form": "저는 반대로 알고 있었네요",
          "original_form": "저는 반대로 알고 있었네요",
          "speaker_id": "1",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029236.1.34",
          "form": "근데 개인적으로 다 아이폰쪽 상술이다 상술이라고 생각이 들어서",
          "original_form": "근데 개인적으로 다 아이폰쪽 상술이다 상술이라고 생각이 들어서",
          "speaker_id": "2",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029236.1.35",
          "form": "그건 저도 동감합니다",
          "original_form": "그건 저도 동감합니다",
          "speaker_id": "1",
          "time": "20210903 00:37"
        },
        {
          "id": "MDRW2100029236.1.36",
          "form": "국내기업 제품은 구성품 다 주잖아요..",
          "original_form": "국내기업 제품은 구성품 다 주잖아요..",
          "speaker_id": "2",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029236.1.37",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "1",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029236.1.38",
          "form": "근데 저쪽은 각자 사야해!!",
          "original_form": "근데 저쪽은 각자 사야해!!",
          "speaker_id": "2",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029236.1.39",
          "form": "한꺼번에 차라리 세트로 팔던지",
          "original_form": "한꺼번에 차라리 세트로 팔던지",
          "speaker_id": "1",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029236.1.40",
          "form": "그러니까요.",
          "original_form": "그러니까요.",
          "speaker_id": "2",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029236.1.41",
          "form": "어차피 비쌀거",
          "original_form": "어차피 비쌀거",
          "speaker_id": "2",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029236.1.42",
          "form": "아이폰 쓰다가",
          "original_form": "아이폰 쓰다가",
          "speaker_id": "1",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029236.1.43",
          "form": "노트로 넘어왔는데",
          "original_form": "노트로 넘어왔는데",
          "speaker_id": "1",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029236.1.44",
          "form": "덕질하기 최상..",
          "original_form": "덕질하기 최상..",
          "speaker_id": "1",
          "time": "20210903 00:38"
        },
        {
          "id": "MDRW2100029236.1.45",
          "form": "아이폰은 이모티콘도 안예뻐요..",
          "original_form": "아이폰은 이모티콘도 안예뻐요..",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029236.1.46",
          "form": "화면 분할도 편하고",
          "original_form": "화면 분할도 편하고",
          "speaker_id": "1",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029236.1.47",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029236.1.48",
          "form": "아이폰 화면 분할 없나요?",
          "original_form": "아이폰 화면 분할 없나요?",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029236.1.49",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029236.1.50",
          "form": "이런이런..",
          "original_form": "이런이런..",
          "speaker_id": "2",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029236.1.51",
          "form": "없어요",
          "original_form": "없어요",
          "speaker_id": "1",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029236.1.52",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:39"
        },
        {
          "id": "MDRW2100029236.1.53",
          "form": "구매할 가치가 없네요.",
          "original_form": "구매할 가치가 없네요.",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029236.1.54",
          "form": "화면 꾸미기도 어플 깔고 탈옥하고 해야함",
          "original_form": "화면 꾸미기도 어플 깔고 탈옥하고 해야함",
          "speaker_id": "1",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029236.1.55",
          "form": "지금은 좀 나아졌지만..",
          "original_form": "지금은 좀 나아졌지만..",
          "speaker_id": "1",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029236.1.56",
          "form": "충격이네요.",
          "original_form": "충격이네요.",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029236.1.57",
          "form": "근데 너무 안티 아니세요?",
          "original_form": "근데 너무 안티 아니세요?",
          "speaker_id": "1",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029236.1.58",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029236.1.59",
          "form": "계속 안드로이드 유저하겠어요",
          "original_form": "계속 안드로이드 유저하겠어요",
          "speaker_id": "2",
          "time": "20210903 00:40"
        },
        {
          "id": "MDRW2100029236.1.60",
          "form": "단호하시다",
          "original_form": "단호하시다",
          "speaker_id": "1",
          "time": "20210903 00:41"
        },
        {
          "id": "MDRW2100029236.1.61",
          "form": "언제 주무세요",
          "original_form": "언제 주무세요",
          "speaker_id": "1",
          "time": "20210903 00:41"
        },
        {
          "id": "MDRW2100029236.1.62",
          "form": "가끔 얘기해요ㅋㅋ",
          "original_form": "가끔 얘기해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029236.1.63",
          "form": "글쎄요 낼 아침 수업이라 자긴 해야하는데",
          "original_form": "글쎄요 낼 아침 수업이라 자긴 해야하는데",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029236.1.64",
          "form": "벌써 한시..",
          "original_form": "벌써 한시..",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029236.1.65",
          "form": "전혀 몰랐어요..",
          "original_form": "전혀 몰랐어요..",
          "speaker_id": "2",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029236.1.66",
          "form": "자야겠네요",
          "original_form": "자야겠네요",
          "speaker_id": "1",
          "time": "20210903 00:42"
        },
        {
          "id": "MDRW2100029236.1.67",
          "form": "그래요 ㅋㄷ",
          "original_form": "그래요 ㅋㄷ",
          "speaker_id": "2",
          "time": "20210903 00:43"
        },
