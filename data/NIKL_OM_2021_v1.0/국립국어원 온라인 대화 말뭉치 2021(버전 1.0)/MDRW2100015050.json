      "utterance": [
        {
          "id": "MDRW2100015050.1.1",
          "form": "카페 좋아하세요?",
          "original_form": "카페 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100015050.1.2",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015050.1.3",
          "form": "음료먹고 디저트 사러가요 ㅎㅎ",
          "original_form": "음료먹고 디저트 사러가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015050.1.4",
          "form": "저도 디저트 진짜 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "저도 디저트 진짜 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:46"
        },
        {
          "id": "MDRW2100015050.1.5",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015050.1.6",
          "form": "케이크 머핀",
          "original_form": "케이크 머핀",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015050.1.7",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015050.1.8",
          "form": "한때 스콘에 ㅋㅋ",
          "original_form": "한때 스콘에 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015050.1.9",
          "form": "저도 케이크 머핀 스콘 다좋아합니다",
          "original_form": "저도 케이크 머핀 스콘 다좋아합니다",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015050.1.10",
          "form": "스콘 ㅠㅠㅠ",
          "original_form": "스콘 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015050.1.11",
          "form": "은근 스콘맛집이 없어서 ㅠㅠ",
          "original_form": "은근 스콘맛집이 없어서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015050.1.12",
          "form": "없어서 못먹죠",
          "original_form": "없어서 못먹죠",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015050.1.13",
          "form": "맞아요 찾기 힘들죠",
          "original_form": "맞아요 찾기 힘들죠",
          "speaker_id": "2",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100015050.1.14",
          "form": "백화점에 가면 식품관 스콘에 치즈 블루베리 얹어 주는데 맛나요",
          "original_form": "백화점에 가면 식품관 스콘에 치즈 블루베리 얹어 주는데 맛나요",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100015050.1.15",
          "form": "듣기만 해도 맛잇을것같아요 ㅠㅠ",
          "original_form": "듣기만 해도 맛잇을것같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015050.1.16",
          "form": "네 진짜 맛나요 크림치즈 부드럽고",
          "original_form": "네 진짜 맛나요 크림치즈 부드럽고",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100015050.1.17",
          "form": "저녁 뭐 드셨어요?",
          "original_form": "저녁 뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015050.1.18",
          "form": "전 불고기요 ㅎㅎ",
          "original_form": "전 불고기요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100015050.1.19",
          "form": "디저트로 아이스크림 대기중요",
          "original_form": "디저트로 아이스크림 대기중요",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015050.1.20",
          "form": "전 로제파스타요 ㅋㅋㅋㅋ",
          "original_form": "전 로제파스타요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015050.1.21",
          "form": "사오고 있어요 ㅋㅋ",
          "original_form": "사오고 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015050.1.22",
          "form": "어머 저도 파스타 좋아해요",
          "original_form": "어머 저도 파스타 좋아해요",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015050.1.23",
          "form": "디저트 아이스크림....!! 저도 사러가야겟어요",
          "original_form": "디저트 아이스크림....!! 저도 사러가야겟어요",
          "speaker_id": "1",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015050.1.24",
          "form": "며칠전에 막었는데",
          "original_form": "며칠전에 막었는데",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015050.1.25",
          "form": "급 먹고싶네요 ㅋㅋㅋㅋ",
          "original_form": "급 먹고싶네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015050.1.26",
          "form": "어서 사러 가세요 ㅋㅋ",
          "original_form": "어서 사러 가세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015050.1.27",
          "form": "ㅋㅋㅋㅋ진짜 사러가고 있어요",
          "original_form": "ㅋㅋㅋㅋ진짜 사러가고 있어요",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015050.1.28",
          "form": "전 ㅋㅋ 남편이 헉 겁나 큰걸 사왔네요 ㅋㅋ",
          "original_form": "전 ㅋㅋ 남편이 헉 겁나 큰걸 사왔네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015050.1.29",
          "form": "뭐사실거예요 ?",
          "original_form": "뭐사실거예요 ?",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015050.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ무인아이스크림가게에서 이것저것 집어오려구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ무인아이스크림가게에서 이것저것 집어오려구요",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015050.1.31",
          "form": "오 저도 거기 잘가요",
          "original_form": "오 저도 거기 잘가요",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015050.1.32",
          "form": "옥동자 쿠앤크 콘 사와요 ㅋㅋ",
          "original_form": "옥동자 쿠앤크 콘 사와요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:12"
        },
        {
          "id": "MDRW2100015050.1.33",
          "form": "ㅋㅋㅋㅋㅋ저도 쿠앤크 좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋ저도 쿠앤크 좋아해요",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015050.1.34",
          "form": "붕어싸만코도 좋아하고",
          "original_form": "붕어싸만코도 좋아하고",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015050.1.35",
          "form": "맞아요 붕싸 ㅋㅋ",
          "original_form": "맞아요 붕싸 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015050.1.36",
          "form": "겨울엔 붕싸 ㅋㅋ",
          "original_form": "겨울엔 붕싸 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015050.1.37",
          "form": "녹차맛이랑 흑임자맛도 나왓더라구요",
          "original_form": "녹차맛이랑 흑임자맛도 나왓더라구요",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015050.1.38",
          "form": "정말요 대박",
          "original_form": "정말요 대박",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015050.1.39",
          "form": "생각보다 괜찮았어요 ㅋㅋㅋㅋ",
