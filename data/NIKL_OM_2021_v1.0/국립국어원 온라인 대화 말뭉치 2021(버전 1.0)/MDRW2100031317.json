      "utterance": [
        {
          "id": "MDRW2100031317.1.1",
          "form": "저는 주로 감자에 체다 치즈 얹어먹어요",
          "original_form": "저는 주로 감자에 체다 치즈 얹어먹어요",
          "speaker_id": "2",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100031317.1.2",
          "form": "감자요?",
          "original_form": "감자요?",
          "speaker_id": "1",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100031317.1.3",
          "form": "오 맛있나요?",
          "original_form": "오 맛있나요?",
          "speaker_id": "1",
          "time": "20210904 20:24"
        },
        {
          "id": "MDRW2100031317.1.4",
          "form": "네 삶은 감자위에",
          "original_form": "네 삶은 감자위에",
          "speaker_id": "2",
          "time": "20210904 20:24"
        },
        {
          "id": "MDRW2100031317.1.5",
          "form": "얹어서 먹으면 맛있어요",
          "original_form": "얹어서 먹으면 맛있어요",
          "speaker_id": "2",
          "time": "20210904 20:24"
        },
        {
          "id": "MDRW2100031317.1.6",
          "form": "헉 한번도 안먹어본 방식인데",
          "original_form": "헉 한번도 안먹어본 방식인데",
          "speaker_id": "1",
          "time": "20210904 20:24"
        },
        {
          "id": "MDRW2100031317.1.7",
          "form": "감자튀김위에 얹어먹는거랑 비슷한데",
          "original_form": "감자튀김위에 얹어먹는거랑 비슷한데",
          "speaker_id": "2",
          "time": "20210904 20:24"
        },
        {
          "id": "MDRW2100031317.1.8",
          "form": "아 하긴 감자튀김에도 치즈 얹어서 먹죠",
          "original_form": "아 하긴 감자튀김에도 치즈 얹어서 먹죠",
          "speaker_id": "1",
          "time": "20210904 20:24"
        },
        {
          "id": "MDRW2100031317.1.9",
          "form": "네 ㅎ 좀 더 담백해요",
          "original_form": "네 ㅎ 좀 더 담백해요",
          "speaker_id": "2",
          "time": "20210904 20:24"
        },
        {
          "id": "MDRW2100031317.1.10",
          "form": "하긴 집에서는 튀김보다 삶아서 먹는게 편하니까요",
          "original_form": "하긴 집에서는 튀김보다 삶아서 먹는게 편하니까요",
          "speaker_id": "1",
          "time": "20210904 20:25"
        },
        {
          "id": "MDRW2100031317.1.11",
          "form": "저는 한번 해먹은 이후로 중독되어서",
          "original_form": "저는 한번 해먹은 이후로 중독되어서",
          "speaker_id": "2",
          "time": "20210904 20:25"
        },
        {
          "id": "MDRW2100031317.1.12",
          "form": "중독성이 있나봐요 ㅎㅎ",
          "original_form": "중독성이 있나봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 20:25"
        },
        {
          "id": "MDRW2100031317.1.13",
          "form": "심심하면 간식으로 해먹어요 쉽기도 하고",
          "original_form": "심심하면 간식으로 해먹어요 쉽기도 하고",
          "speaker_id": "2",
          "time": "20210904 20:25"
        },
        {
          "id": "MDRW2100031317.1.14",
          "form": "저도 궁금한데요",
          "original_form": "저도 궁금한데요",
          "speaker_id": "1",
          "time": "20210904 20:25"
        },
        {
          "id": "MDRW2100031317.1.15",
          "form": "한번 해보세요 감자 삶기만 하면 되니까",
          "original_form": "한번 해보세요 감자 삶기만 하면 되니까",
          "speaker_id": "2",
