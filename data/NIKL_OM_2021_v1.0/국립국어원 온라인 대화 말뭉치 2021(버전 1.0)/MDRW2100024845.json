      "utterance": [
        {
          "id": "MDRW2100024845.1.1",
          "form": "저녁 드셨나요 ㅎㅎ",
          "original_form": "저녁 드셨나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100024845.1.2",
          "form": "넹! 방금 먹었어요 드셨어요?",
          "original_form": "넹! 방금 먹었어요 드셨어요?",
          "speaker_id": "1",
          "time": "20210831 19:37"
        },
        {
          "id": "MDRW2100024845.1.3",
          "form": "넵ㅋㅋ 시켜먹었어요",
          "original_form": "넵ㅋㅋ 시켜먹었어요",
          "speaker_id": "2",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100024845.1.4",
          "form": "어떤거 드셨어요?!",
          "original_form": "어떤거 드셨어요?!",
          "speaker_id": "1",
          "time": "20210831 23:36"
        },
        {
          "id": "MDRW2100024845.1.5",
          "form": "김치볶음밥이랑 어향가지 볶음밥이오ㅋㅋ",
          "original_form": "김치볶음밥이랑 어향가지 볶음밥이오ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100024845.1.6",
          "form": "출근해서ㅋㅋ",
          "original_form": "출근해서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 01:02"
        },
        {
          "id": "MDRW2100024845.1.7",
          "form": "아핫ㅋㅋ 아침우 잘 안드시나요?",
          "original_form": "아핫ㅋㅋ 아침우 잘 안드시나요?",
          "speaker_id": "1",
          "time": "20210901 07:29"
        },
        {
          "id": "MDRW2100024845.1.8",
          "form": "네 자꾸 거르게 되네요ㅠㅠ",
          "original_form": "네 자꾸 거르게 되네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 10:05"
        },
        {
          "id": "MDRW2100024845.1.9",
          "form": "아침 드셨어요?",
          "original_form": "아침 드셨어요?",
          "speaker_id": "2",
          "time": "20210901 10:05"
        },
        {
          "id": "MDRW2100024845.1.10",
          "form": "네! 오늘은 간단히 새싹보리즙이랑 옥수수 먹었어요!",
          "original_form": "네! 오늘은 간단히 새싹보리즙이랑 옥수수 먹었어요!",
          "speaker_id": "1",
          "time": "20210901 10:06"
        },
        {
          "id": "MDRW2100024845.1.11",
          "form": "와 완전 건강휴먼이시네요",
          "original_form": "와 완전 건강휴먼이시네요",
          "speaker_id": "2",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100024845.1.12",
          "form": "넹. 건강해지고 있습니닷!",
          "original_form": "넹. 건강해지고 있습니닷!",
          "speaker_id": "1",
          "time": "20210901 10:30"
        },
        {
          "id": "MDRW2100024845.1.13",
          "form": "바람직한 건강인이군요 ㅎㅎ",
          "original_form": "바람직한 건강인이군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100024845.1.14",
          "form": "점심은 뭐 드시나요",
          "original_form": "점심은 뭐 드시나요",
          "speaker_id": "2",
          "time": "20210901 11:08"
        },
        {
          "id": "MDRW2100024845.1.15",
          "form": "육회랑 오리용!",
          "original_form": "육회랑 오리용!",
          "speaker_id": "1",
          "time": "20210901 12:06"
        },
        {
          "id": "MDRW2100024845.1.16",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210901 12:06"
        },
        {
          "id": "MDRW2100024845.1.17",
          "form": "저는 삼계탕이오",
          "original_form": "저는 삼계탕이오",
          "speaker_id": "2",
          "time": "20210901 12:25"
        },
        {
          "id": "MDRW2100024845.1.18",
          "form": "오오 맛난거 드시네용",
          "original_form": "오오 맛난거 드시네용",
          "speaker_id": "1",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100024845.1.19",
          "form": "ㅋㅋ배부릅니다!",
          "original_form": "ㅋㅋ배부릅니다!",
          "speaker_id": "2",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100024845.1.20",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "2",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100024845.1.21",
          "form": "배부르실 것 같아용ㅋㅋ",
          "original_form": "배부르실 것 같아용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:56"
        },
        {
