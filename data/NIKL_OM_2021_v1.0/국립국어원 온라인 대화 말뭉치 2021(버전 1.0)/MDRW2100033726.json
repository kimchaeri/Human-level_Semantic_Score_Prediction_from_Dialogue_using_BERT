      "utterance": [
        {
          "id": "MDRW2100033726.1.1",
          "form": "너 뭐 살까말까 고민중이라고 하지 않았어?",
          "original_form": "너 뭐 살까말까 고민중이라고 하지 않았어?",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033726.1.2",
          "form": "음 뭐였더라",
          "original_form": "음 뭐였더라",
          "speaker_id": "2",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033726.1.3",
          "form": "혹시 내가 말한 거 기억나니?ㅠ",
          "original_form": "혹시 내가 말한 거 기억나니?ㅠ",
          "speaker_id": "2",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033726.1.4",
          "form": "ㅠㅜ나도 들었었는데 까먹었어",
          "original_form": "ㅠㅜ나도 들었었는데 까먹었어",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033726.1.5",
          "form": "그럼 내 고민을 들어볼래?",
          "original_form": "그럼 내 고민을 들어볼래?",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033726.1.6",
          "form": "아아 맞다 나 스티커를 사고싶었어",
          "original_form": "아아 맞다 나 스티커를 사고싶었어",
          "speaker_id": "2",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100033726.1.7",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033726.1.8",
          "form": "아아 어떤 스티커니?",
          "original_form": "아아 어떤 스티커니?",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033726.1.9",
          "form": "근데 요즘 스티커 값이 금값이더라",
          "original_form": "근데 요즘 스티커 값이 금값이더라",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033726.1.10",
          "form": "그냥 아무 스티커.",
          "original_form": "그냥 아무 스티커.",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033726.1.11",
          "form": "그냥 동심으로 돌아갈 때가 잇잖아.",
          "original_form": "그냥 동심으로 돌아갈 때가 잇잖아.",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033726.1.12",
          "form": "알지 그럼 사서 어디에 붙이는거야?",
          "original_form": "알지 그럼 사서 어디에 붙이는거야?",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033726.1.13",
          "form": "요즘 다꾸라고 유행하잖아",
          "original_form": "요즘 다꾸라고 유행하잖아",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033726.1.14",
          "form": "너두 그거하니?",
          "original_form": "너두 그거하니?",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033726.1.15",
          "form": "휴대폰이나 다이어리나",
          "original_form": "휴대폰이나 다이어리나",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033726.1.16",
          "form": "응 근데 다이어리 잘 안써",
          "original_form": "응 근데 다이어리 잘 안써",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033726.1.17",
          "form": "잘 안꾸며",
          "original_form": "잘 안꾸며",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033726.1.18",
          "form": "난 일기만 한바닥 써.",
          "original_form": "난 일기만 한바닥 써.",
          "speaker_id": "2",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033726.1.19",
          "form": "맞아 그런거 사서 꾸미는 사람들",
          "original_form": "맞아 그런거 사서 꾸미는 사람들",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033726.1.20",
          "form": "정말 부지런한 것 같아",
          "original_form": "정말 부지런한 것 같아",
          "speaker_id": "1",
          "time": "20210824 23:41"
        },
        {
          "id": "MDRW2100033726.1.21",
          "form": "사실 나는 전자기기에 스티커를 잘 붙이지 않아.",
          "original_form": "사실 나는 전자기기에 스티커를 잘 붙이지 않아.",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033726.1.22",
          "form": "전자기기는 그대로가 아름다워.",
          "original_form": "전자기기는 그대로가 아름다워.",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033726.1.23",
          "form": "그리고 전자기기에 붙이면 자국이남아",
          "original_form": "그리고 전자기기에 붙이면 자국이남아",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033726.1.24",
          "form": "하지만 오래된 전자기기에는 붙여",
          "original_form": "하지만 오래된 전자기기에는 붙여",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033726.1.25",
          "form": "맞아. 그리고 난 그상태 그대로가 예뻐",
          "original_form": "맞아. 그리고 난 그상태 그대로가 예뻐",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033726.1.26",
          "form": "오래돤건 가려",
          "original_form": "오래돤건 가려",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033726.1.27",
          "form": "맞아 내 노트북같이.",
          "original_form": "맞아 내 노트북같이.",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033726.1.28",
          "form": "그리고 좀 애정도를 높여줘야해",
          "original_form": "그리고 좀 애정도를 높여줘야해",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100033726.1.29",
          "form": "나는 지금 암막 커튼을 살까 고민중이야",
          "original_form": "나는 지금 암막 커튼을 살까 고민중이야",
          "speaker_id": "1",
          "time": "20210824 23:42"
        }
      ]
    }
