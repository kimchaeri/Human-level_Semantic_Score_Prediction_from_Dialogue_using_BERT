      "utterance": [
        {
          "id": "MDRW2100047316.1.1",
          "form": "어떤 음원 자주 들으세요?",
          "original_form": "어떤 음원 자주 들으세요?",
          "speaker_id": "2",
          "time": "20210826 11:20"
        },
        {
          "id": "MDRW2100047316.1.2",
          "form": "음악방송을 즐겨보긴 하는데 음원을 따로 듣게 되지는 않아요\r\n좋아하는 가수 있으세여?",
          "original_form": "음악방송을 즐겨보긴 하는데 음원을 따로 듣게 되지는 않아요\r\n좋아하는 가수 있으세여?",
          "speaker_id": "1",
          "time": "20210826 11:30"
        },
        {
          "id": "MDRW2100047316.1.3",
          "form": "너는 안예은, 장범준 노래 많이 들어요",
          "original_form": "너는 안예은, 장범준 노래 많이 들어요",
          "speaker_id": "2",
          "time": "20210826 11:32"
        },
        {
          "id": "MDRW2100047316.1.4",
          "form": "장범준 목소리 좋죠",
          "original_form": "장범준 목소리 좋죠",
          "speaker_id": "1",
          "time": "20210826 11:34"
        },
        {
          "id": "MDRW2100047316.1.5",
          "form": "아이유나 악뮤도 좋아해요",
          "original_form": "아이유나 악뮤도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 12:28"
        },
        {
          "id": "MDRW2100047316.1.6",
          "form": "네 저도요\r\n노래들을 참잘해요",
          "original_form": "네 저도요\r\n노래들을 참잘해요",
          "speaker_id": "1",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100047316.1.7",
          "form": "대중성 있으면서 흔하지 않더라구요",
          "original_form": "대중성 있으면서 흔하지 않더라구요",
          "speaker_id": "2",
          "time": "20210826 12:33"
        },
        {
          "id": "MDRW2100047316.1.8",
          "form": "아이유는 연기도 잘하고 좋아요",
          "original_form": "아이유는 연기도 잘하고 좋아요",
          "speaker_id": "1",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100047316.1.9",
          "form": "팔방미인이죠...호텔 델루나 재밌게 봤어요",
          "original_form": "팔방미인이죠...호텔 델루나 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210826 12:36"
        },
        {
          "id": "MDRW2100047316.1.10",
          "form": "저도요 그리고 나의아저씨 보셧나요\r\n연기 넘나 잘해서 놀랐어요",
          "original_form": "저도요 그리고 나의아저씨 보셧나요\r\n연기 넘나 잘해서 놀랐어요",
          "speaker_id": "1",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100047316.1.11",
          "form": "맞아요! 거기서도 감정 연기가 좋았어요",
          "original_form": "맞아요! 거기서도 감정 연기가 좋았어요",
          "speaker_id": "2",
          "time": "20210826 12:38"
        },
        {
          "id": "MDRW2100047316.1.12",
          "form": "네 그거보고 진짜 반했자나요",
          "original_form": "네 그거보고 진짜 반했자나요",
          "speaker_id": "1",
          "time": "20210826 12:39"
        },
        {
          "id": "MDRW2100047316.1.13",
          "form": "연기력이 참 좋았어요",
          "original_form": "연기력이 참 좋았어요",
          "speaker_id": "2",
          "time": "20210826 12:43"
        }
      ]
