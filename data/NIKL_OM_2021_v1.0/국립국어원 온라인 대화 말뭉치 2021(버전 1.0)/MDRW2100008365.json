      "utterance": [
        {
          "id": "MDRW2100008365.1.1",
          "form": "에어플드라마 체널은 찾았는데 ㅋㅋ",
          "original_form": "에어플드라마 체널은 찾았는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008365.1.2",
          "form": "채널만 많고 볼만한거 별로 없음",
          "original_form": "채널만 많고 볼만한거 별로 없음",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008365.1.3",
          "form": "1~200번 까지 거의 3번째 돌려 봤는데 못찾겠다 포기다",
          "original_form": "1~200번 까지 거의 3번째 돌려 봤는데 못찾겠다 포기다",
          "speaker_id": "2",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008365.1.4",
          "form": "옥탑방이나 보고 있자ㅋ",
          "original_form": "옥탑방이나 보고 있자ㅋ",
          "speaker_id": "1",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008365.1.5",
          "form": "옥탑방 문제아들 알았당",
          "original_form": "옥탑방 문제아들 알았당",
          "speaker_id": "2",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008365.1.6",
          "form": "보더콜리가 대개 발랄한 개 구나",
          "original_form": "보더콜리가 대개 발랄한 개 구나",
          "speaker_id": "1",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008365.1.7",
          "form": "양치기 개라서 그런가",
          "original_form": "양치기 개라서 그런가",
          "speaker_id": "2",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008365.1.8",
          "form": "어머 강아지 귀엽다",
          "original_form": "어머 강아지 귀엽다",
          "speaker_id": "1",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008365.1.9",
          "form": "포메 강아지가 커도 귀엽다 대신 견종질병이 있다고 하더라",
          "original_form": "포메 강아지가 커도 귀엽다 대신 견종질병이 있다고 하더라",
          "speaker_id": "2",
          "time": "20210822 15:30"
        },
        {
          "id": "MDRW2100008365.1.10",
          "form": "그래?  그래도 귀엽자나ㅋ",
          "original_form": "그래?  그래도 귀엽자나ㅋ",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100008365.1.11",
          "form": "응 견종마다 잘 걸리는 질병이 있다고 하더라",
          "original_form": "응 견종마다 잘 걸리는 질병이 있다고 하더라",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008365.1.12",
          "form": "기적의 걷기 운동법 뭐지?",
          "original_form": "기적의 걷기 운동법 뭐지?",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008365.1.13",
          "form": "저건 나도 알아야겠다",
          "original_form": "저건 나도 알아야겠다",
          "speaker_id": "2",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008365.1.14",
          "form": "나도 알아야 될듯",
          "original_form": "나도 알아야 될듯",
          "speaker_id": "1",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100008365.1.15",
          "form": "다리 근력이 강화된다는데 뭐지",
          "original_form": "다리 근력이 강화된다는데 뭐지",
          "speaker_id": "2",
          "time": "20210822 15:34"
        }
