      "utterance": [
        {
          "id": "MDRW2100028604.1.1",
          "form": "거기도 지금 비가 와?? 많이??",
          "original_form": "거기도 지금 비가 와?? 많이??",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028604.1.2",
          "form": "아뇨 여기는 어제부터 그쳤어!",
          "original_form": "아뇨 여기는 어제부터 그쳤어!",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028604.1.3",
          "form": "비구름 다 내려갔나보다",
          "original_form": "비구름 다 내려갔나보다",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028604.1.4",
          "form": "오잉 진짜 다 내려갔나보다",
          "original_form": "오잉 진짜 다 내려갔나보다",
          "speaker_id": "1",
          "time": "20210902 21:01"
        },
        {
          "id": "MDRW2100028604.1.5",
          "form": "여긴 지금 계속 비가 와",
          "original_form": "여긴 지금 계속 비가 와",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100028604.1.6",
          "form": "비 많이와ㅜ? 그 구름들 다 내려간거면 비 진짜많이 올텐데",
          "original_form": "비 많이와ㅜ? 그 구름들 다 내려간거면 비 진짜많이 올텐데",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028604.1.7",
          "form": "많이는 아닌데 ㅜㅜ 부슬부슬 오는데",
          "original_form": "많이는 아닌데 ㅜㅜ 부슬부슬 오는데",
          "speaker_id": "1",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028604.1.8",
          "form": "진짜 어제 그저께? 장난아니게왓어 여긴",
          "original_form": "진짜 어제 그저께? 장난아니게왓어 여긴",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028604.1.9",
          "form": "내일까지 쭉 계속 올 거라고 돼있어 예보는",
          "original_form": "내일까지 쭉 계속 올 거라고 돼있어 예보는",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028604.1.10",
          "form": "헉... 그럼 오늘 여기가 그렇게 많이 오겠다",
          "original_form": "헉... 그럼 오늘 여기가 그렇게 많이 오겠다",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028604.1.11",
          "form": "금요일이라 다행이지 뭐 보통 금요일은 다들 정시퇴근해",
          "original_form": "금요일이라 다행이지 뭐 보통 금요일은 다들 정시퇴근해",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028604.1.12",
          "form": "오늘은",
          "original_form": "오늘은",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028604.1.13",
          "form": "목욜인데 아 내일 말하는거엿구나",
          "original_form": "목욜인데 아 내일 말하는거엿구나",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028604.1.14",
          "form": "ㅋ ㅋ ㅋ 아니 나오늘 금요일이라는줄 알고 놀랏어",
          "original_form": "ㅋ ㅋ ㅋ 아니 나오늘 금요일이라는줄 알고 놀랏어",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028604.1.15",
          "form": "ㅋㅋㅋ 응 내일! 내일 날씨도 예보는 비온다고 돼있어서",
          "original_form": "ㅋㅋㅋ 응 내일! 내일 날씨도 예보는 비온다고 돼있어서",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028604.1.16",
          "form": "아니야아니야",
          "original_form": "아니야아니야",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028604.1.17",
          "form": "이제 오늘밤에 비 엄청올거라고 예상해봐요",
          "original_form": "이제 오늘밤에 비 엄청올거라고 예상해봐요",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028604.1.18",
          "form": "아니면... 좋은거구",
          "original_form": "아니면... 좋은거구",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028604.1.19",
          "form": "비 너무 많이 오면 싫어",
          "original_form": "비 너무 많이 오면 싫어",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028604.1.20",
          "form": "습기차고 불편해ㅜ",
          "original_form": "습기차고 불편해ㅜ",
          "speaker_id": "2",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028604.1.21",
          "form": "나도 ㅜㅜ 비 오면 일단",
          "original_form": "나도 ㅜㅜ 비 오면 일단",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028604.1.22",
          "form": "엘리베이터에서 냄새나..",
          "original_form": "엘리베이터에서 냄새나..",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028604.1.23",
          "form": "그 비릿한 쇠냄새가 싫음",
          "original_form": "그 비릿한 쇠냄새가 싫음",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028604.1.24",
          "form": "악!!!! 너무싫어",
          "original_form": "악!!!! 너무싫어",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028604.1.25",
          "form": "난 주차장냄새도",
          "original_form": "난 주차장냄새도",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028604.1.26",
          "form": "그리고 물에젖은 버스냄새도",
          "original_form": "그리고 물에젖은 버스냄새도",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028604.1.27",
          "form": "온갖 냄새들로 괴로워 습해서 맞아!!!!!",
          "original_form": "온갖 냄새들로 괴로워 습해서 맞아!!!!!",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028604.1.28",
          "form": "물에젖은 버스냄새 맡으면진짜 항상멀미함",
          "original_form": "물에젖은 버스냄새 맡으면진짜 항상멀미함",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028604.1.29",
          "form": "정말 싫어함 ㅜㅜ 마스크 써도 느껴져",
          "original_form": "정말 싫어함 ㅜㅜ 마스크 써도 느껴져",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028604.1.30",
          "form": "근데 또 그거 없애려고 에어컨틀면",
          "original_form": "근데 또 그거 없애려고 에어컨틀면",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028604.1.31",
