      "utterance": [
        {
          "id": "MDRW2100044253.1.1",
          "form": "그리고 사실 밖에 못봐따",
          "original_form": "그리고 사실 밖에 못봐따",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044253.1.2",
          "form": "마자...",
          "original_form": "마자...",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044253.1.3",
          "form": "결혼해봤자 돈도 못 벌어",
          "original_form": "결혼해봤자 돈도 못 벌어",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044253.1.4",
          "form": "내 취미를 안그래도 허락맡고 사는데 또 허락맡으면서 살라구..?",
          "original_form": "내 취미를 안그래도 허락맡고 사는데 또 허락맡으면서 살라구..?",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044253.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ 마자",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 마자",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044253.1.6",
          "form": "난 걍 혼자. 살면서",
          "original_form": "난 걍 혼자. 살면서",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044253.1.7",
          "form": "주3회 운동하고 주2회 친구랑 술마시고",
          "original_form": "주3회 운동하고 주2회 친구랑 술마시고",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044253.1.8",
          "form": "주말에 잔뜩 늦잠자고 대충 뒹굴거리는 삶 살거야",
          "original_form": "주말에 잔뜩 늦잠자고 대충 뒹굴거리는 삶 살거야",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044253.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ 완전 워너비 라이프 아니야..?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 완전 워너비 라이프 아니야..?",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044253.1.10",
          "form": "그렇게 한.. 80까지만 살 수 있음 좋겠다",
          "original_form": "그렇게 한.. 80까지만 살 수 있음 좋겠다",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044253.1.11",
          "form": "그치 ㅋㅋㅋㅋ 가끔 내키면 여행다니고",
          "original_form": "그치 ㅋㅋㅋㅋ 가끔 내키면 여행다니고",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044253.1.12",
          "form": "혼자사니까 누구 눈치보거나 챙기지 않아도 되고",
          "original_form": "혼자사니까 누구 눈치보거나 챙기지 않아도 되고",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044253.1.13",
          "form": "애인 대신에 법적 보호자 할 수 있는 놈 하나 잡아다 근처 살고(?)",
          "original_form": "애인 대신에 법적 보호자 할 수 있는 놈 하나 잡아다 근처 살고(?)",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044253.1.14",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044253.1.15",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044253.1.16",
          "form": "맞아 ㅋㅋㅋ 아니면 친구랑 이웃으로 살면 되지",
          "original_form": "맞아 ㅋㅋㅋ 아니면 친구랑 이웃으로 살면 되지",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044253.1.17",
          "form": "보호자 개념이 너무 연애랑 결혼에 집중되어있는듯...",
          "original_form": "보호자 개념이 너무 연애랑 결혼에 집중되어있는듯...",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044253.1.18",
          "form": "그치그치",
          "original_form": "그치그치",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044253.1.19",
          "form": "법적 보호자가 굳이 그래야할 필요가?",
          "original_form": "법적 보호자가 굳이 그래야할 필요가?",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044253.1.20",
          "form": "엄빠랑 나랑 연애하진 않잖아?",
          "original_form": "엄빠랑 나랑 연애하진 않잖아?",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044253.1.21",
          "form": "법적 후건인이 나랑 연애함?",
          "original_form": "법적 후건인이 나랑 연애함?",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044253.1.22",
          "form": "그건 아닌데 (절래",
          "original_form": "그건 아닌데 (절래",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044253.1.23",
          "form": "그니까 ㅋㅋㅋㅋ",
          "original_form": "그니까 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044253.1.24",
          "form": "걍 말 그대로 법적으로 내 권한을 위임해줄 사람이면 되지",
          "original_form": "걍 말 그대로 법적으로 내 권한을 위임해줄 사람이면 되지",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044253.1.25",
          "form": "굳이 그 사람이랑 연애하고 결혼해야하는 이유를 몰겟음",
          "original_form": "굳이 그 사람이랑 연애하고 결혼해야하는 이유를 몰겟음",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044253.1.26",
          "form": "머 신천지같은 사이비가 파고든다고 그런다는데",
          "original_form": "머 신천지같은 사이비가 파고든다고 그런다는데",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044253.1.27",
          "form": "법적 위임그런거 할때 느슨허면?",
          "original_form": "법적 위임그런거 할때 느슨허면?",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044253.1.28",
          "form": "솔직히 신천지는 그런거 엄격해도 파고들듯...",
          "original_form": "솔직히 신천지는 그런거 엄격해도 파고들듯...",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044253.1.29",
          "form": "그런데 사기결혼 같은거 보면 머..",
          "original_form": "그런데 사기결혼 같은거 보면 머..",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044253.1.30",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044253.1.31",
          "form": "그렇지.. 결혼해서 하는 사람도 많지 않을까 선도",
          "original_form": "그렇지.. 결혼해서 하는 사람도 많지 않을까 선도",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044253.1.32",
          "form": "ㅇㅇ 요즘 위장결혼도 많잖아",
          "original_form": "ㅇㅇ 요즘 위장결혼도 많잖아",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044253.1.33",
          "form": "아파트 받으려고...",
          "original_form": "아파트 받으려고...",
          "speaker_id": "1",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044253.1.34",
          "form": "응ㅇ 그러니까",
          "original_form": "응ㅇ 그러니까",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044253.1.35",
          "form": "그럼 결국 엇비슷한데",
          "original_form": "그럼 결국 엇비슷한데",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044253.1.36",
          "form": "막말로 친권도 박탈할 수 있는데 머 내 법적 권리 위임이야",
          "original_form": "막말로 친권도 박탈할 수 있는데 머 내 법적 권리 위임이야",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044253.1.37",
          "form": "내가 정신 없을 때만 해주면 되지",
          "original_form": "내가 정신 없을 때만 해주면 되지",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044253.1.38",
          "form": "ㅋㅋㅋ큐ㅠㅠ 아파트ㅠ",
          "original_form": "ㅋㅋㅋ큐ㅠㅠ 아파트ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044253.1.39",
