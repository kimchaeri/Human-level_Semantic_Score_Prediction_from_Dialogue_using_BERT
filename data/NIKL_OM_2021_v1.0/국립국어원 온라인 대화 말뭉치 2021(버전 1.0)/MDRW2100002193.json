      "utterance": [
        {
          "id": "MDRW2100002193.1.1",
          "form": "유튜브에 재밌는 게임영상들 많아",
          "original_form": "유튜브에 재밌는 게임영상들 많아",
          "speaker_id": "1",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002193.1.2",
          "form": "아 게임영상 보는구나 누구꺼 봐?",
          "original_form": "아 게임영상 보는구나 누구꺼 봐?",
          "speaker_id": "2",
          "time": "20210820 00:23"
        },
        {
          "id": "MDRW2100002193.1.3",
          "form": "선바라고 알려나 요즘 좀 시끄러운 일 생기긴 했는데",
          "original_form": "선바라고 알려나 요즘 좀 시끄러운 일 생기긴 했는데",
          "speaker_id": "1",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002193.1.4",
          "form": "재밌게 봤었어",
          "original_form": "재밌게 봤었어",
          "speaker_id": "1",
          "time": "20210820 00:25"
        },
        {
          "id": "MDRW2100002193.1.5",
          "form": "선바? 나는 잘 모르겠다 사실 게임 영상은 잘 안봐서 ㅠㅠ",
          "original_form": "선바? 나는 잘 모르겠다 사실 게임 영상은 잘 안봐서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 00:26"
        },
        {
          "id": "MDRW2100002193.1.6",
          "form": "그렇구낭 너는 누구봐?",
          "original_form": "그렇구낭 너는 누구봐?",
          "speaker_id": "1",
          "time": "20210820 00:26"
        },
        {
          "id": "MDRW2100002193.1.7",
          "form": "나는 요즘 그거 재밌더라 터키즈라고",
          "original_form": "나는 요즘 그거 재밌더라 터키즈라고",
          "speaker_id": "2",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002193.1.8",
          "form": "이용진 나와서 막 인터뷰하는거있는데",
          "original_form": "이용진 나와서 막 인터뷰하는거있는데",
          "speaker_id": "2",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002193.1.9",
          "form": "텐션이 저세상이야 ㅋㅋㅋ",
          "original_form": "텐션이 저세상이야 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002193.1.10",
          "form": "오 이용진",
          "original_form": "오 이용진",
          "speaker_id": "1",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002193.1.11",
          "form": "난 그거 피식대학",
          "original_form": "난 그거 피식대학",
          "speaker_id": "1",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002193.1.12",
          "form": "아 피식대학도 재밌지 ㅋㅋㅋ",
          "original_form": "아 피식대학도 재밌지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002193.1.13",
          "form": "나는 그거도봐 백종원",
          "original_form": "나는 그거도봐 백종원",
          "speaker_id": "2",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002193.1.14",
          "form": "백종원 유튜브도 해?",
          "original_form": "백종원 유튜브도 해?",
          "speaker_id": "1",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002193.1.15",
          "form": "난 백종원아저씨 넷플릭스에 삼겹살랩소디? 라고 있는데 그거 봐",
          "original_form": "난 백종원아저씨 넷플릭스에 삼겹살랩소디? 라고 있는데 그거 봐",
          "speaker_id": "1",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002193.1.16",
          "form": "웅ㅋㅋㅋ 요리 알려주는데 되게 쉽게 잘알려줌",
          "original_form": "웅ㅋㅋㅋ 요리 알려주는데 되게 쉽게 잘알려줌",
          "speaker_id": "2",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002193.1.17",
          "form": "무려 삼겹살 다큐야",
          "original_form": "무려 삼겹살 다큐야",
          "speaker_id": "1",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002193.1.18",
          "form": "아진짜? 나도 챙겨봐볼게 재밌겠다 ㅋㅋㅋ",
          "original_form": "아진짜? 나도 챙겨봐볼게 재밌겠다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002193.1.19",
          "form": "나는 최근에 닭도리탕 만드는거 봤는데 신기하게 쉽게 하더라",
          "original_form": "나는 최근에 닭도리탕 만드는거 봤는데 신기하게 쉽게 하더라",
          "speaker_id": "2",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002193.1.20",
          "form": "백종원아저씨는 뭐든 쉽게해..",
          "original_form": "백종원아저씨는 뭐든 쉽게해..",
