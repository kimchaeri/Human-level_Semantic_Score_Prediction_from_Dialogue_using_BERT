      "utterance": [
        {
          "id": "MDRW2100013229.1.1",
          "form": "ㅋㅋㅋㅋ 사실 넘 예전이라",
          "original_form": "ㅋㅋㅋㅋ 사실 넘 예전이라",
          "speaker_id": "1",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100013229.1.2",
          "form": "향수도 잘없네요 큰감흥도없고",
          "original_form": "향수도 잘없네요 큰감흥도없고",
          "speaker_id": "1",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100013229.1.3",
          "form": "저는 요즘 그렇게 그립더라고요",
          "original_form": "저는 요즘 그렇게 그립더라고요",
          "speaker_id": "2",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100013229.1.4",
          "form": "대학생활이",
          "original_form": "대학생활이",
          "speaker_id": "2",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100013229.1.5",
          "form": "친구들이랑 아직 연락해요?",
          "original_form": "친구들이랑 아직 연락해요?",
          "speaker_id": "2",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100013229.1.6",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100013229.1.7",
          "form": "거의다안해요ㅋㅋㅋㅋㅋ",
          "original_form": "거의다안해요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100013229.1.8",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210825 13:48"
        },
        {
          "id": "MDRW2100013229.1.9",
          "form": "저도 딱 한명",
          "original_form": "저도 딱 한명",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013229.1.10",
          "form": "아 후배도 한명",
          "original_form": "아 후배도 한명",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013229.1.11",
          "form": "대학친구들이랑은 졸업하고 연락 뜸해지더라고요",
          "original_form": "대학친구들이랑은 졸업하고 연락 뜸해지더라고요",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013229.1.12",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013229.1.13",
          "form": "다시 대학으로가면",
          "original_form": "다시 대학으로가면",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013229.1.14",
          "form": "좀더 열정적으로 살것같긴하네요",
          "original_form": "좀더 열정적으로 살것같긴하네요",
          "speaker_id": "1",
          "time": "20210825 14:17"
        },
        {
          "id": "MDRW2100013229.1.15",
          "form": "네 더 치열하게ㅋㅋ",
          "original_form": "네 더 치열하게ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100013229.1.16",
          "form": "학생때 학업스트레스 많이 받으셨어요?",
          "original_form": "학생때 학업스트레스 많이 받으셨어요?",
          "speaker_id": "2",
          "time": "20210825 15:16"
        },
        {
          "id": "MDRW2100013229.1.17",
          "form": "아니요ㅋㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013229.1.18",
          "form": "공부를 안했죠",
          "original_form": "공부를 안했죠",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013229.1.19",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210825 15:24"
        },
        {
          "id": "MDRW2100013229.1.20",
          "form": "저는 1학년 2학기때 학고 받아서",
          "original_form": "저는 1학년 2학기때 학고 받아서",
          "speaker_id": "2",
          "time": "20210825 15:26"
        },
        {
          "id": "MDRW2100013229.1.21",
          "form": "군대다녀와서부터",
          "original_form": "군대다녀와서부터",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013229.1.22",
          "form": "학점 복구하느라 고생했어요",
          "original_form": "학점 복구하느라 고생했어요",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013229.1.23",
          "form": "동아리 활동은 하셨어요?",
          "original_form": "동아리 활동은 하셨어요?",
          "speaker_id": "2",
          "time": "20210825 15:27"
        },
        {
          "id": "MDRW2100013229.1.24",
          "form": "아 남자분이시군요? 군대라니",
          "original_form": "아 남자분이시군요? 군대라니",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013229.1.25",
          "form": "학고 ㅎㅎㅎㅎ",
          "original_form": "학고 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013229.1.26",
          "form": "자유로운영혼이셨네요",
          "original_form": "자유로운영혼이셨네요",
          "speaker_id": "1",
          "time": "20210825 15:34"
        },
        {
          "id": "MDRW2100013229.1.27",
          "form": "그때 뭘 잘 몰랐어요",
          "original_form": "그때 뭘 잘 몰랐어요",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013229.1.28",
          "form": "그래도 되는줄 알았고",
          "original_form": "그래도 되는줄 알았고",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013229.1.29",
          "form": "학고 한번에 정신 차렸죠",
          "original_form": "학고 한번에 정신 차렸죠",
          "speaker_id": "2",
          "time": "20210825 15:35"
        },
        {
          "id": "MDRW2100013229.1.30",
          "form": "그래도 공대는 취업이 괜찮아서좋겠어요",
          "original_form": "그래도 공대는 취업이 괜찮아서좋겠어요",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013229.1.31",
          "form": "저는 근데",
          "original_form": "저는 근데",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013229.1.32",
          "form": "다시돌아가도",
          "original_form": "다시돌아가도",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013229.1.33",
          "form": "공대못갈거같아요ㅋㅋㅋㅋ",
          "original_form": "공대못갈거같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013229.1.34",
          "form": "수학때문에요?",
          "original_form": "수학때문에요?",
          "speaker_id": "2",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013229.1.35",
          "form": "정답이죠",
          "original_form": "정답이죠",
          "speaker_id": "1",
          "time": "20210825 15:48"
        },
        {
          "id": "MDRW2100013229.1.36",
          "form": "아주",
          "original_form": "아주",
          "speaker_id": "1",
          "time": "20210825 15:48"
        }
      ]
