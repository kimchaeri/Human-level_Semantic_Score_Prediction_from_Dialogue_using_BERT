      "utterance": [
        {
          "id": "MDRW2100022529.1.1",
          "form": "책 자주 읽으세요?",
          "original_form": "책 자주 읽으세요?",
          "speaker_id": "2",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100022529.1.2",
          "form": "저는... 아니요...",
          "original_form": "저는... 아니요...",
          "speaker_id": "1",
          "time": "20210829 15:26"
        },
        {
          "id": "MDRW2100022529.1.3",
          "form": "최근 잘 안읽게되었어요",
          "original_form": "최근 잘 안읽게되었어요",
          "speaker_id": "1",
          "time": "20210829 15:26"
        },
        {
          "id": "MDRW2100022529.1.4",
          "form": "책 어떤거 자주 보세요?",
          "original_form": "책 어떤거 자주 보세요?",
          "speaker_id": "1",
          "time": "20210829 15:26"
        },
        {
          "id": "MDRW2100022529.1.5",
          "form": "저도 책을 잘 안읽어서 자주 보는 책이랄게 없어요 ㅋㅋ",
          "original_form": "저도 책을 잘 안읽어서 자주 보는 책이랄게 없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100022529.1.6",
          "form": "그래도 요즘 소설 하나 읽고 있어요",
          "original_form": "그래도 요즘 소설 하나 읽고 있어요",
          "speaker_id": "2",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100022529.1.7",
          "form": "어떤 책인가요?",
          "original_form": "어떤 책인가요?",
          "speaker_id": "1",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100022529.1.8",
          "form": "별사탕 내리는 밤 이라는 소설책이에요",
          "original_form": "별사탕 내리는 밤 이라는 소설책이에요",
          "speaker_id": "2",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100022529.1.9",
          "form": "에쿠니 가오리 작가분 책이네요!",
          "original_form": "에쿠니 가오리 작가분 책이네요!",
          "speaker_id": "1",
          "time": "20210829 15:36"
        },
        {
          "id": "MDRW2100022529.1.10",
          "form": "예전에 좋아하던 작가분이예요",
          "original_form": "예전에 좋아하던 작가분이예요",
          "speaker_id": "1",
          "time": "20210829 15:37"
        },
        {
          "id": "MDRW2100022529.1.11",
          "form": "와 정말요?",
          "original_form": "와 정말요?",
          "speaker_id": "2",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100022529.1.12",
          "form": "그럼 이 책도 읽으셨어요?",
          "original_form": "그럼 이 책도 읽으셨어요?",
          "speaker_id": "2",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100022529.1.13",
          "form": "앗 아니요 예전에 단편집이랑 반짝반짝빛나는 읽었어요",
          "original_form": "앗 아니요 예전에 단편집이랑 반짝반짝빛나는 읽었어요",
          "speaker_id": "1",
          "time": "20210829 15:45"
        },
        {
          "id": "MDRW2100022529.1.14",
          "form": "책안본지 몇년되어서.... 그책은 모르겠어요",
          "original_form": "책안본지 몇년되어서.... 그책은 모르겠어요",
          "speaker_id": "1",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100022529.1.15",
          "form": "어때요? 요즘도 문체가 참 유려하신가요?",
          "original_form": "어때요? 요즘도 문체가 참 유려하신가요?",
          "speaker_id": "1",
          "time": "20210829 15:46"
        },
        {
          "id": "MDRW2100022529.1.16",
          "form": "제가 문체 그런걸 잘 몰라서요 ㅎㅎ",
          "original_form": "제가 문체 그런걸 잘 몰라서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 15:49"
        },
        {
          "id": "MDRW2100022529.1.17",
          "form": "아직 초반부분만 읽었는데 재밌더라구요 지루하지도 않고",
          "original_form": "아직 초반부분만 읽었는데 재밌더라구요 지루하지도 않고",
          "speaker_id": "2",
          "time": "20210829 15:50"
        },
        {
          "id": "MDRW2100022529.1.18",
          "form": "술술 읽히는 책이 제일 좋아요",
          "original_form": "술술 읽히는 책이 제일 좋아요",
          "speaker_id": "1",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100022529.1.19",
          "form": "읽다가 막히고 생각 많이 하게 하는 책도 읽어야하는데... 힘드네요",
          "original_form": "읽다가 막히고 생각 많이 하게 하는 책도 읽어야하는데... 힘드네요",
          "speaker_id": "1",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100022529.1.20",
          "form": "그래요? 재밌는 책만 읽으면 안되는걸까요 ㅎㅎ",
          "original_form": "그래요? 재밌는 책만 읽으면 안되는걸까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022529.1.21",
          "form": "그렇네요 재밌는책만 읽어도 좋죠",
          "original_form": "그렇네요 재밌는책만 읽어도 좋죠",
          "speaker_id": "1",
          "time": "20210829 16:08"
        },
        {
          "id": "MDRW2100022529.1.22",
          "form": "책을 이렇게 읽어야 한다기보다",
          "original_form": "책을 이렇게 읽어야 한다기보다",
          "speaker_id": "1",
          "time": "20210829 16:09"
        },
        {
          "id": "MDRW2100022529.1.23",
          "form": "그냥 제가 어휘력이나 이런게 떨어지는거 같아서요",
          "original_form": "그냥 제가 어휘력이나 이런게 떨어지는거 같아서요",
          "speaker_id": "1",
          "time": "20210829 16:09"
        },
        {
          "id": "MDRW2100022529.1.24",
          "form": "아~ 그런 이유에서군요",
          "original_form": "아~ 그런 이유에서군요",
          "speaker_id": "2",
          "time": "20210829 16:11"
        },
        {
          "id": "MDRW2100022529.1.25",
          "form": "저도 그런거 느껴서 되도록 한국작가 책을 읽으려하기도 해요",
          "original_form": "저도 그런거 느껴서 되도록 한국작가 책을 읽으려하기도 해요",
          "speaker_id": "2",
          "time": "20210829 16:12"
        },
        {
          "id": "MDRW2100022529.1.26",
          "form": "근데 자꾸 일본이나 외국작가 책을 읽게되더라구요",
          "original_form": "근데 자꾸 일본이나 외국작가 책을 읽게되더라구요",
          "speaker_id": "2",
          "time": "20210829 16:12"
        },
        {
          "id": "MDRW2100022529.1.27",
          "form": "히가시노게이고 등 말잉에요",
          "original_form": "히가시노게이고 등 말잉에요",
          "speaker_id": "2",
          "time": "20210829 16:13"
        },
        {
          "id": "MDRW2100022529.1.28",
          "form": "히가시노게이고 책이 저는 일단 재밌어서ㅋㅋㅋ잘읽게되었던거 같아요",
          "original_form": "히가시노게이고 책이 저는 일단 재밌어서ㅋㅋㅋ잘읽게되었던거 같아요",
          "speaker_id": "1",
          "time": "20210829 16:14"
        }
      ]
