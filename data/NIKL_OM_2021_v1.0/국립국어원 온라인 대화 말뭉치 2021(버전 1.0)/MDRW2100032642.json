      "utterance": [
        {
          "id": "MDRW2100032642.1.1",
          "form": "재난지원금이곧나온다는데",
          "original_form": "재난지원금이곧나온다는데",
          "speaker_id": "1",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100032642.1.2",
          "form": "사고싶은물건있으세요?",
          "original_form": "사고싶은물건있으세요?",
          "speaker_id": "1",
          "time": "20210905 21:48"
        },
        {
          "id": "MDRW2100032642.1.3",
          "form": "전자레인지 사려구요 ㅋㅋㅋ",
          "original_form": "전자레인지 사려구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100032642.1.4",
          "form": "최근에 쇼핑한게 어떤거에요?",
          "original_form": "최근에 쇼핑한게 어떤거에요?",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100032642.1.5",
          "form": "저느은ㅎㅎㅎ",
          "original_form": "저느은ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032642.1.6",
          "form": "가디건을하나샀어요",
          "original_form": "가디건을하나샀어요",
          "speaker_id": "1",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032642.1.7",
          "form": "오 가을옷 쇼핑하셨구나",
          "original_form": "오 가을옷 쇼핑하셨구나",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032642.1.8",
          "form": "저도 갑자기 날이 추워져서 겉옷 하나 사야겠다고 생각중이었어요ㅎ",
          "original_form": "저도 갑자기 날이 추워져서 겉옷 하나 사야겠다고 생각중이었어요ㅎ",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100032642.1.9",
          "form": "당근마켓 해보신적 있으세요?",
          "original_form": "당근마켓 해보신적 있으세요?",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100032642.1.10",
          "form": "앗 당근",
          "original_form": "앗 당근",
          "speaker_id": "1",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100032642.1.11",
          "form": "한번해봤는데..한번올려봤는데",
          "original_form": "한번해봤는데..한번올려봤는데",
          "speaker_id": "1",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100032642.1.12",
          "form": "아무도안사서 주륵ㅠ",
          "original_form": "아무도안사서 주륵ㅠ",
          "speaker_id": "1",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100032642.1.13",
          "form": "아고ㅠㅠ 저도 지금..올려놓은게 안팔리고있습니다 ㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "아고ㅠㅠ 저도 지금..올려놓은게 안팔리고있습니다 ㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032642.1.14",
          "form": "근데 또 2~3개월 지나서 팔리기도해가지구",
          "original_form": "근데 또 2~3개월 지나서 팔리기도해가지구",
          "speaker_id": "2",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032642.1.15",
          "form": "그냥 존중하면서 버티고있습니다 ㅋㅋ",
          "original_form": "그냥 존중하면서 버티고있습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:02"
        },
        {
          "id": "MDRW2100032642.1.16",
          "form": "앗진짜요ㅋㅋㅋ?",
          "original_form": "앗진짜요ㅋㅋㅋ?",
          "speaker_id": "1",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100032642.1.17",
          "form": "다시깔아봐야겠어요",
          "original_form": "다시깔아봐야겠어요",
          "speaker_id": "1",
          "time": "20210905 22:03"
        },
        {
          "id": "MDRW2100032642.1.18",
          "form": "쓰는 물건 중에 계속 재구매하는 제품 있나요?",
          "original_form": "쓰는 물건 중에 계속 재구매하는 제품 있나요?",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032642.1.19",
          "form": "인생템이다 이런거 ㅋㅋ",
          "original_form": "인생템이다 이런거 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032642.1.20",
          "form": "음 ㅎㅎㅎ 리무버중에",
          "original_form": "음 ㅎㅎㅎ 리무버중에",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032642.1.21",
          "form": "립앤아이 리무버중에",
          "original_form": "립앤아이 리무버중에",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032642.1.22",
          "form": "비페스타 립앤아이 리무버라고 좋아요",
          "original_form": "비페스타 립앤아이 리무버라고 좋아요",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032642.1.23",
          "form": "아 올리브영에서 본 거 같아요!",
          "original_form": "아 올리브영에서 본 거 같아요!",
          "speaker_id": "2",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100032642.1.24",
          "form": "눈에 들어가도 안따갑나요?",
          "original_form": "눈에 들어가도 안따갑나요?",
          "speaker_id": "2",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100032642.1.25",
          "form": "잘지워져요",
          "original_form": "잘지워져요",
          "speaker_id": "1",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100032642.1.26",
          "form": "맞아요 거기서팔아요!",
          "original_form": "맞아요 거기서팔아요!",
          "speaker_id": "1",
          "time": "20210905 22:09"
        },
        {
          "id": "MDRW2100032642.1.27",
          "form": "재난지원금 나오면 어디에 쓰실거에요?ㅎ",
          "original_form": "재난지원금 나오면 어디에 쓰실거에요?ㅎ",
          "speaker_id": "2",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032642.1.28",
          "form": "저는ㅎㅎ샐러드나프로틴바먹고싶어요",
          "original_form": "저는ㅎㅎ샐러드나프로틴바먹고싶어요",
          "speaker_id": "1",
          "time": "20210905 22:15"
        }
      ]
    }
  ]
