      "utterance": [
        {
          "id": "MDRW2100010151.1.1",
          "form": "그건 챙겨먹기 귀찮아서 ㅋ",
          "original_form": "그건 챙겨먹기 귀찮아서 ㅋ",
          "speaker_id": "2",
          "time": "20210823 14:52"
        },
        {
          "id": "MDRW2100010151.1.2",
          "form": "드시는거있으세요?",
          "original_form": "드시는거있으세요?",
          "speaker_id": "2",
          "time": "20210823 14:52"
        },
        {
          "id": "MDRW2100010151.1.3",
          "form": "비타민이랑 유산균 정도 챙겨 먹어요",
          "original_form": "비타민이랑 유산균 정도 챙겨 먹어요",
          "speaker_id": "1",
          "time": "20210823 14:53"
        },
        {
          "id": "MDRW2100010151.1.4",
          "form": "요즘 다들 여러개 드시던데",
          "original_form": "요즘 다들 여러개 드시던데",
          "speaker_id": "2",
          "time": "20210823 14:53"
        },
        {
          "id": "MDRW2100010151.1.5",
          "form": "유산균은 확실히 질염 발생도 줄여주고 좋더라고요",
          "original_form": "유산균은 확실히 질염 발생도 줄여주고 좋더라고요",
          "speaker_id": "1",
          "time": "20210823 14:53"
        },
        {
          "id": "MDRW2100010151.1.6",
          "form": "유산균 어디꺼드세요?",
          "original_form": "유산균 어디꺼드세요?",
          "speaker_id": "2",
          "time": "20210823 14:53"
        },
        {
          "id": "MDRW2100010151.1.7",
          "form": "질유산균 드시나요?",
          "original_form": "질유산균 드시나요?",
          "speaker_id": "2",
          "time": "20210823 14:54"
        },
        {
          "id": "MDRW2100010151.1.8",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210823 14:54"
        },
        {
          "id": "MDRW2100010151.1.9",
          "form": "이노플로라? 그거밖에 몰라서",
          "original_form": "이노플로라? 그거밖에 몰라서",
          "speaker_id": "2",
          "time": "20210823 14:54"
        },
        {
          "id": "MDRW2100010151.1.10",
          "form": "프로스랩에서 주문해서 먹었어요",
          "original_form": "프로스랩에서 주문해서 먹었어요",
          "speaker_id": "1",
          "time": "20210823 14:54"
        },
        {
          "id": "MDRW2100010151.1.11",
          "form": "가루 형태인데 시지 않은 비타민 가루 먹는 거 같아서 먹을만 했어요",
          "original_form": "가루 형태인데 시지 않은 비타민 가루 먹는 거 같아서 먹을만 했어요",
          "speaker_id": "1",
          "time": "20210823 14:55"
        },
        {
          "id": "MDRW2100010151.1.12",
          "form": "첨들어보네요 함 찾아볼게요ㅎ",
          "original_form": "첨들어보네요 함 찾아볼게요ㅎ",
          "speaker_id": "2",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100010151.1.13",
          "form": "질건강에 좋음 먹어줘야겠네요",
          "original_form": "질건강에 좋음 먹어줘야겠네요",
          "speaker_id": "2",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100010151.1.14",
          "form": "저는 유산균 먹고 질염 사라졌어요 추천드려요",
          "original_form": "저는 유산균 먹고 질염 사라졌어요 추천드려요",
          "speaker_id": "1",
          "time": "20210823 14:57"
        },
        {
          "id": "MDRW2100010151.1.15",
          "form": "건강도 미리미리 챙겨야되는데 싶지않아요ㅠㅠ",
          "original_form": "건강도 미리미리 챙겨야되는데 싶지않아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 14:58"
        },
        {
          "id": "MDRW2100010151.1.16",
          "form": "이거저것 챙기는게 꽤 귀찮죠",
          "original_form": "이거저것 챙기는게 꽤 귀찮죠",
          "speaker_id": "1",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010151.1.17",
          "form": "비타민 유산균 오메가정도는 다먹더라구요 기본인가봐요",
          "original_form": "비타민 유산균 오메가정도는 다먹더라구요 기본인가봐요",
          "speaker_id": "2",
          "time": "20210823 15:00"
        },
        {
          "id": "MDRW2100010151.1.18",
          "form": "저도 점점 늘리려고요... 한번에는 도저히 무리였어요 비타민이나 유산균 먼저 챙겨보세요",
          "original_form": "저도 점점 늘리려고요... 한번에는 도저히 무리였어요 비타민이나 유산균 먼저 챙겨보세요",
          "speaker_id": "1",
          "time": "20210823 15:02"
        }
      ]
