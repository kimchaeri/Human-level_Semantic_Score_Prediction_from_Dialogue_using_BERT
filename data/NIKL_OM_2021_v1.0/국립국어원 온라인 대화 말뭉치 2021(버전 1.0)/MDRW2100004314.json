      "utterance": [
        {
          "id": "MDRW2100004314.1.1",
          "form": "혹시 엔시티 좋아하니?",
          "original_form": "혹시 엔시티 좋아하니?",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004314.1.2",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004314.1.3",
          "form": "너도?",
          "original_form": "너도?",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004314.1.4",
          "form": "어제 nct127 영상 정말 멋지더라",
          "original_form": "어제 nct127 영상 정말 멋지더라",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004314.1.5",
          "form": "나도 좋아해",
          "original_form": "나도 좋아해",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004314.1.6",
          "form": "맞아 사실 난 공대생이라 살짝 ptsd오긴했는데",
          "original_form": "맞아 사실 난 공대생이라 살짝 ptsd오긴했는데",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004314.1.7",
          "form": "애들 얼굴이 짱이었어",
          "original_form": "애들 얼굴이 짱이었어",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004314.1.8",
          "form": "난 해찬이가 최애인데",
          "original_form": "난 해찬이가 최애인데",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004314.1.9",
          "form": "너무 예뻐서",
          "original_form": "너무 예뻐서",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004314.1.10",
          "form": "심장마비로 갈뻔했어",
          "original_form": "심장마비로 갈뻔했어",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004314.1.11",
          "form": "낮과 밤으로 나눈게 인상적이더라",
          "original_form": "낮과 밤으로 나눈게 인상적이더라",
          "speaker_id": "1",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004314.1.12",
          "form": "맞아 컨셉이 확확 바뀌어서",
          "original_form": "맞아 컨셉이 확확 바뀌어서",
          "speaker_id": "2",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004314.1.13",
          "form": "드림 헬퓨도 좋았는데",
          "original_form": "드림 헬퓨도 좋았는데",
          "speaker_id": "2",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004314.1.14",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004314.1.15",
          "form": "이번에 노래가 어떨지 궁금해",
          "original_form": "이번에 노래가 어떨지 궁금해",
          "speaker_id": "1",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004314.1.16",
          "form": "이번 일이칠도 넘 좋아ㅠㅠ",
          "original_form": "이번 일이칠도 넘 좋아ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004314.1.17",
          "form": "난 최애가 동혁이라 계속 돈쓰고 있어ㅋㅋㅋ",
          "original_form": "난 최애가 동혁이라 계속 돈쓰고 있어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004314.1.18",
          "form": "동혁이 오늘 뜬 사진 귀엽더라",
          "original_form": "동혁이 오늘 뜬 사진 귀엽더라",
          "speaker_id": "1",
          "time": "20210821 16:06"
        },
        {
          "id": "MDRW2100004314.1.19",
          "form": "인스타그램 이용한것도 멋져",
          "original_form": "인스타그램 이용한것도 멋져",
          "speaker_id": "1",
          "time": "20210821 16:07"
        },
        {
          "id": "MDRW2100004314.1.20",
          "form": "맞아근데 인스타는 컨셉에서 사용이 끝났으면 좋겠어",
          "original_form": "맞아근데 인스타는 컨셉에서 사용이 끝났으면 좋겠어",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004314.1.21",
          "form": "난... 우리 동혁이 인스타 안했으면 좋겠어",
          "original_form": "난... 우리 동혁이 인스타 안했으면 좋겠어",
          "speaker_id": "2",
          "time": "20210821 16:08"
        },
        {
          "id": "MDRW2100004314.1.22",
          "form": "쟈니랑 동혁이랑 같이 사진올라왔다",
          "original_form": "쟈니랑 동혁이랑 같이 사진올라왔다",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004314.1.23",
          "form": "대학생 컨셉 귀여워",
          "original_form": "대학생 컨셉 귀여워",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004314.1.24",
          "form": "헉 진짜??",
          "original_form": "헉 진짜??",
          "speaker_id": "2",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004314.1.25",
          "form": "응 서로 사진찍어주는데",
          "original_form": "응 서로 사진찍어주는데",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004314.1.26",
          "form": "백팩을 앞으로 매고있어",
          "original_form": "백팩을 앞으로 매고있어",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004314.1.27",
          "form": "대중교통 예절 잘지키는 아이돌이네",
          "original_form": "대중교통 예절 잘지키는 아이돌이네",
          "speaker_id": "1",
          "time": "20210821 16:09"
        },
        {
          "id": "MDRW2100004314.1.28",
          "form": "아ㅋㅋㄱㄱ백팩 앞으로 국룰이지",
          "original_form": "아ㅋㅋㄱㄱ백팩 앞으로 국룰이지",
          "speaker_id": "2",
          "time": "20210821 16:14"
        },
        {
          "id": "MDRW2100004314.1.29",
          "form": "나는 이번에 티저에서 대학 생활을 이용한게 너무 좋았어",
          "original_form": "나는 이번에 티저에서 대학 생활을 이용한게 너무 좋았어",
          "speaker_id": "1",
          "time": "20210821 16:16"
        },
        {
          "id": "MDRW2100004314.1.30",
          "form": "엔시티중에 대학을 간 사람은 둘뿐이지만...",
          "original_form": "엔시티중에 대학을 간 사람은 둘뿐이지만...",
          "speaker_id": "1",
          "time": "20210821 16:16"
        }
      ]
