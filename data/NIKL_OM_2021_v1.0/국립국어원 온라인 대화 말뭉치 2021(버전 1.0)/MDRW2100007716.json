      "utterance": [
        {
          "id": "MDRW2100007716.1.1",
          "form": "요즘엔 좀 시원해져서 좋아요",
          "original_form": "요즘엔 좀 시원해져서 좋아요",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007716.1.2",
          "form": "올 여름엔 너무 더웠어요",
          "original_form": "올 여름엔 너무 더웠어요",
          "speaker_id": "1",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007716.1.3",
          "form": "네 맞아요ㅠㅠ",
          "original_form": "네 맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 09:51"
        },
        {
          "id": "MDRW2100007716.1.4",
          "form": "여름에 더우면 겨울도 춥다는 얘기가 있던데",
          "original_form": "여름에 더우면 겨울도 춥다는 얘기가 있던데",
          "speaker_id": "1",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007716.1.5",
          "form": "네 저도 들어봤어요",
          "original_form": "네 저도 들어봤어요",
          "speaker_id": "2",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007716.1.6",
          "form": "여름보다는 겨울이 나은거같아요",
          "original_form": "여름보다는 겨울이 나은거같아요",
          "speaker_id": "2",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007716.1.7",
          "form": "어떤계절좋아하세요?",
          "original_form": "어떤계절좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 09:52"
        },
        {
          "id": "MDRW2100007716.1.8",
          "form": "저는 봄이 좋아요",
          "original_form": "저는 봄이 좋아요",
          "speaker_id": "1",
          "time": "20210822 09:53"
        },
        {
          "id": "MDRW2100007716.1.9",
          "form": "저도 봄을 좋아했었는데 요즘엔 가을이 더 좋아요",
          "original_form": "저도 봄을 좋아했었는데 요즘엔 가을이 더 좋아요",
          "speaker_id": "2",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007716.1.10",
          "form": "봄은 미세먼지때문에..",
          "original_form": "봄은 미세먼지때문에..",
          "speaker_id": "2",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007716.1.11",
          "form": "하늘도 뿌옇고",
          "original_form": "하늘도 뿌옇고",
          "speaker_id": "1",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007716.1.12",
          "form": "환기를 못해서 답답해요",
          "original_form": "환기를 못해서 답답해요",
          "speaker_id": "1",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007716.1.13",
          "form": "맞아요 답답해요",
          "original_form": "맞아요 답답해요",
          "speaker_id": "2",
          "time": "20210822 09:56"
        },
        {
          "id": "MDRW2100007716.1.14",
          "form": "벚꽃구경 못간지도 오래됐네요",
          "original_form": "벚꽃구경 못간지도 오래됐네요",
          "speaker_id": "1",
          "time": "20210822 09:57"
        },
        {
          "id": "MDRW2100007716.1.15",
          "form": "코로나가 잠잠해지면 좋을텐데",
          "original_form": "코로나가 잠잠해지면 좋을텐데",
          "speaker_id": "2",
          "time": "20210822 09:57"
        },
        {
          "id": "MDRW2100007716.1.16",
          "form": "내년에는 꽃구경 갈수있으면 좋겠네요",
          "original_form": "내년에는 꽃구경 갈수있으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210822 09:58"
        },
        {
          "id": "MDRW2100007716.1.17",
          "form": "네 내년에는 꼭..",
          "original_form": "네 내년에는 꼭..",
          "speaker_id": "2",
          "time": "20210822 09:58"
        },
        {
          "id": "MDRW2100007716.1.18",
          "form": "오늘도 날씨가 흐리네요",
          "original_form": "오늘도 날씨가 흐리네요",
          "speaker_id": "1",
          "time": "20210822 09:59"
        },
        {
          "id": "MDRW2100007716.1.19",
          "form": "여기는 비가 오다 말다하는중이에요",
          "original_form": "여기는 비가 오다 말다하는중이에요",
          "speaker_id": "2",
          "time": "20210822 09:59"
        },
        {
          "id": "MDRW2100007716.1.20",
          "form": "여기도 그래요",
          "original_form": "여기도 그래요",
          "speaker_id": "1",
          "time": "20210822 09:59"
        }
      ]
