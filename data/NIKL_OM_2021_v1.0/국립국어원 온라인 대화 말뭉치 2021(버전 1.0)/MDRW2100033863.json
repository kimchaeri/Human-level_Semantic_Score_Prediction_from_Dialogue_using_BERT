        },
        {
          "id": "MDRW2100033863.1.2",
          "form": "휴게소 알바 해봤어요",
          "original_form": "휴게소 알바 해봤어요",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100033863.1.3",
          "form": "에버랜드 수저 닦기랑요 ㅋㅋ",
          "original_form": "에버랜드 수저 닦기랑요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100033863.1.4",
          "form": "와! 둘 다 신기해요 주변에 없는!!!",
          "original_form": "와! 둘 다 신기해요 주변에 없는!!!",
          "speaker_id": "1",
          "time": "20210825 13:31"
        },
        {
          "id": "MDRW2100033863.1.5",
          "form": "네 저도 한번 해봤네요",
          "original_form": "네 저도 한번 해봤네요",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100033863.1.6",
          "form": "에버랜드 와 한 만개 넘게 닦았나봐요 ㅜㅜ",
          "original_form": "에버랜드 와 한 만개 넘게 닦았나봐요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100033863.1.7",
          "form": "손수 닦나요? 기계 안쓰고?",
          "original_form": "손수 닦나요? 기계 안쓰고?",
          "speaker_id": "1",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100033863.1.8",
          "form": "아니 수저 닦는데 왜 치마를 미니스커트를 입나요",
          "original_form": "아니 수저 닦는데 왜 치마를 미니스커트를 입나요",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100033863.1.9",
          "form": "그땐 그랬어요",
          "original_form": "그땐 그랬어요",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100033863.1.10",
          "form": "한 20년도 더전에요 ㅋㅋ",
          "original_form": "한 20년도 더전에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:32"
        },
        {
          "id": "MDRW2100033863.1.11",
          "form": "진짜 불편하고 팔빠질뻔했어요 ㅋㅋ",
          "original_form": "진짜 불편하고 팔빠질뻔했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100033863.1.12",
          "form": "제 친구는 에버랜드에서 아르바이트 하다가 남친 만났다고 하더라구요~^^ 놀이기구 아르바이트였나...",
          "original_form": "제 친구는 에버랜드에서 아르바이트 하다가 남친 만났다고 하더라구요~^^ 놀이기구 아르바이트였나...",
          "speaker_id": "1",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100033863.1.13",
          "form": "오 거기 잘생겼어요 ㅋㅋ",
          "original_form": "오 거기 잘생겼어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100033863.1.14",
          "form": "전 하루하고 때려쳤어요",
          "original_form": "전 하루하고 때려쳤어요",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100033863.1.15",
          "form": "근데 대기업이라 돈이 ㅋㅋ",
          "original_form": "근데 대기업이라 돈이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 13:50"
        },
        {
          "id": "MDRW2100033863.1.16",
          "form": "엄청 많이줘요",
          "original_form": "엄청 많이줘요",
          "speaker_id": "2",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100033863.1.17",
          "form": "요새 알바 주말에 하고싶은데 자리가 없어요",
          "original_form": "요새 알바 주말에 하고싶은데 자리가 없어요",
          "speaker_id": "2",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100033863.1.18",
          "form": "그리고 시급도 잘 안준대요",
          "original_form": "그리고 시급도 잘 안준대요",
          "speaker_id": "2",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100033863.1.19",
          "form": "아.. 시급이 세군요! 요즘은 코로나 때문일까요 ㅠ",
          "original_form": "아.. 시급이 세군요! 요즘은 코로나 때문일까요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100033863.1.20",
          "form": "네 알바절벽요",
          "original_form": "네 알바절벽요",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100033863.1.21",
          "form": "안타까워요~ 취업도 힘든데 아르바이트까지 절벽이라니 ㅠ",
          "original_form": "안타까워요~ 취업도 힘든데 아르바이트까지 절벽이라니 ㅠ",
          "speaker_id": "1",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100033863.1.22",
          "form": "네 그리고돈도 안주고",
          "original_form": "네 그리고돈도 안주고",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100033863.1.23",
          "form": "뭐든 일하기 힘들어요",
          "original_form": "뭐든 일하기 힘들어요",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100033863.1.24",
          "form": "그나마 집에서 공모주로 벌이하니 다행이예요",
          "original_form": "그나마 집에서 공모주로 벌이하니 다행이예요",
          "speaker_id": "2",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100033863.1.25",
          "form": "와 주식하시는군요! 그게 곧 벌이니 ! 능력이셔요!",
          "original_form": "와 주식하시는군요! 그게 곧 벌이니 ! 능력이셔요!",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100033863.1.26",
          "form": "아니예요 주식은 폭망 공모주만 ㅋㅋ벌이가돼요",
          "original_form": "아니예요 주식은 폭망 공모주만 ㅋㅋ벌이가돼요",
          "speaker_id": "2",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100033863.1.27",
          "form": "없음 가정이 ㅜㅜ",
          "original_form": "없음 가정이 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 13:56"
        }
      ]
    }
