      "utterance": [
        {
          "id": "MDRW2100043191.1.1",
          "form": "다른것도 아니고",
          "original_form": "다른것도 아니고",
          "speaker_id": "2",
          "time": "20210907 15:33"
        },
        {
          "id": "MDRW2100043191.1.2",
          "form": "그런 이유면\r\n어떤 엄마가 보낼수 있겠어여ㅠ",
          "original_form": "그런 이유면\r\n어떤 엄마가 보낼수 있겠어여ㅠ",
          "speaker_id": "2",
          "time": "20210907 15:33"
        },
        {
          "id": "MDRW2100043191.1.3",
          "form": "선생이 바꼈다고 한들 ㅠㅠ",
          "original_form": "선생이 바꼈다고 한들 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100043191.1.4",
          "form": "원장이 자리를 자주비우는지 여튼 ㅠㅠ 그래서 찝찝해서ㅠㅠㅠ",
          "original_form": "원장이 자리를 자주비우는지 여튼 ㅠㅠ 그래서 찝찝해서ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:34"
        },
        {
          "id": "MDRW2100043191.1.5",
          "form": "잘하셨어요ㅠ",
          "original_form": "잘하셨어요ㅠ",
          "speaker_id": "2",
          "time": "20210907 15:36"
        },
        {
          "id": "MDRW2100043191.1.6",
          "form": "그런 마음이면\r\n보내고도 편치 않았을거에여",
          "original_form": "그런 마음이면\r\n보내고도 편치 않았을거에여",
          "speaker_id": "2",
          "time": "20210907 15:37"
        },
        {
          "id": "MDRW2100043191.1.7",
          "form": "그거 놓치고 나니 이때까지 끼고 ㅠㅠ",
          "original_form": "그거 놓치고 나니 이때까지 끼고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100043191.1.8",
          "form": "코로나땜에 다들 안보내는 판국에 놀면서 ㅠ 보내려니 그래서ㅠㅠ",
          "original_form": "코로나땜에 다들 안보내는 판국에 놀면서 ㅠ 보내려니 그래서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100043191.1.9",
          "form": "저희 꼬맹이도 내년에는 3살이라\r\n우째야하나 고민이에요",
          "original_form": "저희 꼬맹이도 내년에는 3살이라\r\n우째야하나 고민이에요",
          "speaker_id": "2",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100043191.1.10",
          "form": "ㅠㅠㅠㅠㅠㅠㅠㅠ 고민되시겠네여ㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠㅠ 고민되시겠네여ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100043191.1.11",
          "form": "다섯살도 코로나땜에\r\n가정보육한 날들이 더 많아서ㅠㅋ",
          "original_form": "다섯살도 코로나땜에\r\n가정보육한 날들이 더 많아서ㅠㅋ",
          "speaker_id": "2",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043191.1.12",
          "form": "그니까여ㅠㅠㅠㅠㅠㅠ 그나마 초딩은 학교라 보내는데ㅠ",
          "original_form": "그니까여ㅠㅠㅠㅠㅠㅠ 그나마 초딩은 학교라 보내는데ㅠ",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043191.1.13",
          "form": "어린애가 마스크쓰고 하루종일 어린이집에서ㅠㅠ",
          "original_form": "어린애가 마스크쓰고 하루종일 어린이집에서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043191.1.14",
          "form": "그러다가 또 확진자 나오면 어쩌나 싶고ㅠㅠ",
          "original_form": "그러다가 또 확진자 나오면 어쩌나 싶고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100043191.1.15",
          "form": "맞아요ㅠ 애들도 답답하고 힘들죠ㅜ",
          "original_form": "맞아요ㅠ 애들도 답답하고 힘들죠ㅜ",
          "speaker_id": "2",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100043191.1.16",
          "form": "애들이 제일 안타까워요ㅠㅠ",
          "original_form": "애들이 제일 안타까워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 16:18"
        },
        {
          "id": "MDRW2100043191.1.17",
          "form": "그러게요ㅠㅠ",
          "original_form": "그러게요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100043191.1.18",
          "form": "내년에는 좀 더 나아져서\r\n아이들 원도 학교도\r\n자유롭게 갈 수 있음 좋겠어영",
          "original_form": "내년에는 좀 더 나아져서\r\n아이들 원도 학교도\r\n자유롭게 갈 수 있음 좋겠어영",
          "speaker_id": "2",
          "time": "20210907 16:27"
        },
        {
          "id": "MDRW2100043191.1.19",
          "form": "가긴가는데 불안불안해여 지금도 ㅠㅠ",
          "original_form": "가긴가는데 불안불안해여 지금도 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100043191.1.20",
          "form": "지난번에 또 윗학년에서 확진자 나와서ㅠㅠ",
          "original_form": "지난번에 또 윗학년에서 확진자 나와서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 16:28"
        },
        {
          "id": "MDRW2100043191.1.21",
          "form": "ㅠㅠ안전지대가 없죠ㅠ",
          "original_form": "ㅠㅠ안전지대가 없죠ㅠ",
          "speaker_id": "2",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100043191.1.22",
          "form": "학교도 그렇궁",
          "original_form": "학교도 그렇궁",
          "speaker_id": "2",
          "time": "20210907 16:29"
        }
      ]
    }
  ]
