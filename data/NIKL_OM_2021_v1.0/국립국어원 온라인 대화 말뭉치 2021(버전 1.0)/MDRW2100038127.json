      "utterance": [
        {
          "id": "MDRW2100038127.1.1",
          "form": "진짜 장건강부터 챙겨보세요!!",
          "original_form": "진짜 장건강부터 챙겨보세요!!",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038127.1.2",
          "form": "저도 이런저런 고생 많이 해봐서ㅠㅠ",
          "original_form": "저도 이런저런 고생 많이 해봐서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038127.1.3",
          "form": "마음이 쓰이네요",
          "original_form": "마음이 쓰이네요",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038127.1.4",
          "form": "저는 치질로도 고생해봤어요...",
          "original_form": "저는 치질로도 고생해봤어요...",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038127.1.5",
          "form": "진짜 완전ㅜㅜ동지입니당...",
          "original_form": "진짜 완전ㅜㅜ동지입니당...",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038127.1.6",
          "form": "흑흑 아니 왜 성한곳이없으셔요",
          "original_form": "흑흑 아니 왜 성한곳이없으셔요",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038127.1.7",
          "form": "그쵸ㅠ",
          "original_form": "그쵸ㅠ",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038127.1.8",
          "form": "치질은 어쩌시다가",
          "original_form": "치질은 어쩌시다가",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038127.1.9",
          "form": "제가",
          "original_form": "제가",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038127.1.10",
          "form": "성인 되고 나서도 수험생활을 오래 했는데",
          "original_form": "성인 되고 나서도 수험생활을 오래 했는데",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038127.1.11",
          "form": "시험을 하나 준비했거든요",
          "original_form": "시험을 하나 준비했거든요",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038127.1.12",
          "form": "아하ㅜㅜㅜ그쳐ㅜㅜ저도시험준비했었는데",
          "original_form": "아하ㅜㅜㅜ그쳐ㅜㅜ저도시험준비했었는데",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038127.1.13",
          "form": "그래서 계속 앉아서만 지내니까",
          "original_form": "그래서 계속 앉아서만 지내니까",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038127.1.14",
          "form": "병이 생기더라구요",
          "original_form": "병이 생기더라구요",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038127.1.15",
          "form": "아이고ㅠㅠ",
          "original_form": "아이고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100038127.1.16",
          "form": "아갓리였어서",
          "original_form": "아갓리였어서",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038127.1.17",
          "form": "진짜 수험생활이 건강 다 망치더라구요...",
          "original_form": "진짜 수험생활이 건강 다 망치더라구요...",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038127.1.18",
          "form": "맞아요 저 진짜 간만봤는데도",
          "original_form": "맞아요 저 진짜 간만봤는데도",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038127.1.19",
          "form": "허리아작났어요",
          "original_form": "허리아작났어요",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038127.1.20",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038127.1.21",
          "form": "시험준비를요???",
          "original_form": "시험준비를요???",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038127.1.22",
          "form": "원체 자세도안좋은데",
          "original_form": "원체 자세도안좋은데",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038127.1.23",
          "form": "아 맞아요 진짜 허리",
          "original_form": "아 맞아요 진짜 허리",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038127.1.24",
          "form": "네 공시준비살짝해서 노량진갘ㅅ었는데 ㅋㅋㅋ...",
          "original_form": "네 공시준비살짝해서 노량진갘ㅅ었는데 ㅋㅋㅋ...",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038127.1.25",
          "form": "허리도 할 말 많아요...",
          "original_form": "허리도 할 말 많아요...",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038127.1.26",
          "form": "아휴 ㅠㅠ",
          "original_form": "아휴 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038127.1.27",
          "form": "진짜 분위기부터 벌써 다르죠",
          "original_form": "진짜 분위기부터 벌써 다르죠",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038127.1.28",
          "form": "거기서 허리가 휘었져",
          "original_form": "거기서 허리가 휘었져",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100038127.1.29",
          "form": "저는 약대편입 준비했었어요ㅠ",
          "original_form": "저는 약대편입 준비했었어요ㅠ",
          "speaker_id": "2",
          "time": "20210901 23:30"
        }
      ]
