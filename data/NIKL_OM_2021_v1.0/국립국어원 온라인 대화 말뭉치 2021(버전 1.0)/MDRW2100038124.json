      "utterance": [
        {
          "id": "MDRW2100038124.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038124.1.2",
          "form": "이번 홀맨 선물 마음에 드시나요?",
          "original_form": "이번 홀맨 선물 마음에 드시나요?",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100038124.1.3",
          "form": "네 볼펜 잘써서 맘에 드네요ㅎㅎ",
          "original_form": "네 볼펜 잘써서 맘에 드네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100038124.1.4",
          "form": "name2님은요??",
          "original_form": "&name2&님은요??",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100038124.1.5",
          "form": "저두요 볼펜은 자주 사용하는 물건이기도 하고 귀엽더라구요ㅋㅋㅋ",
          "original_form": "저두요 볼펜은 자주 사용하는 물건이기도 하고 귀엽더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100038124.1.6",
          "form": "이러다 홀맨 덕후 될거같아여ㅋㅋ",
          "original_form": "이러다 홀맨 덕후 될거같아여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100038124.1.7",
          "form": "그러라고 계속 굿즈를 뿌리는건가 싶기도 해요",
          "original_form": "그러라고 계속 굿즈를 뿌리는건가 싶기도 해요",
          "speaker_id": "2",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100038124.1.8",
          "form": "저도 그렇게 생각해요ㅋㅋ",
          "original_form": "저도 그렇게 생각해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100038124.1.9",
          "form": "name2님은 이번에 어떤 문제 나왔었어요?",
          "original_form": "&name2&님은 이번에 어떤 문제 나왔었어요?",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100038124.1.10",
          "form": "정신없이 이 답 써보고 저 답 써보고 해서 확실하진 않지만",
          "original_form": "정신없이 이 답 써보고 저 답 써보고 해서 확실하진 않지만",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100038124.1.11",
          "form": "공장장홀맨이랑 묶음머리홀맨이요",
          "original_form": "공장장홀맨이랑 묶음머리홀맨이요",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100038124.1.12",
          "form": "전 과일 홀맨 나왔어요ㅋㅋ",
          "original_form": "전 과일 홀맨 나왔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100038124.1.13",
          "form": "그게 원래 게임 통해서 답 알아내야 했던거 아세요?",
          "original_form": "그게 원래 게임 통해서 답 알아내야 했던거 아세요?",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100038124.1.14",
          "form": "그렇다고하더라구요",
          "original_form": "그렇다고하더라구요",
          "speaker_id": "2",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100038124.1.15",
          "form": "저는 그 게임이 로딩 끝나고 나면 페이지를 찾을 수 없다 다시 시도하세요 이런 식이여서 아직 게임 구경도 못 했어요",
          "original_form": "저는 그 게임이 로딩 끝나고 나면 페이지를 찾을 수 없다 다시 시도하세요 이런 식이여서 아직 게임 구경도 못 했어요",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038124.1.16",
          "form": "저희 언니는 첨에 무조건 게임을 하느라 난리였다네요",
          "original_form": "저희 언니는 첨에 무조건 게임을 하느라 난리였다네요",
          "speaker_id": "1",
          "time": "20210901 23:50"
        },
        {
          "id": "MDRW2100038124.1.17",
          "form": "오 게임에 접속이 되었었나보네요!",
          "original_form": "오 게임에 접속이 되었었나보네요!",
          "speaker_id": "2",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100038124.1.18",
          "form": "저는 그게 게임일거라고는 상상도 못 하고, 상품페이지 그런거일줄 알았어요",
          "original_form": "저는 그게 게임일거라고는 상상도 못 하고, 상품페이지 그런거일줄 알았어요",
          "speaker_id": "2",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100038124.1.19",
          "form": "제 첨엔 사람들이 덜 몰렸어서 가능햇을거에요ㅋㅋ",
          "original_form": "제 첨엔 사람들이 덜 몰렸어서 가능햇을거에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100038124.1.20",
          "form": "근데 급격하게 사람들이 폭주했죠",
          "original_form": "근데 급격하게 사람들이 폭주했죠",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100038124.1.21",
          "form": "그렇군요 저는 딱 사람들이 몰릴때 들어갔나봐요",
          "original_form": "그렇군요 저는 딱 사람들이 몰릴때 들어갔나봐요",
          "speaker_id": "2",
          "time": "20210901 23:56"
        }
      ]
    }
