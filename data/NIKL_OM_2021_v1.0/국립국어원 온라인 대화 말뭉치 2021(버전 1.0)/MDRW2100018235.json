      "utterance": [
        {
          "id": "MDRW2100018235.1.1",
          "form": "쇼핑 젛아하세요?",
          "original_form": "쇼핑 젛아하세요?",
          "speaker_id": "1",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018235.1.2",
          "form": "네 돈쓰는건 다 재밌어요",
          "original_form": "네 돈쓰는건 다 재밌어요",
          "speaker_id": "2",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100018235.1.3",
          "form": "극하게 공감합니다",
          "original_form": "극하게 공감합니다",
          "speaker_id": "1",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018235.1.4",
          "form": "옷에 돈 많이쓰시나요",
          "original_form": "옷에 돈 많이쓰시나요",
          "speaker_id": "1",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018235.1.5",
          "form": "많이는 아니고 그냥 맘에들면 거의 다 사는거같아요",
          "original_form": "많이는 아니고 그냥 맘에들면 거의 다 사는거같아요",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018235.1.6",
          "form": "선호하는 쇼핑몰이나 브랜드 있나요",
          "original_form": "선호하는 쇼핑몰이나 브랜드 있나요",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018235.1.7",
          "form": "저는 무신사에서 주로 사요",
          "original_form": "저는 무신사에서 주로 사요",
          "speaker_id": "2",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018235.1.8",
          "form": "최근엔 뭐사셧나요 ㅎㅎ",
          "original_form": "최근엔 뭐사셧나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:43"
        },
        {
          "id": "MDRW2100018235.1.9",
          "form": "트렌치코트 샀어요 ㅋㅋㅋ",
          "original_form": "트렌치코트 샀어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018235.1.10",
          "form": "와 잘 사셨네요",
          "original_form": "와 잘 사셨네요",
          "speaker_id": "1",
          "time": "20210827 11:01"
        },
        {
          "id": "MDRW2100018235.1.11",
          "form": "오버핏 인가요",
          "original_form": "오버핏 인가요",
          "speaker_id": "1",
          "time": "20210827 11:01"
        },
        {
          "id": "MDRW2100018235.1.12",
          "form": "포멀핏이에요",
          "original_form": "포멀핏이에요",
          "speaker_id": "2",
          "time": "20210827 15:10"
        },
        {
          "id": "MDRW2100018235.1.13",
          "form": "가을옷장만하셨어요?",
          "original_form": "가을옷장만하셨어요?",
          "speaker_id": "2",
          "time": "20210827 15:10"
        },
        {
          "id": "MDRW2100018235.1.14",
          "form": "그러고 싶은데 pt 할 돈 모으느라 자제중입니다 ㅠㅠ",
          "original_form": "그러고 싶은데 pt 할 돈 모으느라 자제중입니다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100018235.1.15",
          "form": "닥터마틴 로퍼도 사고 싶네요",
          "original_form": "닥터마틴 로퍼도 사고 싶네요",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100018235.1.16",
          "form": "피티 너무 비싸죠 ㅠ",
          "original_form": "피티 너무 비싸죠 ㅠ",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
