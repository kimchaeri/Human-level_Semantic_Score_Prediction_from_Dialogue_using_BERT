      "utterance": [
        {
          "id": "MDRW2100019256.1.1",
          "form": "여행 좋아하시나요",
          "original_form": "여행 좋아하시나요",
          "speaker_id": "1",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100019256.1.2",
          "form": "좋아는 하는데.. 많이는 못가여.. \r\n최근 어디 가보셧나요?",
          "original_form": "좋아는 하는데.. 많이는 못가여.. \r\n최근 어디 가보셧나요?",
          "speaker_id": "2",
          "time": "20210827 21:17"
        },
        {
          "id": "MDRW2100019256.1.3",
          "form": "워크샵으로 일본 간 게 최근인데....2년전이네요....ㅋㅋㅋㅋ",
          "original_form": "워크샵으로 일본 간 게 최근인데....2년전이네요....ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:27"
        },
        {
          "id": "MDRW2100019256.1.4",
          "form": "최근에 어디 가보셨나요",
          "original_form": "최근에 어디 가보셨나요",
          "speaker_id": "1",
          "time": "20210828 14:27"
        },
        {
          "id": "MDRW2100019256.1.5",
          "form": "오.. 일본 오디염? \r\n저 도쿄 1년 있었어여",
          "original_form": "오.. 일본 오디염? \r\n저 도쿄 1년 있었어여",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100019256.1.6",
          "form": "오 도쿄요! 닝교초 근처",
          "original_form": "오 도쿄요! 닝교초 근처",
          "speaker_id": "1",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100019256.1.7",
          "form": "무슨구에요?",
          "original_form": "무슨구에요?",
          "speaker_id": "2",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100019256.1.8",
          "form": "츄오쿠인가 그랬던거 같아요ㅋㅋ",
          "original_form": "츄오쿠인가 그랬던거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100019256.1.9",
          "form": "신주쿠 근처였나 보네요..",
          "original_form": "신주쿠 근처였나 보네요..",
          "speaker_id": "2",
          "time": "20210830 15:10"
        },
        {
          "id": "MDRW2100019256.1.10",
          "form": "네 멀진 않았었어요ㅋㅋ",
          "original_form": "네 멀진 않았었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100019256.1.11",
          "form": "어디 계셨었나요",
          "original_form": "어디 계셨었나요",
          "speaker_id": "1",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100019256.1.12",
          "form": "전 신주쿠위에.. 이케부쿠로 근처요..",
          "original_form": "전 신주쿠위에.. 이케부쿠로 근처요..",
          "speaker_id": "2",
          "time": "20210830 16:17"
        },
        {
          "id": "MDRW2100019256.1.13",
          "form": "도쿄 여기저기 많이 보셧어여?",
          "original_form": "도쿄 여기저기 많이 보셧어여?",
          "speaker_id": "2",
          "time": "20210830 16:17"
        },
        {
          "id": "MDRW2100019256.1.14",
          "form": "신주쿠 하라주쿠 도쿄타워 근처 갔어요ㅋㅋ",
          "original_form": "신주쿠 하라주쿠 도쿄타워 근처 갔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:18"
        },
        {
          "id": "MDRW2100019256.1.15",
          "form": "오모테산도인가 거기도요",
          "original_form": "오모테산도인가 거기도요",
          "speaker_id": "1",
          "time": "20210830 16:18"
        },
        {
          "id": "MDRW2100019256.1.16",
          "form": "필수코스는 다 도셨네요..ㅋㅋㅋㅋ \r\n재미는 있었나요?",
          "original_form": "필수코스는 다 도셨네요..ㅋㅋㅋㅋ \r\n재미는 있었나요?",
          "speaker_id": "2",
          "time": "20210830 16:21"
        },
        {
          "id": "MDRW2100019256.1.17",
          "form": "그냥 그랬어요 한국이랑 크게 다르지 않은 느낌..?",
          "original_form": "그냥 그랬어요 한국이랑 크게 다르지 않은 느낌..?",
