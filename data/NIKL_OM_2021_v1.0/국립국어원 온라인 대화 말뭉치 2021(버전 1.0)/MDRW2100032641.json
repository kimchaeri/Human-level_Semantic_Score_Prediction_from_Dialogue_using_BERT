      "utterance": [
        {
          "id": "MDRW2100032641.1.1",
          "form": "맘마미아 다이어트 댄스요!",
          "original_form": "맘마미아 다이어트 댄스요!",
          "speaker_id": "2",
          "time": "20210905 21:48"
        },
        {
          "id": "MDRW2100032641.1.2",
          "form": "쳐보니까 맘마미아 댄스도 나오네요",
          "original_form": "쳐보니까 맘마미아 댄스도 나오네요",
          "speaker_id": "2",
          "time": "20210905 21:48"
        },
        {
          "id": "MDRW2100032641.1.3",
          "form": "sf9 노래에 다이어트댄스 붙인거예요!",
          "original_form": "sf9 노래에 다이어트댄스 붙인거예요!",
          "speaker_id": "2",
          "time": "20210905 21:48"
        },
        {
          "id": "MDRW2100032641.1.4",
          "form": "오오!!!!",
          "original_form": "오오!!!!",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032641.1.5",
          "form": "노래도 좋은데여??",
          "original_form": "노래도 좋은데여??",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032641.1.6",
          "form": "마자요!",
          "original_form": "마자요!",
          "speaker_id": "2",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032641.1.7",
          "form": "동작도 어렵지않고 따라하기좋더라구요ㅋㅋㅋ",
          "original_form": "동작도 어렵지않고 따라하기좋더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100032641.1.8",
          "form": "내일부터 이걸로 해야겠어요ㅎㅎㅋㅋㅋㅋㅋ",
          "original_form": "내일부터 이걸로 해야겠어요ㅎㅎㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032641.1.9",
          "form": "더 운동이 많이 될것같아요",
          "original_form": "더 운동이 많이 될것같아요",
          "speaker_id": "1",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100032641.1.10",
          "form": "원본은 짧으니까 다른 사람이 30분 연속으로 편집해두신거 보시는게 좋을거같아요ㅋㅋ!",
          "original_form": "원본은 짧으니까 다른 사람이 30분 연속으로 편집해두신거 보시는게 좋을거같아요ㅋㅋ!",
          "speaker_id": "2",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100032641.1.11",
          "form": "그거 지금 틀어놨어요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그거 지금 틀어놨어요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100032641.1.12",
          "form": "넘 좋은데요..? 소화가 잘되네요",
          "original_form": "넘 좋은데요..? 소화가 잘되네요",
          "speaker_id": "1",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100032641.1.13",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:20"
        },
        {
          "id": "MDRW2100032641.1.14",
          "form": "빠르시네요",
          "original_form": "빠르시네요",
          "speaker_id": "2",
          "time": "20210905 22:20"
        },
        {
          "id": "MDRW2100032641.1.15",
          "form": "근데 옆구리가 너무 아파요....ㅋㅋㅋㅋㅋㅠ",
          "original_form": "근데 옆구리가 너무 아파요....ㅋㅋㅋㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100032641.1.16",
          "form": "운동이 되고 있다는 신호겠죠?ㅋㅋㅋㅋㅋ",
          "original_form": "운동이 되고 있다는 신호겠죠?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032641.1.17",
          "form": "엌ㅋㄱㅋ제대로 운동하셨나본데요?",
          "original_form": "엌ㅋㄱㅋ제대로 운동하셨나본데요?",
          "speaker_id": "2",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032641.1.18",
          "form": "네ㅋㅋㅋㅋㅋ 오늘도 너무 많이 먹어서",
          "original_form": "네ㅋㅋㅋㅋㅋ 오늘도 너무 많이 먹어서",
          "speaker_id": "1",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100032641.1.19",
          "form": "저 운동 해야겠어요ㅋㅋㅋㅋ",
          "original_form": "저 운동 해야겠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100032641.1.20",
