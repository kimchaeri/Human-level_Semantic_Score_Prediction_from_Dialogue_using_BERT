      "utterance": [
        {
          "id": "MDRW2100045964.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045964.1.2",
          "form": "아침 드셨나요?",
          "original_form": "아침 드셨나요?",
          "speaker_id": "1",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045964.1.3",
          "form": "네 먹었어요",
          "original_form": "네 먹었어요",
          "speaker_id": "2",
          "time": "20210908 09:06"
        },
        {
          "id": "MDRW2100045964.1.4",
          "form": "오 어떤거 드셨나요",
          "original_form": "오 어떤거 드셨나요",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045964.1.5",
          "form": "그냥 집밥이요. 계란국에 밥먹었어요",
          "original_form": "그냥 집밥이요. 계란국에 밥먹었어요",
          "speaker_id": "2",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045964.1.6",
          "form": "와 계란국 ㅠㅠ 맛있었겠네요",
          "original_form": "와 계란국 ㅠㅠ 맛있었겠네요",
          "speaker_id": "1",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100045964.1.7",
          "form": "저는 요플레 하나 먹었어요...",
          "original_form": "저는 요플레 하나 먹었어요...",
          "speaker_id": "1",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100045964.1.8",
          "form": "배고프실거 같은데요...",
          "original_form": "배고프실거 같은데요...",
          "speaker_id": "2",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100045964.1.9",
          "form": "지금 찐감자도 반개 꺼냈어욬ㅋㅋㅋㅋㅋ",
          "original_form": "지금 찐감자도 반개 꺼냈어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:10"
        },
        {
          "id": "MDRW2100045964.1.10",
          "form": "ㅋㅋㅋ아침은 간단하게 드시나봐요",
          "original_form": "ㅋㅋㅋ아침은 간단하게 드시나봐요",
          "speaker_id": "2",
          "time": "20210908 09:12"
        },
        {
          "id": "MDRW2100045964.1.11",
          "form": "네ㅋㅋㅋㅋ 점심도 곧 먹으니까요...",
          "original_form": "네ㅋㅋㅋㅋ 점심도 곧 먹으니까요...",
          "speaker_id": "1",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100045964.1.12",
          "form": "챙겨주는 사람 있으니까 먹게되요ㅋㅋ",
          "original_form": "챙겨주는 사람 있으니까 먹게되요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:27"
        },
        {
          "id": "MDRW2100045964.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ점심은",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ점심은",
          "speaker_id": "1",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100045964.1.14",
          "form": "어떤거 드실 예정이세요?",
          "original_form": "어떤거 드실 예정이세요?",
          "speaker_id": "1",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100045964.1.15",
          "form": "글쎄요. 식당갈거라서ㅎ 백반 먹게될거 같아요",
          "original_form": "글쎄요. 식당갈거라서ㅎ 백반 먹게될거 같아요",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100045964.1.16",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210908 09:36"
        },
        {
          "id": "MDRW2100045964.1.17",
          "form": "백반좋죠 ㅠㅠ",
          "original_form": "백반좋죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:36"
        },
        {
          "id": "MDRW2100045964.1.18",
          "form": "외식은자주하시나요?",
          "original_form": "외식은자주하시나요?",
          "speaker_id": "1",
          "time": "20210908 09:36"
        },
        {
          "id": "MDRW2100045964.1.19",
          "form": "요새는 잘 못해요ㅋㅋ",
          "original_form": "요새는 잘 못해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100045964.1.20",
          "form": "앗... ㅠㅠ 맞네요",
          "original_form": "앗... ㅠㅠ 맞네요",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100045964.1.21",
          "form": "배달은 되니깐요ㅋㅋㅋ",
          "original_form": "배달은 되니깐요ㅋㅋㅋ",
