      "utterance": [
        {
          "id": "MDRW2100042075.1.1",
          "form": "게임을 좋아하시는거예요 ?",
          "original_form": "게임을 좋아하시는거예요 ?",
          "speaker_id": "1",
          "time": "20210906 19:26"
        },
        {
          "id": "MDRW2100042075.1.2",
          "form": "아뇨 잘하질못해서ㅜㅜ",
          "original_form": "아뇨 잘하질못해서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 19:27"
        },
        {
          "id": "MDRW2100042075.1.3",
          "form": "동물의숲만 엄청 열심히 했어요ㅋㅋㅋ",
          "original_form": "동물의숲만 엄청 열심히 했어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:27"
        },
        {
          "id": "MDRW2100042075.1.4",
          "form": "다른 막 온라인게임 모바일게임은 잘몰라요!",
          "original_form": "다른 막 온라인게임 모바일게임은 잘몰라요!",
          "speaker_id": "2",
          "time": "20210906 19:27"
        },
        {
          "id": "MDRW2100042075.1.5",
          "form": "아항 ㅎㅎ 저도 아직도 메이플해요ㅋㅋㅋㅋㅋ 요즘게임은 못하겠어요 ..",
          "original_form": "아항 ㅎㅎ 저도 아직도 메이플해요ㅋㅋㅋㅋㅋ 요즘게임은 못하겠어요 ..",
          "speaker_id": "1",
          "time": "20210906 19:28"
        },
        {
          "id": "MDRW2100042075.1.6",
          "form": "오 메이플!",
          "original_form": "오 메이플!",
          "speaker_id": "2",
          "time": "20210906 19:29"
        },
        {
          "id": "MDRW2100042075.1.7",
          "form": "막 헤어스타일뽑고ㅋㅋ 이런거 봤어요",
          "original_form": "막 헤어스타일뽑고ㅋㅋ 이런거 봤어요",
          "speaker_id": "2",
          "time": "20210906 19:29"
        },
        {
          "id": "MDRW2100042075.1.8",
          "form": "ㅋㅋㅋㅋㅋ학생때도 게임은 안하셨나요?",
          "original_form": "ㅋㅋㅋㅋㅋ학생때도 게임은 안하셨나요?",
          "speaker_id": "1",
          "time": "20210906 19:29"
        },
        {
          "id": "MDRW2100042075.1.9",
          "form": "완전 초창기때 스타크래프트같은거만 좀 했나봐요",
          "original_form": "완전 초창기때 스타크래프트같은거만 좀 했나봐요",
          "speaker_id": "2",
          "time": "20210906 19:30"
        },
        {
          "id": "MDRW2100042075.1.10",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:30"
        },
        {
          "id": "MDRW2100042075.1.11",
          "form": "메이플은 뭐하는게임이에요?",
          "original_form": "메이플은 뭐하는게임이에요?",
          "speaker_id": "2",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100042075.1.12",
          "form": "정확히는 뭔지 몰라가주구ㅋㅋㅋ",
          "original_form": "정확히는 뭔지 몰라가주구ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100042075.1.13",
          "form": "오호 스타ㅋㅋㅋ",
          "original_form": "오호 스타ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100042075.1.14",
          "form": "사냥하고 돈벌고 그걸로 옷사고 뭐그런 ..",
          "original_form": "사냥하고 돈벌고 그걸로 옷사고 뭐그런 ..",
          "speaker_id": "1",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100042075.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100042075.1.16",
          "form": "오 사냥ㅋㅋㅋㅋ",
          "original_form": "오 사냥ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100042075.1.17",
          "form": "엄청 레벨 높으시겠어요",
          "original_form": "엄청 레벨 높으시겠어요",
          "speaker_id": "2",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100042075.1.18",
          "form": "ㅋㅋㅋ 높은편입니당 퇴근하고와서 하면 스트레스도 풀려용 옆에 넷플켜놓고 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 높은편입니당 퇴근하고와서 하면 스트레스도 풀려용 옆에 넷플켜놓고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:33"
        },
        {
          "id": "MDRW2100042075.1.19",
          "form": "오오오ㅋㅋㅋㅋ 재밌을거같아요",
          "original_form": "오오오ㅋㅋㅋㅋ 재밌을거같아요",
          "speaker_id": "2",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100042075.1.20",
          "form": "잼나요 ㅋㅋ가볍게 하기좋아서 .. 현질성도 없구용",
          "original_form": "잼나요 ㅋㅋ가볍게 하기좋아서 .. 현질성도 없구용",
          "speaker_id": "1",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100042075.1.21",
          "form": "지금 시작해도 할만할까요?",
          "original_form": "지금 시작해도 할만할까요?",
          "speaker_id": "2",
          "time": "20210906 19:37"
        },
        {
          "id": "MDRW2100042075.1.22",
          "form": "그럼요 ㅋㅋ 재밌어요 캐릭이 귀여워용",
          "original_form": "그럼요 ㅋㅋ 재밌어요 캐릭이 귀여워용",
          "speaker_id": "1",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100042075.1.23",
          "form": "요즘 좀 심심한데 해볼까봐요",
          "original_form": "요즘 좀 심심한데 해볼까봐요",
          "speaker_id": "2",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100042075.1.24",
          "form": "감사합니당ㅋㅋㅋ",
          "original_form": "감사합니당ㅋㅋㅋ",
          "speaker_id": "2",
