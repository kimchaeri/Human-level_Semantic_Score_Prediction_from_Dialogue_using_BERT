      "utterance": [
        {
          "id": "MDRW2100003869.1.1",
          "form": "아니요 좋아는해요",
          "original_form": "아니요 좋아는해요",
          "speaker_id": "2",
          "time": "20210821 13:06"
        },
        {
          "id": "MDRW2100003869.1.2",
          "form": "관리가 어렵죠",
          "original_form": "관리가 어렵죠",
          "speaker_id": "1",
          "time": "20210821 13:06"
        },
        {
          "id": "MDRW2100003869.1.3",
          "form": "돈도 들고 외출시 혼자 놔두기도 안쓰럽죠",
          "original_form": "돈도 들고 외출시 혼자 놔두기도 안쓰럽죠",
          "speaker_id": "2",
          "time": "20210821 13:07"
        },
        {
          "id": "MDRW2100003869.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210821 13:07"
        },
        {
          "id": "MDRW2100003869.1.5",
          "form": "키우시는 동물 있나요?",
          "original_form": "키우시는 동물 있나요?",
          "speaker_id": "2",
          "time": "20210821 13:07"
        },
        {
          "id": "MDRW2100003869.1.6",
          "form": "저도 아직",
          "original_form": "저도 아직",
          "speaker_id": "1",
          "time": "20210821 13:07"
        },
        {
          "id": "MDRW2100003869.1.7",
          "form": "어떤동물 좋아하세요?",
          "original_form": "어떤동물 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 13:08"
        },
        {
          "id": "MDRW2100003869.1.8",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210821 13:09"
        },
        {
          "id": "MDRW2100003869.1.9",
          "form": "저도 강아지 좋아해요 비숑 키워보고 싶어요",
          "original_form": "저도 강아지 좋아해요 비숑 키워보고 싶어요",
          "speaker_id": "2",
          "time": "20210821 13:09"
        },
        {
          "id": "MDRW2100003869.1.10",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210821 13:10"
        },
        {
          "id": "MDRW2100003869.1.11",
          "form": "변덕쟁이 아니에요?",
          "original_form": "변덕쟁이 아니에요?",
          "speaker_id": "1",
          "time": "20210821 13:10"
        },
        {
          "id": "MDRW2100003869.1.12",
          "form": "왜요?ㅎ 비숑도 강이지인데요",
          "original_form": "왜요?ㅎ 비숑도 강이지인데요",
          "speaker_id": "2",
          "time": "20210821 13:11"
        },
        {
          "id": "MDRW2100003869.1.13",
          "form": "비숑 성격이... 아닌가요?",
          "original_form": "비숑 성격이... 아닌가요?",
          "speaker_id": "1",
          "time": "20210821 13:13"
        },
        {
          "id": "MDRW2100003869.1.14",
          "form": "아 비숑 성격이 안좋다고는하더라구요ㅎ",
          "original_form": "아 비숑 성격이 안좋다고는하더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210821 13:13"
        }
