          "speaker_id": "2",
          "time": "20210905 16:47"
        },
        {
          "id": "MDRW2100041080.1.5",
          "form": "점저 닭도리탕",
          "original_form": "점저 닭도리탕",
          "speaker_id": "1",
          "time": "20210905 16:49"
        },
        {
          "id": "MDRW2100041080.1.6",
          "form": "간식은??",
          "original_form": "간식은??",
          "speaker_id": "2",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100041080.1.7",
          "form": "저꽈배기",
          "original_form": "저꽈배기",
          "speaker_id": "2",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100041080.1.8",
          "form": "헐 꽈배기",
          "original_form": "헐 꽈배기",
          "speaker_id": "1",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041080.1.9",
          "form": "do you know",
          "original_form": "do you know",
          "speaker_id": "1",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041080.1.10",
          "form": "영천시장?",
          "original_form": "영천시장?",
          "speaker_id": "1",
          "time": "20210905 16:54"
        },
        {
          "id": "MDRW2100041080.1.11",
          "form": "대박 알죠 옛날 그 근처 살앗음",
          "original_form": "대박 알죠 옛날 그 근처 살앗음",
          "speaker_id": "2",
          "time": "20210905 16:56"
        },
        {
          "id": "MDRW2100041080.1.12",
          "form": "여긴 3개 2000원 찹쌀꽈배기",
          "original_form": "여긴 3개 2000원 찹쌀꽈배기",
          "speaker_id": "2",
          "time": "20210905 16:57"
        },
        {
          "id": "MDRW2100041080.1.13",
          "form": "우왕...부러워요 꽈세권 ㅠ",
          "original_form": "우왕...부러워요 꽈세권 ㅠ",
          "speaker_id": "1",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100041080.1.14",
          "form": "함 오시죠 대전 터미널 바로근처요",
          "original_form": "함 오시죠 대전 터미널 바로근처요",
          "speaker_id": "2",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100041080.1.15",
          "form": "갈게요 같이 드시죠",
          "original_form": "갈게요 같이 드시죠",
          "speaker_id": "1",
          "time": "20210905 17:00"
        },
        {
          "id": "MDRW2100041080.1.16",
          "form": "좋죠 찹쌀도넛도 잇어요",
          "original_form": "좋죠 찹쌀도넛도 잇어요",
          "speaker_id": "2",
          "time": "20210905 17:01"
        },
        {
          "id": "MDRW2100041080.1.17",
          "form": "우왕 ㅜ 시장인가요 거기?",
          "original_form": "우왕 ㅜ 시장인가요 거기?",
          "speaker_id": "1",
          "time": "20210905 17:04"
        },
        {
          "id": "MDRW2100041080.1.18",
          "form": "아뇨 골목 가게요",
          "original_form": "아뇨 골목 가게요",
