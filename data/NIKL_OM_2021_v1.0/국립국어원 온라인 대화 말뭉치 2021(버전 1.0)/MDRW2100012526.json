      "utterance": [
        {
          "id": "MDRW2100012526.1.1",
          "form": "아빠 언제와?",
          "original_form": "아빠 언제와?",
          "speaker_id": "1",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100012526.1.2",
          "form": "아빠 요즘 일이 바빠",
          "original_form": "아빠 요즘 일이 바빠",
          "speaker_id": "2",
          "time": "20210824 22:48"
        },
        {
          "id": "MDRW2100012526.1.3",
          "form": "무슨 일 때문에?",
          "original_form": "무슨 일 때문에?",
          "speaker_id": "1",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012526.1.4",
          "form": "계약하러 다녀야 해",
          "original_form": "계약하러 다녀야 해",
          "speaker_id": "2",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012526.1.5",
          "form": "그래도 코로나때문에 요즘은 해외는 안가지?",
          "original_form": "그래도 코로나때문에 요즘은 해외는 안가지?",
          "speaker_id": "1",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012526.1.6",
          "form": "한동안 안가다가 다음달에는 미국 가야해",
          "original_form": "한동안 안가다가 다음달에는 미국 가야해",
          "speaker_id": "2",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012526.1.7",
          "form": "미국?",
          "original_form": "미국?",
          "speaker_id": "1",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012526.1.8",
          "form": "위험하지 않아?",
          "original_form": "위험하지 않아?",
          "speaker_id": "1",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100012526.1.9",
          "form": "백신도 맞았고 일도 밀려서 어쩔수없지",
          "original_form": "백신도 맞았고 일도 밀려서 어쩔수없지",
          "speaker_id": "2",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012526.1.10",
          "form": "갔다가 언제와?",
          "original_form": "갔다가 언제와?",
          "speaker_id": "1",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012526.1.11",
          "form": "일주일정도  걸려",
          "original_form": "일주일정도  걸려",
          "speaker_id": "2",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012526.1.12",
          "form": "그래도 오래있지는 않네",
          "original_form": "그래도 오래있지는 않네",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100012526.1.13",
          "form": "그래 다행이지",
          "original_form": "그래 다행이지",
          "speaker_id": "2",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100012526.1.14",
          "form": "맞아 그럼 아빠혼자가?",
          "original_form": "맞아 그럼 아빠혼자가?",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100012526.1.15",
          "form": "사장이랑 둘이 간대",
          "original_form": "사장이랑 둘이 간대",
          "speaker_id": "2",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100012526.1.16",
          "form": "스트레스 엄청 받겟네",
          "original_form": "스트레스 엄청 받겟네",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100012526.1.17",
          "form": "그러게 그래도 방은 따로 잡는대",
          "original_form": "그러게 그래도 방은 따로 잡는대",
          "speaker_id": "2",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012526.1.18",
          "form": "그건 다행이네",
          "original_form": "그건 다행이네",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012526.1.19",
          "form": "이번에는 뭐때문에 가는거야?",
          "original_form": "이번에는 뭐때문에 가는거야?",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012526.1.20",
          "form": "전시회 둘러보고 수입해올거 있나 체크해야지",
          "original_form": "전시회 둘러보고 수입해올거 있나 체크해야지",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012526.1.21",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012526.1.22",
          "form": "바쁘겠네",
          "original_form": "바쁘겠네",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012526.1.23",
          "form": "업무 끝나면 휴가준대",
          "original_form": "업무 끝나면 휴가준대",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012526.1.24",
          "form": "정말?",
          "original_form": "정말?",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012526.1.25",
          "form": "왠일이야",
          "original_form": "왠일이야",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012526.1.26",
          "form": "그러게 그동안 휴일도 없더니",
          "original_form": "그러게 그동안 휴일도 없더니",
          "speaker_id": "2",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100012526.1.27",
          "form": "그러니까 얼마만이야 이게",
          "original_form": "그러니까 얼마만이야 이게",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100012526.1.28",
          "form": "휴일 없는 대신 복지가 좋긴하지",
          "original_form": "휴일 없는 대신 복지가 좋긴하지",
          "speaker_id": "2",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100012526.1.29",
          "form": "무슨 복지?",
          "original_form": "무슨 복지?",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100012526.1.30",
          "form": "휴대폰 요금이랑 자동차 지원해주잖아",
          "original_form": "휴대폰 요금이랑 자동차 지원해주잖아",
          "speaker_id": "2",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100012526.1.31",
          "form": "그런점은 좋네",
          "original_form": "그런점은 좋네",
          "speaker_id": "1",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100012526.1.32",
          "form": "엄마는 왜 가정주부해?",
          "original_form": "엄마는 왜 가정주부해?",
          "speaker_id": "1",
          "time": "20210826 03:24"
        }
      ]
    }
