      "utterance": [
        {
          "id": "MDRW2100023489.1.1",
          "form": "직장  만족하세요?",
          "original_form": "직장  만족하세요?",
          "speaker_id": "1",
          "time": "20210830 09:06"
        },
        {
          "id": "MDRW2100023489.1.2",
          "form": "오 첨해봐서 몰랐네요",
          "original_form": "오 첨해봐서 몰랐네요",
          "speaker_id": "2",
          "time": "20210830 09:06"
        },
        {
          "id": "MDRW2100023489.1.3",
          "form": "아니요ㅋㅋ",
          "original_form": "아니요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:06"
        },
        {
          "id": "MDRW2100023489.1.4",
          "form": "오늘 퇴사해요ㅋㅋ",
          "original_form": "오늘 퇴사해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:06"
        },
        {
          "id": "MDRW2100023489.1.5",
          "form": "오늘요?",
          "original_form": "오늘요?",
          "speaker_id": "1",
          "time": "20210830 09:06"
        },
        {
          "id": "MDRW2100023489.1.6",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210830 09:06"
        },
        {
          "id": "MDRW2100023489.1.7",
          "form": "네ㅋㅋ 내일 연차로 해서 오늘 끝이예요",
          "original_form": "네ㅋㅋ 내일 연차로 해서 오늘 끝이예요",
          "speaker_id": "2",
          "time": "20210830 09:07"
        },
        {
          "id": "MDRW2100023489.1.8",
          "form": "왜 그만두세요?",
          "original_form": "왜 그만두세요?",
          "speaker_id": "1",
          "time": "20210830 09:07"
        },
        {
          "id": "MDRW2100023489.1.9",
          "form": "사무직인데 생산직 일을 가서 하게 되는 경우가 꽤 있었고",
          "original_form": "사무직인데 생산직 일을 가서 하게 되는 경우가 꽤 있었고",
          "speaker_id": "2",
          "time": "20210830 09:08"
        },
        {
          "id": "MDRW2100023489.1.10",
          "form": "여기서 일하면서 그러다가 오른쪽 손목이 안좋아져서 반년 넘게 병원가게되고",
          "original_form": "여기서 일하면서 그러다가 오른쪽 손목이 안좋아져서 반년 넘게 병원가게되고",
          "speaker_id": "2",
          "time": "20210830 09:09"
        },
        {
          "id": "MDRW2100023489.1.11",
          "form": "일은 생산직일..",
          "original_form": "일은 생산직일..",
          "speaker_id": "2",
          "time": "20210830 09:09"
        },
        {
          "id": "MDRW2100023489.1.12",
          "form": "퇴사율이 높아서 저도 2년 겨우 채우고 나가거든요",
          "original_form": "퇴사율이 높아서 저도 2년 겨우 채우고 나가거든요",
          "speaker_id": "2",
          "time": "20210830 09:10"
        },
        {
          "id": "MDRW2100023489.1.13",
          "form": "아 힘드셨겼네요",
          "original_form": "아 힘드셨겼네요",
          "speaker_id": "1",
          "time": "20210830 09:15"
        },
        {
          "id": "MDRW2100023489.1.14",
          "form": "이직자리는 구하셨어요?",
          "original_form": "이직자리는 구하셨어요?",
          "speaker_id": "1",
          "time": "20210830 09:15"
        },
        {
          "id": "MDRW2100023489.1.15",
          "form": "새벽 퇴근이 가장 힘들었어요",
          "original_form": "새벽 퇴근이 가장 힘들었어요",
          "speaker_id": "2",
          "time": "20210830 09:16"
        },
        {
          "id": "MDRW2100023489.1.16",
          "form": "퇴사겸 이사도 하고 있어서 이직자리 구하는데 쉽진 않네요",
          "original_form": "퇴사겸 이사도 하고 있어서 이직자리 구하는데 쉽진 않네요",
          "speaker_id": "2",
          "time": "20210830 09:17"
        },
        {
          "id": "MDRW2100023489.1.17",
          "form": "뭉댕님은 직장 어떠세요?",
          "original_form": "뭉댕님은 직장 어떠세요?",
          "speaker_id": "2",
          "time": "20210830 09:17"
        },
        {
          "id": "MDRW2100023489.1.18",
          "form": "저는 그냥 만족하고 다니고 있어요",
          "original_form": "저는 그냥 만족하고 다니고 있어요",
          "speaker_id": "1",
          "time": "20210830 09:18"
        },
        {
          "id": "MDRW2100023489.1.19",
          "form": "휴식을 취할 계획인가요?",
          "original_form": "휴식을 취할 계획인가요?",
          "speaker_id": "1",
          "time": "20210830 09:18"
        },
        {
          "id": "MDRW2100023489.1.20",
          "form": "그렇군요 그게 가장 좋죠",
          "original_form": "그렇군요 그게 가장 좋죠",
          "speaker_id": "2",
          "time": "20210830 09:18"
        },
        {
          "id": "MDRW2100023489.1.21",
          "form": "되도록 취업을 빨리 하려고 있어요",
          "original_form": "되도록 취업을 빨리 하려고 있어요",
          "speaker_id": "2",
          "time": "20210830 09:19"
        },
        {
          "id": "MDRW2100023489.1.22",
          "form": "휴식 취하면 한도끝도 없더라고요",
          "original_form": "휴식 취하면 한도끝도 없더라고요",
          "speaker_id": "2",
          "time": "20210830 09:19"
        },
        {
          "id": "MDRW2100023489.1.23",
          "form": "아 좋은 결과 있기를 바랍니다",
          "original_form": "아 좋은 결과 있기를 바랍니다",
          "speaker_id": "1",
          "time": "20210830 09:22"
        },
        {
          "id": "MDRW2100023489.1.24",
          "form": "손목은 괜찮아 졌나요?",
          "original_form": "손목은 괜찮아 졌나요?",
          "speaker_id": "1",
          "time": "20210830 09:22"
        },
        {
          "id": "MDRW2100023489.1.25",
          "form": "네 저도 오래 일할 곳을 다니고 싶어요",
          "original_form": "네 저도 오래 일할 곳을 다니고 싶어요",
          "speaker_id": "2",
          "time": "20210830 09:23"
        },
        {
          "id": "MDRW2100023489.1.26",
          "form": "가끔씩 아프더라고요... 질병 추가된거죠",
          "original_form": "가끔씩 아프더라고요... 질병 추가된거죠",
          "speaker_id": "2",
          "time": "20210830 09:23"
        },
        {
          "id": "MDRW2100023489.1.27",
          "form": "산재처리 되나요?",
          "original_form": "산재처리 되나요?",
          "speaker_id": "1",
          "time": "20210830 09:23"
        },
        {
          "id": "MDRW2100023489.1.28",
          "form": "여긴 바랄걸 바래야지란 생각에 실비처리하고 말았어요",
          "original_form": "여긴 바랄걸 바래야지란 생각에 실비처리하고 말았어요",
          "speaker_id": "2",
          "time": "20210830 09:24"
        },
        {
          "id": "MDRW2100023489.1.29",
          "form": "제 일로 그런게 아니고 반복된 작업에 그런거라서요",
          "original_form": "제 일로 그런게 아니고 반복된 작업에 그런거라서요",
          "speaker_id": "2",
          "time": "20210830 09:25"
        },
        {
          "id": "MDRW2100023489.1.30",
          "form": "음 실업수당은 청구하시죠?",
          "original_form": "음 실업수당은 청구하시죠?",
          "speaker_id": "1",
          "time": "20210830 09:26"
        },
        {
          "id": "MDRW2100023489.1.31",
          "form": "실업수당은 계약직인경우나 가능한걸로 알고있어요",
          "original_form": "실업수당은 계약직인경우나 가능한걸로 알고있어요",
          "speaker_id": "2",
          "time": "20210830 09:26"
        },
        {
          "id": "MDRW2100023489.1.32",
          "form": "아니면 결혼으로 거주지가 멀어진경우등등\n해당 안될거예요 저는ㅋㅋ",
          "original_form": "아니면 결혼으로 거주지가 멀어진경우등등\n해당 안될거예요 저는ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:26"
        },
        {
          "id": "MDRW2100023489.1.33",
          "form": "정규직도 가능하지 않나요",
          "original_form": "정규직도 가능하지 않나요",
          "speaker_id": "1",
          "time": "20210830 09:26"
        },
        {
          "id": "MDRW2100023489.1.34",
          "form": "그래돛알아보세요",
          "original_form": "그래돛알아보세요",
          "speaker_id": "1",
          "time": "20210830 09:27"
        },
        {
          "id": "MDRW2100023489.1.35",
          "form": "액수가 꽤되는데",
          "original_form": "액수가 꽤되는데",
          "speaker_id": "1",
          "time": "20210830 09:27"
        }
      ]
    }
