      "utterance": [
        {
          "id": "MDRW2100000722.1.1",
          "form": "하고 있는 온라인 커뮤니티가 있나용?",
          "original_form": "하고 있는 온라인 커뮤니티가 있나용?",
          "speaker_id": "1",
          "time": "20210819 18:43"
        },
        {
          "id": "MDRW2100000722.1.2",
          "form": "네 다양하게 해요",
          "original_form": "네 다양하게 해요",
          "speaker_id": "2",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000722.1.3",
          "form": "볶음밥님은 있으신가요?",
          "original_form": "볶음밥님은 있으신가요?",
          "speaker_id": "2",
          "time": "20210819 18:48"
        },
        {
          "id": "MDRW2100000722.1.4",
          "form": "네 저도 다양하게 하는 편이에요!",
          "original_form": "네 저도 다양하게 하는 편이에요!",
          "speaker_id": "1",
          "time": "20210819 19:33"
        },
        {
          "id": "MDRW2100000722.1.5",
          "form": "그냥 친목 커뮤니티도 재밌고 같이 좋아하는 관심사가 같은 사람끼리 모이는 것도 재밌더라구용",
          "original_form": "그냥 친목 커뮤니티도 재밌고 같이 좋아하는 관심사가 같은 사람끼리 모이는 것도 재밌더라구용",
          "speaker_id": "1",
          "time": "20210819 19:33"
        },
        {
          "id": "MDRW2100000722.1.6",
          "form": "맞아여 그럼 사람들하고 이야기 하면 재밌죠",
          "original_form": "맞아여 그럼 사람들하고 이야기 하면 재밌죠",
          "speaker_id": "2",
          "time": "20210819 19:36"
        },
        {
          "id": "MDRW2100000722.1.7",
          "form": "실제로 정모도 해보셨어요?",
          "original_form": "실제로 정모도 해보셨어요?",
          "speaker_id": "2",
          "time": "20210819 19:36"
        },
        {
          "id": "MDRW2100000722.1.8",
          "form": "어 아뇨 실제로 정모는 못해봤어요",
          "original_form": "어 아뇨 실제로 정모는 못해봤어요",
          "speaker_id": "1",
          "time": "20210819 19:37"
        },
        {
          "id": "MDRW2100000722.1.9",
          "form": "만나면 재밌을거 같긴 해요ㅠㅠ",
          "original_form": "만나면 재밌을거 같긴 해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 19:37"
        },
        {
          "id": "MDRW2100000722.1.10",
          "form": "커뮤에서 정모는 보통 안좋아서 그런지,,",
          "original_form": "커뮤에서 정모는 보통 안좋아서 그런지,,",
          "speaker_id": "2",
          "time": "20210819 19:38"
        },
        {
          "id": "MDRW2100000722.1.11",
          "form": "마음 맞는 사람들끼리 만나서 정모하는건 좋을 거 같아요",
          "original_form": "마음 맞는 사람들끼리 만나서 정모하는건 좋을 거 같아요",
          "speaker_id": "2",
          "time": "20210819 19:38"
        },
        {
          "id": "MDRW2100000722.1.12",
          "form": "그쵸? 커뮤에서 정모하면 아무래도",
          "original_form": "그쵸? 커뮤에서 정모하면 아무래도",
          "speaker_id": "1",
          "time": "20210819 19:56"
        },
        {
          "id": "MDRW2100000722.1.13",
          "form": "친한 사람들이 생겨서 그런가..",
          "original_form": "친한 사람들이 생겨서 그런가..",
          "speaker_id": "1",
          "time": "20210819 19:56"
        },
        {
          "id": "MDRW2100000722.1.14",
          "form": "네 그럴 거에요 소외감도 들고 그런다고 하더라구요",
          "original_form": "네 그럴 거에요 소외감도 들고 그런다고 하더라구요",
          "speaker_id": "2",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000722.1.15",
          "form": "그래도 만나는 사람들은 있을 거 같아요",
          "original_form": "그래도 만나는 사람들은 있을 거 같아요",
          "speaker_id": "2",
          "time": "20210819 20:00"
        },
        {
          "id": "MDRW2100000722.1.16",
          "form": "어 맞아요 아무래도 콘서트나 스포츠경기나 이런데선",
          "original_form": "어 맞아요 아무래도 콘서트나 스포츠경기나 이런데선",
          "speaker_id": "1",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000722.1.17",
          "form": "동선 겹쳐서 어쩔 수 없이 보는거 같기도 하구용",
          "original_form": "동선 겹쳐서 어쩔 수 없이 보는거 같기도 하구용",
          "speaker_id": "1",
          "time": "20210819 20:01"
        },
        {
          "id": "MDRW2100000722.1.18",
          "form": "아 맞아요 ㅎ ㅎㅎㅎ",
          "original_form": "아 맞아요 ㅎ ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:03"
        },
        {
          "id": "MDRW2100000722.1.19",
          "form": "생각해보니 저도 콘서트 때문에 커뮤에서 만난적이 있네요",
          "original_form": "생각해보니 저도 콘서트 때문에 커뮤에서 만난적이 있네요",
          "speaker_id": "2",
          "time": "20210819 20:04"
        },
        {
          "id": "MDRW2100000722.1.20",
          "form": "오 진짜요?",
          "original_form": "오 진짜요?",
          "speaker_id": "1",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000722.1.21",
          "form": "어색하거나 하진 않으셨어요?",
          "original_form": "어색하거나 하진 않으셨어요?",
          "speaker_id": "1",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000722.1.22",
          "form": "처음엔 어색했는데 관심사가 같다보니깐",
          "original_form": "처음엔 어색했는데 관심사가 같다보니깐",
          "speaker_id": "2",
          "time": "20210819 21:05"
        },
        {
          "id": "MDRW2100000722.1.23",
          "form": "금방 풀리더라구요",
          "original_form": "금방 풀리더라구요",
          "speaker_id": "2",
          "time": "20210819 21:05"
        },
        {
          "id": "MDRW2100000722.1.24",
          "form": "오 근데 그럴거같아요",
          "original_form": "오 근데 그럴거같아요",
          "speaker_id": "1",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000722.1.25",
          "form": "어색한 사이엔 대화소재 찾는게 힘드니까ㅠㅠ",
          "original_form": "어색한 사이엔 대화소재 찾는게 힘드니까ㅠㅠ",
          "speaker_id": "1",
