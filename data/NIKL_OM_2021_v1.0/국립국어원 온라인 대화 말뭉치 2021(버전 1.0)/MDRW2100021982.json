      "utterance": [
        {
          "id": "MDRW2100021982.1.1",
          "form": "엉 구관!!",
          "original_form": "엉 구관!!",
          "speaker_id": "2",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100021982.1.2",
          "form": "재료비 많이 나오는거 아냐?",
          "original_form": "재료비 많이 나오는거 아냐?",
          "speaker_id": "1",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100021982.1.3",
          "form": "그것보단 퀄리티차이일듯",
          "original_form": "그것보단 퀄리티차이일듯",
          "speaker_id": "2",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100021982.1.4",
          "form": "옷 디자인은 누가해",
          "original_form": "옷 디자인은 누가해",
          "speaker_id": "1",
          "time": "20210829 00:26"
        },
        {
          "id": "MDRW2100021982.1.5",
          "form": "너가 디자인하고 만들고 나는팔고 (?)",
          "original_form": "너가 디자인하고 만들고 나는팔고 (?)",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021982.1.6",
          "form": "나도 가끔 디자인하고^^",
          "original_form": "나도 가끔 디자인하고^^",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021982.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ 미싱 잇어야 하는거 아냐?",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 미싱 잇어야 하는거 아냐?",
          "speaker_id": "1",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021982.1.8",
          "form": "미싱있어야지!!",
          "original_form": "미싱있어야지!!",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021982.1.9",
          "form": "디자인은 니가 해야지",
          "original_form": "디자인은 니가 해야지",
          "speaker_id": "1",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021982.1.10",
          "form": "옷감각은 니가 더 좋으니까",
          "original_form": "옷감각은 니가 더 좋으니까",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021982.1.11",
          "form": "미싱 다룰줄 모르는데여ㅋㅋㅋ",
          "original_form": "미싱 다룰줄 모르는데여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021982.1.12",
          "form": "손재주잇으니까 미싱도 금방 배울듯!!",
          "original_form": "손재주잇으니까 미싱도 금방 배울듯!!",
          "speaker_id": "2",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100021982.1.13",
          "form": "가즈아ㅏㅏ!!",
          "original_form": "가즈아ㅏㅏ!!",
          "speaker_id": "2",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100021982.1.14",
          "form": "악세서리도 괜찮",
          "original_form": "악세서리도 괜찮",
          "speaker_id": "2",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100021982.1.15",
          "form": "님은 머냐고 유통이냐고 왜 직종을 바꾸냐고",
          "original_form": "님은 머냐고 유통이냐고 왜 직종을 바꾸냐고",
          "speaker_id": "1",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100021982.1.16",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100021982.1.17",
          "form": "사이트만들어줄게",
          "original_form": "사이트만들어줄게",
          "speaker_id": "2",
          "time": "20210829 00:28"
        },
        {
          "id": "MDRW2100021982.1.18",
          "form": "옷만들기가 쉽지않아",
          "original_form": "옷만들기가 쉽지않아",
          "speaker_id": "1",
          "time": "20210829 00:29"
        },
        {
          "id": "MDRW2100021982.1.19",
          "form": "우리 전에 만들어봤잖아 그걸로 돈벌기가 쉽지않을걸...",
          "original_form": "우리 전에 만들어봤잖아 그걸로 돈벌기가 쉽지않을걸...",
