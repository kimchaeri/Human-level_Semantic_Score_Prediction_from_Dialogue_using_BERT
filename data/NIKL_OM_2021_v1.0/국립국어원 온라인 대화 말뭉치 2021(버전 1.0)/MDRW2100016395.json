      "utterance": [
        {
          "id": "MDRW2100016395.1.1",
          "form": "전 먹는걸 진짜 좋아해서",
          "original_form": "전 먹는걸 진짜 좋아해서",
          "speaker_id": "2",
          "time": "20210826 12:29"
        },
        {
          "id": "MDRW2100016395.1.2",
          "form": "먹는생각하면 기분이 좋아져요",
          "original_form": "먹는생각하면 기분이 좋아져요",
          "speaker_id": "2",
          "time": "20210826 12:29"
        },
        {
          "id": "MDRW2100016395.1.3",
          "form": "리얼루",
          "original_form": "리얼루",
          "speaker_id": "2",
          "time": "20210826 12:29"
        },
        {
          "id": "MDRW2100016395.1.4",
          "form": "ㅋㅋㅋㅋㅋ간장게장!!",
          "original_form": "ㅋㅋㅋㅋㅋ간장게장!!",
          "speaker_id": "1",
          "time": "20210826 12:30"
        },
        {
          "id": "MDRW2100016395.1.5",
          "form": "간장게장 맛집 가봤어요?",
          "original_form": "간장게장 맛집 가봤어요?",
          "speaker_id": "1",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016395.1.6",
          "form": "예전에 맛집은 아니구",
          "original_form": "예전에 맛집은 아니구",
          "speaker_id": "2",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016395.1.7",
          "form": "안면도 가서",
          "original_form": "안면도 가서",
          "speaker_id": "2",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016395.1.8",
          "form": "게꾹지랑 간장게장이랑 엄청먹었어요",
          "original_form": "게꾹지랑 간장게장이랑 엄청먹었어요",
          "speaker_id": "2",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016395.1.9",
          "form": "우와....안면도...",
          "original_form": "우와....안면도...",
          "speaker_id": "1",
          "time": "20210826 12:31"
        },
        {
          "id": "MDRW2100016395.1.10",
          "form": "딱새우장 먹어보셨어요?",
          "original_form": "딱새우장 먹어보셨어요?",
          "speaker_id": "2",
          "time": "20210826 12:32"
        },
        {
          "id": "MDRW2100016395.1.11",
          "form": "아뇨ㅜ 새우장 안먹어봤어요",
          "original_form": "아뇨ㅜ 새우장 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100016395.1.12",
          "form": "딱새우장은 식감이 달라요?",
          "original_form": "딱새우장은 식감이 달라요?",
          "speaker_id": "1",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100016395.1.13",
          "form": "더 탱글탱글한?",
          "original_form": "더 탱글탱글한?",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016395.1.14",
          "form": "나름 또 맛이 좀 달라요ㅋㅋ",
          "original_form": "나름 또 맛이 좀 달라요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:50"
        },
        {
          "id": "MDRW2100016395.1.15",
          "form": "맛있겠다....ㅜ",
          "original_form": "맛있겠다....ㅜ",
          "speaker_id": "1",
          "time": "20210826 14:52"
        },
        {
          "id": "MDRW2100016395.1.16",
          "form": "점심은 뭐먹었어요",
          "original_form": "점심은 뭐먹었어요",
          "speaker_id": "2",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016395.1.17",
          "form": "점심 미역죽...?",
          "original_form": "점심 미역죽...?",
          "speaker_id": "1",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016395.1.18",
          "form": "국이 아니라 죽이에요...",
          "original_form": "국이 아니라 죽이에요...",
          "speaker_id": "1",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016395.1.19",
          "form": "어제 체한거 아직 그래요?",
          "original_form": "어제 체한거 아직 그래요?",
          "speaker_id": "2",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100016395.1.20",
          "form": "체했으니까 위가 상해서 죽먹어야해요ㅋㅋ",
          "original_form": "체했으니까 위가 상해서 죽먹어야해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100016395.1.21",
          "form": "ㅜㅜ 미지근한 물 많이 마셔요",
          "original_form": "ㅜㅜ 미지근한 물 많이 마셔요",
          "speaker_id": "2",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016395.1.22",
          "form": "이 참에 다이어트가 되는건가ㅋㅋㅋ",
          "original_form": "이 참에 다이어트가 되는건가ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016395.1.23",
          "form": "으르신... 죽어가는걸 다이어트라고 부르진 않습니다...",
          "original_form": "으르신... 죽어가는걸 다이어트라고 부르진 않습니다...",
          "speaker_id": "2",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016395.1.24",
          "form": "ㅋㅋㅋㅋ그렇지 않아요... 그렇게까지 심각한 건 아닌데...",
          "original_form": "ㅋㅋㅋㅋ그렇지 않아요... 그렇게까지 심각한 건 아닌데...",
          "speaker_id": "1",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100016395.1.25",
          "form": "긍정적으로 생각하는거죠!",
          "original_form": "긍정적으로 생각하는거죠!",
          "speaker_id": "1",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100016395.1.26",
          "form": "얼른 나아서 똠얌꿍 먹고싶어요",
          "original_form": "얼른 나아서 똠얌꿍 먹고싶어요",
          "speaker_id": "1",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100016395.1.27",
          "form": "어우 속 안좋은데 똠얌꿍을 생각하시다니",
          "original_form": "어우 속 안좋은데 똠얌꿍을 생각하시다니",
          "speaker_id": "2",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100016395.1.28",
          "form": "아주 독하신데요",
          "original_form": "아주 독하신데요",
          "speaker_id": "2",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100016395.1.29",
          "form": "똠얌꿍 싫어해요?",
          "original_form": "똠얌꿍 싫어해요?",
          "speaker_id": "1",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016395.1.30",
          "form": "먹으면 속 좀 쓰리지 않아요?",
          "original_form": "먹으면 속 좀 쓰리지 않아요?",
          "speaker_id": "2",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016395.1.31",
          "form": "셔서 그런가...",
          "original_form": "셔서 그런가...",
          "speaker_id": "1",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016395.1.32",
          "form": "전 그 맛에 먹는건데",
          "original_form": "전 그 맛에 먹는건데",
          "speaker_id": "1",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016395.1.33",
          "form": "새콤한 맛",
          "original_form": "새콤한 맛",
          "speaker_id": "1",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016395.1.34",
          "form": "새콤한 맛이 매력인 똠얌꿍....",
          "original_form": "새콤한 맛이 매력인 똠얌꿍....",
          "speaker_id": "1",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016395.1.35",
          "form": "찐맛집은 아는데 너무 비싸요",
          "original_form": "찐맛집은 아는데 너무 비싸요",
          "speaker_id": "1",
          "time": "20210826 15:59"
        }
      ]
    }
