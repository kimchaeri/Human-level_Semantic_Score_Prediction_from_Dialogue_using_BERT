      "utterance": [
        {
          "id": "MDRW2100021706.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 21:46"
        },
        {
          "id": "MDRW2100021706.1.2",
          "form": "영화 즐겨보세요?",
          "original_form": "영화 즐겨보세요?",
          "speaker_id": "1",
          "time": "20210828 21:51"
        },
        {
          "id": "MDRW2100021706.1.3",
          "form": "즐겨보는편은 아니고 가끔 봐요",
          "original_form": "즐겨보는편은 아니고 가끔 봐요",
          "speaker_id": "2",
          "time": "20210828 21:52"
        },
        {
          "id": "MDRW2100021706.1.4",
          "form": "최근 뭐보셨어요?",
          "original_form": "최근 뭐보셨어요?",
          "speaker_id": "1",
          "time": "20210828 21:53"
        },
        {
          "id": "MDRW2100021706.1.5",
          "form": "모가디슈 였던가?\r\n그거 봤었어요",
          "original_form": "모가디슈 였던가?\r\n그거 봤었어요",
          "speaker_id": "2",
          "time": "20210828 21:55"
        },
        {
          "id": "MDRW2100021706.1.6",
          "form": "재밌엇나요?",
          "original_form": "재밌엇나요?",
          "speaker_id": "1",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021706.1.7",
          "form": "괜찮게 봤어요",
          "original_form": "괜찮게 봤어요",
          "speaker_id": "2",
          "time": "20210828 22:07"
        },
        {
          "id": "MDRW2100021706.1.8",
          "form": "평도 괜찮더라구요",
          "original_form": "평도 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021706.1.9",
          "form": "오 그렇군요ㅎ 전 최근 크루엘라라는 영화를 봤어요",
          "original_form": "오 그렇군요ㅎ 전 최근 크루엘라라는 영화를 봤어요",
          "speaker_id": "1",
          "time": "20210828 22:19"
        },
        {
          "id": "MDRW2100021706.1.10",
          "form": "오 그 영화 보신 분 많더라구요!",
          "original_form": "오 그 영화 보신 분 많더라구요!",
          "speaker_id": "2",
          "time": "20210828 22:21"
        },
        {
          "id": "MDRW2100021706.1.11",
          "form": "네 저는 그럭저럭 무난하게 봤어요 의류업계관련 영화를 잘 봐서요ㅎ",
          "original_form": "네 저는 그럭저럭 무난하게 봤어요 의류업계관련 영화를 잘 봐서요ㅎ",
          "speaker_id": "1",
          "time": "20210828 22:24"
        },
        {
          "id": "MDRW2100021706.1.12",
          "form": "의류쪽으로 관심이 많으신가봐요",
          "original_form": "의류쪽으로 관심이 많으신가봐요",
          "speaker_id": "2",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100021706.1.13",
          "form": "그냥 의상 예쁜걸 좋아해서요ㅋㅋ",
          "original_form": "그냥 의상 예쁜걸 좋아해서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100021706.1.14",
          "form": "예쁜 의상들 보면 관심이 자연스레 가죠",
          "original_form": "예쁜 의상들 보면 관심이 자연스레 가죠",
