      "utterance": [
        {
          "id": "MDRW2100015233.1.1",
          "form": "유튜브는 예전에 많이 봤는데",
          "original_form": "유튜브는 예전에 많이 봤는데",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015233.1.2",
          "form": "요즘 광고 때문에 보기 싫어져서 잘 안봐요..",
          "original_form": "요즘 광고 때문에 보기 싫어져서 잘 안봐요..",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015233.1.3",
          "form": "아 맞아요 ㅋㅋㅋ",
          "original_form": "아 맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015233.1.4",
          "form": "저는 동물나오는거",
          "original_form": "저는 동물나오는거",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015233.1.5",
          "form": "하하하 이런거 좋아해요",
          "original_form": "하하하 이런거 좋아해요",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015233.1.6",
          "form": "애니멀봐?",
          "original_form": "애니멀봐?",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015233.1.7",
          "form": "근데 유투브 광고심하죠",
          "original_form": "근데 유투브 광고심하죠",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015233.1.8",
          "form": "애니멀봐 좋아해요!!!",
          "original_form": "애니멀봐 좋아해요!!!",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015233.1.9",
          "form": "애니멀봐 귀여워요 ㅋㅋ",
          "original_form": "애니멀봐 귀여워요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015233.1.10",
          "form": "저는 아이티나 다이어트 관련 영상 봐요~",
          "original_form": "저는 아이티나 다이어트 관련 영상 봐요~",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015233.1.11",
          "form": "맞아요 동물농장이런거재밓더라구요",
          "original_form": "맞아요 동물농장이런거재밓더라구요",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015233.1.12",
          "form": "다이어트 식단 이런거 참고할거 많더라고요",
          "original_form": "다이어트 식단 이런거 참고할거 많더라고요",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015233.1.13",
          "form": "아 동물농장 귀여워요 ㅜ 볼 때마다 저도 개 키우고 싶네요.",
          "original_form": "아 동물농장 귀여워요 ㅜ 볼 때마다 저도 개 키우고 싶네요.",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015233.1.14",
          "form": "오호 .. 저는 이론만 봐요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오호 .. 저는 이론만 봐요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015233.1.15",
          "form": "그 바프찍기 몇주전 영상 올리고 이런것도 신기하고",
          "original_form": "그 바프찍기 몇주전 영상 올리고 이런것도 신기하고",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015233.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015233.1.17",
          "form": "혹시 뷰투버도 보세요??",
          "original_form": "혹시 뷰투버도 보세요??",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015233.1.18",
          "form": "어머 뷰튜버면 뷰티 유튜버요?",
          "original_form": "어머 뷰튜버면 뷰티 유튜버요?",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015233.1.19",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015233.1.20",
          "form": "남자라 그냥 아무거나 쳐바릅니다..",
          "original_form": "남자라 그냥 아무거나 쳐바릅니다..",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015233.1.21",
          "form": "예전에 화해? 그거 비교하는 영상이랑 뷰티겟잇? 이런건 본 적 있었어요..",
          "original_form": "예전에 화해? 그거 비교하는 영상이랑 뷰티겟잇? 이런건 본 적 있었어요..",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015233.1.22",
          "form": "아하 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그런거 재밌죠 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그런거 재밌죠 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015233.1.23",
          "form": "뷰티는 화장품 정보 얻긴 괜찮더라구요",
          "original_form": "뷰티는 화장품 정보 얻긴 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015233.1.24",
          "form": "좋아하는 연예인은 있으세요??",
          "original_form": "좋아하는 연예인은 있으세요??",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015233.1.25",
          "form": "가끔 광고 냄새가 나긴 한데..",
          "original_form": "가끔 광고 냄새가 나긴 한데..",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015233.1.26",
          "form": "저 서강준 형 좋아해요",
          "original_form": "저 서강준 형 좋아해요",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015233.1.27",
          "form": "맞아요 유투브광고심해요",
          "original_form": "맞아요 유투브광고심해요",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015233.1.28",
          "form": "아 서강준 진짜 너무 잘생겼죠 ㅠ",
          "original_form": "아 서강준 진짜 너무 잘생겼죠 ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015233.1.29",
          "form": "박보검씨도 좋아요",
          "original_form": "박보검씨도 좋아요",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015233.1.30",
          "form": "아하 전 차은우 좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "아하 전 차은우 좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015233.1.31",
          "form": "한 때 제 롤모델이였어요 박보검님..",
          "original_form": "한 때 제 롤모델이였어요 박보검님..",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015233.1.32",
          "form": "그럴만해요",
          "original_form": "그럴만해요",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015233.1.33",
          "form": "차은우님 인기 진짜 많은 것 같아요 얼굴천재라고..",
          "original_form": "차은우님 인기 진짜 많은 것 같아요 얼굴천재라고..",
          "speaker_id": "2",
          "time": "20210825 22:24"
        }
      ]
