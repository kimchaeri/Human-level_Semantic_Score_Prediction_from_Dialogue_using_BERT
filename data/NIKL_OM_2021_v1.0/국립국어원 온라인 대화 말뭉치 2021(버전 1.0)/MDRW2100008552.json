      "utterance": [
        {
          "id": "MDRW2100008552.1.1",
          "form": "디저트 좋아하시나요?",
          "original_form": "디저트 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 16:39"
        },
        {
          "id": "MDRW2100008552.1.2",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008552.1.3",
          "form": "단 거 최고예용",
          "original_form": "단 거 최고예용",
          "speaker_id": "1",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008552.1.4",
          "form": "저도 너무 좋아해요",
          "original_form": "저도 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008552.1.5",
          "form": "여름에 빙수 많이 드셨나요!",
          "original_form": "여름에 빙수 많이 드셨나요!",
          "speaker_id": "2",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008552.1.6",
          "form": "오 많이...?는 모르겠고 몇번 땡길 때마다 먹긴 했어요!",
          "original_form": "오 많이...?는 모르겠고 몇번 땡길 때마다 먹긴 했어요!",
          "speaker_id": "1",
          "time": "20210822 16:50"
        },
        {
          "id": "MDRW2100008552.1.7",
          "form": "빙수 어디꺼 좋아하세요?",
          "original_form": "빙수 어디꺼 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008552.1.8",
          "form": "저는 우유얼음 너무 좋아해서 설빙 자주먹어요..",
          "original_form": "저는 우유얼음 너무 좋아해서 설빙 자주먹어요..",
          "speaker_id": "2",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008552.1.9",
          "form": "오 저도 설빙을 주로 먹어요",
          "original_form": "오 저도 설빙을 주로 먹어요",
          "speaker_id": "1",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008552.1.10",
          "form": "설빙 진짜 맛있어요!",
          "original_form": "설빙 진짜 맛있어요!",
          "speaker_id": "2",
          "time": "20210822 17:01"
        },
        {
          "id": "MDRW2100008552.1.11",
          "form": "어떤메뉴 주로 드세요",
          "original_form": "어떤메뉴 주로 드세요",
          "speaker_id": "2",
          "time": "20210822 17:01"
        },
        {
          "id": "MDRW2100008552.1.12",
          "form": "저는 오레오초코빙수 좋아해요",
          "original_form": "저는 오레오초코빙수 좋아해요",
          "speaker_id": "1",
          "time": "20210822 17:01"
        },
        {
          "id": "MDRW2100008552.1.13",
          "form": "오 그거 맛있죠 .. 저는 망고치즈빙수 제일 좋아해요",
          "original_form": "오 그거 맛있죠 .. 저는 망고치즈빙수 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008552.1.14",
          "form": "사실 설빙은 다 맛있긴하지만요",
          "original_form": "사실 설빙은 다 맛있긴하지만요",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008552.1.15",
          "form": "오우 맛있겠네요ㅠ",
          "original_form": "오우 맛있겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210822 17:13"
        },
        {
          "id": "MDRW2100008552.1.16",
          "form": "맞아요 .. 인절미토스트도 맛있는데",
          "original_form": "맞아요 .. 인절미토스트도 맛있는데",
          "speaker_id": "2",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100008552.1.17",
          "form": "집에서 따라해보니 그 맛이 안나더라고요..",
          "original_form": "집에서 따라해보니 그 맛이 안나더라고요..",
          "speaker_id": "2",
          "time": "20210822 17:15"
        },
        {
          "id": "MDRW2100008552.1.18",
          "form": "와 마장요ㅜ",
          "original_form": "와 마장요ㅜ",
          "speaker_id": "1",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008552.1.19",
          "form": "인절미토스트 넘 맛있는",
          "original_form": "인절미토스트 넘 맛있는",
          "speaker_id": "1",
          "time": "20210822 17:20"
        },
        {
          "id": "MDRW2100008552.1.20",
          "form": "맞아요 인절미랑 토스트랑 조합이 좋아서 맛있죠 ㅜㅠ",
          "original_form": "맞아요 인절미랑 토스트랑 조합이 좋아서 맛있죠 ㅜㅠ",
          "speaker_id": "2",
