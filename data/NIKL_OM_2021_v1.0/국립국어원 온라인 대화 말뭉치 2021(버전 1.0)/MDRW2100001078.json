      "utterance": [
        {
          "id": "MDRW2100001078.1.1",
          "form": "혹시 여행 좋아하세요?",
          "original_form": "혹시 여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210723 10:27"
        },
        {
          "id": "MDRW2100001078.1.2",
          "form": "당연하죵ㅋㅋㅋ",
          "original_form": "당연하죵ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210723 10:30"
        },
        {
          "id": "MDRW2100001078.1.3",
          "form": "오오 최근에 다녀오시거나 가고싶은 여행지 있어요?",
          "original_form": "오오 최근에 다녀오시거나 가고싶은 여행지 있어요?",
          "speaker_id": "1",
          "time": "20210723 10:31"
        },
        {
          "id": "MDRW2100001078.1.4",
          "form": "저는 미국 뉴욕 가고싶네용",
          "original_form": "저는 미국 뉴욕 가고싶네용",
          "speaker_id": "2",
          "time": "20210723 10:34"
        },
        {
          "id": "MDRW2100001078.1.5",
          "form": "엄청 높은 건물들을 보고싶어서..ㅋㅋ",
          "original_form": "엄청 높은 건물들을 보고싶어서..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210723 10:34"
        },
        {
          "id": "MDRW2100001078.1.6",
          "form": "크흐ㅠㅠㅠ 뉴욕 좋죠",
          "original_form": "크흐ㅠㅠㅠ 뉴욕 좋죠",
          "speaker_id": "1",
          "time": "20210723 10:34"
        },
        {
          "id": "MDRW2100001078.1.7",
          "form": "뉴욕 건물들이 한눈에 보이는 전망대에서 사진도 찍고 그러고 싶어요..",
          "original_form": "뉴욕 건물들이 한눈에 보이는 전망대에서 사진도 찍고 그러고 싶어요..",
          "speaker_id": "1",
          "time": "20210723 10:35"
        },
        {
          "id": "MDRW2100001078.1.8",
          "form": "그쵸 ㅜㅜ그런감성 너무좋습니다",
          "original_form": "그쵸 ㅜㅜ그런감성 너무좋습니다",
          "speaker_id": "2",
          "time": "20210723 10:44"
        },
        {
          "id": "MDRW2100001078.1.9",
          "form": "맞아요ㅠㅠㅠ 저는 또 특히 유럽도 가보고싶더라구요",
          "original_form": "맞아요ㅠㅠㅠ 저는 또 특히 유럽도 가보고싶더라구요",
          "speaker_id": "1",
          "time": "20210723 10:46"
        },
        {
          "id": "MDRW2100001078.1.10",
          "form": "유럽은 왜용??",
          "original_form": "유럽은 왜용??",
          "speaker_id": "2",
          "time": "20210723 10:47"
        },
        {
          "id": "MDRW2100001078.1.11",
          "form": "유럽 건물들도 너무 예쁘고 특히 유럽만의 감성 있잖아요",
          "original_form": "유럽 건물들도 너무 예쁘고 특히 유럽만의 감성 있잖아요",
          "speaker_id": "1",
          "time": "20210723 10:48"
        },
        {
          "id": "MDRW2100001078.1.12",
          "form": "그게 너무 좋더라고요",
          "original_form": "그게 너무 좋더라고요",
          "speaker_id": "1",
          "time": "20210723 10:48"
        },
        {
          "id": "MDRW2100001078.1.13",
          "form": "아 그 감성 알죠",
          "original_form": "아 그 감성 알죠",
          "speaker_id": "2",
          "time": "20210723 10:49"
        },
        {
          "id": "MDRW2100001078.1.14",
          "form": "이쁜 지붕들 ㅋㅋㅋ",
          "original_form": "이쁜 지붕들 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210723 10:49"
        },
        {
          "id": "MDRW2100001078.1.15",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210723 10:49"
        },
        {
          "id": "MDRW2100001078.1.16",
          "form": "크크",
          "original_form": "크크",
          "speaker_id": "2",
          "time": "20210723 11:42"
        },
        {
          "id": "MDRW2100001078.1.17",
          "form": "제주도 가보셨어요?",
          "original_form": "제주도 가보셨어요?",
          "speaker_id": "1",
          "time": "20210723 11:43"
        },
        {
          "id": "MDRW2100001078.1.18",
          "form": "요즘 방학이라고 제주도에 꽤 놀러가던데",
          "original_form": "요즘 방학이라고 제주도에 꽤 놀러가던데",
          "speaker_id": "1",
          "time": "20210723 11:43"
        },
        {
          "id": "MDRW2100001078.1.19",
          "form": "아뇨 ㅠㅠ",
          "original_form": "아뇨 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210723 11:44"
        },
        {
          "id": "MDRW2100001078.1.20",
          "form": "가고싶긴한데",
          "original_form": "가고싶긴한데",
          "speaker_id": "2",
          "time": "20210723 11:44"
        },
        {
          "id": "MDRW2100001078.1.21",
          "form": "위험하잖아요 코로나 ㅠ",
          "original_form": "위험하잖아요 코로나 ㅠ",
          "speaker_id": "2",
          "time": "20210723 11:44"
        },
        {
          "id": "MDRW2100001078.1.22",
          "form": "아 맞아요ㅠㅠㅠ",
          "original_form": "아 맞아요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210723 11:44"
        },
        {
          "id": "MDRW2100001078.1.23",
          "form": "코로나 끝나면 가야죠 여행 ㅋㅋ",
          "original_form": "코로나 끝나면 가야죠 여행 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210723 11:57"
        },
        {
          "id": "MDRW2100001078.1.24",
          "form": "ㅋㅋㅋ 맞아요 얼른 끝났으면 좋겠네요",
          "original_form": "ㅋㅋㅋ 맞아요 얼른 끝났으면 좋겠네요",
