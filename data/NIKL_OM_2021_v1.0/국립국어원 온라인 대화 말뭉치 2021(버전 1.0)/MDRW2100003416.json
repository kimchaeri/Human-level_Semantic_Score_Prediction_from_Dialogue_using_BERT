      "utterance": [
        {
          "id": "MDRW2100003416.1.1",
          "form": "무슨 영상 보고 있어?",
          "original_form": "무슨 영상 보고 있어?",
          "speaker_id": "1",
          "time": "20210820 22:51"
        },
        {
          "id": "MDRW2100003416.1.2",
          "form": "나 케이팝 클래식 리액션",
          "original_form": "나 케이팝 클래식 리액션",
          "speaker_id": "2",
          "time": "20210820 22:52"
        },
        {
          "id": "MDRW2100003416.1.3",
          "form": "유튜브 채널이야",
          "original_form": "유튜브 채널이야",
          "speaker_id": "2",
          "time": "20210820 22:52"
        },
        {
          "id": "MDRW2100003416.1.4",
          "form": "뭐하는 영상인데?",
          "original_form": "뭐하는 영상인데?",
          "speaker_id": "1",
          "time": "20210820 22:53"
        },
        {
          "id": "MDRW2100003416.1.5",
          "form": "클래식 전공하는 사람들이 케이팝 뮤직비디오 보고 리액션 하는 거ㅋㅋㅋ",
          "original_form": "클래식 전공하는 사람들이 케이팝 뮤직비디오 보고 리액션 하는 거ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:53"
        },
        {
          "id": "MDRW2100003416.1.6",
          "form": "그게 왜 재밌어",
          "original_form": "그게 왜 재밌어",
          "speaker_id": "1",
          "time": "20210820 22:54"
        },
        {
          "id": "MDRW2100003416.1.7",
          "form": "반응 보는 게 재밌지 뭐ㅋㅋㅋ",
          "original_form": "반응 보는 게 재밌지 뭐ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 22:54"
        },
        {
          "id": "MDRW2100003416.1.8",
          "form": "엄마는 유튜브에서 뭐 보는데??",
          "original_form": "엄마는 유튜브에서 뭐 보는데??",
          "speaker_id": "2",
          "time": "20210820 22:54"
        },
        {
          "id": "MDRW2100003416.1.9",
          "form": "엄마는 유튜브 잘 안 보지.",
          "original_form": "엄마는 유튜브 잘 안 보지.",
          "speaker_id": "1",
          "time": "20210820 22:55"
        },
        {
          "id": "MDRW2100003416.1.10",
          "form": "왜 안 봐",
          "original_form": "왜 안 봐",
          "speaker_id": "2",
          "time": "20210820 22:55"
        },
        {
          "id": "MDRW2100003416.1.11",
          "form": "아빠는 많이 보던데",
          "original_form": "아빠는 많이 보던데",
          "speaker_id": "2",
          "time": "20210820 22:55"
        },
        {
          "id": "MDRW2100003416.1.12",
          "form": "정신 사나워서 싫어.",
          "original_form": "정신 사나워서 싫어.",
          "speaker_id": "1",
          "time": "20210820 22:56"
        },
        {
          "id": "MDRW2100003416.1.13",
          "form": "아빠는 맨날 옆에서 트로트 듣는다",
          "original_form": "아빠는 맨날 옆에서 트로트 듣는다",
          "speaker_id": "1",
          "time": "20210820 22:56"
        },
        {
          "id": "MDRW2100003416.1.14",
          "form": "아빠 트로트 프로그램은 안 보면서",
          "original_form": "아빠 트로트 프로그램은 안 보면서",
          "speaker_id": "2",
          "time": "20210820 22:57"
        },
        {
          "id": "MDRW2100003416.1.15",
          "form": "유튜브로 트로트 음악만 듣나 보네",
          "original_form": "유튜브로 트로트 음악만 듣나 보네",
          "speaker_id": "2",
          "time": "20210820 22:57"
        },
        {
          "id": "MDRW2100003416.1.16",
          "form": "응 너무 많이 틀어서 지겨워.",
          "original_form": "응 너무 많이 틀어서 지겨워.",
          "speaker_id": "1",
          "time": "20210820 22:57"
        },
        {
          "id": "MDRW2100003416.1.17",
          "form": "나는 유튜브로 요리하는 것만 가끔 봐.",
          "original_form": "나는 유튜브로 요리하는 것만 가끔 봐.",
          "speaker_id": "1",
          "time": "20210820 22:59"
        },
        {
          "id": "MDRW2100003416.1.18",
          "form": "그거 맨날 우리한테 실험해보잖아ㅋㅋㅋ 막상 엄마는 안 먹으면서",
          "original_form": "그거 맨날 우리한테 실험해보잖아ㅋㅋㅋ 막상 엄마는 안 먹으면서",
          "speaker_id": "2",
          "time": "20210820 23:00"
        },
        {
          "id": "MDRW2100003416.1.19",
          "form": "다 고기 들어가잖아",
          "original_form": "다 고기 들어가잖아",
          "speaker_id": "1",
          "time": "20210820 23:00"
        },
        {
          "id": "MDRW2100003416.1.20",
          "form": "백종원 유튜브에서 인기 엄청 많아",
          "original_form": "백종원 유튜브에서 인기 엄청 많아",
          "speaker_id": "1",
          "time": "20210820 23:01"
        },
        {
          "id": "MDRW2100003416.1.21",
          "form": "사람들이 댓글로 다 맛있대",
          "original_form": "사람들이 댓글로 다 맛있대",
          "speaker_id": "1",
          "time": "20210820 23:01"
        },
        {
          "id": "MDRW2100003416.1.22",
          "form": "ㅋㅋㅋ난 요리하는 거 영상으로 못 보겠더라",
          "original_form": "ㅋㅋㅋ난 요리하는 거 영상으로 못 보겠더라",
          "speaker_id": "2",
          "time": "20210820 23:02"
        },
        {
          "id": "MDRW2100003416.1.23",
          "form": "글로 쓰면 1분만에 읽을 수 있는데 영상으로 보면 10분도 넘게 걸리잖아",
          "original_form": "글로 쓰면 1분만에 읽을 수 있는데 영상으로 보면 10분도 넘게 걸리잖아",
          "speaker_id": "2",
          "time": "20210820 23:03"
        },
        {
          "id": "MDRW2100003416.1.24",
          "form": "그렇긴 하네.",
          "original_form": "그렇긴 하네.",
          "speaker_id": "1",
          "time": "20210820 23:27"
        },
        {
          "id": "MDRW2100003416.1.25",
          "form": "name3이는 맨날 뷰티 영상만 봐",
          "original_form": "&name3&이는 맨날 뷰티 영상만 봐",
          "speaker_id": "1",
          "time": "20210820 23:28"
        },
        {
          "id": "MDRW2100003416.1.26",
          "form": "화장하는 것만 찾아보고 있다",
          "original_form": "화장하는 것만 찾아보고 있다",
          "speaker_id": "1",
          "time": "20210820 23:28"
        },
        {
          "id": "MDRW2100003416.1.27",
          "form": "나도 옛날엔 뷰티 영상 많이 봤어",
          "original_form": "나도 옛날엔 뷰티 영상 많이 봤어",
          "speaker_id": "2",
          "time": "20210820 23:29"
        },
        {
          "id": "MDRW2100003416.1.28",
          "form": "메이크업 튜토리얼 이런 거 많이 가르쳐주니까",
          "original_form": "메이크업 튜토리얼 이런 거 많이 가르쳐주니까",
          "speaker_id": "2",
          "time": "20210820 23:29"
        },
        {
          "id": "MDRW2100003416.1.29",
          "form": "난 요즘 브이로그도 자주 봐",
          "original_form": "난 요즘 브이로그도 자주 봐",
          "speaker_id": "2",
          "time": "20210820 23:30"
        },
        {
          "id": "MDRW2100003416.1.30",
          "form": "그런 게 왜 재밌어?",
          "original_form": "그런 게 왜 재밌어?",
          "speaker_id": "1",
          "time": "20210820 23:30"
        },
        {
          "id": "MDRW2100003416.1.31",
          "form": "몰라 다른 사람들은 어떻게 사나 보는 재미지 뭐",
          "original_form": "몰라 다른 사람들은 어떻게 사나 보는 재미지 뭐",
          "speaker_id": "2",
          "time": "20210820 23:31"
        },
        {
          "id": "MDRW2100003416.1.32",
          "form": "유튜브 본다고 잠 안 자는 거 아니야",
          "original_form": "유튜브 본다고 잠 안 자는 거 아니야",
          "speaker_id": "1",
          "time": "20210820 23:31"
        },
        {
          "id": "MDRW2100003416.1.33",
          "form": "ㅋㅋㅋ아니야",
          "original_form": "ㅋㅋㅋ아니야",
          "speaker_id": "2",
          "time": "20210820 23:31"
        },
        {
          "id": "MDRW2100003416.1.34",
          "form": "잘 때는 asmr 틀고 자",
          "original_form": "잘 때는 asmr 틀고 자",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100003416.1.35",
          "form": "그건 또 뭐야",
          "original_form": "그건 또 뭐야",
          "speaker_id": "1",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100003416.1.36",
          "form": "유튜브는 영상 종류가 너무 많아서 나는 그냥 메인에 뜨는 것만 봐.",
          "original_form": "유튜브는 영상 종류가 너무 많아서 나는 그냥 메인에 뜨는 것만 봐.",
          "speaker_id": "1",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100003416.1.37",
          "form": "백종원 빼고.",
          "original_form": "백종원 빼고.",
          "speaker_id": "1",
          "time": "20210820 23:33"
        },
        {
          "id": "MDRW2100003416.1.38",
          "form": "ㅋㅋㅋasmr은 백색잡음 같은 소리 영상이야",
          "original_form": "ㅋㅋㅋasmr은 백색잡음 같은 소리 영상이야",
          "speaker_id": "2",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100003416.1.39",
          "form": "듣고 있으면 마음이 편해진다구~~",
          "original_form": "듣고 있으면 마음이 편해진다구~~",
          "speaker_id": "2",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100003416.1.40",
          "form": "그래서 잘 때 영상 틀어놓고 자면 좋아",
          "original_form": "그래서 잘 때 영상 틀어놓고 자면 좋아",
          "speaker_id": "2",
          "time": "20210820 23:34"
        },
        {
          "id": "MDRW2100003416.1.41",
          "form": "영상은 안 보고 소리만 듣는 거야?",
          "original_form": "영상은 안 보고 소리만 듣는 거야?",
          "speaker_id": "1",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003416.1.42",
          "form": "영상이 있는 것도 있고 그냥 이미지로 해놓는 것도 있지",
          "original_form": "영상이 있는 것도 있고 그냥 이미지로 해놓는 것도 있지",
          "speaker_id": "2",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003416.1.43",
          "form": "상황극처럼 영상 찍어논 asmr도 재밌어",
          "original_form": "상황극처럼 영상 찍어논 asmr도 재밌어",
          "speaker_id": "2",
          "time": "20210820 23:35"
        },
        {
          "id": "MDRW2100003416.1.44",
          "form": "엄마도 잠 올 때 그거 들어볼까.",
          "original_form": "엄마도 잠 올 때 그거 들어볼까.",
          "speaker_id": "1",
          "time": "20210820 23:36"
        },
        {
          "id": "MDRW2100003416.1.45",
          "form": "ㅋㅋㅋ그럼 내가 토요일에 집에 가서 채널 구독 해줄게",
          "original_form": "ㅋㅋㅋ그럼 내가 토요일에 집에 가서 채널 구독 해줄게",
          "speaker_id": "2",
          "time": "20210820 23:37"
        },
        {
          "id": "MDRW2100003416.1.46",
