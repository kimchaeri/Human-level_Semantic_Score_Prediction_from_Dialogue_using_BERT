      "utterance": [
        {
          "id": "MDRW2100015141.1.1",
          "form": "요즘 날씨 이상하죠?",
          "original_form": "요즘 날씨 이상하죠?",
          "speaker_id": "1",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015141.1.2",
          "form": "마자요ㅋㅋㅋ",
          "original_form": "마자요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015141.1.3",
          "form": "오늘 name1님 동네는 비왔나요?",
          "original_form": "오늘 &name1&님 동네는 비왔나요?",
          "speaker_id": "2",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015141.1.4",
          "form": "네 갑자기 막오다가 보슬비내리고",
          "original_form": "네 갑자기 막오다가 보슬비내리고",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015141.1.5",
          "form": "비안오는것 같은데 내리고",
          "original_form": "비안오는것 같은데 내리고",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015141.1.6",
          "form": "저희동네도요..!!",
          "original_form": "저희동네도요..!!",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015141.1.7",
          "form": "차가리 막 쏟아지고 말지",
          "original_form": "차가리 막 쏟아지고 말지",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015141.1.8",
          "form": "그래도 비 피해는 없어서 다행이예요",
          "original_form": "그래도 비 피해는 없어서 다행이예요",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015141.1.9",
          "form": "아직 본격적인건 아니지만",
          "original_form": "아직 본격적인건 아니지만",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015141.1.10",
          "form": "9월다되가는데",
          "original_form": "9월다되가는데",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015141.1.11",
          "form": "원래 이때도 비가 많이 왔나요ㅎㅎㅎ",
          "original_form": "원래 이때도 비가 많이 왔나요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015141.1.12",
          "form": "작년은 7 8월에 엄청왔던것 같아요",
          "original_form": "작년은 7 8월에 엄청왔던것 같아요",
          "speaker_id": "1",
          "time": "20210825 22:04"
        },
        {
          "id": "MDRW2100015141.1.13",
          "form": "매일 우산",
          "original_form": "매일 우산",
          "speaker_id": "1",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015141.1.14",
          "form": "아 마자요",
          "original_form": "아 마자요",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015141.1.15",
          "form": "우산 진짜 잘챙겨야돼요..",
          "original_form": "우산 진짜 잘챙겨야돼요..",
          "speaker_id": "2",
          "time": "20210825 22:05"
        },
        {
          "id": "MDRW2100015141.1.16",
          "form": "레인부츠까지는 아니더라구요",
          "original_form": "레인부츠까지는 아니더라구요",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015141.1.17",
          "form": "우비는 좋은것같아요!",
          "original_form": "우비는 좋은것같아요!",
          "speaker_id": "2",
          "time": "20210825 22:13"
        },
        {
          "id": "MDRW2100015141.1.18",
          "form": "저는 자전가 잘타는데 그럴때 우비 짱ㅋㅋㅋ",
          "original_form": "저는 자전가 잘타는데 그럴때 우비 짱ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:14"
        },
        {
          "id": "MDRW2100015141.1.19",
          "form": "오 빗속의 우비",
          "original_form": "오 빗속의 우비",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015141.1.20",
          "form": "그래도 빗길이나 자전거 조심하세요",
          "original_form": "그래도 빗길이나 자전거 조심하세요",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015141.1.21",
          "form": "네 감사해요",
          "original_form": "네 감사해요",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015141.1.22",
          "form": "일기예보 잘 보고 다니세요?",
          "original_form": "일기예보 잘 보고 다니세요?",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015141.1.23",
          "form": "아침에 알람울릴때 음성으로 알려주는거 설정해놨어요",
          "original_form": "아침에 알람울릴때 음성으로 알려주는거 설정해놨어요",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015141.1.24",
          "form": "오 좋네요!!!",
          "original_form": "오 좋네요!!!",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015141.1.25",
          "form": "저는 일기예보 그 날씨 하나 누르는게 귀찮아서 그냥 나간적이 많아여 ㅠ",
          "original_form": "저는 일기예보 그 날씨 하나 누르는게 귀찮아서 그냥 나간적이 많아여 ㅠ",
          "speaker_id": "2",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015141.1.26",
          "form": "하긴 요즘은 뉴스도 잘안보니까요",
          "original_form": "하긴 요즘은 뉴스도 잘안보니까요",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015141.1.27",
          "form": "걍 우산은 필수품인듯 틴트대신 우산들고 다닙니다",
          "original_form": "걍 우산은 필수품인듯 틴트대신 우산들고 다닙니다",
          "speaker_id": "1",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015141.1.28",
          "form": "ㅋㅋㅋㅋㅋ 마자요",
          "original_form": "ㅋㅋㅋㅋㅋ 마자요",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015141.1.29",
          "form": "좋아하는 계절있나요?",
          "original_form": "좋아하는 계절있나요?",
          "speaker_id": "2",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015141.1.30",
          "form": "겨울이요",
          "original_form": "겨울이요",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015141.1.31",
          "form": "집순이에게 최적화된 계절",
          "original_form": "집순이에게 최적화된 계절",
          "speaker_id": "1",
          "time": "20210825 22:28"
        },
        {
          "id": "MDRW2100015141.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ 저도 집순이지만",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저도 집순이지만",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015141.1.33",
          "form": "겨울되면 늘 손발이 차가워서 심하면 아플정도여서",
          "original_form": "겨울되면 늘 손발이 차가워서 심하면 아플정도여서",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015141.1.34",
          "form": "저는 가을이요ㅋㅋㅋ",
          "original_form": "저는 가을이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:29"
        },
        {
          "id": "MDRW2100015141.1.35",
          "form": "아 수족내증",
          "original_form": "아 수족내증",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015141.1.36",
          "form": "저두요 아플정도는 아니지만",
          "original_form": "저두요 아플정도는 아니지만",
          "speaker_id": "1",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015141.1.37",
          "form": "가을은 괜히 계절타게 되더라구요",
          "original_form": "가을은 괜히 계절타게 되더라구요",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015141.1.38",
          "form": "여름이 다간건 너무 좋아요",
          "original_form": "여름이 다간건 너무 좋아요",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015141.1.39",
          "form": "땀이 잘 나는 체질이어서요ㅎㅎㅎ",
          "original_form": "땀이 잘 나는 체질이어서요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015141.1.40",
          "form": "저도 더우면 쳐저서 별로",
          "original_form": "저도 더우면 쳐저서 별로",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015141.1.41",
          "form": "어릴땐 방학이라도 있었지... 지금은...",
          "original_form": "어릴땐 방학이라도 있었지... 지금은...",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100015141.1.42",
          "form": "저는 방학이었는데도 이번에 바쁘고 더워서 더 지친것같아요ㅎ..",
          "original_form": "저는 방학이었는데도 이번에 바쁘고 더워서 더 지친것같아요ㅎ..",
          "speaker_id": "2",
          "time": "20210825 22:35"
        }
      ]
