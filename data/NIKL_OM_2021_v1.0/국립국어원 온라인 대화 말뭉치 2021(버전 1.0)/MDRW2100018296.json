      "utterance": [
        {
          "id": "MDRW2100018296.1.1",
          "form": "안녕하세요 봉사 단체모임 하시는 거 있으신가요~?",
          "original_form": "안녕하세요 봉사 단체모임 하시는 거 있으신가요~?",
          "speaker_id": "1",
          "time": "20210826 23:44"
        },
        {
          "id": "MDRW2100018296.1.2",
          "form": "아니요~ 저는 아직 해본적은 없어요ㅎㅎ\r\nname1은 있으세요?",
          "original_form": "아니요~ 저는 아직 해본적은 없어요ㅎㅎ\r\n&name1&은 있으세요?",
          "speaker_id": "2",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018296.1.3",
          "form": "그러시군요 저도 없네요ㅠㅠ 그래도 관심은 있는데",
          "original_form": "그러시군요 저도 없네요ㅠㅠ 그래도 관심은 있는데",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018296.1.4",
          "form": "최근에 어려우신 분들 집짓기 봉사 이런 걸 봤는데 코시국이라 못 하더라고요",
          "original_form": "최근에 어려우신 분들 집짓기 봉사 이런 걸 봤는데 코시국이라 못 하더라고요",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018296.1.5",
          "form": "와 그런건 좋을것같은데요?",
          "original_form": "와 그런건 좋을것같은데요?",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018296.1.6",
          "form": "코로나때문에 못하는게 너무 아쉽네요",
          "original_form": "코로나때문에 못하는게 너무 아쉽네요",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018296.1.7",
          "form": "해비타트 말씀하시는거죠?",
          "original_form": "해비타트 말씀하시는거죠?",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100018296.1.8",
          "form": "아 맞아요 해비타트! 잘 아시네요",
          "original_form": "아 맞아요 해비타트! 잘 아시네요",
          "speaker_id": "1",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018296.1.9",
          "form": "나중에 할 수 있으면 좋겠어요",
          "original_form": "나중에 할 수 있으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018296.1.10",
          "form": "그 안에서 만날 사람들도 궁금하고 해서ㅎㅎ",
          "original_form": "그 안에서 만날 사람들도 궁금하고 해서ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018296.1.11",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018296.1.12",
          "form": "님도 그런 봉사나 모임에 관심 있으세요?",
          "original_form": "님도 그런 봉사나 모임에 관심 있으세요?",
          "speaker_id": "1",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018296.1.13",
          "form": "그리고 엄청 뿌듯할것같아요",
          "original_form": "그리고 엄청 뿌듯할것같아요",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018296.1.14",
          "form": "사실 전 관심 없었는데",
          "original_form": "사실 전 관심 없었는데",
          "speaker_id": "2",
          "time": "20210826 23:51"
        },
        {
          "id": "MDRW2100018296.1.15",
          "form": "요즘 뭔가 관심생겼어요ㅋㅋ",
          "original_form": "요즘 뭔가 관심생겼어요ㅋㅋ",
