        },
        {
          "id": "MDRW2100041863.1.2",
          "form": "저녁하고 있나?",
          "original_form": "저녁하고 있나?",
          "speaker_id": "2",
          "time": "20210906 17:44"
        },
        {
          "id": "MDRW2100041863.1.3",
          "form": "아니ㅋㅋㅋㅋ이제해야자",
          "original_form": "아니ㅋㅋㅋㅋ이제해야자",
          "speaker_id": "1",
          "time": "20210906 18:10"
        },
        {
          "id": "MDRW2100041863.1.4",
          "form": "우리 이틀째 곰국 ㅋㅋㅋ",
          "original_form": "우리 이틀째 곰국 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:33"
        },
        {
          "id": "MDRW2100041863.1.5",
          "form": "곰국은 어서났엉 맛있겠다",
          "original_form": "곰국은 어서났엉 맛있겠다",
          "speaker_id": "1",
          "time": "20210906 20:34"
        },
        {
          "id": "MDRW2100041863.1.6",
          "form": "엄마가 일요일에 와가 김치찜이랑 곰국 주고 갔으 ㅋㅋ",
          "original_form": "엄마가 일요일에 와가 김치찜이랑 곰국 주고 갔으 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:11"
        },
        {
          "id": "MDRW2100041863.1.7",
          "form": "좋다 김치찜ㅠㅠ",
          "original_form": "좋다 김치찜ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 21:12"
        },
        {
          "id": "MDRW2100041863.1.8",
          "form": "오늘 저녁에 클리어 했다 ㅋㅋ",
          "original_form": "오늘 저녁에 클리어 했다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100041863.1.9",
          "form": "신랑 밥 안먹고와서",
          "original_form": "신랑 밥 안먹고와서",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100041863.1.10",
          "form": "냉동피자 돌려먹는구만",
          "original_form": "냉동피자 돌려먹는구만",
          "speaker_id": "2",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100041863.1.11",
          "form": "지코바나 엽떡 먹고 싶은디...ㅠㅠ",
