      "utterance": [
        {
          "id": "MDRW2100027587.1.1",
          "form": "맞아요 그걸로 대리만족 해야겠어요",
          "original_form": "맞아요 그걸로 대리만족 해야겠어요",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027587.1.2",
          "form": "고양이 어떤가요",
          "original_form": "고양이 어떤가요",
          "speaker_id": "2",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027587.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ 너무 기엽겠어요 그래도",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 너무 기엽겠어요 그래도",
          "speaker_id": "2",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027587.1.4",
          "form": "실물로보시니 ㅜㅠ",
          "original_form": "실물로보시니 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100027587.1.5",
          "form": "맞아요 그래도 고양이 유투브 많이봐요 ㅋㅋㅋ",
          "original_form": "맞아요 그래도 고양이 유투브 많이봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:42"
        },
        {
          "id": "MDRW2100027587.1.6",
          "form": "그런거보면 참고가 되나용??",
          "original_form": "그런거보면 참고가 되나용??",
          "speaker_id": "2",
          "time": "20210902 13:49"
        },
        {
          "id": "MDRW2100027587.1.7",
          "form": "귀여워서 자주보네용",
          "original_form": "귀여워서 자주보네용",
          "speaker_id": "1",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100027587.1.8",
          "form": "그거맞어요",
          "original_form": "그거맞어요",
          "speaker_id": "2",
          "time": "20210902 14:03"
        },
        {
          "id": "MDRW2100027587.1.9",
          "form": "티비는 잘 안보시나요?",
          "original_form": "티비는 잘 안보시나요?",
          "speaker_id": "1",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027587.1.10",
          "form": "네네 유튭이있으니 잘안보게되네요",
          "original_form": "네네 유튭이있으니 잘안보게되네요",
          "speaker_id": "2",
          "time": "20210902 14:58"
        },
        {
          "id": "MDRW2100027587.1.11",
          "form": "저는 티비 채널 돌리다 시간 다 가요 ㅋㅋ",
          "original_form": "저는 티비 채널 돌리다 시간 다 가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027587.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ 넷플릭스 고르는 느낌이군요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 넷플릭스 고르는 느낌이군요",
          "speaker_id": "2",
          "time": "20210902 15:02"
        },
        {
          "id": "MDRW2100027587.1.13",
          "form": "최근엔 엠넷 서바이벌 프로그램 봤어요",
          "original_form": "최근엔 엠넷 서바이벌 프로그램 봤어요",
          "speaker_id": "1",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027587.1.14",
          "form": "춤추는거요?",
          "original_form": "춤추는거요?",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027587.1.15",
          "form": "저는 페북에 뜨는거봤어요!",
          "original_form": "저는 페북에 뜨는거봤어요!",
          "speaker_id": "2",
          "time": "20210902 15:07"
        },
        {
          "id": "MDRW2100027587.1.16",
          "form": "오 그거는 짤로 봤는데 디스 장난아님",
          "original_form": "오 그거는 짤로 봤는데 디스 장난아님",
          "speaker_id": "1",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100027587.1.17",
          "form": "그거랑 또 여아이돌 뽑는 프로그램 있어요",
          "original_form": "그거랑 또 여아이돌 뽑는 프로그램 있어요",
          "speaker_id": "1",
          "time": "20210902 15:11"
        }
      ]
