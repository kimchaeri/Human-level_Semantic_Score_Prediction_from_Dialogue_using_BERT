      "utterance": [
        {
          "id": "MDRW2100031889.1.1",
          "form": "아 마지막글 못봣서요 ㅠ",
          "original_form": "아 마지막글 못봣서요 ㅠ",
          "speaker_id": "2",
          "time": "20210905 07:42"
        },
        {
          "id": "MDRW2100031889.1.2",
          "form": "아기가 저 앞에와서",
          "original_form": "아기가 저 앞에와서",
          "speaker_id": "2",
          "time": "20210905 07:43"
        },
        {
          "id": "MDRW2100031889.1.3",
          "form": "춤추길래",
          "original_form": "춤추길래",
          "speaker_id": "2",
          "time": "20210905 07:43"
        },
        {
          "id": "MDRW2100031889.1.4",
          "form": "오 귀엽겠어요",
          "original_form": "오 귀엽겠어요",
          "speaker_id": "1",
          "time": "20210905 07:43"
        },
        {
          "id": "MDRW2100031889.1.5",
          "form": "잠시 놀아주느라...",
          "original_form": "잠시 놀아주느라...",
          "speaker_id": "2",
          "time": "20210905 07:43"
        },
        {
          "id": "MDRW2100031889.1.6",
          "form": "진짜 진짜 짱 귀여워요",
          "original_form": "진짜 진짜 짱 귀여워요",
          "speaker_id": "2",
          "time": "20210905 07:43"
        },
        {
          "id": "MDRW2100031889.1.7",
          "form": "저희 사촌오빠 보니까",
          "original_form": "저희 사촌오빠 보니까",
          "speaker_id": "1",
          "time": "20210905 07:43"
        },
        {
          "id": "MDRW2100031889.1.8",
          "form": "저 닮아서 너무 예쁩니다",
          "original_form": "저 닮아서 너무 예쁩니다",
          "speaker_id": "2",
          "time": "20210905 07:43"
        },
        {
          "id": "MDRW2100031889.1.9",
          "form": "아빠가 잘 보면 아빠를 따르더라고요",
          "original_form": "아빠가 잘 보면 아빠를 따르더라고요",
          "speaker_id": "1",
          "time": "20210905 07:43"
        },
        {
          "id": "MDRW2100031889.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 07:43"
        },
        {
          "id": "MDRW2100031889.1.11",
          "form": "제가 잘 본다고 잘 보는데",
          "original_form": "제가 잘 본다고 잘 보는데",
          "speaker_id": "2",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100031889.1.12",
          "form": "엄마를 너무 좋아합니다",
          "original_form": "엄마를 너무 좋아합니다",
          "speaker_id": "2",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100031889.1.13",
          "form": "약간 제가 삐지기도 합니다",
          "original_form": "약간 제가 삐지기도 합니다",
          "speaker_id": "2",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100031889.1.14",
          "form": "왜 그렇듯이 좀 더 노력해 보세요",
          "original_form": "왜 그렇듯이 좀 더 노력해 보세요",
          "speaker_id": "1",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100031889.1.15",
          "form": "목욕은 맨날 저랑하는데",
          "original_form": "목욕은 맨날 저랑하는데",
          "speaker_id": "2",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100031889.1.16",
          "form": "왜지 ㅠㅠㅠ",
          "original_form": "왜지 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100031889.1.17",
          "form": "그 주양육자에 대한 애착을 뛰어넘지는 못하는 것 같습니다",
          "original_form": "그 주양육자에 대한 애착을 뛰어넘지는 못하는 것 같습니다",
          "speaker_id": "2",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100031889.1.18",
          "form": "그래서 엄마가 위대하지요",
          "original_form": "그래서 엄마가 위대하지요",
          "speaker_id": "2",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100031889.1.19",
          "form": "저는 아빠랑 친하긴 하거든요",
          "original_form": "저는 아빠랑 친하긴 하거든요",
          "speaker_id": "1",
          "time": "20210905 07:44"
        },
        {
          "id": "MDRW2100031889.1.20",
          "form": "근데 아빠가 좀 부족한게",
          "original_form": "근데 아빠가 좀 부족한게",
          "speaker_id": "1",
          "time": "20210905 07:45"
        },
        {
          "id": "MDRW2100031889.1.21",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210905 07:45"
        },
        {
          "id": "MDRW2100031889.1.22",
          "form": "주양육자 이런게 아니라",
          "original_form": "주양육자 이런게 아니라",
          "speaker_id": "1",
          "time": "20210905 07:45"
        },
        {
          "id": "MDRW2100031889.1.23",
          "form": "저 외삼촌도 그랬는데요",
          "original_form": "저 외삼촌도 그랬는데요",
          "speaker_id": "1",
          "time": "20210905 07:45"
        },
        {
          "id": "MDRW2100031889.1.24",
          "form": "뭔가 애를 건드려서 그 반응을 보고 즐기는 느낌",
          "original_form": "뭔가 애를 건드려서 그 반응을 보고 즐기는 느낌",
          "speaker_id": "1",
          "time": "20210905 07:45"
        },
        {
          "id": "MDRW2100031889.1.25",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210905 07:45"
        },
        {
          "id": "MDRW2100031889.1.26",
          "form": "너무싫다",
          "original_form": "너무싫다",
          "speaker_id": "2",
          "time": "20210905 07:45"
        },
        {
          "id": "MDRW2100031889.1.27",
          "form": "그런게 아니고 좀 잘 놀아 주는 사람들이 있긴 있어요",
          "original_form": "그런게 아니고 좀 잘 놀아 주는 사람들이 있긴 있어요",
          "speaker_id": "1",
          "time": "20210905 07:46"
        },
        {
          "id": "MDRW2100031889.1.28",
          "form": "저는 그거 반대에요",
          "original_form": "저는 그거 반대에요",
          "speaker_id": "2",
          "time": "20210905 07:46"
        },
        {
          "id": "MDRW2100031889.1.29",
          "form": "근데 확실히 남자 평균은 그런게 좀 떨어지는 거 같더라고요",
          "original_form": "근데 확실히 남자 평균은 그런게 좀 떨어지는 거 같더라고요",
          "speaker_id": "1",
          "time": "20210905 07:46"
        },
        {
          "id": "MDRW2100031889.1.30",
          "form": "육아에 좋지않습니다",
          "original_form": "육아에 좋지않습니다",
          "speaker_id": "2",
          "time": "20210905 07:46"
        },
        {
          "id": "MDRW2100031889.1.31",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210905 07:46"
        },
        {
          "id": "MDRW2100031889.1.32",
          "form": "요즘에는 그런 것도 다 공부하고 키우는 추세 잖아요",
          "original_form": "요즘에는 그런 것도 다 공부하고 키우는 추세 잖아요",
          "speaker_id": "1",
          "time": "20210905 07:46"
        },
        {
          "id": "MDRW2100031889.1.33",
          "form": "공부해야됩니다",
          "original_form": "공부해야됩니다",
          "speaker_id": "2",
          "time": "20210905 07:47"
        },
        {
          "id": "MDRW2100031889.1.34",
          "form": "책 고르는것도",
          "original_form": "책 고르는것도",
          "speaker_id": "2",
          "time": "20210905 07:47"
        }
      ]
    }
