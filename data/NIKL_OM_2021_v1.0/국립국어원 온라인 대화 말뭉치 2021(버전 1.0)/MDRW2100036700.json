      "utterance": [
        {
          "id": "MDRW2100036700.1.1",
          "form": "쇼핑 자주 하세요?!",
          "original_form": "쇼핑 자주 하세요?!",
          "speaker_id": "1",
          "time": "20210831 09:40"
        },
        {
          "id": "MDRW2100036700.1.2",
          "form": "네 쇼핑은 자주해요~^^  금방도물티슈 샀어요ㅋ",
          "original_form": "네 쇼핑은 자주해요~^^  금방도물티슈 샀어요ㅋ",
          "speaker_id": "2",
          "time": "20210831 09:41"
        },
        {
          "id": "MDRW2100036700.1.3",
          "form": "아ㅎㅎㅎ저는 쓸데없는 소비를 너무 많이해서 탈이에요ㅠ",
          "original_form": "아ㅎㅎㅎ저는 쓸데없는 소비를 너무 많이해서 탈이에요ㅠ",
          "speaker_id": "1",
          "time": "20210831 09:43"
        },
        {
          "id": "MDRW2100036700.1.4",
          "form": "저도 그렇죠ㅎㅎ 주로어떤거?",
          "original_form": "저도 그렇죠ㅎㅎ 주로어떤거?",
          "speaker_id": "2",
          "time": "20210831 09:43"
        },
        {
          "id": "MDRW2100036700.1.5",
          "form": "굳이 안 사도 되는.. 먹을거?ㅋㅋㅋ",
          "original_form": "굳이 안 사도 되는.. 먹을거?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036700.1.6",
          "form": "괜히 육개장이 보이길래 한 박스 사고",
          "original_form": "괜히 육개장이 보이길래 한 박스 사고",
          "speaker_id": "1",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036700.1.7",
          "form": "ㅋㅋㅋㅋㅋ. 왜요 사두면 다먹지 않나요",
          "original_form": "ㅋㅋㅋㅋㅋ. 왜요 사두면 다먹지 않나요",
          "speaker_id": "2",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036700.1.8",
          "form": "사발면 작은거 최앤데",
          "original_form": "사발면 작은거 최앤데",
          "speaker_id": "2",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036700.1.9",
          "form": "없으면 안 먹을텐데ㅋㅋㅋㅋ그리고 괜히 음료수같은것도",
          "original_form": "없으면 안 먹을텐데ㅋㅋㅋㅋ그리고 괜히 음료수같은것도",
          "speaker_id": "1",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036700.1.10",
          "form": "냉장고 자리도 없는데",
          "original_form": "냉장고 자리도 없는데",
          "speaker_id": "1",
          "time": "20210831 09:44"
        },
        {
          "id": "MDRW2100036700.1.11",
          "form": "2+1 해서 3개씩 사고..",
          "original_form": "2+1 해서 3개씩 사고..",
          "speaker_id": "1",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036700.1.12",
          "form": "ㅋㅋㅋㅋㅋ. 음료는 투뿔원이 진리죠",
          "original_form": "ㅋㅋㅋㅋㅋ. 음료는 투뿔원이 진리죠",
          "speaker_id": "2",
          "time": "20210831 09:45"
        },
        {
          "id": "MDRW2100036700.1.13",
          "form": "사발면이 육개장 말하시는거죠?",
          "original_form": "사발면이 육개장 말하시는거죠?",
          "speaker_id": "1",
          "time": "20210831 09:48"
        },
        {
          "id": "MDRW2100036700.1.14",
          "form": "저도 그거 자주 먹어요ㅋㅋㅋ",
          "original_form": "저도 그거 자주 먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:48"
        },
        {
          "id": "MDRW2100036700.1.15",
          "form": "네네 작은거요ㅎㅎ",
          "original_form": "네네 작은거요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 09:53"
        },
        {
          "id": "MDRW2100036700.1.16",
          "form": "하나는 부족하고 두개는 많아서",
          "original_form": "하나는 부족하고 두개는 많아서",
          "speaker_id": "1",
          "time": "20210831 09:53"
        },
        {
          "id": "MDRW2100036700.1.17",
          "form": "2개먹으면서 하나는 면만 먹으면 딱 배부르더라구요",
          "original_form": "2개먹으면서 하나는 면만 먹으면 딱 배부르더라구요",
          "speaker_id": "1",
          "time": "20210831 09:54"
        },
        {
          "id": "MDRW2100036700.1.18",
          "form": "그럼 컵라면도 묶음으로 사세요?",
          "original_form": "그럼 컵라면도 묶음으로 사세요?",
          "speaker_id": "1",
          "time": "20210831 09:54"
        },
        {
          "id": "MDRW2100036700.1.19",
          "form": "아니요ㅎ 저는 낱개로",
          "original_form": "아니요ㅎ 저는 낱개로",
          "speaker_id": "2",
          "time": "20210831 09:54"
        },
        {
          "id": "MDRW2100036700.1.20",
          "form": "쇼핑은 어디서 많이하세요?",
          "original_form": "쇼핑은 어디서 많이하세요?",
          "speaker_id": "1",
          "time": "20210831 09:55"
        },
        {
          "id": "MDRW2100036700.1.21",
          "form": "저는 할인하는 곳이요 ㅋㅋㅋ",
          "original_form": "저는 할인하는 곳이요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 09:55"
        },
        {
          "id": "MDRW2100036700.1.22",
          "form": "저도요 ㅎ쿠폰뜨면 기다렸다사고",
          "original_form": "저도요 ㅎ쿠폰뜨면 기다렸다사고",
          "speaker_id": "2",
