      "utterance": [
        {
          "id": "MDRW2100010749.1.1",
          "form": "울산은어때",
          "original_form": "울산은어때",
          "speaker_id": "1",
          "time": "20210823 21:32"
        },
        {
          "id": "MDRW2100010749.1.2",
          "form": "음 여기도 올거 같은데 보고올게요",
          "original_form": "음 여기도 올거 같은데 보고올게요",
          "speaker_id": "2",
          "time": "20210823 21:32"
        },
        {
          "id": "MDRW2100010749.1.3",
          "form": "남부지방을 지나가더라구",
          "original_form": "남부지방을 지나가더라구",
          "speaker_id": "1",
          "time": "20210823 21:32"
        },
        {
          "id": "MDRW2100010749.1.4",
          "form": "여긴안와요",
          "original_form": "여긴안와요",
          "speaker_id": "2",
          "time": "20210823 21:37"
        },
        {
          "id": "MDRW2100010749.1.5",
          "form": "미묘하게 사기당한기분",
          "original_form": "미묘하게 사기당한기분",
          "speaker_id": "2",
          "time": "20210823 21:37"
        },
        {
          "id": "MDRW2100010749.1.6",
          "form": "오 평안하시군요 동네가 ㅋㅋ",
          "original_form": "오 평안하시군요 동네가 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:37"
        },
        {
          "id": "MDRW2100010749.1.7",
          "form": "괜히 일찍 집에왔네요? ㅋㅋㅋ",
          "original_form": "괜히 일찍 집에왔네요? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:41"
        },
        {
          "id": "MDRW2100010749.1.8",
          "form": "머어때 좋지 ㅋㅋ",
          "original_form": "머어때 좋지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:46"
        },
        {
          "id": "MDRW2100010749.1.9",
          "form": "신발젖는거 싫어유",
          "original_form": "신발젖는거 싫어유",
          "speaker_id": "1",
          "time": "20210823 21:47"
        },
        {
          "id": "MDRW2100010749.1.10",
          "form": "튼튼한 비싼 우산 갖고싶어요",
          "original_form": "튼튼한 비싼 우산 갖고싶어요",
          "speaker_id": "2",
          "time": "20210823 21:48"
        },
        {
          "id": "MDRW2100010749.1.11",
          "form": "비많이오니 우산써도 안으로 비가새서",
          "original_form": "비많이오니 우산써도 안으로 비가새서",
          "speaker_id": "2",
          "time": "20210823 21:49"
        },
        {
          "id": "MDRW2100010749.1.12",
          "form": "태풍때는 꼭 장우산 사야함ㅜ",
          "original_form": "태풍때는 꼭 장우산 사야함ㅜ",
          "speaker_id": "1",
          "time": "20210823 21:49"
        },
        {
          "id": "MDRW2100010749.1.13",
          "form": "고급우산십만원 가까이 하네요 ㄷㄷ",
          "original_form": "고급우산십만원 가까이 하네요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210823 21:53"
        },
        {
          "id": "MDRW2100010749.1.14",
          "form": "ㅜㅜ아부지 쓰던건없낭",
          "original_form": "ㅜㅜ아부지 쓰던건없낭",
          "speaker_id": "1",
          "time": "20210823 21:57"
        },
        {
          "id": "MDRW2100010749.1.15",
          "form": "골프우산은 또 넘 크고 무겁ㅋㅋ",
          "original_form": "골프우산은 또 넘 크고 무겁ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:58"
        },
        {
          "id": "MDRW2100010749.1.16",
          "form": "장단점이있네요",
          "original_form": "장단점이있네요",
          "speaker_id": "2",
          "time": "20210823 21:58"
        },
        {
          "id": "MDRW2100010749.1.17",
          "form": "태풍용아니냔 딱",
          "original_form": "태풍용아니냔 딱",
