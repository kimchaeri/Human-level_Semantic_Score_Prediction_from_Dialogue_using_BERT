      "utterance": [
        {
          "id": "MDRW2100041065.1.1",
          "form": "점심은 뭐 드셨어요?",
          "original_form": "점심은 뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210905 16:26"
        },
        {
          "id": "MDRW2100041065.1.2",
          "form": "저 떡볶이요!",
          "original_form": "저 떡볶이요!",
          "speaker_id": "2",
          "time": "20210905 16:27"
        },
        {
          "id": "MDRW2100041065.1.3",
          "form": "떡볶이가 땡겨서 집에 있는 재료로 대충 해먹었어요 ㅎㅎ",
          "original_form": "떡볶이가 땡겨서 집에 있는 재료로 대충 해먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 16:27"
        },
        {
          "id": "MDRW2100041065.1.4",
          "form": "떡볶이 맛있겠네요",
          "original_form": "떡볶이 맛있겠네요",
          "speaker_id": "1",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100041065.1.5",
          "form": "요리 잘하세요?",
          "original_form": "요리 잘하세요?",
          "speaker_id": "1",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100041065.1.6",
          "form": "잘한다기보단 레시피만 있음 다 할 수 있는 것 같아요 ㅎㅎㅎ",
          "original_form": "잘한다기보단 레시피만 있음 다 할 수 있는 것 같아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100041065.1.7",
          "form": "식혜 떡볶이 쉽다고해서 따라해봤는데 정말 쉽더라구요 ㅎㅎ",
          "original_form": "식혜 떡볶이 쉽다고해서 따라해봤는데 정말 쉽더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100041065.1.8",
          "form": "식혜 떡볶이 뭔가 상상이안가요",
          "original_form": "식혜 떡볶이 뭔가 상상이안가요",
          "speaker_id": "1",
          "time": "20210905 16:29"
        },
        {
          "id": "MDRW2100041065.1.9",
          "form": "떡볶이에 식혜를 넣어요?",
          "original_form": "떡볶이에 식혜를 넣어요?",
          "speaker_id": "1",
          "time": "20210905 16:29"
        },
        {
          "id": "MDRW2100041065.1.10",
          "form": "물에 식혜를 채에 걸러서 육수로 하고 고추장 떡 넣는 레시피래요!",
          "original_form": "물에 식혜를 채에 걸러서 육수로 하고 고추장 떡 넣는 레시피래요!",
          "speaker_id": "2",
          "time": "20210905 16:33"
        },
        {
          "id": "MDRW2100041065.1.11",
          "form": "근데 전 너무 달더라구요 ㅠㅠ",
          "original_form": "근데 전 너무 달더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 16:33"
        },
        {
          "id": "MDRW2100041065.1.12",
          "form": "그래도 고춧가루랑 어묵 파 같이 넣어 먹었더니 맛있었어요 ㅎㅎ",
          "original_form": "그래도 고춧가루랑 어묵 파 같이 넣어 먹었더니 맛있었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 16:33"
        },
        {
          "id": "MDRW2100041065.1.13",
          "form": "신기하네요 뭔가 조합이 상상도 못해본거라",
          "original_form": "신기하네요 뭔가 조합이 상상도 못해본거라",
          "speaker_id": "1",
          "time": "20210905 16:33"
        },
        {
          "id": "MDRW2100041065.1.14",
          "form": "맞아요 ㅋㅋㅋ 저도 신기하다 싶었는데 맛있다고해서 궁금해서 도전해보았죠 ㅎㅎ",
          "original_form": "맞아요 ㅋㅋㅋ 저도 신기하다 싶었는데 맛있다고해서 궁금해서 도전해보았죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 16:35"
        },
        {
          "id": "MDRW2100041065.1.15",
          "form": "간단하게 먹을 수 있어서 좋은 것 같아요",
          "original_form": "간단하게 먹을 수 있어서 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210905 16:35"
        },
        {
          "id": "MDRW2100041065.1.16",
          "form": "오 저도 레시피 찾아봐야겠어요",
          "original_form": "오 저도 레시피 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210905 16:36"
        },
        {
          "id": "MDRW2100041065.1.17",
          "form": "특이 레시피 좋아하시나봐요",
          "original_form": "특이 레시피 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210905 16:36"
        },
        {
          "id": "MDRW2100041065.1.18",
          "form": "아니에욬ㅋㅋㅋ 특이레시피라기보단 재료가 구하기 쉬워서 해봤어요 ㅎㅎ",
          "original_form": "아니에욬ㅋㅋㅋ 특이레시피라기보단 재료가 구하기 쉬워서 해봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 16:40"
        },
        {
          "id": "MDRW2100041065.1.19",
          "form": "전 맛만 있다면 다 오케이에요 ㅎㅎ",
          "original_form": "전 맛만 있다면 다 오케이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 16:40"
        },
        {
          "id": "MDRW2100041065.1.20",
          "form": "요즘은 인터넷레시피중에 순두부라면에 꽂혀있어요 ㅎㅎ",
          "original_form": "요즘은 인터넷레시피중에 순두부라면에 꽂혀있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 16:41"
        },
        {
          "id": "MDRW2100041065.1.21",
          "form": "오 맛있다고 하더라구요",
          "original_form": "오 맛있다고 하더라구요",
          "speaker_id": "1",
          "time": "20210905 16:41"
        },
        {
          "id": "MDRW2100041065.1.22",
          "form": "저도 그건 해보고싶어요",
          "original_form": "저도 그건 해보고싶어요",
          "speaker_id": "1",
          "time": "20210905 16:41"
        },
        {
          "id": "MDRW2100041065.1.23",
          "form": "제 입맛엔 잘 맞았어서 여기저기 추천중이에요 ㅎㅎ",
          "original_form": "제 입맛엔 잘 맞았어서 여기저기 추천중이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 16:42"
        },
        {
          "id": "MDRW2100041065.1.24",
          "form": "약간 든든하고 매콤해서 맛있어요~",
          "original_form": "약간 든든하고 매콤해서 맛있어요~",
          "speaker_id": "2",
          "time": "20210905 16:42"
        },
        {
          "id": "MDRW2100041065.1.25",
          "form": "이미 해보셨구나 ㅎㅎ",
          "original_form": "이미 해보셨구나 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 16:42"
        },
        {
          "id": "MDRW2100041065.1.26",
          "form": "저도 해먹어봐야겠네요",
          "original_form": "저도 해먹어봐야겠네요",
          "speaker_id": "1",
          "time": "20210905 16:42"
        }
      ]
    }
