      "utterance": [
        {
          "id": "MDRW2100013981.1.1",
          "form": "맞아요ㅠㅠㅠㅠㅠㅠㅠㅠ 이곳저곳 가고 음식도 먹고",
          "original_form": "맞아요ㅠㅠㅠㅠㅠㅠㅠㅠ 이곳저곳 가고 음식도 먹고",
          "speaker_id": "2",
          "time": "20210825 17:01"
        },
        {
          "id": "MDRW2100013981.1.2",
          "form": "저 라멘은 안 먹어봤거든요 일본가서 음식 다 먹어놓고 라멘만 안 먹은...사람.저...",
          "original_form": "저 라멘은 안 먹어봤거든요 일본가서 음식 다 먹어놓고 라멘만 안 먹은...사람.저...",
          "speaker_id": "2",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100013981.1.3",
          "form": "아앗 어째서",
          "original_form": "아앗 어째서",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100013981.1.4",
          "form": "먹어보고 싶은 것도 많네요",
          "original_form": "먹어보고 싶은 것도 많네요",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100013981.1.5",
          "form": "일식 좋아해서",
          "original_form": "일식 좋아해서",
          "speaker_id": "1",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100013981.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저도요!!!!!!! 저 소혀구이 먹고....놀랐거든요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 저도요!!!!!!! 저 소혀구이 먹고....놀랐거든요",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100013981.1.7",
          "form": "일본가서....",
          "original_form": "일본가서....",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100013981.1.8",
          "form": "우와 소혀구이",
          "original_form": "우와 소혀구이",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100013981.1.9",
          "form": "맛이 어떤가요",
          "original_form": "맛이 어떤가요",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100013981.1.10",
          "form": "진짜 맛있어요 야들야들하고...",
          "original_form": "진짜 맛있어요 야들야들하고...",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100013981.1.11",
          "form": "전 냄새날 줄 알았는데 생각보다 괜찮은거예요!!",
          "original_form": "전 냄새날 줄 알았는데 생각보다 괜찮은거예요!!",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100013981.1.12",
          "form": "그래서 엄청 놀랐어요",
          "original_form": "그래서 엄청 놀랐어요",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100013981.1.13",
          "form": "헉......",
          "original_form": "헉......",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100013981.1.14",
          "form": "나중에 먹어볼게요",
          "original_form": "나중에 먹어볼게요",
          "speaker_id": "1",
          "time": "20210825 20:53"
        },
        {
          "id": "MDRW2100013981.1.15",
          "form": "다른 음식도 추천해주실 수 있나요",
          "original_form": "다른 음식도 추천해주실 수 있나요",
          "speaker_id": "1",
          "time": "20210825 20:54"
        },
        {
          "id": "MDRW2100013981.1.16",
          "form": "음........어...................오코노미야끼...좋아하시나요",
          "original_form": "음........어...................오코노미야끼...좋아하시나요",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100013981.1.17",
          "form": "전 엄청 좋아합니다..",
          "original_form": "전 엄청 좋아합니다..",
          "speaker_id": "2",
          "time": "20210825 21:34"
        },
        {
          "id": "MDRW2100013981.1.18",
          "form": "최고의 술안주 ... ^__^",
          "original_form": "최고의 술안주 ... ^__^",
          "speaker_id": "1",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100013981.1.19",
          "form": "저도 엄청 좋아합니다...",
          "original_form": "저도 엄청 좋아합니다...",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100013981.1.20",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 잘알이시잖아요.",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 잘알이시잖아요.",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100013981.1.21",
          "form": "소혀...사케랑 먹으면 진짜 맛있어요",
          "original_form": "소혀...사케랑 먹으면 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100013981.1.22",
          "form": "하 씁.... 저 침고이는데요 일본 가고싶다....",
          "original_form": "하 씁.... 저 침고이는데요 일본 가고싶다....",
