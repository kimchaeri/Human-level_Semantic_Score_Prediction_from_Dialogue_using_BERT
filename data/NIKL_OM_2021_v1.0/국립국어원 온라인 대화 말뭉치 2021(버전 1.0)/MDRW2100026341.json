      "utterance": [
        {
          "id": "MDRW2100026341.1.1",
          "form": "이제 사랑의 콜센터도 종영하겠네",
          "original_form": "이제 사랑의 콜센터도 종영하겠네",
          "speaker_id": "2",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026341.1.2",
          "form": "종영한다더나???",
          "original_form": "종영한다더나???",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026341.1.3",
          "form": "그렇다던데 티비에서...",
          "original_form": "그렇다던데 티비에서...",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026341.1.4",
          "form": "맞네 검색해보니 임영웅 하차하고 종영예정이라네",
          "original_form": "맞네 검색해보니 임영웅 하차하고 종영예정이라네",
          "speaker_id": "1",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026341.1.5",
          "form": "이제 볼거 없겠네",
          "original_form": "이제 볼거 없겠네",
          "speaker_id": "1",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026341.1.6",
          "form": "임영웅 안 나오면 재미없지 뭐",
          "original_form": "임영웅 안 나오면 재미없지 뭐",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100026341.1.7",
          "form": "아예 끝나는건가? 아님 임영웅만 안나오는건가",
          "original_form": "아예 끝나는건가? 아님 임영웅만 안나오는건가",
          "speaker_id": "1",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026341.1.8",
          "form": "모르겠지만 임영웅 안 나오면 안 볼 사람 많지",
          "original_form": "모르겠지만 임영웅 안 나오면 안 볼 사람 많지",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026341.1.9",
          "form": "그 무슨프로지 사콜말고 또 무슨프로있잖아",
          "original_form": "그 무슨프로지 사콜말고 또 무슨프로있잖아",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026341.1.10",
          "form": "뽕숭아 학당?",
          "original_form": "뽕숭아 학당?",
          "speaker_id": "2",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026341.1.11",
          "form": "그것도 종영하나?",
          "original_form": "그것도 종영하나?",
          "speaker_id": "1",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026341.1.12",
          "form": "그건 종영 안하나부던데 계속 한다고 그런던데",
          "original_form": "그건 종영 안하나부던데 계속 한다고 그런던데",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026341.1.13",
          "form": "잉? 사콜은 종영하는데 뽕숭아 종영안한다고?",
          "original_form": "잉? 사콜은 종영하는데 뽕숭아 종영안한다고?",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026341.1.14",
          "form": "아 검색해보니 둘다 종영아니네",
          "original_form": "아 검색해보니 둘다 종영아니네",
          "speaker_id": "1",
          "time": "20210901 21:48"
        },
        {
          "id": "MDRW2100026341.1.15",
          "form": "둘다 끝나는거 아니라고?",
          "original_form": "둘다 끝나는거 아니라고?",
          "speaker_id": "2",
          "time": "20210901 21:48"
        }
