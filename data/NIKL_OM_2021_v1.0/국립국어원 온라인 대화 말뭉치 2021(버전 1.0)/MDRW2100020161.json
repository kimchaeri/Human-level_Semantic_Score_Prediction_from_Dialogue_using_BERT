      "utterance": [
        {
          "id": "MDRW2100020161.1.1",
          "form": "오늘 저녁은 뭐드셨어요??",
          "original_form": "오늘 저녁은 뭐드셨어요??",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020161.1.2",
          "form": "전 삼겹살에 김치구워먹었어요",
          "original_form": "전 삼겹살에 김치구워먹었어요",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020161.1.3",
          "form": "저는 떡볶이 먹었어요",
          "original_form": "저는 떡볶이 먹었어요",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020161.1.4",
          "form": "하하하ㅋㅋㅋ",
          "original_form": "하하하ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020161.1.5",
          "form": "떡볶이 맛있었겠어요ㅠㅠ",
          "original_form": "떡볶이 맛있었겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020161.1.6",
          "form": "오 삼겹살 맛있었겠어요",
          "original_form": "오 삼겹살 맛있었겠어요",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020161.1.7",
          "form": "뭐야ㅠ",
          "original_form": "뭐야ㅠ",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020161.1.8",
          "form": "어디 브랜드 좋아하세요?",
          "original_form": "어디 브랜드 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020161.1.9",
          "form": "나랑 바꿔요",
          "original_form": "나랑 바꿔요",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020161.1.10",
          "form": "이거....운명..?",
          "original_form": "이거....운명..?",
          "speaker_id": "1",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020161.1.11",
          "form": "저는 신전!",
          "original_form": "저는 신전!",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020161.1.12",
          "form": "ㅋㅋㄱㅋㅌㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅌㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:01"
        },
        {
          "id": "MDRW2100020161.1.13",
          "form": "헐 저도 신전좋아해요",
          "original_form": "헐 저도 신전좋아해요",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020161.1.14",
          "form": "헐 저도 신전좋아해요",
          "original_form": "헐 저도 신전좋아해요",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020161.1.15",
          "form": "그 약간 카레향 나는 양념",
          "original_form": "그 약간 카레향 나는 양념",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020161.1.16",
          "form": "좋아하거든요",
          "original_form": "좋아하거든요",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020161.1.17",
          "form": "전 떡볶이에 무조건 튀김찍어먹어요",
          "original_form": "전 떡볶이에 무조건 튀김찍어먹어요",
          "speaker_id": "1",
          "time": "20210828 00:02"
        },
        {
          "id": "MDRW2100020161.1.18",
          "form": "맞아요 당연;",
          "original_form": "맞아요 당연;",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020161.1.19",
          "form": "떡볶이국물에 김밥 찍어먹는거",
          "original_form": "떡볶이국물에 김밥 찍어먹는거",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020161.1.20",
          "form": "짱좋아해요",
          "original_form": "짱좋아해요",
          "speaker_id": "2",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020161.1.21",
          "form": "중국당면...추가 잊으면 안됩니다",
          "original_form": "중국당면...추가 잊으면 안됩니다",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020161.1.22",
          "form": "맵게해서 드세요??",
          "original_form": "맵게해서 드세요??",
          "speaker_id": "1",
          "time": "20210828 00:03"
        },
        {
          "id": "MDRW2100020161.1.23",
          "form": "전 맵찔이라",
          "original_form": "전 맵찔이라",
          "speaker_id": "1",
          "time": "20210828 00:04"
        },
        {
          "id": "MDRW2100020161.1.24",
          "form": "저도 맵찔이...",
          "original_form": "저도 맵찔이...",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020161.1.25",
          "form": "저랑 재질이 같으시네요?",
          "original_form": "저랑 재질이 같으시네요?",
          "speaker_id": "2",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020161.1.26",
          "form": "이거이거.... 취향이",
          "original_form": "이거이거.... 취향이",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020161.1.27",
          "form": "비슷하시네요",
          "original_form": "비슷하시네요",
          "speaker_id": "1",
          "time": "20210828 00:05"
        },
        {
          "id": "MDRW2100020161.1.28",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020161.1.29",
          "form": "마라탕 좋아하시나요",
          "original_form": "마라탕 좋아하시나요",
          "speaker_id": "2",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020161.1.30",
          "form": "진짜 미쳐요 마라탕",
          "original_form": "진짜 미쳐요 마라탕",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020161.1.31",
          "form": "무족권 사랑해요",
          "original_form": "무족권 사랑해요",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020161.1.32",
          "form": "단배추많이 넣어서 중국당면이랑 옥수수면이랑",
          "original_form": "단배추많이 넣어서 중국당면이랑 옥수수면이랑",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020161.1.33",
          "form": "후;",
          "original_form": "후;",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020161.1.34",
          "form": "포두부 왕창",
          "original_form": "포두부 왕창",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020161.1.35",
          "form": "푸주왕창",
          "original_form": "푸주왕창",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020161.1.36",
          "form": "두부볼도 추가추가",
          "original_form": "두부볼도 추가추가",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020161.1.37",
          "form": "아. ...;;^^ 두부는 후 전",
          "original_form": "아. ...;;^^ 두부는 후 전",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020161.1.38",
          "form": "안넣습니다",
          "original_form": "안넣습니다",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020161.1.39",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020161.1.40",
          "form": "두부 넣을 공간에 고기넣어야 되거든요",
          "original_form": "두부 넣을 공간에 고기넣어야 되거든요",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020161.1.41",
          "form": "우린 운명 아닌걸로",
          "original_form": "우린 운명 아닌걸로",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020161.1.42",
          "form": "후...,",
          "original_form": "후...,",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020161.1.43",
          "form": "그것도 맞긴한데",
          "original_form": "그것도 맞긴한데",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020161.1.44",
          "form": "아쉽네요",
          "original_form": "아쉽네요",
          "speaker_id": "1",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020161.1.45",
          "form": "얇은 두부에 고기랑 배추랑 이것저것 둘둘감아서 한입 와앙 하면",
          "original_form": "얇은 두부에 고기랑 배추랑 이것저것 둘둘감아서 한입 와앙 하면",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020161.1.46",
          "form": "얼마나 맛있는데요",
          "original_form": "얼마나 맛있는데요",
          "speaker_id": "2",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020161.1.47",
          "form": "흠.....얘기 들어보니 맛있게 느껴지네요",
          "original_form": "흠.....얘기 들어보니 맛있게 느껴지네요",
          "speaker_id": "1",
          "time": "20210828 00:12"
        },
        {
          "id": "MDRW2100020161.1.48",
          "form": "거의 카피라이터 수준의",
          "original_form": "거의 카피라이터 수준의",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020161.1.49",
          "form": "묘사력이십니다",
          "original_form": "묘사력이십니다",
          "speaker_id": "1",
          "time": "20210828 00:13"
        },
        {
          "id": "MDRW2100020161.1.50",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:13"
        }
      ]
