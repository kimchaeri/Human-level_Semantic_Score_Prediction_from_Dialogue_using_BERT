        {
          "id": "MDRW2100013283.1.1",
          "form": "비 언제까지 오려나",
          "original_form": "비 언제까지 오려나",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013283.1.2",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013283.1.3",
          "form": "내일 예보 봤어?",
          "original_form": "내일 예보 봤어?",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013283.1.4",
          "form": "아니 못봣는데",
          "original_form": "아니 못봣는데",
          "speaker_id": "1",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013283.1.5",
          "form": "내일 다행히 비는 안오네",
          "original_form": "내일 다행히 비는 안오네",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100013283.1.6",
          "form": "아 다행이다  비오니 빨래도 꿉꿉해",
          "original_form": "아 다행이다  비오니 빨래도 꿉꿉해",
          "speaker_id": "1",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100013283.1.7",
          "form": "응 낼은 오후만 흐리네",
          "original_form": "응 낼은 오후만 흐리네",
          "speaker_id": "2",
          "time": "20210825 13:54"
        },
        {
          "id": "MDRW2100013283.1.8",
          "form": "낼도 햇볕에 빨래 말리긴 틀렷네 ㅎㅎ",
          "original_form": "낼도 햇볕에 빨래 말리긴 틀렷네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013283.1.9",
          "form": "확실히 비오니 옷에서 냄새나는 기붘",
          "original_form": "확실히 비오니 옷에서 냄새나는 기붘",
          "speaker_id": "2",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013283.1.10",
          "form": "응응 제습기 돌려도 그러더라고",
          "original_form": "응응 제습기 돌려도 그러더라고",
          "speaker_id": "1",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013283.1.11",
          "form": "어쩔수 없지",
          "original_form": "어쩔수 없지",
          "speaker_id": "2",
          "time": "20210825 13:55"
        },
        {
          "id": "MDRW2100013283.1.12",
          "form": "근데 금요일날 또비옴",
          "original_form": "근데 금요일날 또비옴",
          "speaker_id": "1",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013283.1.13",
          "form": "언제 해뜨려나 ㅎㅎㅎ",
          "original_form": "언제 해뜨려나 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:56"
        },
        {
          "id": "MDRW2100013283.1.14",
          "form": "그러게 비만오니 해뜨는게 좋네",
          "original_form": "그러게 비만오니 해뜨는게 좋네",
