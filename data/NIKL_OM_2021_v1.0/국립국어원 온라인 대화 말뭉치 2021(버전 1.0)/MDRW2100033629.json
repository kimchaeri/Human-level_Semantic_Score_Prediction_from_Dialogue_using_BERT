      "utterance": [
        {
          "id": "MDRW2100033629.1.1",
          "form": "코로나 끝나면 어디가고 싶으세요?",
          "original_form": "코로나 끝나면 어디가고 싶으세요?",
          "speaker_id": "1",
          "time": "20210824 18:59"
        },
        {
          "id": "MDRW2100033629.1.2",
          "form": "해외요",
          "original_form": "해외요",
          "speaker_id": "2",
          "time": "20210824 19:00"
        },
        {
          "id": "MDRW2100033629.1.3",
          "form": "어느나라건 해외로요",
          "original_form": "어느나라건 해외로요",
          "speaker_id": "2",
          "time": "20210824 19:01"
        },
        {
          "id": "MDRW2100033629.1.4",
          "form": "저도 해외나가고 싶어요",
          "original_form": "저도 해외나가고 싶어요",
          "speaker_id": "1",
          "time": "20210824 19:01"
        },
        {
          "id": "MDRW2100033629.1.5",
          "form": "출국한다는 그 설레임 느켜보고싶어요",
          "original_form": "출국한다는 그 설레임 느켜보고싶어요",
          "speaker_id": "2",
          "time": "20210824 19:01"
        },
        {
          "id": "MDRW2100033629.1.6",
          "form": "스페인이나 멕시코",
          "original_form": "스페인이나 멕시코",
          "speaker_id": "1",
          "time": "20210824 19:01"
        },
        {
          "id": "MDRW2100033629.1.7",
          "form": "여행하는거 좋아하진않는데",
          "original_form": "여행하는거 좋아하진않는데",
          "speaker_id": "1",
          "time": "20210824 19:01"
        },
        {
          "id": "MDRW2100033629.1.8",
          "form": "현지 음식을 맛보고싶어요",
          "original_form": "현지 음식을 맛보고싶어요",
          "speaker_id": "1",
          "time": "20210824 19:01"
        },
        {
          "id": "MDRW2100033629.1.9",
          "form": "거기로 정해진 이유가 있으세요?",
          "original_form": "거기로 정해진 이유가 있으세요?",
          "speaker_id": "2",
          "time": "20210824 19:01"
        },
        {
          "id": "MDRW2100033629.1.10",
          "form": "아 음식때문에요?",
          "original_form": "아 음식때문에요?",
          "speaker_id": "2",
          "time": "20210824 19:01"
        },
        {
          "id": "MDRW2100033629.1.11",
          "form": "타코를 좋아해요",
          "original_form": "타코를 좋아해요",
          "speaker_id": "1",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100033629.1.12",
          "form": "스페인 음식들이 맛나보여서요",
          "original_form": "스페인 음식들이 맛나보여서요",
          "speaker_id": "1",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100033629.1.13",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "2",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100033629.1.14",
          "form": "저는 안접해봤어요",
          "original_form": "저는 안접해봤어요",
          "speaker_id": "2",
          "time": "20210824 19:02"
        },
        {
          "id": "MDRW2100033629.1.15",
          "form": "비행시간이 길죠?",
          "original_form": "비행시간이 길죠?",
          "speaker_id": "2",
          "time": "20210824 19:03"
        },
        {
          "id": "MDRW2100033629.1.16",
          "form": "네 경유해야해서 길어요",
          "original_form": "네 경유해야해서 길어요",
          "speaker_id": "1",
          "time": "20210824 19:03"
        },
        {
          "id": "MDRW2100033629.1.17",
          "form": "어느나라가고 싶어요?",
          "original_form": "어느나라가고 싶어요?",
          "speaker_id": "1",
          "time": "20210824 19:03"
        },
        {
          "id": "MDRW2100033629.1.18",
          "form": "경유까지해야하는군요",
          "original_form": "경유까지해야하는군요",
          "speaker_id": "2",
          "time": "20210824 19:03"
        },
        {
          "id": "MDRW2100033629.1.19",
          "form": "스위스요",
          "original_form": "스위스요",
          "speaker_id": "2",
          "time": "20210824 19:03"
        },
        {
          "id": "MDRW2100033629.1.20",
          "form": "그 하얀 설산꼭대기에 신라면판다는데",
          "original_form": "그 하얀 설산꼭대기에 신라면판다는데",
          "speaker_id": "2",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100033629.1.21",
          "form": "저도 스위스 가보고 싶네요",
          "original_form": "저도 스위스 가보고 싶네요",
          "speaker_id": "1",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100033629.1.22",
          "form": "가서 먹어보고싶네요 ㅎㅎ",
          "original_form": "가서 먹어보고싶네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100033629.1.23",
          "form": "오 정말요?",
          "original_form": "오 정말요?",
          "speaker_id": "1",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100033629.1.24",
          "form": "색다르네요",
          "original_form": "색다르네요",
          "speaker_id": "1",
          "time": "20210824 19:04"
        },
        {
          "id": "MDRW2100033629.1.25",
          "form": "신라면 별로 안좋아하는데 ㅋㅋ",
          "original_form": "신라면 별로 안좋아하는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:04"
        }
      ]
