      "utterance": [
        {
          "id": "MDRW2100032361.1.1",
          "form": "어린이집 언제부터 보낼거야??",
          "original_form": "어린이집 언제부터 보낼거야??",
          "speaker_id": "1",
          "time": "20210905 16:16"
        },
        {
          "id": "MDRW2100032361.1.2",
          "form": "말하면 보내고 싶었는데 코로나 잡히면으로 맘이 바뀌는중",
          "original_form": "말하면 보내고 싶었는데 코로나 잡히면으로 맘이 바뀌는중",
          "speaker_id": "2",
          "time": "20210905 16:17"
        },
        {
          "id": "MDRW2100032361.1.3",
          "form": "여자애기들은 빠르면 18개월쯤 의사표현됨",
          "original_form": "여자애기들은 빠르면 18개월쯤 의사표현됨",
          "speaker_id": "1",
          "time": "20210905 16:17"
        },
        {
          "id": "MDRW2100032361.1.4",
          "form": "코로나가 잡히긴 할까? ㅋㅋㅋㅋ",
          "original_form": "코로나가 잡히긴 할까? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:17"
        },
        {
          "id": "MDRW2100032361.1.5",
          "form": "그래서 복권 됐음 좋겠어",
          "original_form": "그래서 복권 됐음 좋겠어",
          "speaker_id": "2",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100032361.1.6",
          "form": "얼마전엔 로또도 삿는데 하나도 안 맞드라",
          "original_form": "얼마전엔 로또도 삿는데 하나도 안 맞드라",
          "speaker_id": "2",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100032361.1.7",
          "form": "일확천금은 쉽지 않나봐",
          "original_form": "일확천금은 쉽지 않나봐",
          "speaker_id": "2",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100032361.1.8",
          "form": "요새 확진자 알림 몇 명 정도 떠?",
          "original_form": "요새 확진자 알림 몇 명 정도 떠?",
          "speaker_id": "2",
          "time": "20210905 16:19"
        },
        {
          "id": "MDRW2100032361.1.9",
          "form": "대전이 30~50 세종이 5~8 이정도",
          "original_form": "대전이 30~50 세종이 5~8 이정도",
          "speaker_id": "1",
          "time": "20210905 16:19"
        },
        {
          "id": "MDRW2100032361.1.10",
          "form": "근데 엊그제 갑자기 확 3단계로 내려버려서",
          "original_form": "근데 엊그제 갑자기 확 3단계로 내려버려서",
          "speaker_id": "1",
          "time": "20210905 16:19"
        },
        {
          "id": "MDRW2100032361.1.11",
          "form": "불안불안",
          "original_form": "불안불안",
          "speaker_id": "1",
          "time": "20210905 16:19"
        },
        {
          "id": "MDRW2100032361.1.12",
          "form": "여긴 열 명씩 꾸준히 나오더라",
          "original_form": "여긴 열 명씩 꾸준히 나오더라",
          "speaker_id": "2",
          "time": "20210905 16:20"
        },
        {
          "id": "MDRW2100032361.1.13",
          "form": "학교에서도 확진자 나오고",
          "original_form": "학교에서도 확진자 나오고",
          "speaker_id": "2",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100032361.1.14",
          "form": "영아 확진자도 있다그러고",
          "original_form": "영아 확진자도 있다그러고",
          "speaker_id": "2",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100032361.1.15",
          "form": "아직 어린이집 보내기가 무섭네...ㅠㅠ",
          "original_form": "아직 어린이집 보내기가 무섭네...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100032361.1.16",
          "form": "고등학교는 다음주부터 전면등교거든",
          "original_form": "고등학교는 다음주부터 전면등교거든",
          "speaker_id": "1",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100032361.1.17",
          "form": "근데 엊그제 둔녀에서 확진자 나오고",
          "original_form": "근데 엊그제 둔녀에서 확진자 나오고",
          "speaker_id": "1",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100032361.1.18",
          "form": "세종도 초등 고등 확진자 추가",
          "original_form": "세종도 초등 고등 확진자 추가",
          "speaker_id": "1",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100032361.1.19",
          "form": "어린이집 보내고 하는 휴직이 진짜 휴직인데 ㅠㅠ",
          "original_form": "어린이집 보내고 하는 휴직이 진짜 휴직인데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 16:22"
        },
        {
          "id": "MDRW2100032361.1.20",
          "form": "망할 코로나",
          "original_form": "망할 코로나",
          "speaker_id": "1",
          "time": "20210905 16:22"
        },
        {
          "id": "MDRW2100032361.1.21",
          "form": "전면등교 하는구나. 언제까지 미룰 수도 없고. 근데 무섭기는 하고 그러네",
          "original_form": "전면등교 하는구나. 언제까지 미룰 수도 없고. 근데 무섭기는 하고 그러네",
          "speaker_id": "2",
          "time": "20210905 16:24"
        },
        {
          "id": "MDRW2100032361.1.22",
          "form": "변이는 왜 자꾸 나오고",
          "original_form": "변이는 왜 자꾸 나오고",
          "speaker_id": "2",
          "time": "20210905 16:24"
        },
        {
          "id": "MDRW2100032361.1.23",
          "form": "사스나 메르스는 이렇지 않았잖아?",
          "original_form": "사스나 메르스는 이렇지 않았잖아?",
          "speaker_id": "2",
          "time": "20210905 16:25"
        },
        {
          "id": "MDRW2100032361.1.24",
          "form": "세종은 코로나 터지고 지금까지 그냥 쭉 전면등교였대",
          "original_form": "세종은 코로나 터지고 지금까지 그냥 쭉 전면등교였대",
          "speaker_id": "1",
          "time": "20210905 16:25"
        },
        {
          "id": "MDRW2100032361.1.25",
          "form": "난 2차까지 접종 끝냈지만 불안불안",
          "original_form": "난 2차까지 접종 끝냈지만 불안불안",
          "speaker_id": "1",
          "time": "20210905 16:25"
        },
        {
          "id": "MDRW2100032361.1.26",
          "form": "접종 완료자로군 멋져요",
          "original_form": "접종 완료자로군 멋져요",
          "speaker_id": "2",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100032361.1.27",
          "form": "나도 담주에 2차 맞는다",
          "original_form": "나도 담주에 2차 맞는다",
          "speaker_id": "2",
          "time": "20210905 16:28"
        },
        {
          "id": "MDRW2100032361.1.28",
          "form": "와우 축하드려요",
          "original_form": "와우 축하드려요",
          "speaker_id": "1",
          "time": "20210905 16:29"
        },
        {
          "id": "MDRW2100032361.1.29",
          "form": "2차접종 후 14일 지난자 으쓱",
          "original_form": "2차접종 후 14일 지난자 으쓱",
          "speaker_id": "1",
          "time": "20210905 16:29"
        },
        {
          "id": "MDRW2100032361.1.30",
          "form": "ㅋㅋㅋ그거 큐알코드 찍으면 뜨더라?",
          "original_form": "ㅋㅋㅋ그거 큐알코드 찍으면 뜨더라?",
          "speaker_id": "2",
          "time": "20210905 16:29"
        },
        {
          "id": "MDRW2100032361.1.31",
          "form": "괜히 혼자 뿌듯.. ㅋㅋㅋㅋㅋㅋ",
          "original_form": "괜히 혼자 뿌듯.. ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:30"
        },
        {
          "id": "MDRW2100032361.1.32",
          "form": "애기는 영원히 백신 없는 건가",
          "original_form": "애기는 영원히 백신 없는 건가",
          "speaker_id": "2",
          "time": "20210905 16:31"
        },
        {
          "id": "MDRW2100032361.1.33",
          "form": "오빠랑 애기들 뽀로로키즈카페간대 ㅋㅋㅋ",
          "original_form": "오빠랑 애기들 뽀로로키즈카페간대 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:31"
        },
        {
          "id": "MDRW2100032361.1.34",
          "form": "이야!!!",
          "original_form": "이야!!!",
          "speaker_id": "2",
          "time": "20210905 16:31"
        },
        {
          "id": "MDRW2100032361.1.35",
          "form": "좋은 소식",
          "original_form": "좋은 소식",
          "speaker_id": "2",
          "time": "20210905 16:31"
        },
        {
          "id": "MDRW2100032361.1.36",
          "form": "그치그치 집에선 감당이 안돼",
          "original_form": "그치그치 집에선 감당이 안돼",
          "speaker_id": "1",
          "time": "20210905 16:31"
        },
        {
          "id": "MDRW2100032361.1.37",
          "form": "집에 있으면 내가 더 불안불안",
          "original_form": "집에 있으면 내가 더 불안불안",
          "speaker_id": "1",
          "time": "20210905 16:31"
        },
        {
          "id": "MDRW2100032361.1.38",
          "form": "집에 빨리가야될것같고 막",
          "original_form": "집에 빨리가야될것같고 막",
          "speaker_id": "1",
          "time": "20210905 16:31"
        },
        {
          "id": "MDRW2100032361.1.39",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210905 16:31"
        },
        {
          "id": "MDRW2100032361.1.40",
          "form": "얼마전에 애기 영유아검진할때 의사가 내년이나 후년쯤엔 애기들도 독감 예방접종처럼 같이 접종할거라고 하드라",
          "original_form": "얼마전에 애기 영유아검진할때 의사가 내년이나 후년쯤엔 애기들도 독감 예방접종처럼 같이 접종할거라고 하드라",
          "speaker_id": "1",
