      "utterance": [
        {
          "id": "MDRW2100029625.1.1",
          "form": "오옷 ㅎㅅㅎ님",
          "original_form": "오옷 ㅎㅅㅎ님",
          "speaker_id": "1",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029625.1.2",
          "form": "읽고계신 책 있으신가요^^",
          "original_form": "읽고계신 책 있으신가요^^",
          "speaker_id": "1",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029625.1.3",
          "form": "아니요ㅠㅠㅠㅠ",
          "original_form": "아니요ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029625.1.4",
          "form": "독서를 안하네요",
          "original_form": "독서를 안하네요",
          "speaker_id": "2",
          "time": "20210903 11:00"
        },
        {
          "id": "MDRW2100029625.1.5",
          "form": "새콤달콤 님은 독서 좋아하시나요??",
          "original_form": "새콤달콤 님은 독서 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029625.1.6",
          "form": "읽고계신 책이 있다면 추천을...!!",
          "original_form": "읽고계신 책이 있다면 추천을...!!",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029625.1.7",
          "form": "ㅋㅋㅋ 귀여우시네요",
          "original_form": "ㅋㅋㅋ 귀여우시네요",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029625.1.8",
          "form": "근데 죄송하게도 저도 독서와 거리가멀어서",
          "original_form": "근데 죄송하게도 저도 독서와 거리가멀어서",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029625.1.9",
          "form": "겨우읽는책이 아이들관련 책이에요^^;",
          "original_form": "겨우읽는책이 아이들관련 책이에요^^;",
          "speaker_id": "1",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029625.1.10",
          "form": "아 육아공부를 하시는군요",
          "original_form": "아 육아공부를 하시는군요",
          "speaker_id": "2",
          "time": "20210903 11:01"
        },
        {
          "id": "MDRW2100029625.1.11",
          "form": "지난주에 오랜만에 읽은책이..\r\n보통엄마의 책육아 였던거같은데...",
          "original_form": "지난주에 오랜만에 읽은책이..\r\n보통엄마의 책육아 였던거같은데...",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029625.1.12",
          "form": "사실 엄청 추천하긴 좀그런...ㅡ????ㅋㅋ",
          "original_form": "사실 엄청 추천하긴 좀그런...ㅡ????ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029625.1.13",
          "form": "오 어때요???",
          "original_form": "오 어때요???",
          "speaker_id": "2",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029625.1.14",
          "form": "도움이 되나요???",
          "original_form": "도움이 되나요???",
          "speaker_id": "2",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029625.1.15",
          "form": "그냥  오은영박사님 책을 읽어야겠다 생각이들었어요",
          "original_form": "그냥  오은영박사님 책을 읽어야겠다 생각이들었어요",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029625.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029625.1.17",
          "form": "아니요 전문적이진 못하더라구요",
          "original_form": "아니요 전문적이진 못하더라구요",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029625.1.18",
          "form": "아....아쉽네요",
          "original_form": "아....아쉽네요",
          "speaker_id": "2",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029625.1.19",
          "form": "그냥 경험담이 책한권으로 나온느낌이었어요",
          "original_form": "그냥 경험담이 책한권으로 나온느낌이었어요",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029625.1.20",
          "form": "반복되는 내용도 꽤 많았구요",
          "original_form": "반복되는 내용도 꽤 많았구요",
          "speaker_id": "1",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029625.1.21",
          "form": "앗.....",
          "original_form": "앗.....",
          "speaker_id": "2",
          "time": "20210903 11:02"
        },
        {
          "id": "MDRW2100029625.1.22",
          "form": "사실 뭔가 요새 자기계발서 유튜브 내용을 텍스트로 옮긴 느낌이 나던데요...",
          "original_form": "사실 뭔가 요새 자기계발서 유튜브 내용을 텍스트로 옮긴 느낌이 나던데요...",
          "speaker_id": "2",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029625.1.23",
          "form": "생각난김에",
          "original_form": "생각난김에",
          "speaker_id": "1",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029625.1.24",
          "form": "내일 도서관에 가봐야겠네요",
          "original_form": "내일 도서관에 가봐야겠네요",
          "speaker_id": "1",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029625.1.25",
          "form": "정말 오은영박사님 책이 낫겠네요ㅠㅠㅠ",
          "original_form": "정말 오은영박사님 책이 낫겠네요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029625.1.26",
          "form": "도서관 자주 가세요??",
          "original_form": "도서관 자주 가세요??",
          "speaker_id": "2",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029625.1.27",
          "form": "오 소오오오름",
          "original_form": "오 소오오오름",
          "speaker_id": "1",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029625.1.28",
          "form": "ㅎㅅㅎ님 완전",
          "original_form": "ㅎㅅㅎ님 완전",
          "speaker_id": "1",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029625.1.29",
          "form": "무슨말인지 알거같아요",
          "original_form": "무슨말인지 알거같아요",
          "speaker_id": "1",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029625.1.30",
          "form": "유튜브 내용을 텍스트롷ㅋㅋ",
          "original_form": "유튜브 내용을 텍스트롷ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:03"
        },
        {
          "id": "MDRW2100029625.1.31",
          "form": "ㅋㅋ아니요 가긴가는데 전부 아이들 도서코너만 있다가와서요..",
          "original_form": "ㅋㅋ아니요 가긴가는데 전부 아이들 도서코너만 있다가와서요..",
          "speaker_id": "1",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029625.1.32",
          "form": "저 사실",
          "original_form": "저 사실",
          "speaker_id": "2",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029625.1.33",
          "form": "아이들 읽는 동화 좋아해요 ㅋㅋㅋ",
          "original_form": "아이들 읽는 동화 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029625.1.34",
          "form": "책도 다 저희집 상전 유치원생들 책만..ㅎㅎ",
          "original_form": "책도 다 저희집 상전 유치원생들 책만..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029625.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029625.1.36",
          "form": "ㅋㅋㅋ 근데 읽어주다보면",
          "original_form": "ㅋㅋㅋ 근데 읽어주다보면",
          "speaker_id": "1",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029625.1.37",
          "form": "재밌는것도 많아요!",
          "original_form": "재밌는것도 많아요!",
          "speaker_id": "1",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029625.1.38",
          "form": "맞아요!!!!",
          "original_form": "맞아요!!!!",
          "speaker_id": "2",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029625.1.39",
          "form": "어떤 동화가 가장 좋으세요???",
          "original_form": "어떤 동화가 가장 좋으세요???",
          "speaker_id": "2",
          "time": "20210903 11:04"
        },
        {
          "id": "MDRW2100029625.1.40",
          "form": "저 지나가다가 본 생쥐인지 나오는 동화 삽화가 너무 예뻐서 괜히 사고싶더라구요 ㅋㅋㅋ",
          "original_form": "저 지나가다가 본 생쥐인지 나오는 동화 삽화가 너무 예뻐서 괜히 사고싶더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029625.1.41",
          "form": "아 저희어릴때보던 디즈니책도 재밌겠더라구요 (저희집엔 없어요)",
          "original_form": "아 저희어릴때보던 디즈니책도 재밌겠더라구요 (저희집엔 없어요)",
          "speaker_id": "1",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029625.1.42",
          "form": "추천받아서 빌려 본 책중에",
          "original_form": "추천받아서 빌려 본 책중에",
          "speaker_id": "1",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029625.1.43",
          "form": "\"방귀쟁이 며느리\" 라는책이 있는데 엄청재밌어요 ㅋㄱㅋㄱ",
          "original_form": "\"방귀쟁이 며느리\" 라는책이 있는데 엄청재밌어요 ㅋㄱㅋㄱ",
          "speaker_id": "1",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029625.1.44",
          "form": "아 디즈니책 맞아요 ㅋㅋㅋ삽화도 이쁘구요 ㅋㅋㅋ",
          "original_form": "아 디즈니책 맞아요 ㅋㅋㅋ삽화도 이쁘구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:06"
        },
        {
          "id": "MDRW2100029625.1.45",
          "form": "오 제목부터가 아이들이 좋아할 제목이예요 ㅋㅋㅋ",
          "original_form": "오 제목부터가 아이들이 좋아할 제목이예요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:07"
        },
        {
          "id": "MDRW2100029625.1.46",
          "form": "그쵸 ㅋㅋㅋ  넘 재밌더라구요 애들 깔깔 웃고 ㅎ",
          "original_form": "그쵸 ㅋㅋㅋ  넘 재밌더라구요 애들 깔깔 웃고 ㅎ",
          "speaker_id": "1",
          "time": "20210903 11:07"
        }
      ]
