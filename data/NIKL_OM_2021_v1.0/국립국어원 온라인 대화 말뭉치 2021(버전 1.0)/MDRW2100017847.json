      "utterance": [
        {
          "id": "MDRW2100017847.1.1",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017847.1.2",
          "form": "제가 하는건 안좋아하고 보는 것만 좋아해요 ㅋㅋㅋ",
          "original_form": "제가 하는건 안좋아하고 보는 것만 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017847.1.3",
          "form": "올림픽 보셨나요?",
          "original_form": "올림픽 보셨나요?",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017847.1.4",
          "form": "양궁만 봤어요ㅋㅋ",
          "original_form": "양궁만 봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017847.1.5",
          "form": "저도 양궁 재밌게 봤어요 ㅋㅋ",
          "original_form": "저도 양궁 재밌게 봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017847.1.6",
          "form": "실시간으로 보셨나요?",
          "original_form": "실시간으로 보셨나요?",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017847.1.7",
          "form": "네! 체조나 야구 축구는 짤들 조금씩 봤지만 양궁만큼은 챙겨봤어요!",
          "original_form": "네! 체조나 야구 축구는 짤들 조금씩 봤지만 양궁만큼은 챙겨봤어요!",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017847.1.8",
          "form": "제일 재밌게 보신게 어떤거에요?",
          "original_form": "제일 재밌게 보신게 어떤거에요?",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017847.1.9",
          "form": "저도 양궁 결승전은 실시간으로 봤는데",
          "original_form": "저도 양궁 결승전은 실시간으로 봤는데",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017847.1.10",
          "form": "선수보다 제가 더 긴장하더라구요 ㅋㅋㅋㅋ",
          "original_form": "선수보다 제가 더 긴장하더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017847.1.11",
          "form": "ㅋㅋ저도요ㅋㅋ 선수의 심장박동이 안정적이였죠",
          "original_form": "ㅋㅋ저도요ㅋㅋ 선수의 심장박동이 안정적이였죠",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017847.1.12",
          "form": "그리고 여자배구도 재밌게 봤어요",
          "original_form": "그리고 여자배구도 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017847.1.13",
          "form": "한일전 잊을 수 없을거같아요 ㅋㅋㅋㅋ",
          "original_form": "한일전 잊을 수 없을거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017847.1.14",
          "form": "아 엄청났나봐요!",
          "original_form": "아 엄청났나봐요!",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017847.1.15",
          "form": "저는 준결승만 조금봐서 그건 못봤네요ㅜ",
          "original_form": "저는 준결승만 조금봐서 그건 못봤네요ㅜ",
          "speaker_id": "1",
          "time": "20210826 21:31"
        },
        {
          "id": "MDRW2100017847.1.16",
          "form": "네네 뒤쪽경기는 오히려 선수들이 지친모습이 보이더라구요",
          "original_form": "네네 뒤쪽경기는 오히려 선수들이 지친모습이 보이더라구요",
          "speaker_id": "2",
          "time": "20210826 21:32"
        },
        {
          "id": "MDRW2100017847.1.17",
          "form": "예선전 이긴 경기들이 더 잘했던거같아요!",
          "original_form": "예선전 이긴 경기들이 더 잘했던거같아요!",
          "speaker_id": "2",
          "time": "20210826 21:33"
        },
        {
          "id": "MDRW2100017847.1.18",
          "form": "평소에 직관도 하세요?",
          "original_form": "평소에 직관도 하세요?",
          "speaker_id": "2",
          "time": "20210826 21:34"
        },
        {
          "id": "MDRW2100017847.1.19",
          "form": "아ㅠ 다음에 슬쩍 봐야겠어요",
          "original_form": "아ㅠ 다음에 슬쩍 봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017847.1.20",
          "form": "아뇨 직관은 해본적이 없어요",
          "original_form": "아뇨 직관은 해본적이 없어요",
          "speaker_id": "1",
          "time": "20210826 21:35"
        },
        {
          "id": "MDRW2100017847.1.21",
          "form": "직관 하시는거 좋아하세요?",
          "original_form": "직관 하시는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017847.1.22",
          "form": "가족들이 야구 좋아해서",
          "original_form": "가족들이 야구 좋아해서",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017847.1.23",
          "form": "야구 경기장은 몇번 갔었어요",
          "original_form": "야구 경기장은 몇번 갔었어요",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017847.1.24",
          "form": "직접 보면 아무래도 분위기에 휩쓸리는건 있는데",
          "original_form": "직접 보면 아무래도 분위기에 휩쓸리는건 있는데",
          "speaker_id": "2",
          "time": "20210826 21:36"
        },
        {
          "id": "MDRW2100017847.1.25",
          "form": "집에서 티비로 보는것만큼 선수들이 잘 보이고 그러진 않아서 아쉽더라구요",
          "original_form": "집에서 티비로 보는것만큼 선수들이 잘 보이고 그러진 않아서 아쉽더라구요",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017847.1.26",
          "form": "다들 면봉만하게 보여요 ㅋㅋㅋ",
          "original_form": "다들 면봉만하게 보여요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:37"
        },
        {
          "id": "MDRW2100017847.1.27",
          "form": "아ㅋㅋ 완전 다르네요",
          "original_form": "아ㅋㅋ 완전 다르네요",
          "speaker_id": "1",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017847.1.28",
          "form": "네 그래서 호불호가 갈리는거같아요",
          "original_form": "네 그래서 호불호가 갈리는거같아요",
          "speaker_id": "2",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017847.1.29",
          "form": "면봉이라니 궁금해요ㅋㅋ",
          "original_form": "면봉이라니 궁금해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:39"
        },
        {
          "id": "MDRW2100017847.1.30",
          "form": "전 그냥 그 돈으로 집에서 치킨 시켜놓고 티비로 보자는 주의에요 ㅋㅋㅋㅋ",
          "original_form": "전 그냥 그 돈으로 집에서 치킨 시켜놓고 티비로 보자는 주의에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:39"
        }
      ]
    }
