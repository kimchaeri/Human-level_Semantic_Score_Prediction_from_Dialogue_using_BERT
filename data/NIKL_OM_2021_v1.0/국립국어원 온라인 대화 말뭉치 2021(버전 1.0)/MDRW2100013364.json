      "utterance": [
        {
          "id": "MDRW2100013364.1.1",
          "form": "오늘 설거지 하셨나요?",
          "original_form": "오늘 설거지 하셨나요?",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013364.1.2",
          "form": "일단 하긴했어요",
          "original_form": "일단 하긴했어요",
          "speaker_id": "2",
          "time": "20210825 14:13"
        },
        {
          "id": "MDRW2100013364.1.3",
          "form": "저는 해야되는뎅",
          "original_form": "저는 해야되는뎅",
          "speaker_id": "1",
          "time": "20210825 14:13"
        },
        {
          "id": "MDRW2100013364.1.4",
          "form": "밥먹고나서 설거지하기싫어서요",
          "original_form": "밥먹고나서 설거지하기싫어서요",
          "speaker_id": "2",
          "time": "20210825 14:13"
        },
        {
          "id": "MDRW2100013364.1.5",
          "form": "너무 귀찮네요",
          "original_form": "너무 귀찮네요",
          "speaker_id": "1",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100013364.1.6",
          "form": "미리미리 하는편이에요",
          "original_form": "미리미리 하는편이에요",
          "speaker_id": "2",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100013364.1.7",
          "form": "식기세척기 사고싶어요",
          "original_form": "식기세척기 사고싶어요",
          "speaker_id": "1",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100013364.1.8",
          "form": "하나 장만하시는건어떨까요",
          "original_form": "하나 장만하시는건어떨까요",
          "speaker_id": "2",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100013364.1.9",
          "form": "미리미리구나 저는 미뤘다가 하는 스타일",
          "original_form": "미리미리구나 저는 미뤘다가 하는 스타일",
          "speaker_id": "1",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100013364.1.10",
          "form": "접시 쌓인거 한번에 하죠",
          "original_form": "접시 쌓인거 한번에 하죠",
          "speaker_id": "1",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100013364.1.11",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100013364.1.12",
          "form": "그럼 시간이 더 단축되지 않응까요",
          "original_form": "그럼 시간이 더 단축되지 않응까요",
          "speaker_id": "1",
          "time": "20210825 14:14"
        },
        {
          "id": "MDRW2100013364.1.13",
          "form": "저는 미리 다 하고 마무리로 적게하는게좋더라고요",
          "original_form": "저는 미리 다 하고 마무리로 적게하는게좋더라고요",
          "speaker_id": "2",
          "time": "20210825 14:15"
        },
        {
          "id": "MDRW2100013364.1.14",
          "form": "제 설거지도 해주시면 되겠더",
          "original_form": "제 설거지도 해주시면 되겠더",
          "speaker_id": "1",
          "time": "20210825 14:15"
        }
