      "utterance": [
        {
          "id": "MDRW2100000465.1.1",
          "form": "혹시 저녁 뭐먹어?",
          "original_form": "혹시 저녁 뭐먹어?",
          "speaker_id": "1",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000465.1.2",
          "form": "돼지불고기 먹으려고",
          "original_form": "돼지불고기 먹으려고",
          "speaker_id": "2",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000465.1.3",
          "form": "코스트코에서 잔뜩 사 놨거든",
          "original_form": "코스트코에서 잔뜩 사 놨거든",
          "speaker_id": "2",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000465.1.4",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000465.1.5",
          "form": "돼지불고기 좋지~ 나는 점심 너무 많이먹어서 저녁 안먹으려구",
          "original_form": "돼지불고기 좋지~ 나는 점심 너무 많이먹어서 저녁 안먹으려구",
          "speaker_id": "1",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000465.1.6",
          "form": "상큼한 무언가가 땡겨",
          "original_form": "상큼한 무언가가 땡겨",
          "speaker_id": "1",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000465.1.7",
          "form": "오호 상큼한 거라면",
          "original_form": "오호 상큼한 거라면",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000465.1.8",
          "form": "샐러드 같은?",
          "original_form": "샐러드 같은?",
          "speaker_id": "2",
          "time": "20210819 16:16"
        },
        {
          "id": "MDRW2100000465.1.9",
          "form": "아 아니 음료로 ㅎ",
          "original_form": "아 아니 음료로 ㅎ",
          "speaker_id": "1",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000465.1.10",
          "form": "자몽에이드?",
          "original_form": "자몽에이드?",
          "speaker_id": "1",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000465.1.11",
          "form": "이런거",
          "original_form": "이런거",
          "speaker_id": "1",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000465.1.12",
          "form": "오 나 방금 아이스자몽티 마셨어",
          "original_form": "오 나 방금 아이스자몽티 마셨어",
          "speaker_id": "2",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000465.1.13",
          "form": "달콤씁쓸 좋지",
          "original_form": "달콤씁쓸 좋지",
          "speaker_id": "2",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000465.1.14",
          "form": "아 좋겠다,, 나 스벅 쿠폰이 너무많아서 쓰긴해야하는데 가기가 귀찮네",
          "original_form": "아 좋겠다,, 나 스벅 쿠폰이 너무많아서 쓰긴해야하는데 가기가 귀찮네",
          "speaker_id": "1",
          "time": "20210819 16:19"
        },
        {
          "id": "MDRW2100000465.1.15",
          "form": "없어서 못 마시지...",
          "original_form": "없어서 못 마시지...",
          "speaker_id": "2",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000465.1.16",
          "form": "더울 땐 시원한 음료가 최고",
          "original_form": "더울 땐 시원한 음료가 최고",
          "speaker_id": "2",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000465.1.17",
          "form": "난 아인슈페너도 좋아해서 도장깨기 하고 있어",
          "original_form": "난 아인슈페너도 좋아해서 도장깨기 하고 있어",
          "speaker_id": "2",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000465.1.18",
          "form": "맛있다는 곳 추천받아서!",
          "original_form": "맛있다는 곳 추천받아서!",
          "speaker_id": "2",
          "time": "20210819 16:20"
        },
        {
          "id": "MDRW2100000465.1.19",
          "form": "아 아인슈페너 너무맛있지 ㅠ 나는아까 밥먹고 아메리카노랑 마카롱 때렸어",
          "original_form": "아 아인슈페너 너무맛있지 ㅠ 나는아까 밥먹고 아메리카노랑 마카롱 때렸어",
          "speaker_id": "1",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000465.1.20",
          "form": "크흐",
          "original_form": "크흐",
          "speaker_id": "2",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000465.1.21",
          "form": "얼마전에 일리 커피 샀는데 너무 맛있더라",
          "original_form": "얼마전에 일리 커피 샀는데 너무 맛있더라",
          "speaker_id": "2",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000465.1.22",
          "form": "아인슈페너 어디가 맛있때?",
          "original_form": "아인슈페너 어디가 맛있때?",
          "speaker_id": "1",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000465.1.23",
          "form": "나도 일리좋아해 ㅠㅠ어떤색 즐겨먹어?",
          "original_form": "나도 일리좋아해 ㅠㅠ어떤색 즐겨먹어?",
          "speaker_id": "1",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000465.1.24",
          "form": "일단 내 마음속 1등은 혜화에 있는 곳!",
          "original_form": "일단 내 마음속 1등은 혜화에 있는 곳!",
          "speaker_id": "2",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000465.1.25",
          "form": "이름이 잠시만",
          "original_form": "이름이 잠시만",
          "speaker_id": "2",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000465.1.26",
          "form": "온더모먼트!",
          "original_form": "온더모먼트!",
          "speaker_id": "2",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000465.1.27",
          "form": "우와 나도 나중에 가봐야겠다",
          "original_form": "우와 나도 나중에 가봐야겠다",
          "speaker_id": "1",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000465.1.28",
          "form": "혹시 서울에서 맛집 추천해줄곳 있어?",
          "original_form": "혹시 서울에서 맛집 추천해줄곳 있어?",
          "speaker_id": "1",
          "time": "20210819 16:25"
        },
        {
          "id": "MDRW2100000465.1.29",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000465.1.30",
          "form": "어떤 종류?",
          "original_form": "어떤 종류?",
          "speaker_id": "2",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000465.1.31",
          "form": "음,, 종류 상관없이 잊혀지지않는 맛? 이런곳 있어?",
          "original_form": "음,, 종류 상관없이 잊혀지지않는 맛? 이런곳 있어?",
          "speaker_id": "1",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000465.1.32",
          "form": "갑자기 삼계탕이 먹고 싶어서",
          "original_form": "갑자기 삼계탕이 먹고 싶어서",
          "speaker_id": "2",
          "time": "20210819 16:26"
        },
        {
          "id": "MDRW2100000465.1.33",
          "form": "토속촌?",
          "original_form": "토속촌?",
          "speaker_id": "2",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000465.1.34",
          "form": "삼계탕 ㅋㅋㅋㅋ 음 양식으로는?",
          "original_form": "삼계탕 ㅋㅋㅋㅋ 음 양식으로는?",
          "speaker_id": "1",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000465.1.35",
          "form": "이태원에 보니스피자",
          "original_form": "이태원에 보니스피자",
          "speaker_id": "2",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000465.1.36",
          "form": "부자피자!",
          "original_form": "부자피자!",
          "speaker_id": "2",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000465.1.37",
          "form": "이태원 가야겠다 한번밖에 못가봤는데",
          "original_form": "이태원 가야겠다 한번밖에 못가봤는데",
          "speaker_id": "1",
          "time": "20210819 16:31"
        },
        {
          "id": "MDRW2100000465.1.38",
          "form": "난 저번에 도곡에 마린레스토랑이라고 있거든 거기 고등어파스타가 진짜맛있었어",
          "original_form": "난 저번에 도곡에 마린레스토랑이라고 있거든 거기 고등어파스타가 진짜맛있었어",
          "speaker_id": "1",
          "time": "20210819 16:31"
        },
        {
          "id": "MDRW2100000465.1.39",
          "form": "영등포역에 항정사이라고 고깃집있는데 여기 내최애고깃집이야 ㅠ",
          "original_form": "영등포역에 항정사이라고 고깃집있는데 여기 내최애고깃집이야 ㅠ",
          "speaker_id": "1",
          "time": "20210819 16:31"
        },
        {
          "id": "MDRW2100000465.1.40",
          "form": "말만 들어도 맛있겠다!",
          "original_form": "말만 들어도 맛있겠다!",
          "speaker_id": "2",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000465.1.41",
          "form": "고기고기",
          "original_form": "고기고기",
          "speaker_id": "2",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000465.1.42",
          "form": "남친이 요리해서 뭐만먹으면 평가를 하는데 ㅋㅋㅋ그래서 어딜가도 좀 까다로워서 제대로된곳 가야해",
          "original_form": "남친이 요리해서 뭐만먹으면 평가를 하는데 ㅋㅋㅋ그래서 어딜가도 좀 까다로워서 제대로된곳 가야해",
          "speaker_id": "1",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000465.1.43",
          "form": "와 미식가구나",
          "original_form": "와 미식가구나",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000465.1.44",
          "form": "그럼 진짜 맛집 찾아 가야겠네",
          "original_form": "그럼 진짜 맛집 찾아 가야겠네",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000465.1.45",
          "form": "그럼 웨이팅하는 것도 괜찮아 해?",
          "original_form": "그럼 웨이팅하는 것도 괜찮아 해?",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000465.1.46",
          "form": "난 웨이팅 너무 싫어 ㅠㅠ",
          "original_form": "난 웨이팅 너무 싫어 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 16:36"
        },
        {
          "id": "MDRW2100000465.1.47",
          "form": "아 웨이팅 진짜싫어해 ㅋㅋㅋㅋ 나도 사실 좋아하지않고",
          "original_form": "아 웨이팅 진짜싫어해 ㅋㅋㅋㅋ 나도 사실 좋아하지않고",
          "speaker_id": "1",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000465.1.48",
          "form": "진짜 맛집 찾아야돼,, 힘들어그래서 사실 내가 안먹는게많아서 그럴수도있을거같아 ㅋㅋㅋ",
          "original_form": "진짜 맛집 찾아야돼,, 힘들어그래서 사실 내가 안먹는게많아서 그럴수도있을거같아 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:37"
        },
        {
          "id": "MDRW2100000465.1.49",
          "form": "그럼 남친이보고 찾으라고 하자",
          "original_form": "그럼 남친이보고 찾으라고 하자",
          "speaker_id": "2",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000465.1.50",
          "form": "어떤 거 안 먹어?",
          "original_form": "어떤 거 안 먹어?",
          "speaker_id": "2",
          "time": "20210819 16:38"
        },
        {
          "id": "MDRW2100000465.1.51",
          "form": "칼국수 카레 초밥 돈까스 등등,,",
          "original_form": "칼국수 카레 초밥 돈까스 등등,,",
          "speaker_id": "1",
          "time": "20210819 16:39"
        },
        {
          "id": "MDRW2100000465.1.52",
          "form": "안먹는다기보다는 즐겨하질않는거같아",
          "original_form": "안먹는다기보다는 즐겨하질않는거같아",
          "speaker_id": "1",
          "time": "20210819 16:39"
        },
        {
          "id": "MDRW2100000465.1.53",
          "form": "아이쿠",
          "original_form": "아이쿠",
          "speaker_id": "2",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000465.1.54",
          "form": "다 너무 맛있는데",
          "original_form": "다 너무 맛있는데",
          "speaker_id": "2",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000465.1.55",
          "form": "그럼 어떤 거 즐겨먹어?",
          "original_form": "그럼 어떤 거 즐겨먹어?",
          "speaker_id": "2",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000465.1.56",
          "form": "역시 즐겨먹는건 고기지! ㅋㅋㅋ 나 족발좋아해서 자주먹어 불족발!",
          "original_form": "역시 즐겨먹는건 고기지! ㅋㅋㅋ 나 족발좋아해서 자주먹어 불족발!",
          "speaker_id": "1",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000465.1.57",
          "form": "저번주에 먹었는데 또먹고싶다",
          "original_form": "저번주에 먹었는데 또먹고싶다",
          "speaker_id": "1",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000465.1.58",
          "form": "매운 거 잘 먹는구나?",
          "original_form": "매운 거 잘 먹는구나?",
          "speaker_id": "2",
          "time": "20210819 16:54"
        },
        {
          "id": "MDRW2100000465.1.59",
          "form": "난 매운 거 진짜 못 먹거든",
          "original_form": "난 매운 거 진짜 못 먹거든",
          "speaker_id": "2",
          "time": "20210819 16:55"
        },
        {
          "id": "MDRW2100000465.1.60",
          "form": "아그렇구나 내남친도 진짜못먹는데 ㅠ 난 닭발 이런거좋아하는데 남친은 매워서 못먹어 ㅜ",
