      "utterance": [
        {
          "id": "MDRW2100023903.1.1",
          "form": "네 전 얀센 맞았습니다",
          "original_form": "네 전 얀센 맞았습니다",
          "speaker_id": "2",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100023903.1.2",
          "form": "요새 1차는 거진 다 맞으셨더라구요",
          "original_form": "요새 1차는 거진 다 맞으셨더라구요",
          "speaker_id": "2",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100023903.1.3",
          "form": "얀센이면 한번에 끝나셨겠네요?",
          "original_form": "얀센이면 한번에 끝나셨겠네요?",
          "speaker_id": "1",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023903.1.4",
          "form": "부러워요",
          "original_form": "부러워요",
          "speaker_id": "1",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023903.1.5",
          "form": "전 담달에나 1차라 언제 2차까지 맞을런지",
          "original_form": "전 담달에나 1차라 언제 2차까지 맞을런지",
          "speaker_id": "1",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023903.1.6",
          "form": "한방컷!",
          "original_form": "한방컷!",
          "speaker_id": "2",
          "time": "20210830 20:53"
        },
        {
          "id": "MDRW2100023903.1.7",
          "form": "담달초에 맞으시는구나",
          "original_form": "담달초에 맞으시는구나",
          "speaker_id": "2",
          "time": "20210830 20:53"
        },
        {
          "id": "MDRW2100023903.1.8",
          "form": "2차는 2~3주뒤에 바로 맞지않나요??",
          "original_form": "2차는 2~3주뒤에 바로 맞지않나요??",
          "speaker_id": "2",
          "time": "20210830 20:53"
        },
        {
          "id": "MDRW2100023903.1.9",
          "form": "일단 6주 뒤라고 써 잇는데 바뀔  수도 있겠죠",
          "original_form": "일단 6주 뒤라고 써 잇는데 바뀔  수도 있겠죠",
          "speaker_id": "1",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100023903.1.10",
          "form": "와; 6주요?...",
          "original_form": "와; 6주요?...",
          "speaker_id": "2",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100023903.1.11",
          "form": "수급 어렵다 어렵다 그러던데",
          "original_form": "수급 어렵다 어렵다 그러던데",
          "speaker_id": "2",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100023903.1.12",
          "form": "너무 걸리네요",
          "original_form": "너무 걸리네요",
          "speaker_id": "2",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100023903.1.13",
          "form": "현재는 그렇더라구요",
          "original_form": "현재는 그렇더라구요",
          "speaker_id": "1",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100023903.1.14",
          "form": "그때가서 4주 될수도 있고",
          "original_form": "그때가서 4주 될수도 있고",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100023903.1.15",
          "form": "알수가 없네요 ㅎ",
          "original_form": "알수가 없네요 ㅎ",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100023903.1.16",
          "form": "참...",
          "original_form": "참...",
          "speaker_id": "2",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100023903.1.17",
          "form": "3주때 맞아야 가장 좋다더니",
          "original_form": "3주때 맞아야 가장 좋다더니",
          "speaker_id": "2",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100023903.1.18",
          "form": "얀센은 부스터샷 생각중인가봐요 조만간 백신 또 맞으실수도 있어요",
          "original_form": "얀센은 부스터샷 생각중인가봐요 조만간 백신 또 맞으실수도 있어요",
          "speaker_id": "1",
          "time": "20210831 06:32"
        },
        {
          "id": "MDRW2100023903.1.19",
          "form": "나쁘지 않네요 유효기간도 끝나가는데ㅎㅎ",
          "original_form": "나쁘지 않네요 유효기간도 끝나가는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 09:17"
        },
        {
          "id": "MDRW2100023903.1.20",
          "form": "네 어제 뉴스에 살짝 나오더라구요",
          "original_form": "네 어제 뉴스에 살짝 나오더라구요",
          "speaker_id": "1",
          "time": "20210831 09:19"
        },
        {
          "id": "MDRW2100023903.1.21",
          "form": "확정은 아직 아니구요",
          "original_form": "확정은 아직 아니구요",
          "speaker_id": "1",
          "time": "20210831 09:19"
        },
        {
          "id": "MDRW2100023903.1.22",
          "form": "안 맞은 사람이 많아서 좀 걸리겠죠",
          "original_form": "안 맞은 사람이 많아서 좀 걸리겠죠",
          "speaker_id": "2",
          "time": "20210831 09:29"
        },
        {
          "id": "MDRW2100023903.1.23",
          "form": "네.. 저처럼 아직 1차도 안맞은 사람들이 아직도 있을테니까요..ㅎㅎ",
          "original_form": "네.. 저처럼 아직 1차도 안맞은 사람들이 아직도 있을테니까요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 09:32"
        },
        {
          "id": "MDRW2100023903.1.24",
          "form": "10월말이면 많이들 2차까지 맞을거 같아요",
          "original_form": "10월말이면 많이들 2차까지 맞을거 같아요",
          "speaker_id": "1",
          "time": "20210831 09:32"
        },
        {
          "id": "MDRW2100023903.1.25",
          "form": "9월에 집단 면역 얘기했었었는데ㅎㅎ 한참 멀은듯요",
          "original_form": "9월에 집단 면역 얘기했었었는데ㅎㅎ 한참 멀은듯요",
          "speaker_id": "2",
          "time": "20210831 09:36"
        },
        {
          "id": "MDRW2100023903.1.26",
          "form": "집단면역은 11월에나 가능하지 않을까요? 그것도 빨라야",
          "original_form": "집단면역은 11월에나 가능하지 않을까요? 그것도 빨라야",
          "speaker_id": "1",
          "time": "20210831 09:39"
        }
      ]
    }
