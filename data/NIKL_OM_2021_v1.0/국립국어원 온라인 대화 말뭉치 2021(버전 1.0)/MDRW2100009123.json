      "utterance": [
        {
          "id": "MDRW2100009123.1.1",
          "form": "가족분들이랑 같이 사세요?",
          "original_form": "가족분들이랑 같이 사세요?",
          "speaker_id": "1",
          "time": "20210822 21:29"
        },
        {
          "id": "MDRW2100009123.1.2",
          "form": "네 저는 엄마랑 같이 살고 있어요",
          "original_form": "네 저는 엄마랑 같이 살고 있어요",
          "speaker_id": "2",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009123.1.3",
          "form": "그럼 집안일은 부모님이 다해주시나요?",
          "original_form": "그럼 집안일은 부모님이 다해주시나요?",
          "speaker_id": "1",
          "time": "20210822 21:30"
        },
        {
          "id": "MDRW2100009123.1.4",
          "form": "뭔가 그렇게되요",
          "original_form": "뭔가 그렇게되요",
          "speaker_id": "2",
          "time": "20210822 21:33"
        },
        {
          "id": "MDRW2100009123.1.5",
          "form": "제가 하는걸 맘에 안들어하신달까ㅜㅜ",
          "original_form": "제가 하는걸 맘에 안들어하신달까ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 21:33"
        },
        {
          "id": "MDRW2100009123.1.6",
          "form": "안시키는게 더 좋죠 뭐 ㅋ 편하고",
          "original_form": "안시키는게 더 좋죠 뭐 ㅋ 편하고",
          "speaker_id": "1",
          "time": "20210822 21:34"
        },
        {
          "id": "MDRW2100009123.1.7",
          "form": "전 혼자있으니 주말내내 집안일이 산더미네요",
          "original_form": "전 혼자있으니 주말내내 집안일이 산더미네요",
          "speaker_id": "1",
          "time": "20210822 21:35"
        },
        {
          "id": "MDRW2100009123.1.8",
          "form": "근데 계속 투덜투덜대신달까",
          "original_form": "근데 계속 투덜투덜대신달까",
          "speaker_id": "2",
          "time": "20210822 21:37"
        },
        {
          "id": "MDRW2100009123.1.9",
          "form": "차라리 제 맘대로 하고싶기도해요 ㅋㅋ",
          "original_form": "차라리 제 맘대로 하고싶기도해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:37"
        },
        {
          "id": "MDRW2100009123.1.10",
          "form": "방청소만 하심 되죠ㅎ",
          "original_form": "방청소만 하심 되죠ㅎ",
          "speaker_id": "1",
          "time": "20210822 21:38"
        },
        {
          "id": "MDRW2100009123.1.11",
          "form": "전 청소 빨래 분리수거도 해야되고",
          "original_form": "전 청소 빨래 분리수거도 해야되고",
          "speaker_id": "1",
          "time": "20210822 21:39"
        },
        {
          "id": "MDRW2100009123.1.12",
          "form": "그걸 들어주는게 뭐라고 ㅋㅋ 사실 배부른 소리같기도해요",
          "original_form": "그걸 들어주는게 뭐라고 ㅋㅋ 사실 배부른 소리같기도해요",
          "speaker_id": "2",
          "time": "20210822 21:44"
        },
        {
          "id": "MDRW2100009123.1.13",
          "form": "화장실청소가 진짜 짜증나는거같아요",
          "original_form": "화장실청소가 진짜 짜증나는거같아요",
          "speaker_id": "2",
          "time": "20210822 21:45"
        },
        {
          "id": "MDRW2100009123.1.14",
          "form": "맞아요  젤싫은게 화장실청소ㅠㅠ",
          "original_form": "맞아요  젤싫은게 화장실청소ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 21:47"
        },
        {
          "id": "MDRW2100009123.1.15",
          "form": "진심 곰팡이는 어떻게 하면 되나요",
          "original_form": "진심 곰팡이는 어떻게 하면 되나요",
          "speaker_id": "2",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100009123.1.16",
          "form": "답이없다는ㅋㅋ",
          "original_form": "답이없다는ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:49"
        },
        {
          "id": "MDRW2100009123.1.17",
          "form": "홈스타 곰팡이 젤팔아요ㅎ",
          "original_form": "홈스타 곰팡이 젤팔아요ㅎ",
          "speaker_id": "1",
          "time": "20210822 21:50"
        },
        {
          "id": "MDRW2100009123.1.18",
          "form": "그거 뿌려놓음 사라지긴 하던데요",
          "original_form": "그거 뿌려놓음 사라지긴 하던데요",
          "speaker_id": "1",
          "time": "20210822 21:50"
        },
        {
          "id": "MDRW2100009123.1.19",
          "form": "뿌리고 안문질러도 되나요?",
          "original_form": "뿌리고 안문질러도 되나요?",
          "speaker_id": "2",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009123.1.20",
          "form": "네 면적이 넓어요?",
          "original_form": "네 면적이 넓어요?",
          "speaker_id": "1",
          "time": "20210822 21:52"
        },
        {
          "id": "MDRW2100009123.1.21",
          "form": "아뇨 딱히 그렇지도않은거같은데",
          "original_form": "아뇨 딱히 그렇지도않은거같은데",
          "speaker_id": "2",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009123.1.22",
          "form": "홈스타 곰팡이젤 적어놨어요",
          "original_form": "홈스타 곰팡이젤 적어놨어요",
          "speaker_id": "2",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100009123.1.23",
          "form": "시도해봐야겠어요 ㅋㅋㅋ",
          "original_form": "시도해봐야겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:54"
        }
      ]
    }
