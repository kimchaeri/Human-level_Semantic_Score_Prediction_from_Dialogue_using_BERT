      "utterance": [
        {
          "id": "MDRW2100014598.1.1",
          "form": "콘서트 가보신 적 있으세요?",
          "original_form": "콘서트 가보신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014598.1.2",
          "form": "몇 번 가봤어요",
          "original_form": "몇 번 가봤어요",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014598.1.3",
          "form": "오 어디 가셨어요?",
          "original_form": "오 어디 가셨어요?",
          "speaker_id": "1",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014598.1.4",
          "form": "코로나 때문에 한참 전이지만 언더 힙합 공연",
          "original_form": "코로나 때문에 한참 전이지만 언더 힙합 공연",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014598.1.5",
          "form": "전 한번도 가본 적 없거든요 ㅠ",
          "original_form": "전 한번도 가본 적 없거든요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014598.1.6",
          "form": "언더힙합!",
          "original_form": "언더힙합!",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014598.1.7",
          "form": "재밌었어요ㅋㅋㅋ 뮤지컬 같은건요?",
          "original_form": "재밌었어요ㅋㅋㅋ 뮤지컬 같은건요?",
          "speaker_id": "2",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014598.1.8",
          "form": "좋아하는데 기회가 없었어요. \r\n여긴 너무 지방이라 ㅠ",
          "original_form": "좋아하는데 기회가 없었어요. \r\n여긴 너무 지방이라 ㅠ",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014598.1.9",
          "form": "아 그렇군요. 극장은 좀 가시나요",
          "original_form": "아 그렇군요. 극장은 좀 가시나요",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014598.1.10",
          "form": "극장도 전혀..ㅋㅋ\r\n이쪽에 관심은 있는데 찾아갈 정도는 아닌 것 같아요.",
          "original_form": "극장도 전혀..ㅋㅋ\r\n이쪽에 관심은 있는데 찾아갈 정도는 아닌 것 같아요.",
          "speaker_id": "1",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014598.1.11",
          "form": "최근에 보신 영화 있으세요?",
          "original_form": "최근에 보신 영화 있으세요?",
          "speaker_id": "1",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014598.1.12",
          "form": "전 코로나 때문에 극장은 못가고 넷플릭스로 고전 보고 있습니다",
          "original_form": "전 코로나 때문에 극장은 못가고 넷플릭스로 고전 보고 있습니다",
          "speaker_id": "2",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014598.1.13",
          "form": "고스트 버스터즈나 백투더퓨쳐요 ㅋㅋㅋ",
          "original_form": "고스트 버스터즈나 백투더퓨쳐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014598.1.14",
          "form": "오 고전 좋아하시는군요!",
          "original_form": "오 고전 좋아하시는군요!",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014598.1.15",
          "form": "저도 넷플릭스로 봐요 ㅎㅎ",
          "original_form": "저도 넷플릭스로 봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014598.1.16",
          "form": "뭐 보샸어요 ㅋㅋ",
          "original_form": "뭐 보샸어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014598.1.17",
          "form": "영화는 안 보고 드라마 위주로ㅋㅋㅋ",
          "original_form": "영화는 안 보고 드라마 위주로ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014598.1.18",
          "form": "아 그거 봤어요 최근에 장기용이랑 크리스탈 나온거",
          "original_form": "아 그거 봤어요 최근에 장기용이랑 크리스탈 나온거",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014598.1.19",
          "form": "전 드라마 잘 몰라서...ㅋㅋ 재밌던가요",
          "original_form": "전 드라마 잘 몰라서...ㅋㅋ 재밌던가요",
          "speaker_id": "2",
          "time": "20210825 20:57"
        },
        {
          "id": "MDRW2100014598.1.20",
          "form": "이건 영화에요!!ㅎㅎ",
          "original_form": "이건 영화에요!!ㅎㅎ",
