      "utterance": [
        {
          "id": "MDRW2100032321.1.1",
          "form": "왓챠보셔요?",
          "original_form": "왓챠보셔요?",
          "speaker_id": "1",
          "time": "20210905 15:27"
        },
        {
          "id": "MDRW2100032321.1.2",
          "form": "왓챠랑 넷플릭스 라프텔 보다가 지금은 안봐요",
          "original_form": "왓챠랑 넷플릭스 라프텔 보다가 지금은 안봐요",
          "speaker_id": "2",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100032321.1.3",
          "form": "영화관은 자주 갑니당",
          "original_form": "영화관은 자주 갑니당",
          "speaker_id": "2",
          "time": "20210905 15:28"
        },
        {
          "id": "MDRW2100032321.1.4",
          "form": "저도 저번달까지 왓챠봤어요",
          "original_form": "저도 저번달까지 왓챠봤어요",
          "speaker_id": "1",
          "time": "20210905 15:30"
        },
        {
          "id": "MDRW2100032321.1.5",
          "form": "라프텔이 보이니 애니 보시나봐요? 방가워요",
          "original_form": "라프텔이 보이니 애니 보시나봐요? 방가워요",
          "speaker_id": "1",
          "time": "20210905 15:30"
        },
        {
          "id": "MDRW2100032321.1.6",
          "form": "네넹, 애니 좋아해요ㅎㅎ 요즘은 잘 안보지만",
          "original_form": "네넹, 애니 좋아해요ㅎㅎ 요즘은 잘 안보지만",
          "speaker_id": "2",
          "time": "20210905 15:30"
        },
        {
          "id": "MDRW2100032321.1.7",
          "form": "좋아하는 애니 있으세요??",
          "original_form": "좋아하는 애니 있으세요??",
          "speaker_id": "2",
          "time": "20210905 15:30"
        },
        {
          "id": "MDRW2100032321.1.8",
          "form": "본 건 정말 많아요. 근데 다 보면 왜 기억이 안나는지 ㅎㅎ",
          "original_form": "본 건 정말 많아요. 근데 다 보면 왜 기억이 안나는지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 15:32"
        },
        {
          "id": "MDRW2100032321.1.9",
          "form": "최근에 본건 강연!! 아시나요?",
          "original_form": "최근에 본건 강연!! 아시나요?",
          "speaker_id": "1",
          "time": "20210905 15:32"
        },
        {
          "id": "MDRW2100032321.1.10",
          "form": "내머릿속의지우개ㅋㅋㅋ",
          "original_form": "내머릿속의지우개ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 15:36"
        },
        {
          "id": "MDRW2100032321.1.11",
          "form": "강철의 연금술사요??",
          "original_form": "강철의 연금술사요??",
          "speaker_id": "2",
          "time": "20210905 15:36"
        },
        {
          "id": "MDRW2100032321.1.12",
          "form": "네!! 유명해서 도전해봤어요",
          "original_form": "네!! 유명해서 도전해봤어요",
          "speaker_id": "1",
          "time": "20210905 15:37"
        },
        {
          "id": "MDRW2100032321.1.13",
          "form": "아직 반절정도 봤어요",
          "original_form": "아직 반절정도 봤어요",
          "speaker_id": "1",
          "time": "20210905 15:37"
        },
        {
          "id": "MDRW2100032321.1.14",
          "form": "저 중학생 때 한창 인기였죠ㅋㅋㅋ 저는 국내방영분만 좀 봤어요",
          "original_form": "저 중학생 때 한창 인기였죠ㅋㅋㅋ 저는 국내방영분만 좀 봤어요",
          "speaker_id": "2",
          "time": "20210905 15:41"
        },
        {
          "id": "MDRW2100032321.1.15",
          "form": "그럼 오리지널 보셨겠네요!",
          "original_form": "그럼 오리지널 보셨겠네요!",
          "speaker_id": "1",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032321.1.16",
          "form": "강연 애니가 오리지널이랑 보이후드로 나눠더라고요",
          "original_form": "강연 애니가 오리지널이랑 보이후드로 나눠더라고요",
          "speaker_id": "1",
          "time": "20210905 15:43"
        },
        {
          "id": "MDRW2100032321.1.17",
          "form": "네넹ㅋㅋㅋ 대강의 스토리는 알아요",
          "original_form": "네넹ㅋㅋㅋ 대강의 스토리는 알아요",
          "speaker_id": "2",
          "time": "20210905 15:43"
        },
        {
          "id": "MDRW2100032321.1.18",
          "form": "재밌었나요?",
          "original_form": "재밌었나요?",
          "speaker_id": "1",
          "time": "20210905 15:43"
        },
        {
          "id": "MDRW2100032321.1.19",
          "form": "네네, 리멬을 안 봐서 차이는 못 설명하지만",
          "original_form": "네네, 리멬을 안 봐서 차이는 못 설명하지만",
          "speaker_id": "2",
          "time": "20210905 15:44"
        },
        {
          "id": "MDRW2100032321.1.20",
          "form": "일단 작품 자체가 세계관이 독특하달까 참신했죠",
          "original_form": "일단 작품 자체가 세계관이 독특하달까 참신했죠",
          "speaker_id": "2",
          "time": "20210905 15:44"
        },
        {
          "id": "MDRW2100032321.1.21",
          "form": "맞아요!! 그래서 꽤 매력있는듯요",
          "original_form": "맞아요!! 그래서 꽤 매력있는듯요",
          "speaker_id": "1",
          "time": "20210905 15:45"
        },
        {
          "id": "MDRW2100032321.1.22",
          "form": "근데 사실 전 그렇게까지 재밌는 줄은 모르겠더라고요 ㅎㅎ 반절넘게 봐도",
          "original_form": "근데 사실 전 그렇게까지 재밌는 줄은 모르겠더라고요 ㅎㅎ 반절넘게 봐도",
          "speaker_id": "1",
          "time": "20210905 15:46"
        },
        {
          "id": "MDRW2100032321.1.23",
          "form": "오, 그러셨구나",
          "original_form": "오, 그러셨구나",
          "speaker_id": "2",
          "time": "20210905 15:46"
        },
        {
          "id": "MDRW2100032321.1.24",
          "form": "그럼 좋아하시는 작품은 어떤 거예요??",
          "original_form": "그럼 좋아하시는 작품은 어떤 거예요??",
          "speaker_id": "2",
          "time": "20210905 15:46"
        },
        {
          "id": "MDRW2100032321.1.25",
          "form": "그래도 마지막까지 볼거예요!!",
          "original_form": "그래도 마지막까지 볼거예요!!",
          "speaker_id": "1",
          "time": "20210905 15:46"
        },
        {
          "id": "MDRW2100032321.1.26",
          "form": "하이큐 재밌게 봤어요",
          "original_form": "하이큐 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210905 15:46"
        },
        {
          "id": "MDRW2100032321.1.27",
          "form": "오, 하이큐 건전하고 좋더라구요",
          "original_form": "오, 하이큐 건전하고 좋더라구요",
          "speaker_id": "2",
          "time": "20210905 15:47"
        },
        {
          "id": "MDRW2100032321.1.28",
          "form": "4기1쿨까지 일주일안에 다 봤자나여",
          "original_form": "4기1쿨까지 일주일안에 다 봤자나여",
          "speaker_id": "2",
          "time": "20210905 15:47"
        },
        {
          "id": "MDRW2100032321.1.29",
          "form": "정말 4기 몇년만에 나왔죠",
          "original_form": "정말 4기 몇년만에 나왔죠",
          "speaker_id": "1",
          "time": "20210905 15:47"
        },
        {
          "id": "MDRW2100032321.1.30",
          "form": "저도 4기 봤는데",
          "original_form": "저도 4기 봤는데",
          "speaker_id": "1",
          "time": "20210905 15:48"
        },
        {
          "id": "MDRW2100032321.1.31",
          "form": "저는 페이트랑 문스독, 하루히 요런것들 좋아했어요",
          "original_form": "저는 페이트랑 문스독, 하루히 요런것들 좋아했어요",
          "speaker_id": "2",
          "time": "20210905 15:54"
        },
        {
          "id": "MDRW2100032321.1.32",
          "form": "문스독 저도 봤는데!! 만화도 봤어요",
          "original_form": "문스독 저도 봤는데!! 만화도 봤어요",
          "speaker_id": "1",
          "time": "20210905 15:55"
        },
        {
          "id": "MDRW2100032321.1.33",
          "form": "오랜만에 듣네요 다시 보고 싶어져요 2기는 안봤거든요",
          "original_form": "오랜만에 듣네요 다시 보고 싶어져요 2기는 안봤거든요",
          "speaker_id": "1",
          "time": "20210905 15:55"
        }
      ]
