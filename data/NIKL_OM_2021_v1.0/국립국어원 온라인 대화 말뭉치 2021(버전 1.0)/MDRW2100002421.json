        },
        {
          "id": "MDRW2100002421.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210820 12:11"
        },
        {
          "id": "MDRW2100002421.1.3",
          "form": "요즘 최근구매 하신 물건이 있으신가요~?",
          "original_form": "요즘 최근구매 하신 물건이 있으신가요~?",
          "speaker_id": "2",
          "time": "20210820 12:11"
        },
        {
          "id": "MDRW2100002421.1.4",
          "form": "최근에 쇼핑하신 물던니 헉",
          "original_form": "최근에 쇼핑하신 물던니 헉",
          "speaker_id": "1",
          "time": "20210820 12:11"
        },
        {
          "id": "MDRW2100002421.1.5",
          "form": "통했다",
          "original_form": "통했다",
          "speaker_id": "1",
          "time": "20210820 12:11"
        },
        {
          "id": "MDRW2100002421.1.6",
          "form": "저는 먹을 거 주로 사요",
          "original_form": "저는 먹을 거 주로 사요",
          "speaker_id": "1",
          "time": "20210820 12:11"
        },
        {
          "id": "MDRW2100002421.1.7",
          "form": "저도 먹을거 위주로사요",
          "original_form": "저도 먹을거 위주로사요",
          "speaker_id": "2",
          "time": "20210820 12:12"
        },
        {
          "id": "MDRW2100002421.1.8",
          "form": "어떤 음식을 구매하셨나요",
          "original_form": "어떤 음식을 구매하셨나요",
          "speaker_id": "1",
          "time": "20210820 12:12"
        },
        {
          "id": "MDRW2100002421.1.9",
          "form": "최근엔 오뎅사서 김치찌개 해먹었어요",
          "original_form": "최근엔 오뎅사서 김치찌개 해먹었어요",
          "speaker_id": "2",
          "time": "20210820 12:13"
        },
        {
          "id": "MDRW2100002421.1.10",
          "form": "맛이었겠네요",
          "original_form": "맛이었겠네요",
          "speaker_id": "1",
          "time": "20210820 12:13"
        },
        {
          "id": "MDRW2100002421.1.11",
          "form": "어떤거 사셨나요 추천해주세요",
          "original_form": "어떤거 사셨나요 추천해주세요",
          "speaker_id": "2",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100002421.1.12",
          "form": "저는 어제 돼지고기를 사서 김치찜을 해먹었어요",
          "original_form": "저는 어제 돼지고기를 사서 김치찜을 해먹었어요",
          "speaker_id": "1",
          "time": "20210820 12:14"
        },
        {
          "id": "MDRW2100002421.1.13",
          "form": "저도 추천해주시겠어요?",
          "original_form": "저도 추천해주시겠어요?",
          "speaker_id": "1",
          "time": "20210820 12:15"
        },
        {
          "id": "MDRW2100002421.1.14",
          "form": "빙고에서 튀김류 사서 드셔보세요",
          "original_form": "빙고에서 튀김류 사서 드셔보세요",
          "speaker_id": "2",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100002421.1.15",
          "form": "너무 맛있어요",
          "original_form": "너무 맛있어요",
          "speaker_id": "2",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100002421.1.16",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "1",
          "time": "20210820 12:16"
        },
        {
          "id": "MDRW2100002421.1.17",
          "form": "튀김 좋아해요",
          "original_form": "튀김 좋아해요",
          "speaker_id": "1",
          "time": "20210820 12:17"
        },
        {
          "id": "MDRW2100002421.1.18",
          "form": "튀김류 좋아하시나요",
          "original_form": "튀김류 좋아하시나요",
          "speaker_id": "1",
          "time": "20210820 12:17"
        },
        {
          "id": "MDRW2100002421.1.19",
          "form": "네 좋아하는데 요즘 튀김이 비싸서 완제품 사먹어요",
          "original_form": "네 좋아하는데 요즘 튀김이 비싸서 완제품 사먹어요",
          "speaker_id": "2",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002421.1.20",
          "form": "그죠 넘 비싸더라구요",
          "original_form": "그죠 넘 비싸더라구요",
          "speaker_id": "1",
          "time": "20210820 12:18"
        },
        {
          "id": "MDRW2100002421.1.21",
          "form": "구매예정이신 물건이 있으신가요~?",
          "original_form": "구매예정이신 물건이 있으신가요~?",
          "speaker_id": "2",
          "time": "20210820 12:19"
        },
        {
          "id": "MDRW2100002421.1.22",
          "form": "마켓컬리 쿠폰이 오늘까지라 또 먹을 거 쇼핑할 거 같네요 ㅎㅎ",
          "original_form": "마켓컬리 쿠폰이 오늘까지라 또 먹을 거 쇼핑할 거 같네요 ㅎㅎ",
