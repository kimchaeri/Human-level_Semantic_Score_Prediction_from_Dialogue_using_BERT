      "utterance": [
        {
          "id": "MDRW2100023325.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210830 00:40"
        },
        {
          "id": "MDRW2100023325.1.2",
          "form": "네 키우고있어욥",
          "original_form": "네 키우고있어욥",
          "speaker_id": "1",
          "time": "20210830 00:52"
        },
        {
          "id": "MDRW2100023325.1.3",
          "form": "우와 모키우세여??",
          "original_form": "우와 모키우세여??",
          "speaker_id": "2",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100023325.1.4",
          "form": "치와와 키우고있습니다",
          "original_form": "치와와 키우고있습니다",
          "speaker_id": "1",
          "time": "20210830 18:51"
        },
        {
          "id": "MDRW2100023325.1.5",
          "form": "치와와 귀여운데 ㅜㅜ",
          "original_form": "치와와 귀여운데 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100023325.1.6",
          "form": "맞아요ㅠ엄청나죠",
          "original_form": "맞아요ㅠ엄청나죠",
          "speaker_id": "1",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100023325.1.7",
          "form": "사실 모든 댕댕이가 귀여워요 안귀여운 동물친구들은 없어요 ㅋㅋ 키울수 없어 못키우지만 보면 너무 귀여워요 진짜 힐링",
          "original_form": "사실 모든 댕댕이가 귀여워요{emoji:♥} 안귀여운 동물친구들은 없어요 ㅋㅋ 키울수 없어 못키우지만 보면 너무 귀여워요 진짜 힐링",
          "speaker_id": "2",
          "time": "20210830 21:36"
        },
        {
          "id": "MDRW2100023325.1.8",
          "form": "그렇죠ㅠ 저는 댕댕이좋아하지만 수달을 너무좋아해서",
          "original_form": "그렇죠ㅠ 저는 댕댕이좋아하지만 수달을 너무좋아해서",
          "speaker_id": "1",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100023325.1.9",
          "form": "가끔 영상보고 힐링합니다",
          "original_form": "가끔 영상보고 힐링합니다",
          "speaker_id": "1",
          "time": "20210830 21:45"
        },
        {
          "id": "MDRW2100023325.1.10",
          "form": "수달 받고 해달요 ㅠㅠㅠ 저 해달도 보고 힐링해요 너무 귀여움 ㅠㅠㅠ",
          "original_form": "수달 받고 해달요 ㅠㅠㅠ 저 해달도 보고 힐링해요 너무 귀여움 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100023325.1.11",
          "form": "크으 진짜사랑스럽죠",
          "original_form": "크으 진짜사랑스럽죠",
          "speaker_id": "1",
          "time": "20210830 21:47"
        },
        {
          "id": "MDRW2100023325.1.12",
          "form": "너무 귀여워요 진짜 손잡고 자는거 ㅠㅠ 조개 깨는거하며 ㅋㅋㅋ",
          "original_form": "너무 귀여워요 진짜 손잡고 자는거 ㅠㅠ 조개 깨는거하며 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100023325.1.13",
          "form": "아ㅠ 상상해도 귀엽네요",
          "original_form": "아ㅠ 상상해도 귀엽네요",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100023325.1.14",
          "form": "그쵸? 너무 귀여워요 ㅠㅠ 저 그 카피바라 그아이도 너무 귀엽던데",
          "original_form": "그쵸? 너무 귀여워요 ㅠㅠ 저 그 카피바라 그아이도 너무 귀엽던데",
          "speaker_id": "2",
          "time": "20210830 21:52"
        },
        {
          "id": "MDRW2100023325.1.15",
          "form": "저도 딱한번 본적있는데 귀엽더라구요",
          "original_form": "저도 딱한번 본적있는데 귀엽더라구요",
          "speaker_id": "1",
          "time": "20210830 21:53"
        },
        {
          "id": "MDRW2100023325.1.16",
          "form": "실제로요????",
          "original_form": "실제로요????",
          "speaker_id": "2",
          "time": "20210830 21:53"
        },
        {
          "id": "MDRW2100023325.1.17",
          "form": "네ㅋㅋㅋ 촬영차 갔다가",
          "original_form": "네ㅋㅋㅋ 촬영차 갔다가",
          "speaker_id": "1",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100023325.1.18",
          "form": "왕 부럽다 ㅜㅜ",
          "original_form": "왕 부럽다 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100023325.1.19",
          "form": "진짜 처음보는데도 다가오더라구요",
          "original_form": "진짜 처음보는데도 다가오더라구요",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100023325.1.20",
          "form": "간식주니깐",
          "original_form": "간식주니깐",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100023325.1.21",
          "form": "당근 좋아하던데 그아이들",
          "original_form": "당근 좋아하던데 그아이들",
          "speaker_id": "2",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100023325.1.22",
          "form": "만져주면 좋아서 털이 스던데 ㅜㅜ",
          "original_form": "만져주면 좋아서 털이 스던데 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100023325.1.23",
          "form": "맞아요 바짝서더라구요",
          "original_form": "맞아요 바짝서더라구요",
          "speaker_id": "1",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100023325.1.24",
          "form": "그래서 처음에 화내나했어요",
          "original_form": "그래서 처음에 화내나했어요",
          "speaker_id": "1",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100023325.1.25",
          "form": "동물 친구들 많이 만나 부러워용 ㅋㅋㅋ",
          "original_form": "동물 친구들 많이 만나 부러워용 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100023325.1.26",
          "form": "시기가시기인지라 동물원도못가고ㅠ",
          "original_form": "시기가시기인지라 동물원도못가고ㅠ",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100023325.1.27",
          "form": "동물원도 가세요? 어릴적은 동물원 좋았는데 불쌍해보여서 전 못가겠어용 ㅠㅠ",
          "original_form": "동물원도 가세요? 어릴적은 동물원 좋았는데 불쌍해보여서 전 못가겠어용 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100023325.1.28",
          "form": "가끔갔죠 머리식힐겸?",
          "original_form": "가끔갔죠 머리식힐겸?",
          "speaker_id": "1",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100023325.1.29",
          "form": "지금은 못가지만",
          "original_form": "지금은 못가지만",
          "speaker_id": "1",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100023325.1.30",
          "form": "불쌍하죠 그래도 거기서라도 살수있어서 다행이라 생각해요",
          "original_form": "불쌍하죠 그래도 거기서라도 살수있어서 다행이라 생각해요",
          "speaker_id": "1",
          "time": "20210830 22:09"
        },
        {
          "id": "MDRW2100023325.1.31",
          "form": "엇 역발상 그렇네요 그렇게보면 구래도 먹을건 풍족하니",
          "original_form": "엇 역발상 그렇네요 그렇게보면 구래도 먹을건 풍족하니",
          "speaker_id": "2",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100023325.1.32",
          "form": "전 그저 불쌍해라고만 생각했네요",
          "original_form": "전 그저 불쌍해라고만 생각했네요",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100023325.1.33",
          "form": "근데 저도 수족관 좋아해요 해파리 보는거라던가",
          "original_form": "근데 저도 수족관 좋아해요 해파리 보는거라던가",
          "speaker_id": "2",
          "time": "20210830 22:13"
        },
        {
          "id": "MDRW2100023325.1.34",
          "form": "오? 저랑 비슷하시네요",
          "original_form": "오? 저랑 비슷하시네요",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100023325.1.35",
          "form": "저도 수조관가서 수달보는거 좋아합니다.",
          "original_form": "저도 수조관가서 수달보는거 좋아합니다.",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100023325.1.36",
          "form": "수달 귀엽죠 힐링될듯  롯데타워 수족관 좋다던데 가보셨나요??",
          "original_form": "수달 귀엽죠 힐링될듯 {emoji:♥} 롯데타워 수족관 좋다던데 가보셨나요??",
          "speaker_id": "2",
          "time": "20210830 22:17"
        },
        {
          "id": "MDRW2100023325.1.37",
          "form": "아뇨 아직이요...가보고싶은데 바빠서요",
          "original_form": "아뇨 아직이요...가보고싶은데 바빠서요",
          "speaker_id": "1",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100023325.1.38",
          "form": "많이 바쁘신가봐여 ㅠㅠ",
          "original_form": "많이 바쁘신가봐여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100023325.1.39",
          "form": "저도 못가봤어요 롯데타워 수족관은",
          "original_form": "저도 못가봤어요 롯데타워 수족관은",
          "speaker_id": "2",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100023325.1.40",
          "form": "오 시간내서 한번가보세요!",
          "original_form": "오 시간내서 한번가보세요!",
          "speaker_id": "1",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100023325.1.41",
          "form": "저도 시간나면 가보도록 하겠습니다~ 지금도 일중이세요?",
          "original_form": "저도 시간나면 가보도록 하겠습니다~ 지금도 일중이세요?",
          "speaker_id": "2",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100023325.1.42",
          "form": "지금은 퇴근중입니다",
          "original_form": "지금은 퇴근중입니다",
          "speaker_id": "1",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100023325.1.43",
          "form": "늦네요 진짜 오늘 하루 수고하셨어요!!!!",
          "original_form": "늦네요 진짜 오늘 하루 수고하셨어요!!!!",
          "speaker_id": "2",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100023325.1.44",
          "form": "감사합니다!",
          "original_form": "감사합니다!",
          "speaker_id": "1",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100023325.1.45",
          "form": "보통 몇시에주무세요?",
          "original_form": "보통 몇시에주무세요?",
          "speaker_id": "1",
          "time": "20210830 22:32"
        },
        {
          "id": "MDRW2100023325.1.46",
          "form": "저는 12시쯤에서 1시요~~~ 이제 퇴근하시니 늦은 식사 드시기엔 야식수준이네요 ㅠ",
          "original_form": "저는 12시쯤에서 1시요~~~ 이제 퇴근하시니 늦은 식사 드시기엔 야식수준이네요 ㅠ",
          "speaker_id": "2",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100023325.1.47",
          "form": "그렇죠ㅠ 가끔 먹어요ㅋㅋㅋ",
          "original_form": "그렇죠ㅠ 가끔 먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:41"
        },
        {
          "id": "MDRW2100023325.1.48",
          "form": "드셔야해여 혼자 찔수없어요 ㅋㅋㅋ",
          "original_form": "드셔야해여 혼자 찔수없어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100023325.1.49",
          "form": "앗ㅋㅋㅋ 이미좀쪄서...",
          "original_form": "앗ㅋㅋㅋ 이미좀쪄서...",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100023325.1.50",
          "form": "거짓말 말아요 확찐자 아직이시자나여 ㅋㅋㅋ",
          "original_form": "거짓말 말아요 확찐자 아직이시자나여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:48"
        },
        {
          "id": "MDRW2100023325.1.51",
          "form": "맞는듯해요ㅋㅋㅋ",
          "original_form": "맞는듯해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100023325.1.52",
