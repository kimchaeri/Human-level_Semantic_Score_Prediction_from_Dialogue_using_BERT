          "speaker_id": "1",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100038010.1.3",
          "form": "그게뭐요",
          "original_form": "그게뭐요",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100038010.1.4",
          "form": "반찬 이것저것",
          "original_form": "반찬 이것저것",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100038010.1.5",
          "form": "마트에서 3팩에.만원하는거 있자나",
          "original_form": "마트에서 3팩에.만원하는거 있자나",
          "speaker_id": "1",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100038010.1.6",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100038010.1.7",
          "form": "땅콩",
          "original_form": "땅콩",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100038010.1.8",
          "form": "멀치",
          "original_form": "멀치",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100038010.1.9",
          "form": "일미",
          "original_form": "일미",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100038010.1.10",
          "form": "메추리알조림",
          "original_form": "메추리알조림",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100038010.1.11",
          "form": "마싯겠다",
          "original_form": "마싯겠다",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100038010.1.12",
          "form": "고추장아찌",
          "original_form": "고추장아찌",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100038010.1.13",
          "form": "마트에 팔저라구",
          "original_form": "마트에 팔저라구",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100038010.1.14",
          "form": "맛난거만골라드신거아닙니카",
          "original_form": "맛난거만골라드신거아닙니카",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100038010.1.15",
          "form": "반찬은 몇번 먹으니까 심사숙고해서 골를수밖에",
          "original_form": "반찬은 몇번 먹으니까 심사숙고해서 골를수밖에",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100038010.1.16",
          "form": "역시 똑똑해",
          "original_form": "역시 똑똑해",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100038010.1.17",
          "form": "근데.혼밥은 재미가 없어",
          "original_form": "근데.혼밥은 재미가 없어",
          "speaker_id": "1",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100038010.1.18",
          "form": "재미로묵습니까?",
          "original_form": "재미로묵습니까?",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100038010.1.19",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100038010.1.20",
          "form": "맛나고 재미있게",
          "original_form": "맛나고 재미있게",
          "speaker_id": "1",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100038010.1.21",
          "form": "배채울라고 묵는기제",
          "original_form": "배채울라고 묵는기제",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100038010.1.22",
          "form": "제주는 날씨가 흐려서 뜨근한데 땡긴다",
          "original_form": "제주는 날씨가 흐려서 뜨근한데 땡긴다",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100038010.1.23",
          "form": "점심은 라면먹었어",
          "original_form": "점심은 라면먹었어",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
