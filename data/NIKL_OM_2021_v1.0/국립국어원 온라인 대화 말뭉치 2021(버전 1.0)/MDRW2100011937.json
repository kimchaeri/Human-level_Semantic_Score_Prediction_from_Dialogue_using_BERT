      "utterance": [
        {
          "id": "MDRW2100011937.1.1",
          "form": "빵이랑 음료랑 같이 먹으면 되겠어요ㅎㅎ",
          "original_form": "빵이랑 음료랑 같이 먹으면 되겠어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:44"
        },
        {
          "id": "MDRW2100011937.1.2",
          "form": "마라탕 좋아하세요?",
          "original_form": "마라탕 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 18:00"
        },
        {
          "id": "MDRW2100011937.1.3",
          "form": "간장새우앞에선 상관없더라구요",
          "original_form": "간장새우앞에선 상관없더라구요",
          "speaker_id": "2",
          "time": "20210824 18:05"
        },
        {
          "id": "MDRW2100011937.1.4",
          "form": "오늘 날씨 어땠나요",
          "original_form": "오늘 날씨 어땠나요",
          "speaker_id": "1",
          "time": "20210824 18:19"
        },
        {
          "id": "MDRW2100011937.1.5",
          "form": "종일 비오네요",
          "original_form": "종일 비오네요",
          "speaker_id": "2",
          "time": "20210824 18:19"
        },
        {
          "id": "MDRW2100011937.1.6",
          "form": "바람 소리도 들리고",
          "original_form": "바람 소리도 들리고",
          "speaker_id": "2",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011937.1.7",
          "form": "그쵸 여름 다 갔는데",
          "original_form": "그쵸 여름 다 갔는데",
          "speaker_id": "1",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011937.1.8",
          "form": "태풍오고",
          "original_form": "태풍오고",
          "speaker_id": "1",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011937.1.9",
          "form": "장마오고",
          "original_form": "장마오고",
          "speaker_id": "1",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011937.1.10",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011937.1.11",
          "form": "오마이스인가",
          "original_form": "오마이스인가",
          "speaker_id": "2",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011937.1.12",
          "form": "참 가지가지하는 거 같아요",
          "original_form": "참 가지가지하는 거 같아요",
          "speaker_id": "1",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011937.1.13",
          "form": "제말이요ㅎㅎ",
          "original_form": "제말이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011937.1.14",
          "form": "비오는 날 어때요",
          "original_form": "비오는 날 어때요",
          "speaker_id": "1",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011937.1.15",
          "form": "집에 있을땐 좋아요ㅎ",
          "original_form": "집에 있을땐 좋아요ㅎ",
          "speaker_id": "2",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011937.1.16",
          "form": "그건 공감합니다",
          "original_form": "그건 공감합니다",
          "speaker_id": "1",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011937.1.17",
          "form": "비오는날 좋아하세요?",
          "original_form": "비오는날 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011937.1.18",
          "form": "집에 있을 땐 날씨 영향을 안받으니까 상관없죠",
          "original_form": "집에 있을 땐 날씨 영향을 안받으니까 상관없죠",
          "speaker_id": "1",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100011937.1.19",
          "form": "저 비싫어해요",
          "original_form": "저 비싫어해요",
          "speaker_id": "1",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100011937.1.20",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "2",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100011937.1.21",
          "form": "그게 그거 있잖아요",
          "original_form": "그게 그거 있잖아요",
          "speaker_id": "1",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100011937.1.22",
          "form": "우산쓰고있는데도 다 젖는거",
          "original_form": "우산쓰고있는데도 다 젖는거",
          "speaker_id": "1",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100011937.1.23",
          "form": "분명 우산을 쓰고있는데 젖어",
          "original_form": "분명 우산을 쓰고있는데 젖어",
          "speaker_id": "1",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100011937.1.24",
          "form": "오우 그건 정말 별로에요",
          "original_form": "오우 그건 정말 별로에요",
          "speaker_id": "2",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100011937.1.25",
          "form": "비 막 가로로 내리고",
          "original_form": "비 막 가로로 내리고",
          "speaker_id": "2",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100011937.1.26",
          "form": "바람불고",
          "original_form": "바람불고",
          "speaker_id": "2",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100011937.1.27",
          "form": "우산의 의미가 없어지는..",
          "original_form": "우산의 의미가 없어지는..",
          "speaker_id": "1",
          "time": "20210824 18:21"
        }
      ]
    }
