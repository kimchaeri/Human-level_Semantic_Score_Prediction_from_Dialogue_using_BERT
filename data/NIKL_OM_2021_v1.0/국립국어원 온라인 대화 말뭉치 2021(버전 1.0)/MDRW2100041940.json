      "utterance": [
        {
          "id": "MDRW2100041940.1.1",
          "form": "뭐 키우고 싶으세요?",
          "original_form": "뭐 키우고 싶으세요?",
          "speaker_id": "2",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100041940.1.2",
          "form": "저는 보호소에 있는 강아지나 고양이",
          "original_form": "저는 보호소에 있는 강아지나 고양이",
          "speaker_id": "1",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100041940.1.3",
          "form": "데려오고싶어요",
          "original_form": "데려오고싶어요",
          "speaker_id": "1",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100041940.1.4",
          "form": "아아아~~~저도 그런생각은 했었어요",
          "original_form": "아아아~~~저도 그런생각은 했었어요",
          "speaker_id": "2",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100041940.1.5",
          "form": "요즘에 유기견 많이 나오더라구요. 예쁜 강아지, 고양이들이 많던데 ㅠ 안타까웟어요",
          "original_form": "요즘에 유기견 많이 나오더라구요. 예쁜 강아지, 고양이들이 많던데 ㅠ 안타까웟어요",
          "speaker_id": "2",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100041940.1.6",
          "form": "근데 아직 경제력이 없어서ㅠ",
          "original_form": "근데 아직 경제력이 없어서ㅠ",
          "speaker_id": "1",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041940.1.7",
          "form": "데려오면 애도 저도 고생할일밖에 없어서 못데려와요",
          "original_form": "데려오면 애도 저도 고생할일밖에 없어서 못데려와요",
          "speaker_id": "1",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041940.1.8",
          "form": "아 경제력 ㅠㅠ 중요하죠 ㅠㅠ 뭘 키울려면...돈이 많이 들어가는건 사실이니깐요 ㅠ",
          "original_form": "아 경제력 ㅠㅠ 중요하죠 ㅠㅠ 뭘 키울려면...돈이 많이 들어가는건 사실이니깐요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041940.1.9",
          "form": "그래서 저도 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ못키워요",
          "original_form": "그래서 저도 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ못키워요",
          "speaker_id": "2",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041940.1.10",
          "form": "구피친구들은",
          "original_form": "구피친구들은",
          "speaker_id": "1",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041940.1.11",
          "form": "사료값같은거",
          "original_form": "사료값같은거",
          "speaker_id": "1",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041940.1.12",
          "form": "그친구들도 꽤들지요?",
          "original_form": "그친구들도 꽤들지요?",
          "speaker_id": "1",
          "time": "20210906 18:00"
        },
        {
          "id": "MDRW2100041940.1.13",
          "form": "사료값은 괜찮아요 ㅋ",
          "original_form": "사료값은 괜찮아요 ㅋ",
          "speaker_id": "2",
          "time": "20210906 18:04"
        },
        {
          "id": "MDRW2100041940.1.14",
          "form": "근데 ㅋㅋㅋ 어항 물갈이할때는 좀 귀찮아요 ㅋㅋ",
          "original_form": "근데 ㅋㅋㅋ 어항 물갈이할때는 좀 귀찮아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:05"
        },
        {
          "id": "MDRW2100041940.1.15",
          "form": "헉 물갈이 얼마나 자주해야하나요?!",
          "original_form": "헉 물갈이 얼마나 자주해야하나요?!",
          "speaker_id": "1",
          "time": "20210906 18:06"
        },
        {
          "id": "MDRW2100041940.1.16",
          "form": "일주일~2주일에 한번씩이요",
          "original_form": "일주일~2주일에 한번씩이요",
          "speaker_id": "2",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100041940.1.17",
          "form": "헉 생각보다 자주하네요..",
          "original_form": "헉 생각보다 자주하네요..",
          "speaker_id": "1",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100041940.1.18",
          "form": "저는 한두달에 한번일줄,,",
          "original_form": "저는 한두달에 한번일줄,,",
          "speaker_id": "1",
          "time": "20210906 18:09"
        },
        {
          "id": "MDRW2100041940.1.19",
          "form": "네네 ㅋㅋ 저는 어항이 작아서 ㅋㅋ",
          "original_form": "네네 ㅋㅋ 저는 어항이 작아서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100041940.1.20",
          "form": "그렇게 해주고잇어요",
          "original_form": "그렇게 해주고잇어요",
          "speaker_id": "2",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100041940.1.21",
          "form": "역시 정성이 ㅠㅠ",
          "original_form": "역시 정성이 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041940.1.22",
          "form": "네 맞아요 쉬운게 없어요 ㅠ",
          "original_form": "네 맞아요 쉬운게 없어요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 18:17"
        }
      ]
    }
  ]
