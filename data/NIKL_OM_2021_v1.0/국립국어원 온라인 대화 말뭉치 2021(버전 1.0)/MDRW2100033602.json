      "utterance": [
        {
          "id": "MDRW2100033602.1.1",
          "form": "싫어하는 청소 있으신가여?",
          "original_form": "싫어하는 청소 있으신가여?",
          "speaker_id": "2",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100033602.1.2",
          "form": "어우 청소는 다싫지만 욕실청소요...",
          "original_form": "어우 청소는 다싫지만 욕실청소요...",
          "speaker_id": "1",
          "time": "20210824 16:50"
        },
        {
          "id": "MDRW2100033602.1.3",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100033602.1.4",
          "form": "욕실청소 최악이죠",
          "original_form": "욕실청소 최악이죠",
          "speaker_id": "2",
          "time": "20210824 16:53"
        },
        {
          "id": "MDRW2100033602.1.5",
          "form": "특히 변기...저는 비위상할때도 있고",
          "original_form": "특히 변기...저는 비위상할때도 있고",
          "speaker_id": "1",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100033602.1.6",
          "form": "너무 별로예요",
          "original_form": "너무 별로예요",
          "speaker_id": "1",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100033602.1.7",
          "form": "룸메는 괜찮아해서 룸메가 담당해주고있어요",
          "original_form": "룸메는 괜찮아해서 룸메가 담당해주고있어요",
          "speaker_id": "1",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100033602.1.8",
          "form": "저도 변기는 비위 상해요",
          "original_form": "저도 변기는 비위 상해요",
          "speaker_id": "2",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100033602.1.9",
          "form": "머리카락두요 으",
          "original_form": "머리카락두요 으",
          "speaker_id": "2",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100033602.1.10",
          "form": "어우 ....",
          "original_form": "어우 ....",
          "speaker_id": "1",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100033602.1.11",
          "form": "머리카락 어찌나 많이 빠지는지...",
          "original_form": "머리카락 어찌나 많이 빠지는지...",
          "speaker_id": "1",
          "time": "20210824 16:55"
        },
        {
          "id": "MDRW2100033602.1.12",
          "form": "=_= 정말 싫습니다",
          "original_form": "=_= 정말 싫습니다",
          "speaker_id": "2",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100033602.1.13",
          "form": "근데 룸메랑 같이 지내세요?",
          "original_form": "근데 룸메랑 같이 지내세요?",
          "speaker_id": "2",
          "time": "20210824 16:56"
        },
        {
          "id": "MDRW2100033602.1.14",
          "form": "네 룸메랑 지내고있어요",
          "original_form": "네 룸메랑 지내고있어요",
          "speaker_id": "1",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100033602.1.15",
          "form": "룸메랑 지내도 안불편한가요?",
          "original_form": "룸메랑 지내도 안불편한가요?",
          "speaker_id": "2",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100033602.1.16",
          "form": "집안일도 나눠서 하고있구요",
          "original_form": "집안일도 나눠서 하고있구요",
          "speaker_id": "1",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100033602.1.17",
          "form": "지금 일년좀 넘었는데",
          "original_form": "지금 일년좀 넘었는데",
          "speaker_id": "1",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100033602.1.18",
          "form": "헉 1년이나요?",
          "original_form": "헉 1년이나요?",
          "speaker_id": "2",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100033602.1.19",
          "form": "기숙산가요?",
          "original_form": "기숙산가요?",
          "speaker_id": "2",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100033602.1.20",
          "form": "생활패턴이 엄청 비슷한 친구라 불편함이 없어요",
          "original_form": "생활패턴이 엄청 비슷한 친구라 불편함이 없어요",
          "speaker_id": "1",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100033602.1.21",
          "form": "아니요 월세 얻어서 살고있죠",
          "original_form": "아니요 월세 얻어서 살고있죠",
          "speaker_id": "1",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100033602.1.22",
          "form": "우와 재밌을 거 같아요",
          "original_form": "우와 재밌을 거 같아요",
          "speaker_id": "2",
          "time": "20210824 17:00"
        },
        {
          "id": "MDRW2100033602.1.23",
          "form": "친구랑 같이 살면요",
          "original_form": "친구랑 같이 살면요",
