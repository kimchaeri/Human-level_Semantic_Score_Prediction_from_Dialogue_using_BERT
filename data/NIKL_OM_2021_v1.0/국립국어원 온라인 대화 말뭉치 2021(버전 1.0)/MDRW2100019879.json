        },
        {
          "id": "MDRW2100019879.1.2",
          "form": "저는 군대 는 안갓죠 ㅋ",
          "original_form": "저는 군대 는 안갓죠 ㅋ",
          "speaker_id": "1",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019879.1.3",
          "form": "남자들 군대 이야기 좋아하자나여!!",
          "original_form": "남자들 군대 이야기 좋아하자나여!!",
          "speaker_id": "1",
          "time": "20210827 22:38"
        },
        {
          "id": "MDRW2100019879.1.4",
          "form": "아^^ 좋아하진않아요ㅋ",
          "original_form": "아^^ 좋아하진않아요ㅋ",
          "speaker_id": "2",
          "time": "20210827 22:39"
        },
        {
          "id": "MDRW2100019879.1.5",
          "form": "엇 보통은 군대 이야기 많이 하든데",
          "original_form": "엇 보통은 군대 이야기 많이 하든데",
          "speaker_id": "1",
          "time": "20210827 22:40"
        },
        {
          "id": "MDRW2100019879.1.6",
          "form": "다른더 재미난 이야기많으니요",
          "original_form": "다른더 재미난 이야기많으니요",
          "speaker_id": "2",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019879.1.7",
          "form": "전군대는 쉽게가서요",
          "original_form": "전군대는 쉽게가서요",
          "speaker_id": "2",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019879.1.8",
          "form": "아항 그렇구낭",
          "original_form": "아항 그렇구낭",
          "speaker_id": "1",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019879.1.9",
          "form": "주제를.잘못골랏다아~~",
          "original_form": "주제를.잘못골랏다아~~",
          "speaker_id": "1",
          "time": "20210827 22:41"
        },
        {
          "id": "MDRW2100019879.1.10",
          "form": "힘들지않았어요^^~",
          "original_form": "힘들지않았어요^^~",
          "speaker_id": "2",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019879.1.11",
          "form": "총은 잘쏩니다",
          "original_form": "총은 잘쏩니다",
          "speaker_id": "2",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019879.1.12",
          "form": "그러시구낭 ㅡㅁ ㅡ/",
          "original_form": "그러시구낭 ㅡㅁ ㅡ/",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019879.1.13",
          "form": "저는 집안에 군인이 많아서",
          "original_form": "저는 집안에 군인이 많아서",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019879.1.14",
          "form": "군대 이야기 안지루하거든요",
          "original_form": "군대 이야기 안지루하거든요",
          "speaker_id": "1",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019879.1.15",
          "form": "요즘은 핸드폰들고드간데요",
          "original_form": "요즘은 핸드폰들고드간데요",
          "speaker_id": "2",
          "time": "20210827 22:42"
        },
        {
          "id": "MDRW2100019879.1.16",
          "form": "맞아요 ㅡㅁ ㅡ",
          "original_form": "맞아요 ㅡㅁ ㅡ",
          "speaker_id": "1",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019879.1.17",
          "form": "기간도 짧고 돈은많이주고",
          "original_form": "기간도 짧고 돈은많이주고",
          "speaker_id": "2",
          "time": "20210827 22:43"
        },
        {
          "id": "MDRW2100019879.1.18",
          "form": "ㅎㅎㅎ 예전같진 않쥬 ㅎㅎ",
