      "utterance": [
        {
          "id": "MDRW2100016438.1.1",
          "form": "넷플보시나요?",
          "original_form": "넷플보시나요?",
          "speaker_id": "1",
          "time": "20210826 12:48"
        },
        {
          "id": "MDRW2100016438.1.2",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016438.1.3",
          "form": "체인지데이즈 돌싱글즈 봐요 ㅋㅋㅋㅋ",
          "original_form": "체인지데이즈 돌싱글즈 봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100016438.1.4",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100016438.1.5",
          "form": "와 제스타일",
          "original_form": "와 제스타일",
          "speaker_id": "1",
          "time": "20210827 00:34"
        },
        {
          "id": "MDRW2100016438.1.6",
          "form": "둘다봅니다 저도ㅋ 돌싱이 더 재미나지않나요?",
          "original_form": "둘다봅니다 저도ㅋ 돌싱이 더 재미나지않나요?",
          "speaker_id": "1",
          "time": "20210827 00:34"
        },
        {
          "id": "MDRW2100016438.1.7",
          "form": "앜ㅋㅋ네 ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋ네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:34"
        },
        {
          "id": "MDRW2100016438.1.8",
          "form": "진 도도 빠르고 꿀잼",
          "original_form": "진 도도 빠르고 꿀잼",
          "speaker_id": "2",
          "time": "20210827 00:35"
        },
        {
          "id": "MDRW2100016438.1.9",
          "form": "체데는 너무 끌어서ㅜ",
          "original_form": "체데는 너무 끌어서ㅜ",
          "speaker_id": "2",
          "time": "20210827 00:35"
        },
        {
          "id": "MDRW2100016438.1.10",
          "form": "아 정말 제가하고픈말ㅋㅋㅋ",
          "original_form": "아 정말 제가하고픈말ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:36"
        },
        {
          "id": "MDRW2100016438.1.11",
          "form": "연예인도 아닌데 일반인이라 더 재미난건지ㅎㅎ",
          "original_form": "연예인도 아닌데 일반인이라 더 재미난건지ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 00:37"
        },
        {
          "id": "MDRW2100016438.1.12",
          "form": "ㅋㅋㅋㅋㅋ 이재열? ㅋㅋㅋ지짜 웃겨가지곸ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 이재열? ㅋㅋㅋ지짜 웃겨가지곸ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:37"
        },
        {
          "id": "MDRW2100016438.1.13",
          "form": "비호감인데웃겨 ㅋㅋ 유세윤도 반응 웃기구요 약간 맥이는 ㅋㅋ",
          "original_form": "비호감인데웃겨 ㅋㅋ 유세윤도 반응 웃기구요 약간 맥이는 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:38"
        },
        {
          "id": "MDRW2100016438.1.14",
          "form": "김재열!ㅋ",
          "original_form": "김재열!ㅋ",
          "speaker_id": "1",
          "time": "20210827 00:39"
        },
        {
          "id": "MDRW2100016438.1.15",
          "form": "그 이아영인가 걔랑 뭐 실제사귀느니 얘기나오던데여 추씨는ㅋ",
          "original_form": "그 이아영인가 걔랑 뭐 실제사귀느니 얘기나오던데여 추씨는ㅋ",
          "speaker_id": "1",
          "time": "20210827 00:39"
        },
        {
          "id": "MDRW2100016438.1.16",
          "form": "아진짜여 ㅋㅋㅋㅋ 짝되고 겁나 적극적이어지던데",
          "original_form": "아진짜여 ㅋㅋㅋㅋ 짝되고 겁나 적극적이어지던데",
          "speaker_id": "2",
          "time": "20210827 01:09"
        },
        {
          "id": "MDRW2100016438.1.17",
          "form": "이아영 몸매 부럽 ㅋㅋㅋ",
          "original_form": "이아영 몸매 부럽 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 01:10"
        },
        {
          "id": "MDRW2100016438.1.18",
          "form": "하 넘 재미나여",
          "original_form": "하 넘 재미나여",
          "speaker_id": "1",
          "time": "20210904 09:19"
        },
        {
          "id": "MDRW2100016438.1.19",
          "form": "매주기다려지는 방송ㅋㅋㅋ",
          "original_form": "매주기다려지는 방송ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 09:19"
        },
        {
          "id": "MDRW2100016438.1.20",
          "form": "그죠 궁금하다 ㅋㅋ",
          "original_form": "그죠 궁금하다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 09:24"
        },
        {
          "id": "MDRW2100016438.1.21",
          "form": "진짜 사귀고 다 할까요? 작가가 시킨걸까여?ㅋ",
          "original_form": "진짜 사귀고 다 할까요? 작가가 시킨걸까여?ㅋ",
          "speaker_id": "1",
          "time": "20210904 17:01"
        },
        {
          "id": "MDRW2100016438.1.22",
          "form": "ㅋㅋㅋㅋ 넘 궁금하요근디 ... 시켯음 연기천재들",
          "original_form": "ㅋㅋㅋㅋ 넘 궁금하요근디 ... 시켯음 연기천재들",
          "speaker_id": "2",
          "time": "20210904 17:18"
        },
        {
          "id": "MDRW2100016438.1.23",
          "form": "진짜여ㅋㅋ 매주 그래도 덕분에 시간 순삭이네요",
          "original_form": "진짜여ㅋㅋ 매주 그래도 덕분에 시간 순삭이네요",
          "speaker_id": "1",
          "time": "20210904 21:11"
        }
      ]
    }
