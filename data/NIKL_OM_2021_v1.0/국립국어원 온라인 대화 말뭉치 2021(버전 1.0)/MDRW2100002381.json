      "utterance": [
        {
          "id": "MDRW2100002381.1.1",
          "form": "아침엔 비올것같이 흐리더니 햇빛쨍쨍",
          "original_form": "아침엔 비올것같이 흐리더니 햇빛쨍쨍",
          "speaker_id": "2",
          "time": "20210820 11:51"
        },
        {
          "id": "MDRW2100002381.1.2",
          "form": "그러게요 이러다 또 금새 추워지겟죠 ㅠㅠ",
          "original_form": "그러게요 이러다 또 금새 추워지겟죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 13:07"
        },
        {
          "id": "MDRW2100002381.1.3",
          "form": "입추가 지나니까 확실히 기온이 떨어졌어요",
          "original_form": "입추가 지나니까 확실히 기온이 떨어졌어요",
          "speaker_id": "2",
          "time": "20210820 14:19"
        },
        {
          "id": "MDRW2100002381.1.4",
          "form": "저는 중부지방에 있는데, 담주내내 비온다네요",
          "original_form": "저는 중부지방에 있는데, 담주내내 비온다네요",
          "speaker_id": "2",
          "time": "20210820 14:19"
        },
        {
          "id": "MDRW2100002381.1.5",
          "form": "엇 저도 중부지방이요",
          "original_form": "엇 저도 중부지방이요",
          "speaker_id": "1",
          "time": "20210820 14:21"
        },
        {
          "id": "MDRW2100002381.1.6",
          "form": "헐 담부내내 비온대요..?",
          "original_form": "헐 담부내내 비온대요..?",
          "speaker_id": "1",
          "time": "20210820 14:22"
        },
        {
          "id": "MDRW2100002381.1.7",
          "form": "강원도로 놀러갈건뎅 ㅠㅠ",
          "original_form": "강원도로 놀러갈건뎅 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 14:22"
        },
        {
          "id": "MDRW2100002381.1.8",
          "form": "월화수목 비온데요",
          "original_form": "월화수목 비온데요",
          "speaker_id": "2",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002381.1.9",
          "form": "강원도는 중부지방아니예요",
          "original_form": "강원도는 중부지방아니예요",
          "speaker_id": "2",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002381.1.10",
          "form": "ㅋㅋㅋㅋ강원도도 올까봐용",
          "original_form": "ㅋㅋㅋㅋ강원도도 올까봐용",
          "speaker_id": "1",
          "time": "20210820 14:25"
        },
        {
          "id": "MDRW2100002381.1.11",
          "form": "어제는 낮에 해 비 해 비 이래서 비맞고다녔어요",
          "original_form": "어제는 낮에 해 비 해 비 이래서 비맞고다녔어요",
          "speaker_id": "2",
          "time": "20210820 14:25"
        },
        {
          "id": "MDRW2100002381.1.12",
          "form": "소나기도자주오고",
          "original_form": "소나기도자주오고",
          "speaker_id": "2",
          "time": "20210820 14:25"
        },
        {
          "id": "MDRW2100002381.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100002381.1.14",
          "form": "갑자기 먹구름끼고 소나기오고",
          "original_form": "갑자기 먹구름끼고 소나기오고",
          "speaker_id": "1",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100002381.1.15",
          "form": "하드라구요",
          "original_form": "하드라구요",
          "speaker_id": "1",
          "time": "20210820 14:28"
        },
        {
          "id": "MDRW2100002381.1.16",
          "form": "우산챙기셨어요?\n저는 누가훔쳐가서 오늘 하나 더 가져왔네요",
          "original_form": "우산챙기셨어요?\n저는 누가훔쳐가서 오늘 하나 더 가져왔네요",
          "speaker_id": "2",
          "time": "20210820 14:34"
        },
        {
          "id": "MDRW2100002381.1.17",
          "form": "ㅋㅋㅋ전 우산 제가잃어버려요",
          "original_form": "ㅋㅋㅋ전 우산 제가잃어버려요",
          "speaker_id": "1",
          "time": "20210820 14:44"
        },
        {
          "id": "MDRW2100002381.1.18",
          "form": "오늘 비온다는뎅 퇴근시간에만 안오면",
          "original_form": "오늘 비온다는뎅 퇴근시간에만 안오면",
          "speaker_id": "1",
          "time": "20210820 14:47"
        },
        {
          "id": "MDRW2100002381.1.19",
          "form": "차타고 후딱가려구엽 ㅠㅋ",
          "original_form": "차타고 후딱가려구엽 ㅠㅋ",
          "speaker_id": "1",
          "time": "20210820 14:47"
        },
        {
          "id": "MDRW2100002381.1.20",
          "form": "아 치과도 퇴근 6시예요?",
          "original_form": "아 치과도 퇴근 6시예요?",
          "speaker_id": "2",
          "time": "20210820 14:53"
        },
        {
          "id": "MDRW2100002381.1.21",
          "form": "보통은 6시반인데 환자없을땐 5시도 끝나고해요",
          "original_form": "보통은 6시반인데 환자없을땐 5시도 끝나고해요",
          "speaker_id": "1",
          "time": "20210820 14:55"
        },
        {
          "id": "MDRW2100002381.1.22",
          "form": "탄력근무네요",
          "original_form": "탄력근무네요",
          "speaker_id": "2",
          "time": "20210820 14:58"
        },
        {
          "id": "MDRW2100002381.1.23",
          "form": "저도5시에끝나요ㅎ.ㅅ",
          "original_form": "저도5시에끝나요ㅎ.ㅅ",
          "speaker_id": "2",
          "time": "20210820 14:58"
        }
      ]
