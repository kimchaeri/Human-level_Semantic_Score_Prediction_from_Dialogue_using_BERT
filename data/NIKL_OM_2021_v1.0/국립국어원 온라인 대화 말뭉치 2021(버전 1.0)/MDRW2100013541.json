      "utterance": [
        {
          "id": "MDRW2100013541.1.1",
          "form": "악 놀러가고시퍼! 코로나 언제끝나",
          "original_form": "악 놀러가고시퍼! 코로나 언제끝나",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013541.1.2",
          "form": "맞아 코로나때문에 어딜 가질 못하겠어",
          "original_form": "맞아 코로나때문에 어딜 가질 못하겠어",
          "speaker_id": "1",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013541.1.3",
          "form": "코로나 끝나면 여행사 박터질거같애",
          "original_form": "코로나 끝나면 여행사 박터질거같애",
          "speaker_id": "1",
          "time": "20210825 15:01"
        },
        {
          "id": "MDRW2100013541.1.4",
          "form": "코로나 끝나고도 여행 제대로 못갈듯",
          "original_form": "코로나 끝나고도 여행 제대로 못갈듯",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013541.1.5",
          "form": "한 1년은 어디에 가도 성수기일꺼같음...ㅠㅠ",
          "original_form": "한 1년은 어디에 가도 성수기일꺼같음...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013541.1.6",
          "form": "맞아 지금 외국 못나가니까",
          "original_form": "맞아 지금 외국 못나가니까",
          "speaker_id": "1",
          "time": "20210825 15:02"
        },
        {
          "id": "MDRW2100013541.1.7",
          "form": "국내 펜션같은 곳들 배짱장사해서",
          "original_form": "국내 펜션같은 곳들 배짱장사해서",
          "speaker_id": "1",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013541.1.8",
          "form": "빨리 코로나 잡혔으면 좋겠어",
          "original_form": "빨리 코로나 잡혔으면 좋겠어",
          "speaker_id": "1",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013541.1.9",
          "form": "와..진짜싫어",
          "original_form": "와..진짜싫어",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013541.1.10",
          "form": "숙박업 진짜 자꾸 호구만 잡으려하네ㅋㅋ",
          "original_form": "숙박업 진짜 자꾸 호구만 잡으려하네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013541.1.11",
          "form": "그치만 대전은 놀러 갈 거시다",
          "original_form": "그치만 대전은 놀러 갈 거시다",
          "speaker_id": "2",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013541.1.12",
          "form": "빵을 사먹을거시다",
          "original_form": "빵을 사먹을거시다",
          "speaker_id": "2",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013541.1.13",
          "form": "수영장에 온수 넣어주는걸",
          "original_form": "수영장에 온수 넣어주는걸",
          "speaker_id": "1",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013541.1.14",
          "form": "온수 온도에 따라 금액을 더 받는대..",
          "original_form": "온수 온도에 따라 금액을 더 받는대..",
          "speaker_id": "1",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013541.1.15",
          "form": "네??",
          "original_form": "네??",
          "speaker_id": "2",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013541.1.16",
          "form": "그걸 그렇게 바가지 씌운다고?ㅋㅋ",
          "original_form": "그걸 그렇게 바가지 씌운다고?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013541.1.17",
          "form": "역시 여행은 해외로 가야한다...",
          "original_form": "역시 여행은 해외로 가야한다...",
          "speaker_id": "2",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013541.1.18",
          "form": "코로나 끝나면 나와",
          "original_form": "코로나 끝나면 나와",
          "speaker_id": "2",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013541.1.19",
          "form": "그리고 예를들어서 최대 수용인원 8명인 방이면",
          "original_form": "그리고 예를들어서 최대 수용인원 8명인 방이면",
          "speaker_id": "1",
          "time": "20210825 15:06"
        },
        {
          "id": "MDRW2100013541.1.20",
          "form": "타이완. 놀러가자",
          "original_form": "타이완. 놀러가자",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013541.1.21",
          "form": "ㅇㅇㅇ?",
          "original_form": "ㅇㅇㅇ?",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013541.1.22",
          "form": "기본2인으로 걸어놓고 인원마다 추가금액 내게하고",
          "original_form": "기본2인으로 걸어놓고 인원마다 추가금액 내게하고",
          "speaker_id": "1",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013541.1.23",
          "form": "8인방이라고 광고하고 ㅋㅋ",
          "original_form": "8인방이라고 광고하고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013541.1.24",
          "form": "아 진짜 에바야ㅋㅋㅋ",
          "original_form": "아 진짜 에바야ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013541.1.25",
          "form": "그래서 타이완 오때유?태국",
          "original_form": "그래서 타이완 오때유?태국",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013541.1.26",
          "form": "동남아도 놀러가고싶긴 하다",
          "original_form": "동남아도 놀러가고싶긴 하다",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013541.1.27",
          "form": "나 태국 갔다왔는디 ㅠㅠ",
          "original_form": "나 태국 갔다왔는디 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013541.1.28",
          "form": "태국이래ㅋㅋ 대만",
          "original_form": "태국이래ㅋㅋ 대만",
          "speaker_id": "2",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100013541.1.29",
          "form": "동남아 너무 덥드라 음식도 너무 안맞고",
          "original_form": "동남아 너무 덥드라 음식도 너무 안맞고",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013541.1.30",
          "form": "타이완은 대만입니다",
          "original_form": "타이완은 대만입니다",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013541.1.31",
          "form": "나 한번도 안가봐서",
          "original_form": "나 한번도 안가봐서",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013541.1.32",
          "form": "좋아 안가본곳은 환영이야",
          "original_form": "좋아 안가본곳은 환영이야",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013541.1.33",
          "form": "우리 제주도도 가봐야되는데 그치",
          "original_form": "우리 제주도도 가봐야되는데 그치",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013541.1.34",
          "form": "대만 음식 웬만하면 괜찮오",
          "original_form": "대만 음식 웬만하면 괜찮오",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013541.1.35",
          "form": "마자마자",
          "original_form": "마자마자",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013541.1.36",
          "form": "면허따면 가고싶어",
          "original_form": "면허따면 가고싶어",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013541.1.37",
          "form": "자차필수라서..",
          "original_form": "자차필수라서..",
          "speaker_id": "1",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013541.1.38",
          "form": "우리중 면허 누가따냐",
          "original_form": "우리중 면허 누가따냐",
          "speaker_id": "2",
          "time": "20210825 15:08"
        },
        {
          "id": "MDRW2100013541.1.39",
          "form": "아님 셔틀이라도 빌려서",
          "original_form": "아님 셔틀이라도 빌려서",
          "speaker_id": "2",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013541.1.40",
          "form": "반쯤자유여행처럼",
          "original_form": "반쯤자유여행처럼",
          "speaker_id": "2",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013541.1.41",
          "form": "난 다른데 갈때마다",
          "original_form": "난 다른데 갈때마다",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013541.1.42",
          "form": "글케가도 좋겠다",
          "original_form": "글케가도 좋겠다",
          "speaker_id": "2",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013541.1.43",
          "form": "동생데리고 가니까",
          "original_form": "동생데리고 가니까",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013541.1.44",
          "form": "더 따기 귀찮은거 알지..",
          "original_form": "더 따기 귀찮은거 알지..",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013541.1.45",
          "form": "오 훌륭한 운전노예",
          "original_form": "오 훌륭한 운전노예",
          "speaker_id": "2",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013541.1.46",
          "form": "근데 여행 다닐려면 면허는 필수인듯",
          "original_form": "근데 여행 다닐려면 면허는 필수인듯",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013541.1.47",
          "form": "맞..아... 면허 따긴 해야되",
          "original_form": "맞..아... 면허 따긴 해야되",
          "speaker_id": "2",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013541.1.48",
          "form": "강원 삼척시에",
          "original_form": "강원 삼척시에",
          "speaker_id": "1",
          "time": "20210825 15:09"
        },
        {
          "id": "MDRW2100013541.1.49",
          "form": "방탕소년단 버터 앨범자켓 촬영을 했었나봐",
          "original_form": "방탕소년단 버터 앨범자켓 촬영을 했었나봐",
          "speaker_id": "1",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013541.1.50",
          "form": "팬들이 요청해서 자켓 촬영 소품재현해놓고",
          "original_form": "팬들이 요청해서 자켓 촬영 소품재현해놓고",
          "speaker_id": "1",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013541.1.51",
          "form": "포토존 설치했다고하네",
          "original_form": "포토존 설치했다고하네",
          "speaker_id": "1",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013541.1.52",
          "form": "방탄 덕좀 보는듯?",
          "original_form": "방탄 덕좀 보는듯?",
          "speaker_id": "1",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013541.1.53",
          "form": "방문기념 이벤트도 할 예정이래",
          "original_form": "방문기념 이벤트도 할 예정이래",
          "speaker_id": "1",
          "time": "20210825 15:10"
        }
      ]
