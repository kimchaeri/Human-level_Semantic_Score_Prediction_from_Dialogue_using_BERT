      "utterance": [
        {
          "id": "MDRW2100034993.1.1",
          "form": "오ㅡ그정도는 통과인가요ㅋ",
          "original_form": "오ㅡ그정도는 통과인가요ㅋ",
          "speaker_id": "1",
          "time": "20210828 23:33"
        },
        {
          "id": "MDRW2100034993.1.2",
          "form": "마라탕도 단계가 있어서",
          "original_form": "마라탕도 단계가 있어서",
          "speaker_id": "2",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100034993.1.3",
          "form": "순한맛 고르면돼요",
          "original_form": "순한맛 고르면돼요",
          "speaker_id": "2",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100034993.1.4",
          "form": "아ㅋㅋ1단계부터 도전해야겠네요",
          "original_form": "아ㅋㅋ1단계부터 도전해야겠네요",
          "speaker_id": "1",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100034993.1.5",
          "form": "어떤맛이랑 비슷해요?",
          "original_form": "어떤맛이랑 비슷해요?",
          "speaker_id": "1",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100034993.1.6",
          "form": "짬뽕같은건가",
          "original_form": "짬뽕같은건가",
          "speaker_id": "1",
          "time": "20210828 23:34"
        },
        {
          "id": "MDRW2100034993.1.7",
          "form": "음 향이 강해서 짬뽕과 달라요",
          "original_form": "음 향이 강해서 짬뽕과 달라요",
          "speaker_id": "2",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100034993.1.8",
          "form": "마라 향식료와 땅콩소스가 들어가요",
          "original_form": "마라 향식료와 땅콩소스가 들어가요",
          "speaker_id": "2",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100034993.1.9",
          "form": "땅콩소스라~ 상상도안되네요ㅎ",
          "original_form": "땅콩소스라~ 상상도안되네요ㅎ",
          "speaker_id": "1",
          "time": "20210829 01:23"
        },
        {
          "id": "MDRW2100034993.1.10",
          "form": "먹어보시면 맘에드실거 같아요 야채많이 넣구요",
          "original_form": "먹어보시면 맘에드실거 같아요 야채많이 넣구요",
          "speaker_id": "2",
          "time": "20210829 01:25"
        },
        {
          "id": "MDRW2100034993.1.11",
          "form": "오오~ 한번시켜봐야겠네요ㅡ어디가맛있을까요",
          "original_form": "오오~ 한번시켜봐야겠네요ㅡ어디가맛있을까요",
          "speaker_id": "1",
          "time": "20210829 01:33"
        },
        {
          "id": "MDRW2100034993.1.12",
          "form": "오 name1님네 동네는 어디가 유명한지 몰겠군용",
          "original_form": "오 &name1&님네 동네는 어디가 유명한지 몰겠군용",
          "speaker_id": "2",
          "time": "20210829 01:42"
        },
        {
          "id": "MDRW2100034993.1.13",
          "form": "아ㅡ체인은 아니신가바영ㅎ",
          "original_form": "아ㅡ체인은 아니신가바영ㅎ",
          "speaker_id": "1",
          "time": "20210829 10:16"
        },
        {
          "id": "MDRW2100034993.1.14",
          "form": "마라탕체인이 엄청 많아요",
          "original_form": "마라탕체인이 엄청 많아요",
          "speaker_id": "2",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100034993.1.15",
          "form": "엄청엄청",
          "original_form": "엄청엄청",
          "speaker_id": "2",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100034993.1.16",
          "form": "그쵸. 드셔보신거중에 맛있던 체인은요?ㅋ",
          "original_form": "그쵸. 드셔보신거중에 맛있던 체인은요?ㅋ",
          "speaker_id": "1",
          "time": "20210829 12:54"
        },
        {
          "id": "MDRW2100034993.1.17",
          "form": "음 여기저기 근처로 다녀서 상호는 잘 기억못해요ㅜㅜ",
          "original_form": "음 여기저기 근처로 다녀서 상호는 잘 기억못해요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 13:35"
        },
        {
          "id": "MDRW2100034993.1.18",
          "form": "아ㅎㅎ. 그렇구나ㅎ 나중에꼭 마라한번 도전해볼게요",
          "original_form": "아ㅎㅎ. 그렇구나ㅎ 나중에꼭 마라한번 도전해볼게요",
          "speaker_id": "1",
          "time": "20210829 13:48"
        }
      ]
