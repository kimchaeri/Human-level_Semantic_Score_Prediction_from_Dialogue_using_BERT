      "utterance": [
        {
          "id": "MDRW2100005001.1.1",
          "form": "여행 진짜",
          "original_form": "여행 진짜",
          "speaker_id": "2",
          "time": "20210821 19:52"
        },
        {
          "id": "MDRW2100005001.1.2",
          "form": "괌 하와이 몰디브 어디든",
          "original_form": "괌 하와이 몰디브 어디든",
          "speaker_id": "2",
          "time": "20210821 19:52"
        },
        {
          "id": "MDRW2100005001.1.3",
          "form": "너희랑 가서 놀고 싶음",
          "original_form": "너희랑 가서 놀고 싶음",
          "speaker_id": "2",
          "time": "20210821 19:53"
        },
        {
          "id": "MDRW2100005001.1.4",
          "form": "ㅠㅠㅠ 그니까",
          "original_form": "ㅠㅠㅠ 그니까",
          "speaker_id": "1",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100005001.1.5",
          "form": "그런 피서지",
          "original_form": "그런 피서지",
          "speaker_id": "1",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100005001.1.6",
          "form": "너네랑 가면 너무 재밌을 구 같음",
          "original_form": "너네랑 가면 너무 재밌을 구 같음",
          "speaker_id": "1",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100005001.1.7",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210821 19:56"
        },
        {
          "id": "MDRW2100005001.1.8",
          "form": "레저도 즐기고",
          "original_form": "레저도 즐기고",
          "speaker_id": "2",
          "time": "20210821 19:56"
        },
        {
          "id": "MDRW2100005001.1.9",
          "form": "맛있는 것도 먹고",
          "original_form": "맛있는 것도 먹고",
          "speaker_id": "2",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100005001.1.10",
          "form": "수상스키 이런 거",
          "original_form": "수상스키 이런 거",
          "speaker_id": "1",
          "time": "20210821 19:58"
        },
        {
          "id": "MDRW2100005001.1.11",
          "form": "너무 하고 싶다",
          "original_form": "너무 하고 싶다",
          "speaker_id": "1",
          "time": "20210821 19:58"
        },
        {
          "id": "MDRW2100005001.1.12",
          "form": "이번 여름은 워터파크도 못 가고",
          "original_form": "이번 여름은 워터파크도 못 가고",
          "speaker_id": "1",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100005001.1.13",
          "form": "여름 하나도 못 즐김 ㅡㅠㅠ",
          "original_form": "여름 하나도 못 즐김 ㅡㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100005001.1.14",
          "form": "스쿠버다이빙이랑 수상스키 진짜",
          "original_form": "스쿠버다이빙이랑 수상스키 진짜",
          "speaker_id": "2",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100005001.1.15",
          "form": "서핑도 배워보고 싶음",
          "original_form": "서핑도 배워보고 싶음",
          "speaker_id": "2",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100005001.1.16",
          "form": "재밌어 보이더라",
          "original_form": "재밌어 보이더라",
          "speaker_id": "2",
          "time": "20210821 19:59"
        },
        {
          "id": "MDRW2100005001.1.17",
          "form": "ㅠㅠㅠㅠ서핑 인정",
          "original_form": "ㅠㅠㅠㅠ서핑 인정",
          "speaker_id": "1",
          "time": "20210821 20:02"
        },
        {
          "id": "MDRW2100005001.1.18",
          "form": "스쿠버다이빙 너무재밌어...",
          "original_form": "스쿠버다이빙 너무재밌어...",
          "speaker_id": "1",
          "time": "20210821 20:02"
        },
        {
          "id": "MDRW2100005001.1.19",
          "form": "수영도 못하는 물찔이(?)지만",
          "original_form": "수영도 못하는 물찔이(?)지만",
          "speaker_id": "2",
          "time": "20210821 20:04"
        },
        {
          "id": "MDRW2100005001.1.20",
          "form": "요즘 수상 레저 찾아보면 뭔가 알 수 없는 자신감이랑 기대로 물공포증도 앖어질 거 같음",
          "original_form": "요즘 수상 레저 찾아보면 뭔가 알 수 없는 자신감이랑 기대로 물공포증도 앖어질 거 같음",
          "speaker_id": "2",
          "time": "20210821 20:05"
        },
        {
          "id": "MDRW2100005001.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:06"
        },
        {
          "id": "MDRW2100005001.1.22",
          "form": "수영하고 싶다...",
          "original_form": "수영하고 싶다...",
          "speaker_id": "1",
          "time": "20210821 20:06"
        },
        {
          "id": "MDRW2100005001.1.23",
          "form": "너네랑 같이",
          "original_form": "너네랑 같이",
          "speaker_id": "1",
          "time": "20210821 20:06"
        },
        {
          "id": "MDRW2100005001.1.24",
          "form": "수영 다니고 싶음",
          "original_form": "수영 다니고 싶음",
          "speaker_id": "1",
          "time": "20210821 20:06"
        },
        {
          "id": "MDRW2100005001.1.25",
          "form": "백신 접종률 높아지면 같이 다닐 수 있을지도",
          "original_form": "백신 접종률 높아지면 같이 다닐 수 있을지도",
          "speaker_id": "2",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100005001.1.26",
          "form": "행복 회로 끼긱끼긱",
          "original_form": "행복 회로 끼긱끼긱",
          "speaker_id": "2",
          "time": "20210821 20:12"
        },
        {
          "id": "MDRW2100005001.1.27",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005001.1.28",
          "form": "코러나 끝나면",
          "original_form": "코러나 끝나면",
          "speaker_id": "1",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005001.1.29",
          "form": "딱대",
          "original_form": "딱대",
          "speaker_id": "1",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005001.1.30",
          "form": "좀만 잠잠해져도",
          "original_form": "좀만 잠잠해져도",
          "speaker_id": "1",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005001.1.31",
          "form": "바로 제주도 각이야",
          "original_form": "바로 제주도 각이야",
          "speaker_id": "1",
          "time": "20210821 20:13"
        },
        {
          "id": "MDRW2100005001.1.32",
          "form": "아 좋다",
          "original_form": "아 좋다",
          "speaker_id": "2",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100005001.1.33",
          "form": "제주도 맛집 다 털어",
          "original_form": "제주도 맛집 다 털어",
          "speaker_id": "2",
          "time": "20210821 20:16"
        },
        {
          "id": "MDRW2100005001.1.34",
          "form": "너무 좋다",
          "original_form": "너무 좋다",
          "speaker_id": "1",
          "time": "20210821 20:17"
        },
        {
          "id": "MDRW2100005001.1.35",
          "form": "근데 면허 따고 제주도 여행 가려 했는데",
          "original_form": "근데 면허 따고 제주도 여행 가려 했는데",
          "speaker_id": "1",
          "time": "20210821 20:17"
        },
        {
          "id": "MDRW2100005001.1.36",
          "form": "렌트카 3년 뒤에 대여 가능인가? 그렇다 들어서",
          "original_form": "렌트카 3년 뒤에 대여 가능인가? 그렇다 들어서",
          "speaker_id": "1",
          "time": "20210821 20:17"
        },
        {
          "id": "MDRW2100005001.1.37",
          "form": "지금 따길 잘했네",
          "original_form": "지금 따길 잘했네",
          "speaker_id": "2",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005001.1.38",
          "form": "그니까...",
          "original_form": "그니까...",
          "speaker_id": "1",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005001.1.39",
          "form": "에효~",
          "original_form": "에효~",
          "speaker_id": "1",
          "time": "20210821 20:22"
        }
      ]
