      "utterance": [
        {
          "id": "MDRW2100011676.1.1",
          "form": "옷사고싶다",
          "original_form": "옷사고싶다",
          "speaker_id": "2",
          "time": "20210824 15:02"
        },
        {
          "id": "MDRW2100011676.1.2",
          "form": "식탁이랑 쇼파사야하는데...",
          "original_form": "식탁이랑 쇼파사야하는데...",
          "speaker_id": "1",
          "time": "20210824 15:02"
        },
        {
          "id": "MDRW2100011676.1.3",
          "form": "옷 많잖아",
          "original_form": "옷 많잖아",
          "speaker_id": "1",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011676.1.4",
          "form": "식탁이쁜거 사라",
          "original_form": "식탁이쁜거 사라",
          "speaker_id": "2",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011676.1.5",
          "form": "이쁜거 사야지",
          "original_form": "이쁜거 사야지",
          "speaker_id": "1",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011676.1.6",
          "form": "식탁은 보면... 의자가 맘에안들더라",
          "original_form": "식탁은 보면... 의자가 맘에안들더라",
          "speaker_id": "2",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011676.1.7",
          "form": "옷 뭐 사고 싶어?",
          "original_form": "옷 뭐 사고 싶어?",
          "speaker_id": "1",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011676.1.8",
          "form": "자켓",
          "original_form": "자켓",
          "speaker_id": "2",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011676.1.9",
          "form": "그래 두개다 마음에 들기 쉽지않더라",
          "original_form": "그래 두개다 마음에 들기 쉽지않더라",
          "speaker_id": "1",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011676.1.10",
          "form": "쇼파는 뭐살려고?",
          "original_form": "쇼파는 뭐살려고?",
          "speaker_id": "2",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011676.1.11",
          "form": "무슨 자켓?",
          "original_form": "무슨 자켓?",
          "speaker_id": "1",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011676.1.12",
          "form": "그때 박은영 식탁이 젤맘에듦",
          "original_form": "그때 박은영 식탁이 젤맘에듦",
          "speaker_id": "2",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100011676.1.13",
          "form": "고민중.....",
          "original_form": "고민중.....",
          "speaker_id": "1",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011676.1.14",
          "form": "박은영???",
          "original_form": "박은영???",
          "speaker_id": "1",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011676.1.15",
          "form": "자켓 이쁜거",
          "original_form": "자켓 이쁜거",
          "speaker_id": "2",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011676.1.16",
          "form": "아나운서 박은영 집",
          "original_form": "아나운서 박은영 집",
          "speaker_id": "2",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011676.1.17",
          "form": "어떤 종류?",
          "original_form": "어떤 종류?",
          "speaker_id": "1",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011676.1.18",
          "form": "쏘홈 식탁",
          "original_form": "쏘홈 식탁",
          "speaker_id": "2",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011676.1.19",
          "form": "한번 찾아봐야겠네",
          "original_form": "한번 찾아봐야겠네",
          "speaker_id": "1",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011676.1.20",
          "form": "얼만데?",
          "original_form": "얼만데?",
          "speaker_id": "1",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011676.1.21",
          "form": "흰색세라믹에",
          "original_form": "흰색세라믹에",
          "speaker_id": "2",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011676.1.22",
          "form": "의자 플라스틱 블랙",
          "original_form": "의자 플라스틱 블랙",
          "speaker_id": "2",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011676.1.23",
          "form": "가격은?",
          "original_form": "가격은?",
          "speaker_id": "1",
          "time": "20210824 15:04"
        },
        {
          "id": "MDRW2100011676.1.24",
          "form": "못찾겠더라\r\n쏘홈이 비싸단다",
          "original_form": "못찾겠더라\r\n쏘홈이 비싸단다",
