      "utterance": [
        {
          "id": "MDRW2100002564.1.1",
          "form": "혹시 하고 계신 커뮤니티같은거 있으신가요",
          "original_form": "혹시 하고 계신 커뮤니티같은거 있으신가요",
          "speaker_id": "1",
          "time": "20210820 13:35"
        },
        {
          "id": "MDRW2100002564.1.2",
          "form": "커뮤니티는 딱히 안하고",
          "original_form": "커뮤니티는 딱히 안하고",
          "speaker_id": "2",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002564.1.3",
          "form": "카카오톡 단톡?은 하긴해용",
          "original_form": "카카오톡 단톡?은 하긴해용",
          "speaker_id": "2",
          "time": "20210820 13:36"
        },
        {
          "id": "MDRW2100002564.1.4",
          "form": "오픈채팅은요??",
          "original_form": "오픈채팅은요??",
          "speaker_id": "1",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002564.1.5",
          "form": "약간 소규모 커뮤니티 느낌아닌가요 ㅎㅎ",
          "original_form": "약간 소규모 커뮤니티 느낌아닌가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002564.1.6",
          "form": "오픈채팅은 옛날에 하긴했죠!",
          "original_form": "오픈채팅은 옛날에 하긴했죠!",
          "speaker_id": "2",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002564.1.7",
          "form": "아이돌 덕질했을때 했던 기억이나네요",
          "original_form": "아이돌 덕질했을때 했던 기억이나네요",
          "speaker_id": "2",
          "time": "20210820 13:37"
        },
        {
          "id": "MDRW2100002564.1.8",
          "form": "어떤아이돌 좋아하셧나요",
          "original_form": "어떤아이돌 좋아하셧나요",
          "speaker_id": "1",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002564.1.9",
          "form": "아이돌 팬카페같은것도 가입해보신적 있으신가요 ㅋㅋㅋ",
          "original_form": "아이돌 팬카페같은것도 가입해보신적 있으신가요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002564.1.10",
          "form": "저는 지금은 해체한",
          "original_form": "저는 지금은 해체한",
          "speaker_id": "2",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002564.1.11",
          "form": "아이즈원 좋아했어요!",
          "original_form": "아이즈원 좋아했어요!",
          "speaker_id": "2",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002564.1.12",
          "form": "팬카페도 생각해보니",
          "original_form": "팬카페도 생각해보니",
          "speaker_id": "2",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002564.1.13",
          "form": "가입했었네요 ㅋㅋㅋ",
          "original_form": "가입했었네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002564.1.14",
          "form": "누구꺼 가입하셨나요 ㅋㅋ",
          "original_form": "누구꺼 가입하셨나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002564.1.15",
          "form": "전 슈퍼주니어 팬카페 가입해봤어요",
          "original_form": "전 슈퍼주니어 팬카페 가입해봤어요",
          "speaker_id": "1",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002564.1.16",
          "form": "아이즈원이랑 트와이스 가입해봤네요~!",
          "original_form": "아이즈원이랑 트와이스 가입해봤네요~!",
          "speaker_id": "2",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002564.1.17",
          "form": "트와이스 좋아해요",
          "original_form": "트와이스 좋아해요",
          "speaker_id": "1",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002564.1.18",
          "form": "근데 요즘 팬카페는 잘 활용안되는것 같아요",
          "original_form": "근데 요즘 팬카페는 잘 활용안되는것 같아요",
          "speaker_id": "1",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002564.1.19",
          "form": "안해서 모르는건가.. ㅋㅋ",
          "original_form": "안해서 모르는건가.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002564.1.20",
          "form": "팬카페보단",
          "original_form": "팬카페보단",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002564.1.21",
          "form": "오픈채팅으로 많이들 접하니",
          "original_form": "오픈채팅으로 많이들 접하니",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002564.1.22",
          "form": "크게 활용되지는 않는거같더라고요",
          "original_form": "크게 활용되지는 않는거같더라고요",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002564.1.23",
          "form": "그쵸 ㅋㅋ 옛날에는 팬카페에서 가수 일정들 알려주고 그랬는데",
          "original_form": "그쵸 ㅋㅋ 옛날에는 팬카페에서 가수 일정들 알려주고 그랬는데",
          "speaker_id": "1",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002564.1.24",
          "form": "요즘은 고독한 ㅇㅇㅇ으로 소통하는 것 같아요",
          "original_form": "요즘은 고독한 ㅇㅇㅇ으로 소통하는 것 같아요",
          "speaker_id": "1",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002564.1.25",
          "form": "고독한, 안고독한 등 많긴하죠!",
          "original_form": "고독한, 안고독한 등 많긴하죠!",
          "speaker_id": "2",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002564.1.26",
          "form": "옛날에는 박명수방이 유명했는데",
          "original_form": "옛날에는 박명수방이 유명했는데",
          "speaker_id": "1",
          "time": "20210820 16:02"
        },
        {
          "id": "MDRW2100002564.1.27",
          "form": "지금은 올림픽 선수들이 유명할까요 ㅋㅋ",
          "original_form": "지금은 올림픽 선수들이 유명할까요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:02"
        },
        {
          "id": "MDRW2100002564.1.28",
          "form": "그럴거같아요!",
          "original_form": "그럴거같아요!",
          "speaker_id": "2",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002564.1.29",
          "form": "아니면 에이프릴쪽도 최근에 유명하다고 하던데",
          "original_form": "아니면 에이프릴쪽도 최근에 유명하다고 하던데",
          "speaker_id": "2",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002564.1.30",
          "form": "에이프릴은 왜요??",
          "original_form": "에이프릴은 왜요??",
          "speaker_id": "1",
          "time": "20210820 16:04"
        },
        {
          "id": "MDRW2100002564.1.31",
          "form": "아까전에 들어가봤는데 에이프릴이 알고리즘으로 뜨더라고요",
          "original_form": "아까전에 들어가봤는데 에이프릴이 알고리즘으로 뜨더라고요",
          "speaker_id": "2",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002564.1.32",
          "form": "이유는 모르겠지만..",
          "original_form": "이유는 모르겠지만..",
          "speaker_id": "2",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002564.1.33",
          "form": "ㅋㅋ최근에 왕따설 이유로 팬들이 많이 사라진줄 알았는데 아닌가보네용..",
          "original_form": "ㅋㅋ최근에 왕따설 이유로 팬들이 많이 사라진줄 알았는데 아닌가보네용..",
          "speaker_id": "1",
          "time": "20210820 16:33"
        }
      ]
    }
