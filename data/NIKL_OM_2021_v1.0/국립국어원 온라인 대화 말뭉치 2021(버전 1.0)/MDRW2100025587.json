      "utterance": [
        {
          "id": "MDRW2100025587.1.1",
          "form": "좋아하시는 스포츠가 있으신가요",
          "original_form": "좋아하시는 스포츠가 있으신가요",
          "speaker_id": "1",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100025587.1.2",
          "form": "저는 딱히 없어요",
          "original_form": "저는 딱히 없어요",
          "speaker_id": "2",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100025587.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210901 13:59"
        },
        {
          "id": "MDRW2100025587.1.4",
          "form": "저는 한 10년전에? 대학생땐 야구좋아했어요",
          "original_form": "저는 한 10년전에? 대학생땐 야구좋아했어요",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025587.1.5",
          "form": "어떤 팀 좋아하셨나요?",
          "original_form": "어떤 팀 좋아하셨나요?",
          "speaker_id": "2",
          "time": "20210901 14:13"
        },
        {
          "id": "MDRW2100025587.1.6",
          "form": "저는 대구에서 학교나와서 삼성라이온즈 팬이었어요ㅎㅎ",
          "original_form": "저는 대구에서 학교나와서 삼성라이온즈 팬이었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100025587.1.7",
          "form": "삼성이 대구였나요??",
          "original_form": "삼성이 대구였나요??",
          "speaker_id": "2",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100025587.1.8",
          "form": "네네 ㅋㅋ그때만해도 까마득한 옛날이었죠 감독 선동열이고 ㅋㅋ",
          "original_form": "네네 ㅋㅋ그때만해도 까마득한 옛날이었죠 감독 선동열이고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100025587.1.9",
          "form": "그렇군요 그럼 직관도 자주 가셨나요?",
          "original_form": "그렇군요 그럼 직관도 자주 가셨나요?",
          "speaker_id": "2",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100025587.1.10",
          "form": "그때는 자주갔었어요 ㅋㅋ",
          "original_form": "그때는 자주갔었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100025587.1.11",
          "form": "표를 공짜로 얻거나 2천원에 사거나 그렇게해서요",
          "original_form": "표를 공짜로 얻거나 2천원에 사거나 그렇게해서요",
          "speaker_id": "1",
          "time": "20210901 21:11"
        },
        {
          "id": "MDRW2100025587.1.12",
          "form": "어떻게 그렇게 싸게 가요?",
          "original_form": "어떻게 그렇게 싸게 가요?",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100025587.1.13",
          "form": "매우 오래전이었어요 ㅋㅋㄱ",
          "original_form": "매우 오래전이었어요 ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100025587.1.14",
          "form": "물가가 쌌던것도있고",
          "original_form": "물가가 쌌던것도있고",
          "speaker_id": "1",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100025587.1.15",
          "form": "그러면 요즘 관심가는 스포츠 있으신가요?",
          "original_form": "그러면 요즘 관심가는 스포츠 있으신가요?",
          "speaker_id": "2",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100025587.1.16",
          "form": "여전히 야구는좋아하는데 전처럼 막 챙겨보지는않네요ㅜ",
          "original_form": "여전히 야구는좋아하는데 전처럼 막 챙겨보지는않네요ㅜ",
          "speaker_id": "1",
