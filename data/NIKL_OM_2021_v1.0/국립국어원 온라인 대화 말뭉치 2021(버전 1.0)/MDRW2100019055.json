      "utterance": [
        {
          "id": "MDRW2100019055.1.1",
          "form": "오늘 계신곳의 날씨는 어때요?",
          "original_form": "오늘 계신곳의 날씨는 어때요?",
          "speaker_id": "1",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019055.1.2",
          "form": "이쪽은 비는 안오는데 우중충해요",
          "original_form": "이쪽은 비는 안오는데 우중충해요",
          "speaker_id": "2",
          "time": "20210827 16:13"
        },
        {
          "id": "MDRW2100019055.1.3",
          "form": "거긴 어떠신가요ㅣ",
          "original_form": "거긴 어떠신가요ㅣ",
          "speaker_id": "2",
          "time": "20210827 16:13"
        },
        {
          "id": "MDRW2100019055.1.4",
          "form": "저희는 오전에는 하늘만 좀 어둡더니\r\n오후부터는 비가 조금씩 내리네요:)",
          "original_form": "저희는 오전에는 하늘만 좀 어둡더니\r\n오후부터는 비가 조금씩 내리네요:)",
          "speaker_id": "1",
          "time": "20210827 16:16"
        },
        {
          "id": "MDRW2100019055.1.5",
          "form": "아하 이쪽도 곧 내릴 것 같아요",
          "original_form": "아하 이쪽도 곧 내릴 것 같아요",
          "speaker_id": "2",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100019055.1.6",
          "form": "우산은 오늘 챙기셨어요?",
          "original_form": "우산은 오늘 챙기셨어요?",
          "speaker_id": "2",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100019055.1.7",
          "form": "저는 지금 집인데\r\n우산 챙기셨나요?",
          "original_form": "저는 지금 집인데\r\n우산 챙기셨나요?",
          "speaker_id": "1",
          "time": "20210827 16:19"
        },
        {
          "id": "MDRW2100019055.1.8",
          "form": "저도 집이에요ㅋㅋㅋ",
          "original_form": "저도 집이에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019055.1.9",
          "form": "둘다 비 걱정은 안해도 되겠네요",
          "original_form": "둘다 비 걱정은 안해도 되겠네요",
          "speaker_id": "2",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019055.1.10",
          "form": "앗ㅋㅋ 집이셨군요",
          "original_form": "앗ㅋㅋ 집이셨군요",
          "speaker_id": "1",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019055.1.11",
          "form": "이따 미용실 가려고\r\n예약해 뒀는데\r\n비가 오니 집밖에 나가기 싫어지네요ㅠㅋ",
          "original_form": "이따 미용실 가려고\r\n예약해 뒀는데\r\n비가 오니 집밖에 나가기 싫어지네요ㅠㅋ",
          "speaker_id": "1",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019055.1.12",
          "form": "아하ㅋㅋㅋㅋㅋㅋㅠ 그러시군요",
          "original_form": "아하ㅋㅋㅋㅋㅋㅋㅠ 그러시군요",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019055.1.13",
          "form": "미용실은 비안올때 가는 것도 넘 귀찮은거 같아요",
          "original_form": "미용실은 비안올때 가는 것도 넘 귀찮은거 같아요",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019055.1.14",
          "form": "오늘 비오는데 저녁은 뭐드세요?",
          "original_form": "오늘 비오는데 저녁은 뭐드세요?",
          "speaker_id": "2",
          "time": "20210827 16:30"
        },
        {
          "id": "MDRW2100019055.1.15",
          "form": "비오는날은 또 기름 아니겠어여ㅋㅋ",
          "original_form": "비오는날은 또 기름 아니겠어여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019055.1.16",
          "form": "김치전이 더 땡기긴 합니다만\r\n치킨이 먹으려구요ㅋㅋ",
          "original_form": "김치전이 더 땡기긴 합니다만\r\n치킨이 먹으려구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:32"
        },
        {
          "id": "MDRW2100019055.1.17",
          "form": "아 뭘 좀 아시네여",
          "original_form": "아 뭘 좀 아시네여",
          "speaker_id": "2",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019055.1.18",
          "form": "저는 부추전 먹으려구요",
          "original_form": "저는 부추전 먹으려구요",
          "speaker_id": "2",
          "time": "20210827 16:35"
        },
        {
          "id": "MDRW2100019055.1.19",
          "form": "부추를 얻어서 빨리 먹어야해요 ㅠㅠ",
          "original_form": "부추를 얻어서 빨리 먹어야해요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019055.1.20",
          "form": "ㅋㅋ맞아요\r\n오늘 같은 날에 정말 딱이죠",
          "original_form": "ㅋㅋ맞아요\r\n오늘 같은 날에 정말 딱이죠{emoji:♡}",
          "speaker_id": "1",
          "time": "20210827 16:36"
        },
        {
          "id": "MDRW2100019055.1.21",
          "form": "밤맛 막걸리 사오고 싶네요ㅋㅋㅋ",
          "original_form": "밤맛 막걸리 사오고 싶네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019055.1.22",
          "form": "비오는날에는 알밤동동인데 드셔보셨어요?",
          "original_form": "비오는날에는 알밤동동인데 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210827 16:37"
        },
        {
          "id": "MDRW2100019055.1.23",
          "form": "비오는날의 막걸리라...",
          "original_form": "비오는날의 막걸리라...",
          "speaker_id": "1",
          "time": "20210827 16:39"
        }
      ]
