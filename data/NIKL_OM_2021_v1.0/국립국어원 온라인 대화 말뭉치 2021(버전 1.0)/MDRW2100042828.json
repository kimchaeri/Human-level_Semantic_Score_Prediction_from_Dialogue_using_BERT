      "utterance": [
        {
          "id": "MDRW2100042828.1.1",
          "form": "미용은 따로 하시는거 있으세요?",
          "original_form": "미용은 따로 하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042828.1.2",
          "form": "미용 흠 팩이요",
          "original_form": "미용 흠 팩이요",
          "speaker_id": "1",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042828.1.3",
          "form": "프레쉬 로즈팩 아시나요",
          "original_form": "프레쉬 로즈팩 아시나요",
          "speaker_id": "1",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042828.1.4",
          "form": "아뇨!",
          "original_form": "아뇨!",
          "speaker_id": "2",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042828.1.5",
          "form": "좋아요?",
          "original_form": "좋아요?",
          "speaker_id": "2",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042828.1.6",
          "form": "저 이틀연속 시트팩하댜 지금 트러블났어요ㅜㅜ",
          "original_form": "저 이틀연속 시트팩하댜 지금 트러블났어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042828.1.7",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042828.1.8",
          "form": "시트팩보담은 귀찮아여",
          "original_form": "시트팩보담은 귀찮아여",
          "speaker_id": "1",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042828.1.9",
          "form": "워시오프라",
          "original_form": "워시오프라",
          "speaker_id": "1",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042828.1.10",
          "form": "그럼 씻고 그거 바르고 다시 씻는거죠?",
          "original_form": "그럼 씻고 그거 바르고 다시 씻는거죠?",
          "speaker_id": "2",
          "time": "20210907 09:52"
        },
        {
          "id": "MDRW2100042828.1.11",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210907 09:55"
        },
        {
          "id": "MDRW2100042828.1.12",
          "form": "올영세일때 뭐 안사셨어요",
          "original_form": "올영세일때 뭐 안사셨어요",
          "speaker_id": "1",
          "time": "20210907 09:56"
        },
        {
          "id": "MDRW2100042828.1.13",
          "form": "안사면 백프로다!! ㅋㅋ",
          "original_form": "안사면 백프로다!! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042828.1.14",
          "form": "시트팩이 너무 많아요",
          "original_form": "시트팩이 너무 많아요",
          "speaker_id": "2",
          "time": "20210907 09:59"
        },
        {
          "id": "MDRW2100042828.1.15",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210907 10:00"
        },
        {
          "id": "MDRW2100042828.1.16",
          "form": "전 이번에도 엄청 질렀어요ㅠ",
          "original_form": "전 이번에도 엄청 질렀어요ㅠ",
          "speaker_id": "1",
          "time": "20210907 10:00"
        },
        {
          "id": "MDRW2100042828.1.17",
          "form": "ㅋㅋ필요한거 사면 좋은 쇼핑이죠?",
          "original_form": "ㅋㅋ필요한거 사면 좋은 쇼핑이죠?",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100042828.1.18",
          "form": "!",
          "original_form": "!",
          "speaker_id": "2",
          "time": "20210907 10:01"
        },
        {
          "id": "MDRW2100042828.1.19",
          "form": "가르시니아 어쩌구 그런거고 드셔보셨나요",
          "original_form": "가르시니아 어쩌구 그런거고 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210907 10:03"
        },
        {
          "id": "MDRW2100042828.1.20",
          "form": "어 들어본거같아요",
          "original_form": "어 들어본거같아요",
          "speaker_id": "2",
          "time": "20210907 10:06"
        },
        {
          "id": "MDRW2100042828.1.21",
          "form": "지방분해 효과있다는데",
          "original_form": "지방분해 효과있다는데",
          "speaker_id": "1",
          "time": "20210907 10:07"
        },
        {
          "id": "MDRW2100042828.1.22",
          "form": "효과있나요?",
          "original_form": "효과있나요?",
          "speaker_id": "2",
          "time": "20210907 10:07"
        },
        {
          "id": "MDRW2100042828.1.23",
          "form": "구매만 하신 상태셔요?",
          "original_form": "구매만 하신 상태셔요?",
          "speaker_id": "2",
          "time": "20210907 10:08"
        },
        {
          "id": "MDRW2100042828.1.24",
          "form": "아뇨 안가봤는데 다이어트 된다해서 관심만요",
          "original_form": "아뇨 안가봤는데 다이어트 된다해서 관심만요",
          "speaker_id": "1",
          "time": "20210907 10:08"
        }
