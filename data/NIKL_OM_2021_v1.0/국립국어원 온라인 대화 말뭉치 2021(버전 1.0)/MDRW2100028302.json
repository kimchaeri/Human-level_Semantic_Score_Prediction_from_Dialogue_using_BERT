      "utterance": [
        {
          "id": "MDRW2100028302.1.1",
          "form": "아무튼 냉면 맛있겟다",
          "original_form": "아무튼 냉면 맛있겟다",
          "speaker_id": "1",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100028302.1.2",
          "form": "난 저녁 뭐먹을지 엄마랑 고민중이야",
          "original_form": "난 저녁 뭐먹을지 엄마랑 고민중이야",
          "speaker_id": "1",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100028302.1.3",
          "form": "헉 아직 안정했어?",
          "original_form": "헉 아직 안정했어?",
          "speaker_id": "2",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100028302.1.4",
          "form": "고기먹어 (?",
          "original_form": "고기먹어 (?",
          "speaker_id": "2",
          "time": "20210902 19:04"
        },
        {
          "id": "MDRW2100028302.1.5",
          "form": "아무래도 남은 스파게티 먹을거같다고 추정중",
          "original_form": "아무래도 남은 스파게티 먹을거같다고 추정중",
          "speaker_id": "1",
          "time": "20210902 19:07"
        },
        {
          "id": "MDRW2100028302.1.6",
          "form": "어떻게 스파게티가 남아있을수 있지",
          "original_form": "어떻게 스파게티가 남아있을수 있지",
          "speaker_id": "2",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100028302.1.7",
          "form": "면 안 불어?",
          "original_form": "면 안 불어?",
          "speaker_id": "2",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100028302.1.8",
          "form": "면 삶아놓고 냉장고행",
          "original_form": "면 삶아놓고 냉장고행",
          "speaker_id": "1",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100028302.1.9",
          "form": "오.... 냉장고 안에서도 불을수 있을거같은데 소스랑 분리해두는건가",
          "original_form": "오.... 냉장고 안에서도 불을수 있을거같은데 소스랑 분리해두는건가",
          "speaker_id": "2",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100028302.1.10",
          "form": "난 불어도 잘먹어서 괜찮아",
          "original_form": "난 불어도 잘먹어서 괜찮아",
          "speaker_id": "1",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100028302.1.11",
          "form": "맞아!",
          "original_form": "맞아!",
          "speaker_id": "1",
          "time": "20210902 19:08"
        },
        {
          "id": "MDRW2100028302.1.12",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100028302.1.13",
          "form": "잘먹는구나 요시요시",
          "original_form": "잘먹는구나 요시요시",
          "speaker_id": "2",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100028302.1.14",
          "form": "난 스파게티면도 좋지만 푸실리나 마카로니도 좋아",
          "original_form": "난 스파게티면도 좋지만 푸실리나 마카로니도 좋아",
          "speaker_id": "2",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100028302.1.15",
          "form": "나도 먹는 거 좋아해 할 말 짱많아",
          "original_form": "나도 먹는 거 좋아해 할 말 짱많아",
          "speaker_id": "1",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100028302.1.16",
          "form": "헐 나 마카로니 대박 좋아해",
          "original_form": "헐 나 마카로니 대박 좋아해",
          "speaker_id": "1",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100028302.1.17",
          "form": "관련 일화도 있어",
          "original_form": "관련 일화도 있어",
          "speaker_id": "1",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100028302.1.18",
          "form": "머???",
          "original_form": "머???",
          "speaker_id": "2",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100028302.1.19",
          "form": "안 들을 수 없다",
          "original_form": "안 들을 수 없다",
          "speaker_id": "2",
          "time": "20210902 19:09"
        },
        {
          "id": "MDRW2100028302.1.20",
          "form": "마카로니 마마카로니",
          "original_form": "마카로니 마마카로니",
          "speaker_id": "2",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028302.1.21",
          "form": "나 어릴적 유딩시절",
          "original_form": "나 어릴적 유딩시절",
          "speaker_id": "1",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028302.1.22",
          "form": "마카로니 러버",
          "original_form": "마카로니 러버",
          "speaker_id": "1",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028302.1.23",
          "form": "혼자 해먹고 싶었는데 방법을 모름",
          "original_form": "혼자 해먹고 싶었는데 방법을 모름",
          "speaker_id": "1",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028302.1.24",
          "form": "삶는걸 몰랐어",
          "original_form": "삶는걸 몰랐어",
          "speaker_id": "1",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028302.1.25",
          "form": "맥앤치즈같은거?",
          "original_form": "맥앤치즈같은거?",
          "speaker_id": "2",
          "time": "20210902 19:10"
        },
        {
          "id": "MDRW2100028302.1.26",
          "form": "아 설마",
          "original_form": "아 설마",
