      "utterance": [
        {
          "id": "MDRW2100009735.1.1",
          "form": "한번 봐보세요 진짜 너무 좋아요",
          "original_form": "한번 봐보세요 진짜 너무 좋아요",
          "speaker_id": "1",
          "time": "20210823 08:48"
        },
        {
          "id": "MDRW2100009735.1.2",
          "form": "편의점 가면 꼭 사는 물품이 있나요",
          "original_form": "편의점 가면 꼭 사는 물품이 있나요",
          "speaker_id": "2",
          "time": "20210823 08:50"
        },
        {
          "id": "MDRW2100009735.1.3",
          "form": "예전에는 맥주 많이 샀었는데 요즘에는 과자??",
          "original_form": "예전에는 맥주 많이 샀었는데 요즘에는 과자??",
          "speaker_id": "1",
          "time": "20210823 08:53"
        },
        {
          "id": "MDRW2100009735.1.4",
          "form": "name2님은 어떤거 사세요??",
          "original_form": "&name2&님은 어떤거 사세요??",
          "speaker_id": "1",
          "time": "20210823 08:54"
        },
        {
          "id": "MDRW2100009735.1.5",
          "form": "저는 아무래도 삼김 핫바 이런거요ㅎㅎ",
          "original_form": "저는 아무래도 삼김 핫바 이런거요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 08:54"
        },
        {
          "id": "MDRW2100009735.1.6",
          "form": "보통 출출 할 때 들어가니까",
          "original_form": "보통 출출 할 때 들어가니까",
          "speaker_id": "2",
          "time": "20210823 08:54"
        },
        {
          "id": "MDRW2100009735.1.7",
          "form": "과자 어떤거 주로 구매하시나요",
          "original_form": "과자 어떤거 주로 구매하시나요",
          "speaker_id": "2",
          "time": "20210823 08:55"
        },
        {
          "id": "MDRW2100009735.1.8",
          "form": "나이 드니까 편의점 음식은 허해서... ㅎㅎ",
          "original_form": "나이 드니까 편의점 음식은 허해서... ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 09:03"
        },
        {
          "id": "MDRW2100009735.1.9",
          "form": "가성비 좋은거 편의점에서 나온 과자나",
          "original_form": "가성비 좋은거 편의점에서 나온 과자나",
          "speaker_id": "1",
          "time": "20210823 09:03"
        },
        {
          "id": "MDRW2100009735.1.10",
          "form": "요즘에는 초코맛 꼬북칩!!",
          "original_form": "요즘에는 초코맛 꼬북칩!!",
          "speaker_id": "1",
          "time": "20210823 09:03"
        },
        {
          "id": "MDRW2100009735.1.11",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210823 09:04"
        },
        {
          "id": "MDRW2100009735.1.12",
          "form": "먹어도 먹어도 맛있습니다...",
          "original_form": "먹어도 먹어도 맛있습니다...",
          "speaker_id": "1",
          "time": "20210823 09:04"
        },
        {
          "id": "MDRW2100009735.1.13",
          "form": "그거 완전 맛있어",
          "original_form": "그거 완전 맛있어",
          "speaker_id": "2",
          "time": "20210823 09:04"
        },
        {
          "id": "MDRW2100009735.1.14",
          "form": "얼려드셔보세요",
          "original_form": "얼려드셔보세요",
          "speaker_id": "2",
          "time": "20210823 09:04"
        },
        {
          "id": "MDRW2100009735.1.15",
          "form": "더맛있어요",
          "original_form": "더맛있어요",
          "speaker_id": "2",
          "time": "20210823 09:04"
        },
        {
          "id": "MDRW2100009735.1.16",
          "form": "헐헐!!!",
          "original_form": "헐헐!!!",
          "speaker_id": "1",
          "time": "20210823 09:13"
        },
        {
          "id": "MDRW2100009735.1.17",
          "form": "개꿀팁 감사합니다 ㅎㅎ",
          "original_form": "개꿀팁 감사합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 09:13"
        },
        {
          "id": "MDRW2100009735.1.18",
          "form": "다른거 맛있는거 있으면 추천좀 해주세요 ㅎㅎ",
          "original_form": "다른거 맛있는거 있으면 추천좀 해주세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 09:13"
        },
        {
          "id": "MDRW2100009735.1.19",
          "form": "음 저는 무뚝뚝한 감자칩 이것도 자주 사는 편인데 드셔보신 적 있으신가요?",
          "original_form": "음 저는 무뚝뚝한 감자칩 이것도 자주 사는 편인데 드셔보신 적 있으신가요?",
          "speaker_id": "2",
          "time": "20210823 09:13"
        },
        {
          "id": "MDRW2100009735.1.20",
          "form": "감자칩이 무뚝뚝한게 있어요??",
          "original_form": "감자칩이 무뚝뚝한게 있어요??",
          "speaker_id": "1",
          "time": "20210823 09:17"
        },
        {
          "id": "MDRW2100009735.1.21",
          "form": "어 과자이름이 무뚝뚝한 감자칩인데",
          "original_form": "어 과자이름이 무뚝뚝한 감자칩인데",
          "speaker_id": "2",
          "time": "20210823 09:17"
        },
        {
          "id": "MDRW2100009735.1.22",
          "form": "포카칩처럼 쾌활한 애는 먹는데",
          "original_form": "포카칩처럼 쾌활한 애는 먹는데",
          "speaker_id": "1",
          "time": "20210823 09:17"
        },
        {
          "id": "MDRW2100009735.1.23",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 09:18"
        },
        {
          "id": "MDRW2100009735.1.24",
          "form": "유쾌하시네요",
          "original_form": "유쾌하시네요",
          "speaker_id": "2",
          "time": "20210823 09:18"
        },
        {
          "id": "MDRW2100009735.1.25",
          "form": "저건 좀 짜서 아 내가 오늘 짠거 먹고싶다 하면 저걸 드셔보세요",
