      "utterance": [
        {
          "id": "MDRW2100024848.1.1",
          "form": "무기화학 좋아하세요?",
          "original_form": "무기화학 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 19:38"
        },
        {
          "id": "MDRW2100024848.1.2",
          "form": "아니요..? 화학과이신가봐요",
          "original_form": "아니요..? 화학과이신가봐요",
          "speaker_id": "2",
          "time": "20210831 19:38"
        },
        {
          "id": "MDRW2100024848.1.3",
          "form": "무기징역의 무기입니다",
          "original_form": "무기징역의 무기입니다",
          "speaker_id": "2",
          "time": "20210831 19:38"
        },
        {
          "id": "MDRW2100024848.1.4",
          "form": "앗....",
          "original_form": "앗....",
          "speaker_id": "1",
          "time": "20210831 20:16"
        },
        {
          "id": "MDRW2100024848.1.5",
          "form": "죄송합니다",
          "original_form": "죄송합니다",
          "speaker_id": "1",
          "time": "20210831 20:16"
        },
        {
          "id": "MDRW2100024848.1.6",
          "form": "화학과지만 무기화학 저도 싫어해요ㅎㅎ",
          "original_form": "화학과지만 무기화학 저도 싫어해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 20:16"
        },
        {
          "id": "MDRW2100024848.1.7",
          "form": "오 저 화학과 갔다가 자퇴했어요 ㅋㅋㅋㅋㅋ",
          "original_form": "오 저 화학과 갔다가 자퇴했어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100024848.1.8",
          "form": "1학년 1학기에 관둔거라 유기화학 무기화학 생화학 ??? 뭐 이름만 들어봤네요",
          "original_form": "1학년 1학기에 관둔거라 유기화학 무기화학 생화학 ??? 뭐 이름만 들어봤네요",
          "speaker_id": "2",
          "time": "20210831 20:29"
        },
        {
          "id": "MDRW2100024848.1.9",
          "form": "헉 전 이제 막학기에요ㅜㅜ",
          "original_form": "헉 전 이제 막학기에요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100024848.1.10",
          "form": "같은 화학과셨다니 묘하게 반갑네요ㅋㅋ",
          "original_form": "같은 화학과셨다니 묘하게 반갑네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:11"
        },
        {
          "id": "MDRW2100024848.1.11",
          "form": "ㅋㅋㅋ금방 관뒀는걸요 뭐.. 전공살려서 일할 자신이없어서 자퇴했는데 동기들은 연구직 들어가서 돈 잘 벌드라구요 ㅎㅎ",
          "original_form": "ㅋㅋㅋ금방 관뒀는걸요 뭐.. 전공살려서 일할 자신이없어서 자퇴했는데 동기들은 연구직 들어가서 돈 잘 벌드라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 21:13"
        },
        {
          "id": "MDRW2100024848.1.12",
          "form": "가끔 버텨볼걸 그랬나 싶어요 ..ㅎ",
          "original_form": "가끔 버텨볼걸 그랬나 싶어요 ..ㅎ",
          "speaker_id": "2",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024848.1.13",
          "form": "많이 바쁘시겠어요 막학기면",
          "original_form": "많이 바쁘시겠어요 막학기면",
          "speaker_id": "2",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024848.1.14",
          "form": "저도 1 2학년때는 화학과 온거 후회했는데",
          "original_form": "저도 1 2학년때는 화학과 온거 후회했는데",
          "speaker_id": "1",
          "time": "20210831 21:14"
        },
        {
          "id": "MDRW2100024848.1.15",
          "form": "4학년되니까 괜찮더라고요 나름",
          "original_form": "4학년되니까 괜찮더라고요 나름",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024848.1.16",
          "form": "막학기되니까 정신이 괴로워요ㅋㅋ",
          "original_form": "막학기되니까 정신이 괴로워요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024848.1.17",
          "form": "졸업시험 같은거 보죠? 무슨 자격증 같은거도 따던데",
          "original_form": "졸업시험 같은거 보죠? 무슨 자격증 같은거도 따던데",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024848.1.18",
          "form": "대학원 진학할지 취업할지 고민도 될거같구.. 어떠세요?",
          "original_form": "대학원 진학할지 취업할지 고민도 될거같구.. 어떠세요?",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024848.1.19",
          "form": "전 졸업논문이요",
          "original_form": "전 졸업논문이요",
          "speaker_id": "1",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024848.1.20",
          "form": "대학원 진학하고싶어서 랩인턴을 했었는데",
          "original_form": "대학원 진학하고싶어서 랩인턴을 했었는데",
          "speaker_id": "1",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024848.1.21",
          "form": "이곳을 진학하지않게돼서",
          "original_form": "이곳을 진학하지않게돼서",
          "speaker_id": "1",
          "time": "20210831 21:19"
        },
        {
          "id": "MDRW2100024848.1.22",
          "form": "고민이 많아졌어요ㅜㅠ",
          "original_form": "고민이 많아졌어요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210831 21:19"
        },
        {
          "id": "MDRW2100024848.1.23",
          "form": "아이고 좋은 방향으로 길이 나셨음 좋겠네요",
          "original_form": "아이고 좋은 방향으로 길이 나셨음 좋겠네요",
          "speaker_id": "2",
          "time": "20210831 21:44"
        },
        {
          "id": "MDRW2100024848.1.24",
          "form": "어쩌다가 화학과로 진학하신거예요?",
          "original_form": "어쩌다가 화학과로 진학하신거예요?",
          "speaker_id": "2",
          "time": "20210831 21:46"
        },
        {
          "id": "MDRW2100024848.1.25",
          "form": "고딩때 화장품연구원이 하고싶었어요",
          "original_form": "고딩때 화장품연구원이 하고싶었어요",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100024848.1.26",
          "form": "지금은 반도체 소재쪽으로 관심이 생겨서요ㅋㅋ",
          "original_form": "지금은 반도체 소재쪽으로 관심이 생겨서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100024848.1.27",
          "form": "그래도 이번방학에 졸논 완성했답니다!!",
          "original_form": "그래도 이번방학에 졸논 완성했답니다!!",
          "speaker_id": "1",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100024848.1.28",
          "form": "오오 고생하셨어요ㅎㅎㅎ 반도체라니 멋있습니다.. 리스펙",
          "original_form": "오오 고생하셨어요ㅎㅎㅎ 반도체라니 멋있습니다.. 리스펙",
