      "utterance": [
        {
          "id": "MDRW2100015709.1.1",
          "form": "야식먹고싶은 시간이네요",
          "original_form": "야식먹고싶은 시간이네요",
          "speaker_id": "1",
          "time": "20210826 00:28"
        },
        {
          "id": "MDRW2100015709.1.2",
          "form": "맞아요 야식 좋죠 ㅋㅋㅋㅋ",
          "original_form": "맞아요 야식 좋죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:30"
        },
        {
          "id": "MDRW2100015709.1.3",
          "form": "야식 시키면 주로 뭐 드세요?",
          "original_form": "야식 시키면 주로 뭐 드세요?",
          "speaker_id": "1",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015709.1.4",
          "form": "음 주로 치킨 많이 먹는 것 같아요",
          "original_form": "음 주로 치킨 많이 먹는 것 같아요",
          "speaker_id": "2",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015709.1.5",
          "form": "간간히 김피탕??",
          "original_form": "간간히 김피탕??",
          "speaker_id": "2",
          "time": "20210826 00:31"
        },
        {
          "id": "MDRW2100015709.1.6",
          "form": "오 김피탕 맛있나요?",
          "original_form": "오 김피탕 맛있나요?",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015709.1.7",
          "form": "한번도 안먹어봤어요",
          "original_form": "한번도 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015709.1.8",
          "form": "아 진짜요??",
          "original_form": "아 진짜요??",
          "speaker_id": "2",
          "time": "20210826 00:32"
        },
        {
          "id": "MDRW2100015709.1.9",
          "form": "전 맛있어하는 편인데",
          "original_form": "전 맛있어하는 편인데",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015709.1.10",
          "form": "막 특별할건 없는 맛이예요",
          "original_form": "막 특별할건 없는 맛이예요",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015709.1.11",
          "form": "그냥 딱 생각하는 그대로의 맛 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그냥 딱 생각하는 그대로의 맛 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 00:33"
        },
        {
          "id": "MDRW2100015709.1.12",
          "form": "ㅋㅋㅋㅋ궁금해서 매번 먹어봐야지 해보고 한번도 못먹어봤어요",
          "original_form": "ㅋㅋㅋㅋ궁금해서 매번 먹어봐야지 해보고 한번도 못먹어봤어요",
          "speaker_id": "1",
          "time": "20210826 08:20"
        },
        {
          "id": "MDRW2100015709.1.13",
          "form": "ㅋㅋㅋㅋㅋ나중에 궁금하시면",
          "original_form": "ㅋㅋㅋㅋㅋ나중에 궁금하시면",
          "speaker_id": "2",
          "time": "20210826 08:26"
        },
        {
          "id": "MDRW2100015709.1.14",
          "form": "한 번쯤 시켜드세요!",
          "original_form": "한 번쯤 시켜드세요!",
          "speaker_id": "2",
          "time": "20210826 08:26"
        },
        {
          "id": "MDRW2100015709.1.15",
          "form": "그냥 평범한 맛이라 ㅋㅋㅋㅋㅋ 한 번 시도해보기엔 나쁘지않아요",
          "original_form": "그냥 평범한 맛이라 ㅋㅋㅋㅋㅋ 한 번 시도해보기엔 나쁘지않아요",
          "speaker_id": "2",
          "time": "20210826 08:26"
        },
        {
          "id": "MDRW2100015709.1.16",
          "form": "ㅋㅋㅋ한번 도전해봐야겠어요",
          "original_form": "ㅋㅋㅋ한번 도전해봐야겠어요",
          "speaker_id": "1",
          "time": "20210826 08:58"
        },
        {
          "id": "MDRW2100015709.1.17",
          "form": "못드시는 음식은 있으신가요??",
          "original_form": "못드시는 음식은 있으신가요??",
          "speaker_id": "2",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100015709.1.18",
          "form": "저는 간장게장 못먹어요ㅠㅠ갑각류 알러지 있어서",
          "original_form": "저는 간장게장 못먹어요ㅠㅠ갑각류 알러지 있어서",
          "speaker_id": "1",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100015709.1.19",
          "form": "답장이 많이 늦어졌네요 ㅠㅜ",
          "original_form": "답장이 많이 늦어졌네요 ㅠㅜ",
          "speaker_id": "2",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100015709.1.20",
          "form": "갑각류 알러지 있으시구나 ㅠㅠㅠㅠ그럼 껍질 다 제거해도 못드시는건가요..? ㅠㅠ",
          "original_form": "갑각류 알러지 있으시구나 ㅠㅠㅠㅠ그럼 껍질 다 제거해도 못드시는건가요..? ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 16:40"
        },
        {
          "id": "MDRW2100015709.1.21",
          "form": "익힌건 먹을 수 있어요!",
          "original_form": "익힌건 먹을 수 있어요!",
          "speaker_id": "1",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100015709.1.22",
          "form": "생새우나 간장게장같은거만 못먹어용",
          "original_form": "생새우나 간장게장같은거만 못먹어용",
          "speaker_id": "1",
          "time": "20210901 18:43"
        },
        {
          "id": "MDRW2100015709.1.23",
          "form": "아 익힌건 괜찮으시구나 ㅠㅠ",
          "original_form": "아 익힌건 괜찮으시구나 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100015709.1.24",
          "form": "저도 간장게장은 비려서 못먹어요...",
          "original_form": "저도 간장게장은 비려서 못먹어요...",
          "speaker_id": "2",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100015709.1.25",
          "form": "홈쇼핑광고보면 그렇게 맛있어보이는데...",
          "original_form": "홈쇼핑광고보면 그렇게 맛있어보이는데...",
          "speaker_id": "2",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100015709.1.26",
          "form": "어우 그분들은 대체 어떻게 그렇게 많이 먹는지",
          "original_form": "어우 그분들은 대체 어떻게 그렇게 많이 먹는지",
          "speaker_id": "1",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100015709.1.27",
          "form": "오바..",
          "original_form": "오바..",
          "speaker_id": "1",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100015709.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그러니까욬ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그러니까욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100015709.1.29",
          "form": "홈쇼핑 광고하시는 분들 너무 맛있게 잘드세요...",
          "original_form": "홈쇼핑 광고하시는 분들 너무 맛있게 잘드세요...",
          "speaker_id": "2",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100015709.1.30",
          "form": "뭐다른음식 좋아하는거 있으세요?",
          "original_form": "뭐다른음식 좋아하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100015709.1.31",
          "form": "담양 숯불갈비 드셔보셨어요? 맛있어요...",
          "original_form": "담양 숯불갈비 드셔보셨어요? 맛있어요...",
          "speaker_id": "2",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100015709.1.32",
          "form": "헐 아녀 안먹어봤어요",
          "original_form": "헐 아녀 안먹어봤어요",
          "speaker_id": "1",
          "time": "20210905 23:48"
        }
