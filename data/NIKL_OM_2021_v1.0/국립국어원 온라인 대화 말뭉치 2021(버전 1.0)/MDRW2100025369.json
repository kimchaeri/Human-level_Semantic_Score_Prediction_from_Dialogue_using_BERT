          "speaker_id": "1",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025369.1.29",
          "form": "그쪽이세요?",
          "original_form": "그쪽이세요?",
          "speaker_id": "1",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025369.1.30",
          "form": "취향 알았고",
          "original_form": "취향 알았고",
          "speaker_id": "1",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025369.1.31",
          "form": "고3이라 재밌었떤듯",
          "original_form": "고3이라 재밌었떤듯",
          "speaker_id": "2",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025369.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ과연",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ과연",
          "speaker_id": "1",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025369.1.33",
          "form": "본거 많으시네요",
          "original_form": "본거 많으시네요",
          "speaker_id": "1",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025369.1.34",
          "form": "거기주인공 이름이 제이름이랑 똑같아서",
          "original_form": "거기주인공 이름이 제이름이랑 똑같아서",
          "speaker_id": "2",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025369.1.35",
          "form": "감정이입제대로함",
          "original_form": "감정이입제대로함",
          "speaker_id": "2",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025369.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025369.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025369.1.38",
          "form": "레전드..",
          "original_form": "레전드..",
          "speaker_id": "1",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025369.1.39",
          "form": "이기적 유전자 알아요?",
          "original_form": "이기적 유전자 알아요?",
          "speaker_id": "2",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025369.1.40",
          "form": "넘나어려운책,,",
          "original_form": "넘나어려운책,,",
          "speaker_id": "2",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025369.1.41",
          "form": "머죠",
          "original_form": "머죠",
          "speaker_id": "1",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025369.1.42",
          "form": "생물동아리에서 선물받은책,,",
          "original_form": "생물동아리에서 선물받은책,,",
          "speaker_id": "2",
          "time": "20210901 10:23"
        },
        {
          "id": "MDRW2100025369.1.43",
          "form": "이기적인 유전자가 살아남는다",
          "original_form": "이기적인 유전자가 살아남는다",
          "speaker_id": "1",
          "time": "20210901 10:24"
        },
        {
          "id": "MDRW2100025369.1.44",
          "form": "머 그런건가요",
          "original_form": "머 그런건가요",
          "speaker_id": "1",
          "time": "20210901 10:24"
        },
        {
          "id": "MDRW2100025369.1.45",
          "form": "두장읽고 안읽어서 모르겟네요 ㅋㅋㅋ",
          "original_form": "두장읽고 안읽어서 모르겟네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:24"
        },
        {
          "id": "MDRW2100025369.1.46",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:24"
        },
        {
          "id": "MDRW2100025369.1.47",
          "form": "좋은 냄비받침이었다",
          "original_form": "좋은 냄비받침이었다",
          "speaker_id": "1",
          "time": "20210901 10:24"
        },
        {
          "id": "MDRW2100025369.1.48",
          "form": "당근에 팔았움 ㅎㅎㅎㅎ",
          "original_form": "당근에 팔았움 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100025369.1.49",
          "form": "오 현명ㅋㅋㅋㅋ",
          "original_form": "오 현명ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100025369.1.50",
          "form": "커피관련책도 있었는데 마케팅?하고",
          "original_form": "커피관련책도 있었는데 마케팅?하고",
          "speaker_id": "2",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100025369.1.51",
          "form": "싹다팔아버림",
          "original_form": "싹다팔아버림",
          "speaker_id": "2",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100025369.1.52",
          "form": "ㅋㅋㅋㅋ잘파시네",
          "original_form": "ㅋㅋㅋㅋ잘파시네",
          "speaker_id": "1",
          "time": "20210901 10:26"
        },
        {
          "id": "MDRW2100025369.1.53",
          "form": "아깝자나요 어차피 안볼책들 ㅋㅋㅋ다팔아버려요",
          "original_form": "아깝자나요 어차피 안볼책들 ㅋㅋㅋ다팔아버려요",
          "speaker_id": "2",
          "time": "20210901 10:26"
        },
        {
          "id": "MDRW2100025369.1.54",
          "form": "근데 아까 수필 재밋다는건 읽어보고싶네요",
          "original_form": "근데 아까 수필 재밋다는건 읽어보고싶네요",
          "speaker_id": "2",
          "time": "20210901 10:27"
        },
        {
          "id": "MDRW2100025369.1.55",
          "form": "ㅋㅋㅋ잘팔려요?",
          "original_form": "ㅋㅋㅋ잘팔려요?",
          "speaker_id": "1",
          "time": "20210901 10:27"
        },
        {
          "id": "MDRW2100025369.1.56",
          "form": "넹 그때 두권에 만사천인가햇던듯",
          "original_form": "넹 그때 두권에 만사천인가햇던듯",
          "speaker_id": "2",
          "time": "20210901 10:27"
        },
        {
          "id": "MDRW2100025369.1.57",
          "form": "저 들고 있는데 빌려드릴까요",
          "original_form": "저 들고 있는데 빌려드릴까요",
          "speaker_id": "1",
          "time": "20210901 10:27"
        },
        {
          "id": "MDRW2100025369.1.58",
          "form": "와 비싸게 팔리네",
          "original_form": "와 비싸게 팔리네",
          "speaker_id": "1",
          "time": "20210901 10:27"
        },
        {
          "id": "MDRW2100025369.1.59",
          "form": "커피책이 25000짜리엿음",
          "original_form": "커피책이 25000짜리엿음",
          "speaker_id": "2",
          "time": "20210901 10:27"
        },
        {
          "id": "MDRW2100025369.1.60",
          "form": "한번도안읽은거라 ㅋㅋㅋㅋㅋ",
          "original_form": "한번도안읽은거라 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:27"
        },
        {
          "id": "MDRW2100025369.1.61",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025369.1.62",
          "form": "뭔데 글케비쌈",
          "original_form": "뭔데 글케비쌈",
          "speaker_id": "1",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025369.1.63",
          "form": "전공책인가",
          "original_form": "전공책인가",
          "speaker_id": "1",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025369.1.64",
          "form": "뭐 여기서 직원복지라고 해가지고",
          "original_form": "뭐 여기서 직원복지라고 해가지고",
          "speaker_id": "2",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025369.1.65",
          "form": "읽고싶은책 사주겟다고 ㅡㅡㅋㅋㅋ",
          "original_form": "읽고싶은책 사주겟다고 ㅡㅡㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:28"
        },
        {
          "id": "MDRW2100025369.1.66",
          "form": "아뇨 무슨 커피원두부터시작해서 책좀두꺼웠음",
