        },
        {
          "id": "MDRW2100025369.1.2",
          "form": "종이인형이였나",
          "original_form": "종이인형이였나",
          "speaker_id": "1",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100025369.1.3",
          "form": "암튼 왜요?",
          "original_form": "암튼 왜요?",
          "speaker_id": "1",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100025369.1.4",
          "form": "궁금해서요",
          "original_form": "궁금해서요",
          "speaker_id": "2",
          "time": "20210901 10:18"
        },
        {
          "id": "MDRW2100025369.1.5",
          "form": "스토리?",
          "original_form": "스토리?",
          "speaker_id": "1",
          "time": "20210901 10:18"
        },
        {
          "id": "MDRW2100025369.1.6",
          "form": "넹 ㄱㄱ",
          "original_form": "넹 ㄱㄱ",
          "speaker_id": "2",
          "time": "20210901 10:18"
        },
        {
          "id": "MDRW2100025369.1.7",
          "form": "제대로 기억은 안나는데",
          "original_form": "제대로 기억은 안나는데",
          "speaker_id": "1",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100025369.1.8",
          "form": "남주가 소설가였나 뭐 그런 사람인데 갑자기 종이에서 여자가 튀어나와가지고",
          "original_form": "남주가 소설가였나 뭐 그런 사람인데 갑자기 종이에서 여자가 튀어나와가지고",
          "speaker_id": "1",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025369.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025369.1.10",
          "form": "썸타고 끌려다니고 이리저리 다니다가",
          "original_form": "썸타고 끌려다니고 이리저리 다니다가",
          "speaker_id": "1",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025369.1.11",
          "form": "벌써재밌음",
          "original_form": "벌써재밌음",
          "speaker_id": "2",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025369.1.12",
          "form": "상상인줄 알았는데 아? 현실이었다??",
          "original_form": "상상인줄 알았는데 아? 현실이었다??",
          "speaker_id": "1",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025369.1.13",
          "form": "뭐 여자는 아팠나 그랬고",
          "original_form": "뭐 여자는 아팠나 그랬고",
          "speaker_id": "1",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025369.1.14",
          "form": "아 W 알아요? 드라마",
          "original_form": "아 W 알아요? 드라마",
          "speaker_id": "2",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025369.1.15",
          "form": "비슷하네",
          "original_form": "비슷하네",
          "speaker_id": "2",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025369.1.16",
          "form": "들어는 봄",
          "original_form": "들어는 봄",
          "speaker_id": "1",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025369.1.17",
          "form": "뭐야 카피했나",
          "original_form": "뭐야 카피했나",
          "speaker_id": "1",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025369.1.18",
          "form": "w도 졸잼임",
          "original_form": "w도 졸잼임",
          "speaker_id": "2",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025369.1.19",
          "form": "저 고3때 도서관에서 책 하두많이빌려서 도서상탓는데",
          "original_form": "저 고3때 도서관에서 책 하두많이빌려서 도서상탓는데",
          "speaker_id": "2",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025369.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025369.1.21",
          "form": "와",
