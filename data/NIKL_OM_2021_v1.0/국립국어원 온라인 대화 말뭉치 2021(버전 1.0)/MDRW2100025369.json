          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025369.1.22",
          "form": "대박ㅋㅋㅋㅋ",
          "original_form": "대박ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025369.1.23",
          "form": "고수셨네",
          "original_form": "고수셨네",
          "speaker_id": "1",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025369.1.24",
          "form": "북촌 꽃 선비의 연인들 있거든요 소설책",
          "original_form": "북촌 꽃 선비의 연인들 있거든요 소설책",
          "speaker_id": "2",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025369.1.25",
          "form": "겁나야,함",
          "original_form": "겁나야,함",
          "speaker_id": "2",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025369.1.26",
          "form": "죄송합니다",
          "original_form": "죄송합니다",
          "speaker_id": "1",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025369.1.27",
