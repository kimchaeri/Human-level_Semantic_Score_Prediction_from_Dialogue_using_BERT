      "utterance": [
        {
          "id": "MDRW2100025514.1.1",
          "form": "언니 점심 언제 드세요~?",
          "original_form": "언니 점심 언제 드세요~?",
          "speaker_id": "2",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100025514.1.2",
          "form": "한 십분뒤요. hplc  끝나면요",
          "original_form": "한 십분뒤요. hplc  끝나면요",
          "speaker_id": "1",
          "time": "20210901 12:29"
        },
        {
          "id": "MDRW2100025514.1.3",
          "form": "오호라",
          "original_form": "오호라",
          "speaker_id": "2",
          "time": "20210901 12:32"
        },
        {
          "id": "MDRW2100025514.1.4",
          "form": "오늘 점심 메뉴는 뭔가요~~~",
          "original_form": "오늘 점심 메뉴는 뭔가요~~~",
          "speaker_id": "2",
          "time": "20210901 12:32"
        },
        {
          "id": "MDRW2100025514.1.5",
          "form": "김치전이요",
          "original_form": "김치전이요",
          "speaker_id": "1",
          "time": "20210901 12:35"
        },
        {
          "id": "MDRW2100025514.1.6",
          "form": "쬐금 맛보실래요?",
          "original_form": "쬐금 맛보실래요?",
          "speaker_id": "1",
          "time": "20210901 12:35"
        },
        {
          "id": "MDRW2100025514.1.7",
          "form": "헐 대박 네네",
          "original_form": "헐 대박 네네",
          "speaker_id": "2",
          "time": "20210901 12:35"
        },
        {
          "id": "MDRW2100025514.1.8",
          "form": "조금 타긴했는데 ㅎㅎ",
          "original_form": "조금 타긴했는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 12:35"
        },
        {
          "id": "MDRW2100025514.1.9",
          "form": "어제 우리가 얘기했던... 바로 그 김치전!!!!",
          "original_form": "어제 우리가 얘기했던... 바로 그 김치전!!!!",
          "speaker_id": "2",
          "time": "20210901 12:35"
        },
        {
          "id": "MDRW2100025514.1.10",
          "form": "원래 좀 타야 바삭하고 맛있어요 (?)",
          "original_form": "원래 좀 타야 바삭하고 맛있어요 (?)",
          "speaker_id": "2",
          "time": "20210901 12:35"
        },
        {
          "id": "MDRW2100025514.1.11",
          "form": "잘 잘라내고 드세요",
          "original_form": "잘 잘라내고 드세요",
          "speaker_id": "1",
          "time": "20210901 12:36"
        },
        {
          "id": "MDRW2100025514.1.12",
          "form": "조금 쓰더라구요 ㅎㅎ",
          "original_form": "조금 쓰더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 12:36"
        },
        {
          "id": "MDRW2100025514.1.13",
          "form": "히히 좋습니다 ",
          "original_form": "히히 좋습니다 ",
          "speaker_id": "2",
          "time": "20210901 12:36"
        },
        {
          "id": "MDRW2100025514.1.14",
          "form": "저는 지금 액상과당을 섭취 중이에요...",
          "original_form": "저는 지금 액상과당을 섭취 중이에요...",
          "speaker_id": "2",
          "time": "20210901 12:37"
        },
        {
          "id": "MDRW2100025514.1.15",
          "form": "김 빠진 콜라....",
          "original_form": "김 빠진 콜라....",
          "speaker_id": "2",
          "time": "20210901 12:37"
        },
        {
          "id": "MDRW2100025514.1.16",
          "form": "많이 드세요. 그거 2병인데",
          "original_form": "많이 드세요. 그거 2병인데",
          "speaker_id": "1",
          "time": "20210901 12:39"
        },
        {
          "id": "MDRW2100025514.1.17",
          "form": "네 옛날거부터",
          "original_form": "네 옛날거부터",
          "speaker_id": "2",
          "time": "20210901 12:39"
        },
        {
          "id": "MDRW2100025514.1.18",
          "form": "쫌쫌따리 마시는 중이에요,,,",
          "original_form": "쫌쫌따리 마시는 중이에요,,,",
          "speaker_id": "2",
          "time": "20210901 12:39"
        },
        {
          "id": "MDRW2100025514.1.19",
          "form": "김치전이랑 어울릴지도 모르겠네요",
          "original_form": "김치전이랑 어울릴지도 모르겠네요",
          "speaker_id": "1",
          "time": "20210901 12:40"
        },
        {
          "id": "MDRW2100025514.1.20",
          "form": "그러게요!!!! 신난다~~~",
          "original_form": "그러게요!!!! 신난다~~~",
          "speaker_id": "2",
          "time": "20210901 12:44"
        },
        {
          "id": "MDRW2100025514.1.21",
          "form": "생각보다 금방 만들어졌다니 신기해요~~",
          "original_form": "생각보다 금방 만들어졌다니 신기해요~~",
          "speaker_id": "2",
          "time": "20210901 12:45"
        },
        {
          "id": "MDRW2100025514.1.22",
          "form": "대충하면 금방되더라구요",
          "original_form": "대충하면 금방되더라구요",
          "speaker_id": "1",
          "time": "20210901 12:47"
        },
        {
          "id": "MDRW2100025514.1.23",
          "form": "그리고 김치가 맛있으니까 대충해도 먹을만해요",
          "original_form": "그리고 김치가 맛있으니까 대충해도 먹을만해요",
