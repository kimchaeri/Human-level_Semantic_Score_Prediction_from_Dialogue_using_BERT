        },
        {
          "id": "MDRW2100015793.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 01:11"
        },
        {
          "id": "MDRW2100015793.1.3",
          "form": "가족 몇명과 사세요?",
          "original_form": "가족 몇명과 사세요?",
          "speaker_id": "1",
          "time": "20210826 01:11"
        },
        {
          "id": "MDRW2100015793.1.4",
          "form": "가족과 사세여?",
          "original_form": "가족과 사세여?",
          "speaker_id": "2",
          "time": "20210826 01:12"
        },
        {
          "id": "MDRW2100015793.1.5",
          "form": "전 혼자 살아요",
          "original_form": "전 혼자 살아요",
          "speaker_id": "2",
          "time": "20210826 01:12"
        },
        {
          "id": "MDRW2100015793.1.6",
          "form": "아아 혼자사시는구나",
          "original_form": "아아 혼자사시는구나",
          "speaker_id": "1",
          "time": "20210826 01:13"
        },
        {
          "id": "MDRW2100015793.1.7",
          "form": "네.. 누구랑 사세요?",
          "original_form": "네.. 누구랑 사세요?",
          "speaker_id": "2",
          "time": "20210826 01:14"
        },
        {
          "id": "MDRW2100015793.1.8",
          "form": "부모님이랑 동생이랑 살아요!",
          "original_form": "부모님이랑 동생이랑 살아요!",
          "speaker_id": "1",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015793.1.9",
          "form": "혼자사시면 집안일 많이하시겟어요",
          "original_form": "혼자사시면 집안일 많이하시겟어요",
          "speaker_id": "1",
          "time": "20210826 01:15"
        },
        {
          "id": "MDRW2100015793.1.10",
          "form": "가볍게 청소 정도죠 뭐...",
          "original_form": "가볍게 청소 정도죠 뭐...",
          "speaker_id": "2",
          "time": "20210826 01:16"
        },
        {
          "id": "MDRW2100015793.1.11",
          "form": "부모님이 다 해주시나용?",
          "original_form": "부모님이 다 해주시나용?",
          "speaker_id": "2",
          "time": "20210826 01:16"
        },
        {
          "id": "MDRW2100015793.1.12",
          "form": "음 그냥 자연스럽게 나눠서하긴해요!",
          "original_form": "음 그냥 자연스럽게 나눠서하긴해요!",
          "speaker_id": "1",
          "time": "20210826 01:17"
        },
        {
          "id": "MDRW2100015793.1.13",
          "form": "설거지나 청소기돌리기 같이 그때그때",
          "original_form": "설거지나 청소기돌리기 같이 그때그때",
          "speaker_id": "1",
          "time": "20210826 01:18"
        },
        {
          "id": "MDRW2100015793.1.14",
          "form": "전 부모님이랑 살때 한개도 안했는데\n.ㅋㅋㅋㅋ",
          "original_form": "전 부모님이랑 살때 한개도 안했는데\n.ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100015793.1.15",
          "form": "엄청 효자시네요..",
          "original_form": "엄청 효자시네요..",
          "speaker_id": "2",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100015793.1.16",
          "form": "가족이랑 살면 안 불편한가요?",
          "original_form": "가족이랑 살면 안 불편한가요?",
          "speaker_id": "2",
          "time": "20210826 01:21"
        },
        {
          "id": "MDRW2100015793.1.17",
          "form": "저도 원래는 잘 안햇어요..ㅋㅋㅋㅋ",
          "original_form": "저도 원래는 잘 안햇어요..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:22"
        },
        {
          "id": "MDRW2100015793.1.18",
          "form": "나이들다보니 약간 눈치보여서 ㅋㅋㅋㅋ",
          "original_form": "나이들다보니 약간 눈치보여서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:22"
        },
        {
          "id": "MDRW2100015793.1.19",
          "form": "저녁때 나서서 설거지도 하고 그러세요?",
          "original_form": "저녁때 나서서 설거지도 하고 그러세요?",
          "speaker_id": "2",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015793.1.20",
          "form": "약간 불편해요 ㅠ 가능하면 독립하고싶네요 ㅠ",
          "original_form": "약간 불편해요 ㅠ 가능하면 독립하고싶네요 ㅠ",
          "speaker_id": "1",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015793.1.21",
          "form": "뭐 잘못했을때 말공.. ㅎㅎ",
          "original_form": "뭐 잘못했을때 말공.. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015793.1.22",
          "form": "근데 주로 엄마가 먼저 하는편이라...음",
          "original_form": "근데 주로 엄마가 먼저 하는편이라...음",
          "speaker_id": "1",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015793.1.23",
          "form": "쌓여있는거 제가먼저 보면 하는편이에요",
          "original_form": "쌓여있는거 제가먼저 보면 하는편이에요",
          "speaker_id": "1",
          "time": "20210826 01:23"
        }
      ]
