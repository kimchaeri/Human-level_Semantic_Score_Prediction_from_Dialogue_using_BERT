      "utterance": [
        {
          "id": "MDRW2100043259.1.1",
          "form": "카톡에서 하는 게임도 ㅋㅋㅋ 진짜 열심히 했는데",
          "original_form": "카톡에서 하는 게임도 ㅋㅋㅋ 진짜 열심히 했는데",
          "speaker_id": "2",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100043259.1.2",
          "form": "경쟁하면서",
          "original_form": "경쟁하면서",
          "speaker_id": "2",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100043259.1.3",
          "form": "꿈의 집 말고 하신 게임 있으세여??",
          "original_form": "꿈의 집 말고 하신 게임 있으세여??",
          "speaker_id": "1",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100043259.1.4",
          "form": "무슨팡..",
          "original_form": "무슨팡..",
          "speaker_id": "2",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100043259.1.5",
          "form": "ㅋㅋㅋ 저 모두의 마블",
          "original_form": "ㅋㅋㅋ 저 모두의 마블",
          "speaker_id": "1",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100043259.1.6",
          "form": "지금??하는거요>?",
          "original_form": "지금??하는거요>?",
          "speaker_id": "2",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100043259.1.7",
          "form": "그것도 꽤 했져 애니팡ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그것도 꽤 했져 애니팡ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100043259.1.8",
          "form": "맞다맞다 애니팡",
          "original_form": "맞다맞다 애니팡",
          "speaker_id": "2",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100043259.1.9",
          "form": "점점 경쟁자가 사라지면서",
          "original_form": "점점 경쟁자가 사라지면서",
          "speaker_id": "2",
          "time": "20210907 16:31"
        },
        {
          "id": "MDRW2100043259.1.10",
          "form": "재미없어서 관두고",
          "original_form": "재미없어서 관두고",
          "speaker_id": "2",
          "time": "20210907 16:32"
        },
        {
          "id": "MDRW2100043259.1.11",
          "form": "그것도 누가쫌 따라와주고 해야되는데 ㅎㅎ",
          "original_form": "그것도 누가쫌 따라와주고 해야되는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 16:32"
        },
        {
          "id": "MDRW2100043259.1.12",
          "form": "ㅋㅋㅋㅋ 열심히 하셨군요",
          "original_form": "ㅋㅋㅋㅋ 열심히 하셨군요",
          "speaker_id": "1",
          "time": "20210907 16:32"
        },
        {
          "id": "MDRW2100043259.1.13",
          "form": "저 레스토랑 운영하는거였나",
          "original_form": "저 레스토랑 운영하는거였나",
          "speaker_id": "1",
          "time": "20210907 16:32"
        },
        {
          "id": "MDRW2100043259.1.14",
          "form": "두근두근 레스토랑인가 그것도 했었어요",
          "original_form": "두근두근 레스토랑인가 그것도 했었어요",
          "speaker_id": "1",
          "time": "20210907 16:32"
        },
        {
          "id": "MDRW2100043259.1.15",
          "form": "그건 안해본것 같은데!",
          "original_form": "그건 안해본것 같은데!",
          "speaker_id": "2",
          "time": "20210907 16:37"
        },
        {
          "id": "MDRW2100043259.1.16",
          "form": "친구들이랑 맨날 재료 주고 받고",
          "original_form": "친구들이랑 맨날 재료 주고 받고",
          "speaker_id": "1",
          "time": "20210907 16:37"
        },
        {
          "id": "MDRW2100043259.1.17",
          "form": "대학친구들이랑 했네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "대학친구들이랑 했네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:38"
        },
        {
          "id": "MDRW2100043259.1.18",
          "form": "그거때문에 연락은 해도 실제로 연락안하고 ㅋㅋㅋㅋㅋ",
          "original_form": "그거때문에 연락은 해도 실제로 연락안하고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:38"
        },
        {
          "id": "MDRW2100043259.1.19",
          "form": "ㅋㅋㅋ 저도 그런친구있었는데 게임친구 진짜 친구인데 사이버친구인듯..ㅋㅋ",
          "original_form": "ㅋㅋㅋ 저도 그런친구있었는데 게임친구 진짜 친구인데 사이버친구인듯..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100043259.1.20",
          "form": "ㅋㅋㅋ 그랬어요 뭐 내놔 하면서 연락만 했어욬ㅋㅋ",
          "original_form": "ㅋㅋㅋ 그랬어요 뭐 내놔 하면서 연락만 했어욬ㅋㅋ",
