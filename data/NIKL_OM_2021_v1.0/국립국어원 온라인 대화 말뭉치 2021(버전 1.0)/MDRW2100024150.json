      "utterance": [
        {
          "id": "MDRW2100024150.1.1",
          "form": "헤헤",
          "original_form": "헤헤",
          "speaker_id": "2",
          "time": "20210830 22:41"
        },
        {
          "id": "MDRW2100024150.1.2",
          "form": "저 서강준 나온 작품도 뭐 본거 같은데",
          "original_form": "저 서강준 나온 작품도 뭐 본거 같은데",
          "speaker_id": "2",
          "time": "20210830 22:41"
        },
        {
          "id": "MDRW2100024150.1.3",
          "form": "기억이 잘안나요",
          "original_form": "기억이 잘안나요",
          "speaker_id": "2",
          "time": "20210830 22:41"
        },
        {
          "id": "MDRW2100024150.1.4",
          "form": "서강준은 진짜 잘생겻는데",
          "original_form": "서강준은 진짜 잘생겻는데",
          "speaker_id": "1",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100024150.1.5",
          "form": "대표 작품이 뭐죠?",
          "original_form": "대표 작품이 뭐죠?",
          "speaker_id": "2",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100024150.1.6",
          "form": "아직 흥행한 대표 작품 뭐 없지 않나요",
          "original_form": "아직 흥행한 대표 작품 뭐 없지 않나요",
          "speaker_id": "1",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100024150.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그래서 기억이 안나는건가",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그래서 기억이 안나는건가",
          "speaker_id": "2",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100024150.1.8",
          "form": "뭐 사이보그로 한번",
          "original_form": "뭐 사이보그로 한번",
          "speaker_id": "1",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100024150.1.9",
          "form": "그럼 그냥 SNS 같은데에서 남친짤로 봤나봐요",
          "original_form": "그럼 그냥 SNS 같은데에서 남친짤로 봤나봐요",
          "speaker_id": "2",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100024150.1.10",
          "form": "나왔던거 같은데",
          "original_form": "나왔던거 같은데",
          "speaker_id": "1",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100024150.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100024150.1.12",
          "form": "검색해봤는데 뭐 없네",
          "original_form": "검색해봤는데 뭐 없네",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024150.1.13",
          "form": "서강준 얼굴로 사는 느낌 궁금해요..",
          "original_form": "서강준 얼굴로 사는 느낌 궁금해요..",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024150.1.14",
          "form": "아 서강준 남주혁 둘다 왜 잘생겼어",
          "original_form": "아 서강준 남주혁 둘다 왜 잘생겼어",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024150.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024150.1.16",
          "form": "혹시 그 영화 보셨어요?",
          "original_form": "혹시 그 영화 보셨어요?",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024150.1.17",
          "form": "뭐요?",
          "original_form": "뭐요?",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024150.1.18",
          "form": "너의 결혼식....?ㅠㅠㅠ",
          "original_form": "너의 결혼식....?ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024150.1.19",
          "form": "아 봤죠",
          "original_form": "아 봤죠",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024150.1.20",
          "form": "어디서 보셨어요?",
          "original_form": "어디서 보셨어요?",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024150.1.21",
          "form": "김영광이랑 박보영",
          "original_form": "김영광이랑 박보영",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024150.1.22",
          "form": "그 어디지",
          "original_form": "그 어디지",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024150.1.23",
          "form": "넷플릭스 아니면 왓챠",
          "original_form": "넷플릭스 아니면 왓챠",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024150.1.24",
          "form": "저 그거 영화관에서 보다가 펑펑 울었어요",
          "original_form": "저 그거 영화관에서 보다가 펑펑 울었어요",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024150.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100024150.1.26",
          "form": "망한 첫사랑 얘기 늘 슬퍼요 ㅋㅋㅋㅋ",
          "original_form": "망한 첫사랑 얘기 늘 슬퍼요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024150.1.27",
          "form": "진짜 마지막의 그녀의 결혼식에 김영광 있는데",
          "original_form": "진짜 마지막의 그녀의 결혼식에 김영광 있는데",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024150.1.28",
          "form": "그때서야 영화제목의 뜻을 깨달은거에요",
          "original_form": "그때서야 영화제목의 뜻을 깨달은거에요",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024150.1.29",
          "form": "그래서 너무 슬퍼서 울었오요...",
          "original_form": "그래서 너무 슬퍼서 울었오요...",
          "speaker_id": "2",
          "time": "20210830 22:44"
        },
        {
          "id": "MDRW2100024150.1.30",
          "form": "아니 해피엔딩일 줄 알고 방심한 내 잘못이지 모",
          "original_form": "아니 해피엔딩일 줄 알고 방심한 내 잘못이지 모",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024150.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024150.1.32",
          "form": "그니까요 당황",
          "original_form": "그니까요 당황",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024150.1.33",
          "form": "김영광은 요새 활동 안하나요",
          "original_form": "김영광은 요새 활동 안하나요",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024150.1.34",
          "form": "너의 결혼식이지 우리의 결혼식이 아니었어..",
          "original_form": "너의 결혼식이지 우리의 결혼식이 아니었어..",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024150.1.35",
          "form": "모르겠어요 따로 찾아보는 배우는 아니라 ㅋㅋㅋㅋ",
          "original_form": "모르겠어요 따로 찾아보는 배우는 아니라 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024150.1.36",
          "form": "어우 너무 잘생겼어",
          "original_form": "어우 너무 잘생겼어",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024150.1.37",
          "form": "이 배우도 서강준처럼 흥행작이 없네요..",
          "original_form": "이 배우도 서강준처럼 흥행작이 없네요..",
          "speaker_id": "2",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024150.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100024150.1.39",
          "form": "너의 결혼식이 그나마",
          "original_form": "너의 결혼식이 그나마",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100024150.1.40",
          "form": "흥행작 아니에요?",
          "original_form": "흥행작 아니에요?",
          "speaker_id": "1",
          "time": "20210830 22:46"
        },
        {
          "id": "MDRW2100024150.1.41",
          "form": "맞아요 ㅜㅜ 그거 말곤 없네요 힝",
          "original_form": "맞아요 ㅜㅜ 그거 말곤 없네요 힝",
          "speaker_id": "2",
          "time": "20210830 22:46"
        }
      ]
