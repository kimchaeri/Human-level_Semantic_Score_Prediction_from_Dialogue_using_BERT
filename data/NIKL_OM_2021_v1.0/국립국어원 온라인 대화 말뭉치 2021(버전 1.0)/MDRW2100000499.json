      "utterance": [
        {
          "id": "MDRW2100000499.1.1",
          "form": "9월4일 합천캠핑가는거  해인사들어가야된다는데",
          "original_form": "9월4일 합천캠핑가는거  해인사들어가야된다는데",
          "speaker_id": "1",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000499.1.2",
          "form": "해인사 안에 있는 거야?",
          "original_form": "해인사 안에 있는 거야?",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000499.1.3",
          "form": "응 그런데 해인사 입장료가있어",
          "original_form": "응 그런데 해인사 입장료가있어",
          "speaker_id": "1",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000499.1.4",
          "form": "아 맞다 근데 그렇게 비싼거 아니지 않아?",
          "original_form": "아 맞다 근데 그렇게 비싼거 아니지 않아?",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000499.1.5",
          "form": "알아봐야되 비싸지는 않을것같아",
          "original_form": "알아봐야되 비싸지는 않을것같아",
          "speaker_id": "1",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000499.1.6",
          "form": "찾아보고 알려줘",
          "original_form": "찾아보고 알려줘",
          "speaker_id": "2",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000499.1.7",
          "form": "먹을껀 뭐사가지?",
          "original_form": "먹을껀 뭐사가지?",
          "speaker_id": "2",
          "time": "20210819 16:31"
        },
        {
          "id": "MDRW2100000499.1.8",
          "form": "응 해인사 봄에 가면 벚꽃길이쁜다던데",
          "original_form": "응 해인사 봄에 가면 벚꽃길이쁜다던데",
          "speaker_id": "1",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000499.1.9",
          "form": "많이 챙겨오지 말라셨어",
          "original_form": "많이 챙겨오지 말라셨어",
          "speaker_id": "1",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000499.1.10",
          "form": "저번 캠핑때 등심이 더낮더라",
          "original_form": "저번 캠핑때 등심이 더낮더라",
          "speaker_id": "1",
          "time": "20210819 16:32"
        },
        {
          "id": "MDRW2100000499.1.11",
          "form": "응 그래 등심을 조금더 사가자",
          "original_form": "응 그래 등심을 조금더 사가자",
          "speaker_id": "2",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000499.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000499.1.13",
          "form": "등심이아니라",
          "original_form": "등심이아니라",
          "speaker_id": "1",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000499.1.14",
          "form": "목살",
          "original_form": "목살",
          "speaker_id": "1",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000499.1.15",
          "form": "그전에 형님네랑 가는 캠핑을 더 먼저 챙겨야지",
          "original_form": "그전에 형님네랑 가는 캠핑을 더 먼저 챙겨야지",
          "speaker_id": "2",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000499.1.16",
          "form": "응 목살 2근?",
          "original_form": "응 목살 2근?",
          "speaker_id": "2",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000499.1.17",
          "form": "그때도 비슷하게 챙겨 가자",
          "original_form": "그때도 비슷하게 챙겨 가자",
          "speaker_id": "1",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000499.1.18",
          "form": "염통도 사가야지",
          "original_form": "염통도 사가야지",
          "speaker_id": "1",
          "time": "20210819 16:33"
        },
        {
          "id": "MDRW2100000499.1.19",
          "form": "합천만 2번가네",
          "original_form": "합천만 2번가네",
          "speaker_id": "1",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000499.1.20",
          "form": "응 근데 비올까봐 걱정이다",
          "original_form": "응 근데 비올까봐 걱정이다",
          "speaker_id": "2",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000499.1.21",
          "form": "괜찬을것같은데",
          "original_form": "괜찬을것같은데",
          "speaker_id": "1",
          "time": "20210819 16:34"
        },
        {
          "id": "MDRW2100000499.1.22",
          "form": "저번처럼 비많이오면... 너무 힘들 것 같아",
          "original_form": "저번처럼 비많이오면... 너무 힘들 것 같아",
          "speaker_id": "2",
          "time": "20210819 16:36"
        }
      ]
