      "utterance": [
        {
          "id": "MDRW2100005622.1.1",
          "form": "식료품은 온라인 오프라인 뭐로 구매하는거 선호하세요",
          "original_form": "식료품은 온라인 오프라인 뭐로 구매하는거 선호하세요",
          "speaker_id": "2",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005622.1.2",
          "form": "온라인요",
          "original_form": "온라인요",
          "speaker_id": "1",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005622.1.3",
          "form": "요새 온라인 할인 이 넘 좋아서요",
          "original_form": "요새 온라인 할인 이 넘 좋아서요",
          "speaker_id": "1",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005622.1.4",
          "form": "아 동의합니다 최근에 알았어요ㅋㅋ",
          "original_form": "아 동의합니다 최근에 알았어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005622.1.5",
          "form": "정가러 먹는게 얼마나 비싼건지 체험중이에요",
          "original_form": "정가러 먹는게 얼마나 비싼건지 체험중이에요",
          "speaker_id": "2",
          "time": "20210821 23:32"
        },
        {
          "id": "MDRW2100005622.1.6",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005622.1.7",
          "form": "쿠폰에 할인에 행사에",
          "original_form": "쿠폰에 할인에 행사에",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005622.1.8",
          "form": "그냥사면 바보돼요 ㅜㅜ",
          "original_form": "그냥사면 바보돼요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005622.1.9",
          "form": "쿠팡은 새벽배송이라좋고",
          "original_form": "쿠팡은 새벽배송이라좋고",
          "speaker_id": "1",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005622.1.10",
          "form": "ㅋㅋㅋㅋ어디 애용하세요",
          "original_form": "ㅋㅋㅋㅋ어디 애용하세요",
          "speaker_id": "2",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005622.1.11",
          "form": "오네네ㅋㅋㅋㅋㅋ",
          "original_form": "오네네ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005622.1.12",
          "form": "마트는 쿠폰좋고 ㅎㅎ",
          "original_form": "마트는 쿠폰좋고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005622.1.13",
          "form": "저 쿠팡 마트들",
          "original_form": "저 쿠팡 마트들",
          "speaker_id": "1",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005622.1.14",
          "form": "식품파는 업체들 다해요",
          "original_form": "식품파는 업체들 다해요",
          "speaker_id": "1",
          "time": "20210821 23:34"
        },
        {
          "id": "MDRW2100005622.1.15",
          "form": "컬리도 쿠폰이 많다고 들었는데!!",
          "original_form": "컬리도 쿠폰이 많다고 들었는데!!",
          "speaker_id": "2",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005622.1.16",
          "form": "첨 가입시",
          "original_form": "첨 가입시",
          "speaker_id": "1",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005622.1.17",
          "form": "100원딜이 있어요",
          "original_form": "100원딜이 있어요",
          "speaker_id": "1",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005622.1.18",
          "form": "여긴 시골 배송이 새벽에 안돼요 ㅜㅜ컬리는",
          "original_form": "여긴 시골 배송이 새벽에 안돼요 ㅜㅜ컬리는",
          "speaker_id": "1",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005622.1.19",
          "form": "저는 비마트 첫배송시 100원딜 있던거 2개나 못쓰고  그냥 구매해서",
          "original_form": "저는 비마트 첫배송시 100원딜 있던거 2개나 못쓰고  그냥 구매해서",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005622.1.20",
          "form": "쓰라렸던 기억이 있어요ㅋㅋㅋ",
          "original_form": "쓰라렸던 기억이 있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005622.1.21",
          "form": "마트도 새벽이 안돼요 ㅋㅋ",
          "original_form": "마트도 새벽이 안돼요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005622.1.22",
          "form": "헉 저희 그거 안돼요",
          "original_form": "헉 저희 그거 안돼요",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005622.1.23",
          "form": "급 슬프네요",
          "original_form": "급 슬프네요",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005622.1.24",
          "form": "그럼 완전 이불킥이죠",
          "original_form": "그럼 완전 이불킥이죠",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005622.1.25",
          "form": "헉 아까워라",
          "original_form": "헉 아까워라",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005622.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ그럼요ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그럼요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005622.1.27",
          "form": "아이디를 무려 두개나...!! ㅋㅋㅋ",
          "original_form": "아이디를 무려 두개나...!! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005622.1.28",
          "form": "항상 잘 체크한다고 생각했는데 좋운 치즈를 두개나 못받았다 싶으니 속상하더라고요",
          "original_form": "항상 잘 체크한다고 생각했는데 좋운 치즈를 두개나 못받았다 싶으니 속상하더라고요",
          "speaker_id": "2",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005622.1.29",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005622.1.30",
          "form": "아 치즈를",
          "original_form": "아 치즈를",
          "speaker_id": "1",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005622.1.31",
          "form": "하필 더 아깝네요",
          "original_form": "하필 더 아깝네요",
          "speaker_id": "1",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005622.1.32",
          "form": "전 써보고 싶네요",
          "original_form": "전 써보고 싶네요",
          "speaker_id": "1",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005622.1.33",
          "form": "배송 노노 안돼요",
          "original_form": "배송 노노 안돼요",
          "speaker_id": "1",
          "time": "20210821 23:38"
        }
      ]
