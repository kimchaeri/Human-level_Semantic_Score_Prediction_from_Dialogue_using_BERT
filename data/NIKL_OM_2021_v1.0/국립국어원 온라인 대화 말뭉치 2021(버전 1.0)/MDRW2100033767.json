      "utterance": [
        {
          "id": "MDRW2100033767.1.1",
          "form": "오늘 점심 뭐 드실거에요 ~~",
          "original_form": "오늘 점심 뭐 드실거에요 ~~",
          "speaker_id": "2",
          "time": "20210825 07:45"
        },
        {
          "id": "MDRW2100033767.1.2",
          "form": "오늘 점심은 고민이네요.",
          "original_form": "오늘 점심은 고민이네요.",
          "speaker_id": "1",
          "time": "20210825 08:15"
        },
        {
          "id": "MDRW2100033767.1.3",
          "form": "뭐드셔요?",
          "original_form": "뭐드셔요?",
          "speaker_id": "1",
          "time": "20210825 08:15"
        },
        {
          "id": "MDRW2100033767.1.4",
          "form": "전 반찬가게에서 배달 해주는거라",
          "original_form": "전 반찬가게에서 배달 해주는거라",
          "speaker_id": "2",
          "time": "20210825 08:22"
        },
        {
          "id": "MDRW2100033767.1.5",
          "form": "그걸보고 알 수 있어요ㅋㅋㅋㅋ",
          "original_form": "그걸보고 알 수 있어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 08:22"
        },
        {
          "id": "MDRW2100033767.1.6",
          "form": "와 반찬가게에서 매일받나봐요?ㅋ",
          "original_form": "와 반찬가게에서 매일받나봐요?ㅋ",
          "speaker_id": "1",
          "time": "20210825 08:24"
        },
        {
          "id": "MDRW2100033767.1.7",
          "form": "네 점심은 그렇게 먹어요ㅋㅋㅋㅋㅋ",
          "original_form": "네 점심은 그렇게 먹어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 11:33"
        },
        {
          "id": "MDRW2100033767.1.8",
          "form": "와 편하시겠어요ㅋㅋ",
          "original_form": "와 편하시겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100033767.1.9",
          "form": "저도그러고싶네요ㅋㅋ",
          "original_form": "저도그러고싶네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:35"
        },
        {
          "id": "MDRW2100033767.1.10",
          "form": "ㅋㅋㅋㅋㅋ 근데",
          "original_form": "ㅋㅋㅋㅋㅋ 근데",
          "speaker_id": "2",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100033767.1.11",
          "form": "맛없을 때도 있어서요 ㅠㅠ",
          "original_form": "맛없을 때도 있어서요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 11:58"
        },
        {
          "id": "MDRW2100033767.1.12",
          "form": "ㅜㅜ아하 그건 좀 그렇네요ㅜ",
          "original_form": "ㅜㅜ아하 그건 좀 그렇네요ㅜ",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100033767.1.13",
          "form": "ㅠㅠㅠ 네 그래도 먹어야죠",
          "original_form": "ㅠㅠㅠ 네 그래도 먹어야죠",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100033767.1.14",
          "form": "맞아요., 저는 그냥 김치볶음밥 먹으려구요..ㅋㅋㅋ",
          "original_form": "맞아요., 저는 그냥 김치볶음밥 먹으려구요..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100033767.1.15",
          "form": "헉 김볶",
          "original_form": "헉 김볶",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100033767.1.16",
          "form": "맛있겠어요 ㅠㅠ",
          "original_form": "맛있겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100033767.1.17",
          "form": "간단하게 할수있는게 김볶인것같아요ㅋ",
          "original_form": "간단하게 할수있는게 김볶인것같아요ㅋ",
          "speaker_id": "1",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100033767.1.18",
          "form": "저녁이 또 문제네요ㅋ",
          "original_form": "저녁이 또 문제네요ㅋ",
          "speaker_id": "1",
          "time": "20210825 15:07"
        },
        {
          "id": "MDRW2100033767.1.19",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100033767.1.20",
          "form": "밥 고르능 것도",
          "original_form": "밥 고르능 것도",
          "speaker_id": "2",
          "time": "20210825 15:13"
        },
        {
          "id": "MDRW2100033767.1.21",
          "form": "은근 일이에요",
          "original_form": "은근 일이에요",
          "speaker_id": "2",
