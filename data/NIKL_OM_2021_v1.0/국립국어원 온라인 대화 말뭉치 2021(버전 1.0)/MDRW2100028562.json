      "utterance": [
        {
          "id": "MDRW2100028562.1.1",
          "form": "요즘 아침저녁 좀 쌀쌀하죠",
          "original_form": "요즘 아침저녁 좀 쌀쌀하죠",
          "speaker_id": "2",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100028562.1.2",
          "form": "어제는 선풍기도 안틀고 잤어요",
          "original_form": "어제는 선풍기도 안틀고 잤어요",
          "speaker_id": "1",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100028562.1.3",
          "form": "일기예보는 잘맞지않지만,,,",
          "original_form": "일기예보는 잘맞지않지만,,,",
          "speaker_id": "1",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100028562.1.4",
          "form": "이제 슬슬 선풍기 분해해서 들여놔야할까봐요",
          "original_form": "이제 슬슬 선풍기 분해해서 들여놔야할까봐요",
          "speaker_id": "2",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028562.1.5",
          "form": "ㅋㅋㅋㅋ 이제 전 일기예보에 대한 신뢰가 없어요..",
          "original_form": "ㅋㅋㅋㅋ 이제 전 일기예보에 대한 신뢰가 없어요..",
          "speaker_id": "2",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028562.1.6",
          "form": "그냥 매일 우산들고 다니고 겉옷들고 다니고",
          "original_form": "그냥 매일 우산들고 다니고 겉옷들고 다니고",
          "speaker_id": "2",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028562.1.7",
          "form": "모든걸 대비해요",
          "original_form": "모든걸 대비해요",
          "speaker_id": "2",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100028562.1.8",
          "form": "저도 우산매일들고다녀요ㅋㅋㅋㅋ",
          "original_form": "저도 우산매일들고다녀요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100028562.1.9",
          "form": "겉옷남방도,,,",
          "original_form": "겉옷남방도,,,",
          "speaker_id": "1",
          "time": "20210902 20:53"
        },
        {
          "id": "MDRW2100028562.1.10",
          "form": "내몸 내가 챙기는 게 최고죠ㅋㅋㅋㅋ",
          "original_form": "내몸 내가 챙기는 게 최고죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028562.1.11",
          "form": "요즘 미세먼지는 없는거죠,,,?",
          "original_form": "요즘 미세먼지는 없는거죠,,,?",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100028562.1.12",
          "form": "그런거 같아요",
          "original_form": "그런거 같아요",
          "speaker_id": "2",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100028562.1.13",
          "form": "확인해보진 않았지만요 ㅎㅎ",
          "original_form": "확인해보진 않았지만요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028562.1.14",
          "form": "그래도 맨날 마스크 끼니까 미세먼지는 좀 안심되더라고요",
          "original_form": "그래도 맨날 마스크 끼니까 미세먼지는 좀 안심되더라고요",
          "speaker_id": "2",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028562.1.15",
          "form": "어맞아요 마스크의 장점ㅋㅋㅋㄱ",
          "original_form": "어맞아요 마스크의 장점ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028562.1.16",
          "form": "name2님사시는곳은 이번주간 날씨가 어땠나요",
          "original_form": "&name2&님사시는곳은 이번주간 날씨가 어땠나요",
          "speaker_id": "1",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100028562.1.17",
          "form": "비가 한번 오고 계속 흐렸어요",
          "original_form": "비가 한번 오고 계속 흐렸어요",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028562.1.18",
          "form": "name1님사시는곳은 어땠어요?",
          "original_form": "&name1&님사시는곳은 어땠어요?",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028562.1.19",
          "form": "거의 비가 매일 온것같아요",
          "original_form": "거의 비가 매일 온것같아요",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028562.1.20",
          "form": "조금이라도 내리더라고요",
          "original_form": "조금이라도 내리더라고요",
          "speaker_id": "1",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100028562.1.21",
          "form": "가을을 즐겨야하는데,,,",
          "original_form": "가을을 즐겨야하는데,,,",
