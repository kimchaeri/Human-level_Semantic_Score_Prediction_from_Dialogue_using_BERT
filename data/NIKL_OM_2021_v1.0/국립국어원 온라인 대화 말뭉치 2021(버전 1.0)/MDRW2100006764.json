      "utterance": [
        {
          "id": "MDRW2100006764.1.1",
          "form": "저는 닭발 곱창 좋아해요!!",
          "original_form": "저는 닭발 곱창 좋아해요!!",
          "speaker_id": "2",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100006764.1.2",
          "form": "곱창 저도 좋아해요!",
          "original_form": "곱창 저도 좋아해요!",
          "speaker_id": "1",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100006764.1.3",
          "form": "닭발은 아직 도전해 본 적 없는...",
          "original_form": "닭발은 아직 도전해 본 적 없는...",
          "speaker_id": "1",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100006764.1.4",
          "form": "오오 닭발은 못드시나요??",
          "original_form": "오오 닭발은 못드시나요??",
          "speaker_id": "2",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100006764.1.5",
          "form": "그냥 아직 시도해 본 적이 없어요",
          "original_form": "그냥 아직 시도해 본 적이 없어요",
          "speaker_id": "1",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100006764.1.6",
          "form": "오오 마라탕 드셔보셧어요?",
          "original_form": "오오 마라탕 드셔보셧어요?",
          "speaker_id": "2",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100006764.1.7",
          "form": "네 몇 번 먹어봤어요",
          "original_form": "네 몇 번 먹어봤어요",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100006764.1.8",
          "form": "저는 먹으니까 배가 아픈 거 같더라고요 ㅠㅠ",
          "original_form": "저는 먹으니까 배가 아픈 거 같더라고요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100006764.1.9",
          "form": "오 저는 못먹어요 ㅠㅠ",
          "original_form": "오 저는 못먹어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100006764.1.10",
          "form": "고수도 드세요?",
          "original_form": "고수도 드세요?",
          "speaker_id": "2",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100006764.1.11",
          "form": "아뇨 고수는 잘 못 먹어요...",
          "original_form": "아뇨 고수는 잘 못 먹어요...",
          "speaker_id": "1",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100006764.1.12",
          "form": "오 민트는 어떠세요",
          "original_form": "오 민트는 어떠세요",
          "speaker_id": "2",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100006764.1.13",
          "form": "앗 민트도 싫어해요",
          "original_form": "앗 민트도 싫어해요",
          "speaker_id": "1",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100006764.1.14",
          "form": "우아 저랑 비슷해요 저도호불호 갈리는거",
          "original_form": "우아 저랑 비슷해요 저도호불호 갈리는거",
          "speaker_id": "2",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100006764.1.15",
          "form": "거의 불이에요 ㅋㅋㅋ",
          "original_form": "거의 불이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100006764.1.16",
          "form": "앗 그럼 혹시",
          "original_form": "앗 그럼 혹시",
          "speaker_id": "1",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100006764.1.17",
          "form": "붕어빵 팥붕 vs 슈붕",
          "original_form": "붕어빵 팥붕 vs 슈붕",
          "speaker_id": "1",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100006764.1.18",
