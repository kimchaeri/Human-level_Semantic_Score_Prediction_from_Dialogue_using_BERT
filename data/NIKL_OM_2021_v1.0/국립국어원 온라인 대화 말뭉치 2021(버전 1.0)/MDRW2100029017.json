      "utterance": [
        {
          "id": "MDRW2100029017.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210902 23:20"
        },
        {
          "id": "MDRW2100029017.1.2",
          "form": "보통 설거지 하죠.",
          "original_form": "보통 설거지 하죠.",
          "speaker_id": "2",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029017.1.3",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100029017.1.4",
          "form": "아하 저도 가끔 어머니가 시키면 청소기도 돌려요.",
          "original_form": "아하 저도 가끔 어머니가 시키면 청소기도 돌려요.",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029017.1.5",
          "form": "혹시 외동이신가요?",
          "original_form": "혹시 외동이신가요?",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100029017.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100029017.1.7",
          "form": "아하 외동은 어때요? 저는 둘째라서 외동 어떤지 궁금해요",
          "original_form": "아하 외동은 어때요? 저는 둘째라서 외동 어떤지 궁금해요",
          "speaker_id": "2",
          "time": "20210902 23:34"
        },
        {
          "id": "MDRW2100029017.1.8",
          "form": "혼자사 잘 못해요",
          "original_form": "혼자사 잘 못해요",
          "speaker_id": "1",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100029017.1.9",
          "form": "그럴 수 있죠 어차피 다 배워가면서 하는거인걸요",
          "original_form": "그럴 수 있죠 어차피 다 배워가면서 하는거인걸요",
          "speaker_id": "2",
          "time": "20210902 23:39"
        },
        {
          "id": "MDRW2100029017.1.10",
          "form": "스스로 좀 했으몀",
          "original_form": "스스로 좀 했으몀",
          "speaker_id": "1",
          "time": "20210902 23:42"
        },
        {
          "id": "MDRW2100029017.1.11",
          "form": "잘 할 수 있어요!",
          "original_form": "잘 할 수 있어요!",
          "speaker_id": "2",
          "time": "20210902 23:43"
        },
        {
          "id": "MDRW2100029017.1.12",
          "form": "네 그렇겠져",
          "original_form": "네 그렇겠져",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100029017.1.13",
          "form": "가족들이 많이 도와주시나요?",
          "original_form": "가족들이 많이 도와주시나요?",
