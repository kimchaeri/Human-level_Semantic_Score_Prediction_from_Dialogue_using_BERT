      "utterance": [
        {
          "id": "MDRW2100020788.1.1",
          "form": "전 빵 스프레드도 케이크도 다 좋아하지만",
          "original_form": "전 빵 스프레드도 케이크도 다 좋아하지만",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020788.1.2",
          "form": "그것도 너무 곰팡이가 잘 슬어요",
          "original_form": "그것도 너무 곰팡이가 잘 슬어요",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020788.1.3",
          "form": "냉동실에 넣음 또 맛이 없구요ㅜ",
          "original_form": "냉동실에 넣음 또 맛이 없구요ㅜ",
          "speaker_id": "1",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020788.1.4",
          "form": "이제 날이 추워질테니 좀 걱정이 덜해지나요?",
          "original_form": "이제 날이 추워질테니 좀 걱정이 덜해지나요?",
          "speaker_id": "1",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020788.1.5",
          "form": "포션으로 먹다가 감질나서 250그램샀다가 반을 버렷ㅅ었어요",
          "original_form": "포션으로 먹다가 감질나서 250그램샀다가 반을 버렷ㅅ었어요",
          "speaker_id": "2",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020788.1.6",
          "form": "여름보단 덜하지만 그래도 2주안엔 먹어야할거에요",
          "original_form": "여름보단 덜하지만 그래도 2주안엔 먹어야할거에요",
          "speaker_id": "2",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020788.1.7",
          "form": "그렇군요 관리가 중요하네요",
          "original_form": "그렇군요 관리가 중요하네요",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020788.1.8",
          "form": "name2님 제가요 흑당을 어디 쓸수있을까 좀 고민해봤는데요",
          "original_form": "&name2&님 제가요 흑당을 어디 쓸수있을까 좀 고민해봤는데요",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020788.1.9",
          "form": "ㅋㅋ큐 저 그거 치즈케이크에 넣고 구웠다가 치즈맛이 다 사라지고",
          "original_form": "ㅋㅋ큐 저 그거 치즈케이크에 넣고 구웠다가 치즈맛이 다 사라지고",
          "speaker_id": "2",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020788.1.10",
          "form": "일본식 푸딩에 넣어보시는건 어떠세요?",
          "original_form": "일본식 푸딩에 넣어보시는건 어떠세요?",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020788.1.11",
          "form": "흑당케이크가 되서 피눈물 흘렷ㄱ어요 ㅠ",
          "original_form": "흑당케이크가 되서 피눈물 흘렷ㄱ어요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020788.1.12",
          "form": "아 검은케이끄..ㅜ",
          "original_form": "아 검은케이끄..ㅜ",
          "speaker_id": "1",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020788.1.13",
          "form": "좋은데요? 근데 푸딩하나당 한 티스푼 정도 들어갈까..ㅋㅋㅋ",
          "original_form": "좋은데요? 근데 푸딩하나당 한 티스푼 정도 들어갈까..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020788.1.14",
          "form": "푸딩에 흑설탕을 넣더라구요!",
          "original_form": "푸딩에 흑설탕을 넣더라구요!",
          "speaker_id": "1",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020788.1.15",
          "form": "ㅋㅋㅋㅋㅋ한스푼이라도..ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ한스푼이라도..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020788.1.16",
          "form": "검은색은 상관이 없는데 흑당향이 치즈케이크의 향 산미와 치즈향을 잡아먹어요",
          "original_form": "검은색은 상관이 없는데 흑당향이 치즈케이크의 향 산미와 치즈향을 잡아먹어요",
          "speaker_id": "2",
          "time": "20210828 12:36"
        },
        {
          "id": "MDRW2100020788.1.17",
          "form": "시도해봐야겠어요 고마워요!",
          "original_form": "시도해봐야겠어요 고마워요!",
          "speaker_id": "2",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020788.1.18",
          "form": "그렇겠네요 흑당은 맛이 진하고 빵에쓰이는 치즈는 보통 향이 약하니까요",
          "original_form": "그렇겠네요 흑당은 맛이 진하고 빵에쓰이는 치즈는 보통 향이 약하니까요",
          "speaker_id": "1",
          "time": "20210828 12:37"
        },
        {
          "id": "MDRW2100020788.1.19",
          "form": "네..치즈향 먹으려고 치즈케이크를 굽는건데..",
          "original_form": "네..치즈향 먹으려고 치즈케이크를 굽는건데..",
          "speaker_id": "2",
          "time": "20210828 12:37"
        }
      ]
    }
