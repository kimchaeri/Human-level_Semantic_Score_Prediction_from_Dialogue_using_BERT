      "utterance": [
        {
          "id": "MDRW2100037699.1.1",
          "form": "네 ! 챙겨먹어요",
          "original_form": "네 ! 챙겨먹어요",
          "speaker_id": "2",
          "time": "20210901 16:32"
        },
        {
          "id": "MDRW2100037699.1.2",
          "form": "뭐 드시나요?",
          "original_form": "뭐 드시나요?",
          "speaker_id": "1",
          "time": "20210901 16:33"
        },
        {
          "id": "MDRW2100037699.1.3",
          "form": "전 유산균 오메가3 마그네슘 비타민이여!",
          "original_form": "전 유산균 오메가3 마그네슘 비타민이여!",
          "speaker_id": "2",
          "time": "20210901 16:33"
        },
        {
          "id": "MDRW2100037699.1.4",
          "form": "오... 유산균 괜찮나요?",
          "original_form": "오... 유산균 괜찮나요?",
          "speaker_id": "1",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100037699.1.5",
          "form": "사실... 얼마전에 질염때문에 고생좀 해서 유산균 챙겨먹어야하나 싶거든요 ㅠㅠ",
          "original_form": "사실... 얼마전에 질염때문에 고생좀 해서 유산균 챙겨먹어야하나 싶거든요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 16:36"
        },
        {
          "id": "MDRW2100037699.1.6",
          "form": "네 괜찮아요 !!! 근데",
          "original_form": "네 괜찮아요 !!! 근데",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100037699.1.7",
          "form": "매일 잊지않고 먹어야해요 ㅋㅋㅋ ㅜㅜ",
          "original_form": "매일 잊지않고 먹어야해요 ㅋㅋㅋ ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100037699.1.8",
          "form": "이게 한번 안챙겨먹으면 바로 효과가 없더라구요 !!",
          "original_form": "이게 한번 안챙겨먹으면 바로 효과가 없더라구요 !!",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100037699.1.9",
          "form": "아... 그거 제일 자신없는건데",
          "original_form": "아... 그거 제일 자신없는건데",
          "speaker_id": "1",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100037699.1.10",
          "form": "사실 다른 영양제도 먹다 까먹고 먹다 까먹고 해요 ㅠㅠ",
          "original_form": "사실 다른 영양제도 먹다 까먹고 먹다 까먹고 해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100037699.1.11",
          "form": "맞죠 ㅋㅋ ㅠㅠㅠ",
          "original_form": "맞죠 ㅋㅋ ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100037699.1.12",
          "form": "근데 유산균은 꼭 매일매일 먹으래요 ...",
          "original_form": "근데 유산균은 꼭 매일매일 먹으래요 ...",
          "speaker_id": "2",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100037699.1.13",
          "form": "그래서 전 아침에 일어나서 공복인 상태로 바로 먹어욧",
          "original_form": "그래서 전 아침에 일어나서 공복인 상태로 바로 먹어욧",
          "speaker_id": "2",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100037699.1.14",
          "form": "하... 저도 하나 사야겠네요",
          "original_form": "하... 저도 하나 사야겠네요",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100037699.1.15",
          "form": "근데 종류가 넘 많아요ㅠㅠ",
          "original_form": "근데 종류가 넘 많아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100037699.1.16",
          "form": "전 자로우꺼 먹어요 !!",
          "original_form": "전 자로우꺼 먹어요 !!",
          "speaker_id": "2",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100037699.1.17",
          "form": "한번 찾아보셔요 !",
          "original_form": "한번 찾아보셔요 !",
          "speaker_id": "2",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100037699.1.18",
          "form": "자로우요?",
          "original_form": "자로우요?",
          "speaker_id": "1",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100037699.1.19",
          "form": "한번 찾아볼게요!",
          "original_form": "한번 찾아볼게요!",
          "speaker_id": "1",
          "time": "20210901 17:32"
        },
        {
          "id": "MDRW2100037699.1.20",
          "form": "ㅋㅋㅋㅋ 저 지금 지르고 왔어요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 저 지금 지르고 왔어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100037699.1.21",
          "form": "헐 진짜옄ㅋㅋㄱ ???",
          "original_form": "헐 진짜옄ㅋㅋㄱ ???",
          "speaker_id": "2",
          "time": "20210901 18:15"
        },
        {
          "id": "MDRW2100037699.1.22",
          "form": "왤케 빠르셔요 !!!",
          "original_form": "왤케 빠르셔요 !!!",
          "speaker_id": "2",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100037699.1.23",
          "form": "ㅋㅋㅋㄱㄱ",
          "original_form": "ㅋㅋㅋㄱㄱ",
          "speaker_id": "2",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100037699.1.24",
          "form": "안그래도 사야하는데 싶었는데 검색해보니까",
          "original_form": "안그래도 사야하는데 싶었는데 검색해보니까",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037699.1.25",
          "form": "후기가 괜찮더라구요 ㅋㅋㅋ",
          "original_form": "후기가 괜찮더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037699.1.26",
          "form": "엇 마자요 ㅋㅋㅋㅋ",
          "original_form": "엇 마자요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037699.1.27",
          "form": "저도 후기 찾아보고 샀는데",
          "original_form": "저도 후기 찾아보고 샀는데",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037699.1.28",
          "form": "한번 꾸준히 드셔보시구 안맞으면",
          "original_form": "한번 꾸준히 드셔보시구 안맞으면",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100037699.1.29",
          "form": "다른걸로도 사서 계속 드셔보세요 !",
          "original_form": "다른걸로도 사서 계속 드셔보세요 !",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100037699.1.30",
          "form": "유산균은 사람마다 맞는게 다르더라구여",
          "original_form": "유산균은 사람마다 맞는게 다르더라구여",
          "speaker_id": "2",
          "time": "20210901 18:18"
        },
        {
          "id": "MDRW2100037699.1.31",
          "form": "그래요? 안맞으면 뭐 부작용같은게 있나요?",
          "original_form": "그래요? 안맞으면 뭐 부작용같은게 있나요?",
          "speaker_id": "1",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100037699.1.32",
          "form": "부작용은 아닌데 효과가 안나타나요 !!",
          "original_form": "부작용은 아닌데 효과가 안나타나요 !!",
          "speaker_id": "2",
          "time": "20210901 18:19"
        }
