      "utterance": [
        {
          "id": "MDRW2100019120.1.1",
          "form": "전 교환할게 있는데 너무 귀찮아요 ㅠ",
          "original_form": "전 교환할게 있는데 너무 귀찮아요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100019120.1.2",
          "form": "그쵸ㅜㅜㅜ 교환신청해야지ㅜㅜ 다시 포장해야지ㅜㅜ 그리고 기다려야지ㅜㅜ",
          "original_form": "그쵸ㅜㅜㅜ 교환신청해야지ㅜㅜ 다시 포장해야지ㅜㅜ 그리고 기다려야지ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019120.1.3",
          "form": "맞아요 ㅠ 가져갈때까지 신경쓰는게 전 넘 싫어요",
          "original_form": "맞아요 ㅠ 가져갈때까지 신경쓰는게 전 넘 싫어요",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019120.1.4",
          "form": "그쵸그쳐!! 교환접수 잘되는지도 신경써야하구 ㅠㅠ 저 한번은 가방을 샀는데 수제 가방?!",
          "original_form": "그쵸그쳐!! 교환접수 잘되는지도 신경써야하구 ㅠㅠ 저 한번은 가방을 샀는데 수제 가방?!",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019120.1.5",
          "form": "베지터블 가죽으로 만들어서",
          "original_form": "베지터블 가죽으로 만들어서",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019120.1.6",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "1",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019120.1.7",
          "form": "베지터블 가죽?! 신기하네요",
          "original_form": "베지터블 가죽?! 신기하네요",
          "speaker_id": "1",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019120.1.8",
          "form": "약간의 흠은 교환 환불 사유가 아니라고 되어있었거든요",
          "original_form": "약간의 흠은 교환 환불 사유가 아니라고 되어있었거든요",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019120.1.9",
          "form": "아 그렇구나..ㅠㅠ",
          "original_form": "아 그렇구나..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019120.1.10",
          "form": "근데 받아보니까 약간의 흠정도가 아니라 전면적으로 막 세로줄이 가있는거에요",
          "original_form": "근데 받아보니까 약간의 흠정도가 아니라 전면적으로 막 세로줄이 가있는거에요",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019120.1.11",
          "form": "그래서 사진찍어서 보내구 그랬는데",
          "original_form": "그래서 사진찍어서 보내구 그랬는데",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019120.1.12",
          "form": "흠 아니라고 ...ㅡㅡ",
          "original_form": "흠 아니라고 ...ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019120.1.13",
          "form": "ㄱ그래서 그냥",
          "original_form": "ㄱ그래서 그냥",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019120.1.14",
          "form": "헐 어이없네요",
          "original_form": "헐 어이없네요",
          "speaker_id": "1",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019120.1.15",
          "form": "제가 왕복 배송비내고 환불했어요",
          "original_form": "제가 왕복 배송비내고 환불했어요",
          "speaker_id": "2",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019120.1.16",
          "form": "헐 ㅠㅠ 너무하다",
          "original_form": "헐 ㅠㅠ 너무하다",
          "speaker_id": "1",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019120.1.17",
          "form": "진짜 기분이 넘 ㅜㅠ 드러워서ㅠㅠ",
          "original_form": "진짜 기분이 넘 ㅜㅠ 드러워서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019120.1.18",
          "form": "저같아도 짜증났을거같아요",
          "original_form": "저같아도 짜증났을거같아요",
          "speaker_id": "1",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019120.1.19",
          "form": "돈만 버리고 ㅠㅠㅠ",
          "original_form": "돈만 버리고 ㅠㅠㅠ",
