      "utterance": [
        {
          "id": "MDRW2100024075.1.1",
          "form": "최근 유럽에서 일자리관련 뉴스보셨어요?",
          "original_form": "최근 유럽에서 일자리관련 뉴스보셨어요?",
          "speaker_id": "1",
          "time": "20210830 21:54"
        },
        {
          "id": "MDRW2100024075.1.2",
          "form": "아니용 어렵겠쥬ㅜ",
          "original_form": "아니용 어렵겠쥬ㅜ",
          "speaker_id": "2",
          "time": "20210830 21:54"
        },
        {
          "id": "MDRW2100024075.1.3",
          "form": "주 4일제 어떻게 생각하시나요?",
          "original_form": "주 4일제 어떻게 생각하시나요?",
          "speaker_id": "1",
          "time": "20210830 21:55"
        },
        {
          "id": "MDRW2100024075.1.4",
          "form": "수요일쉬면 좋겠어요",
          "original_form": "수요일쉬면 좋겠어요",
          "speaker_id": "2",
          "time": "20210830 21:56"
        },
        {
          "id": "MDRW2100024075.1.5",
          "form": "근데 주 4일하는 대신 월급을 줄이겠대요",
          "original_form": "근데 주 4일하는 대신 월급을 줄이겠대요",
          "speaker_id": "1",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100024075.1.6",
          "form": "그월급으로 실업률을 높이겠대요",
          "original_form": "그월급으로 실업률을 높이겠대요",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100024075.1.7",
          "form": "앗 낮추겠대요ㅋㅋ",
          "original_form": "앗 낮추겠대요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:59"
        },
        {
          "id": "MDRW2100024075.1.8",
          "form": "에ㅜㅜㅜ월급이ㅜㅜㅜㅜㅜ",
          "original_form": "에ㅜㅜㅜ월급이ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024075.1.9",
          "form": "좀 슬프네요",
          "original_form": "좀 슬프네요",
          "speaker_id": "2",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024075.1.10",
          "form": "내 월급을 나눠서 고용을 하겠다는 거죠ㅋ",
          "original_form": "내 월급을 나눠서 고용을 하겠다는 거죠ㅋ",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024075.1.11",
          "form": "그 기사보니 화가 나더라구요",
          "original_form": "그 기사보니 화가 나더라구요",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024075.1.12",
          "form": "다른 신입이 제가 쉬는 하루와서 일하시능가",
          "original_form": "다른 신입이 제가 쉬는 하루와서 일하시능가",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024075.1.13",
          "form": "그건 안되는디",
          "original_form": "그건 안되는디",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024075.1.14",
          "form": "그런거죠",
          "original_form": "그런거죠",
          "speaker_id": "1",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024075.1.15",
          "form": "그게 찰흙 나누듯이 되는게 아니잖어",
          "original_form": "그게 찰흙 나누듯이 되는게 아니잖어",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024075.1.16",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024075.1.17",
          "form": "머릿속에 뇌가 찰흙이신가",
          "original_form": "머릿속에 뇌가 찰흙이신가",
          "speaker_id": "2",
          "time": "20210830 22:01"
        },
        {
          "id": "MDRW2100024075.1.18",
          "form": "한국도 따라할것 같던데",
          "original_form": "한국도 따라할것 같던데",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024075.1.19",
          "form": "걱정이네요",
          "original_form": "걱정이네요",
          "speaker_id": "1",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024075.1.20",
          "form": "아놔",
          "original_form": "아놔",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024075.1.21",
          "form": "서양것이 다 좋아보이는 생각은 후진데",
          "original_form": "서양것이 다 좋아보이는 생각은 후진데",
          "speaker_id": "2",
          "time": "20210830 22:02"
        },
        {
          "id": "MDRW2100024075.1.22",
          "form": "서양것이라서 그렇다기보단",
          "original_form": "서양것이라서 그렇다기보단",
          "speaker_id": "1",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100024075.1.23",
          "form": "일자리해결하려는 이유인거 같아요",
          "original_form": "일자리해결하려는 이유인거 같아요",
          "speaker_id": "1",
          "time": "20210830 22:03"
        },
        {
          "id": "MDRW2100024075.1.24",
          "form": "음 문제해결이 어려우니",
          "original_form": "음 문제해결이 어려우니",
          "speaker_id": "2",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100024075.1.25",
          "form": "별 신기한시도를 하는군요",
          "original_form": "별 신기한시도를 하는군요",
          "speaker_id": "2",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100024075.1.26",
          "form": "음 확실한 해결책은 되더군요ㅋㅋ",
          "original_form": "음 확실한 해결책은 되더군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100024075.1.27",
          "form": "아...어려운 주제인데.'",
          "original_form": "아...어려운 주제인데.'",
          "speaker_id": "1",
          "time": "20210831 14:10"
        }
      ]
    }
