      "utterance": [
        {
          "id": "MDRW2100002062.1.1",
          "form": "여항 좋아하시나요?",
          "original_form": "여항 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210819 23:14"
        },
        {
          "id": "MDRW2100002062.1.2",
          "form": "네 해외여행 한번도 안가봤는데 한번 가보고 싶네요",
          "original_form": "네 해외여행 한번도 안가봤는데 한번 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210819 23:15"
        },
        {
          "id": "MDRW2100002062.1.3",
          "form": "저도 가야하는데 리조트로",
          "original_form": "저도 가야하는데 리조트로",
          "speaker_id": "1",
          "time": "20210819 23:16"
        },
        {
          "id": "MDRW2100002062.1.4",
          "form": "전 일본쪽 가보고싶더라구요",
          "original_form": "전 일본쪽 가보고싶더라구요",
          "speaker_id": "2",
          "time": "20210819 23:17"
        },
        {
          "id": "MDRW2100002062.1.5",
          "form": "게임 좋아해서",
          "original_form": "게임 좋아해서",
          "speaker_id": "2",
          "time": "20210819 23:17"
        },
        {
          "id": "MDRW2100002062.1.6",
          "form": "일본 어느지역이요(?)",
          "original_form": "일본 어느지역이요(?)",
          "speaker_id": "1",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100002062.1.7",
          "form": "도쿄나 그 이름이 생각 안나는데",
          "original_form": "도쿄나 그 이름이 생각 안나는데",
          "speaker_id": "2",
          "time": "20210819 23:22"
        },
        {
          "id": "MDRW2100002062.1.8",
          "form": "달리는 캐릭터 유명한거 있는곳",
          "original_form": "달리는 캐릭터 유명한거 있는곳",
          "speaker_id": "2",
          "time": "20210819 23:22"
        },
        {
          "id": "MDRW2100002062.1.9",
          "form": "기억이 안나네요",
          "original_form": "기억이 안나네요",
          "speaker_id": "2",
          "time": "20210819 23:22"
        },
        {
          "id": "MDRW2100002062.1.10",
          "form": "하니",
          "original_form": "하니",
          "speaker_id": "1",
          "time": "20210819 23:24"
        },
        {
          "id": "MDRW2100002062.1.11",
          "form": "아뇨 남자캐릭턴데",
          "original_form": "아뇨 남자캐릭턴데",
          "speaker_id": "2",
          "time": "20210819 23:25"
        },
        {
          "id": "MDRW2100002062.1.12",
          "form": "운동복 비슷한거 입고있고",
          "original_form": "운동복 비슷한거 입고있고",
          "speaker_id": "2",
          "time": "20210819 23:25"
        },
        {
          "id": "MDRW2100002062.1.13",
          "form": "그 지역 유명한건데",
          "original_form": "그 지역 유명한건데",
          "speaker_id": "2",
          "time": "20210819 23:25"
        },
        {
          "id": "MDRW2100002062.1.14",
          "form": "잘모르겠네요 일본가본신저있어요(?)",
          "original_form": "잘모르겠네요 일본가본신저있어요(?)",
          "speaker_id": "1",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100002062.1.15",
          "form": "아뇨 전 해외여행을 한번도 못가봐서...",
          "original_form": "아뇨 전 해외여행을 한번도 못가봐서...",
          "speaker_id": "2",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100002062.1.16",
          "form": "혹시 가보셨어요?",
          "original_form": "혹시 가보셨어요?",
          "speaker_id": "2",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100002062.1.17",
          "form": "전 일본은 못가봤네요",
          "original_form": "전 일본은 못가봤네요",
          "speaker_id": "1",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100002062.1.18",
          "form": "호주  필리핀 중국",
          "original_form": "호주  필리핀 중국",
          "speaker_id": "1",
          "time": "20210819 23:36"
        },
        {
          "id": "MDRW2100002062.1.19",
          "form": "코로나 전에 가셨어요?",
          "original_form": "코로나 전에 가셨어요?",
          "speaker_id": "2",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100002062.1.20",
          "form": "네 당연하죠",
          "original_form": "네 당연하죠",
          "speaker_id": "1",
          "time": "20210819 23:37"
        }
      ]
