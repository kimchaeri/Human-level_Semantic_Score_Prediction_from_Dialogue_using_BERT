      "utterance": [
        {
          "id": "MDRW2100006941.1.1",
          "form": "네 좋아요",
          "original_form": "네 좋아요",
          "speaker_id": "2",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006941.1.2",
          "form": "잘자요~!",
          "original_form": "잘자요~!",
          "speaker_id": "2",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006941.1.3",
          "form": "영화관가서 본것중애 뭐가 제일 좋으셨나요?",
          "original_form": "영화관가서 본것중애 뭐가 제일 좋으셨나요?",
          "speaker_id": "2",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100006941.1.4",
          "form": "게임 얘기 하고 있었어요!",
          "original_form": "게임 얘기 하고 있었어요!",
          "speaker_id": "1",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100006941.1.5",
          "form": "뮤지컬 본적 있으세요?",
          "original_form": "뮤지컬 본적 있으세요?",
          "speaker_id": "2",
          "time": "20210822 16:43"
        },
        {
          "id": "MDRW2100006941.1.6",
          "form": "학교에서 단체로 간 걸 제외하고는 <더 언더독> 정도 본 게 다인 거 같아요.",
          "original_form": "학교에서 단체로 간 걸 제외하고는 <더 언더독> 정도 본 게 다인 거 같아요.",
          "speaker_id": "1",
          "time": "20210823 14:39"
        },
        {
          "id": "MDRW2100006941.1.7",
          "form": "name2 님은 뮤지컬 자주 보시나요?",
          "original_form": "&name2& 님은 뮤지컬 자주 보시나요?",
          "speaker_id": "1",
          "time": "20210823 14:39"
        },
        {
          "id": "MDRW2100006941.1.8",
          "form": "저도 경험이 거의 없는데",
          "original_form": "저도 경험이 거의 없는데",
          "speaker_id": "2",
          "time": "20210823 18:43"
        },
        {
          "id": "MDRW2100006941.1.9",
          "form": "최근에 관심이 생겨서 알아보고있어요 ㅎㅎ",
          "original_form": "최근에 관심이 생겨서 알아보고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 18:43"
        },
        {
          "id": "MDRW2100006941.1.10",
          "form": "휴가대신 보고오려고요",
          "original_form": "휴가대신 보고오려고요",
          "speaker_id": "2",
          "time": "20210823 18:43"
        },
        {
          "id": "MDRW2100006941.1.11",
          "form": "더 언더독이 유기견들의 이야기인데",
          "original_form": "더 언더독이 유기견들의 이야기인데",
          "speaker_id": "1",
          "time": "20210823 18:46"
        },
        {
          "id": "MDRW2100006941.1.12",
          "form": "맨 앞자리 앉았다가 강아지용 참치캔 같은 것도 받았어요ㅋㅋㅋ",
          "original_form": "맨 앞자리 앉았다가 강아지용 참치캔 같은 것도 받았어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:46"
        },
        {
          "id": "MDRW2100006941.1.13",
          "form": "이런 소소한 이벤트도 재밌더라고요",
          "original_form": "이런 소소한 이벤트도 재밌더라고요",
          "speaker_id": "1",
          "time": "20210823 18:47"
        },
        {
          "id": "MDRW2100006941.1.14",
          "form": "우와 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "우와 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:51"
        },
        {
          "id": "MDRW2100006941.1.15",
          "form": "재밌는 이벤트네요",
          "original_form": "재밌는 이벤트네요",
          "speaker_id": "2",
          "time": "20210823 18:51"
        },
        {
          "id": "MDRW2100006941.1.16",
          "form": "기억에 쏙쏙 남겠어요",
          "original_form": "기억에 쏙쏙 남겠어요",
          "speaker_id": "2",
          "time": "20210823 18:51"
        },
        {
          "id": "MDRW2100006941.1.17",
          "form": "전시회같은건 가보셨나요?",
          "original_form": "전시회같은건 가보셨나요?",
          "speaker_id": "2",
          "time": "20210823 18:51"
        },
        {
          "id": "MDRW2100006941.1.18",
          "form": "고등학생 때는 미술 전시 좀 찾아다녔는데",
          "original_form": "고등학생 때는 미술 전시 좀 찾아다녔는데",
          "speaker_id": "1",
          "time": "20210823 18:54"
        },
        {
          "id": "MDRW2100006941.1.19",
          "form": "요즘은 안 간지 오래됐어요",
          "original_form": "요즘은 안 간지 오래됐어요",
          "speaker_id": "1",
          "time": "20210823 18:54"
        },
        {
          "id": "MDRW2100006941.1.20",
          "form": "시국이 시국이니 어쩔 수 없죠",
          "original_form": "시국이 시국이니 어쩔 수 없죠",
          "speaker_id": "2",
          "time": "20210823 18:55"
        },
        {
          "id": "MDRW2100006941.1.21",
          "form": "그런걸 봐야 좀 세상이 넓어진다던데",
          "original_form": "그런걸 봐야 좀 세상이 넓어진다던데",
          "speaker_id": "2",
          "time": "20210823 18:55"
        },
        {
          "id": "MDRW2100006941.1.22",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100006941.1.23",
          "form": "가기가 조금 망설여지더라고요",
          "original_form": "가기가 조금 망설여지더라고요",
          "speaker_id": "1",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100006941.1.24",
          "form": "영화는 어떤장르좋아하세오?",
          "original_form": "영화는 어떤장르좋아하세오?",
          "speaker_id": "2",
          "time": "20210823 19:14"
        },
        {
          "id": "MDRW2100006941.1.25",
          "form": "코미디랑 액션 좋아해요!",
          "original_form": "코미디랑 액션 좋아해요!",
          "speaker_id": "1",
          "time": "20210823 19:16"
        },
        {
          "id": "MDRW2100006941.1.26",
          "form": "name2 님은요?",
          "original_form": "&name2& 님은요?",
          "speaker_id": "1",
          "time": "20210823 19:16"
        },
        {
          "id": "MDRW2100006941.1.27",
          "form": "저는 음악영화요!",
          "original_form": "저는 음악영화요!",
          "speaker_id": "2",
          "time": "20210823 20:00"
        },
        {
          "id": "MDRW2100006941.1.28",
          "form": "이번에 코다? 라라랜드 작가 영화 나온다는데 너무 기대중이에요",
          "original_form": "이번에 코다? 라라랜드 작가 영화 나온다는데 너무 기대중이에요",
          "speaker_id": "2",
          "time": "20210823 20:00"
        },
        {
          "id": "MDRW2100006941.1.29",
          "form": "차기작 나오는 군요!",
          "original_form": "차기작 나오는 군요!",
          "speaker_id": "1",
          "time": "20210823 20:01"
        },
        {
          "id": "MDRW2100006941.1.30",
          "form": "라라랜드 저도 진짜 재밌게 봤어요",
          "original_form": "라라랜드 저도 진짜 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210823 20:01"
        },
        {
          "id": "MDRW2100006941.1.31",
          "form": "노래랑 분위기랑 다 너무 좋았어요",
          "original_form": "노래랑 분위기랑 다 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210823 20:01"
        },
        {
          "id": "MDRW2100006941.1.32",
          "form": "그쵸 ㅠㅠ 결말까지 저는 너무 마음에 들더라고요",
          "original_form": "그쵸 ㅠㅠ 결말까지 저는 너무 마음에 들더라고요",
          "speaker_id": "2",
          "time": "20210823 20:02"
        },
        {
          "id": "MDRW2100006941.1.33",
          "form": "이상괴 현실.. 딱 그사이를 넘나드는것같고",
          "original_form": "이상괴 현실.. 딱 그사이를 넘나드는것같고",
          "speaker_id": "2",
          "time": "20210823 20:02"
        }
      ]
