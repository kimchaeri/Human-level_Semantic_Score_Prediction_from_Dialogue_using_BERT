        },
        {
          "id": "MDRW2100035360.1.2",
          "form": "안냥하세여",
          "original_form": "안냥하세여",
          "speaker_id": "1",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100035360.1.3",
          "form": "오늘 점심 뭐먹었게요?",
          "original_form": "오늘 점심 뭐먹었게요?",
          "speaker_id": "2",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100035360.1.4",
          "form": "안먹엇네요",
          "original_form": "안먹엇네요",
          "speaker_id": "1",
          "time": "20210829 15:35"
        },
        {
          "id": "MDRW2100035360.1.5",
          "form": "저말이에요..",
          "original_form": "저말이에요..",
          "speaker_id": "2",
          "time": "20210829 15:36"
        },
        {
          "id": "MDRW2100035360.1.6",
          "form": "뭔데요",
          "original_form": "뭔데요",
          "speaker_id": "1",
          "time": "20210829 15:37"
        },
        {
          "id": "MDRW2100035360.1.7",
          "form": "궁금해집니다",
          "original_form": "궁금해집니다",
          "speaker_id": "1",
          "time": "20210829 15:37"
        },
        {
          "id": "MDRW2100035360.1.8",
          "form": "망뇨끼",
          "original_form": "망뇨끼",
          "speaker_id": "2",
          "time": "20210829 15:38"
        },
        {
          "id": "MDRW2100035360.1.9",
          "form": "그게 무슨뜻인가요",
          "original_form": "그게 무슨뜻인가요",
          "speaker_id": "1",
          "time": "20210829 15:38"
        },
        {
          "id": "MDRW2100035360.1.10",
          "form": "뇨끼가 이상해요",
          "original_form": "뇨끼가 이상해요",
          "speaker_id": "2",
          "time": "20210829 15:38"
        },
        {
          "id": "MDRW2100035360.1.11",
          "form": "왜죠",
          "original_form": "왜죠",
          "speaker_id": "1",
          "time": "20210829 15:38"
        },
        {
          "id": "MDRW2100035360.1.12",
          "form": "??무슨일잇으신가요?",
          "original_form": "??무슨일잇으신가요?",
          "speaker_id": "1",
          "time": "20210829 15:38"
        },
        {
          "id": "MDRW2100035360.1.13",
          "form": "퍼석퍼석해야라는데 쫀득해젺어요",
          "original_form": "퍼석퍼석해야라는데 쫀득해젺어요",
          "speaker_id": "2",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100035360.1.14",
          "form": "저는 쫀득파라 너무 좋은데",
          "original_form": "저는 쫀득파라 너무 좋은데",
          "speaker_id": "1",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100035360.1.15",
          "form": "글루텐인가 생기면 안된댂는데ㅜ",
          "original_form": "글루텐인가 생기면 안된댂는데ㅜ",
          "speaker_id": "2",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100035360.1.16",
          "form": "그냥 감자 말가루.떡.뇨끼 라고 함시다",
          "original_form": "그냥 감자 말가루.떡.뇨끼 라고 함시다",
          "speaker_id": "1",
          "time": "20210829 15:39"
        },
        {
          "id": "MDRW2100035360.1.17",
          "form": "모찌급이요",
          "original_form": "모찌급이요",
          "speaker_id": "2",
          "time": "20210829 15:40"
        },
        {
          "id": "MDRW2100035360.1.18",
          "form": "안녕하새요",
          "original_form": "안녕하새요",
