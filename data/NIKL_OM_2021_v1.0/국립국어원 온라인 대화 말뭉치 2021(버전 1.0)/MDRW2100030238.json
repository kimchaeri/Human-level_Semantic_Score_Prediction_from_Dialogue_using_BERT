      "utterance": [
        {
          "id": "MDRW2100030238.1.1",
          "form": "피자가 먹고싶군요",
          "original_form": "피자가 먹고싶군요",
          "speaker_id": "1",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030238.1.2",
          "form": "맛집이에요",
          "original_form": "맛집이에요",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030238.1.3",
          "form": "커요맛나요",
          "original_form": "커요맛나요",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030238.1.4",
          "form": "아 야시장이요",
          "original_form": "아 야시장이요",
          "speaker_id": "1",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030238.1.5",
          "form": "옆쪽에 빵가게있는데",
          "original_form": "옆쪽에 빵가게있는데",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030238.1.6",
          "form": "저녁에만 파나요",
          "original_form": "저녁에만 파나요",
          "speaker_id": "1",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030238.1.7",
          "form": "복숭아 타르트 맛나보이더군요",
          "original_form": "복숭아 타르트 맛나보이더군요",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100030238.1.8",
          "form": "옛날빵집인가요",
          "original_form": "옛날빵집인가요",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030238.1.9",
          "form": "낮부터먹었어요",
          "original_form": "낮부터먹었어요",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030238.1.10",
          "form": "상투과자 좋아해요",
          "original_form": "상투과자 좋아해요",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030238.1.11",
          "form": "타르트팔고",
          "original_form": "타르트팔고",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030238.1.12",
          "form": "생크림도 올렸나요",
          "original_form": "생크림도 올렸나요",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030238.1.13",
          "form": "맛나보였어요",
          "original_form": "맛나보였어요",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030238.1.14",
          "form": "네생크림은진리에요",
          "original_form": "네생크림은진리에요",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030238.1.15",
          "form": "그렇군요 고고",
          "original_form": "그렇군요 고고",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030238.1.16",
          "form": "앞쪽에꽈배기",
          "original_form": "앞쪽에꽈배기",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030238.1.17",
          "form": "궁금한건 먹어야죠",
          "original_form": "궁금한건 먹어야죠",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030238.1.18",
          "form": "엄청큰거파는 가게도 있어요",
          "original_form": "엄청큰거파는 가게도 있어요",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030238.1.19",
          "form": "존맛이에요",
          "original_form": "존맛이에요",
          "speaker_id": "2",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100030238.1.20",
          "form": "생크림하니  파스타가 있지요",
          "original_form": "생크림하니  파스타가 있지요",
          "speaker_id": "1",
