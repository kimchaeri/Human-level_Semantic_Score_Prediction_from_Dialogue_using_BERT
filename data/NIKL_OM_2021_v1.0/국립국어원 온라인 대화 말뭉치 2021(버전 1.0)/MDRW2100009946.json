      "utterance": [
        {
          "id": "MDRW2100009946.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 12:02"
        },
        {
          "id": "MDRW2100009946.1.2",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210823 12:05"
        },
        {
          "id": "MDRW2100009946.1.3",
          "form": "자주 하시는 게임 장르가 있나요?",
          "original_form": "자주 하시는 게임 장르가 있나요?",
          "speaker_id": "2",
          "time": "20210823 12:06"
        },
        {
          "id": "MDRW2100009946.1.4",
          "form": "저 퍼즐게임 좋아해요",
          "original_form": "저 퍼즐게임 좋아해요",
          "speaker_id": "1",
          "time": "20210823 12:06"
        },
        {
          "id": "MDRW2100009946.1.5",
          "form": "저도 시간때울때 자주 해요",
          "original_form": "저도 시간때울때 자주 해요",
          "speaker_id": "2",
          "time": "20210823 12:08"
        },
        {
          "id": "MDRW2100009946.1.6",
          "form": "모바일 게임 자주 하시나요?",
          "original_form": "모바일 게임 자주 하시나요?",
          "speaker_id": "2",
          "time": "20210823 12:08"
        },
        {
          "id": "MDRW2100009946.1.7",
          "form": "넵 모바일 게임 엄청 많이 해요",
          "original_form": "넵 모바일 게임 엄청 많이 해요",
          "speaker_id": "1",
          "time": "20210823 12:09"
        },
        {
          "id": "MDRW2100009946.1.8",
          "form": "저두요. PC게임은 플레이하기 힘들어서ㅎ",
          "original_form": "저두요. PC게임은 플레이하기 힘들어서ㅎ",
          "speaker_id": "2",
          "time": "20210823 12:10"
        },
        {
          "id": "MDRW2100009946.1.9",
          "form": "PC가 제약도 많죠... 모바일은 가볍게 즐기기도 좋구",
          "original_form": "PC가 제약도 많죠... 모바일은 가볍게 즐기기도 좋구",
          "speaker_id": "1",
          "time": "20210823 12:14"
        },
        {
          "id": "MDRW2100009946.1.10",
          "form": "맞아요 그래도 보는건 PC게임이 재밌긴 하더라구요",
          "original_form": "맞아요 그래도 보는건 PC게임이 재밌긴 하더라구요",
          "speaker_id": "2",
          "time": "20210823 12:18"
        },
        {
          "id": "MDRW2100009946.1.11",
          "form": "e스포츠 리그 보신적 있나요?",
          "original_form": "e스포츠 리그 보신적 있나요?",
          "speaker_id": "2",
          "time": "20210823 12:19"
        },
        {
          "id": "MDRW2100009946.1.12",
          "form": "저 엄청 많이 봐요",
          "original_form": "저 엄청 많이 봐요",
          "speaker_id": "1",
          "time": "20210823 12:20"
        },
        {
          "id": "MDRW2100009946.1.13",
          "form": "배그도 보고 롤도 봅니다",
          "original_form": "배그도 보고 롤도 봅니다",
          "speaker_id": "1",
          "time": "20210823 12:21"
        },
        {
          "id": "MDRW2100009946.1.14",
          "form": "저는 가끔 찾아보기만 했어요",
          "original_form": "저는 가끔 찾아보기만 했어요",
          "speaker_id": "2",
          "time": "20210823 12:23"
        },
        {
          "id": "MDRW2100009946.1.15",
          "form": "카트라이더 리그를 본 적도 있어요",
          "original_form": "카트라이더 리그를 본 적도 있어요",
          "speaker_id": "2",
          "time": "20210823 12:23"
        },
        {
          "id": "MDRW2100009946.1.16",
          "form": "아 맞아요 카트도 잠깐 봤었어요",
          "original_form": "아 맞아요 카트도 잠깐 봤었어요",
          "speaker_id": "1",
          "time": "20210823 12:24"
        },
        {
          "id": "MDRW2100009946.1.17",
          "form": "문호준 선수 이번에 한화 나가고 직접 팀 꾸렸다고 들었는데",
          "original_form": "문호준 선수 이번에 한화 나가고 직접 팀 꾸렸다고 들었는데",
          "speaker_id": "1",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100009946.1.18",
          "form": "들었어요. 마지막 리그 경기는 봤거든요.",
          "original_form": "들었어요. 마지막 리그 경기는 봤거든요.",
          "speaker_id": "2",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100009946.1.19",
          "form": "리그 보면 게임 잘 하고싶은데 손이 따라 오진 않네요ㅋㅋ",
          "original_form": "리그 보면 게임 잘 하고싶은데 손이 따라 오진 않네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009946.1.20",
          "form": "저도 카트 해봤는데 진짜 감이 안 잡혀요",
          "original_form": "저도 카트 해봤는데 진짜 감이 안 잡혀요",
          "speaker_id": "1",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009946.1.21",
          "form": "선수들이 대단힌더라구요",
          "original_form": "선수들이 대단힌더라구요",
          "speaker_id": "1",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009946.1.22",
          "form": "프로게이머들 넘 굉장하져ㅎ",
          "original_form": "프로게이머들 넘 굉장하져ㅎ",
          "speaker_id": "2",
