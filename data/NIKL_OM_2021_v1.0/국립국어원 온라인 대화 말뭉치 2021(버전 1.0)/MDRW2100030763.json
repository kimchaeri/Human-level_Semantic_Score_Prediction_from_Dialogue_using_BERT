      "utterance": [
        {
          "id": "MDRW2100030763.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 12:21"
        },
        {
          "id": "MDRW2100030763.1.2",
          "form": "제 동생 지금 군인이에요! ㅋㅋㅋ",
          "original_form": "제 동생 지금 군인이에요! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:29"
        },
        {
          "id": "MDRW2100030763.1.3",
          "form": "앗 그러시구나",
          "original_form": "앗 그러시구나",
          "speaker_id": "1",
          "time": "20210904 22:16"
        },
        {
          "id": "MDRW2100030763.1.4",
          "form": "육군이신가요?",
          "original_form": "육군이신가요?",
          "speaker_id": "1",
          "time": "20210904 22:16"
        },
        {
          "id": "MDRW2100030763.1.5",
          "form": "넵넵 강원도에 있어용",
          "original_form": "넵넵 강원도에 있어용",
          "speaker_id": "2",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100030763.1.6",
          "form": "아휴 산이많은동네네요ㅠ",
          "original_form": "아휴 산이많은동네네요ㅠ",
          "speaker_id": "1",
          "time": "20210904 22:30"
        },
        {
          "id": "MDRW2100030763.1.7",
          "form": "저희오빠는의경갔었는데",
          "original_form": "저희오빠는의경갔었는데",
          "speaker_id": "1",
          "time": "20210904 22:30"
        },
        {
          "id": "MDRW2100030763.1.8",
          "form": "외박이많았어욯ㅎ",
          "original_form": "외박이많았어욯ㅎ",
          "speaker_id": "1",
          "time": "20210904 22:30"
        },
        {
          "id": "MDRW2100030763.1.9",
          "form": "아항 요즘은 시국이 이래서 외박은 아예 없더라구요ㅠ",
          "original_form": "아항 요즘은 시국이 이래서 외박은 아예 없더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100030763.1.10",
          "form": "휴가도 백일휴가 못나오고",
          "original_form": "휴가도 백일휴가 못나오고",
          "speaker_id": "2",
          "time": "20210904 22:40"
        },
        {
          "id": "MDRW2100030763.1.11",
          "form": "8개월만에 나왔었어요ㅠ",
          "original_form": "8개월만에 나왔었어요ㅠ",
          "speaker_id": "2",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100030763.1.12",
          "form": "아휴ㅜㅜ",
          "original_form": "아휴ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 22:41"
        },
        {
          "id": "MDRW2100030763.1.13",
          "form": "너무보고싶으시겠어요",
          "original_form": "너무보고싶으시겠어요",
          "speaker_id": "1",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100030763.1.14",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100030763.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100030763.1.16",
          "form": "동생이랔ㅋㅋㅋㅋㅋㅋ",
          "original_form": "동생이랔ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100030763.1.17",
          "form": "조금 안타깝지만 뭐",
          "original_form": "조금 안타깝지만 뭐",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100030763.1.18",
          "form": "어쩔수없죠",
          "original_form": "어쩔수없죠",
          "speaker_id": "2",
          "time": "20210904 22:42"
        },
        {
          "id": "MDRW2100030763.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ웃으면안되는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ웃으면안되는데",
          "speaker_id": "1",
          "time": "20210905 20:22"
        },
        {
          "id": "MDRW2100030763.1.20",
          "form": "제동생 그냥 안다쳤으면 좋겠어요ㅠ 그뿐",
          "original_form": "제동생 그냥 안다쳤으면 좋겠어요ㅠ 그뿐",
          "speaker_id": "2",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100030763.1.21",
          "form": "ㅠㅜ그쵸ㅠㅜ",
          "original_form": "ㅠㅜ그쵸ㅠㅜ",
          "speaker_id": "1",
          "time": "20210905 20:25"
        },
        {
          "id": "MDRW2100030763.1.22",
          "form": "몸건강히다녀와야할텐데",
          "original_form": "몸건강히다녀와야할텐데",
          "speaker_id": "1",
          "time": "20210905 20:26"
        },
        {
          "id": "MDRW2100030763.1.23",
          "form": "맞아용 그래도 훈련을 덜하는건 좋은것 같아요",
          "original_form": "맞아용 그래도 훈련을 덜하는건 좋은것 같아요",
          "speaker_id": "2",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100030763.1.24",
          "form": "코로나라서ㅠ",
          "original_form": "코로나라서ㅠ",
          "speaker_id": "2",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100030763.1.25",
          "form": "그치만 또 너무 외출을 못하는건 안타깝고 그렇네요 양날의 검같아요ㅠㅋㅋㅋ",
          "original_form": "그치만 또 너무 외출을 못하는건 안타깝고 그렇네요 양날의 검같아요ㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100030763.1.26",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "1",
          "time": "20210905 20:36"
        }
      ]
    }
