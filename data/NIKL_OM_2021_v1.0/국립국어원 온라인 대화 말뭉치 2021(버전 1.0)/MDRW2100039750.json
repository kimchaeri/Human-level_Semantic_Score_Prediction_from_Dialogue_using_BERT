      "utterance": [
        {
          "id": "MDRW2100039750.1.1",
          "form": "1만보걷기요 님은요?!",
          "original_form": "1만보걷기요 님은요?!",
          "speaker_id": "2",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100039750.1.2",
          "form": "저도 빨리걷기요",
          "original_form": "저도 빨리걷기요",
          "speaker_id": "1",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100039750.1.3",
          "form": "매일 걸으세요?",
          "original_form": "매일 걸으세요?",
          "speaker_id": "1",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100039750.1.4",
          "form": "저는 천천히 걸어서 그런가바요 ㅜㅜ",
          "original_form": "저는 천천히 걸어서 그런가바요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100039750.1.5",
          "form": "효과가 그냥 유지정도더라구요",
          "original_form": "효과가 그냥 유지정도더라구요",
          "speaker_id": "2",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039750.1.6",
          "form": "저도 유지정도예요ㅜ",
          "original_form": "저도 유지정도예요ㅜ",
          "speaker_id": "1",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039750.1.7",
          "form": "더 빡센운동이 필요한가봐요",
          "original_form": "더 빡센운동이 필요한가봐요",
          "speaker_id": "1",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039750.1.8",
          "form": "심박수를 올려야하는지 ㅜㅜ",
          "original_form": "심박수를 올려야하는지 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100039750.1.9",
          "form": "ㅅ그래서 실내자전거 사고싶어요",
          "original_form": "ㅅ그래서 실내자전거 사고싶어요",
          "speaker_id": "2",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100039750.1.10",
          "form": "저도 가지고싶은데 집이 좁아요ㅜ",
          "original_form": "저도 가지고싶은데 집이 좁아요ㅜ",
          "speaker_id": "1",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100039750.1.11",
          "form": "마자요  그리고 조립이 힘들데요",
          "original_form": "마자요  그리고 조립이 힘들데요",
          "speaker_id": "2",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039750.1.12",
          "form": "헉 조립도 해야되요?ㄷㄷ",
          "original_form": "헉 조립도 해야되요?ㄷㄷ",
          "speaker_id": "1",
          "time": "20210903 21:17"
        },
        {
          "id": "MDRW2100039750.1.13",
          "form": "다 분해되서 와요 ㅜㅜ",
          "original_form": "다 분해되서 와요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039750.1.14",
          "form": "몰랐어요ㅜㅜ",
          "original_form": "몰랐어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039750.1.15",
          "form": "완제품오는줄 ᆢ",
          "original_form": "완제품오는줄 ᆢ",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039750.1.16",
          "form": "그니까요 ㅜㅜ 집만 넓음 런닝머신도 재밌을거 같아요 ㅎㅎ",
          "original_form": "그니까요 ㅜㅜ 집만 넓음 런닝머신도 재밌을거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039750.1.17",
          "form": "맞아요 런닝머신은 진짜 가지고싶어요",
          "original_form": "맞아요 런닝머신은 진짜 가지고싶어요",
          "speaker_id": "1",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039750.1.18",
          "form": "근데 층간소음 있을까봐 ㅎㅎ",
          "original_form": "근데 층간소음 있을까봐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100039750.1.19",
          "form": "글네요 ㅎㅎ 1층만 가능할듯",
          "original_form": "글네요 ㅎㅎ 1층만 가능할듯",
