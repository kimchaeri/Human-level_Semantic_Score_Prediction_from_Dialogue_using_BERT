      "utterance": [
        {
          "id": "MDRW2100013506.1.1",
          "form": "여름에 여행 가신곳있으세요?",
          "original_form": "여름에 여행 가신곳있으세요?",
          "speaker_id": "1",
          "time": "20210825 15:10"
        },
        {
          "id": "MDRW2100013506.1.2",
          "form": "저는 아직 없어요",
          "original_form": "저는 아직 없어요",
          "speaker_id": "2",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013506.1.3",
          "form": "해외여행 가보셨어요?",
          "original_form": "해외여행 가보셨어요?",
          "speaker_id": "2",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013506.1.4",
          "form": "작년이후론 못갔어요",
          "original_form": "작년이후론 못갔어요",
          "speaker_id": "1",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013506.1.5",
          "form": "가고싶은데있으세요?",
          "original_form": "가고싶은데있으세요?",
          "speaker_id": "1",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013506.1.6",
          "form": "저는 일본 한번 가보고싶어요",
          "original_form": "저는 일본 한번 가보고싶어요",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013506.1.7",
          "form": "어디어디 가보셨어요?",
          "original_form": "어디어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210825 16:27"
        },
        {
          "id": "MDRW2100013506.1.8",
          "form": "일본 홍콩 가봤어요",
          "original_form": "일본 홍콩 가봤어요",
          "speaker_id": "1",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100013506.1.9",
          "form": "일본 어디 가고싶으세요?",
          "original_form": "일본 어디 가고싶으세요?",
          "speaker_id": "1",
          "time": "20210825 17:11"
        },
        {
          "id": "MDRW2100013506.1.10",
          "form": "저는 유니버셜스튜디오 한번 가보고 싶네요",
          "original_form": "저는 유니버셜스튜디오 한번 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100013506.1.11",
          "form": "그럼 오사카쪽?으로 가셔야겠어요",
          "original_form": "그럼 오사카쪽?으로 가셔야겠어요",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100013506.1.12",
          "form": "일본어는 잘하세요?",
          "original_form": "일본어는 잘하세요?",
          "speaker_id": "1",
          "time": "20210825 17:23"
        },
        {
          "id": "MDRW2100013506.1.13",
          "form": "일본어 하나도 못해요..",
          "original_form": "일본어 하나도 못해요..",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100013506.1.14",
          "form": "일본 어디 가보셨어요?",
          "original_form": "일본 어디 가보셨어요?",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100013506.1.15",
          "form": "히라가나 가타카나는 알아두시는게 편할거에요",
          "original_form": "히라가나 가타카나는 알아두시는게 편할거에요",
          "speaker_id": "1",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100013506.1.16",
          "form": "패키지면 상관없겠지만",
          "original_form": "패키지면 상관없겠지만",
          "speaker_id": "1",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100013506.1.17",
          "form": "저는 오키나와 가봤어요",
          "original_form": "저는 오키나와 가봤어요",
          "speaker_id": "1",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100013506.1.18",
          "form": "오키나와 가면 어디가 제일 좋으셨어요?",
          "original_form": "오키나와 가면 어디가 제일 좋으셨어요?",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100013506.1.19",
          "form": "바닷가가 제일 예뻐서 좋았어요 슈리성도 괜찮았는데 몇년전에 불타서 복구되었는지 모르겠네요.,",
          "original_form": "바닷가가 제일 예뻐서 좋았어요 슈리성도 괜찮았는데 몇년전에 불타서 복구되었는지 모르겠네요.,",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100013506.1.20",
          "form": "나중에 한번 가보세요 정말 좋았어요",
          "original_form": "나중에 한번 가보세요 정말 좋았어요",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100013506.1.21",
          "form": "일본가면 어떤 음식이 제일 좋으셨어요?",
          "original_form": "일본가면 어떤 음식이 제일 좋으셨어요?",
          "speaker_id": "2",
          "time": "20210825 17:53"
        },
        {
          "id": "MDRW2100013506.1.22",
          "form": "스시나 덮밥이 맛있었어요~",
          "original_form": "스시나 덮밥이 맛있었어요~",
          "speaker_id": "1",
          "time": "20210825 18:00"
        }
      ]
