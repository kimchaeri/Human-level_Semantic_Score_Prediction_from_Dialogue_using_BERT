      "utterance": [
        {
          "id": "MDRW2100007986.1.1",
          "form": "아하 출퇴근하시는거에요?",
          "original_form": "아하 출퇴근하시는거에요?",
          "speaker_id": "2",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007986.1.2",
          "form": "네ㅎㅎname2님은?",
          "original_form": "네ㅎㅎ&name2&님은?",
          "speaker_id": "1",
          "time": "20210822 12:33"
        },
        {
          "id": "MDRW2100007986.1.3",
          "form": "전 지방이에요ㅎㅎ",
          "original_form": "전 지방이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:37"
        },
        {
          "id": "MDRW2100007986.1.4",
          "form": "ㅎㅎ 그렇군요",
          "original_form": "ㅎㅎ 그렇군요",
          "speaker_id": "1",
          "time": "20210822 12:54"
        },
        {
          "id": "MDRW2100007986.1.5",
          "form": "자만추 하기 어려운 시기인듯요",
          "original_form": "자만추 하기 어려운 시기인듯요",
          "speaker_id": "1",
          "time": "20210822 12:54"
        },
        {
          "id": "MDRW2100007986.1.6",
          "form": "맞아요 일던 모임자체가 없으니까 사람자체를 만나기가 어려워서요 ㅎㅎ",
          "original_form": "맞아요 일던 모임자체가 없으니까 사람자체를 만나기가 어려워서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100007986.1.7",
          "form": "그러니까요 님을 봐야 뽕을 따는데",
          "original_form": "그러니까요 님을 봐야 뽕을 따는데",
          "speaker_id": "1",
          "time": "20210822 12:57"
        },
        {
          "id": "MDRW2100007986.1.8",
          "form": "ㅋㅋㅋㅋ이젠 혼자 보내는게 익숙해져서 연애 못하겠어요",
          "original_form": "ㅋㅋㅋㅋ이젠 혼자 보내는게 익숙해져서 연애 못하겠어요",
          "speaker_id": "2",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100007986.1.9",
          "form": "ㅎㅎ인연이 있겠죠",
          "original_form": "ㅎㅎ인연이 있겠죠",
          "speaker_id": "1",
          "time": "20210822 12:58"
        },
        {
          "id": "MDRW2100007986.1.10",
          "form": "맞아요 시국 핑계대지만 이 시국에도 만날 사람들은 만나니까요 ㅋㅋ",
          "original_form": "맞아요 시국 핑계대지만 이 시국에도 만날 사람들은 만나니까요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100007986.1.11",
          "form": "맞아요 전쟁통에도 사랑은 합니다ㅋㅋ",
          "original_form": "맞아요 전쟁통에도 사랑은 합니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100007986.1.12",
          "form": "ㅋㅋㅋ어떤 데이트 코스 선호하시나여",
          "original_form": "ㅋㅋㅋ어떤 데이트 코스 선호하시나여",
          "speaker_id": "2",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100007986.1.13",
          "form": "맛집이요!",
          "original_form": "맛집이요!",
          "speaker_id": "1",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100007986.1.14",
          "form": "오오 맛집투어 좋죠 겸사겸사 여행도 하구요 ㅎㅎ",
          "original_form": "오오 맛집투어 좋죠 겸사겸사 여행도 하구요 ㅎㅎ",
