        {
          "id": "MDRW2100005852.1.2",
          "form": "위드코로나!!!!!",
          "original_form": "위드코로나!!!!!",
          "speaker_id": "1",
          "time": "20210907 14:15"
        },
        {
          "id": "MDRW2100005852.1.3",
          "form": "백신은 다 맞았어요?",
          "original_form": "백신은 다 맞았어요?",
          "speaker_id": "1",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100005852.1.4",
          "form": "아직 1차만요",
          "original_form": "아직 1차만요",
          "speaker_id": "2",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100005852.1.5",
          "form": "다 맞으셧어요?",
          "original_form": "다 맞으셧어요?",
          "speaker_id": "2",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100005852.1.6",
          "form": "네 화이자 다맞았어요",
          "original_form": "네 화이자 다맞았어요",
          "speaker_id": "1",
          "time": "20210907 14:44"
        },
        {
          "id": "MDRW2100005852.1.7",
          "form": "전 마스크벗고 돌아다녀도..",
          "original_form": "전 마스크벗고 돌아다녀도..",
          "speaker_id": "1",
          "time": "20210907 14:44"
        },
        {
          "id": "MDRW2100005852.1.8",
          "form": "국가에서 허용해준다더니..ㅜㅜ",
          "original_form": "국가에서 허용해준다더니..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 14:45"
        },
        {
          "id": "MDRW2100005852.1.9",
          "form": "많이 아프셧나요?",
          "original_form": "많이 아프셧나요?",
          "speaker_id": "2",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100005852.1.10",
          "form": "2차맞고 사경을 헤맸죠",
          "original_form": "2차맞고 사경을 헤맸죠",
          "speaker_id": "1",
          "time": "20210907 15:07"
        },
        {
          "id": "MDRW2100005852.1.11",
          "form": "진짜 ㅡ.ㅡ",
          "original_form": "진짜 ㅡ.ㅡ",
          "speaker_id": "1",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100005852.1.12",
          "form": "몸살 설사 근육통",
          "original_form": "몸살 설사 근육통",
          "speaker_id": "1",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100005852.1.13",
          "form": "합병증으로 옵니다",
          "original_form": "합병증으로 옵니다",
          "speaker_id": "1",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100005852.1.14",
          "form": "ㄷㄷ...저는 곧 모더나 2차 맞아요",
          "original_form": "ㄷㄷ...저는 곧 모더나 2차 맞아요",
          "speaker_id": "2",
          "time": "20210907 15:29"
        },
        {
          "id": "MDRW2100005852.1.15",
          "form": "오 젤좋은거 맞으시네요",
          "original_form": "오 젤좋은거 맞으시네요",
          "speaker_id": "1",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100005852.1.16",
          "form": "1차만 맞앗는데도 꽤 고생햇엇는데 2차 두렵네요 ㅋㅋㅋㅋ",
          "original_form": "1차만 맞앗는데도 꽤 고생햇엇는데 2차 두렵네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:17"
        },
        {
          "id": "MDRW2100005852.1.17",
          "form": "모더나는 모르겠는데 화이자는 2차가 진짜 힘들었어요 ㅋㅋㅋ",
          "original_form": "모더나는 모르겠는데 화이자는 2차가 진짜 힘들었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 16:45"
        },
        {
          "id": "MDRW2100005852.1.18",
          "form": "화이자 2차 힘들엇다는 얘기 주변에서 많이 들엇어요",
          "original_form": "화이자 2차 힘들엇다는 얘기 주변에서 많이 들엇어요",
          "speaker_id": "2",
          "time": "20210907 17:33"
        },
        {
          "id": "MDRW2100005852.1.19",
          "form": "고생하셧어요",
          "original_form": "고생하셧어요",
          "speaker_id": "2",
          "time": "20210907 17:33"
        },
        {
          "id": "MDRW2100005852.1.20",
          "form": "ㅎㅎㅎ 타이레놀은 사놨어요?",
          "original_form": "ㅎㅎㅎ 타이레놀은 사놨어요?",
          "speaker_id": "1",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100005852.1.21",
          "form": "거기 주식을 사놨어야했어..ㅜㅜ",
          "original_form": "거기 주식을 사놨어야했어..ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100005852.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ네 사놧어요 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ네 사놧어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100005852.1.23",
          "form": "전 모더나 맞고 팔에 두드러기 나서 고생 ㅠㅠ",
          "original_form": "전 모더나 맞고 팔에 두드러기 나서 고생 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100005852.1.24",
          "form": "헐 피부질환이라니",
          "original_form": "헐 피부질환이라니",
          "speaker_id": "1",
          "time": "20210907 18:38"
        },
        {
          "id": "MDRW2100005852.1.25",
          "form": "2차맞고 가족들옆에 꼭붙어있어어ㅡ",
          "original_form": "2차맞고 가족들옆에 꼭붙어있어어ㅡ",
          "speaker_id": "1",
          "time": "20210907 18:39"
        },
        {
          "id": "MDRW2100005852.1.26",
          "form": "맞고 열흘 후에 갑자기 생기더라구요",
          "original_form": "맞고 열흘 후에 갑자기 생기더라구요",
          "speaker_id": "2",
          "time": "20210907 18:39"
        },
        {
          "id": "MDRW2100005852.1.27",
          "form": "맞은부위 주변으로",
          "original_form": "맞은부위 주변으로",
          "speaker_id": "2",
          "time": "20210907 18:39"
        },
        {
          "id": "MDRW2100005852.1.28",
          "form": "모더나 암(arm)이라고 부른대요",
          "original_form": "모더나 암(arm)이라고 부른대요",
          "speaker_id": "2",
          "time": "20210907 18:39"
        },
        {
          "id": "MDRW2100005852.1.29",
          "form": "그래도 괜찮아지신거죠?",
          "original_form": "그래도 괜찮아지신거죠?",
          "speaker_id": "1",
          "time": "20210907 18:48"
        },
        {
          "id": "MDRW2100005852.1.30",
          "form": "아휴 무슨일이야 그게ㅜㅜㅜ",
          "original_form": "아휴 무슨일이야 그게ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 18:48"
        },
        {
          "id": "MDRW2100005852.1.31",
          "form": "저녁은 드셨나요",
