      "utterance": [
        {
          "id": "MDRW2100025888.1.1",
          "form": "각각 하얗고 빨갛고 해요 ㅎㅎ",
          "original_form": "각각 하얗고 빨갛고 해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 17:41"
        },
        {
          "id": "MDRW2100025888.1.2",
          "form": "오 버전이 두갠가봐요",
          "original_form": "오 버전이 두갠가봐요",
          "speaker_id": "2",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025888.1.3",
          "form": "사골라면 맛있으니 후추 버전도 땡기네요",
          "original_form": "사골라면 맛있으니 후추 버전도 땡기네요",
          "speaker_id": "2",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025888.1.4",
          "form": "오늘 저녁 식사 메뉴는 뭔가요?",
          "original_form": "오늘 저녁 식사 메뉴는 뭔가요?",
          "speaker_id": "2",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025888.1.5",
          "form": "아직 고민중이에요!",
          "original_form": "아직 고민중이에요!",
          "speaker_id": "1",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100025888.1.6",
          "form": "지금은 전골이 먹고싶네요 ㅎㅎ",
          "original_form": "지금은 전골이 먹고싶네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100025888.1.7",
          "form": "저녁 뭐 드세요??",
          "original_form": "저녁 뭐 드세요??",
          "speaker_id": "1",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100025888.1.8",
          "form": "저는 꽃게를 장봐놔서",
          "original_form": "저는 꽃게를 장봐놔서",
          "speaker_id": "2",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100025888.1.9",
          "form": "꽃게를 먹을것같아요",
          "original_form": "꽃게를 먹을것같아요",
          "speaker_id": "2",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100025888.1.10",
          "form": "고게 지금 제철이래요",
          "original_form": "고게 지금 제철이래요",
          "speaker_id": "2",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100025888.1.11",
          "form": "통통하겠네요!",
          "original_form": "통통하겠네요!",
          "speaker_id": "1",
          "time": "20210901 17:56"
        },
        {
          "id": "MDRW2100025888.1.12",
          "form": "꽃게탕 해드실건가요??",
          "original_form": "꽃게탕 해드실건가요??",
          "speaker_id": "1",
          "time": "20210901 17:56"
        },
        {
          "id": "MDRW2100025888.1.13",
          "form": "조리법을 몰라서 그냥 쪄먹을까해요",
          "original_form": "조리법을 몰라서 그냥 쪄먹을까해요",
          "speaker_id": "2",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025888.1.14",
          "form": "찌는것도 검색해봐야해요 ㅋㅋ",
          "original_form": "찌는것도 검색해봐야해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025888.1.15",
          "form": "제철이라서 어떻게 해 먹어도 맛있을거에요!",
          "original_form": "제철이라서 어떻게 해 먹어도 맛있을거에요!",
          "speaker_id": "1",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100025888.1.16",
          "form": "찜이 더 간편할 것 같네요 ㅎㅎ",
          "original_form": "찜이 더 간편할 것 같네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100025888.1.17",
          "form": "글켓죠? 꽃게 라면도 맛있을것같아요 ㅎㅎ",
          "original_form": "글켓죠? 꽃게 라면도 맛있을것같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100025888.1.18",
          "form": "맞아요ㅋㅋㅋ 예능 보면 꽃게라면 많이 나오던데 꽃게 남으면 도전해보세요!",
          "original_form": "맞아요ㅋㅋㅋ 예능 보면 꽃게라면 많이 나오던데 꽃게 남으면 도전해보세요!",
          "speaker_id": "1",
          "time": "20210901 18:05"
        },
        {
          "id": "MDRW2100025888.1.19",
          "form": "배달이 질려서 이것저것 해먹게 되는것같아요",
          "original_form": "배달이 질려서 이것저것 해먹게 되는것같아요",
          "speaker_id": "2",
          "time": "20210901 18:07"
        },
        {
          "id": "MDRW2100025888.1.20",
          "form": "맞아요 사 먹는 것도 한두번이지 집밥이 제일 좋은 것 같아요",
          "original_form": "맞아요 사 먹는 것도 한두번이지 집밥이 제일 좋은 것 같아요",
          "speaker_id": "1",
