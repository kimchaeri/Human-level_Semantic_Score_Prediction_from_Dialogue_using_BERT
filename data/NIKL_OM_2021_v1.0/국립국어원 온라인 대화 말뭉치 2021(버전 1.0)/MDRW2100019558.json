      "utterance": [
        {
          "id": "MDRW2100019558.1.1",
          "form": "어릴 때 꿈과 지금 꿈이 같아?",
          "original_form": "어릴 때 꿈과 지금 꿈이 같아?",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019558.1.2",
          "form": "다르지",
          "original_form": "다르지",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019558.1.3",
          "form": "어떻게?",
          "original_form": "어떻게?",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019558.1.4",
          "form": "어릴땐 하고 싶은 직업이 많았어",
          "original_form": "어릴땐 하고 싶은 직업이 많았어",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019558.1.5",
          "form": "예를 들자면?",
          "original_form": "예를 들자면?",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019558.1.6",
          "form": "2개가 떠오르는데 일단 의사가 하고 싶었어",
          "original_form": "2개가 떠오르는데 일단 의사가 하고 싶었어",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019558.1.7",
          "form": "사람을 고쳐주고 싶었구나",
          "original_form": "사람을 고쳐주고 싶었구나",
          "speaker_id": "2",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100019558.1.8",
          "form": "맞아 이타적인 삶 멋있잖아",
          "original_form": "맞아 이타적인 삶 멋있잖아",
          "speaker_id": "1",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100019558.1.9",
          "form": "또 다른 하나는?",
          "original_form": "또 다른 하나는?",
          "speaker_id": "2",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100019558.1.10",
          "form": "파일럿!",
          "original_form": "파일럿!",
          "speaker_id": "1",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100019558.1.11",
          "form": "오?의왼데.",
          "original_form": "오?의왼데.",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019558.1.12",
          "form": "나 여행 좋아하잖아",
          "original_form": "나 여행 좋아하잖아",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019558.1.13",
          "form": "높은 것도 좋아해?",
          "original_form": "높은 것도 좋아해?",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019558.1.14",
          "form": "당연하지. 하지만 시력이 나빠서 포기했어",
          "original_form": "당연하지. 하지만 시력이 나빠서 포기했어",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019558.1.15",
          "form": "저런 안됐네",
          "original_form": "저런 안됐네",
