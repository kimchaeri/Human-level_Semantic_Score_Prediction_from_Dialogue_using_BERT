      "utterance": [
        {
          "id": "MDRW2100005447.1.1",
          "form": "여행 다니는 거 좋아해?",
          "original_form": "여행 다니는 거 좋아해?",
          "speaker_id": "1",
          "time": "20210821 22:26"
        },
        {
          "id": "MDRW2100005447.1.2",
          "form": "아니 난 집이 좋아ㅜ",
          "original_form": "아니 난 집이 좋아ㅜ",
          "speaker_id": "2",
          "time": "20210821 22:27"
        },
        {
          "id": "MDRW2100005447.1.3",
          "form": "집순이구나~~ 그럼 여행가는거 별로야?",
          "original_form": "집순이구나~~ 그럼 여행가는거 별로야?",
          "speaker_id": "1",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005447.1.4",
          "form": "별로는 아닌데 주도적으로 막 나가지는 않아ㅜ",
          "original_form": "별로는 아닌데 주도적으로 막 나가지는 않아ㅜ",
          "speaker_id": "2",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100005447.1.5",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210821 22:29"
        },
        {
          "id": "MDRW2100005447.1.6",
          "form": "나는 여행 순인데 요즘에는 너무 막무가내로 가는 것같아서",
          "original_form": "나는 여행 순인데 요즘에는 너무 막무가내로 가는 것같아서",
          "speaker_id": "1",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005447.1.7",
          "form": "올해부터 일년에 2번정도 길게 다니려고 하고 있어!",
          "original_form": "올해부터 일년에 2번정도 길게 다니려고 하고 있어!",
          "speaker_id": "1",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005447.1.8",
          "form": "돈도 많이 쓰게 돼ㅠㅠ",
          "original_form": "돈도 많이 쓰게 돼ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005447.1.9",
          "form": "ㅠㅠ 그건 그렇지",
          "original_form": "ㅠㅠ 그건 그렇지",
          "speaker_id": "2",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005447.1.10",
          "form": "요즘은 코로나 때문에 더 꺼려지는 것도 있고",
          "original_form": "요즘은 코로나 때문에 더 꺼려지는 것도 있고",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005447.1.11",
          "form": "그래서 여행 좀 줄이려고 ~~",
          "original_form": "그래서 여행 좀 줄이려고 ~~",
          "speaker_id": "1",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005447.1.12",
          "form": "코로나 끝나면 가볼까 생각중이야ㅋㅋㅋ",
          "original_form": "코로나 끝나면 가볼까 생각중이야ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005447.1.13",
          "form": "코로나 맞지 그래서 제주도 못갔어 올해ㅠㅠ",
          "original_form": "코로나 맞지 그래서 제주도 못갔어 올해ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005447.1.14",
          "form": "오 어디 가고 싶어?",
          "original_form": "오 어디 가고 싶어?",
          "speaker_id": "1",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005447.1.15",
          "form": "나 일본!",
          "original_form": "나 일본!",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005447.1.16",
          "form": "예전에 일본어를 배웠었는데 까먹기 전에 실전에서도 써보고 싶어가지고ㅋㅋ",
          "original_form": "예전에 일본어를 배웠었는데 까먹기 전에 실전에서도 써보고 싶어가지고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:20"
        },
        {
          "id": "MDRW2100005447.1.17",
          "form": "오 나 예전에 후쿠오카 가봤어!",
          "original_form": "오 나 예전에 후쿠오카 가봤어!",
          "speaker_id": "1",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005447.1.18",
          "form": "오 진짜? 어때?",
          "original_form": "오 진짜? 어때?",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005447.1.19",
          "form": "완전 좋아 ㅎㅎㅎㅎ 오사카 가고 싶었는데 후회 안해!",
          "original_form": "완전 좋아 ㅎㅎㅎㅎ 오사카 가고 싶었는데 후회 안해!",
          "speaker_id": "1",
          "time": "20210821 23:22"
        }
      ]
