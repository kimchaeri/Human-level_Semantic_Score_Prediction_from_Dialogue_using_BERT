      "utterance": [
        {
          "id": "MDRW2100020925.1.1",
          "form": "오늘 날씨 정말 좋아보여요",
          "original_form": "오늘 날씨 정말 좋아보여요",
          "speaker_id": "1",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100020925.1.2",
          "form": "거기는  맑나요?",
          "original_form": "거기는  맑나요?",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020925.1.3",
          "form": "네 저는 서울사는데 엄청 맑네요",
          "original_form": "네 저는 서울사는데 엄청 맑네요",
          "speaker_id": "1",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020925.1.4",
          "form": "여기는 아까 잠깐 해났다가 또 흐려졌어요ㅠ",
          "original_form": "여기는 아까 잠깐 해났다가 또 흐려졌어요ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100020925.1.5",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020925.1.6",
          "form": "오랜만에 맑은거같아요",
          "original_form": "오랜만에 맑은거같아요",
          "speaker_id": "2",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020925.1.7",
          "form": "바람이 좀 불었으면 좋겠어요",
          "original_form": "바람이 좀 불었으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020925.1.8",
          "form": "여기는 흐려서 그런지 시원해요",
          "original_form": "여기는 흐려서 그런지 시원해요",
          "speaker_id": "2",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020925.1.9",
          "form": "바람이 안부는군요ㅠ",
          "original_form": "바람이 안부는군요ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020925.1.10",
          "form": "부러워요ㅠ",
          "original_form": "부러워요ㅠ",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020925.1.11",
          "form": "덥나요?",
          "original_form": "덥나요?",
          "speaker_id": "2",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100020925.1.12",
          "form": "여긴 좀 땀나요",
          "original_form": "여긴 좀 땀나요",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020925.1.13",
          "form": "해쬐러나가야하는데",
          "original_form": "해쬐러나가야하는데",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020925.1.14",
          "form": "마자요 해날때 햇빛 좀  쬐야하는데",
          "original_form": "마자요 해날때 햇빛 좀  쬐야하는데",
          "speaker_id": "2",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100020925.1.15",
          "form": "더워서 못나가시겠네요ㅠ",
          "original_form": "더워서 못나가시겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020925.1.16",
          "form": "저녁때 나가려고요!",
          "original_form": "저녁때 나가려고요!",
          "speaker_id": "1",
          "time": "20210828 14:25"
        },
        {
          "id": "MDRW2100020925.1.17",
          "form": "공원으로 걷기운동하러!",
          "original_form": "공원으로 걷기운동하러!",
          "speaker_id": "1",
          "time": "20210828 14:25"
        }
      ]
