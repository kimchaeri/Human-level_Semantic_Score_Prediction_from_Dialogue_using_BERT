      "utterance": [
        {
          "id": "MDRW2100000552.1.1",
          "form": "자긴 종교 없지?",
          "original_form": "자긴 종교 없지?",
          "speaker_id": "1",
          "time": "20210819 16:49"
        },
        {
          "id": "MDRW2100000552.1.2",
          "form": "난 없어~~",
          "original_form": "난 없어~~",
          "speaker_id": "2",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000552.1.3",
          "form": "자기는???",
          "original_form": "자기는???",
          "speaker_id": "2",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000552.1.4",
          "form": "나도 없엉ㅋ\r\n자기 부모님도 무교셔?",
          "original_form": "나도 없엉ㅋ\r\n자기 부모님도 무교셔?",
          "speaker_id": "1",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000552.1.5",
          "form": "응. 절도 안감ㅎ",
          "original_form": "응. 절도 안감ㅎ",
          "speaker_id": "2",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000552.1.6",
          "form": "울집은  할아버지때까진 불교였던거같은데 부모님은 그냥그러신거같더라",
          "original_form": "울집은  할아버지때까진 불교였던거같은데 부모님은 그냥그러신거같더라",
          "speaker_id": "1",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000552.1.7",
          "form": "나 어릴때 할아버지따라 절에 가본적은 있엉",
          "original_form": "나 어릴때 할아버지따라 절에 가본적은 있엉",
          "speaker_id": "1",
          "time": "20210819 16:58"
        },
        {
          "id": "MDRW2100000552.1.8",
          "form": "난 중학교때 교회 잠시 갔었는데 그건 친구따라 놀러간거ㅋ",
          "original_form": "난 중학교때 교회 잠시 갔었는데 그건 친구따라 놀러간거ㅋ",
          "speaker_id": "2",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000552.1.9",
          "form": "앗 나도 초등학교때ㅋ",
          "original_form": "앗 나도 초등학교때ㅋ",
          "speaker_id": "1",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000552.1.10",
          "form": "초코파이 먹으러 많이 갔었지",
          "original_form": "초코파이 먹으러 많이 갔었지",
          "speaker_id": "1",
          "time": "20210819 16:59"
        },
        {
          "id": "MDRW2100000552.1.11",
          "form": "종교에 대해선 상당히 부정적",
          "original_form": "종교에 대해선 상당히 부정적",
          "speaker_id": "2",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000552.1.12",
          "form": "나도 별로 좋아하진 않아\r\n힘든 사람들 꾀어내는거 같아서",
          "original_form": "나도 별로 좋아하진 않아\r\n힘든 사람들 꾀어내는거 같아서",
          "speaker_id": "1",
          "time": "20210819 17:02"
        },
        {
          "id": "MDRW2100000552.1.13",
          "form": "힘든 사람이 종교로 힘을 얻으면 좋지~",
          "original_form": "힘든 사람이 종교로 힘을 얻으면 좋지~",
          "speaker_id": "2",
          "time": "20210819 17:02"
        },
        {
          "id": "MDRW2100000552.1.14",
          "form": "근데 이미 종교자체가 하나의 기업이야",
          "original_form": "근데 이미 종교자체가 하나의 기업이야",
          "speaker_id": "2",
          "time": "20210819 17:02"
        },
        {
          "id": "MDRW2100000552.1.15",
          "form": "허긴 그렇지\r\n근데 힘든 사람들 사이비에 빠지는게 걱정이지",
          "original_form": "허긴 그렇지\r\n근데 힘든 사람들 사이비에 빠지는게 걱정이지",
          "speaker_id": "1",
          "time": "20210819 17:04"
        },
        {
          "id": "MDRW2100000552.1.16",
          "form": "사이비가 있는게 문제지ㅋㅋ",
          "original_form": "사이비가 있는게 문제지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000552.1.17",
          "form": "ㅋㅋ 맞아\r\n근데 왜 난 주변이 왠만하면 다 사이비같지?",
          "original_form": "ㅋㅋ 맞아\r\n근데 왜 난 주변이 왠만하면 다 사이비같지?",
          "speaker_id": "1",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000552.1.18",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:10"
        },
        {
          "id": "MDRW2100000552.1.19",
          "form": "다 사이비로 보일정도로 변질됐다는 의미겠지~",
          "original_form": "다 사이비로 보일정도로 변질됐다는 의미겠지~",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000552.1.20",
          "form": "그렇지\r\n거의  모든 종교가 돈이야",
          "original_form": "그렇지\r\n거의  모든 종교가 돈이야",
