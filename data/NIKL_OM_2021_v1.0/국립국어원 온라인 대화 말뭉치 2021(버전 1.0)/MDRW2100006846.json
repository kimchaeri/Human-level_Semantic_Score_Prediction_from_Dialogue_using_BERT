      "utterance": [
        {
          "id": "MDRW2100006846.1.1",
          "form": "디저트 안먹습니다..ㅎ",
          "original_form": "디저트 안먹습니다..ㅎ",
          "speaker_id": "2",
          "time": "20210908 10:44"
        },
        {
          "id": "MDRW2100006846.1.2",
          "form": "앗 아예 안드세요??",
          "original_form": "앗 아예 안드세요??",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006846.1.3",
          "form": "저는 엄청 좋아해요 ㅋㅋㅋ",
          "original_form": "저는 엄청 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:45"
        },
        {
          "id": "MDRW2100006846.1.4",
          "form": "네 간식을 안좋아해요",
          "original_form": "네 간식을 안좋아해요",
          "speaker_id": "2",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100006846.1.5",
          "form": "헉 저는 과자도 좋아하구 ,, 음료수도 좋아하구,, 호호...",
          "original_form": "헉 저는 과자도 좋아하구 ,, 음료수도 좋아하구,, 호호...",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100006846.1.6",
          "form": "아이스크림도 좋아합니다.",
          "original_form": "아이스크림도 좋아합니다.",
          "speaker_id": "1",
          "time": "20210908 10:46"
        },
        {
          "id": "MDRW2100006846.1.7",
          "form": "ㅡ와이프가 좋아해요 ㅎㅎ",
          "original_form": "ㅡ와이프가 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:47"
        },
        {
          "id": "MDRW2100006846.1.8",
          "form": "앗 같이는 안드시는거예여? 흠 달달한거랑 안맞으시나봐요",
          "original_form": "앗 같이는 안드시는거예여? 흠 달달한거랑 안맞으시나봐요",
          "speaker_id": "1",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006846.1.9",
          "form": "네 안좋아해요 ㅎㅎ",
          "original_form": "네 안좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:48"
        },
        {
          "id": "MDRW2100006846.1.10",
          "form": "오옹 이런친구 있으면",
          "original_form": "오옹 이런친구 있으면",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006846.1.11",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006846.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ제가 다먹을수 있구",
          "original_form": "ㅋㅋㅋㅋㅋㅋ제가 다먹을수 있구",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006846.1.13",
          "form": "맞네요 ㅎㅎ 어디 디저트 좋아하세요",
          "original_form": "맞네요 ㅎㅎ 어디 디저트 좋아하세요",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100006846.1.14",
          "form": "저는 음 어디 디저트를 가리진 않지만,, 민초파에요 ㅋㅋㅋ",
          "original_form": "저는 음 어디 디저트를 가리진 않지만,, 민초파에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:51"
        },
        {
          "id": "MDRW2100006846.1.15",
          "form": "아...민초..저는 반민초..",
          "original_form": "아...민초..저는 반민초..",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100006846.1.16",
          "form": "ㅋㅋㅋㅋ앗 그러시구낭 ㅠㅠ 민초 좋아해요... 디저트는 민촣ㅎ...",
          "original_form": "ㅋㅋㅋㅋ앗 그러시구낭 ㅠㅠ 민초 좋아해요... 디저트는 민촣ㅎ...",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100006846.1.17",
          "form": "빼빼로 이런 초코 과자도 좋아하구...그렇슴니당",
          "original_form": "빼빼로 이런 초코 과자도 좋아하구...그렇슴니당",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100006846.1.18",
          "form": "스벅 민초 음료 드셔보셨나요",
          "original_form": "스벅 민초 음료 드셔보셨나요",
