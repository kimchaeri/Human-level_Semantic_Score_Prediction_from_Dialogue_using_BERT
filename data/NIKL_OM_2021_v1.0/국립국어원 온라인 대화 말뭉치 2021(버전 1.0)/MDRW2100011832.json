      "utterance": [
        {
          "id": "MDRW2100011832.1.1",
          "form": "진짜 이렇게보니까",
          "original_form": "진짜 이렇게보니까",
          "speaker_id": "2",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100011832.1.2",
          "form": "놀이공원으 치마입고간다는게 정말 해괴하네요",
          "original_form": "놀이공원으 치마입고간다는게 정말 해괴하네요",
          "speaker_id": "1",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100011832.1.3",
          "form": "연애가 하나의 문화네요",
          "original_form": "연애가 하나의 문화네요",
          "speaker_id": "2",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100011832.1.4",
          "form": "맞아요 정말 기괴해요",
          "original_form": "맞아요 정말 기괴해요",
          "speaker_id": "2",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100011832.1.5",
          "form": "맞아요 되게 이걸 둘러싼 기괴한 문화산업이 많은거같아요",
          "original_form": "맞아요 되게 이걸 둘러싼 기괴한 문화산업이 많은거같아요",
          "speaker_id": "1",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100011832.1.6",
          "form": "아 커플운동화랑 커플패딩맞추고",
          "original_form": "아 커플운동화랑 커플패딩맞추고",
          "speaker_id": "1",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100011832.1.7",
          "form": "그리고 돈도 되니까",
          "original_form": "그리고 돈도 되니까",
          "speaker_id": "2",
          "time": "20210824 16:25"
        },
        {
          "id": "MDRW2100011832.1.8",
          "form": "무슨 분신술쓰는것도아니고",
          "original_form": "무슨 분신술쓰는것도아니고",
          "speaker_id": "1",
          "time": "20210824 16:26"
        },
        {
          "id": "MDRW2100011832.1.9",
          "form": "맞아요 커플템",
          "original_form": "맞아요 커플템",
          "speaker_id": "2",
          "time": "20210824 16:26"
        },
        {
          "id": "MDRW2100011832.1.10",
          "form": "분신술ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "분신술ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:26"
        },
        {
          "id": "MDRW2100011832.1.11",
          "form": "커플템 맞추면 또 돈 들고 그렇죠",
          "original_form": "커플템 맞추면 또 돈 들고 그렇죠",
          "speaker_id": "2",
          "time": "20210824 16:26"
        },
        {
          "id": "MDRW2100011832.1.12",
          "form": "아 진짜 알바해서 연애에 돈 쓰는거 아까워요",
          "original_form": "아 진짜 알바해서 연애에 돈 쓰는거 아까워요",
          "speaker_id": "2",
          "time": "20210824 16:26"
        },
        {
          "id": "MDRW2100011832.1.13",
          "form": "커플한테 장사하면 돈 진짜 많이 되는거같아요",
          "original_form": "커플한테 장사하면 돈 진짜 많이 되는거같아요",
          "speaker_id": "1",
          "time": "20210824 16:26"
        },
        {
          "id": "MDRW2100011832.1.14",
          "form": "하이고 알바해서 몇푼번다고",
          "original_form": "하이고 알바해서 몇푼번다고",
          "speaker_id": "1",
          "time": "20210824 16:26"
        },
        {
          "id": "MDRW2100011832.1.15",
          "form": "영화관 커플석도 좀 기괴해요",
          "original_form": "영화관 커플석도 좀 기괴해요",
          "speaker_id": "2",
          "time": "20210824 16:27"
        },
        {
          "id": "MDRW2100011832.1.16",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210824 16:27"
        },
        {
          "id": "MDRW2100011832.1.17",
          "form": "따지고보면 스킨십 쉽게 하려고 그러는거잖아요",
          "original_form": "따지고보면 스킨십 쉽게 하려고 그러는거잖아요",
          "speaker_id": "2",
          "time": "20210824 16:27"
        },
        {
          "id": "MDRW2100011832.1.18",
          "form": "다른사람 영화보는데 왜...",
          "original_form": "다른사람 영화보는데 왜...",
          "speaker_id": "2",
          "time": "20210824 16:27"
        },
        {
          "id": "MDRW2100011832.1.19",
          "form": "헐 정말 역겹네요",
          "original_form": "헐 정말 역겹네요",
