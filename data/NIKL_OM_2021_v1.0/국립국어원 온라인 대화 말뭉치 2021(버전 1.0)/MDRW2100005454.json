      "utterance": [
        {
          "id": "MDRW2100005454.1.1",
          "form": "최근에 산 물건은?",
          "original_form": "최근에 산 물건은?",
          "speaker_id": "1",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005454.1.2",
          "form": "나는 오타쿠... 물건을 샀습니다",
          "original_form": "나는 오타쿠... 물건을 샀습니다",
          "speaker_id": "1",
          "time": "20210821 22:28"
        },
        {
          "id": "MDRW2100005454.1.3",
          "form": "최근에 산 물건?",
          "original_form": "최근에 산 물건?",
          "speaker_id": "2",
          "time": "20210821 22:30"
        },
        {
          "id": "MDRW2100005454.1.4",
          "form": "어 뭐샀지",
          "original_form": "어 뭐샀지",
          "speaker_id": "2",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005454.1.5",
          "form": "약..?",
          "original_form": "약..?",
          "speaker_id": "2",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005454.1.6",
          "form": "옷..?",
          "original_form": "옷..?",
          "speaker_id": "2",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005454.1.7",
          "form": "오타쿠 물건도 두달전쯤 산 듯",
          "original_form": "오타쿠 물건도 두달전쯤 산 듯",
          "speaker_id": "2",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005454.1.8",
          "form": "물욕이 없나요",
          "original_form": "물욕이 없나요",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005454.1.9",
          "form": "최근에 뭐 산 게 없나봐",
          "original_form": "최근에 뭐 산 게 없나봐",
          "speaker_id": "1",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005454.1.10",
          "form": "물욕은 넘치는데",
          "original_form": "물욕은 넘치는데",
          "speaker_id": "2",
          "time": "20210821 22:32"
        },
        {
          "id": "MDRW2100005454.1.11",
          "form": "내가 뭘 사고 싶어했는지 까먹네요",
          "original_form": "내가 뭘 사고 싶어했는지 까먹네요",
          "speaker_id": "2",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005454.1.12",
          "form": "그러면 지금 사고 싶은 물건은?",
          "original_form": "그러면 지금 사고 싶은 물건은?",
          "speaker_id": "1",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005454.1.13",
          "form": "폰 거치대..",
          "original_form": "폰 거치대..",
          "speaker_id": "2",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005454.1.14",
          "form": "폰 들고 있으려니 불편쓰하네요",
          "original_form": "폰 들고 있으려니 불편쓰하네요",
          "speaker_id": "2",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005454.1.15",
          "form": "나는 갤럭시 워치 4...",
          "original_form": "나는 갤럭시 워치 4...",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005454.1.16",
          "form": "별로 쓸 일 없는 거 아는데",
          "original_form": "별로 쓸 일 없는 거 아는데",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005454.1.17",
          "form": "그냥 간지용으로 사고 싶음",
          "original_form": "그냥 간지용으로 사고 싶음",
          "speaker_id": "1",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005454.1.18",
          "form": "수입 없는 지금은 무리고 나중에 일 시작하고 수입생기면 그때 최신 모델로 사려고 생각 중",
          "original_form": "수입 없는 지금은 무리고 나중에 일 시작하고 수입생기면 그때 최신 모델로 사려고 생각 중",
          "speaker_id": "1",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005454.1.19",
          "form": "오 워치4...",
          "original_form": "오 워치4...",
          "speaker_id": "2",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005454.1.20",
          "form": "보통 운동하면 쓰지 않나",
          "original_form": "보통 운동하면 쓰지 않나",
          "speaker_id": "2",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005454.1.21",
          "form": "내가 운동을 안 하니까 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "내가 운동을 안 하니까 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005454.1.22",
          "form": "워치4는 인바디 기능도 있대서",
          "original_form": "워치4는 인바디 기능도 있대서",
          "speaker_id": "1",
          "time": "20210821 22:40"
        },
        {
          "id": "MDRW2100005454.1.23",
          "form": "혹했지",
          "original_form": "혹했지",
          "speaker_id": "1",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005454.1.24",
          "form": "인바디라니",
          "original_form": "인바디라니",
          "speaker_id": "2",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005454.1.25",
          "form": "무섭다",
          "original_form": "무섭다",
          "speaker_id": "2",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005454.1.26",
          "form": "아이폰 쓰는 친구들도 그거 보고 혹했는데",
          "original_form": "아이폰 쓰는 친구들도 그거 보고 혹했는데",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005454.1.27",
          "form": "애플이랑 갤럭시워치랑 호환 안 되는 부분들이 좀 있어서 포기하더라",
          "original_form": "애플이랑 갤럭시워치랑 호환 안 되는 부분들이 좀 있어서 포기하더라",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005454.1.28",
          "form": "호환...",
          "original_form": "호환...",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005454.1.29",
          "form": "중요하지",
          "original_form": "중요하지",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005454.1.30",
          "form": "내가 이럴 줄 알고",
          "original_form": "내가 이럴 줄 알고",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005454.1.31",
          "form": "갤럭시로 바꿨지",
          "original_form": "갤럭시로 바꿨지",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005454.1.32",
          "form": "그래...",
          "original_form": "그래...",
          "speaker_id": "1",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005454.1.33",
          "form": "핸드폰 바꾼지 얼마나 됐더라",
          "original_form": "핸드폰 바꾼지 얼마나 됐더라",
          "speaker_id": "1",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005454.1.34",
          "form": "다음에도 갤럭시폰 쓰실 건지?",
          "original_form": "다음에도 갤럭시폰 쓰실 건지?",
          "speaker_id": "1",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005454.1.35",
          "form": "이미 반년이 넘었습니다",
          "original_form": "이미 반년이 넘었습니다",
          "speaker_id": "2",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005454.1.36",
          "form": "당빠 갤럭시죠",
          "original_form": "당빠 갤럭시죠",
          "speaker_id": "2",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005454.1.37",
          "form": "반년이나 됐냐... 시간 머선일이고",
          "original_form": "반년이나 됐냐... 시간 머선일이고",
          "speaker_id": "1",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005454.1.38",
          "form": "나는 수능 끝나고 바꿨던 폰 아직 쓰고 있는데",
          "original_form": "나는 수능 끝나고 바꿨던 폰 아직 쓰고 있는데",
          "speaker_id": "1",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005454.1.39",
          "form": "아마 취업할 때 쯤에 새 폰 살 거 같음",
          "original_form": "아마 취업할 때 쯤에 새 폰 살 거 같음",
          "speaker_id": "1",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005454.1.40",
          "form": "취업...?",
          "original_form": "취업...?",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005454.1.41",
          "form": "정말 휴대폰 사면",
          "original_form": "정말 휴대폰 사면",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005454.1.42",
          "form": "오래 쓰는군요",
          "original_form": "오래 쓰는군요",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005454.1.43",
          "form": "원래 2년 주기로 바꾸는 편인데",
          "original_form": "원래 2년 주기로 바꾸는 편인데",
          "speaker_id": "1",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005454.1.44",
          "form": "약정 끝나고도 반년이 지나서",
          "original_form": "약정 끝나고도 반년이 지나서",
          "speaker_id": "1",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005454.1.45",
          "form": "그냥 이따가 바꾸자 싶어졌음",
          "original_form": "그냥 이따가 바꾸자 싶어졌음",
          "speaker_id": "1",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005454.1.46",
          "form": "바꿀 필요성을 못느끼는 건가",
          "original_form": "바꿀 필요성을 못느끼는 건가",
          "speaker_id": "2",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005454.1.47",
          "form": "나도 근데 약정 끝날때까진",
          "original_form": "나도 근데 약정 끝날때까진",
          "speaker_id": "2",
          "time": "20210821 22:48"
        },
        {
          "id": "MDRW2100005454.1.48",
          "form": "별로 바꾸고 싶진 않았음",
          "original_form": "별로 바꾸고 싶진 않았음",
          "speaker_id": "2",
          "time": "20210821 22:48"
        }
      ]
