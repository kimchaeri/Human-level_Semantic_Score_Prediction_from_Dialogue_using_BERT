      "utterance": [
        {
          "id": "MDRW2100039168.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210903 12:15"
        },
        {
          "id": "MDRW2100039168.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210903 12:28"
        },
        {
          "id": "MDRW2100039168.1.3",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "2",
          "time": "20210903 12:28"
        },
        {
          "id": "MDRW2100039168.1.4",
          "form": "시리얼먹었어요",
          "original_form": "시리얼먹었어요",
          "speaker_id": "1",
          "time": "20210903 12:29"
        },
        {
          "id": "MDRW2100039168.1.5",
          "form": "시리얼로 되시나요?",
          "original_form": "시리얼로 되시나요?",
          "speaker_id": "2",
          "time": "20210903 12:30"
        },
        {
          "id": "MDRW2100039168.1.6",
          "form": "저는 구내식당에서 먹었어요",
          "original_form": "저는 구내식당에서 먹었어요",
          "speaker_id": "2",
          "time": "20210903 12:30"
        },
        {
          "id": "MDRW2100039168.1.7",
          "form": "오늘 간만에 탕수육이라 맛나게 먹었네요 ㅎ",
          "original_form": "오늘 간만에 탕수육이라 맛나게 먹었네요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 12:31"
        },
        {
          "id": "MDRW2100039168.1.8",
          "form": "배고프긴한데 계속앉아있으니 소화가 안되서요",
          "original_form": "배고프긴한데 계속앉아있으니 소화가 안되서요",
          "speaker_id": "1",
          "time": "20210903 12:31"
        },
        {
          "id": "MDRW2100039168.1.9",
          "form": "아...활동량이 적으신가부다...",
          "original_form": "아...활동량이 적으신가부다...",
          "speaker_id": "2",
          "time": "20210903 12:32"
        },
        {
          "id": "MDRW2100039168.1.10",
          "form": "저희는 오늘 탕수육이라 그런가 식당에 사람들이 많더라구요",
          "original_form": "저희는 오늘 탕수육이라 그런가 식당에 사람들이 많더라구요",
          "speaker_id": "2",
          "time": "20210903 12:33"
        },
        {
          "id": "MDRW2100039168.1.11",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 12:34"
        },
        {
          "id": "MDRW2100039168.1.12",
          "form": "탕슉좋네요\r\n전 커피는 못마셔요",
          "original_form": "탕슉좋네요\r\n전 커피는 못마셔요",
          "speaker_id": "1",
          "time": "20210903 12:35"
        },
        {
          "id": "MDRW2100039168.1.13",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "2",
          "time": "20210903 12:35"
        },
        {
          "id": "MDRW2100039168.1.14",
          "form": "카페인때문에요?",
          "original_form": "카페인때문에요?",
          "speaker_id": "2",
          "time": "20210903 12:36"
        },
        {
          "id": "MDRW2100039168.1.15",
          "form": "네 가슴이 두근거려요\r\n좋아하시나요",
          "original_form": "네 가슴이 두근거려요\r\n좋아하시나요",
          "speaker_id": "1",
          "time": "20210903 12:36"
        },
        {
          "id": "MDRW2100039168.1.16",
          "form": "저는 하루에 2잔은 마시는듯해요 ㅎ",
          "original_form": "저는 하루에 2잔은 마시는듯해요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 12:36"
        },
        {
          "id": "MDRW2100039168.1.17",
          "form": "오전에는 좀 달달하게 오후에는 아메스타일로 마셔요",
          "original_form": "오전에는 좀 달달하게 오후에는 아메스타일로 마셔요",
          "speaker_id": "2",
          "time": "20210903 12:37"
        },
        {
          "id": "MDRW2100039168.1.18",
          "form": "요즘은 많이들 마시더라고요\r\n카페도 대용량팔고ㅋ",
          "original_form": "요즘은 많이들 마시더라고요\r\n카페도 대용량팔고ㅋ",
          "speaker_id": "1",
          "time": "20210903 12:37"
        },
        {
          "id": "MDRW2100039168.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 12:37"
        },
        {
          "id": "MDRW2100039168.1.20",
          "form": "그럼 물많이 드세요?",
          "original_form": "그럼 물많이 드세요?",
          "speaker_id": "2",
          "time": "20210903 12:37"
        },
        {
          "id": "MDRW2100039168.1.21",
          "form": "마시려고 노력은하는데 잘안되욬ㅋ",
          "original_form": "마시려고 노력은하는데 잘안되욬ㅋ",
          "speaker_id": "1",
          "time": "20210903 12:38"
        },
        {
          "id": "MDRW2100039168.1.22",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210903 12:38"
        },
        {
          "id": "MDRW2100039168.1.23",
          "form": "하루에 1리터? 마시라는데",
          "original_form": "하루에 1리터? 마시라는데",
          "speaker_id": "2",
          "time": "20210903 12:38"
        },
        {
          "id": "MDRW2100039168.1.24",
          "form": "500도 안먹는거같아요",
          "original_form": "500도 안먹는거같아요",
          "speaker_id": "2",
          "time": "20210903 12:38"
        },
        {
          "id": "MDRW2100039168.1.25",
          "form": "텀블러 세컵마셔야지 하는데 한컵도 겨우ㅋ",
          "original_form": "텀블러 세컵마셔야지 하는데 한컵도 겨우ㅋ",
          "speaker_id": "1",
          "time": "20210903 12:38"
        }
      ]
    }
