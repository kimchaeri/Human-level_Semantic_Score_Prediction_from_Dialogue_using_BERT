      "utterance": [
        {
          "id": "MDRW2100005853.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 14:18"
        },
        {
          "id": "MDRW2100005853.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210907 14:19"
        },
        {
          "id": "MDRW2100005853.1.3",
          "form": "ㅎㅎ네 혹시 공연같은거 좋아하세요?",
          "original_form": "ㅎㅎ네 혹시 공연같은거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 14:19"
        },
        {
          "id": "MDRW2100005853.1.4",
          "form": "네 좋아하는 가수 공연 자주 가요",
          "original_form": "네 좋아하는 가수 공연 자주 가요",
          "speaker_id": "2",
          "time": "20210907 14:22"
        },
        {
          "id": "MDRW2100005853.1.5",
          "form": "오 좋아하는가수가 누구인데요?",
          "original_form": "오 좋아하는가수가 누구인데요?",
          "speaker_id": "1",
          "time": "20210907 14:25"
        },
        {
          "id": "MDRW2100005853.1.6",
          "form": "인디 좋아해서 좀 많아요 ㅋㅋㅋ",
          "original_form": "인디 좋아해서 좀 많아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:28"
        },
        {
          "id": "MDRW2100005853.1.7",
          "form": "오 인디좋아하시는구나",
          "original_form": "오 인디좋아하시는구나",
          "speaker_id": "1",
          "time": "20210907 14:28"
        },
        {
          "id": "MDRW2100005853.1.8",
          "form": "추천해주실만한 밴드 있으신가요",
          "original_form": "추천해주실만한 밴드 있으신가요",
          "speaker_id": "1",
          "time": "20210907 14:28"
        },
        {
          "id": "MDRW2100005853.1.9",
          "form": "새소년이요",
          "original_form": "새소년이요",
          "speaker_id": "2",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100005853.1.10",
          "form": "아 나중에한번",
          "original_form": "아 나중에한번",
          "speaker_id": "1",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100005853.1.11",
          "form": "꼭 앨범들어볼게요 ㅎ",
          "original_form": "꼭 앨범들어볼게요 ㅎ",
          "speaker_id": "1",
          "time": "20210907 14:30"
        },
        {
          "id": "MDRW2100005853.1.12",
          "form": "저도 코로나 이전엔 공연같은걸 많이보러 간 것 같은데",
          "original_form": "저도 코로나 이전엔 공연같은걸 많이보러 간 것 같은데",
          "speaker_id": "1",
          "time": "20210907 14:31"
        },
        {
          "id": "MDRW2100005853.1.13",
          "form": "요즘은 통 못가네요",
          "original_form": "요즘은 통 못가네요",
          "speaker_id": "1",
          "time": "20210907 14:31"
        },
        {
          "id": "MDRW2100005853.1.14",
          "form": "맞아요 공연 자체가 열리기 힘들어서..",
          "original_form": "맞아요 공연 자체가 열리기 힘들어서..",
          "speaker_id": "2",
          "time": "20210907 14:40"
        },
        {
          "id": "MDRW2100005853.1.15",
          "form": "누구 좋아하세요?",
          "original_form": "누구 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 14:40"
        },
        {
          "id": "MDRW2100005853.1.16",
          "form": "옛날에 국카스텐 한창 공연할때많이갔었구",
          "original_form": "옛날에 국카스텐 한창 공연할때많이갔었구",
          "speaker_id": "1",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100005853.1.17",
          "form": "그전에 장덕철이라구 그쪽 보컬이 아는형이라",
          "original_form": "그전에 장덕철이라구 그쪽 보컬이 아는형이라",
          "speaker_id": "1",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100005853.1.18",
          "form": "몇번 초대권줘서 갔었어요",
          "original_form": "몇번 초대권줘서 갔었어요",
          "speaker_id": "1",
          "time": "20210907 14:41"
        },
        {
          "id": "MDRW2100005853.1.19",
          "form": "오 역시 인맥이 짱이네요 ㅋㅋ",
          "original_form": "오 역시 인맥이 짱이네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 14:42"
        },
        {
          "id": "MDRW2100005853.1.20",
          "form": "ㅋㅋㅋㅋ음악자체를 좋아해서",
          "original_form": "ㅋㅋㅋㅋ음악자체를 좋아해서",
          "speaker_id": "1",
          "time": "20210907 14:42"
        },
        {
          "id": "MDRW2100005853.1.21",
          "form": "그런 공연 연주회 많이 다니려고 했었던거같아요",
          "original_form": "그런 공연 연주회 많이 다니려고 했었던거같아요",
          "speaker_id": "1",
          "time": "20210907 14:42"
        }
      ]
