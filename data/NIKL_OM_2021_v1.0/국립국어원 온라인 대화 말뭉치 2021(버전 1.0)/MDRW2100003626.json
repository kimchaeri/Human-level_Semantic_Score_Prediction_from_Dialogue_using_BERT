        },
        {
          "id": "MDRW2100003626.1.2",
          "form": "전 친구들이 가는것만 봐서 ..ㅋㅋ",
          "original_form": "전 친구들이 가는것만 봐서 ..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 09:13"
        },
        {
          "id": "MDRW2100003626.1.3",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "1",
          "time": "20210821 09:13"
        },
        {
          "id": "MDRW2100003626.1.4",
          "form": "여자에요?",
          "original_form": "여자에요?",
          "speaker_id": "1",
          "time": "20210821 09:14"
        },
        {
          "id": "MDRW2100003626.1.5",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 09:14"
        },
        {
          "id": "MDRW2100003626.1.6",
          "form": "얼마전에 남친이 전역하긴했어요 ㅎㅎ",
          "original_form": "얼마전에 남친이 전역하긴했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 09:14"
        },
        {
          "id": "MDRW2100003626.1.7",
          "form": "아~~~!!! 그렇군요",
          "original_form": "아~~~!!! 그렇군요",
          "speaker_id": "1",
          "time": "20210821 09:14"
        },
        {
          "id": "MDRW2100003626.1.8",
          "form": "오오 축하해요",
          "original_form": "오오 축하해요",
          "speaker_id": "1",
          "time": "20210821 09:14"
        },
        {
          "id": "MDRW2100003626.1.9",
          "form": "2년동안힘들었겠어요",
          "original_form": "2년동안힘들었겠어요",
          "speaker_id": "1",
          "time": "20210821 09:14"
        },
        {
          "id": "MDRW2100003626.1.10",
          "form": "감사합니다 ㅎㅎ",
          "original_form": "감사합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 09:17"
        },
        {
          "id": "MDRW2100003626.1.11",
          "form": "근데요즘코로나때매",
          "original_form": "근데요즘코로나때매",
          "speaker_id": "2",
          "time": "20210821 09:17"
        },
        {
          "id": "MDRW2100003626.1.12",
          "form": "휴가 땡겨서 좀 일찍나오긴했어요 ㅎㅎ",
          "original_form": "휴가 땡겨서 좀 일찍나오긴했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 09:17"
        },
        {
          "id": "MDRW2100003626.1.13",
          "form": "아니그럼 무슨소리인가요",
          "original_form": "아니그럼 무슨소리인가요",
          "speaker_id": "1",
          "time": "20210821 09:17"
        },
        {
          "id": "MDRW2100003626.1.14",
          "form": "이해가잘안되요",
          "original_form": "이해가잘안되요",
          "speaker_id": "1",
          "time": "20210821 09:17"
        },
        {
          "id": "MDRW2100003626.1.15",
          "form": "아 코로나로",
          "original_form": "아 코로나로",
          "speaker_id": "2",
          "time": "20210821 09:18"
        },
        {
          "id": "MDRW2100003626.1.16",
          "form": "휴가를 몇번 못나와서",
          "original_form": "휴가를 몇번 못나와서",
          "speaker_id": "2",
          "time": "20210821 09:18"
        },
        {
          "id": "MDRW2100003626.1.17",
          "form": "못쓴 휴가를 마지막에 다쓰면",
          "original_form": "못쓴 휴가를 마지막에 다쓰면",
          "speaker_id": "2",
          "time": "20210821 09:18"
        },
        {
          "id": "MDRW2100003626.1.18",
          "form": "전역을 빨리할수가있어서 ㅎㅎ",
          "original_form": "전역을 빨리할수가있어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 09:18"
        },
        {
          "id": "MDRW2100003626.1.19",
          "form": "아!!!!",
          "original_form": "아!!!!",
          "speaker_id": "1",
          "time": "20210821 09:19"
        },
        {
          "id": "MDRW2100003626.1.20",
          "form": "ㅋㅋㅋㅋㅋ똑같은 의미군요",
          "original_form": "ㅋㅋㅋㅋㅋ똑같은 의미군요",
          "speaker_id": "1",
          "time": "20210821 09:19"
        },
        {
          "id": "MDRW2100003626.1.21",
          "form": "넹 ㅋㅋㅋ",
          "original_form": "넹 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 09:28"
        },
        {
          "id": "MDRW2100003626.1.22",
          "form": "친구들도",
          "original_form": "친구들도",
          "speaker_id": "2",
          "time": "20210821 09:28"
        },
        {
          "id": "MDRW2100003626.1.23",
          "form": "이제 좀있음 전역해요 ㅋㅋ",
          "original_form": "이제 좀있음 전역해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 09:29"
        },
        {
          "id": "MDRW2100003626.1.24",
          "form": "한달뒤에",
