      "utterance": [
        {
          "id": "MDRW2100008662.1.1",
          "form": "종교 있으세요?",
          "original_form": "종교 있으세요?",
          "speaker_id": "1",
          "time": "20210822 17:37"
        },
        {
          "id": "MDRW2100008662.1.2",
          "form": "아니요~ 무교예요",
          "original_form": "아니요~ 무교예요",
          "speaker_id": "2",
          "time": "20210822 17:39"
        },
        {
          "id": "MDRW2100008662.1.3",
          "form": "요즘 종교 관련해서 부정적인 일이 많이 발생하는데 어떻게 생각하시나요",
          "original_form": "요즘 종교 관련해서 부정적인 일이 많이 발생하는데 어떻게 생각하시나요",
          "speaker_id": "1",
          "time": "20210822 17:40"
        },
        {
          "id": "MDRW2100008662.1.4",
          "form": "글쎄요 별로 관심이 없어서 잘 모르겠는데",
          "original_form": "글쎄요 별로 관심이 없어서 잘 모르겠는데",
          "speaker_id": "2",
          "time": "20210822 17:43"
        },
        {
          "id": "MDRW2100008662.1.5",
          "form": "다른 사람에게 피해만 안주면 되지 않나 생각해요",
          "original_form": "다른 사람에게 피해만 안주면 되지 않나 생각해요",
          "speaker_id": "2",
          "time": "20210822 17:43"
        },
        {
          "id": "MDRW2100008662.1.6",
          "form": "집단 감염 사례가 종교 단체에서 많이 나와서요",
          "original_form": "집단 감염 사례가 종교 단체에서 많이 나와서요",
          "speaker_id": "1",
          "time": "20210822 17:43"
        },
        {
          "id": "MDRW2100008662.1.7",
          "form": "그런거 보면 반감이 많이 들더라구요",
          "original_form": "그런거 보면 반감이 많이 들더라구요",
          "speaker_id": "1",
          "time": "20210822 17:43"
        },
        {
          "id": "MDRW2100008662.1.8",
          "form": "아 그쵸..저도 그런 곳들은 이해가 안되더라고요",
          "original_form": "아 그쵸..저도 그런 곳들은 이해가 안되더라고요",
          "speaker_id": "2",
          "time": "20210822 17:44"
        },
        {
          "id": "MDRW2100008662.1.9",
          "form": "방역수칙을 준수하는 곳도 많은데 굳이 모이는 사람들을 보면",
          "original_form": "방역수칙을 준수하는 곳도 많은데 굳이 모이는 사람들을 보면",
          "speaker_id": "2",
          "time": "20210822 17:45"
        },
        {
          "id": "MDRW2100008662.1.10",
          "form": "신념 때문이 아니라 돈 때문에 사람을 부르는 것 같아서 보기도 안좋고요",
          "original_form": "신념 때문이 아니라 돈 때문에 사람을 부르는 것 같아서 보기도 안좋고요",
          "speaker_id": "2",
          "time": "20210822 17:45"
        },
        {
          "id": "MDRW2100008662.1.11",
          "form": "뭐가 더 중요한지 모르는거 같아요",
          "original_form": "뭐가 더 중요한지 모르는거 같아요",
          "speaker_id": "1",
          "time": "20210822 17:46"
        },
        {
          "id": "MDRW2100008662.1.12",
          "form": "건강보다 신앙심이 위라니",
          "original_form": "건강보다 신앙심이 위라니",
          "speaker_id": "1",
          "time": "20210822 17:46"
        },
        {
          "id": "MDRW2100008662.1.13",
          "form": "저는 최근에는 기독교도 그렇지만",
          "original_form": "저는 최근에는 기독교도 그렇지만",
          "speaker_id": "2",
          "time": "20210822 17:47"
        },
        {
          "id": "MDRW2100008662.1.14",
          "form": "아프간니스탄 상황 보면서 이슬람이 정말 싫더라고요",
          "original_form": "아프간니스탄 상황 보면서 이슬람이 정말 싫더라고요",
          "speaker_id": "2",
          "time": "20210822 17:48"
        },
        {
          "id": "MDRW2100008662.1.15",
          "form": "이슬람도 참 문제가 매번 터져도",
          "original_form": "이슬람도 참 문제가 매번 터져도",
          "speaker_id": "1",
          "time": "20210822 17:49"
        },
        {
          "id": "MDRW2100008662.1.16",
          "form": "바뀔 기미가 안보이더라구요",
          "original_form": "바뀔 기미가 안보이더라구요",
          "speaker_id": "1",
          "time": "20210822 17:49"
        },
        {
          "id": "MDRW2100008662.1.17",
          "form": "네 그리고 뉴스에서도 우리나라에 이슬람 사원을 짓는다고 말이 많던데..",
          "original_form": "네 그리고 뉴스에서도 우리나라에 이슬람 사원을 짓는다고 말이 많던데..",
          "speaker_id": "2",
          "time": "20210822 17:50"
        },
        {
          "id": "MDRW2100008662.1.18",
          "form": "저는 반대예요",
          "original_form": "저는 반대예요",
          "speaker_id": "2",
          "time": "20210822 17:50"
        },
        {
          "id": "MDRW2100008662.1.19",
          "form": "저도 반대합니다",
          "original_form": "저도 반대합니다",
          "speaker_id": "1",
          "time": "20210822 17:50"
        },
        {
          "id": "MDRW2100008662.1.20",
          "form": "사원 지으면 결국 이슬람 신도자가 모일텐데",
          "original_form": "사원 지으면 결국 이슬람 신도자가 모일텐데",
          "speaker_id": "1",
          "time": "20210822 17:50"
        },
        {
          "id": "MDRW2100008662.1.21",
          "form": "요즘 세상 보면 너무 무서워요",
          "original_form": "요즘 세상 보면 너무 무서워요",
          "speaker_id": "1",
          "time": "20210822 17:50"
        },
        {
          "id": "MDRW2100008662.1.22",
          "form": "맞아요..난민을 받아주더라도",
          "original_form": "맞아요..난민을 받아주더라도",
          "speaker_id": "2",
          "time": "20210822 17:51"
        },
        {
          "id": "MDRW2100008662.1.23",
          "form": "종교활동은 절대 금지했으면 좋겠어요",
          "original_form": "종교활동은 절대 금지했으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210822 17:51"
        },
        {
          "id": "MDRW2100008662.1.24",
          "form": "특히 이슬람은 여성인권이 너무 낮아서 걱정이에요",
          "original_form": "특히 이슬람은 여성인권이 너무 낮아서 걱정이에요",
          "speaker_id": "2",
          "time": "20210822 17:52"
        },
        {
          "id": "MDRW2100008662.1.25",
          "form": "맞아요 괜히 안좋은 영향 끼칠거 같고",
          "original_form": "맞아요 괜히 안좋은 영향 끼칠거 같고",
          "speaker_id": "1",
          "time": "20210822 17:53"
        }
      ]
    }
