      "utterance": [
        {
          "id": "MDRW2100011954.1.1",
          "form": "네! 설탕이랑 액상과당 안들어있고",
          "original_form": "네! 설탕이랑 액상과당 안들어있고",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011954.1.2",
          "form": "칼로리도 엄청 낮아요",
          "original_form": "칼로리도 엄청 낮아요",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011954.1.3",
          "form": "헐 원래 음료수를 자주드셨나요?",
          "original_form": "헐 원래 음료수를 자주드셨나요?",
          "speaker_id": "2",
          "time": "20210824 17:46"
        },
        {
          "id": "MDRW2100011954.1.4",
          "form": "자주먹는건 아닌데 좀 단맛에 익숙해져있어서",
          "original_form": "자주먹는건 아닌데 좀 단맛에 익숙해져있어서",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011954.1.5",
          "form": "액상과당 끊으려고 사봤어요ㅋㅋㅋ",
          "original_form": "액상과당 끊으려고 사봤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:47"
        },
        {
          "id": "MDRW2100011954.1.6",
          "form": "크리스탈라이트 말고 콤부차도있고 요즘 이런 저칼로리식품들 많이 나오더라구요",
          "original_form": "크리스탈라이트 말고 콤부차도있고 요즘 이런 저칼로리식품들 많이 나오더라구요",
          "speaker_id": "1",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011954.1.7",
          "form": "좋은변화인거 같아요",
          "original_form": "좋은변화인거 같아요",
          "speaker_id": "1",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011954.1.8",
          "form": "아 헐 저도 카페 자주가거든요 액상과당 중독일거같아요",
          "original_form": "아 헐 저도 카페 자주가거든요 액상과당 중독일거같아요",
          "speaker_id": "2",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011954.1.9",
          "form": "콤부차 맛 되게 희한하던뎈ㅋㅋㅋ",
          "original_form": "콤부차 맛 되게 희한하던뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011954.1.10",
          "form": "좋아하시나요?!",
          "original_form": "좋아하시나요?!",
          "speaker_id": "2",
          "time": "20210824 17:48"
        },
        {
          "id": "MDRW2100011954.1.11",
          "form": "콤부차는 아직 안먹어봤어요ㅋㅋㅋㅋ",
          "original_form": "콤부차는 아직 안먹어봤어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011954.1.12",
          "form": "레몬맛은 맛있다던데",
          "original_form": "레몬맛은 맛있다던데",
          "speaker_id": "1",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011954.1.13",
          "form": "맛이 특이하나요?",
          "original_form": "맛이 특이하나요?",
          "speaker_id": "1",
          "time": "20210824 17:56"
        },
        {
          "id": "MDRW2100011954.1.14",
          "form": "되게 시큼하고 발효음료 같아요",
          "original_form": "되게 시큼하고 발효음료 같아요",
          "speaker_id": "2",
          "time": "20210824 18:30"
        },
        {
          "id": "MDRW2100011954.1.15",
          "form": "액상과당의 친숙한 단맛은 아니에요",
          "original_form": "액상과당의 친숙한 단맛은 아니에요",
          "speaker_id": "2",
          "time": "20210824 18:30"
        },
        {
          "id": "MDRW2100011954.1.16",
          "form": "아하...",
          "original_form": "아하...",
          "speaker_id": "1",
          "time": "20210824 18:55"
        },
        {
          "id": "MDRW2100011954.1.17",
          "form": "그래도 콤부차는 에리스리톨 써서",
          "original_form": "그래도 콤부차는 에리스리톨 써서",
          "speaker_id": "1",
          "time": "20210824 18:55"
        },
        {
          "id": "MDRW2100011954.1.18",
          "form": "아스파탐 들어가는 크리스탈라이트보다 성분은 괜찮다 하더라구요",
          "original_form": "아스파탐 들어가는 크리스탈라이트보다 성분은 괜찮다 하더라구요",
          "speaker_id": "1",
          "time": "20210824 18:55"
        },
        {
          "id": "MDRW2100011954.1.19",
          "form": "아 에리스리톨이 아스파탐보다 낫나요??",
          "original_form": "아 에리스리톨이 아스파탐보다 낫나요??",
          "speaker_id": "2",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100011954.1.20",
          "form": "그렇다면 콤부차를..ㅋㅋㅋ",
          "original_form": "그렇다면 콤부차를..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:09"
        },
        {
          "id": "MDRW2100011954.1.21",
          "form": "아 시큼하고 맥주같이 약간 탄산도 있어요",
          "original_form": "아 시큼하고 맥주같이 약간 탄산도 있어요",
          "speaker_id": "2",
          "time": "20210824 19:09"
        },
        {
          "id": "MDRW2100011954.1.22",
          "form": "설탕대체감미료들이 다 많이먹으면 안좋은데",
          "original_form": "설탕대체감미료들이 다 많이먹으면 안좋은데",
          "speaker_id": "1",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100011954.1.23",
          "form": "그래도 에리스리톨이 그나마~괜찮다고 들은거같아요..ㅎ",
          "original_form": "그래도 에리스리톨이 그나마~괜찮다고 들은거같아요..ㅎ",
          "speaker_id": "1",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100011954.1.24",
          "form": "요즘 밥은 어떤거 주로 드시나요?",
          "original_form": "요즘 밥은 어떤거 주로 드시나요?",
          "speaker_id": "1",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100011954.1.25",
          "form": "오 화학과신거요?",
          "original_form": "오 화학과신거요?",
          "speaker_id": "2",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100011954.1.26",
          "form": "완전 전문가같으세요",
          "original_form": "완전 전문가같으세요",
          "speaker_id": "2",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100011954.1.27",
          "form": "저는 학교에서 일해서 급 식 먹 고있습니닼ㅋㅋ",
          "original_form": "저는 학교에서 일해서 급 식 먹 고있습니닼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100011954.1.28",
          "form": "아니에요 그냥 건강하고 맛있게 먹는거에 관심이 많아서ㅋㅋㅋㅋㅋ",
          "original_form": "아니에요 그냥 건강하고 맛있게 먹는거에 관심이 많아서ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:17"
        },
        {
          "id": "MDRW2100011954.1.29",
          "form": "급식 부럽네요! 집에서 혼자 챙겨먹는게 은근 엄청귀찮은거같아요..",
          "original_form": "급식 부럽네요! 집에서 혼자 챙겨먹는게 은근 엄청귀찮은거같아요..",
          "speaker_id": "1",
          "time": "20210824 19:18"
        },
        {
          "id": "MDRW2100011954.1.30",
          "form": "그쵸 ㅋㅋㅋㅋ 저도 메뉴 정해져있는게 너무 편해요",
          "original_form": "그쵸 ㅋㅋㅋㅋ 저도 메뉴 정해져있는게 너무 편해요",
          "speaker_id": "2",
          "time": "20210824 19:18"
        },
        {
          "id": "MDRW2100011954.1.31",
          "form": "특히 건강하게먹는게 쉽지가않잖아요",
          "original_form": "특히 건강하게먹는게 쉽지가않잖아요",
          "speaker_id": "2",
          "time": "20210824 19:18"
        },
        {
          "id": "MDRW2100011954.1.32",
          "form": "맞아요 돈도많이들고 시간도 많이들고...",
          "original_form": "맞아요 돈도많이들고 시간도 많이들고...",
          "speaker_id": "1",
          "time": "20210824 19:37"
        },
        {
          "id": "MDRW2100011954.1.33",
          "form": "건강하게 살려면 진짜진짜 부지런해야하는거같아요ㅋㅋ",
          "original_form": "건강하게 살려면 진짜진짜 부지런해야하는거같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 19:38"
        }
      ]
    }
