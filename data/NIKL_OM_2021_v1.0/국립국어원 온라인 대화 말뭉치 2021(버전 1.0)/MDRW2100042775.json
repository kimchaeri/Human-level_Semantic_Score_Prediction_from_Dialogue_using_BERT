      "utterance": [
        {
          "id": "MDRW2100042775.1.1",
          "form": "네 ㅋ중학교때 강아지키웠어요ㅋ",
          "original_form": "네 ㅋ중학교때 강아지키웠어요ㅋ",
          "speaker_id": "2",
          "time": "20210907 08:55"
        },
        {
          "id": "MDRW2100042775.1.2",
          "form": "어떤 강아지였나요? 믹스였나요?",
          "original_form": "어떤 강아지였나요? 믹스였나요?",
          "speaker_id": "1",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100042775.1.3",
          "form": "그냥 ㅋㅋㅋㅋㅋㅋㅋ 마당에서 키우는 강아지였어요 ㅋㅋ",
          "original_form": "그냥 ㅋㅋㅋㅋㅋㅋㅋ 마당에서 키우는 강아지였어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042775.1.4",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042775.1.5",
          "form": "전 말티즈 키웠었어요 과거에",
          "original_form": "전 말티즈 키웠었어요 과거에",
          "speaker_id": "1",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042775.1.6",
          "form": "아 밀티즈 귀엽죠 ㅋㅋㅋ",
          "original_form": "아 밀티즈 귀엽죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042775.1.7",
          "form": "사랑스럽게 생겼잖아요",
          "original_form": "사랑스럽게 생겼잖아요",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042775.1.8",
          "form": "그래도 귀여움은 믹스를 따라올ㅇ 수 없죠 ㅎㅎ",
          "original_form": "그래도 귀여움은 믹스를 따라올ㅇ 수 없죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100042775.1.9",
          "form": "맞아요 저도 말티즈를 키우고싶었어요 ㅋㅋ",
          "original_form": "맞아요 저도 말티즈를 키우고싶었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100042775.1.10",
          "form": "지금은 키우는 동물 없으신거죠?",
          "original_form": "지금은 키우는 동물 없으신거죠?",
          "speaker_id": "1",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042775.1.11",
          "form": "네 지금은 구피만 키우고 있어요 ㅋㅋ",
          "original_form": "네 지금은 구피만 키우고 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:42"
        },
        {
          "id": "MDRW2100042775.1.12",
          "form": "우리집도 구피 키워요",
          "original_form": "우리집도 구피 키워요",
          "speaker_id": "1",
          "time": "20210907 09:45"
        },
        {
          "id": "MDRW2100042775.1.13",
          "form": "남편이 키우고 있네요",
          "original_form": "남편이 키우고 있네요",
          "speaker_id": "1",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042775.1.14",
          "form": "구피 몇마리 정도 있나요?",
          "original_form": "구피 몇마리 정도 있나요?",
          "speaker_id": "1",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042775.1.15",
          "form": "네 지금은 4마리밖에 없어요",
          "original_form": "네 지금은 4마리밖에 없어요",
          "speaker_id": "2",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042775.1.16",
          "form": "원래는 수십마리 키웠다가 다 분양하고 ㅋㅋ 4마리만 있어요",
          "original_form": "원래는 수십마리 키웠다가 다 분양하고 ㅋㅋ 4마리만 있어요",
          "speaker_id": "2",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042775.1.17",
          "form": "지금 4마리에요?",
          "original_form": "지금 4마리에요?",
          "speaker_id": "1",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042775.1.18",
          "form": "간촐하네요",
          "original_form": "간촐하네요",
          "speaker_id": "1",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042775.1.19",
          "form": "저희 집은 10마리에서 100마리쯤 됐어요",
          "original_form": "저희 집은 10마리에서 100마리쯤 됐어요",
          "speaker_id": "1",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042775.1.20",
          "form": "네네 ㅋㅋㅋ와 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ많으시네요 ㅋ",
          "original_form": "네네 ㅋㅋㅋ와 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ많으시네요 ㅋ",
