      "utterance": [
        {
          "id": "MDRW2100000974.1.1",
          "form": "결혼 해야하는데 계획만 있다고 되는게 아니더라구요",
          "original_form": "결혼 해야하는데 계획만 있다고 되는게 아니더라구요",
          "speaker_id": "1",
          "time": "20210819 21:01"
        },
        {
          "id": "MDRW2100000974.1.2",
          "form": "쫓기듯 결혼말고 \r\n놀거 다 놀고 하세요 후회없게",
          "original_form": "쫓기듯 결혼말고 \r\n놀거 다 놀고 하세요 후회없게",
          "speaker_id": "2",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000974.1.3",
          "form": "name2님은 그런일은 없으시겠지만 혹시 어떨때 결혼한거 후회되세요",
          "original_form": "&name2&님은 그런일은 없으시겠지만 혹시 어떨때 결혼한거 후회되세요",
          "speaker_id": "1",
          "time": "20210819 21:05"
        },
        {
          "id": "MDRW2100000974.1.4",
          "form": ".......많아요! \r\n많았어요",
          "original_form": ".......많아요! \r\n많았어요",
          "speaker_id": "2",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100000974.1.5",
          "form": "10년 되니 조금 내려놓게 돠지만요 ㅠ",
          "original_form": "10년 되니 조금 내려놓게 돠지만요 ㅠ",
          "speaker_id": "2",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100000974.1.6",
          "form": "남의편이라잖아요^^*",
          "original_form": "남의편이라잖아요^^*",
          "speaker_id": "2",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100000974.1.7",
          "form": "그래도 애들때문은 아니져?",
          "original_form": "그래도 애들때문은 아니져?",
          "speaker_id": "1",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100000974.1.8",
          "form": "전 어렸을땐 뭣모르고 결혼일찍하고싶었는데",
          "original_form": "전 어렸을땐 뭣모르고 결혼일찍하고싶었는데",
          "speaker_id": "1",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000974.1.9",
          "form": "솔직히 한번 해봤어요",
          "original_form": "솔직히 한번 해봤어요",
          "speaker_id": "2",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000974.1.10",
          "form": "알콩달콩 행복한가정을 상상하며",
          "original_form": "알콩달콩 행복한가정을 상상하며",
          "speaker_id": "1",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000974.1.11",
          "form": "남편이랑 사이가 안좋은데 갈라서고 싶어진 지경이 되니까",
          "original_form": "남편이랑 사이가 안좋은데 갈라서고 싶어진 지경이 되니까",
          "speaker_id": "2",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000974.1.12",
          "form": "애들이 짐같더라고요 이러지도 저러지도 못하는",
          "original_form": "애들이 짐같더라고요 이러지도 저러지도 못하는",
          "speaker_id": "2",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000974.1.13",
          "form": "나쁜 생각했죠",
          "original_form": "나쁜 생각했죠",
          "speaker_id": "2",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000974.1.14",
          "form": "요새는 티비에 남자들 나와서 농담으로 결혼후회하는 얘기하는거보면 정떨어져요",
          "original_form": "요새는 티비에 남자들 나와서 농담으로 결혼후회하는 얘기하는거보면 정떨어져요",
          "speaker_id": "1",
          "time": "20210819 21:08"
        },
        {
          "id": "MDRW2100000974.1.15",
          "form": "사람인데 한번도 안했다하면 인간미 떨어질뻔했어요",
          "original_form": "사람인데 한번도 안했다하면 인간미 떨어질뻔했어요",
          "speaker_id": "1",
          "time": "20210819 21:09"
        },
        {
          "id": "MDRW2100000974.1.16",
          "form": "연애할때랑\r\n결혼해서는\r\n완전 다르더라고요",
          "original_form": "연애할때랑\r\n결혼해서는\r\n완전 다르더라고요",
          "speaker_id": "2",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100000974.1.17",
          "form": "좋은 점도 잇지만 현실이....",
          "original_form": "좋은 점도 잇지만 현실이....",
          "speaker_id": "2",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100000974.1.18",
          "form": "전 시압쥐때문에 많이 싸웠어요",
          "original_form": "전 시압쥐때문에 많이 싸웠어요",
          "speaker_id": "2",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000974.1.19",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210819 21:18"
        },
        {
          "id": "MDRW2100000974.1.20",
          "form": "아니 근데진짜",
          "original_form": "아니 근데진짜",
          "speaker_id": "1",
          "time": "20210819 21:18"
        },
        {
          "id": "MDRW2100000974.1.21",
          "form": "내가 고른건 남편이지 남편 가족들까지는 아닌데",
          "original_form": "내가 고른건 남편이지 남편 가족들까지는 아닌데",
          "speaker_id": "1",
          "time": "20210819 21:19"
        },
        {
          "id": "MDRW2100000974.1.22",
          "form": "남편도 효자가 되거든요",
          "original_form": "남편도 효자가 되거든요",
          "speaker_id": "2",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100000974.1.23",
          "form": "내 부모님인데 니가 잘해줘 하는",
          "original_form": "내 부모님인데 니가 잘해줘 하는",
          "speaker_id": "2",
          "time": "20210819 21:21"
        },
        {
          "id": "MDRW2100000974.1.24",
          "form": "효자도 효자나름인데",
          "original_form": "효자도 효자나름인데",
          "speaker_id": "1",
          "time": "20210819 21:25"
        },
        {
          "id": "MDRW2100000974.1.25",
          "form": "대리효도 뭐 그런거도 요구한다면서요",
          "original_form": "대리효도 뭐 그런거도 요구한다면서요",
          "speaker_id": "1",
          "time": "20210819 21:25"
        },
        {
          "id": "MDRW2100000974.1.26",
          "form": "아니 자기네들은 친정에 그렇게 잘하나",
          "original_form": "아니 자기네들은 친정에 그렇게 잘하나",
