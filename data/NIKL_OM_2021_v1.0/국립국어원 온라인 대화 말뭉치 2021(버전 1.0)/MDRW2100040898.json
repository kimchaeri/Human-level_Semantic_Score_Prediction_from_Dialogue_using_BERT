      "utterance": [
        {
          "id": "MDRW2100040898.1.1",
          "form": "티비에 김수미 선생님이 나와서",
          "original_form": "티비에 김수미 선생님이 나와서",
          "speaker_id": "2",
          "time": "20210905 11:41"
        },
        {
          "id": "MDRW2100040898.1.2",
          "form": "엄마가 요즘 입맛없으신데 저건 맛나겠다하셔서",
          "original_form": "엄마가 요즘 입맛없으신데 저건 맛나겠다하셔서",
          "speaker_id": "2",
          "time": "20210905 11:41"
        },
        {
          "id": "MDRW2100040898.1.3",
          "form": "얼른 시켜서 같이 먹었어요",
          "original_form": "얼른 시켜서 같이 먹었어요",
          "speaker_id": "2",
          "time": "20210905 11:41"
        },
        {
          "id": "MDRW2100040898.1.4",
          "form": "이야ㅎㅎ잘하셧어용",
          "original_form": "이야ㅎㅎ잘하셧어용",
          "speaker_id": "1",
          "time": "20210905 11:41"
        },
        {
          "id": "MDRW2100040898.1.5",
          "form": "점심 맛잇는거먹을려구 생각중인데",
          "original_form": "점심 맛잇는거먹을려구 생각중인데",
          "speaker_id": "1",
          "time": "20210905 11:41"
        },
        {
          "id": "MDRW2100040898.1.6",
          "form": "고르기힘들어요ㅜㅜ",
          "original_form": "고르기힘들어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 11:42"
        },
        {
          "id": "MDRW2100040898.1.7",
          "form": "오호 찜닭어떠세요",
          "original_form": "오호 찜닭어떠세요",
          "speaker_id": "2",
          "time": "20210905 11:42"
        },
        {
          "id": "MDRW2100040898.1.8",
          "form": "찜닭 완전 좋져 ㅎㅎ 일단 후식은 아이스크림 대기중이에요 ㅋㅋㅋ",
          "original_form": "찜닭 완전 좋져 ㅎㅎ 일단 후식은 아이스크림 대기중이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:51"
        },
        {
          "id": "MDRW2100040898.1.9",
          "form": "저는 아이스커피 마시고잇어요",
          "original_form": "저는 아이스커피 마시고잇어요",
          "speaker_id": "2",
          "time": "20210905 11:58"
        },
        {
          "id": "MDRW2100040898.1.10",
          "form": "아침드시구 출근하셧나용",
          "original_form": "아침드시구 출근하셧나용",
          "speaker_id": "1",
          "time": "20210906 10:48"
        },
        {
          "id": "MDRW2100040898.1.11",
          "form": "토마토 쥬스 마시고 출근했러요",
          "original_form": "토마토 쥬스 마시고 출근했러요",
          "speaker_id": "2",
          "time": "20210906 11:39"
        },
        {
          "id": "MDRW2100040898.1.12",
          "form": "지금은 점심 시간이에요",
          "original_form": "지금은 점심 시간이에요",
          "speaker_id": "2",
          "time": "20210906 11:39"
        },
        {
          "id": "MDRW2100040898.1.13",
          "form": "뭐드세요?",
          "original_form": "뭐드세요?",
          "speaker_id": "2",
          "time": "20210906 11:39"
        },
        {
          "id": "MDRW2100040898.1.14",
          "form": "저희는 다같이 백반 도시락 시켜먹어요",
          "original_form": "저희는 다같이 백반 도시락 시켜먹어요",
          "speaker_id": "1",
          "time": "20210906 11:40"
        },
        {
          "id": "MDRW2100040898.1.15",
          "form": "오오 매일이요??",
          "original_form": "오오 매일이요??",
          "speaker_id": "2",
          "time": "20210906 11:40"
        },
        {
          "id": "MDRW2100040898.1.16",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 11:40"
        },
        {
          "id": "MDRW2100040898.1.17",
          "form": "점심 머드시나욤",
          "original_form": "점심 머드시나욤",
          "speaker_id": "1",
          "time": "20210906 11:40"
        },
        {
          "id": "MDRW2100040898.1.18",
          "form": "회사식당 급식 먹어뇨 ㅋㅋㅋ",
          "original_form": "회사식당 급식 먹어뇨 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:41"
        },
        {
          "id": "MDRW2100040898.1.19",
          "form": "ㅎㅎㅎ잘나오나용",
          "original_form": "ㅎㅎㅎ잘나오나용",
          "speaker_id": "1",
          "time": "20210906 11:43"
        },
        {
          "id": "MDRW2100040898.1.20",
          "form": "네네 잘나오는데 맛은 그냥저냥",
          "original_form": "네네 잘나오는데 맛은 그냥저냥",
          "speaker_id": "2",
          "time": "20210906 11:44"
        },
        {
