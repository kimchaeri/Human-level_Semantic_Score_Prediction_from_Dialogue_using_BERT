      "utterance": [
        {
          "id": "MDRW2100033072.1.1",
          "form": "연말..?",
          "original_form": "연말..?",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100033072.1.2",
          "form": "저기 지금 여기 홍수 났거 느껴져요?",
          "original_form": "저기 지금 여기 홍수 났거 느껴져요?",
          "speaker_id": "1",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100033072.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100033072.1.4",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100033072.1.5",
          "form": "아이쿠 발 디딜데가 없네",
          "original_form": "아이쿠 발 디딜데가 없네",
          "speaker_id": "2",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100033072.1.6",
          "form": "내 책... 지금 몇년째 기다리는거죠 ㅠ",
          "original_form": "내 책... 지금 몇년째 기다리는거죠 ㅠ",
          "speaker_id": "1",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100033072.1.7",
          "form": "이대로 가다간",
          "original_form": "이대로 가다간",
          "speaker_id": "1",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100033072.1.8",
          "form": "책 살 돈은 잇다고 내달라고",
          "original_form": "책 살 돈은 잇다고 내달라고",
          "speaker_id": "2",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100033072.1.9",
          "form": "지구가 먼저 가라 앉을 거 같은데",
          "original_form": "지구가 먼저 가라 앉을 거 같은데",
          "speaker_id": "1",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100033072.1.10",
          "form": "책... 특전도 궁금해",
          "original_form": "책... 특전도 궁금해",
          "speaker_id": "1",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100033072.1.11",
          "form": "얼마나 빡센거 준비중일지",
          "original_form": "얼마나 빡센거 준비중일지",
          "speaker_id": "1",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100033072.1.12",
          "form": "종이책 나와요!! 하고선 또 깜깜무소식이라",
          "original_form": "종이책 나와요!! 하고선 또 깜깜무소식이라",
          "speaker_id": "2",
          "time": "20210906 15:29"
        },
        {
          "id": "MDRW2100033072.1.13",
          "form": "종이책 출간되는 곳이라도 알려져서 다행이라 해야하나",
          "original_form": "종이책 출간되는 곳이라도 알려져서 다행이라 해야하나",
          "speaker_id": "2",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100033072.1.14",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100033072.1.15",
          "form": "와.. 그거 진짜 ㅠ",
          "original_form": "와.. 그거 진짜 ㅠ",
          "speaker_id": "1",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100033072.1.16",
          "form": "알려준건 고마운데",
          "original_form": "알려준건 고마운데",
          "speaker_id": "1",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100033072.1.17",
          "form": "특전 웹독만큼은 해줘야할텐데 말야",
          "original_form": "특전 웹독만큼은 해줘야할텐데 말야",
          "speaker_id": "2",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100033072.1.18",
          "form": "아예 그때쯤에 들고 와줘야할건데",
          "original_form": "아예 그때쯤에 들고 와줘야할건데",
          "speaker_id": "1",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100033072.1.19",
          "form": "특전 부분 말하면 대만판 ...",
          "original_form": "특전 부분 말하면 대만판 ...",
          "speaker_id": "1",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100033072.1.20",
          "form": "양심 있음 본국이다  지지말라구",
          "original_form": "양심 있음 본국이다  지지말라구",
          "speaker_id": "1",
          "time": "20210906 15:30"
        },
        {
          "id": "MDRW2100033072.1.21",
          "form": "본국의 맛을 보여줘라",
          "original_form": "본국의 맛을 보여줘라",
          "speaker_id": "2",
          "time": "20210906 15:31"
        },
        {
          "id": "MDRW2100033072.1.22",
          "form": "우린 이만큼 할 수 있다 보여조라!!",
          "original_form": "우린 이만큼 할 수 있다 보여조라!!",
          "speaker_id": "2",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100033072.1.23",
          "form": "마저",
          "original_form": "마저",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100033072.1.24",
          "form": "대만판 일러레도 우리나라분이다",
          "original_form": "대만판 일러레도 우리나라분이다",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100033072.1.25",
          "form": "뭐하냐!!",
          "original_form": "뭐하냐!!",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100033072.1.26",
          "form": "이런 분들 섭외 안하고!!@",
          "original_form": "이런 분들 섭외 안하고!!@",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100033072.1.27",
          "form": "역시 해외로 뻗어나가는 우리나라 인재들",
          "original_form": "역시 해외로 뻗어나가는 우리나라 인재들",
          "speaker_id": "2",
          "time": "20210906 16:08"
        },
        {
          "id": "MDRW2100033072.1.28",
          "form": "ㅋㅋㅋㅋ 대한민국이 너무 좁은 장소였다고",
          "original_form": "ㅋㅋㅋㅋ 대한민국이 너무 좁은 장소였다고",
          "speaker_id": "1",
          "time": "20210906 16:09"
        },
        {
          "id": "MDRW2100033072.1.29",
          "form": "소설로 글로벌이 하나가 됐는걸",
          "original_form": "소설로 글로벌이 하나가 됐는걸",
          "speaker_id": "1",
          "time": "20210906 16:09"
        },
        {
          "id": "MDRW2100033072.1.30",
          "form": "ㅋㅋㅋㅋ 가만 있어도 해외 연성들까지 들어오는뎈",
          "original_form": "ㅋㅋㅋㅋ 가만 있어도 해외 연성들까지 들어오는뎈",
          "speaker_id": "2",
          "time": "20210906 16:11"
        },
        {
          "id": "MDRW2100033072.1.31",
          "form": "아이고 배부르다 배불러 ㅋㅋㅋ",
          "original_form": "아이고 배부르다 배불러 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:11"
        },
        {
          "id": "MDRW2100033072.1.32",
          "form": "ㅋㅋㅋㅋ 다른 소설도 많이 알려지면 좋겠어",
          "original_form": "ㅋㅋㅋㅋ 다른 소설도 많이 알려지면 좋겠어",
          "speaker_id": "1",
          "time": "20210906 16:14"
        },
        {
          "id": "MDRW2100033072.1.33",
          "form": "마젘ㅋㅋㅋ",
          "original_form": "마젘ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:15"
        },
        {
          "id": "MDRW2100033072.1.34",
          "form": "장르소설로 해외진출 많이 했음 좋겠닼",
          "original_form": "장르소설로 해외진출 많이 했음 좋겠닼",
          "speaker_id": "2",
          "time": "20210906 16:16"
        },
        {
          "id": "MDRW2100033072.1.35",
          "form": "해외까지 건너가기 전에",
          "original_form": "해외까지 건너가기 전에",
          "speaker_id": "1",
          "time": "20210906 16:16"
        },
        {
          "id": "MDRW2100033072.1.36",
          "form": "국내 독자를 먼저 늘 리 게",
          "original_form": "국내 독자를 먼저 늘 리 게",
          "speaker_id": "1",
          "time": "20210906 16:16"
        },
        {
          "id": "MDRW2100033072.1.37",
          "form": "너도 협조를 해야지",
          "original_form": "너도 협조를 해야지",
          "speaker_id": "1",
          "time": "20210906 16:17"
        },
        {
          "id": "MDRW2100033072.1.38",
          "form": "그러니 내 책 영업은 정당하다",
          "original_form": "그러니 내 책 영업은 정당하다",
          "speaker_id": "1",
          "time": "20210906 16:17"
        },
        {
          "id": "MDRW2100033072.1.39",
          "form": "열심히 독자로 활동하고는 있습니다만?",
          "original_form": "열심히 독자로 활동하고는 있습니다만?",
