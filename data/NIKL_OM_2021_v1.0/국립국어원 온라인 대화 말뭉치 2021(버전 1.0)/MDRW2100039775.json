      "utterance": [
        {
          "id": "MDRW2100039775.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039775.1.2",
          "form": "저녁 식사하셨어요?",
          "original_form": "저녁 식사하셨어요?",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100039775.1.3",
          "form": "저는  저녁은 굶었어요. 싫어하는 반찬이 나왔거든요",
          "original_form": "저는  저녁은 굶었어요. 싫어하는 반찬이 나왔거든요",
          "speaker_id": "2",
          "time": "20210903 21:22"
        },
        {
          "id": "MDRW2100039775.1.4",
          "form": "어디서 드셨어요~?",
          "original_form": "어디서 드셨어요~?",
          "speaker_id": "1",
          "time": "20210903 21:24"
        },
        {
          "id": "MDRW2100039775.1.5",
          "form": "에고 아예 굶으셨음 배고프시겠어요 ㅜ",
          "original_form": "에고 아예 굶으셨음 배고프시겠어요 ㅜ",
          "speaker_id": "1",
          "time": "20210903 21:24"
        },
        {
          "id": "MDRW2100039775.1.6",
          "form": "퇴근하고  들어왔는데 집에서 생선비린내가 나서 ㅠㅠ\r\n\r\n너무배고픈데 다이어트한다 생각하려고요",
          "original_form": "퇴근하고  들어왔는데 집에서 생선비린내가 나서 ㅠㅠ\r\n\r\n너무배고픈데 다이어트한다 생각하려고요",
          "speaker_id": "2",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100039775.1.7",
          "form": "낼 주말이니까 맛있는거 먹으면돼요!",
          "original_form": "낼 주말이니까 맛있는거 먹으면돼요!",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039775.1.8",
          "form": "요리하는거 좋아하세요?",
          "original_form": "요리하는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100039775.1.9",
          "form": "아 ㅜ 그렇군요",
          "original_form": "아 ㅜ 그렇군요",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039775.1.10",
          "form": "네 좋아는하는데 귀찮아요",
          "original_form": "네 좋아는하는데 귀찮아요",
          "speaker_id": "1",
          "time": "20210903 22:16"
        },
        {
          "id": "MDRW2100039775.1.11",
          "form": "ㅋㅋ 대부분 가지고있는 공통점같아요.\r\n귀찮은거",
          "original_form": "ㅋㅋ 대부분 가지고있는 공통점같아요.\r\n귀찮은거",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039775.1.12",
          "form": "그래도 우리나라는 배달음식이  많아서 다행이엥ㄷ",
          "original_form": "그래도 우리나라는 배달음식이  많아서 다행이엥{emoji:‥}ㄷ",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039775.1.13",
          "form": "외국에살았으면 못견뎠을거에요",
          "original_form": "외국에살았으면 못견뎠을거에요",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039775.1.14",
          "form": "ㅋㅋㅋㅋ배달음식중 뭐 좋아하세요?",
          "original_form": "ㅋㅋㅋㅋ배달음식중 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100039775.1.15",
          "form": "저는 분식이요.떡볶이는 저의 소울푸드에요",
          "original_form": "저는 분식이요.떡볶이는 저의 소울푸드에요",
          "speaker_id": "2",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100039775.1.16",
          "form": "세상여자들은 3종류로 나뉜대요",
          "original_form": "세상여자들은 3종류로 나뉜대요",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039775.1.17",
          "form": "떡순이  빵순이  빵떡순이",
          "original_form": "떡순이  빵순이  빵떡순이",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039775.1.18",
          "form": "전 빵떡순이에요",
          "original_form": "전 빵떡순이에요",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039775.1.19",
          "form": "ㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100039775.1.20",
          "form": "저도 둘다 너무 좋아요",
          "original_form": "저도 둘다 너무 좋아요",
          "speaker_id": "1",
          "time": "20210903 22:59"
        },
        {
          "id": "MDRW2100039775.1.21",
          "form": "밥은 주  만들어드시나요?   바깥음식 드시나요?",
          "original_form": "밥은 주  만들어드시나요?   바깥음식 드시나요?",
          "speaker_id": "2",
          "time": "20210904 07:46"
        },
        {
          "id": "MDRW2100039775.1.22",
          "form": "바깥음식도 먹고",
          "original_form": "바깥음식도 먹고",
          "speaker_id": "1",
          "time": "20210904 15:19"
        },
        {
          "id": "MDRW2100039775.1.23",
          "form": "직접해먹기도해요",
          "original_form": "직접해먹기도해요",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100039775.1.24",
          "form": "배달음식이  성행해서 편하긴한데 위생적으로 걱정되서 요즘  자중하고있어요.",
          "original_form": "배달음식이  성행해서 편하긴한데 위생적으로 걱정되서 요즘  자중하고있어요.",
          "speaker_id": "2",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100039775.1.25",
          "form": "식중독 이야기 나오면 흠칫 하게되더라고요",
          "original_form": "식중독 이야기 나오면 흠칫 하게되더라고요",
          "speaker_id": "2",
          "time": "20210904 16:23"
        },
        {
          "id": "MDRW2100039775.1.26",
          "form": "맞아요 여름이라",
          "original_form": "맞아요 여름이라",
          "speaker_id": "1",
          "time": "20210904 18:40"
        },
        {
          "id": "MDRW2100039775.1.27",
          "form": "더 조심하는게 좋긴하죠 ㅜ",
          "original_form": "더 조심하는게 좋긴하죠 ㅜ",
          "speaker_id": "1",
          "time": "20210904 18:40"
        },
        {
          "id": "MDRW2100039775.1.28",
          "form": "며칠전에 명란젓을 사먹었는데 맛이 새콤 하더라고요",
          "original_form": "며칠전에 명란젓을 사먹었는데 맛이 새콤 하더라고요",
          "speaker_id": "2",
          "time": "20210904 19:14"
        },
        {
          "id": "MDRW2100039775.1.29",
          "form": "마트에서 바로산건데도 날것이라 운반과정에서  익어버렸나봐요",
          "original_form": "마트에서 바로산건데도 날것이라 운반과정에서  익어버렸나봐요",
          "speaker_id": "2",
          "time": "20210904 19:14"
        },
        {
          "id": "MDRW2100039775.1.30",
          "form": "그렇군요 ㅠ",
          "original_form": "그렇군요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 19:20"
        }
      ]
    }
