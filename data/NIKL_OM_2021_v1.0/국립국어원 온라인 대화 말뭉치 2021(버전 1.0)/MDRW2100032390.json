      "utterance": [
        {
          "id": "MDRW2100032390.1.1",
          "form": "떡 주문했는데 아직 안왔어",
          "original_form": "떡 주문했는데 아직 안왔어",
          "speaker_id": "2",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100032390.1.2",
          "form": "무슨 떡인데?",
          "original_form": "무슨 떡인데?",
          "speaker_id": "1",
          "time": "20210905 16:48"
        },
        {
          "id": "MDRW2100032390.1.3",
          "form": "경북떡인데 쑥앙금절편",
          "original_form": "경북떡인데 쑥앙금절편",
          "speaker_id": "2",
          "time": "20210905 16:49"
        },
        {
          "id": "MDRW2100032390.1.4",
          "form": "앙금절편 맛있지",
          "original_form": "앙금절편 맛있지",
          "speaker_id": "1",
          "time": "20210905 16:49"
        },
        {
          "id": "MDRW2100032390.1.5",
          "form": "맛있어? 난 아직 안먹어봐서",
          "original_form": "맛있어? 난 아직 안먹어봐서",
          "speaker_id": "2",
          "time": "20210905 16:49"
        },
        {
          "id": "MDRW2100032390.1.6",
          "form": "쫀득한데 가운데 팥소가 있어서 달달해",
          "original_form": "쫀득한데 가운데 팥소가 있어서 달달해",
          "speaker_id": "1",
          "time": "20210905 16:49"
        },
        {
          "id": "MDRW2100032390.1.7",
          "form": "바람떡도 샀어",
          "original_form": "바람떡도 샀어",
          "speaker_id": "2",
          "time": "20210905 16:49"
        },
        {
          "id": "MDRW2100032390.1.8",
          "form": "많이 안달았음 좋겠는데",
          "original_form": "많이 안달았음 좋겠는데",
          "speaker_id": "2",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100032390.1.9",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100032390.1.10",
          "form": "나도 먹고 싶다",
          "original_form": "나도 먹고 싶다",
          "speaker_id": "2",
          "time": "20210905 16:50"
        },
        {
          "id": "MDRW2100032390.1.11",
          "form": "추석이라 택배 밀이긴 하지",
          "original_form": "추석이라 택배 밀이긴 하지",
          "speaker_id": "1",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100032390.1.12",
          "form": "응 부모님 댁으로 보냈는데 주문이 밀려서 언제 출발할 지 모르겠어",
          "original_form": "응 부모님 댁으로 보냈는데 주문이 밀려서 언제 출발할 지 모르겠어",
          "speaker_id": "2",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100032390.1.13",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100032390.1.14",
          "form": "인기상품인가",
          "original_form": "인기상품인가",
          "speaker_id": "1",
          "time": "20210905 16:52"
        },
        {
          "id": "MDRW2100032390.1.15",
          "form": "응 경북 행사해서 엄청 저렴하게 샀어",
          "original_form": "응 경북 행사해서 엄청 저렴하게 샀어",
          "speaker_id": "2",
          "time": "20210905 16:52"
        },
