        },
        {
          "id": "MDRW2100043404.1.3",
          "form": "초면에 실례지만 ㅎㅎ 이상형어떻게되시나여",
          "original_form": "초면에 실례지만 ㅎㅎ 이상형어떻게되시나여",
          "speaker_id": "1",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043404.1.4",
          "form": "이상형...ㅋㅋㅋ",
          "original_form": "이상형...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043404.1.5",
          "form": "일단 전 담배 안피는 사람을 선호해요",
          "original_form": "일단 전 담배 안피는 사람을 선호해요",
          "speaker_id": "2",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043404.1.6",
          "form": "ㅋㅋㅋㅋㅋㅎㅎ.",
          "original_form": "ㅋㅋㅋㅋㅋㅎㅎ.",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043404.1.7",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043404.1.8",
          "form": "글고 유흥과 거리가먼사람",
          "original_form": "글고 유흥과 거리가먼사람",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043404.1.9",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043404.1.10",
          "form": "외적인이상형은 어때요",
          "original_form": "외적인이상형은 어때요",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043404.1.11",
          "form": "근데 술은 뭐 적당히 괜찮아요",
          "original_form": "근데 술은 뭐 적당히 괜찮아요",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043404.1.12",
          "form": "키는 75이상",
          "original_form": "키는 75이상",
          "speaker_id": "2",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043404.1.13",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043404.1.14",
          "form": "마른편을 선호하는듯해요",
          "original_form": "마른편을 선호하는듯해요",
          "speaker_id": "2",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043404.1.15",
          "form": "슬랜더형 선호하시는군요",
          "original_form": "슬랜더형 선호하시는군요",
          "speaker_id": "1",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043404.1.16",
          "form": "저는 두툼..",
          "original_form": "저는 두툼..",
          "speaker_id": "1",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043404.1.17",
          "form": "말그대로 이상형입니다",
          "original_form": "말그대로 이상형입니다",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043404.1.18",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043404.1.19",
          "form": "어떤 사람을 선호하세요?",
          "original_form": "어떤 사람을 선호하세요?",
          "speaker_id": "2",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100043404.1.20",
          "form": "전 베이글남이요 ㅎㅎ 얼굴은귀여운데 몸은그렇지않은!",
          "original_form": "전 베이글남이요 ㅎㅎ 얼굴은귀여운데 몸은그렇지않은!",
