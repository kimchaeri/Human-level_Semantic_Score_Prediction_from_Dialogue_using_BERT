      "utterance": [
        {
          "id": "MDRW2100015509.1.1",
          "form": "낼 출근하시나요",
          "original_form": "낼 출근하시나요",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015509.1.2",
          "form": "넵...",
          "original_form": "넵...",
          "speaker_id": "2",
          "time": "20210825 23:23"
        },
        {
          "id": "MDRW2100015509.1.3",
          "form": "몇시 출근하세요?",
          "original_form": "몇시 출근하세요?",
          "speaker_id": "1",
          "time": "20210825 23:24"
        },
        {
          "id": "MDRW2100015509.1.4",
          "form": "9시요!",
          "original_form": "9시요!",
          "speaker_id": "2",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015509.1.5",
          "form": "보통들 9시네요 ㅎㅎ 저두요",
          "original_form": "보통들 9시네요 ㅎㅎ 저두요",
          "speaker_id": "1",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015509.1.6",
          "form": "스트레스 받으시는거있어요?",
          "original_form": "스트레스 받으시는거있어요?",
          "speaker_id": "1",
          "time": "20210825 23:27"
        },
        {
          "id": "MDRW2100015509.1.7",
          "form": "아무래도 사람관계 스트레스죠",
          "original_form": "아무래도 사람관계 스트레스죠",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015509.1.8",
          "form": "맞아요 성격 안맞는 직원있으신가요",
          "original_form": "맞아요 성격 안맞는 직원있으신가요",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015509.1.9",
          "form": "네네 그런경우도있고ㅠ",
          "original_form": "네네 그런경우도있고ㅠ",
          "speaker_id": "2",
          "time": "20210826 10:24"
        },
        {
          "id": "MDRW2100015509.1.10",
          "form": "ㅋㅋㅋ저도요 말많은직원있는데 짜증나요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ저도요 말많은직원있는데 짜증나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100015509.1.11",
          "form": "헉 괴로우시겠어요",
          "original_form": "헉 괴로우시겠어요",
          "speaker_id": "2",
          "time": "20210826 11:53"
        },
        {
          "id": "MDRW2100015509.1.12",
          "form": "넼ㅋㅋㅋㅋ 출근부터 퇴근까지 말이 끊이질않아요 ㅠ 피해다녀요 ..",
          "original_form": "넼ㅋㅋㅋㅋ 출근부터 퇴근까지 말이 끊이질않아요 ㅠ 피해다녀요 ..",
          "speaker_id": "1",
          "time": "20210826 15:00"
        },
        {
          "id": "MDRW2100015509.1.13",
          "form": "피하는게 젤 나은것가타효..",
          "original_form": "피하는게 젤 나은것가타효..",
          "speaker_id": "2",
          "time": "20210826 15:05"
        },
        {
          "id": "MDRW2100015509.1.14",
          "form": "되게 소심해서 한마디하면 꽁해있어요 ㅋㅋㅋㅋ그냥 피하는게 상책",
          "original_form": "되게 소심해서 한마디하면 꽁해있어요 ㅋㅋㅋㅋ그냥 피하는게 상책",
          "speaker_id": "1",
          "time": "20210826 15:13"
        }
      ]
