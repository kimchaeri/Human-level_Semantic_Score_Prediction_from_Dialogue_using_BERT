      "utterance": [
        {
          "id": "MDRW2100035644.1.1",
          "form": "외국 팬들 오픈마인드라서 그렇다기엔",
          "original_form": "외국 팬들 오픈마인드라서 그렇다기엔",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100035644.1.2",
          "form": "인스타 같은 거 보시면 히잡 쓴 프사하고 있는 터키 나 이쪽 팬들도 엄청 많아요",
          "original_form": "인스타 같은 거 보시면 히잡 쓴 프사하고 있는 터키 나 이쪽 팬들도 엄청 많아요",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100035644.1.3",
          "form": "아 ㅋㅋㅋ 그건 또 그렇네요 ???",
          "original_form": "아 ㅋㅋㅋ 그건 또 그렇네요 ???",
          "speaker_id": "1",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100035644.1.4",
          "form": "그분들 보수적이잖아요",
          "original_form": "그분들 보수적이잖아요",
          "speaker_id": "1",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100035644.1.5",
          "form": "그니까요....",
          "original_form": "그니까요....",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100035644.1.6",
          "form": "왕보수몬스터",
          "original_form": "왕보수몬스터",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100035644.1.7",
          "form": "아니 그럼 결혼한걸 믿지못하는 ..?",
          "original_form": "아니 그럼 결혼한걸 믿지못하는 ..?",
          "speaker_id": "1",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100035644.1.8",
          "form": "설마욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "설마욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100035644.1.9",
          "form": "이미 낳은분도 있는데?ㅠㅠ",
          "original_form": "이미 낳은분도 있는데?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100035644.1.10",
          "form": "아 ㅋㅋㅋ 제 머릿속 한분이 떠오르는군요",
          "original_form": "아 ㅋㅋㅋ 제 머릿속 한분이 떠오르는군요",
          "speaker_id": "1",
          "time": "20210829 21:50"
        },
        {
          "id": "MDRW2100035644.1.11",
          "form": "ㅋㅋㅋㅋ저 아는 동생이",
          "original_form": "ㅋㅋㅋㅋ저 아는 동생이",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035644.1.12",
          "form": "약간 못믿는건가 .. 애를 보여줘야 믿으려나요",
          "original_form": "약간 못믿는건가 .. 애를 보여줘야 믿으려나요",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035644.1.13",
          "form": "엑소팬인데",
          "original_form": "엑소팬인데",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035644.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035644.1.15",
          "form": "트위터에서 한창 그쪽팬들이랑",
          "original_form": "트위터에서 한창 그쪽팬들이랑",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035644.1.16",
          "form": "다른 팬들이랑",
          "original_form": "다른 팬들이랑",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035644.1.17",
          "form": "막 총공하면서 싸우던 시절이...",
          "original_form": "막 총공하면서 싸우던 시절이...",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035644.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ혹시아시나여",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ혹시아시나여",
          "speaker_id": "2",
          "time": "20210829 21:51"
        },
        {
          "id": "MDRW2100035644.1.19",
          "form": "아 아니 그쪽이 팬이 있는게",
          "original_form": "아 아니 그쪽이 팬이 있는게",
          "speaker_id": "1",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100035644.1.20",
          "form": "하긴..한국팬도 있는데 ..",
          "original_form": "하긴..한국팬도 있는데 ..",
          "speaker_id": "1",
          "time": "20210829 21:52"
        },
        {
          "id": "MDRW2100035644.1.21",
