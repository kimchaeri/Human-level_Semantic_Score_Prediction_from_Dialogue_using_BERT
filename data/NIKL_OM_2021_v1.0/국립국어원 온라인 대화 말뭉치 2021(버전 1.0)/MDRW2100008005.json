      "utterance": [
        {
          "id": "MDRW2100008005.1.1",
          "form": "앗... 그렇다면...편의점에 가셔야ㅠㅋㅋㅋㅋ",
          "original_form": "앗... 그렇다면...편의점에 가셔야ㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:35"
        },
        {
          "id": "MDRW2100008005.1.2",
          "form": "전 밥 먹었어요",
          "original_form": "전 밥 먹었어요",
          "speaker_id": "2",
          "time": "20210822 12:35"
        },
        {
          "id": "MDRW2100008005.1.3",
          "form": "늘 그렇듯...오징어 젓갈과 ... 김자반으로 ...",
          "original_form": "늘 그렇듯...오징어 젓갈과 ... 김자반으로 ...",
          "speaker_id": "2",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100008005.1.4",
          "form": "염분이 매우 충분한 식단이네 ㅋㅋㅋㅋ",
          "original_form": "염분이 매우 충분한 식단이네 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100008005.1.5",
          "form": "집에 먹을게 없는건 아닌데",
          "original_form": "집에 먹을게 없는건 아닌데",
          "speaker_id": "1",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100008005.1.6",
          "form": "꼭 이럴때만 라면이 없어서 외출하게 만들어 ㅋ ㅠㅠ",
          "original_form": "꼭 이럴때만 라면이 없어서 외출하게 만들어 ㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100008005.1.7",
          "form": "저희집은 라면 사놓으면",
          "original_form": "저희집은 라면 사놓으면",
          "speaker_id": "2",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100008005.1.8",
          "form": "동생이 다 먹어서",
          "original_form": "동생이 다 먹어서",
          "speaker_id": "2",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100008005.1.9",
          "form": "집에 일반라면 하나도 없고 멸치 칼국수만 남았아요",
          "original_form": "집에 일반라면 하나도 없고 멸치 칼국수만 남았아요",
          "speaker_id": "2",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100008005.1.10",
          "form": "앗 동생이... 맑은 국물 면은 싫어하는구나 ㅋㅋㅋㅋ",
          "original_form": "앗 동생이... 맑은 국물 면은 싫어하는구나 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100008005.1.11",
          "form": "호불호가 확실하구만",
          "original_form": "호불호가 확실하구만",
          "speaker_id": "1",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100008005.1.12",
          "form": "라면.. 오늘은 무슨 라면 사올까.. 추천좀",
          "original_form": "라면.. 오늘은 무슨 라면 사올까.. 추천좀",
          "speaker_id": "1",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100008005.1.13",
          "form": "ㄴㄴ 동생 국물은 좋은데",
          "original_form": "ㄴㄴ 동생 국물은 좋은데",
          "speaker_id": "2",
          "time": "20210822 12:41"
        },
        {
          "id": "MDRW2100008005.1.14",
          "form": "칼국수 면이 싫대요",
          "original_form": "칼국수 면이 싫대요",
          "speaker_id": "2",
          "time": "20210822 12:41"
        },
        {
          "id": "MDRW2100008005.1.15",
          "form": "저는 꼬꼬면이나 나가사끼 짬뽕을 추천해봅니다",
          "original_form": "저는 꼬꼬면이나 나가사끼 짬뽕을 추천해봅니다",
          "speaker_id": "2",
          "time": "20210822 12:41"
        },
        {
          "id": "MDRW2100008005.1.16",
          "form": "꼬꼬면은 집 앞 슈퍼에서 안팔고",
          "original_form": "꼬꼬면은 집 앞 슈퍼에서 안팔고",
          "speaker_id": "1",
          "time": "20210822 12:42"
        },
        {
          "id": "MDRW2100008005.1.17",
          "form": "나가사끼 짬뽕은 안먹어봤는데",
          "original_form": "나가사끼 짬뽕은 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210822 12:42"
        },
        {
          "id": "MDRW2100008005.1.18",
          "form": "맛있어? 어때?",
          "original_form": "맛있어? 어때?",
          "speaker_id": "1",
          "time": "20210822 12:42"
        },
        {
          "id": "MDRW2100008005.1.19",
          "form": "제 기준엔 ㄱㅊ았어요 맛있었습니다",
          "original_form": "제 기준엔 ㄱㅊ았어요 맛있었습니다",
          "speaker_id": "2",
          "time": "20210822 12:43"
        },
        {
          "id": "MDRW2100008005.1.20",
          "form": "독특한 시도를 하고 싶은데",
          "original_form": "독특한 시도를 하고 싶은데",
          "speaker_id": "2",
          "time": "20210822 12:43"
        },
        {
          "id": "MDRW2100008005.1.21",
          "form": "안전하게 가고 싶으면 추천드려욧",
          "original_form": "안전하게 가고 싶으면 추천드려욧",
          "speaker_id": "2",
          "time": "20210822 12:43"
        },
        {
          "id": "MDRW2100008005.1.22",
          "form": "오 참고할게 ㅋㅋ",
          "original_form": "오 참고할게 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:44"
        },
        {
          "id": "MDRW2100008005.1.23",
          "form": "찾아보니 이거도 흰국물이구나",
          "original_form": "찾아보니 이거도 흰국물이구나",
          "speaker_id": "1",
          "time": "20210822 12:44"
        },
        {
          "id": "MDRW2100008005.1.24",
          "form": "혹시 모르니 한봉지만 사봐야겠다",
          "original_form": "혹시 모르니 한봉지만 사봐야겠다",
          "speaker_id": "1",
          "time": "20210822 12:44"
        },
        {
          "id": "MDRW2100008005.1.25",
          "form": "넹넹 ㅇㅅㅇb 맛나답니당",
          "original_form": "넹넹 ㅇㅅㅇb 맛나답니당",
          "speaker_id": "2",
          "time": "20210822 12:47"
        },
        {
          "id": "MDRW2100008005.1.26",
          "form": "다른 일반 라면은",
          "original_form": "다른 일반 라면은",
          "speaker_id": "2",
          "time": "20210822 12:47"
        },
        {
          "id": "MDRW2100008005.1.27",
          "form": "언니는 어떤거 드세요??",
          "original_form": "언니는 어떤거 드세요??",
          "speaker_id": "2",
          "time": "20210822 12:47"
        },
        {
          "id": "MDRW2100008005.1.28",
          "form": "나는 매운거 좋아해서",
          "original_form": "나는 매운거 좋아해서",
          "speaker_id": "1",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100008005.1.29",
          "form": "틈새라면 먹거나 열라면에 순두부 섞어서 먹거나",
          "original_form": "틈새라면 먹거나 열라면에 순두부 섞어서 먹거나",
          "speaker_id": "1",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100008005.1.30",
          "form": "아니면 진라면 매운맛이나 신라면?",
          "original_form": "아니면 진라면 매운맛이나 신라면?",
          "speaker_id": "1",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100008005.1.31",
          "form": "열라면 순두부 조합 유명하긴 하던데",
          "original_form": "열라면 순두부 조합 유명하긴 하던데",
          "speaker_id": "2",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100008005.1.32",
          "form": "저는 열라면 따로는 먹었어도",
          "original_form": "저는 열라면 따로는 먹었어도",
          "speaker_id": "2",
          "time": "20210822 12:48"
        },
        {
          "id": "MDRW2100008005.1.33",
          "form": "순두부 넣는건 못해봤어요 엄마가 오바라고 할까바 ㅠㅠ",
          "original_form": "순두부 넣는건 못해봤어요 엄마가 오바라고 할까바 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 12:49"
        },
        {
          "id": "MDRW2100008005.1.34",
          "form": "ㅋㅋㅋㅋㅋ 시중에 파는 순두부 찌개 맛보단 덜하긴 한데",
          "original_form": "ㅋㅋㅋㅋㅋ 시중에 파는 순두부 찌개 맛보단 덜하긴 한데",
          "speaker_id": "1",
          "time": "20210822 12:49"
        },
        {
          "id": "MDRW2100008005.1.35",
          "form": "라면 하나에 밥말아 먹기는 부담스럽고",
          "original_form": "라면 하나에 밥말아 먹기는 부담스럽고",
          "speaker_id": "1",
          "time": "20210822 12:49"
        },
        {
          "id": "MDRW2100008005.1.36",
          "form": "하나만 먹기에 아쉬울 때 좋더라",
          "original_form": "하나만 먹기에 아쉬울 때 좋더라",
          "speaker_id": "1",
          "time": "20210822 12:50"
        },
        {
          "id": "MDRW2100008005.1.37",
          "form": "헐 ...",
          "original_form": "헐 ...",
          "speaker_id": "2",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100008005.1.38",
          "form": "나도 먹어보도 싶다",
          "original_form": "나도 먹어보도 싶다",
          "speaker_id": "2",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100008005.1.39",
          "form": "언니 집가면 해주나요",
          "original_form": "언니 집가면 해주나요",
          "speaker_id": "2",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100008005.1.40",
          "form": "아니... 이거때문에 몇시간 거리인 우리집까지 오겠다구...?",
          "original_form": "아니... 이거때문에 몇시간 거리인 우리집까지 오겠다구...?",
          "speaker_id": "1",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100008005.1.41",
          "form": "그냥... 해먹어...",
          "original_form": "그냥... 해먹어...",
          "speaker_id": "1",
          "time": "20210822 12:52"
        }
      ]
    }
