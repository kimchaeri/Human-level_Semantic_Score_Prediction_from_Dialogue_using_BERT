      "utterance": [
        {
          "id": "MDRW2100045116.1.1",
          "form": "배달은 자주하세요?",
          "original_form": "배달은 자주하세요?",
          "speaker_id": "1",
          "time": "20210908 00:54"
        },
        {
          "id": "MDRW2100045116.1.2",
          "form": "가끔요. 자주 하세요?",
          "original_form": "가끔요. 자주 하세요?",
          "speaker_id": "2",
          "time": "20210908 00:55"
        },
        {
          "id": "MDRW2100045116.1.3",
          "form": "기본인것같아요vvip ㅋㅋㅋ",
          "original_form": "기본인것같아요vvip ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045116.1.4",
          "form": "오늘도찜닭 먹었답니다",
          "original_form": "오늘도찜닭 먹었답니다",
          "speaker_id": "1",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100045116.1.5",
          "form": "저는 오늘 회랑 매운탕 배달요.",
          "original_form": "저는 오늘 회랑 매운탕 배달요.",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045116.1.6",
          "form": "와 한잔하셨나요?",
          "original_form": "와 한잔하셨나요?",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045116.1.7",
          "form": "저는 술을 못 마셔서 남편만요",
          "original_form": "저는 술을 못 마셔서 남편만요",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045116.1.8",
          "form": "남편은 회. 저는 매운탕",
          "original_form": "남편은 회. 저는 매운탕",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045116.1.9",
          "form": "매운탕은 서비스로안줘요?",
          "original_form": "매운탕은 서비스로안줘요?",
          "speaker_id": "1",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045116.1.10",
          "form": "5천원 주고 시켰어요",
          "original_form": "5천원 주고 시켰어요",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045116.1.11",
          "form": "거긴 매운탕 서비스로 주나요?",
          "original_form": "거긴 매운탕 서비스로 주나요?",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045116.1.12",
          "form": "아 추가구나ㅎㅎ",
          "original_form": "아 추가구나ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045116.1.13",
          "form": "네.ㅋㅋㅋ",
          "original_form": "네.ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045116.1.14",
          "form": "저는 배달은안해봣고 집앞에 포장전문있어서",
          "original_form": "저는 배달은안해봣고 집앞에 포장전문있어서",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045116.1.15",
          "form": "거기는 걍 주더라구요ㅋ",
          "original_form": "거기는 걍 주더라구요ㅋ",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045116.1.16",
          "form": "맛있겟다 매운탕",
          "original_form": "맛있겟다 매운탕",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045116.1.17",
          "form": "오..좋네요. 우린 포장도 다 사는데",
          "original_form": "오..좋네요. 우린 포장도 다 사는데",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045116.1.18",
          "form": "국물맛이 중요하자나요ㅎㅎ",
          "original_form": "국물맛이 중요하자나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045116.1.19",
          "form": "그쵸. 국물맛.ㅋㅋㅌ",
          "original_form": "그쵸. 국물맛.ㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045116.1.20",
          "form": "갑자기 먹고 싶네요",
          "original_form": "갑자기 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045116.1.21",
          "form": "전에 장이 서는 동네에서 살 때는 회 포장하고 매운탕거리 받아서 끓여 먹었는데..",
          "original_form": "전에 장이 서는 동네에서 살 때는 회 포장하고 매운탕거리 받아서 끓여 먹었는데..",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045116.1.22",
          "form": "아 근데 머리넣어주면",
          "original_form": "아 근데 머리넣어주면",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045116.1.23",
          "form": "저는 첨에 당황했었다는ㅋㅋㅋㅋ",
