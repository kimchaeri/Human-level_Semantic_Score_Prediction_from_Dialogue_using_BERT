      "utterance": [
        {
          "id": "MDRW2100030788.1.1",
          "form": "name3 집에 놀러가 본 적 있는데",
          "original_form": "&name3& 집에 놀러가 본 적 있는데",
          "speaker_id": "2",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100030788.1.2",
          "form": "거기서도 헤드스파 7 쓰더라고요",
          "original_form": "거기서도 헤드스파 7 쓰더라고요",
          "speaker_id": "2",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100030788.1.3",
          "form": "그 이름 다시 한번 부탁합니다",
          "original_form": "그 이름 다시 한번 부탁합니다",
          "speaker_id": "1",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100030788.1.4",
          "form": "아 헤드스파",
          "original_form": "아 헤드스파",
          "speaker_id": "1",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030788.1.5",
          "form": "헤드스파 7입니다",
          "original_form": "헤드스파 7입니다",
          "speaker_id": "2",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030788.1.6",
          "form": "국산인가요",
          "original_form": "국산인가요",
          "speaker_id": "1",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030788.1.7",
          "form": "7을 숫자 7로 잘 인식하네요 이름이 7 맞거든요",
          "original_form": "7을 숫자 7로 잘 인식하네요 이름이 7 맞거든요",
          "speaker_id": "2",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030788.1.8",
          "form": "관련 주가 궁금하네요",
          "original_form": "관련 주가 궁금하네요",
          "speaker_id": "1",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030788.1.9",
          "form": "국산인 거 같습니다",
          "original_form": "국산인 거 같습니다",
          "speaker_id": "2",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030788.1.10",
          "form": "한번 알아보겠습니다",
          "original_form": "한번 알아보겠습니다",
          "speaker_id": "1",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030788.1.11",
          "form": "저도 갑자기 궁금해지네요",
          "original_form": "저도 갑자기 궁금해지네요",
          "speaker_id": "2",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030788.1.12",
          "form": "알게 되시면 저한테도 정보 부탁드려요",
          "original_form": "알게 되시면 저한테도 정보 부탁드려요",
          "speaker_id": "2",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030788.1.13",
          "form": "생활 속에서 조식을 공부 할 수 있습니다",
          "original_form": "생활 속에서 조식을 공부 할 수 있습니다",
          "speaker_id": "1",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030788.1.14",
          "form": "정말 좋은 자세입니다 주식은 우리 주위 곳곳에 있군요",
          "original_form": "정말 좋은 자세입니다 주식은 우리 주위 곳곳에 있군요",
          "speaker_id": "2",
          "time": "20210904 12:34"
        },
        {
          "id": "MDRW2100030788.1.15",
          "form": "허니버터로 감자칩회사 주가가 올랐지요",
          "original_form": "허니버터로 감자칩회사 주가가 올랐지요",
          "speaker_id": "1",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030788.1.16",
          "form": "쇼핑이 제일 재밌어요",
          "original_form": "쇼핑이 제일 재밌어요",
          "speaker_id": "2",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030788.1.17",
          "form": "입소문을 무시하면 안 됩니다",
          "original_form": "입소문을 무시하면 안 됩니다",
          "speaker_id": "1",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030788.1.18",
          "form": "맞아요 아까 영상에서 찾아봤는데 이제 제조품 쪽에서",
          "original_form": "맞아요 아까 영상에서 찾아봤는데 이제 제조품 쪽에서",
          "speaker_id": "2",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030788.1.19",
          "form": "전에 코로나일때 쌍코에서",
          "original_form": "전에 코로나일때 쌍코에서",
          "speaker_id": "1",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030788.1.20",
          "form": "주식 오를 것 같더라고요",
          "original_form": "주식 오를 것 같더라고요",
          "speaker_id": "2",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030788.1.21",
          "form": "알코올 구하는 거 보고",
          "original_form": "알코올 구하는 거 보고",
          "speaker_id": "1",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030788.1.22",
          "form": "알코올 회사 산 적 있어요",
          "original_form": "알코올 회사 산 적 있어요",
          "speaker_id": "1",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030788.1.23",
          "form": "아직 시세가 없었거든요",
          "original_form": "아직 시세가 없었거든요",
          "speaker_id": "1",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030788.1.24",
          "form": "이득 많이 보셨나요",
          "original_form": "이득 많이 보셨나요",
          "speaker_id": "2",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030788.1.25",
          "form": "며칠 뒤에 상가더라고요",
          "original_form": "며칠 뒤에 상가더라고요",
          "speaker_id": "1",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030788.1.26",
          "form": "입소문 매매라고 하지요",
          "original_form": "입소문 매매라고 하지요",
          "speaker_id": "1",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100030788.1.27",
          "form": "역시 쌍 코는 사랑입니다",
          "original_form": "역시 쌍 코는 사랑입니다",
          "speaker_id": "2",
          "time": "20210904 12:35"
        }
      ]
    }
