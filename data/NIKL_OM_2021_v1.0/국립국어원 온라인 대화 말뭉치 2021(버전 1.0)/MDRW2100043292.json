      "utterance": [
        {
          "id": "MDRW2100043292.1.1",
          "form": "어쩌다 다치셨나요ㅠ",
          "original_form": "어쩌다 다치셨나요ㅠ",
          "speaker_id": "1",
          "time": "20210907 16:58"
        },
        {
          "id": "MDRW2100043292.1.2",
          "form": "ㅠㅠㅠ 집에서 체중계 못보고 발로차서 금이갔더라구요 ㅎㅎ..",
          "original_form": "ㅠㅠㅠ 집에서 체중계 못보고 발로차서 금이갔더라구요 ㅎㅎ..",
          "speaker_id": "2",
          "time": "20210907 17:10"
        },
        {
          "id": "MDRW2100043292.1.3",
          "form": "더이어트못해서 슬푸더라구요",
          "original_form": "더이어트못해서 슬푸더라구요",
          "speaker_id": "2",
          "time": "20210907 17:10"
        },
        {
          "id": "MDRW2100043292.1.4",
          "form": "헉.....",
          "original_form": "헉.....",
          "speaker_id": "1",
          "time": "20210907 17:10"
        },
        {
          "id": "MDRW2100043292.1.5",
          "form": "진짜아프셨겠다ㅠㅠㅠ",
          "original_form": "진짜아프셨겠다ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 17:11"
        },
        {
          "id": "MDRW2100043292.1.6",
          "form": "놀래셨겠어요",
          "original_form": "놀래셨겠어요",
          "speaker_id": "1",
          "time": "20210907 17:11"
        },
        {
          "id": "MDRW2100043292.1.7",
          "form": "맞아요ㅠㅠㅠㅠ그래도 얼마전에 깁스풀어서요 !!",
          "original_form": "맞아요ㅠㅠㅠㅠ그래도 얼마전에 깁스풀어서요 !!",
          "speaker_id": "2",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100043292.1.8",
          "form": "조만간 운동시작하려구용 ㅎㅎ",
          "original_form": "조만간 운동시작하려구용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100043292.1.9",
          "form": "다행이예요",
          "original_form": "다행이예요",
          "speaker_id": "1",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100043292.1.10",
          "form": "운동은 어떤거 하시나요?",
          "original_form": "운동은 어떤거 하시나요?",
          "speaker_id": "1",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100043292.1.11",
          "form": "헬스용!",
          "original_form": "헬스용!",
          "speaker_id": "2",
          "time": "20210907 17:31"
        },
        {
          "id": "MDRW2100043292.1.12",
          "form": "name1님도 하시나요?",
          "original_form": "&name1&님도 하시나요?",
          "speaker_id": "2",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100043292.1.13",
          "form": "저는 링피트 해요",
          "original_form": "저는 링피트 해요",
          "speaker_id": "1",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100043292.1.14",
          "form": "오오 링피트가 뭐죠??",
          "original_form": "오오 링피트가 뭐죠??",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100043292.1.15",
          "form": "닌텐도스위치를 이용해서 하는 운동게임이예요",
          "original_form": "닌텐도스위치를 이용해서 하는 운동게임이예요",
          "speaker_id": "1",
          "time": "20210907 18:00"
        },
        {
          "id": "MDRW2100043292.1.16",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "2",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100043292.1.17",
          "form": "운동...",
          "original_form": "운동...",
          "speaker_id": "2",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100043292.1.18",
          "form": "게임도되고운돝도되고 재밌겠네용!",
          "original_form": "게임도되고운돝도되고 재밌겠네용!",
          "speaker_id": "2",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100043292.1.19",
          "form": "네 스쿼트 100개하기 이런거 걸리면 한숨나오지만",
          "original_form": "네 스쿼트 100개하기 이런거 걸리면 한숨나오지만",
          "speaker_id": "1",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043292.1.20",
          "form": "그래도 어떻게 하게돼요ㅋㅋㅋㅋ",
          "original_form": "그래도 어떻게 하게돼요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:04"
        },
        {
          "id": "MDRW2100043292.1.21",
          "form": "오우진짜 운동되겠어욬ㅋㅋㅋㅋㅋ",
          "original_form": "오우진짜 운동되겠어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:40"
        },
        {
          "id": "MDRW2100043292.1.22",
          "form": "게임과 운동 최고의 조합이죠",
          "original_form": "게임과 운동 최고의 조합이죠",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043292.1.23",
          "form": "꾸준함에 도움이 돼요",
