      "utterance": [
        {
          "id": "MDRW2100007597.1.1",
          "form": "와 저 최근에 후드집업샀어요 기모로",
          "original_form": "와 저 최근에 후드집업샀어요 기모로",
          "speaker_id": "2",
          "time": "20210822 03:48"
        },
        {
          "id": "MDRW2100007597.1.2",
          "form": "겨울준비시작하고있다고요",
          "original_form": "겨울준비시작하고있다고요",
          "speaker_id": "2",
          "time": "20210822 03:48"
        },
        {
          "id": "MDRW2100007597.1.3",
          "form": "와 벌써 가을준비하세여?",
          "original_form": "와 벌써 가을준비하세여?",
          "speaker_id": "1",
          "time": "20210822 03:48"
        },
        {
          "id": "MDRW2100007597.1.4",
          "form": "혹시 곰이신지",
          "original_form": "혹시 곰이신지",
          "speaker_id": "1",
          "time": "20210822 03:49"
        },
        {
          "id": "MDRW2100007597.1.5",
          "form": "아니",
          "original_form": "아니",
