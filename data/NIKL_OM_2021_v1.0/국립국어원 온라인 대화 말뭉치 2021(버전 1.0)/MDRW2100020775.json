      "utterance": [
        {
          "id": "MDRW2100020775.1.1",
          "form": "얼음님 점심메뉴 정하셨나요?",
          "original_form": "얼음님 점심메뉴 정하셨나요?",
          "speaker_id": "1",
          "time": "20210828 12:24"
        },
        {
          "id": "MDRW2100020775.1.2",
          "form": "닭갈비를 먹을 예정입니다ㅎㅎ",
          "original_form": "닭갈비를 먹을 예정입니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 12:25"
        },
        {
          "id": "MDRW2100020775.1.3",
          "form": "가을님은요?",
          "original_form": "가을님은요?",
          "speaker_id": "2",
          "time": "20210828 12:25"
        },
        {
          "id": "MDRW2100020775.1.4",
          "form": "닭갈비 너무 맛있겠어요~~~! 저는 오늘 죽 먹을 예정입니다",
          "original_form": "닭갈비 너무 맛있겠어요~~~! 저는 오늘 죽 먹을 예정입니다",
          "speaker_id": "1",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100020775.1.5",
          "form": "계란죽이에욧",
          "original_form": "계란죽이에욧",
          "speaker_id": "1",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100020775.1.6",
          "form": "몸이 많이 안좋으신가봐요 어제부터 계속 죽드시고ㅠ",
          "original_form": "몸이 많이 안좋으신가봐요 어제부터 계속 죽드시고ㅠ",
          "speaker_id": "2",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100020775.1.7",
          "form": "아니에요!ㅋㅋㅋㅋㅋ 소화가 안되니까 먹는 거일 뿐이랍니다!!!!!!",
          "original_form": "아니에요!ㅋㅋㅋㅋㅋ 소화가 안되니까 먹는 거일 뿐이랍니다!!!!!!",
          "speaker_id": "1",
          "time": "20210828 12:29"
        },
        {
          "id": "MDRW2100020775.1.8",
          "form": "그리고 계란죽은 약간 맛있어서...즐기는 중....이에요...",
          "original_form": "그리고 계란죽은 약간 맛있어서...즐기는 중....이에요...",
          "speaker_id": "1",
          "time": "20210828 12:29"
        },
        {
          "id": "MDRW2100020775.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:29"
        },
        {
          "id": "MDRW2100020775.1.10",
          "form": "다행이네요ㅎㅎ 계란죽은 먹어본적 없지만 맛있겠네요",
          "original_form": "다행이네요ㅎㅎ 계란죽은 먹어본적 없지만 맛있겠네요",
          "speaker_id": "2",
          "time": "20210828 12:29"
        },
        {
          "id": "MDRW2100020775.1.11",
          "form": "나중에 꼭 드셔보세요 간편하기도 하고 맛있고!!!!",
          "original_form": "나중에 꼭 드셔보세요 간편하기도 하고 맛있고!!!!",
          "speaker_id": "1",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100020775.1.12",
          "form": "만드는 법이 쉬워서 좋더라고요 ㅋㅋㅎㅎ",
          "original_form": "만드는 법이 쉬워서 좋더라고요 ㅋㅋㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 12:30"
        },
        {
          "id": "MDRW2100020775.1.13",
          "form": "직접 요리하신다니 대단하시네요",
          "original_form": "직접 요리하신다니 대단하시네요",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020775.1.14",
          "form": "저는 요리에는 재능이 없어서ㅋㅋ",
          "original_form": "저는 요리에는 재능이 없어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020775.1.15",
          "form": "ㅋㅋㅋㅋ앗 저도 엄청 간단한것만 해먹습니다....",
          "original_form": "ㅋㅋㅋㅋ앗 저도 엄청 간단한것만 해먹습니다....",
          "speaker_id": "1",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020775.1.16",
          "form": "어려운건 못하겠더라고요ㅠㅠ",
          "original_form": "어려운건 못하겠더라고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020775.1.17",
          "form": "잘하는 분등 보면 신기해요",
          "original_form": "잘하는 분등 보면 신기해요",
          "speaker_id": "1",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020775.1.18",
          "form": "죽만드는것도 대단한거에요 저는 라면, 계란후라이가 다 입니다",
          "original_form": "죽만드는것도 대단한거에요 저는 라면, 계란후라이가 다 입니다",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020775.1.19",
          "form": "전 계란후라이를 잘 못하는 걸요 ㅋㅋㅎㅎ",
          "original_form": "전 계란후라이를 잘 못하는 걸요 ㅋㅋㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020775.1.20",
          "form": "저 반숙은 아예 시도도 못합니다..",
          "original_form": "저 반숙은 아예 시도도 못합니다..",
          "speaker_id": "1",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020775.1.21",
          "form": "반숙은 정말 어려워요ㅋㅋ",
          "original_form": "반숙은 정말 어려워요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 14:04"
        },
        {
          "id": "MDRW2100020775.1.22",
          "form": "맞아요ㅜㅜㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅜㅜㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:49"
        },
        {
          "id": "MDRW2100020775.1.23",
          "form": "반숙 잘하는 사람 너무 부러워요",
          "original_form": "반숙 잘하는 사람 너무 부러워요",
          "speaker_id": "1",
          "time": "20210828 15:49"
        },
        {
          "id": "MDRW2100020775.1.24",
          "form": "비빔밥이나 볶음밥에 예쁘게 올리고 싶은데 전 그게 안되더라고요",
          "original_form": "비빔밥이나 볶음밥에 예쁘게 올리고 싶은데 전 그게 안되더라고요",
