        },
        {
          "id": "MDRW2100042912.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210907 11:19"
        },
        {
          "id": "MDRW2100042912.1.3",
          "form": "넷플릭스구독하시나요",
          "original_form": "넷플릭스구독하시나요",
          "speaker_id": "1",
          "time": "20210907 11:21"
        },
        {
          "id": "MDRW2100042912.1.4",
          "form": "네 구독하고 있어요",
          "original_form": "네 구독하고 있어요",
          "speaker_id": "2",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100042912.1.5",
          "form": "넷플릭스 구독하세요?",
          "original_form": "넷플릭스 구독하세요?",
          "speaker_id": "2",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100042912.1.6",
          "form": "저도해요ㅎㅎ",
          "original_form": "저도해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100042912.1.7",
          "form": "최근에뭐보세요",
          "original_form": "최근에뭐보세요",
          "speaker_id": "1",
          "time": "20210907 11:25"
        },
        {
          "id": "MDRW2100042912.1.8",
          "form": "저는 슬의생 보고 있어요 ㅎㅎ",
          "original_form": "저는 슬의생 보고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 11:26"
        },
        {
          "id": "MDRW2100042912.1.9",
          "form": "근데 이번주 결방이라 ..ㅠㅠ",
          "original_form": "근데 이번주 결방이라 ..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 11:26"
        },
        {
          "id": "MDRW2100042912.1.10",
          "form": "뭐 보세요??",
          "original_form": "뭐 보세요??",
          "speaker_id": "2",
          "time": "20210907 11:26"
        },
        {
          "id": "MDRW2100042912.1.11",
          "form": "앗ㅜ글쿤요",
          "original_form": "앗ㅜ글쿤요",
          "speaker_id": "1",
          "time": "20210907 11:27"
        },
        {
          "id": "MDRW2100042912.1.12",
          "form": "저최근에 에밀리파리에가다 봤어오ㅡ",
          "original_form": "저최근에 에밀리파리에가다 봤어오ㅡ",
          "speaker_id": "1",
          "time": "20210907 11:27"
        },
        {
          "id": "MDRW2100042912.1.13",
          "form": "아 저도 그거 봤어요",
          "original_form": "아 저도 그거 봤어요",
          "speaker_id": "2",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042912.1.14",
          "form": "에밀리 진짜 자존감 높은 거 같아요 ㅎㅎ 대단",
          "original_form": "에밀리 진짜 자존감 높은 거 같아요 ㅎㅎ 대단",
          "speaker_id": "2",
          "time": "20210907 11:33"
        },
        {
          "id": "MDRW2100042912.1.15",
          "form": "ㅎㅎ너무이쁘더라고요",
          "original_form": "ㅎㅎ너무이쁘더라고요",
          "speaker_id": "1",
          "time": "20210907 11:34"
        },
        {
          "id": "MDRW2100042912.1.16",
          "form": "마자요 ㅎㅎ 그 중국인 친구도 너무 성격 좋아요",
          "original_form": "마자요 ㅎㅎ 그 중국인 친구도 너무 성격 좋아요",
          "speaker_id": "2",
          "time": "20210907 11:35"
        },
        {
          "id": "MDRW2100042912.1.17",
          "form": "ㅎㅎ맞아요",
          "original_form": "ㅎㅎ맞아요",
          "speaker_id": "1",
          "time": "20210907 11:37"
        },
        {
          "id": "MDRW2100042912.1.18",
          "form": "유투브도보세요?",
          "original_form": "유투브도보세요?",
          "speaker_id": "1",
          "time": "20210907 11:37"
        },
        {
          "id": "MDRW2100042912.1.19",
          "form": "네 그럼요",
          "original_form": "네 그럼요",
          "speaker_id": "2",
          "time": "20210907 11:38"
        },
        {
          "id": "MDRW2100042912.1.20",
          "form": "혹시 빠니보틀 아세요?",
          "original_form": "혹시 빠니보틀 아세요?",
          "speaker_id": "2",
          "time": "20210907 11:38"
        },
        {
          "id": "MDRW2100042912.1.21",
          "form": "어들어봤는데",
          "original_form": "어들어봤는데",
          "speaker_id": "1",
          "time": "20210907 11:41"
        },
        {
          "id": "MDRW2100042912.1.22",
          "form": "무슨유투브죠?",
          "original_form": "무슨유투브죠?",
          "speaker_id": "1",
          "time": "20210907 11:41"
        },
        {
          "id": "MDRW2100042912.1.23",
          "form": "여행유튜버예요 ㅎㅎ",
          "original_form": "여행유튜버예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 11:46"
        },
        {
          "id": "MDRW2100042912.1.24",
          "form": "요즘 너무 답답한데 보면서 대리만족 하고 있어요 ㅎㅎ",
          "original_form": "요즘 너무 답답한데 보면서 대리만족 하고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 11:47"
        },
        {
          "id": "MDRW2100042912.1.25",
          "form": "좋아하는 유튜버가 누구예요?",
          "original_form": "좋아하는 유튜버가 누구예요?",
          "speaker_id": "2",
          "time": "20210907 11:47"
        },
        {
          "id": "MDRW2100042912.1.26",
          "form": "여행아득하네요ㅜ코로나",
          "original_form": "여행아득하네요ㅜ코로나",
          "speaker_id": "1",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100042912.1.27",
          "form": "저는쯔양?먹방이요ㅎㅎ",
          "original_form": "저는쯔양?먹방이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 11:51"
        },
        {
          "id": "MDRW2100042912.1.28",
          "form": "으아 잘 드시는분 ㅋㅋㅋㅋ",
          "original_form": "으아 잘 드시는분 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:52"
        },
        {
          "id": "MDRW2100042912.1.29",
          "form": "진짜 그 작은 체구에 어떻게 그렇게 드시는지 ㅎㅎ",
          "original_form": "진짜 그 작은 체구에 어떻게 그렇게 드시는지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 11:52"
        }
