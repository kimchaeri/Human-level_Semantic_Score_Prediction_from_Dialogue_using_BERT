      "utterance": [
        {
          "id": "MDRW2100006933.1.1",
          "form": "좋아하긴 한데",
          "original_form": "좋아하긴 한데",
          "speaker_id": "2",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100006933.1.2",
          "form": "건강 생각해서 줄이려고 하지",
          "original_form": "건강 생각해서 줄이려고 하지",
          "speaker_id": "2",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100006933.1.3",
          "form": "야식 줄여야지",
          "original_form": "야식 줄여야지",
          "speaker_id": "1",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100006933.1.4",
          "form": "야식 뭐 좋아해?",
          "original_form": "야식 뭐 좋아해?",
          "speaker_id": "2",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100006933.1.5",
          "form": "야식의 꽃은 아무래도 라면?",
          "original_form": "야식의 꽃은 아무래도 라면?",
          "speaker_id": "1",
          "time": "20210822 00:00"
        },
        {
          "id": "MDRW2100006933.1.6",
          "form": "맞아 라면 ㅋㅋㅋ",
          "original_form": "맞아 라면 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100006933.1.7",
          "form": "사실 야식에 음식 가림은 없지 ㅋㅋㅋ",
          "original_form": "사실 야식에 음식 가림은 없지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100006933.1.8",
          "form": "맞아 나도 안가려 ㅋㅋ",
          "original_form": "맞아 나도 안가려 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100006933.1.9",
          "form": "아무래",
          "original_form": "아무래",
          "speaker_id": "1",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100006933.1.10",
          "form": "아무래도 간편한 게 편하니까",
          "original_form": "아무래도 간편한 게 편하니까",
          "speaker_id": "1",
          "time": "20210822 00:01"
        },
        {
          "id": "MDRW2100006933.1.11",
          "form": "요즘에는 간편식이 많아서 좋은 것 같아",
          "original_form": "요즘에는 간편식이 많아서 좋은 것 같아",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006933.1.12",
          "form": "그러게 렌지에 데우기만 하면 되니까",
          "original_form": "그러게 렌지에 데우기만 하면 되니까",
          "speaker_id": "1",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006933.1.13",
          "form": "편의점 음식도 다양하고",
          "original_form": "편의점 음식도 다양하고",
          "speaker_id": "2",
          "time": "20210822 00:02"
        },
        {
          "id": "MDRW2100006933.1.14",
          "form": "세상에 먹을 것은 많다 ㅋㅋㅋ",
          "original_form": "세상에 먹을 것은 많다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006933.1.15",
          "form": "내가 안먹어본 음식도 많겠지?",
          "original_form": "내가 안먹어본 음식도 많겠지?",
          "speaker_id": "2",
          "time": "20210822 00:03"
        },
        {
          "id": "MDRW2100006933.1.16",
          "form": "식도락 여행 가고 싶다",
          "original_form": "식도락 여행 가고 싶다",
          "speaker_id": "2",
          "time": "20210822 00:03"
        }
      ]
    }
