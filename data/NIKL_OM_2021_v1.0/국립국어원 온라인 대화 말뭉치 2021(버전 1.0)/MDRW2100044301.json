      "utterance": [
        {
          "id": "MDRW2100044301.1.1",
          "form": "감자샐러드 맛있지요",
          "original_form": "감자샐러드 맛있지요",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044301.1.2",
          "form": "저는 감자랑 계란들어간거 좋아해요",
          "original_form": "저는 감자랑 계란들어간거 좋아해요",
          "speaker_id": "1",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044301.1.3",
          "form": "맛있어요ㅠ 모닝빵에 발라먹으면 정말 맛있는데",
          "original_form": "맛있어요ㅠ 모닝빵에 발라먹으면 정말 맛있는데",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044301.1.4",
          "form": "맞아요 감자샐러드는 식빵보다 모닝빵이 잘어울려요",
          "original_form": "맞아요 감자샐러드는 식빵보다 모닝빵이 잘어울려요",
          "speaker_id": "1",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044301.1.5",
          "form": "먹을줄아시네요",
          "original_form": "먹을줄아시네요",
          "speaker_id": "2",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100044301.1.6",
          "form": "저는 그리고 오이를 꼭 넣어줘야해요",
          "original_form": "저는 그리고 오이를 꼭 넣어줘야해요",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044301.1.7",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044301.1.8",
          "form": "오이가 들어가야 씹는맛이 있어요",
          "original_form": "오이가 들어가야 씹는맛이 있어요",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044301.1.9",
          "form": "아 다져서 넣으시나요?",
          "original_form": "아 다져서 넣으시나요?",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044301.1.10",
          "form": "네 작게 다져서 볶음밥용 사이즈정도로요",
          "original_form": "네 작게 다져서 볶음밥용 사이즈정도로요",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044301.1.11",
          "form": "오이는 좀 호불호가 갈려서",
          "original_form": "오이는 좀 호불호가 갈려서",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044301.1.12",
          "form": "오이 싫어하세요?",
          "original_form": "오이 싫어하세요?",
          "speaker_id": "1",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044301.1.13",
          "form": "저는 좋아해요",
          "original_form": "저는 좋아해요",
          "speaker_id": "2",
          "time": "20210907 22:14"
        },
        {
          "id": "MDRW2100044301.1.14",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:15"
        }
