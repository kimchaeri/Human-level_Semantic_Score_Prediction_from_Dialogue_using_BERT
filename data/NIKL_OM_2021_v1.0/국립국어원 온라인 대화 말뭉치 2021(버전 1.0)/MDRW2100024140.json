      "utterance": [
        {
          "id": "MDRW2100024140.1.1",
          "form": "요즘에 뭐 사신거 있나요?",
          "original_form": "요즘에 뭐 사신거 있나요?",
          "speaker_id": "2",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100024140.1.2",
          "form": "일요일에 원피스와 가디건을 샀어요",
          "original_form": "일요일에 원피스와 가디건을 샀어요",
          "speaker_id": "1",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100024140.1.3",
          "form": "입어보고 사셨나요?",
          "original_form": "입어보고 사셨나요?",
          "speaker_id": "2",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100024140.1.4",
          "form": "네에~ 오랜만에 서울에있어서 아울렛을 갔어요",
          "original_form": "네에~ 오랜만에 서울에있어서 아울렛을 갔어요",
          "speaker_id": "1",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024140.1.5",
          "form": "어디 아울렛 나들이를 다녀오셨나요",
          "original_form": "어디 아울렛 나들이를 다녀오셨나요",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024140.1.6",
          "form": "파주에 다녀왔습니다",
          "original_form": "파주에 다녀왔습니다",
          "speaker_id": "1",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024140.1.7",
          "form": "파주요?",
          "original_form": "파주요?",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024140.1.8",
          "form": "쇼핑하고 식당에갔는데",
          "original_form": "쇼핑하고 식당에갔는데",
          "speaker_id": "1",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024140.1.9",
          "form": "저희 밭이 파주에 있는데 반갑네요!",
          "original_form": "저희 밭이 파주에 있는데 반갑네요!",
          "speaker_id": "2",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024140.1.10",
          "form": "개성손만두집이었어요",
          "original_form": "개성손만두집이었어요",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024140.1.11",
          "form": "파주장단콩 콩국수를 먹었는데",
          "original_form": "파주장단콩 콩국수를 먹었는데",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024140.1.12",
          "form": "파주는 칼국수 만두 이런게 많더라고요",
          "original_form": "파주는 칼국수 만두 이런게 많더라고요",
          "speaker_id": "2",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024140.1.13",
          "form": "너무 맛있었어요",
          "original_form": "너무 맛있었어요",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024140.1.14",
          "form": "맛있으셨나요?",
          "original_form": "맛있으셨나요?",
          "speaker_id": "2",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024140.1.15",
          "form": "엄청 맛있더라고요",
          "original_form": "엄청 맛있더라고요",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024140.1.16",
          "form": "콩이 유명한 곳이더라고요 두부도 사오시지 그랬어요",
          "original_form": "콩이 유명한 곳이더라고요 두부도 사오시지 그랬어요",
          "speaker_id": "2",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024140.1.17",
          "form": "두부는 없고 만두랑 콩국수만 있었어요",
          "original_form": "두부는 없고 만두랑 콩국수만 있었어요",
          "speaker_id": "1",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100024140.1.18",
          "form": "두부도 웬만하면 다 맛있어요",
          "original_form": "두부도 웬만하면 다 맛있어요",
          "speaker_id": "2",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100024140.1.19",
          "form": "원피스는 집에 와서 입어봐도 마음에 드시나요?",
          "original_form": "원피스는 집에 와서 입어봐도 마음에 드시나요?",
          "speaker_id": "2",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100024140.1.20",
          "form": "집에와서는 안입어봤습니다 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "집에와서는 안입어봤습니다 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100024140.1.21",
          "form": "쇼핑백에 들어있는채로 방에 둔것같아요",
          "original_form": "쇼핑백에 들어있는채로 방에 둔것같아요",
          "speaker_id": "1",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100024140.1.22",
          "form": "빨리 입어서 트렌드세터가 되시길 바라요",
          "original_form": "빨리 입어서 트렌드세터가 되시길 바라요",
          "speaker_id": "2",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100024140.1.23",
          "form": "피곤해서 ㅎㅎㅎ",
          "original_form": "피곤해서 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100024140.1.24",
          "form": "가을옷은 누구보다 빨리 입어야 한답니다",
          "original_form": "가을옷은 누구보다 빨리 입어야 한답니다",
          "speaker_id": "2",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100024140.1.25",
          "form": "짧아서 금방 지나가요",
          "original_form": "짧아서 금방 지나가요",
          "speaker_id": "2",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100024140.1.26",
          "form": "네에 ㅋㅋㅋㅋㅋㅋ 그러겠습니다",
          "original_form": "네에 ㅋㅋㅋㅋㅋㅋ 그러겠습니다",
          "speaker_id": "1",
          "time": "20210830 22:38"
        }
      ]
    }
