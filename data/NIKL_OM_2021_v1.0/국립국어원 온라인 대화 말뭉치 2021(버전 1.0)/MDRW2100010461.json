      "utterance": [
        {
          "id": "MDRW2100010461.1.1",
          "form": "스포츠라 좋아하시는 스포츠 있으세요",
          "original_form": "스포츠라 좋아하시는 스포츠 있으세요",
          "speaker_id": "2",
          "time": "20210823 20:06"
        },
        {
          "id": "MDRW2100010461.1.2",
          "form": "아뇨 스포츠는 딱히 없어요",
          "original_form": "아뇨 스포츠는 딱히 없어요",
          "speaker_id": "1",
          "time": "20210823 20:41"
        },
        {
          "id": "MDRW2100010461.1.3",
          "form": "제가 운동 신경이 꽝이예요",
          "original_form": "제가 운동 신경이 꽝이예요",
          "speaker_id": "1",
          "time": "20210823 20:42"
        },
        {
          "id": "MDRW2100010461.1.4",
          "form": "그래서 별로 안좋아 하지만",
          "original_form": "그래서 별로 안좋아 하지만",
          "speaker_id": "1",
          "time": "20210823 20:42"
        },
        {
          "id": "MDRW2100010461.1.5",
          "form": "올림픽은 간간히 응원했어요 ㅎㅎ",
          "original_form": "올림픽은 간간히 응원했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 20:42"
        },
        {
          "id": "MDRW2100010461.1.6",
          "form": "저는 대학때는 야구좋아했어요",
          "original_form": "저는 대학때는 야구좋아했어요",
          "speaker_id": "2",
          "time": "20210823 20:42"
        },
        {
          "id": "MDRW2100010461.1.7",
          "form": "학교를 대구에서 나와서요",
          "original_form": "학교를 대구에서 나와서요",
          "speaker_id": "2",
          "time": "20210823 20:42"
        },
        {
          "id": "MDRW2100010461.1.8",
          "form": "아 그렇군요 ㅎㅎ",
          "original_form": "아 그렇군요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 20:42"
        },
        {
          "id": "MDRW2100010461.1.9",
          "form": "저 코로나 전에 sk",
          "original_form": "저 코로나 전에 sk",
          "speaker_id": "1",
          "time": "20210823 20:43"
        },
        {
          "id": "MDRW2100010461.1.10",
          "form": "야구갔는데 완전 코앞에서보니 잼났어요",
          "original_form": "야구갔는데 완전 코앞에서보니 잼났어요",
          "speaker_id": "1",
          "time": "20210823 20:43"
        },
        {
          "id": "MDRW2100010461.1.11",
          "form": "그게 벌써 십년도 더전이라",
          "original_form": "그게 벌써 십년도 더전이라",
          "speaker_id": "2",
          "time": "20210823 20:44"
        },
        {
          "id": "MDRW2100010461.1.12",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210823 20:44"
        },
        {
          "id": "MDRW2100010461.1.13",
          "form": "그때는 이승엽도 현역이었죠",
          "original_form": "그때는 이승엽도 현역이었죠",
          "speaker_id": "2",
          "time": "20210823 20:44"
        },
        {
          "id": "MDRW2100010461.1.14",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 20:44"
        },
        {
          "id": "MDRW2100010461.1.15",
          "form": "양준혁도",
          "original_form": "양준혁도",
          "speaker_id": "2",
          "time": "20210823 20:44"
        },
        {
          "id": "MDRW2100010461.1.16",
          "form": "세상에 왠일",
          "original_form": "세상에 왠일",
          "speaker_id": "1",
          "time": "20210823 20:44"
        },
        {
          "id": "MDRW2100010461.1.17",
          "form": "양준혁",
          "original_form": "양준혁",
          "speaker_id": "1",
          "time": "20210823 20:44"
        },
        {
          "id": "MDRW2100010461.1.18",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:44"
        },
        {
          "id": "MDRW2100010461.1.19",
          "form": "요새 예능에 봐서 야구 선수인지도 모르 겠어요 ㅎㅎ",
          "original_form": "요새 예능에 봐서 야구 선수인지도 모르 겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 20:45"
        },
        {
          "id": "MDRW2100010461.1.20",
          "form": "양신 양신 이러면서 응원도 제법했어요",
          "original_form": "양신 양신 이러면서 응원도 제법했어요",
          "speaker_id": "2",
          "time": "20210823 20:45"
        },
        {
          "id": "MDRW2100010461.1.21",
          "form": "진짜 둘다 유명했죠",
          "original_form": "진짜 둘다 유명했죠",
          "speaker_id": "1",
          "time": "20210823 20:45"
        },
        {
          "id": "MDRW2100010461.1.22",
          "form": "은퇴가 가깝긴했지만",
          "original_form": "은퇴가 가깝긴했지만",
          "speaker_id": "2",
          "time": "20210823 20:45"
        },
        {
          "id": "MDRW2100010461.1.23",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210823 20:45"
        },
        {
          "id": "MDRW2100010461.1.24",
          "form": "참 그때가 오히려 야구 전성기죠",
          "original_form": "참 그때가 오히려 야구 전성기죠",
          "speaker_id": "1",
          "time": "20210823 20:45"
        },
        {
          "id": "MDRW2100010461.1.25",
          "form": "은퇴전에 안타 2천개 달성 이런거 응원하고",
          "original_form": "은퇴전에 안타 2천개 달성 이런거 응원하고",
          "speaker_id": "2",
          "time": "20210823 20:46"
        },
        {
          "id": "MDRW2100010461.1.26",
          "form": "그때도 야구장 꽉차고 그러지는 않았던거같아요",
          "original_form": "그때도 야구장 꽉차고 그러지는 않았던거같아요",
          "speaker_id": "2",
          "time": "20210823 20:46"
        },
        {
          "id": "MDRW2100010461.1.27",
          "form": "이승엽이 일본 활동할때라",
          "original_form": "이승엽이 일본 활동할때라",
          "speaker_id": "2",
          "time": "20210823 20:46"
        },
        {
          "id": "MDRW2100010461.1.28",
          "form": "일본 야구도매일 티비로봤는데",
          "original_form": "일본 야구도매일 티비로봤는데",
          "speaker_id": "2",
          "time": "20210823 20:46"
        },
        {
          "id": "MDRW2100010461.1.29",
          "form": "그땐 그랬죠",
          "original_form": "그땐 그랬죠",
          "speaker_id": "1",
          "time": "20210823 20:47"
        },
        {
          "id": "MDRW2100010461.1.30",
