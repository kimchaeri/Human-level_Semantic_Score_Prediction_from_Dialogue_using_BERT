      "utterance": [
        {
          "id": "MDRW2100044227.1.1",
          "form": "고소해서 맛있더라구요 ㅋㅋ",
          "original_form": "고소해서 맛있더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044227.1.2",
          "form": "연어초밥도 해먹으면 맛있죠!",
          "original_form": "연어초밥도 해먹으면 맛있죠!",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044227.1.3",
          "form": "맞아요... 그런데",
          "original_form": "맞아요... 그런데",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044227.1.4",
          "form": "초밥은",
          "original_form": "초밥은",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044227.1.5",
          "form": "손이 넘 많이가서ㅠㅠ",
          "original_form": "손이 넘 많이가서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044227.1.6",
          "form": "맞아요 그럴땐 그냥.. 연어덮밥이죠",
          "original_form": "맞아요 그럴땐 그냥.. 연어덮밥이죠",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044227.1.7",
          "form": "그냥 퍼먹게되는...ㅋㅋ",
          "original_form": "그냥 퍼먹게되는...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044227.1.8",
          "form": "ㅋㅋㅋㅋㅋ 좋죠",
          "original_form": "ㅋㅋㅋㅋㅋ 좋죠",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044227.1.9",
          "form": "하 근데 훈제연어는 넘 별로인거같아여",
          "original_form": "하 근데 훈제연어는 넘 별로인거같아여",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044227.1.10",
          "form": "무조건 생연어져...",
          "original_form": "무조건 생연어져...",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044227.1.11",
          "form": "훈제연어는 가끔 샐러드에나 먹는거죠..",
          "original_form": "훈제연어는 가끔 샐러드에나 먹는거죠..",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044227.1.12",
          "form": "맞아요 아님",
          "original_form": "맞아요 아님",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044227.1.13",
          "form": "억지로 빕스에 갔을 경우...",
          "original_form": "억지로 빕스에 갔을 경우...",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044227.1.14",
          "form": "그쵸 .. 빕스가면 그래도 훈제연어는 좀 먹어줘야죠..",
          "original_form": "그쵸 .. 빕스가면 그래도 훈제연어는 좀 먹어줘야죠..",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044227.1.15",
          "form": "패밀리레스토랑중에 어디 제일 좋아하세요?",
          "original_form": "패밀리레스토랑중에 어디 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100044227.1.16",
          "form": "저는 애슐리나 매드포갈릭이요 !!",
          "original_form": "저는 애슐리나 매드포갈릭이요 !!",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044227.1.17",
          "form": "자주 가는 건 아니지만 이 두개를 좋아하네요",
          "original_form": "자주 가는 건 아니지만 이 두개를 좋아하네요",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044227.1.18",
          "form": "어디 좋아하시나요?",
          "original_form": "어디 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 22:15"
        },
        {
          "id": "MDRW2100044227.1.19",
          "form": "저는 아웃백이요!!",
          "original_form": "저는 아웃백이요!!",
          "speaker_id": "1",
          "time": "20210907 22:16"
        },
        {
          "id": "MDRW2100044227.1.20",
          "form": "뷔페식은 애슐리나 쿠우쿠우 좋아해요",
          "original_form": "뷔페식은 애슐리나 쿠우쿠우 좋아해요",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044227.1.21",
          "form": "아악 아웃백 잊었어요 아웃백 투움바.. 맛있어요",
          "original_form": "아악 아웃백 잊었어요 아웃백 투움바.. 맛있어요",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044227.1.22",
          "form": "쿠우쿠우도 가성비 좋아요.. 가서 연어잔뜩먹으면.. 굿이에요",
          "original_form": "쿠우쿠우도 가성비 좋아요.. 가서 연어잔뜩먹으면.. 굿이에요",
          "speaker_id": "2",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044227.1.23",
          "form": "파스타중에 제일 맛있는 것 같아요",
          "original_form": "파스타중에 제일 맛있는 것 같아요",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044227.1.24",
          "form": "맞아요 쿠우쿠우도ㅠㅠ",
          "original_form": "맞아요 쿠우쿠우도ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044227.1.25",
          "form": "쿠우쿠우는... 와플이 최고죠..",
          "original_form": "쿠우쿠우는... 와플이 최고죠..",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100044227.1.26",
          "form": "와 맞아요... 쿠우쿠우 와플 짱이에요 ..",
          "original_form": "와 맞아요... 쿠우쿠우 와플 짱이에요 ..",
          "speaker_id": "2",
          "time": "20210907 22:28"
        },
        {
          "id": "MDRW2100044227.1.27",
          "form": "저 가면 항상 2개씩 먹고와요 ㅋㅋ",
          "original_form": "저 가면 항상 2개씩 먹고와요 ㅋㅋ",
          "speaker_id": "2",
