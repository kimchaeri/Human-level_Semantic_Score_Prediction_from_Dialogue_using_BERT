      "utterance": [
        {
          "id": "MDRW2100018456.1.1",
          "form": "연애중이세여?",
          "original_form": "연애중이세여?",
          "speaker_id": "1",
          "time": "20210827 02:13"
        },
        {
          "id": "MDRW2100018456.1.2",
          "form": "아니요~~ name1은요?",
          "original_form": "아니요~~ &name1&은요?",
          "speaker_id": "2",
          "time": "20210827 02:13"
        },
        {
          "id": "MDRW2100018456.1.3",
          "form": "저두요 ㅋㅋㅋ 소개팅같은거 받아보셧여요?",
          "original_form": "저두요 ㅋㅋㅋ 소개팅같은거 받아보셧여요?",
          "speaker_id": "1",
          "time": "20210827 02:14"
        },
        {
          "id": "MDRW2100018456.1.4",
          "form": "소개팅 예전에 해봤는데 결국 자만추...",
          "original_form": "소개팅 예전에 해봤는데 결국 자만추...",
          "speaker_id": "2",
          "time": "20210827 02:16"
        },
        {
          "id": "MDRW2100018456.1.5",
          "form": "ㅋㅋㅋㅋ소개팅 좋아하세요?",
          "original_form": "ㅋㅋㅋㅋ소개팅 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 02:16"
        },
        {
          "id": "MDRW2100018456.1.6",
          "form": "ㅋㅋㅋㅌㅋ 아뇨 전 해본적이 없어서리ㅜㅜ",
          "original_form": "ㅋㅋㅋㅌㅋ 아뇨 전 해본적이 없어서리ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018456.1.7",
          "form": "근데 저도 자만추 ㅋㅋ",
          "original_form": "근데 저도 자만추 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018456.1.8",
          "form": "첨만낫을때 어색한게 싫어요",
          "original_form": "첨만낫을때 어색한게 싫어요",
          "speaker_id": "1",
          "time": "20210827 02:17"
        },
        {
          "id": "MDRW2100018456.1.9",
          "form": "ㅋㅋㅋㅋ아 자만추들은 다 공감할 거예요",
          "original_form": "ㅋㅋㅋㅋ아 자만추들은 다 공감할 거예요",
          "speaker_id": "2",
          "time": "20210827 02:19"
        },
        {
          "id": "MDRW2100018456.1.10",
          "form": "만나서 한시간 내내 땀흘리고 싶지 않아요ㅜ",
          "original_form": "만나서 한시간 내내 땀흘리고 싶지 않아요ㅜ",
          "speaker_id": "2",
          "time": "20210827 02:20"
        },
        {
          "id": "MDRW2100018456.1.11",
          "form": "맞아요!!! 잠깐의 대화없는 그 정적 견딜수가 없어욬ㅋㅋㅋ",
          "original_form": "맞아요!!! 잠깐의 대화없는 그 정적 견딜수가 없어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:20"
        },
        {
          "id": "MDRW2100018456.1.12",
          "form": "진짜 음료만 벌컥벌컥",
          "original_form": "진짜 음료만 벌컥벌컥",
          "speaker_id": "2",
          "time": "20210827 02:23"
        },
        {
          "id": "MDRW2100018456.1.13",
          "form": "리필 3번 가능",
          "original_form": "리필 3번 가능",
          "speaker_id": "2",
          "time": "20210827 02:23"
        },
        {
          "id": "MDRW2100018456.1.14",
          "form": "이상형은 있으세요? 연예인 중에ㅎㅎ",
          "original_form": "이상형은 있으세요? 연예인 중에ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 02:23"
        },
        {
          "id": "MDRW2100018456.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 전 옥택연이요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 전 옥택연이요",
          "speaker_id": "1",
          "time": "20210827 02:24"
        },
        {
          "id": "MDRW2100018456.1.16",
          "form": "지금은 일반인 여친이랑 잘 사귀고잇지만..",
          "original_form": "지금은 일반인 여친이랑 잘 사귀고잇지만..",
          "speaker_id": "1",
          "time": "20210827 02:24"
        },
        {
          "id": "MDRW2100018456.1.17",
          "form": "ㅋㅋㅋㅋㅋ기회는 돌아온다!",
          "original_form": "ㅋㅋㅋㅋㅋ기회는 돌아온다!",
          "speaker_id": "2",
          "time": "20210827 02:27"
        },
        {
          "id": "MDRW2100018456.1.18",
          "form": "좀 덩치도 있고 건강한 스타일",
          "original_form": "좀 덩치도 있고 건강한 스타일",
          "speaker_id": "2",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018456.1.19",
          "form": "좋아하시나봐요",
          "original_form": "좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018456.1.20",
          "form": "네!! 저 몸을 좀 봐서 ㅎㅎ",
          "original_form": "네!! 저 몸을 좀 봐서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018456.1.21",
          "form": "님은 이상형이 누구예요?",
          "original_form": "님은 이상형이 누구예요?",
          "speaker_id": "1",
          "time": "20210827 02:28"
        },
        {
          "id": "MDRW2100018456.1.22",
          "form": "저는 서인국이요",
          "original_form": "저는 서인국이요",
          "speaker_id": "2",
          "time": "20210827 02:31"
        },
        {
          "id": "MDRW2100018456.1.23",
          "form": "ㅎ..아시죠 그 뭐랄까 에잇 모르겠다",
          "original_form": "ㅎ..아시죠 그 뭐랄까 에잇 모르겠다",
          "speaker_id": "2",
          "time": "20210827 02:31"
        },
        {
          "id": "MDRW2100018456.1.24",
          "form": "멋있더라고요 요즘ㅋㅋ",
          "original_form": "멋있더라고요 요즘ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 02:31"
        },
        {
          "id": "MDRW2100018456.1.25",
          "form": "아앜ㅋㅌㅌㅌㅋㅌㅌㅋㅋㅋ 서인국은 웃을때 넘 매력적이죠",
          "original_form": "아앜ㅋㅌㅌㅌㅋㅌㅌㅋㅋㅋ 서인국은 웃을때 넘 매력적이죠",
          "speaker_id": "1",
          "time": "20210827 02:32"
        },
        {
          "id": "MDRW2100018456.1.26",
          "form": "그 눈웃음이 어휴 ^^",
          "original_form": "그 눈웃음이 어휴 ^^",
          "speaker_id": "1",
          "time": "20210827 02:32"
        },
        {
          "id": "MDRW2100018456.1.27",
          "form": "ㅋㅋㅋㅋㅋㄱ사람 홀리는 짝눈 아닙니까!",
          "original_form": "ㅋㅋㅋㅋㅋㄱ사람 홀리는 짝눈 아닙니까!",
          "speaker_id": "2",
          "time": "20210827 02:33"
        },
        {
          "id": "MDRW2100018456.1.28",
