      "utterance": [
        {
          "id": "MDRW2100010085.1.1",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "1",
          "time": "20210823 14:02"
        },
        {
          "id": "MDRW2100010085.1.2",
          "form": "아뇨! 직장인이에요",
          "original_form": "아뇨! 직장인이에요",
          "speaker_id": "2",
          "time": "20210823 14:02"
        },
        {
          "id": "MDRW2100010085.1.3",
          "form": "아 저두 직장인입니다ㅋㅋ",
          "original_form": "아 저두 직장인입니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:03"
        },
        {
          "id": "MDRW2100010085.1.4",
          "form": "요즘 일하다보면 대학생 때가 너무 그리워요",
          "original_form": "요즘 일하다보면 대학생 때가 너무 그리워요",
          "speaker_id": "1",
          "time": "20210823 14:03"
        },
        {
          "id": "MDRW2100010085.1.5",
          "form": "그쵸ㅠㅠ 저도 예전에 지금이 좋을 때다 그럼 뭔소리야했는데",
          "original_form": "그쵸ㅠㅠ 저도 예전에 지금이 좋을 때다 그럼 뭔소리야했는데",
          "speaker_id": "2",
          "time": "20210823 14:04"
        },
        {
          "id": "MDRW2100010085.1.6",
          "form": "약간이나마 알 거같아요",
          "original_form": "약간이나마 알 거같아요",
          "speaker_id": "2",
          "time": "20210823 14:04"
        },
        {
          "id": "MDRW2100010085.1.7",
          "form": "진짜 그때만큼 맘편할 때가 없었는데",
          "original_form": "진짜 그때만큼 맘편할 때가 없었는데",
          "speaker_id": "1",
          "time": "20210823 14:04"
        },
        {
          "id": "MDRW2100010085.1.8",
          "form": "시험공부하던 것도 그리워요",
          "original_form": "시험공부하던 것도 그리워요",
          "speaker_id": "1",
          "time": "20210823 14:05"
        },
        {
          "id": "MDRW2100010085.1.9",
          "form": "그러게요ㅋㅋ 저는 기숙사 살았어서 시험기간에 동기들이랑 모여서 밤샘하고 야식시켜먹고 ㅋㅋ",
          "original_form": "그러게요ㅋㅋ 저는 기숙사 살았어서 시험기간에 동기들이랑 모여서 밤샘하고 야식시켜먹고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 14:05"
        },
        {
          "id": "MDRW2100010085.1.10",
          "form": "너무재밌었겠어요",
          "original_form": "너무재밌었겠어요",
          "speaker_id": "1",
          "time": "20210823 14:06"
        },
        {
          "id": "MDRW2100010085.1.11",
          "form": "저는 자취했어가지구",
          "original_form": "저는 자취했어가지구",
          "speaker_id": "1",
          "time": "20210823 14:06"
        },
        {
          "id": "MDRW2100010085.1.12",
          "form": "학교랑 가까운 곳에서 다니셨어요?",
          "original_form": "학교랑 가까운 곳에서 다니셨어요?",
          "speaker_id": "2",
          "time": "20210823 14:06"
        },
        {
          "id": "MDRW2100010085.1.13",
          "form": "네 대학부근에 살았어서 동기들이랑 수업끝나고 저희집에서 밥먹고",
          "original_form": "네 대학부근에 살았어서 동기들이랑 수업끝나고 저희집에서 밥먹고",
          "speaker_id": "1",
          "time": "20210823 14:07"
        },
        {
          "id": "MDRW2100010085.1.14",
          "form": "아지트였어요ㅋㅋㅋ",
          "original_form": "아지트였어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:07"
        },
        {
          "id": "MDRW2100010085.1.15",
          "form": "아 뭔지 알 거같아요",
          "original_form": "아 뭔지 알 거같아요",
          "speaker_id": "2",
          "time": "20210823 14:07"
        },
        {
          "id": "MDRW2100010085.1.16",
          "form": "저는 수강신청하러 피시방가곤했는데",
          "original_form": "저는 수강신청하러 피시방가곤했는데",
          "speaker_id": "1",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100010085.1.17",
          "form": "마라탕님은요?",
          "original_form": "마라탕님은요?",
          "speaker_id": "1",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100010085.1.18",
          "form": "저는 그냥 기숙사에서 수강신청했어요ㅋㅋ",
          "original_form": "저는 그냥 기숙사에서 수강신청했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100010085.1.19",
          "form": "기숙사 인터넷이 빨랐나봐요ㅋㅋ?",
          "original_form": "기숙사 인터넷이 빨랐나봐요ㅋㅋ?",
          "speaker_id": "1",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100010085.1.20",
          "form": "그냥 되면 되고 안되며 안되지...하는 마음으로 학교다녔던 거같아요ㅋㅋ",
          "original_form": "그냥 되면 되고 안되며 안되지...하는 마음으로 학교다녔던 거같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 14:09"
        },
        {
          "id": "MDRW2100010085.1.21",
          "form": "ㅋㅋ 쿨하시네요ㅋㅋ 요즘 졸업하고 대학동기들이 다 뭐하고사냐고 연락만하고 만나질 못하네요",
          "original_form": "ㅋㅋ 쿨하시네요ㅋㅋ 요즘 졸업하고 대학동기들이 다 뭐하고사냐고 연락만하고 만나질 못하네요",
          "speaker_id": "1",
          "time": "20210823 14:10"
        },
        {
          "id": "MDRW2100010085.1.22",
          "form": "저는 졸업한지 얼마안돼서 대부분 취준이에요ㅠ",
          "original_form": "저는 졸업한지 얼마안돼서 대부분 취준이에요ㅠ",
          "speaker_id": "2",
          "time": "20210823 14:11"
        }
      ]
    }
  ]
