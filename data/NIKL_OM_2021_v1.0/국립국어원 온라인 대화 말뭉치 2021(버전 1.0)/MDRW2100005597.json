      "utterance": [
        {
          "id": "MDRW2100005597.1.1",
          "form": "엄마 내일 뭐먹지?",
          "original_form": "엄마 내일 뭐먹지?",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100005597.1.2",
          "form": "추어탕 사먹을까",
          "original_form": "추어탕 사먹을까",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100005597.1.3",
          "form": "오 맛있겠다 그래 ㅎㅎ",
          "original_form": "오 맛있겠다 그래 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100005597.1.4",
          "form": "그래 교회마치고",
          "original_form": "그래 교회마치고",
          "speaker_id": "2",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100005597.1.5",
          "form": "웅웅 교회 가까이 있는 추어탕집 맛있다",
          "original_form": "웅웅 교회 가까이 있는 추어탕집 맛있다",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100005597.1.6",
          "form": "구랭 ㅋ그럼 아침은 적게 먹어야겠군",
          "original_form": "구랭 ㅋ그럼 아침은 적게 먹어야겠군",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100005597.1.7",
          "form": "그래 추어탕 든든히 먹으려면",
          "original_form": "그래 추어탕 든든히 먹으려면",
          "speaker_id": "2",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100005597.1.8",
          "form": "웅 ㅋㅋ 아침은 굶어야겠는디?",
          "original_form": "웅 ㅋㅋ 아침은 굶어야겠는디?",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100005597.1.9",
          "form": "굶을것 까지야",
          "original_form": "굶을것 까지야",
          "speaker_id": "2",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100005597.1.10",
          "form": "그럼 좀만 먹자 ㅎㅎ",
          "original_form": "그럼 좀만 먹자 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:59"
        },
        {
          "id": "MDRW2100005597.1.11",
          "form": "그래 거기 김치가 맛있다",
          "original_form": "그래 거기 김치가 맛있다",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100005597.1.12",
          "form": "맞아 ㅠㅠ 김치랑 깍두기 둘다",
          "original_form": "맞아 ㅠㅠ 김치랑 깍두기 둘다",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100005597.1.13",
          "form": "그래 반찬 리필해서 많이 먹자",
          "original_form": "그래 반찬 리필해서 많이 먹자",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100005597.1.14",
          "form": "구래용 ㅎㅎ 기대되는군",
          "original_form": "구래용 ㅎㅎ 기대되는군",
          "speaker_id": "1",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100005597.1.15",
          "form": "그러게 ㅋ",
          "original_form": "그러게 ㅋ",
          "speaker_id": "2",
          "time": "20210822 01:00"
        },
        {
          "id": "MDRW2100005597.1.16",
          "form": "추어탕 넘 맛나다",
          "original_form": "추어탕 넘 맛나다",
