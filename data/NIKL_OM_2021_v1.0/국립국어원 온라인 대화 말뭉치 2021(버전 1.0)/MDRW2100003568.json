      "utterance": [
        {
          "id": "MDRW2100003568.1.1",
          "form": "전시회 보러 다니세요?",
          "original_form": "전시회 보러 다니세요?",
          "speaker_id": "2",
          "time": "20210821 02:11"
        },
        {
          "id": "MDRW2100003568.1.2",
          "form": "전시회는 잘안보고 영화 가끔보러다녀요",
          "original_form": "전시회는 잘안보고 영화 가끔보러다녀요",
          "speaker_id": "1",
          "time": "20210821 02:11"
        },
        {
          "id": "MDRW2100003568.1.3",
          "form": "전시회 좋아하세요?",
          "original_form": "전시회 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 02:11"
        },
        {
          "id": "MDRW2100003568.1.4",
          "form": "저는 대학 때 과제하러 보러 다녔는데 요즘에는 잘 안다녀요",
          "original_form": "저는 대학 때 과제하러 보러 다녔는데 요즘에는 잘 안다녀요",
          "speaker_id": "2",
          "time": "20210821 02:12"
        },
        {
          "id": "MDRW2100003568.1.5",
          "form": "코로나 때문인지 큰 전시도 많이 없더라구요",
          "original_form": "코로나 때문인지 큰 전시도 많이 없더라구요",
          "speaker_id": "2",
          "time": "20210821 02:12"
        },
        {
          "id": "MDRW2100003568.1.6",
          "form": "코로나땜에 공연들도 줄고 타격이 심하겠더라구요",
          "original_form": "코로나땜에 공연들도 줄고 타격이 심하겠더라구요",
          "speaker_id": "1",
          "time": "20210821 02:12"
        },
        {
          "id": "MDRW2100003568.1.7",
          "form": "네 공연 업계가 타격이 크겠더라구요",
          "original_form": "네 공연 업계가 타격이 크겠더라구요",
          "speaker_id": "2",
          "time": "20210821 02:13"
        },
        {
          "id": "MDRW2100003568.1.8",
          "form": "주말에 영화보고왔는데 그래도 영화관은 사람 많아요",
          "original_form": "주말에 영화보고왔는데 그래도 영화관은 사람 많아요",
          "speaker_id": "1",
          "time": "20210821 02:13"
        },
        {
          "id": "MDRW2100003568.1.9",
          "form": "콘서트 같은 건 전혀 못하는 것 같고요",
          "original_form": "콘서트 같은 건 전혀 못하는 것 같고요",
          "speaker_id": "2",
          "time": "20210821 02:13"
        },
        {
          "id": "MDRW2100003568.1.10",
          "form": "무슨 영화 보셨어요?",
          "original_form": "무슨 영화 보셨어요?",
          "speaker_id": "2",
          "time": "20210821 02:13"
        },
        {
          "id": "MDRW2100003568.1.11",
          "form": "모가디슈  보고왔어요",
          "original_form": "모가디슈  보고왔어요",
          "speaker_id": "1",
          "time": "20210821 02:13"
        },
        {
          "id": "MDRW2100003568.1.12",
          "form": "아 그거 재밌다고 하더라구요",
          "original_form": "아 그거 재밌다고 하더라구요",
          "speaker_id": "2",
          "time": "20210821 02:14"
        },
        {
          "id": "MDRW2100003568.1.13",
          "form": "어떠셨어요?",
          "original_form": "어떠셨어요?",
          "speaker_id": "2",
          "time": "20210821 02:14"
        },
        {
          "id": "MDRW2100003568.1.14",
          "form": "저도 내용모르고갔는데 생각보다 재밌었어요",
          "original_form": "저도 내용모르고갔는데 생각보다 재밌었어요",
          "speaker_id": "1",
          "time": "20210821 02:14"
        },
        {
          "id": "MDRW2100003568.1.15",
          "form": "2시간인데 지루하지도 않고",
          "original_form": "2시간인데 지루하지도 않고",
          "speaker_id": "1",
          "time": "20210821 02:14"
        },
        {
          "id": "MDRW2100003568.1.16",
          "form": "실화라고 하니까 더 재밌어 보이더라구요",
          "original_form": "실화라고 하니까 더 재밌어 보이더라구요",
          "speaker_id": "2",
          "time": "20210821 02:15"
        },
        {
          "id": "MDRW2100003568.1.17",
          "form": "다들 연기 잘하더라구요  조인성이랑 김윤석이랑",
          "original_form": "다들 연기 잘하더라구요  조인성이랑 김윤석이랑",
          "speaker_id": "1",
          "time": "20210821 02:15"
        },
        {
          "id": "MDRW2100003568.1.18",
          "form": "저도 극장 가서 봐야겠네요",
          "original_form": "저도 극장 가서 봐야겠네요",
          "speaker_id": "2",
          "time": "20210821 02:17"
        },
        {
          "id": "MDRW2100003568.1.19",
          "form": "미리 어플에서 예매해야겠어요",
          "original_form": "미리 어플에서 예매해야겠어요",
          "speaker_id": "2",
          "time": "20210821 02:18"
        },
        {
          "id": "MDRW2100003568.1.20",
          "form": "요즘 티켓 값도 비싸더라구요",
          "original_form": "요즘 티켓 값도 비싸더라구요",
          "speaker_id": "2",
          "time": "20210821 02:18"
        },
        {
          "id": "MDRW2100003568.1.21",
          "form": "시간나실때 보고오세요 추천해요ㅎ",
          "original_form": "시간나실때 보고오세요 추천해요ㅎ",
          "speaker_id": "1",
          "time": "20210821 02:18"
        },
        {
