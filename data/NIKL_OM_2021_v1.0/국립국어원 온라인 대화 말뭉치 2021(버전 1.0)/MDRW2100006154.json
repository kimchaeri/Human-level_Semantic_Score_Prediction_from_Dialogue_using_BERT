        },
        {
          "id": "MDRW2100006154.1.2",
          "form": "탔어요",
          "original_form": "탔어요",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100006154.1.3",
          "form": "ktx타고 가서 ㅎㅎ",
          "original_form": "ktx타고 가서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100006154.1.4",
          "form": "강릉에서 울릉도 가는 배편이 있는건가요크",
          "original_form": "강릉에서 울릉도 가는 배편이 있는건가요크",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006154.1.5",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006154.1.6",
          "form": "네 강릉에서 많죠",
          "original_form": "네 강릉에서 많죠",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006154.1.7",
          "form": "울진인가도 있을텐데",
          "original_form": "울진인가도 있을텐데",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006154.1.8",
          "form": "그렇군요 강릉..울진 모두 멀어서 울릉도 가긴 쉽지 않겠어요.ㅡㅜㅜㅋㅋ",
          "original_form": "그렇군요 강릉..울진 모두 멀어서 울릉도 가긴 쉽지 않겠어요.ㅡㅜㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100006154.1.9",
          "form": "진짜 작정하고 가야죠",
          "original_form": "진짜 작정하고 가야죠",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100006154.1.10",
          "form": "날씨 안좋으면 배끊깁니다 ㅋㅋ",
          "original_form": "날씨 안좋으면 배끊깁니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100006154.1.11",
          "form": "아..그럼 못나오는건가요?",
          "original_form": "아..그럼 못나오는건가요?",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006154.1.12",
          "form": "네 동기 갇혔어요ㅋㅋ",
          "original_form": "네 동기 갇혔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006154.1.13",
          "form": "휴가 펑!!",
          "original_form": "휴가 펑!!",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006154.1.14",
          "form": "헉.. 함부로 가면 안되겠네요",
          "original_form": "헉.. 함부로 가면 안되겠네요",
          "speaker_id": "2",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100006154.1.15",
          "form": "네 근데 저는 좋아서",
          "original_form": "네 근데 저는 좋아서",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006154.1.16",
          "form": "대만족이었어요",
          "original_form": "대만족이었어요",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006154.1.17",
          "form": "그러게요 쉽지않은 경험이네요 부럽습니다",
          "original_form": "그러게요 쉽지않은 경험이네요 부럽습니다",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006154.1.18",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100006154.1.19",
          "form": "독도도 갔거든요!ㅋㅋ",
          "original_form": "독도도 갔거든요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100006154.1.20",
          "form": "와 독도 가기 어렵다던데요",
          "original_form": "와 독도 가기 어렵다던데요",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100006154.1.21",
          "form": "3대가 덕을 쌓아야 합니다",
          "original_form": "3대가 덕을 쌓아야 합니다",
          "speaker_id": "1",
          "time": "20210907 21:47"
