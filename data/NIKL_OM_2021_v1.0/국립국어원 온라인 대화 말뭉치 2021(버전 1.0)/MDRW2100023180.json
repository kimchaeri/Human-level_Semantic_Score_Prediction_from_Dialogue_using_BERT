      "utterance": [
        {
          "id": "MDRW2100023180.1.1",
          "form": "원래는 한식이 아닌데 한국식으로 변한 것도 있어요",
          "original_form": "원래는 한식이 아닌데 한국식으로 변한 것도 있어요",
          "speaker_id": "2",
          "time": "20210829 22:35"
        },
        {
          "id": "MDRW2100023180.1.2",
          "form": "k당했군요..",
          "original_form": "k당했군요..",
          "speaker_id": "1",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023180.1.3",
          "form": "1인 1메뉴 음식인가요",
          "original_form": "1인 1메뉴 음식인가요",
          "speaker_id": "1",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023180.1.4",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023180.1.5",
          "form": "네 그렇습니다 1인 1메뉴예요",
          "original_form": "네 그렇습니다 1인 1메뉴예요",
          "speaker_id": "2",
          "time": "20210829 22:36"
        },
        {
          "id": "MDRW2100023180.1.6",
          "form": "밥이 들어가나요",
          "original_form": "밥이 들어가나요",
          "speaker_id": "1",
          "time": "20210829 22:37"
        },
        {
          "id": "MDRW2100023180.1.7",
          "form": "네 원본도 한국식도 둘 다 밥이 있어요",
          "original_form": "네 원본도 한국식도 둘 다 밥이 있어요",
          "speaker_id": "2",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023180.1.8",
          "form": "원래는 일본식인가요?",
          "original_form": "원래는 일본식인가요?",
          "speaker_id": "1",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023180.1.9",
          "form": "네 맞습니다 일본식입니다!",
          "original_form": "네 맞습니다 일본식입니다!",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023180.1.10",
          "form": "두구두구",
          "original_form": "두구두구",
          "speaker_id": "2",
          "time": "20210829 22:45"
        },
        {
          "id": "MDRW2100023180.1.11",
          "form": "앗 덮밥!",
          "original_form": "앗 덮밥!",
          "speaker_id": "1",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023180.1.12",
          "form": "덮밥이군요!",
          "original_form": "덮밥이군요!",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023180.1.13",
          "form": "돈부리?",
          "original_form": "돈부리?",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023180.1.14",
          "form": "앗 아쉽습니다! 비슷합니다",
          "original_form": "앗 아쉽습니다! 비슷합니다",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023180.1.15",
          "form": "비슷해요!!!",
          "original_form": "비슷해요!!!",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023180.1.16",
          "form": "그럼 카레인가요?",
          "original_form": "그럼 카레인가요?",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023180.1.17",
          "form": "아닙니다 원본은 일본식이여요",
          "original_form": "아닙니다 원본은 일본식이여요",
          "speaker_id": "2",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100023180.1.18",
          "form": "아 제가 말을 잘못했어요",
          "original_form": "아 제가 말을 잘못했어요",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023180.1.19",
          "form": "특정 돈부리랑 닮았어요",
          "original_form": "특정 돈부리랑 닮았어요",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023180.1.20",
          "form": "그 돈부리에 들어가는 재료가 메인입니다",
          "original_form": "그 돈부리에 들어가는 재료가 메인입니다",
          "speaker_id": "2",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023180.1.21",
          "form": "그렇다면 돈가스이군요!",
          "original_form": "그렇다면 돈가스이군요!",
          "speaker_id": "1",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023180.1.22",
          "form": "딩동댕!!!!!!!!",
          "original_form": "딩동댕!!!!!!!!",
          "speaker_id": "2",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023180.1.23",
          "form": "이번에 되게 빨리 맞추셨어요",
          "original_form": "이번에 되게 빨리 맞추셨어요",
          "speaker_id": "2",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023180.1.24",
          "form": "점점 실력이 느나봐요 ㅋㅋㅋㅋ",
          "original_form": "점점 실력이 느나봐요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023180.1.25",
          "form": "제 최애음식 5대장이 있거든요",
          "original_form": "제 최애음식 5대장이 있거든요",
          "speaker_id": "1",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023180.1.26",
          "form": "그 중에 하나는 타코구요",
          "original_form": "그 중에 하나는 타코구요",
          "speaker_id": "1",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023180.1.27",
          "form": "4게가 남았습니다",
          "original_form": "4게가 남았습니다",
          "speaker_id": "1",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023180.1.28",
          "form": "하나씩 맞춰보기 어떠세요",
          "original_form": "하나씩 맞춰보기 어떠세요",
          "speaker_id": "1",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100023180.1.29",
          "form": "오 좋아요 좋아요",
          "original_form": "오 좋아요 좋아요",
          "speaker_id": "2",
          "time": "20210829 22:51"
        }
      ]
    }
