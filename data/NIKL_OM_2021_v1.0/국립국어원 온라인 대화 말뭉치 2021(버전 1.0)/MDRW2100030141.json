      "utterance": [
        {
          "id": "MDRW2100030141.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210903 23:24"
        },
        {
          "id": "MDRW2100030141.1.2",
          "form": "반가오여",
          "original_form": "반가오여",
          "speaker_id": "1",
          "time": "20210903 23:29"
        },
        {
          "id": "MDRW2100030141.1.3",
          "form": "팔이 점점 아파오네여",
          "original_form": "팔이 점점 아파오네여",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030141.1.4",
          "form": "원래 그래여",
          "original_form": "원래 그래여",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030141.1.5",
          "form": "욕나와요 막",
          "original_form": "욕나와요 막",
          "speaker_id": "1",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030141.1.6",
          "form": "그쵸 팔아픈건 모두의 공통 증상인가봐요",
          "original_form": "그쵸 팔아픈건 모두의 공통 증상인가봐요",
          "speaker_id": "2",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030141.1.7",
          "form": "잘때 대박..ㅎ",
          "original_form": "잘때 대박..ㅎ",
          "speaker_id": "1",
          "time": "20210903 23:39"
        },
        {
          "id": "MDRW2100030141.1.8",
          "form": "30분전에 저희 엄마가",
          "original_form": "30분전에 저희 엄마가",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030141.1.9",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030141.1.10",
          "form": "증상 물어보며 얼음찜질이라도 해줄까 물어보고 가시더니",
          "original_form": "증상 물어보며 얼음찜질이라도 해줄까 물어보고 가시더니",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030141.1.11",
          "form": "제가 드라마보고 가니까 주무시고 계시네요",
          "original_form": "제가 드라마보고 가니까 주무시고 계시네요",
          "speaker_id": "2",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030141.1.12",
          "form": "그거라더 하라던데",
          "original_form": "그거라더 하라던데",
          "speaker_id": "1",
          "time": "20210903 23:40"
        },
        {
          "id": "MDRW2100030141.1.13",
          "form": "크게 걱정되셨던건 아닌건지..\r\n졸음이 많이 왔던건지",
          "original_form": "크게 걱정되셨던건 아닌건지..\r\n졸음이 많이 왔던건지",
          "speaker_id": "2",
          "time": "20210903 23:41"
        },
        {
          "id": "MDRW2100030141.1.14",
          "form": "1차는 조금 걱정이 덜한가봐요 원래",
          "original_form": "1차는 조금 걱정이 덜한가봐요 원래",
          "speaker_id": "1",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030141.1.15",
          "form": "아픔은 본인만 제일 잘 알죠",
          "original_form": "아픔은 본인만 제일 잘 알죠",
          "speaker_id": "1",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100030141.1.16",
          "form": "아플ㄸㅐ 잘 안알아주면 서운하죠.",
          "original_form": "아플ㄸㅐ 잘 안알아주면 서운하죠.",
          "speaker_id": "1",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100030141.1.17",
          "form": "네 그렇긴 하죠\r\n그래도 아까 많이 걱정해주셨던걸로 봐서 졸음이 걱정을 이겼었나봐요ㅋㅋㅋ",
          "original_form": "네 그렇긴 하죠\r\n그래도 아까 많이 걱정해주셨던걸로 봐서 졸음이 걱정을 이겼었나봐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100030141.1.18",
          "form": "원래 그렇죠..",
