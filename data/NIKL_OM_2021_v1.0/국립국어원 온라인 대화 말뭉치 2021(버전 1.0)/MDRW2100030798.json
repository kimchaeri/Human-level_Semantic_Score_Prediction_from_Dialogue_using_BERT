        },
        {
          "id": "MDRW2100030798.1.2",
          "form": "여행?",
          "original_form": "여행?",
          "speaker_id": "1",
          "time": "20210904 20:35"
        },
        {
          "id": "MDRW2100030798.1.3",
          "form": "나도 키워드 바꾸려고 했는데 ㅋㅋㅋ",
          "original_form": "나도 키워드 바꾸려고 했는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:35"
        },
        {
          "id": "MDRW2100030798.1.4",
          "form": "그럼 name2씨는 가본곳 중에 젤 좋은 여행지 어디야?",
          "original_form": "그럼 &name2&씨는 가본곳 중에 젤 좋은 여행지 어디야?",
          "speaker_id": "1",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100030798.1.5",
          "form": "나는 올드퀘벡이 제일좋았어!",
          "original_form": "나는 올드퀘벡이 제일좋았어!",
          "speaker_id": "2",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100030798.1.6",
          "form": "불란서느낌의 캐나다...",
          "original_form": "불란서느낌의 캐나다...",
          "speaker_id": "2",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100030798.1.7",
          "form": "도깨비촬영지 가봤는데",
          "original_form": "도깨비촬영지 가봤는데",
          "speaker_id": "2",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100030798.1.8",
          "form": "엄청좋았어",
          "original_form": "엄청좋았어",
          "speaker_id": "2",
          "time": "20210904 20:36"
        },
        {
          "id": "MDRW2100030798.1.9",
          "form": "오 캐나다",
          "original_form": "오 캐나다",
          "speaker_id": "1",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100030798.1.10",
          "form": "거긴 불어 쓰지??",
          "original_form": "거긴 불어 쓰지??",
          "speaker_id": "1",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100030798.1.11",
          "form": "ㅇㅇㅇㅇ불어쓰더라...!",
          "original_form": "ㅇㅇㅇㅇ불어쓰더라...!",
          "speaker_id": "2",
          "time": "20210904 21:16"
        },
        {
          "id": "MDRW2100030798.1.12",
          "form": "고렇구만",
          "original_form": "고렇구만",
          "speaker_id": "1",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100030798.1.13",
          "form": "사실 난 도깨비 본 적 없어서 잘 모르겠다 ㅋㅋㅋ",
          "original_form": "사실 난 도깨비 본 적 없어서 잘 모르겠다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:24"
        },
        {
          "id": "MDRW2100030798.1.14",
          "form": "그때 필리핀에 있었거든...",
          "original_form": "그때 필리핀에 있었거든...",
          "speaker_id": "1",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100030798.1.15",
          "form": "사실나도",
          "original_form": "사실나도",
          "speaker_id": "2",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100030798.1.16",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100030798.1.17",
          "form": "어땠어",
          "original_form": "어땠어",
          "speaker_id": "2",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100030798.1.18",
          "form": "잘은 기억 안 나네",
          "original_form": "잘은 기억 안 나네",
          "speaker_id": "1",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100030798.1.19",
          "form": "영어공부 하러 갔는데 가서 놀기만 했어 ㅋㅋㅋ",
          "original_form": "영어공부 하러 갔는데 가서 놀기만 했어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 21:25"
        },
        {
          "id": "MDRW2100030798.1.20",
          "form": "나 로그원이랑 신과함께도(그 한국영화 맞음) 필리핀에서 봤어",
          "original_form": "나 로그원이랑 신과함께도(그 한국영화 맞음) 필리핀에서 봤어",
          "speaker_id": "1",
          "time": "20210904 21:26"
        },
        {
          "id": "MDRW2100030798.1.21",
          "form": "헉헉",
          "original_form": "헉헉",
          "speaker_id": "2",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100030798.1.22",
          "form": "초ㅔ근에구나",
          "original_form": "초ㅔ근에구나",
          "speaker_id": "2",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100030798.1.23",
          "form": "17년 초 18년 초 두 번 갔을걸",
          "original_form": "17년 초 18년 초 두 번 갔을걸",
          "speaker_id": "1",
          "time": "20210904 23:05"
        },
        {
          "id": "MDRW2100030798.1.24",
          "form": "올드퀘벡 가서 뭐 했어??",
          "original_form": "올드퀘벡 가서 뭐 했어??",
          "speaker_id": "1",
          "time": "20210904 23:33"
        },
        {
          "id": "MDRW2100030798.1.25",
          "form": "뭐 맛있는거 먹은거 있어...?",
          "original_form": "뭐 맛있는거 먹은거 있어...?",
          "speaker_id": "1",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100030798.1.26",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100030798.1.27",
          "form": "푸틴을먹었러",
          "original_form": "푸틴을먹었러",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100030798.1.28",
          "form": "맛있더라",
          "original_form": "맛있더라",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100030798.1.29",
          "form": "그리고 푸아그라랑",
          "original_form": "그리고 푸아그라랑",
          "speaker_id": "2",
          "time": "20210904 23:35"
        },
        {
          "id": "MDRW2100030798.1.30",
          "form": "암튼그런걸먹렀러",
          "original_form": "암튼그런걸먹렀러",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100030798.1.31",
          "form": "그리고풍경이랑자연이너무좋앙",
          "original_form": "그리고풍경이랑자연이너무좋앙",
          "speaker_id": "2",
          "time": "20210904 23:36"
        },
        {
          "id": "MDRW2100030798.1.32",
          "form": "푸아그라 말로만 들어봤는데",
          "original_form": "푸아그라 말로만 들어봤는데",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100030798.1.33",
          "form": "근데 푸틴이라니...????",
          "original_form": "근데 푸틴이라니...????",
          "speaker_id": "1",
          "time": "20210904 23:37"
        },
        {
          "id": "MDRW2100030798.1.34",
          "form": "푸팀....",
          "original_form": "푸팀....",
          "speaker_id": "2",
          "time": "20210905 02:12"
        },
        {
          "id": "MDRW2100030798.1.35",
          "form": "푸틴...",
          "original_form": "푸틴...",
          "speaker_id": "2",
          "time": "20210905 02:12"
        },
        {
          "id": "MDRW2100030798.1.36",
          "form": "그쪽 푸틴말고... 캐나다푸틴...",
          "original_form": "그쪽 푸틴말고... 캐나다푸틴...",
          "speaker_id": "2",
          "time": "20210905 02:13"
        },
        {
          "id": "MDRW2100030798.1.37",
          "form": "감자튀김에 치즈있는",
          "original_form": "감자튀김에 치즈있는",
          "speaker_id": "2",
          "time": "20210905 02:13"
        },
        {
          "id": "MDRW2100030798.1.38",
          "form": "음식인데...",
          "original_form": "음식인데...",
          "speaker_id": "2",
          "time": "20210905 02:13"
        },
        {
          "id": "MDRW2100030798.1.39",
          "form": "맛좋아..",
          "original_form": "맛좋아..",
          "speaker_id": "2",
          "time": "20210905 02:13"
        },
        {
          "id": "MDRW2100030798.1.40",
          "form": "풍경 얘기 하니까",
          "original_form": "풍경 얘기 하니까",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100030798.1.41",
          "form": "나 아일랜드 갔는데 엄청 좋더라",
          "original_form": "나 아일랜드 갔는데 엄청 좋더라",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100030798.1.42",
          "form": "막 산이랑 바다랑 절벽이랑 들판이 사방에 있고",
          "original_form": "막 산이랑 바다랑 절벽이랑 들판이 사방에 있고",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100030798.1.43",
          "form": "그 스켈리그 마이클이라고",
          "original_form": "그 스켈리그 마이클이라고",
          "speaker_id": "1",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100030798.1.44",
          "form": "스타워즈 촬영지도 갔어 ㅋㅋㅋ",
          "original_form": "스타워즈 촬영지도 갔어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100030798.1.45",
          "form": "아일랜드?!",
          "original_form": "아일랜드?!",
          "speaker_id": "2",
          "time": "20210905 14:05"
        },
        {
          "id": "MDRW2100030798.1.46",
          "form": "너무부럽다잉",
          "original_form": "너무부럽다잉",
          "speaker_id": "2",
          "time": "20210905 14:06"
        }
      ]
    }
