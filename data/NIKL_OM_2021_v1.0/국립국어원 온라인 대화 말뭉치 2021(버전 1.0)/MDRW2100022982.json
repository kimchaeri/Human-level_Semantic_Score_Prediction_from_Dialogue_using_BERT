      "utterance": [
        {
          "id": "MDRW2100022982.1.1",
          "form": "한달차이면 이정돈 해야한다는게 있는데",
          "original_form": "한달차이면 이정돈 해야한다는게 있는데",
          "speaker_id": "2",
          "time": "20210829 19:59"
        },
        {
          "id": "MDRW2100022982.1.2",
          "form": "일이 안 늘어서ㅠㅠ",
          "original_form": "일이 안 늘어서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 20:00"
        },
        {
          "id": "MDRW2100022982.1.3",
          "form": "괜찮어 이런사람 있고 저런사람 있는거지",
          "original_form": "괜찮어 이런사람 있고 저런사람 있는거지",
          "speaker_id": "1",
          "time": "20210829 20:00"
        },
        {
          "id": "MDRW2100022982.1.4",
          "form": "구박받고있엉?",
          "original_form": "구박받고있엉?",
          "speaker_id": "1",
          "time": "20210829 20:00"
        },
        {
          "id": "MDRW2100022982.1.5",
          "form": "차라리 권고사직해줬으면 하는 마음으로",
          "original_form": "차라리 권고사직해줬으면 하는 마음으로",
          "speaker_id": "2",
          "time": "20210829 20:00"
        },
        {
          "id": "MDRW2100022982.1.6",
          "form": "다니고 있어..",
          "original_form": "다니고 있어..",
          "speaker_id": "2",
          "time": "20210829 20:00"
        },
        {
          "id": "MDRW2100022982.1.7",
          "form": "눈치가 보이징",
          "original_form": "눈치가 보이징",
          "speaker_id": "2",
          "time": "20210829 20:00"
        },
        {
          "id": "MDRW2100022982.1.8",
          "form": "쭈그리에다가..",
          "original_form": "쭈그리에다가..",
          "speaker_id": "2",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100022982.1.9",
          "form": "ㅋㅋㅋㅋㅋ눈치보지마 경력직+1년된 나도 모르는거 투성이임",
          "original_form": "ㅋㅋㅋㅋㅋ눈치보지마 경력직+1년된 나도 모르는거 투성이임",
          "speaker_id": "1",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100022982.1.10",
          "form": "이게 눈치보고 있으면 그게 눈에 보이더라구",
          "original_form": "이게 눈치보고 있으면 그게 눈에 보이더라구",
          "speaker_id": "1",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100022982.1.11",
          "form": "위축되면 일도 더 안돼",
          "original_form": "위축되면 일도 더 안돼",
          "speaker_id": "1",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100022982.1.12",
          "form": "맞아.. 다 보이니까 일하는 게 더 꼬이는 느낌이야",
          "original_form": "맞아.. 다 보이니까 일하는 게 더 꼬이는 느낌이야",
          "speaker_id": "2",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100022982.1.13",
          "form": "약간 맘을 비워야 더 잘돼 ㅋㅋ 특히 말하는건",
          "original_form": "약간 맘을 비워야 더 잘돼 ㅋㅋ 특히 말하는건",
          "speaker_id": "1",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100022982.1.14",
          "form": "눈치보이는 건 어쩔 수 없는 것 같아",
          "original_form": "눈치보이는 건 어쩔 수 없는 것 같아",
          "speaker_id": "2",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100022982.1.15",
          "form": "동료들은 잘 대해줘?",
          "original_form": "동료들은 잘 대해줘?",
          "speaker_id": "1",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100022982.1.16",
          "form": "잘해주는데 일할 때는 좀 스타일들이 안 맞는다고 해야하나",
          "original_form": "잘해주는데 일할 때는 좀 스타일들이 안 맞는다고 해야하나",
          "speaker_id": "2",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100022982.1.17",
          "form": "그냥 내가 쭈글거리는 게 문제인 것 같아",
          "original_form": "그냥 내가 쭈글거리는 게 문제인 것 같아",
          "speaker_id": "2",
          "time": "20210829 20:04"
        },
        {
          "id": "MDRW2100022982.1.18",
          "form": "오 업무스타일은 맞춰가야지",
          "original_form": "오 업무스타일은 맞춰가야지",
          "speaker_id": "1",
          "time": "20210829 20:04"
        },
        {
          "id": "MDRW2100022982.1.19",
          "form": "괜찮앙 너무 떨지말고해 곧 잘할 수 있게 될거야 ㅋㅋ",
          "original_form": "괜찮앙 너무 떨지말고해 곧 잘할 수 있게 될거야 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:04"
        },
        {
          "id": "MDRW2100022982.1.20",
          "form": "각자 일하는 거다보니까 맞춰가거나 그런 일은 없는데 일 사후처리에서 부딪치는 건 어쩔 수 없는 것 같어",
          "original_form": "각자 일하는 거다보니까 맞춰가거나 그런 일은 없는데 일 사후처리에서 부딪치는 건 어쩔 수 없는 것 같어",
          "speaker_id": "2",
          "time": "20210829 20:05"
        },
        {
          "id": "MDRW2100022982.1.21",
          "form": "고마워",
          "original_form": "고마워",
          "speaker_id": "2",
          "time": "20210829 20:05"
        },
        {
          "id": "MDRW2100022982.1.22",
          "form": "너는 힘든 거 없니",
          "original_form": "너는 힘든 거 없니",
          "speaker_id": "2",
          "time": "20210829 20:05"
        },
        {
          "id": "MDRW2100022982.1.23",
          "form": "요즘 일이 좀 많긴 한데 괜찮아 버틸 수 있는 정도라 ㅋㅋ",
          "original_form": "요즘 일이 좀 많긴 한데 괜찮아 버틸 수 있는 정도라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:05"
        },
        {
          "id": "MDRW2100022982.1.24",
          "form": "힘들땐 말하렴",
          "original_form": "힘들땐 말하렴",
          "speaker_id": "2",
          "time": "20210829 20:06"
        },
        {
          "id": "MDRW2100022982.1.25",
          "form": "웅 그리고 다같이 일하는거라 괜찮아 혼자하면 넘 힘들지",
          "original_form": "웅 그리고 다같이 일하는거라 괜찮아 혼자하면 넘 힘들지",
          "speaker_id": "1",
          "time": "20210829 20:06"
        }
      ]
    }
