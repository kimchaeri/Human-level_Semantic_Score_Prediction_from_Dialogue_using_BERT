      "utterance": [
        {
          "id": "MDRW2100018009.1.1",
          "form": "도서나 잡지 좋아하시나요?",
          "original_form": "도서나 잡지 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100018009.1.2",
          "form": "도서나 잡지 좋아하시나요?",
          "original_form": "도서나 잡지 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100018009.1.3",
          "form": "요즘은 책을 거의 안보게 되네요",
          "original_form": "요즘은 책을 거의 안보게 되네요",
          "speaker_id": "2",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100018009.1.4",
          "form": "저도 장르소설만 보고 있어요ㅎㅎ",
          "original_form": "저도 장르소설만 보고 있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:18"
        },
        {
          "id": "MDRW2100018009.1.5",
          "form": "어떤 장르요",
          "original_form": "어떤 장르요",
          "speaker_id": "2",
          "time": "20210826 22:21"
        },
        {
          "id": "MDRW2100018009.1.6",
          "form": "신무협, 대체역사, 로판, 현판, 스포츠 등등 많이 봐요ㅋㅋ",
          "original_form": "신무협, 대체역사, 로판, 현판, 스포츠 등등 많이 봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:24"
        },
        {
          "id": "MDRW2100018009.1.7",
          "form": "엄청 여러가지 보시네요\n전 고작 웹툰정도요ㅋ",
          "original_form": "엄청 여러가지 보시네요\n전 고작 웹툰정도요ㅋ",
          "speaker_id": "2",
          "time": "20210826 22:25"
        },
        {
          "id": "MDRW2100018009.1.8",
          "form": "그래서 매번 충전을 해야해요ㅋㅋㅌ 어떤 웹툰 좋아하세요?",
          "original_form": "그래서 매번 충전을 해야해요ㅋㅋㅌ 어떤 웹툰 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100018009.1.9",
          "form": "전엔 간떨어지는동거 봤었고요",
          "original_form": "전엔 간떨어지는동거 봤었고요",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018009.1.10",
          "form": "보다가 재미없어서 접는게 많네요",
          "original_form": "보다가 재미없어서 접는게 많네요",
          "speaker_id": "2",
          "time": "20210826 22:49"
        },
        {
          "id": "MDRW2100018009.1.11",
          "form": "아무래도 작가님과 독자가 완전히 같은 생각을 하지는 않으니까요...저도 보다가 하차한 작품이 많아요ㅋㅋ",
          "original_form": "아무래도 작가님과 독자가 완전히 같은 생각을 하지는 않으니까요...저도 보다가 하차한 작품이 많아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100018009.1.12",
          "form": "네 짜증나는 캐릭터가 나오면 넘어가기가 쉽지 않더라고요ㅋ",
          "original_form": "네 짜증나는 캐릭터가 나오면 넘어가기가 쉽지 않더라고요ㅋ",
          "speaker_id": "2",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018009.1.13",
          "form": "처분이 너무 가볍다고 생각이 들어도 그렇구요ㅠ",
          "original_form": "처분이 너무 가볍다고 생각이 들어도 그렇구요ㅠ",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018009.1.14",
          "form": "제입맛에 맞는거 찾기가 쉽지않네요",
          "original_form": "제입맛에 맞는거 찾기가 쉽지않네요",
          "speaker_id": "2",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100018009.1.15",
          "form": "이래서 독자들이 창작을 시작하나봐요ㅋㅋ",
          "original_form": "이래서 독자들이 창작을 시작하나봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:22"
        }
      ]
