      "utterance": [
        {
          "id": "MDRW2100023909.1.1",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100023909.1.2",
          "form": "제주도 보쌈집에 중국인들도 와서 기웃 거리길래",
          "original_form": "제주도 보쌈집에 중국인들도 와서 기웃 거리길래",
          "speaker_id": "1",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100023909.1.3",
          "form": "음식이 낯설어서 그런가보다~ 뭐 파는지 궁금한가보다~",
          "original_form": "음식이 낯설어서 그런가보다~ 뭐 파는지 궁금한가보다~",
          "speaker_id": "1",
          "time": "20210830 19:59"
        },
        {
          "id": "MDRW2100023909.1.4",
          "form": "했는데 자꾸 저희 테이블에 기웃거리면서",
          "original_form": "했는데 자꾸 저희 테이블에 기웃거리면서",
          "speaker_id": "1",
          "time": "20210830 20:00"
        },
        {
          "id": "MDRW2100023909.1.5",
          "form": "큰소리로 막 떠들고 삿대질하고 그래서",
          "original_form": "큰소리로 막 떠들고 삿대질하고 그래서",
          "speaker_id": "1",
          "time": "20210830 20:00"
        },
        {
          "id": "MDRW2100023909.1.6",
          "form": "제가 중국어로 조용해달라 했어요",
          "original_form": "제가 중국어로 조용해달라 했어요",
          "speaker_id": "1",
          "time": "20210830 20:00"
        },
        {
          "id": "MDRW2100023909.1.7",
          "form": "그랬더니 동기들 와아아 박수짝짞짝ㅋㅋㅋㅋ반응 핵웃김",
          "original_form": "그랬더니 동기들 와아아 박수짝짞짝ㅋㅋㅋㅋ반응 핵웃김",
          "speaker_id": "1",
          "time": "20210830 20:00"
        },
        {
          "id": "MDRW2100023909.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100023909.1.9",
          "form": "중국어로 조용히가 머에요",
          "original_form": "중국어로 조용히가 머에요",
          "speaker_id": "2",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100023909.1.10",
          "form": "진짜 조용히 시키고 싶을때 많앗는데 ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 조용히 시키고 싶을때 많앗는데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100023909.1.11",
          "form": "안찡디엔!",
          "original_form": "안찡디엔!",
          "speaker_id": "1",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100023909.1.12",
          "form": "안찡디엔",
          "original_form": "안찡디엔",
          "speaker_id": "2",
          "time": "20210830 20:02"
        },
        {
          "id": "MDRW2100023909.1.13",
          "form": "성조가 굉장히 중요하지만 뭐 알아듣겠죠",
          "original_form": "성조가 굉장히 중요하지만 뭐 알아듣겠죠",
          "speaker_id": "1",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100023909.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100023909.1.15",
          "form": "그냥 고래고래 화내면 알아듣지않을까여",
          "original_form": "그냥 고래고래 화내면 알아듣지않을까여",
          "speaker_id": "2",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100023909.1.16",
          "form": "모를걸요...",
          "original_form": "모를걸요...",
          "speaker_id": "1",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100023909.1.17",
          "form": "우리 전공 수업 들으러온 중국인들 진짜 너무 떠들어서",
          "original_form": "우리 전공 수업 들으러온 중국인들 진짜 너무 떠들어서",
          "speaker_id": "2",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100023909.1.18",
          "form": "저 중국에 여행 갔을 때 영어로 엄청 열불냈는데도",
          "original_form": "저 중국에 여행 갔을 때 영어로 엄청 열불냈는데도",
          "speaker_id": "1",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100023909.1.19",
          "form": "콧방구도 안껴요",
          "original_form": "콧방구도 안껴요",
          "speaker_id": "1",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100023909.1.20",
          "form": "우리가 수업을 못들을 정도엿는데",
          "original_form": "우리가 수업을 못들을 정도엿는데",
          "speaker_id": "2",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100023909.1.21",
          "form": "중국어를 몰라서 한이엇어요",
          "original_form": "중국어를 몰라서 한이엇어요",
          "speaker_id": "2",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100023909.1.22",
          "form": "그러면 차오쓰러 안찡디엔! 이렇게 하면 될 거같아요",
          "original_form": "그러면 차오쓰러 안찡디엔! 이렇게 하면 될 거같아요",
          "speaker_id": "1",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100023909.1.23",
          "form": "대충 시끄럽다 조용 좀 해라 이런 뜻이구",
          "original_form": "대충 시끄럽다 조용 좀 해라 이런 뜻이구",
          "speaker_id": "1",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100023909.1.24",
          "form": "좀 세게 말하고 싶으시면 \"삐주이!\" 하면 돼요",
          "original_form": "좀 세게 말하고 싶으시면 \"삐주이!\" 하면 돼요",
          "speaker_id": "1",
          "time": "20210830 20:04"
        },
        {
          "id": "MDRW2100023909.1.25",
          "form": "삐주이",
          "original_form": "삐주이",
          "speaker_id": "2",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023909.1.26",
          "form": "가 젤 쉽겠네여",
          "original_form": "가 젤 쉽겠네여",
          "speaker_id": "2",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023909.1.27",
          "form": "네네 외우기도 쉽죠",
          "original_form": "네네 외우기도 쉽죠",
          "speaker_id": "1",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023909.1.28",
          "form": "어차피 젤 강하게 말하고 싶어요",
          "original_form": "어차피 젤 강하게 말하고 싶어요",
          "speaker_id": "2",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023909.1.29",
          "form": "뭐 뜻은 그냥 닥.. 그런 뜻이에요",
          "original_form": "뭐 뜻은 그냥 닥.. 그런 뜻이에요",
          "speaker_id": "1",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023909.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023909.1.31",
          "form": "맘에 드네요 굿",
          "original_form": "맘에 드네요 굿",
          "speaker_id": "2",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023909.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023909.1.33",
          "form": "입 다물어! 이런 뜻",
          "original_form": "입 다물어! 이런 뜻",
          "speaker_id": "1",
          "time": "20210830 20:05"
        },
        {
          "id": "MDRW2100023909.1.34",
          "form": "되게 알차게 살아오셧네여",
          "original_form": "되게 알차게 살아오셧네여",
          "speaker_id": "2",
          "time": "20210830 20:05"
        }
      ]
    }
