      "utterance": [
        {
          "id": "MDRW2100001408.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211015 11:41"
        },
        {
          "id": "MDRW2100001408.1.2",
          "form": "계시나요?",
          "original_form": "계시나요?",
          "speaker_id": "1",
          "time": "20211015 11:43"
        },
        {
          "id": "MDRW2100001408.1.3",
          "form": "안녕하세요! 점심메뉴는 정하셨나요",
          "original_form": "안녕하세요! 점심메뉴는 정하셨나요",
          "speaker_id": "2",
          "time": "20211015 11:43"
        },
        {
          "id": "MDRW2100001408.1.4",
          "form": "우동 이미 먹었어요",
          "original_form": "우동 이미 먹었어요",
          "speaker_id": "1",
          "time": "20211015 11:43"
        },
        {
          "id": "MDRW2100001408.1.5",
          "form": "오 점심을 일찍 드시나봐요?",
          "original_form": "오 점심을 일찍 드시나봐요?",
          "speaker_id": "2",
          "time": "20211015 11:43"
        },
        {
          "id": "MDRW2100001408.1.6",
          "form": "네 오늘은 아점개념으로요",
          "original_form": "네 오늘은 아점개념으로요",
          "speaker_id": "1",
          "time": "20211015 11:43"
        },
        {
          "id": "MDRW2100001408.1.7",
          "form": "저도 점심으로 국물 메뉴 먹고싶네요",
          "original_form": "저도 점심으로 국물 메뉴 먹고싶네요",
          "speaker_id": "2",
          "time": "20211015 11:44"
        },
        {
          "id": "MDRW2100001408.1.8",
          "form": "짬뽕 드세요 맛나겠네유",
          "original_form": "짬뽕 드세요 맛나겠네유",
          "speaker_id": "1",
          "time": "20211015 11:45"
        },
        {
          "id": "MDRW2100001408.1.9",
          "form": "흠 해물짬뽕 갑자기 땡기네요 ㅋㅋ",
          "original_form": "흠 해물짬뽕 갑자기 땡기네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 11:46"
        },
        {
          "id": "MDRW2100001408.1.10",
          "form": "메뉴 추천 감사합니다 ",
          "original_form": "메뉴 추천 감사합니다 ",
          "speaker_id": "2",
          "time": "20211015 11:46"
        },
        {
          "id": "MDRW2100001408.1.11",
          "form": "네 곧 식사시간이시죠",
          "original_form": "네 곧 식사시간이시죠",
          "speaker_id": "1",
          "time": "20211015 11:46"
        },
        {
          "id": "MDRW2100001408.1.12",
          "form": "즐거운 점심시간 되세요",
          "original_form": "즐거운 점심시간 되세요",
          "speaker_id": "1",
          "time": "20211015 11:47"
        },
        {
          "id": "MDRW2100001408.1.13",
          "form": "커피도 드시구용",
          "original_form": "커피도 드시구용",
          "speaker_id": "1",
          "time": "20211015 11:47"
        },
        {
          "id": "MDRW2100001408.1.14",
          "form": "커피도 추천해주실래요?",
          "original_form": "커피도 추천해주실래요?",
          "speaker_id": "2",
          "time": "20211015 11:49"
        },
        {
          "id": "MDRW2100001408.1.15",
          "form": "달달한거 당기시면 이디야 흑당라떼 엑스트라사이즈용",
          "original_form": "달달한거 당기시면 이디야 흑당라떼 엑스트라사이즈용",
          "speaker_id": "1",
          "time": "20211015 11:50"
        },
        {
          "id": "MDRW2100001408.1.16",
          "form": "오 저 달다구리 완전 좋아합니다 ㅋㅋ",
          "original_form": "오 저 달다구리 완전 좋아합니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 11:50"
        },
        {
          "id": "MDRW2100001408.1.17",
          "form": "흑당라떼.. 펄까지 추가해야겠어요",
          "original_form": "흑당라떼.. 펄까지 추가해야겠어요",
          "speaker_id": "2",
          "time": "20211015 11:50"
        },
        {
          "id": "MDRW2100001408.1.18",
          "form": "ㅋㅋㅋㅋ 좋아하시는구나",
          "original_form": "ㅋㅋㅋㅋ 좋아하시는구나",
          "speaker_id": "1",
          "time": "20211015 11:51"
        },
        {
          "id": "MDRW2100001408.1.19",
          "form": "네 취저 메뉴추천 감사합니다 ㅋㅋ",
          "original_form": "네 취저 메뉴추천 감사합니다 ㅋㅋ",
