      "utterance": [
        {
          "id": "MDRW2100023660.1.1",
          "form": "원하는 상사의 모습 있나요?",
          "original_form": "원하는 상사의 모습 있나요?",
          "speaker_id": "2",
          "time": "20210830 15:02"
        },
        {
          "id": "MDRW2100023660.1.2",
          "form": "ㅋㅋ 꼰대가 아니었으면 해요",
          "original_form": "ㅋㅋ 꼰대가 아니었으면 해요",
          "speaker_id": "1",
          "time": "20210830 16:15"
        },
        {
          "id": "MDRW2100023660.1.3",
          "form": "그거 진짜 크네요ㅋㅋㅋㅋ",
          "original_form": "그거 진짜 크네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 16:45"
        },
        {
          "id": "MDRW2100023660.1.4",
          "form": "그게 최고긴 하죠",
          "original_form": "그게 최고긴 하죠",
          "speaker_id": "2",
          "time": "20210830 16:45"
        },
        {
          "id": "MDRW2100023660.1.5",
          "form": "하.. 전 진짜 보수적인 직장은 싫은데",
          "original_form": "하.. 전 진짜 보수적인 직장은 싫은데",
          "speaker_id": "1",
          "time": "20210830 16:50"
        },
        {
          "id": "MDRW2100023660.1.6",
          "form": "맘대로 될지는 모르겠네요ㅋㅋ",
          "original_form": "맘대로 될지는 모르겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:50"
        },
        {
          "id": "MDRW2100023660.1.7",
          "form": "상사가 일 못하는 것도 끔직하죠",
          "original_form": "상사가 일 못하는 것도 끔직하죠",
          "speaker_id": "1",
          "time": "20210830 16:50"
        },
        {
          "id": "MDRW2100023660.1.8",
          "form": "그쵸ㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:08"
        },
        {
          "id": "MDRW2100023660.1.9",
          "form": "아.. 그것도 최악이죠ㅋㅋㅋㅋ",
          "original_form": "아.. 그것도 최악이죠ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:08"
        },
        {
          "id": "MDRW2100023660.1.10",
          "form": "거기에 플러스는 잘하는줄 안다는거에요",
          "original_form": "거기에 플러스는 잘하는줄 안다는거에요",
          "speaker_id": "2",
          "time": "20210830 18:08"
        },
        {
          "id": "MDRW2100023660.1.11",
          "form": "오 완전 싫네요 ㅋㅋㅋㅋ",
          "original_form": "오 완전 싫네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:05"
        },
        {
          "id": "MDRW2100023660.1.12",
          "form": "아 이것도 싫어요 아부떠는거 좋아하는 사람",
          "original_form": "아 이것도 싫어요 아부떠는거 좋아하는 사람",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100023660.1.13",
          "form": "제가 그런걸 잘 못해서^^",
          "original_form": "제가 그런걸 잘 못해서^^",
          "speaker_id": "1",
          "time": "20210830 19:06"
        },
        {
          "id": "MDRW2100023660.1.14",
          "form": "그것도 싫죠 일이라도 잘하면 모를까 사교만 하러 오는 사람 있잖아요!",
          "original_form": "그것도 싫죠 일이라도 잘하면 모를까 사교만 하러 오는 사람 있잖아요!",
          "speaker_id": "2",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023660.1.15",
          "form": "그런분은 꼭 선을 넘으셔..",
          "original_form": "그런분은 꼭 선을 넘으셔..",
          "speaker_id": "2",
          "time": "20210830 19:19"
        },
        {
          "id": "MDRW2100023660.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100023660.1.17",
          "form": "입만 잘 놀리고ㅋㅋ",
          "original_form": "입만 잘 놀리고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:38"
        },
        {
          "id": "MDRW2100023660.1.18",
          "form": "하지만 사실 뭐 상사 욕이라도 하고 싶은 심정입니다",
          "original_form": "하지만 사실 뭐 상사 욕이라도 하고 싶은 심정입니다",
          "speaker_id": "1",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100023660.1.19",
          "form": "얼른 돈벌고 싶어요~",
          "original_form": "얼른 돈벌고 싶어요~",
          "speaker_id": "1",
          "time": "20210830 19:40"
        },
        {
          "id": "MDRW2100023660.1.20",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100023660.1.21",
          "form": "저두요 돈 한 번 있어보면 없을때 더 답답해요",
          "original_form": "저두요 돈 한 번 있어보면 없을때 더 답답해요",
          "speaker_id": "2",
          "time": "20210830 19:44"
        },
        {
          "id": "MDRW2100023660.1.22",
          "form": "맞아요 얼른 취업하고 싶어요",
          "original_form": "맞아요 얼른 취업하고 싶어요",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100023660.1.23",
          "form": "수험 공부 그만하고 ㅎㅎ",
          "original_form": "수험 공부 그만하고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:15"
        },
        {
          "id": "MDRW2100023660.1.24",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100023660.1.25",
          "form": "후딱 탈출해요!",
          "original_form": "후딱 탈출해요!",
          "speaker_id": "2",
          "time": "20210830 22:23"
        },
        {
          "id": "MDRW2100023660.1.26",
          "form": "직업 고를때 젤 중요한 기준이 뭐예요??",
          "original_form": "직업 고를때 젤 중요한 기준이 뭐예요??",
          "speaker_id": "1",
          "time": "20210831 09:05"
        },
        {
          "id": "MDRW2100023660.1.27",
