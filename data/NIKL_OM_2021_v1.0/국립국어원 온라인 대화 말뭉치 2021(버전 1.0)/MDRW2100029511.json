      "utterance": [
        {
          "id": "MDRW2100029511.1.1",
          "form": "커피창고에서",
          "original_form": "커피창고에서",
          "speaker_id": "2",
          "time": "20210903 09:48"
        },
        {
          "id": "MDRW2100029511.1.2",
          "form": "시켜요",
          "original_form": "시켜요",
          "speaker_id": "2",
          "time": "20210903 09:48"
        },
        {
          "id": "MDRW2100029511.1.3",
          "form": "매달 그달의 커피 50프로 세일해서 팔죠",
          "original_form": "매달 그달의 커피 50프로 세일해서 팔죠",
          "speaker_id": "2",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100029511.1.4",
          "form": "커피쇼핑몰인가요",
          "original_form": "커피쇼핑몰인가요",
          "speaker_id": "1",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100029511.1.5",
          "form": "커피맛을 잘아시나봅니다",
          "original_form": "커피맛을 잘아시나봅니다",
          "speaker_id": "1",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100029511.1.6",
          "form": "네 커피 원두 쇼핑몰이요",
          "original_form": "네 커피 원두 쇼핑몰이요",
          "speaker_id": "2",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100029511.1.7",
          "form": "신커피 좋아하는데",
          "original_form": "신커피 좋아하는데",
          "speaker_id": "2",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100029511.1.8",
          "form": "갈아서 파나요",
          "original_form": "갈아서 파나요",
          "speaker_id": "1",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100029511.1.9",
          "form": "신커피 싫어합니다",
          "original_form": "신커피 싫어합니다",
          "speaker_id": "1",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100029511.1.10",
          "form": "역류성 있어서 못마셔요",
          "original_form": "역류성 있어서 못마셔요",
          "speaker_id": "2",
          "time": "20210903 09:49"
        },
        {
          "id": "MDRW2100029511.1.11",
          "form": "갈아서도 팔고 원두로도 팔아요",
          "original_form": "갈아서도 팔고 원두로도 팔아요",
          "speaker_id": "2",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100029511.1.12",
          "form": "저런 안타깝네요",
          "original_form": "저런 안타깝네요",
          "speaker_id": "1",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100029511.1.13",
          "form": "우유섞어 드세요",
          "original_form": "우유섞어 드세요",
          "speaker_id": "1",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100029511.1.14",
          "form": "전 원두로 사서",
          "original_form": "전 원두로 사서",
          "speaker_id": "2",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100029511.1.15",
          "form": "직접갈아먹어요",
          "original_form": "직접갈아먹어요",
          "speaker_id": "2",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100029511.1.16",
          "form": "그냥 고소한 기본커피로 사요",
          "original_form": "그냥 고소한 기본커피로 사요",
          "speaker_id": "2",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100029511.1.17",
          "form": "기계로 갈아먹나요",
          "original_form": "기계로 갈아먹나요",
          "speaker_id": "1",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100029511.1.18",
          "form": "커피 마시고 싶네요",
          "original_form": "커피 마시고 싶네요",
          "speaker_id": "1",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100029511.1.19",
          "form": "네 매번 향이좋고 신선하게 먹을수있죠",
          "original_form": "네 매번 향이좋고 신선하게 먹을수있죠",
          "speaker_id": "2",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100029511.1.20",
          "form": "한잔드세이",
          "original_form": "한잔드세이",
          "speaker_id": "2",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100029511.1.21",
          "form": "카페가나요?",
          "original_form": "카페가나요?",
          "speaker_id": "2",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100029511.1.22",
          "form": "콜드브루좋아하나요",
          "original_form": "콜드브루좋아하나요",
          "speaker_id": "1",
          "time": "20210903 09:50"
        },
        {
          "id": "MDRW2100029511.1.23",
          "form": "좋아했죠",
          "original_form": "좋아했죠",
          "speaker_id": "2",
          "time": "20210903 09:51"
        },
        {
          "id": "MDRW2100029511.1.24",
          "form": "카페 가고싶은데 비가 안그쳐요",
          "original_form": "카페 가고싶은데 비가 안그쳐요",
          "speaker_id": "1",
          "time": "20210903 09:51"
        },
        {
          "id": "MDRW2100029511.1.25",
          "form": "커피마다 맛이달라요",
          "original_form": "커피마다 맛이달라요",
          "speaker_id": "2",
          "time": "20210903 09:51"
        },
        {
          "id": "MDRW2100029511.1.26",
          "form": "장화신고 ㄱㄱ",
          "original_form": "장화신고 ㄱㄱ",
          "speaker_id": "2",
          "time": "20210903 09:51"
        },
        {
          "id": "MDRW2100029511.1.27",
          "form": "음료는 다 좋아합니다",
          "original_form": "음료는 다 좋아합니다",
          "speaker_id": "1",
          "time": "20210903 09:51"
        },
        {
          "id": "MDRW2100029511.1.28",
          "form": "저는 한때 프라푸치노를 마셨습니다",
          "original_form": "저는 한때 프라푸치노를 마셨습니다",
          "speaker_id": "1",
          "time": "20210903 09:53"
        }
      ]
