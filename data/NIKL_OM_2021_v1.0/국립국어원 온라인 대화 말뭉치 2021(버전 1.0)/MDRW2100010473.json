      "utterance": [
        {
          "id": "MDRW2100010473.1.1",
          "form": "안녕하세요 저녁 드셨나용?",
          "original_form": "안녕하세요 저녁 드셨나용?",
          "speaker_id": "1",
          "time": "20210823 18:40"
        },
        {
          "id": "MDRW2100010473.1.2",
          "form": "아니요 아직이요 식사하셨나요???",
          "original_form": "아니요 아직이요 식사하셨나요???",
          "speaker_id": "2",
          "time": "20210823 18:42"
        },
        {
          "id": "MDRW2100010473.1.3",
          "form": "네 오리고기랑 먹었어요! 저녁 뭐 드실 계획이신가요?",
          "original_form": "네 오리고기랑 먹었어요! 저녁 뭐 드실 계획이신가요?",
          "speaker_id": "1",
          "time": "20210823 18:45"
        },
        {
          "id": "MDRW2100010473.1.4",
          "form": "저는 갈비탕입니다ㅋㅋ",
          "original_form": "저는 갈비탕입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:02"
        },
        {
          "id": "MDRW2100010473.1.5",
          "form": "오리고기는 맛있었나요?",
          "original_form": "오리고기는 맛있었나요?",
          "speaker_id": "2",
          "time": "20210823 19:03"
        },
        {
          "id": "MDRW2100010473.1.6",
          "form": "네  오리고기 맛있게 먹었어요 갈비탕 맛있겠네요!!",
          "original_form": "네  오리고기 맛있게 먹었어요 갈비탕 맛있겠네요!!",
          "speaker_id": "1",
          "time": "20210823 19:04"
        },
        {
          "id": "MDRW2100010473.1.7",
          "form": "네 이제 먹네요ㅜ",
          "original_form": "네 이제 먹네요ㅜ",
          "speaker_id": "2",
          "time": "20210823 19:27"
        },
        {
          "id": "MDRW2100010473.1.8",
          "form": "좋아하는 음식이 뭔가요?",
          "original_form": "좋아하는 음식이 뭔가요?",
          "speaker_id": "2",
          "time": "20210823 19:27"
        },
        {
          "id": "MDRW2100010473.1.9",
          "form": "떡볶이요!! name2님은 어떤음식 좋아하세요?",
          "original_form": "떡볶이요!! &name2&님은 어떤음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 19:28"
        },
        {
          "id": "MDRW2100010473.1.10",
          "form": "저는 스파게티요~~~면요리 좋아해요",
          "original_form": "저는 스파게티요~~~면요리 좋아해요",
          "speaker_id": "2",
          "time": "20210823 19:28"
        },
        {
          "id": "MDRW2100010473.1.11",
          "form": "오오 스파게티 맛있죠!! 토마토스파게트 크림스파게티 중 어느거 더 좋아하세요?",
          "original_form": "오오 스파게티 맛있죠!! 토마토스파게트 크림스파게티 중 어느거 더 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 19:33"
        },
        {
          "id": "MDRW2100010473.1.12",
          "form": "둘중에는 토마토요!",
          "original_form": "둘중에는 토마토요!",
          "speaker_id": "2",
          "time": "20210823 19:34"
        },
        {
          "id": "MDRW2100010473.1.13",
          "form": "어떤거 좋아하세요??",
          "original_form": "어떤거 좋아하세요??",
          "speaker_id": "2",
          "time": "20210823 19:34"
        },
        {
          "id": "MDRW2100010473.1.14",
          "form": "저도요!!",
          "original_form": "저도요!!",
