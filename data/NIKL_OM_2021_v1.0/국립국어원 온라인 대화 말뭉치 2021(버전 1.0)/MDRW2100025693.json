      "utterance": [
        {
          "id": "MDRW2100025693.1.1",
          "form": "언니는 견과류 좋아해?",
          "original_form": "언니는 견과류 좋아해?",
          "speaker_id": "2",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025693.1.2",
          "form": "응 난 엄청 좋아해 너는?",
          "original_form": "응 난 엄청 좋아해 너는?",
          "speaker_id": "1",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025693.1.3",
          "form": "나도 나도ㅋㅋ 근데 그건 싫어해...그 뭐드라...",
          "original_form": "나도 나도ㅋㅋ 근데 그건 싫어해...그 뭐드라...",
          "speaker_id": "2",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025693.1.4",
          "form": "뭐?",
          "original_form": "뭐?",
          "speaker_id": "1",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025693.1.5",
          "form": "캐슈넛! 싫어해",
          "original_form": "캐슈넛! 싫어해",
          "speaker_id": "2",
          "time": "20210901 16:03"
        },
        {
          "id": "MDRW2100025693.1.6",
          "form": "헐 난 그거 좋아하는데...",
          "original_form": "헐 난 그거 좋아하는데...",
          "speaker_id": "1",
          "time": "20210901 16:04"
        },
        {
          "id": "MDRW2100025693.1.7",
          "form": "아 진짜??",
          "original_form": "아 진짜??",
          "speaker_id": "2",
          "time": "20210901 16:04"
        },
        {
          "id": "MDRW2100025693.1.8",
          "form": "응 싫어하는 특별한 이유가 있어?",
          "original_form": "응 싫어하는 특별한 이유가 있어?",
          "speaker_id": "1",
          "time": "20210901 16:04"
        },
        {
          "id": "MDRW2100025693.1.9",
          "form": "나는 그 특유의 너무 꼬수운 맛이 좀 느끼하게 느껴지더라규ㅜ",
          "original_form": "나는 그 특유의 너무 꼬수운 맛이 좀 느끼하게 느껴지더라규ㅜ",
          "speaker_id": "2",
          "time": "20210901 16:04"
        },
        {
          "id": "MDRW2100025693.1.10",
          "form": "아하..캐슈넛 난 냉동실에 넣어놓고 먹으면 오독오독해서 맛있던데..",
          "original_form": "아하..캐슈넛 난 냉동실에 넣어놓고 먹으면 오독오독해서 맛있던데..",
          "speaker_id": "1",
          "time": "20210901 16:04"
        },
        {
          "id": "MDRW2100025693.1.11",
          "form": "그럼 나중에 같이 하루견과 먹을때 내 캐슈넛 언니가 다 먹어",
          "original_form": "그럼 나중에 같이 하루견과 먹을때 내 캐슈넛 언니가 다 먹어",
          "speaker_id": "2",
          "time": "20210901 16:05"
        },
        {
          "id": "MDRW2100025693.1.12",
          "form": "나는 아몬드는 견과류중에선 별로 안좋아해",
          "original_form": "나는 아몬드는 견과류중에선 별로 안좋아해",
          "speaker_id": "1",
          "time": "20210901 16:05"
        },
        {
          "id": "MDRW2100025693.1.13",
          "form": "좋지 ㅋㅋ",
          "original_form": "좋지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:05"
        },
        {
          "id": "MDRW2100025693.1.14",
          "form": "어 나는 아몬드 좋아해 그럼 언니 아몬드 나 줘ㅋㅋ",
          "original_form": "어 나는 아몬드 좋아해 그럼 언니 아몬드 나 줘ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:05"
        }
