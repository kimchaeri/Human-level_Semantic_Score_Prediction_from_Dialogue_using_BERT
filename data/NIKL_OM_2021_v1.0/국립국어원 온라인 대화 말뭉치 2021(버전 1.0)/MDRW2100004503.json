      "utterance": [
        {
          "id": "MDRW2100004503.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210821 16:51"
        },
        {
          "id": "MDRW2100004503.1.2",
          "form": "아침에 쏟아지더니 잠깐 해떴다가 다시 흐리네요",
          "original_form": "아침에 쏟아지더니 잠깐 해떴다가 다시 흐리네요",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004503.1.3",
          "form": "오락가락해요",
          "original_form": "오락가락해요",
          "speaker_id": "1",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004503.1.4",
          "form": "네ㅎㅎ종잡을 수 없네요",
          "original_form": "네ㅎㅎ종잡을 수 없네요",
          "speaker_id": "2",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004503.1.5",
          "form": "ㄴ\r\n요즘날씨가 그렇죠",
          "original_form": "ㄴ\r\n요즘날씨가 그렇죠",
          "speaker_id": "1",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004503.1.6",
          "form": "이러다 정신차려보면 가을돼있겠죠",
          "original_form": "이러다 정신차려보면 가을돼있겠죠",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004503.1.7",
          "form": "네 금방일것 같아요",
          "original_form": "네 금방일것 같아요",
          "speaker_id": "1",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004503.1.8",
          "form": "가을 옷 장만하셨나요?",
          "original_form": "가을 옷 장만하셨나요?",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004503.1.9",
          "form": "일단 작년꺼 ㅋㅋ",
          "original_form": "일단 작년꺼 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004503.1.10",
          "form": "님은?",
          "original_form": "님은?",
          "speaker_id": "1",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004503.1.11",
          "form": "저는 얇은점퍼하나 샀네요ㅋ 바스락거리는거",
          "original_form": "저는 얇은점퍼하나 샀네요ㅋ 바스락거리는거",
          "speaker_id": "2",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004503.1.12",
          "form": "여름옷위에 입으려구요",
          "original_form": "여름옷위에 입으려구요",
          "speaker_id": "2",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004503.1.13",
          "form": "오 신상",
          "original_form": "오 신상",
          "speaker_id": "1",
          "time": "20210821 18:44"
        },
        {
          "id": "MDRW2100004503.1.14",
          "form": "ㅋㅋ네 막입기 좋게 까만색으로 샀어요",
          "original_form": "ㅋㅋ네 막입기 좋게 까만색으로 샀어요",
          "speaker_id": "2",
          "time": "20210821 18:58"
        },
        {
          "id": "MDRW2100004503.1.15",
          "form": "좋네요",
          "original_form": "좋네요",
