      "utterance": [
        {
          "id": "MDRW2100031625.1.1",
          "form": "백신맞으셨너요",
          "original_form": "백신맞으셨너요",
          "speaker_id": "1",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031625.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031625.1.3",
          "form": "화이자",
          "original_form": "화이자",
          "speaker_id": "2",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031625.1.4",
          "form": "저는 10부제 신청할때 뭣도 모르고 멀리신청해서 속상하네욬ㅋㅋㅋ",
          "original_form": "저는 10부제 신청할때 뭣도 모르고 멀리신청해서 속상하네욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 23:13"
        },
        {
          "id": "MDRW2100031625.1.5",
          "form": "아프지않나요",
          "original_form": "아프지않나요",
          "speaker_id": "1",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031625.1.6",
          "form": "당시엔 최선의 선택이었던것 같네요 사이트 들어가는것만도 경쟁률 치열해서 맘이 급했으니까요",
          "original_form": "당시엔 최선의 선택이었던것 같네요 사이트 들어가는것만도 경쟁률 치열해서 맘이 급했으니까요",
          "speaker_id": "2",
          "time": "20210904 23:14"
        },
        {
          "id": "MDRW2100031625.1.7",
          "form": "4일후 가슴통증 살짝 있었네요",
          "original_form": "4일후 가슴통증 살짝 있었네요",
          "speaker_id": "2",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031625.1.8",
          "form": "괜히 하혈같은 증상날까 걱정되네요",
          "original_form": "괜히 하혈같은 증상날까 걱정되네요",
          "speaker_id": "1",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031625.1.9",
          "form": "가슴통증이요?",
          "original_form": "가슴통증이요?",
          "speaker_id": "1",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031625.1.10",
          "form": "네 심장인지 근육인지",
          "original_form": "네 심장인지 근육인지",
          "speaker_id": "2",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031625.1.11",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031625.1.12",
          "form": "살짝 불편감이요",
          "original_form": "살짝 불편감이요",
          "speaker_id": "2",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031625.1.13",
          "form": "저 셋째날에 힘쓰는일해야하는데 괜찮을까요",
          "original_form": "저 셋째날에 힘쓰는일해야하는데 괜찮을까요",
          "speaker_id": "1",
          "time": "20210904 23:15"
        },
        {
          "id": "MDRW2100031625.1.14",
          "form": "되도록이면 비추입니다 눈치껏 설렁설렁 하는수밖에..",
          "original_form": "되도록이면 비추입니다 눈치껏 설렁설렁 하는수밖에..",
          "speaker_id": "2",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031625.1.15",
          "form": "일주일은 무리하면 안됩니다",
          "original_form": "일주일은 무리하면 안됩니다",
          "speaker_id": "2",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031625.1.16",
          "form": "ㅋㅋㅋㅋㅋ 괜히 백신맞아서 아플거같넹6",
          "original_form": "ㅋㅋㅋㅋㅋ 괜히 백신맞아서 아플거같넹6",
          "speaker_id": "1",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031625.1.17",
          "form": "주변에 물어보니 약간씩 아프드라구요",
          "original_form": "주변에 물어보니 약간씩 아프드라구요",
          "speaker_id": "2",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031625.1.18",
          "form": "무섭네요 오히려 젊은애들이 아프다하니",
          "original_form": "무섭네요 오히려 젊은애들이 아프다하니",
          "speaker_id": "1",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031625.1.19",
          "form": "가슴통증이라 해야하나 불편감이라 해야하나 그런거",
          "original_form": "가슴통증이라 해야하나 불편감이라 해야하나 그런거",
