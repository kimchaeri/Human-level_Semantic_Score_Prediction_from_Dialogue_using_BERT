      "utterance": [
        {
          "id": "MDRW2100044796.1.1",
          "form": "훗후 이번엔 제 최애음식을 맞춰보세요",
          "original_form": "훗후 이번엔 제 최애음식을 맞춰보세요",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044796.1.2",
          "form": "넵 좋아요",
          "original_form": "넵 좋아요",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044796.1.3",
          "form": "중식인가요?",
          "original_form": "중식인가요?",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044796.1.4",
          "form": "아니요 중국요리 아닙니다 후후",
          "original_form": "아니요 중국요리 아닙니다 후후",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044796.1.5",
          "form": "그럼 한식인가요?",
          "original_form": "그럼 한식인가요?",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044796.1.6",
          "form": "아니요 한식도 아니랍니다~~~!!",
          "original_form": "아니요 한식도 아니랍니다~~~!!",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044796.1.7",
          "form": "그럼 양식인가여?",
          "original_form": "그럼 양식인가여?",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044796.1.8",
          "form": "네 양식입니다!",
          "original_form": "네 양식입니다!",
          "speaker_id": "1",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044796.1.9",
          "form": "고기가 들어가나요?",
          "original_form": "고기가 들어가나요?",
          "speaker_id": "2",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044796.1.10",
          "form": "고기가 들어갈 때도 있고 안 들어갈 때도 있답니다",
          "original_form": "고기가 들어갈 때도 있고 안 들어갈 때도 있답니다",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044796.1.11",
          "form": "토마토가 들어가나요?",
          "original_form": "토마토가 들어가나요?",
          "speaker_id": "2",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044796.1.12",
          "form": "허억 네! 너무 결정적인 힌트다",
          "original_form": "허억 네! 너무 결정적인 힌트다",
          "speaker_id": "1",
          "time": "20210907 23:50"
        },
        {
          "id": "MDRW2100044796.1.13",
          "form": "면요리인가요?",
          "original_form": "면요리인가요?",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044796.1.14",
          "form": "네! 면 요리입니다",
          "original_form": "네! 면 요리입니다",
          "speaker_id": "1",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044796.1.15",
          "form": "앗 스파게티!",
          "original_form": "앗 스파게티!",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044796.1.16",
          "form": "정답 정답 !!!!!",
          "original_form": "정답 정답 !!!!!",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044796.1.17",
          "form": "제 최애는 스파게티 였습니다",
          "original_form": "제 최애는 스파게티 였습니다",
          "speaker_id": "1",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044796.1.18",
          "form": "토마토 스파게티가 최애이신가요?",
          "original_form": "토마토 스파게티가 최애이신가요?",
          "speaker_id": "2",
          "time": "20210907 23:53"
        },
        {
          "id": "MDRW2100044796.1.19",
          "form": "네 맞아요! 도매도 서파게티 좋아합니다",
          "original_form": "네 맞아요! 도매도 서파게티 좋아합니다",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044796.1.20",
          "form": "로제도 좋아하고 크림도 좋아해요",
          "original_form": "로제도 좋아하고 크림도 좋아해요",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044796.1.21",
          "form": "ㅋㅋㅋㅋ 발음이 구수하시네요",
          "original_form": "ㅋㅋㅋㅋ 발음이 구수하시네요",
          "speaker_id": "2",
          "time": "20210907 23:54"
        }
      ]
