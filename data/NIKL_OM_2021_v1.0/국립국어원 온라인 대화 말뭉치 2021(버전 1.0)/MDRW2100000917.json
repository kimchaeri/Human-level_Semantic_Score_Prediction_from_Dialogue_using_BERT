      "utterance": [
        {
          "id": "MDRW2100000917.1.1",
          "form": "많이 선선해졌어",
          "original_form": "많이 선선해졌어",
          "speaker_id": "2",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000917.1.2",
          "form": "그치 입추지나고나니까",
          "original_form": "그치 입추지나고나니까",
          "speaker_id": "1",
          "time": "20210819 20:50"
        },
        {
          "id": "MDRW2100000917.1.3",
          "form": "너무 신기할만큼 아침저녁으로 시원해졌어",
          "original_form": "너무 신기할만큼 아침저녁으로 시원해졌어",
          "speaker_id": "1",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000917.1.4",
          "form": "새벽엔 이불없인 안되겠더라구",
          "original_form": "새벽엔 이불없인 안되겠더라구",
          "speaker_id": "2",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000917.1.5",
          "form": "이러다 애들 감기 걸릴까봐 긴장빡 ㅋ",
          "original_form": "이러다 애들 감기 걸릴까봐 긴장빡 ㅋ",
          "speaker_id": "2",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000917.1.6",
          "form": "맞아맞아 애들이 자꾸 이불을 발로 걷어차서ㅜㅜ",
          "original_form": "맞아맞아 애들이 자꾸 이불을 발로 걷어차서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000917.1.7",
          "form": "그렇다고 문꽁꽁 닫고자면 너무 답답해서",
          "original_form": "그렇다고 문꽁꽁 닫고자면 너무 답답해서",
          "speaker_id": "1",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000917.1.8",
          "form": "그러니까 말이야 코로나때문에 단순감기도 병원가기 싫더라구",
          "original_form": "그러니까 말이야 코로나때문에 단순감기도 병원가기 싫더라구",
          "speaker_id": "2",
          "time": "20210819 20:53"
        },
        {
          "id": "MDRW2100000917.1.9",
          "form": "맞아맞아 ㅜㅜ 이제 곧 환절기가 오겠다",
          "original_form": "맞아맞아 ㅜㅜ 이제 곧 환절기가 오겠다",
          "speaker_id": "1",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100000917.1.10",
          "form": "일기예보가 하나도 안맞아서 언제 갑자기 비가 내릴지도 모르고",
          "original_form": "일기예보가 하나도 안맞아서 언제 갑자기 비가 내릴지도 모르고",
          "speaker_id": "1",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100000917.1.11",
          "form": "꼭 우산챙겨다녀야겠더라",
          "original_form": "꼭 우산챙겨다녀야겠더라",
          "speaker_id": "1",
          "time": "20210819 21:33"
        },
        {
          "id": "MDRW2100000917.1.12",
          "form": "맞아 그래도 더위 가니까 너무 좋아",
          "original_form": "맞아 그래도 더위 가니까 너무 좋아",
          "speaker_id": "2",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100000917.1.13",
          "form": "애들 데리고 산책다니기 좋은 날씬데",
          "original_form": "애들 데리고 산책다니기 좋은 날씬데",
          "speaker_id": "2",
          "time": "20210819 21:34"
        },
        {
          "id": "MDRW2100000917.1.14",
          "form": "금방 추워지겠지 ㅋㅋㅋ  아직은  반팔 원피스 입혀보내는데",
          "original_form": "금방 추워지겠지 ㅋㅋㅋ  아직은  반팔 원피스 입혀보내는데",
          "speaker_id": "1",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100000917.1.15",
          "form": "금방 바람막이 필요할거같아",
          "original_form": "금방 바람막이 필요할거같아",
          "speaker_id": "1",
          "time": "20210819 21:35"
        },
        {
          "id": "MDRW2100000917.1.16",
          "form": "그치 옷장정리 또 해야겠어",
          "original_form": "그치 옷장정리 또 해야겠어",
          "speaker_id": "2",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100000917.1.17",
          "form": "금방 추워질거야 롱패딩 장만해야겠다",
          "original_form": "금방 추워질거야 롱패딩 장만해야겠다",
          "speaker_id": "2",
          "time": "20210819 21:36"
        },
        {
          "id": "MDRW2100000917.1.18",
          "form": "집안일은 정말 끝이없다ㅜㅜ옷장정리 너무싫엉",
          "original_form": "집안일은 정말 끝이없다ㅜㅜ옷장정리 너무싫엉",
          "speaker_id": "1",
          "time": "20210819 21:38"
        },
        {
          "id": "MDRW2100000917.1.19",
          "form": "특히 겨울옷은 먼지 엄청 마셔야해ㅜㅎ",
          "original_form": "특히 겨울옷은 먼지 엄청 마셔야해ㅜㅎ",
          "speaker_id": "2",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100000917.1.20",
          "form": "부피커서 ㅋㅋ자리도 많이 차지하고",
          "original_form": "부피커서 ㅋㅋ자리도 많이 차지하고",
          "speaker_id": "2",
          "time": "20210819 21:40"
        },
        {
          "id": "MDRW2100000917.1.21",
          "form": "장마온다는데",
          "original_form": "장마온다는데",
          "speaker_id": "2",
          "time": "20210819 21:42"
        },
        {
          "id": "MDRW2100000917.1.22",
          "form": "다음주쯤이 죄다 비소식이던데",
          "original_form": "다음주쯤이 죄다 비소식이던데",
          "speaker_id": "1",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100000917.1.23",
          "form": "비그치면  해야겠다!  ㅎㅎ",
          "original_form": "비그치면  해야겠다!  ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100000917.1.24",
          "form": "다음주 장마야?",
          "original_form": "다음주 장마야?",
          "speaker_id": "2",
          "time": "20210820 08:51"
        },
        {
          "id": "MDRW2100000917.1.25",
          "form": "나 비오는날 무지 싫어하거든",
          "original_form": "나 비오는날 무지 싫어하거든",
          "speaker_id": "2",
          "time": "20210820 08:51"
        },
        {
          "id": "MDRW2100000917.1.26",
          "form": "넌 어때",
          "original_form": "넌 어때",
          "speaker_id": "2",
          "time": "20210820 08:52"
        },
        {
          "id": "MDRW2100000917.1.27",
          "form": "나는 비 좋아하는데 보는것만!ㅋㅋ",
          "original_form": "나는 비 좋아하는데 보는것만!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 08:52"
        },
        {
          "id": "MDRW2100000917.1.28",
          "form": "출근길에 비가 쏟아져서 바지가 축축해졌어 ㅠ",
          "original_form": "출근길에 비가 쏟아져서 바지가 축축해졌어 ㅠ",
          "speaker_id": "1",
          "time": "20210820 08:52"
        },
        {
          "id": "MDRW2100000917.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ그건 그래 \r\n근데 외출할때 비는 심해 ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그건 그래 \r\n근데 외출할때 비는 심해 ㅠ",
          "speaker_id": "2",
          "time": "20210820 08:53"
        },
        {
          "id": "MDRW2100000917.1.30",
          "form": "출근했어? 오늘 비와?",
          "original_form": "출근했어? 오늘 비와?",
          "speaker_id": "2",
          "time": "20210820 08:53"
        },
        {
          "id": "MDRW2100000917.1.31",
          "form": "여긴 쨍해 엄청 더워",
          "original_form": "여긴 쨍해 엄청 더워",
          "speaker_id": "2",
          "time": "20210820 08:53"
        },
        {
          "id": "MDRW2100000917.1.32",
          "form": "비안왔는데  애들 버스타고 출발하는데 갑자기 쏟아졌어ㅜ",
          "original_form": "비안왔는데  애들 버스타고 출발하는데 갑자기 쏟아졌어ㅜ",
          "speaker_id": "1",
          "time": "20210820 08:53"
        },
        {
          "id": "MDRW2100000917.1.33",
          "form": "국지성이 심각~",
          "original_form": "국지성이 심각~",
          "speaker_id": "1",
          "time": "20210820 08:53"
        },
        {
          "id": "MDRW2100000917.1.34",
          "form": "비올줄알았음  원피스입는건데  ㅠ 내바지~~",
          "original_form": "비올줄알았음  원피스입는건데  ㅠ 내바지~~",
          "speaker_id": "1",
          "time": "20210820 08:54"
        },
        {
          "id": "MDRW2100000917.1.35",
          "form": "워킹맘이었구나",
          "original_form": "워킹맘이었구나",
          "speaker_id": "2",
          "time": "20210820 08:54"
        },
        {
          "id": "MDRW2100000917.1.36",
          "form": "아침부터 진빠지겠다",
          "original_form": "아침부터 진빠지겠다",
          "speaker_id": "2",
          "time": "20210820 08:54"
        },
        {
          "id": "MDRW2100000917.1.37",
          "form": "파트타임으로 짧게^^",
          "original_form": "파트타임으로 짧게^^",
          "speaker_id": "1",
          "time": "20210820 08:55"
        },
        {
          "id": "MDRW2100000917.1.38",
          "form": "난 퇴사해서 전업됐어 ㅎ",
          "original_form": "난 퇴사해서 전업됐어 ㅎ",
          "speaker_id": "2",
          "time": "20210820 08:55"
        },
        {
          "id": "MDRW2100000917.1.39",
          "form": "안그럼 애봐줄사람이 없어서 ㅠㅠ",
          "original_form": "안그럼 애봐줄사람이 없어서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 08:55"
        },
        {
          "id": "MDRW2100000917.1.40",
          "form": "부럽다 나도 나가고싶어",
          "original_form": "부럽다 나도 나가고싶어",
          "speaker_id": "2",
          "time": "20210820 08:55"
        },
        {
          "id": "MDRW2100000917.1.41",
          "form": "맞아 방학되니 멘붕이더라 그만둔지 얼마 안됏거든",
          "original_form": "맞아 방학되니 멘붕이더라 그만둔지 얼마 안됏거든",
          "speaker_id": "2",
          "time": "20210820 08:55"
        },
        {
          "id": "MDRW2100000917.1.42",
          "form": "어느날은 출장가있는데 갑자기 소나기 와서",
          "original_form": "어느날은 출장가있는데 갑자기 소나기 와서",
          "speaker_id": "2",
          "time": "20210820 08:55"
        },
        {
          "id": "MDRW2100000917.1.43",
          "form": "학교간 아이가 전화온거야 엄마 비오는데 우산없어요",
          "original_form": "학교간 아이가 전화온거야 엄마 비오는데 우산없어요",
          "speaker_id": "2",
          "time": "20210820 08:55"
        },
        {
          "id": "MDRW2100000917.1.44",
          "form": "초등학교가면 많이 그렇다고들..",
          "original_form": "초등학교가면 많이 그렇다고들..",
          "speaker_id": "1",
          "time": "20210820 08:55"
        },
        {
          "id": "MDRW2100000917.1.45",
          "form": "어떻게ㅠ ㅜ",
          "original_form": "어떻게ㅠ ㅜ",
          "speaker_id": "1",
          "time": "20210820 08:56"
        },
        {
          "id": "MDRW2100000917.1.46",
          "form": "얼른 커야지 아이도 나도",
          "original_form": "얼른 커야지 아이도 나도",
          "speaker_id": "2",
          "time": "20210820 08:58"
        },
        {
          "id": "MDRW2100000917.1.47",
          "form": "그나저나 바지 찝찝하겟다",
          "original_form": "그나저나 바지 찝찝하겟다",
          "speaker_id": "2",
          "time": "20210820 08:59"
        },
        {
          "id": "MDRW2100000917.1.48",
          "form": "비는 그쳣어?",
          "original_form": "비는 그쳣어?",
          "speaker_id": "2",
          "time": "20210820 08:59"
        },
        {
          "id": "MDRW2100000917.1.49",
          "form": "출근하니까 비는 그쳐있던데 우리동네는 비가온대!ㅋㅋ",
          "original_form": "출근하니까 비는 그쳐있던데 우리동네는 비가온대!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 10:48"
        },
        {
          "id": "MDRW2100000917.1.50",
          "form": "날씨정말 알수없어",
          "original_form": "날씨정말 알수없어",
          "speaker_id": "1",
          "time": "20210820 10:48"
        },
        {
          "id": "MDRW2100000917.1.51",
          "form": "일주일넘게 비비비",
          "original_form": "일주일넘게 비비비",
          "speaker_id": "1",
