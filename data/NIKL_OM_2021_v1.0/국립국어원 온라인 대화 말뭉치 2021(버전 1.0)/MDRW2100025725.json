      "utterance": [
        {
          "id": "MDRW2100025725.1.1",
          "form": "그치 뭐 고를 수 있는 상황이면 안하겠지 굳이",
          "original_form": "그치 뭐 고를 수 있는 상황이면 안하겠지 굳이",
          "speaker_id": "2",
          "time": "20210901 16:32"
        },
        {
          "id": "MDRW2100025725.1.2",
          "form": "그치 고르고 들어가는 사람은 진짜 소수일듯",
          "original_form": "그치 고르고 들어가는 사람은 진짜 소수일듯",
          "speaker_id": "1",
          "time": "20210901 16:32"
        },
        {
          "id": "MDRW2100025725.1.3",
          "form": "마잨ㅋㅋㅋㅋ주말은 다 비슷해",
          "original_form": "마잨ㅋㅋㅋㅋ주말은 다 비슷해",
          "speaker_id": "2",
          "time": "20210901 16:33"
        },
        {
          "id": "MDRW2100025725.1.4",
          "form": "관상도 비슷해ㅋㅋㅋㅋ",
          "original_form": "관상도 비슷해ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:33"
        },
        {
          "id": "MDRW2100025725.1.5",
          "form": "고정 캐스팅이 있지 갑자기 옛날에 윤아 나온거 생각나넼ㅋㅋ",
          "original_form": "고정 캐스팅이 있지 갑자기 옛날에 윤아 나온거 생각나넼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:33"
        },
        {
          "id": "MDRW2100025725.1.6",
          "form": "새벽이?ㅋㅋ 난 그거 안봤어",
          "original_form": "새벽이?ㅋㅋ 난 그거 안봤어",
          "speaker_id": "1",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100025725.1.7",
          "form": "그때 진짜 핫했는데",
          "original_form": "그때 진짜 핫했는데",
          "speaker_id": "2",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100025725.1.8",
          "form": "맞아 유명했지",
          "original_form": "맞아 유명했지",
          "speaker_id": "1",
          "time": "20210901 16:35"
        },
        {
          "id": "MDRW2100025725.1.9",
          "form": "어머니들 사이에서 모르는사람이 없었을거얔ㅋㅋ",
          "original_form": "어머니들 사이에서 모르는사람이 없었을거얔ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:36"
        },
        {
          "id": "MDRW2100025725.1.10",
          "form": "나는 챙겨본 드라마는 한다다뿐인듯",
          "original_form": "나는 챙겨본 드라마는 한다다뿐인듯",
          "speaker_id": "1",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100025725.1.11",
          "form": "주말 드라마 중에?",
          "original_form": "주말 드라마 중에?",
          "speaker_id": "2",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100025725.1.12",
          "form": "아 차차차에 이상이 서브남으로 나온대ㅋㅋ",
          "original_form": "아 차차차에 이상이 서브남으로 나온대ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025725.1.13",
          "form": "헐 이상이도 나와?",
          "original_form": "헐 이상이도 나와?",
          "speaker_id": "2",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100025725.1.14",
          "form": "응 주말드라마!\r\n응 나도 까먹고있었어 아직 안나와서ㅋㅋ",
          "original_form": "응 주말드라마!\r\n응 나도 까먹고있었어 아직 안나와서ㅋㅋ",
