      "utterance": [
        {
          "id": "MDRW2100011089.1.1",
          "form": "저기요 제일 최근에.관람한개 뭔가요",
          "original_form": "저기요 제일 최근에.관람한개 뭔가요",
          "speaker_id": "2",
          "time": "20210824 15:00"
        },
        {
          "id": "MDRW2100011089.1.2",
          "form": "궁금하네요",
          "original_form": "궁금하네요",
          "speaker_id": "2",
          "time": "20210824 15:00"
        },
        {
          "id": "MDRW2100011089.1.3",
          "form": "미대생의취향이 궁금하네요",
          "original_form": "미대생의취향이 궁금하네요",
          "speaker_id": "2",
          "time": "20210824 15:00"
        },
        {
          "id": "MDRW2100011089.1.4",
          "form": "저는 아무래도 국립중앙박물관의 이집트전을 보러갔었죠..",
          "original_form": "저는 아무래도 국립중앙박물관의 이집트전을 보러갔었죠..",
          "speaker_id": "1",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011089.1.5",
          "form": "졸작답사....",
          "original_form": "졸작답사....",
          "speaker_id": "1",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011089.1.6",
          "form": "멋지네요",
          "original_form": "멋지네요",
          "speaker_id": "2",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100011089.1.7",
          "form": "역시 이집트를사랑하시는군요",
          "original_form": "역시 이집트를사랑하시는군요",
          "speaker_id": "2",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011089.1.8",
          "form": "살인",
          "original_form": "살인",
          "speaker_id": "1",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011089.1.9",
          "form": "간 김에 코리안컬쳐랑 아시아전 다 봤어요",
          "original_form": "간 김에 코리안컬쳐랑 아시아전 다 봤어요",
          "speaker_id": "1",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011089.1.10",
          "form": "수천년지나~마침내선택받은 미대생",
          "original_form": "수천년지나~마침내선택받은 미대생",
          "speaker_id": "2",
          "time": "20210824 15:09"
        },
        {
          "id": "MDRW2100011089.1.11",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011089.1.12",
          "form": "이집트전도",
          "original_form": "이집트전도",
          "speaker_id": "2",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011089.1.13",
          "form": "티켓예매하고가야해요?",
          "original_form": "티켓예매하고가야해요?",
          "speaker_id": "2",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011089.1.14",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210824 15:10"
        },
        {
          "id": "MDRW2100011089.1.15",
          "form": "국중박 전시는",
          "original_form": "국중박 전시는",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011089.1.16",
          "form": "대부분 무료예요",
          "original_form": "대부분 무료예요",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011089.1.17",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011089.1.18",
          "form": "짱이다~",
          "original_form": "짱이다~",
          "speaker_id": "2",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011089.1.19",
          "form": "한국에서 젤 큰 국립박물관 아닌가",
          "original_form": "한국에서 젤 큰 국립박물관 아닌가",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011089.1.20",
          "form": "킹왕짱~",
          "original_form": "킹왕짱~",
          "speaker_id": "2",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011089.1.21",
          "form": "전시장 옆에",
          "original_form": "전시장 옆에",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011089.1.22",
          "form": "작은 휴게소? 있는데",
          "original_form": "작은 휴게소? 있는데",
          "speaker_id": "1",
          "time": "20210824 15:11"
        },
        {
          "id": "MDRW2100011089.1.23",
          "form": "거기 해당문화 관련 원서가 엄청많아서",
          "original_form": "거기 해당문화 관련 원서가 엄청많아서",
          "speaker_id": "1",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100011089.1.24",
          "form": "그거 몇권 훑는데",
          "original_form": "그거 몇권 훑는데",
          "speaker_id": "1",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100011089.1.25",
          "form": "한시간 넘게 걸렸어요",
          "original_form": "한시간 넘게 걸렸어요",
          "speaker_id": "1",
          "time": "20210824 15:12"
        },
        {
          "id": "MDRW2100011089.1.26",
          "form": "완전",
          "original_form": "완전",
          "speaker_id": "2",
          "time": "20210824 18:08"
        },
        {
          "id": "MDRW2100011089.1.27",
          "form": "지식인이세요^^",
          "original_form": "지식인이세요^^",
          "speaker_id": "2",
          "time": "20210824 18:08"
        },
        {
          "id": "MDRW2100011089.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:08"
        },
        {
          "id": "MDRW2100011089.1.29",
          "form": "자본주의란..",
          "original_form": "자본주의란..",
          "speaker_id": "1",
          "time": "20210824 18:08"
        },
        {
          "id": "MDRW2100011089.1.30",
          "form": "당신의 열정에 겨두랑이가떨려요~",
          "original_form": "당신의 열정에 겨두랑이가떨려요~",
          "speaker_id": "2",
          "time": "20210824 18:08"
        },
        {
          "id": "MDRW2100011089.1.31",
          "form": "ㅋㅋㅋㅋ̤̫ㅋ̤̮ㅋ̤̻ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ̤̫ㅋ̤̮ㅋ̤̻ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011089.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011089.1.33",
          "form": "제정신 아닌데?",
          "original_form": "제정신 아닌데?",
          "speaker_id": "1",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011089.1.34",
          "form": "당신은",
          "original_form": "당신은",
          "speaker_id": "1",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011089.1.35",
          "form": "이집트 뭐가 제일 감동적이였나요~?",
          "original_form": "이집트 뭐가 제일 감동적이였나요~?",
          "speaker_id": "2",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011089.1.36",
          "form": "하하",
          "original_form": "하하",
          "speaker_id": "2",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011089.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011089.1.38",
          "form": "어떤 왕족의",
          "original_form": "어떤 왕족의",
          "speaker_id": "1",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011089.1.39",
          "form": "황금관이 있었는데",
          "original_form": "황금관이 있었는데",
          "speaker_id": "1",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011089.1.40",
          "form": "와 그거정말 훔치고싶네요~",
          "original_form": "와 그거정말 훔치고싶네요~",
          "speaker_id": "2",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011089.1.41",
          "form": "모든 면이 그림으로 꽉 차있더라고요",
          "original_form": "모든 면이 그림으로 꽉 차있더라고요",
          "speaker_id": "1",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011089.1.42",
          "form": "이집트에선 장의사도 함부로 못하겠어요",
          "original_form": "이집트에선 장의사도 함부로 못하겠어요",
          "speaker_id": "1",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011089.1.43",
          "form": "마치 당신의관처럼",
          "original_form": "마치 당신의관처럼",
          "speaker_id": "2",
          "time": "20210824 18:09"
        },
        {
          "id": "MDRW2100011089.1.44",
          "form": "저는 태워주세요",
          "original_form": "저는 태워주세요",
          "speaker_id": "1",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100011089.1.45",
          "form": "관꾸",
          "original_form": "관꾸",
          "speaker_id": "2",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100011089.1.46",
          "form": "유골항아리하면",
          "original_form": "유골항아리하면",
          "speaker_id": "1",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100011089.1.47",
          "form": "당신 장의사로 대성할수있다",
          "original_form": "당신 장의사로 대성할수있다",
          "speaker_id": "2",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100011089.1.48",
          "form": "그려야하는 범위",
          "original_form": "그려야하는 범위",
          "speaker_id": "1",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100011089.1.49",
          "form": "팍줄어든다",
          "original_form": "팍줄어든다",
          "speaker_id": "1",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100011089.1.50",
          "form": "관.꾸를 합시다",
          "original_form": "관.꾸를 합시다",
          "speaker_id": "2",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100011089.1.51",
          "form": "저도 박물관에 전시되겠네요..",
          "original_form": "저도 박물관에 전시되겠네요..",
          "speaker_id": "1",
          "time": "20210824 18:10"
        },
        {
          "id": "MDRW2100011089.1.52",
          "form": "그것도 공짜관람으로",
          "original_form": "그것도 공짜관람으로",
          "speaker_id": "1",
          "time": "20210824 18:11"
        },
        {
          "id": "MDRW2100011089.1.53",
          "form": "멋져요~~",
          "original_form": "멋져요~~",
          "speaker_id": "2",
          "time": "20210824 18:11"
        }
      ]
