      "utterance": [
        {
          "id": "MDRW2100027846.1.1",
          "form": "그러니까요 많이나왔으면 좋겠네요",
          "original_form": "그러니까요 많이나왔으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210902 15:37"
        },
        {
          "id": "MDRW2100027846.1.2",
          "form": "저는 요즘 티비 서바이벌 잘봐여",
          "original_form": "저는 요즘 티비 서바이벌 잘봐여",
          "speaker_id": "1",
          "time": "20210902 15:42"
        },
        {
          "id": "MDRW2100027846.1.3",
          "form": "뭐가 재밌나요?",
          "original_form": "뭐가 재밌나요?",
          "speaker_id": "2",
          "time": "20210902 15:43"
        },
        {
          "id": "MDRW2100027846.1.4",
          "form": "한중일 모아서 그룹 만드나봐요",
          "original_form": "한중일 모아서 그룹 만드나봐요",
          "speaker_id": "1",
          "time": "20210902 15:45"
        },
        {
          "id": "MDRW2100027846.1.5",
          "form": "그런가요? 가수 데뷔하고 그런건가요?",
          "original_form": "그런가요? 가수 데뷔하고 그런건가요?",
          "speaker_id": "2",
          "time": "20210902 15:46"
        },
        {
          "id": "MDRW2100027846.1.6",
          "form": "그런느낌  9명이래요",
          "original_form": "그런느낌  9명이래요",
          "speaker_id": "1",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027846.1.7",
          "form": "또 주작 인거 아니겠죠",
          "original_form": "또 주작 인거 아니겠죠",
          "speaker_id": "1",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027846.1.8",
          "form": "그럴지도요 이미 잊혀졌다고 생각할수도있으니까요",
          "original_form": "그럴지도요 이미 잊혀졌다고 생각할수도있으니까요",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100027846.1.9",
          "form": "데뷔못한사람만 안타까윤",
          "original_form": "데뷔못한사람만 안타까윤",
          "speaker_id": "1",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100027846.1.10",
          "form": "그러게요 그때 정말 충격적이었죠 순위로 인생이 바뀔텐데",
          "original_form": "그러게요 그때 정말 충격적이었죠 순위로 인생이 바뀔텐데",
          "speaker_id": "2",
          "time": "20210902 16:02"
        },
        {
          "id": "MDRW2100027846.1.11",
          "form": "돈도 손해",
          "original_form": "돈도 손해",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027846.1.12",
          "form": "너무해요",
          "original_form": "너무해요",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027846.1.13",
          "form": "그걸로 데뷔한사람은",
          "original_form": "그걸로 데뷔한사람은",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027846.1.14",
          "form": "그러니까요 안타깝네요 이번에는 절대 조작이 없어야할텐데 말이죠",
          "original_form": "그러니까요 안타깝네요 이번에는 절대 조작이 없어야할텐데 말이죠",
          "speaker_id": "2",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100027846.1.15",
          "form": "댄스 서바이벌도 인기에요",
          "original_form": "댄스 서바이벌도 인기에요",
          "speaker_id": "1",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100027846.1.16",
          "form": "채널 돌리다가 잠깐본것같아요 인기라고 하니 한번 제대로봐야겠어요",
          "original_form": "채널 돌리다가 잠깐본것같아요 인기라고 하니 한번 제대로봐야겠어요",
          "speaker_id": "2",
          "time": "20210902 16:33"
        },
        {
          "id": "MDRW2100027846.1.17",
          "form": "근데 자기들 춤 잘춘다고",
          "original_form": "근데 자기들 춤 잘춘다고",
          "speaker_id": "1",
          "time": "20210902 16:35"
        },
        {
          "id": "MDRW2100027846.1.18",
          "form": "아이돌 무시 하더라구요",
          "original_form": "아이돌 무시 하더라구요",
          "speaker_id": "1",
          "time": "20210902 16:36"
        }
      ]
