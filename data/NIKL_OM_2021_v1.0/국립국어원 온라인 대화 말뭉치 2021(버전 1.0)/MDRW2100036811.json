      "utterance": [
        {
          "id": "MDRW2100036811.1.1",
          "form": "뮤지컬 보고싶어요!",
          "original_form": "뮤지컬 보고싶어요!",
          "speaker_id": "1",
          "time": "20210831 12:42"
        },
        {
          "id": "MDRW2100036811.1.2",
          "form": "보신적 있으세요?",
          "original_form": "보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210831 12:42"
        },
        {
          "id": "MDRW2100036811.1.3",
          "form": "전 아직 없네요",
          "original_form": "전 아직 없네요",
          "speaker_id": "2",
          "time": "20210831 12:46"
        },
        {
          "id": "MDRW2100036811.1.4",
          "form": "보고싶으신 뮤지컬 있으신가요?",
          "original_form": "보고싶으신 뮤지컬 있으신가요?",
          "speaker_id": "2",
          "time": "20210831 12:46"
        },
        {
          "id": "MDRW2100036811.1.5",
          "form": "그냥 아무거나 봐보고싶어요",
          "original_form": "그냥 아무거나 봐보고싶어요",
          "speaker_id": "1",
          "time": "20210831 12:49"
        },
        {
          "id": "MDRW2100036811.1.6",
          "form": "저한텐 문화생활의 끝판왕 느낌이라ㅋㅋ",
          "original_form": "저한텐 문화생활의 끝판왕 느낌이라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:49"
        },
        {
          "id": "MDRW2100036811.1.7",
          "form": "맞아요 저도 한 번 찾아보다가 앞자리는 20되는거보고 놀랐어요",
          "original_form": "맞아요 저도 한 번 찾아보다가 앞자리는 20되는거보고 놀랐어요",
          "speaker_id": "2",
          "time": "20210831 12:50"
        },
        {
          "id": "MDRW2100036811.1.8",
          "form": "진짜요! 보고 우왕.. 담에 봐야겠다.. 하다가보니 지금까지 와버렸어요",
          "original_form": "진짜요! 보고 우왕.. 담에 봐야겠다.. 하다가보니 지금까지 와버렸어요",
          "speaker_id": "1",
          "time": "20210831 12:53"
        },
        {
          "id": "MDRW2100036811.1.9",
          "form": "ㅋㅋㅋㅋ그렇군요",
          "original_form": "ㅋㅋㅋㅋ그렇군요",
          "speaker_id": "2",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100036811.1.10",
          "form": "가보신 전시회나 콘서트 있으신가요?",
          "original_form": "가보신 전시회나 콘서트 있으신가요?",
          "speaker_id": "2",
          "time": "20210831 12:58"
        },
        {
          "id": "MDRW2100036811.1.11",
          "form": "콘서트는 멜로어워즈요!",
          "original_form": "콘서트는 멜로어워즈요!",
          "speaker_id": "1",
          "time": "20210831 12:59"
        },
        {
          "id": "MDRW2100036811.1.12",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210831 12:59"
        },
        {
          "id": "MDRW2100036811.1.13",
          "form": "저는 아이유랑 가요대전 한 번 가봤어요",
          "original_form": "저는 아이유랑 가요대전 한 번 가봤어요",
          "speaker_id": "2",
          "time": "20210831 13:01"
        },
        {
          "id": "MDRW2100036811.1.14",
          "form": "멜론어워즈는 티켓팅해서 가신건가요?",
          "original_form": "멜론어워즈는 티켓팅해서 가신건가요?",
          "speaker_id": "2",
          "time": "20210831 13:01"
        },
        {
          "id": "MDRW2100036811.1.15",
          "form": "넹! 친구덕으로 보고왔어요",
          "original_form": "넹! 친구덕으로 보고왔어요",
          "speaker_id": "1",
          "time": "20210831 13:03"
        },
        {
          "id": "MDRW2100036811.1.16",
          "form": "몇년도에 가신건가요?",
          "original_form": "몇년도에 가신건가요?",
          "speaker_id": "2",
          "time": "20210831 13:04"
        },
        {
          "id": "MDRW2100036811.1.17",
          "form": "아이유ㅠ 가고싶었는데ㅠ 못갔어요",
          "original_form": "아이유ㅠ 가고싶었는데ㅠ 못갔어요",
          "speaker_id": "1",
          "time": "20210831 13:04"
        },
        {
          "id": "MDRW2100036811.1.18",
          "form": "음.. 엑소랑 빅뱅 있던때였어요",
          "original_form": "음.. 엑소랑 빅뱅 있던때였어요",
          "speaker_id": "1",
          "time": "20210831 13:04"
        },
        {
          "id": "MDRW2100036811.1.19",
          "form": "빅뱅 뱅뱅뱅때요!",
          "original_form": "빅뱅 뱅뱅뱅때요!",
          "speaker_id": "1",
          "time": "20210831 13:04"
        },
        {
          "id": "MDRW2100036811.1.20",
          "form": "오~~재밌으셨나요??",
          "original_form": "오~~재밌으셨나요??",
          "speaker_id": "2",
          "time": "20210831 13:06"
        },
        {
          "id": "MDRW2100036811.1.21",
          "form": "넹! 얼굴은 진짜 가까이서 봐서ㅋㅋ 눈이 즐거웠어요",
          "original_form": "넹! 얼굴은 진짜 가까이서 봐서ㅋㅋ 눈이 즐거웠어요",
          "speaker_id": "1",
          "time": "20210831 13:07"
        },
