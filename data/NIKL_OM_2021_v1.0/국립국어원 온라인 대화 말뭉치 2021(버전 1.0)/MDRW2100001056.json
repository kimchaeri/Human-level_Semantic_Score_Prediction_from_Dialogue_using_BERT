      "utterance": [
        {
          "id": "MDRW2100001056.1.1",
          "form": "저는 제주도 여행왔어요",
          "original_form": "저는 제주도 여행왔어요",
          "speaker_id": "2",
          "time": "20210722 21:25"
        },
        {
          "id": "MDRW2100001056.1.2",
          "form": "여행..",
          "original_form": "여행..",
          "speaker_id": "1",
          "time": "20210722 21:25"
        },
        {
          "id": "MDRW2100001056.1.3",
          "form": "부러워요ㅠㅠ",
          "original_form": "부러워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210722 21:25"
        },
        {
          "id": "MDRW2100001056.1.4",
          "form": "근데 사람이 많아서",
          "original_form": "근데 사람이 많아서",
          "speaker_id": "2",
          "time": "20210722 21:26"
        },
        {
          "id": "MDRW2100001056.1.5",
          "form": "없는곳으로 찾아다니느라",
          "original_form": "없는곳으로 찾아다니느라",
          "speaker_id": "2",
          "time": "20210722 21:26"
        },
        {
          "id": "MDRW2100001056.1.6",
          "form": "코로나때문에 여행가지도 못하고ㅠㅠ",
          "original_form": "코로나때문에 여행가지도 못하고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210722 21:26"
        },
        {
          "id": "MDRW2100001056.1.7",
          "form": "ㅠㅠ 저도 취소를 하긴 애매해서 검사받고 갔어요 ㅜ",
          "original_form": "ㅠㅠ 저도 취소를 하긴 애매해서 검사받고 갔어요 ㅜ",
          "speaker_id": "2",
          "time": "20210722 21:26"
        },
        {
          "id": "MDRW2100001056.1.8",
          "form": "으 검사 아프다고하던데",
          "original_form": "으 검사 아프다고하던데",
          "speaker_id": "1",
          "time": "20210722 21:27"
        },
        {
          "id": "MDRW2100001056.1.9",
          "form": "코만 살짝아파요",
          "original_form": "코만 살짝아파요",
          "speaker_id": "2",
          "time": "20210722 21:27"
        },
        {
          "id": "MDRW2100001056.1.10",
          "form": "혹시 여행 가신 경험있나요?",
          "original_form": "혹시 여행 가신 경험있나요?",
          "speaker_id": "2",
          "time": "20210722 21:27"
        },
        {
          "id": "MDRW2100001056.1.11",
          "form": "제주도 두번 일본 두번이요!",
          "original_form": "제주도 두번 일본 두번이요!",
          "speaker_id": "1",
          "time": "20210722 21:28"
        },
        {
          "id": "MDRW2100001056.1.12",
          "form": "어린때는 경주도 많이갔었어요",
          "original_form": "어린때는 경주도 많이갔었어요",
          "speaker_id": "1",
          "time": "20210722 21:28"
        },
        {
          "id": "MDRW2100001056.1.13",
          "form": "오 일본! 일본은 안가봤어요!",
          "original_form": "오 일본! 일본은 안가봤어요!",
          "speaker_id": "2",
          "time": "20210722 21:28"
        },
        {
          "id": "MDRW2100001056.1.14",
          "form": "전 제주도만 5번째",
          "original_form": "전 제주도만 5번째",
          "speaker_id": "2",
          "time": "20210722 21:28"
        },
        {
          "id": "MDRW2100001056.1.15",
          "form": "제주도 가서 움직이는 박물관? 갔었는데",
          "original_form": "제주도 가서 움직이는 박물관? 갔었는데",
          "speaker_id": "1",
          "time": "20210722 21:29"
        },
        {
          "id": "MDRW2100001056.1.16",
          "form": "재밌었어요",
          "original_form": "재밌었어요",
          "speaker_id": "1",
          "time": "20210722 21:29"
        },
        {
          "id": "MDRW2100001056.1.17",
          "form": "오오 지금은 공항쪽으로 올라왔어요",
          "original_form": "오오 지금은 공항쪽으로 올라왔어요",
          "speaker_id": "2",
          "time": "20210722 21:29"
        },
        {
          "id": "MDRW2100001056.1.18",
          "form": "어제 중문쪽에 있다가",
          "original_form": "어제 중문쪽에 있다가",
          "speaker_id": "2",
          "time": "20210722 21:29"
        },
        {
          "id": "MDRW2100001056.1.19",
          "form": "제주도가게 되면 방탄 슈가님 형이 운영하시는 카페가보고싶어요",
          "original_form": "제주도가게 되면 방탄 슈가님 형이 운영하시는 카페가보고싶어요",
          "speaker_id": "1",
          "time": "20210722 21:31"
        },
        {
          "id": "MDRW2100001056.1.20",
          "form": "오오 거긴어디예요.?",
          "original_form": "오오 거긴어디예요.?",
          "speaker_id": "2",
          "time": "20210722 21:31"
        },
        {
          "id": "MDRW2100001056.1.21",
          "form": "카페이름이 공백이라고 알고있어요",
          "original_form": "카페이름이 공백이라고 알고있어요",
          "speaker_id": "1",
          "time": "20210722 21:32"
        },
        {
          "id": "MDRW2100001056.1.22",
          "form": "제주도 동쪽에있다는데 슈가님 형이 운영하신데요 후기보니까 너무좋아서 한번 가보고싶어요!",
          "original_form": "제주도 동쪽에있다는데 슈가님 형이 운영하신데요 후기보니까 너무좋아서 한번 가보고싶어요!",
          "speaker_id": "1",
          "time": "20210722 21:33"
        }
      ]
    }
