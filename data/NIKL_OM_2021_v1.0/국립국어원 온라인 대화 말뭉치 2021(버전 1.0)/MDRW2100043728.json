      "utterance": [
        {
          "id": "MDRW2100043728.1.1",
          "form": "거기도 비 많이 오나요?",
          "original_form": "거기도 비 많이 오나요?",
          "speaker_id": "1",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043728.1.2",
          "form": "요즘 비너무많이오는거같지않나요",
          "original_form": "요즘 비너무많이오는거같지않나요",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100043728.1.3",
          "form": "오늘은안와요",
          "original_form": "오늘은안와요",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043728.1.4",
          "form": "계속오긴오네요",
          "original_form": "계속오긴오네요",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043728.1.5",
          "form": "앗 거긴 안 오는군요 여긴 엄청 오는 중이에요..",
          "original_form": "앗 거긴 안 오는군요 여긴 엄청 오는 중이에요..",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100043728.1.6",
          "form": "아 여긴 평소에 비거의안오는곳이라",
          "original_form": "아 여긴 평소에 비거의안오는곳이라",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043728.1.7",
          "form": "올해처럼 자주오는거 나쁘진않네요",
          "original_form": "올해처럼 자주오는거 나쁘진않네요",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043728.1.8",
          "form": "많이오는곳은 걱정될거같아요",
          "original_form": "많이오는곳은 걱정될거같아요",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043728.1.9",
          "form": "그쵸 지역별로 편차가 좀 심한거 같네요..",
          "original_form": "그쵸 지역별로 편차가 좀 심한거 같네요..",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043728.1.10",
          "form": "거기 온도는 어떤가요",
          "original_form": "거기 온도는 어떤가요",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043728.1.11",
          "form": "한 18-23도 정도로 시원해요",
          "original_form": "한 18-23도 정도로 시원해요",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043728.1.12",
          "form": "요즘 살만해서 산책도 좀 해요",
          "original_form": "요즘 살만해서 산책도 좀 해요",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043728.1.13",
          "form": "오 딱 좋네요 비도 안오는 상태에서 그 온도면",
          "original_form": "오 딱 좋네요 비도 안오는 상태에서 그 온도면",
          "speaker_id": "1",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043728.1.14",
          "form": "넵넵 시원하니까 습한것도 좀 줄었네요",
          "original_form": "넵넵 시원하니까 습한것도 좀 줄었네요",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043728.1.15",
          "form": "비오면 습한게 짜증나지않나요?ㅋㅋ",
          "original_form": "비오면 습한게 짜증나지않나요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043728.1.16",
          "form": "아 정말요.. 머리도 막 부풀어오르고요 ㅋㅋㅋ",
          "original_form": "아 정말요.. 머리도 막 부풀어오르고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043728.1.17",
          "form": "네 전 곱슬이라서",
          "original_form": "네 전 곱슬이라서",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043728.1.18",
          "form": "진짜 출근할때 난감이에요",
          "original_form": "진짜 출근할때 난감이에요",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043728.1.19",
          "form": "아 저도 반곱슬 ㅋㅋㅋ 앞머리쪽이 난리나요...",
          "original_form": "아 저도 반곱슬 ㅋㅋㅋ 앞머리쪽이 난리나요...",
          "speaker_id": "1",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043728.1.20",
          "form": "저는 잔머리들이 너무붕떠서",
          "original_form": "저는 잔머리들이 너무붕떠서",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043728.1.21",
          "form": "안빗은거같이돼요",
          "original_form": "안빗은거같이돼요",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043728.1.22",
          "form": "저도요 비오는 날은 그래서 항상 거울봐야돼요",
          "original_form": "저도요 비오는 날은 그래서 항상 거울봐야돼요",
          "speaker_id": "1",
          "time": "20210907 20:18"
        }
