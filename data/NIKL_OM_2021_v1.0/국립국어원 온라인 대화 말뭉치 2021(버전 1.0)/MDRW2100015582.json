          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015582.1.17",
          "form": "하지만 뿌듯한",
          "original_form": "하지만 뿌듯한",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015582.1.18",
