      "utterance": [
        {
          "id": "MDRW2100018843.1.1",
          "form": "공연이나 전시회 좋아하세요?",
          "original_form": "공연이나 전시회 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 12:50"
        },
        {
          "id": "MDRW2100018843.1.2",
          "form": "그렇게 즐겨가는 편은 아니에요",
          "original_form": "그렇게 즐겨가는 편은 아니에요",
          "speaker_id": "2",
          "time": "20210827 12:51"
        },
        {
          "id": "MDRW2100018843.1.3",
          "form": "최근에는 더안가고요",
          "original_form": "최근에는 더안가고요",
          "speaker_id": "2",
          "time": "20210827 12:52"
        },
        {
          "id": "MDRW2100018843.1.4",
          "form": "아..ㅠ 요즘 코로나 때문에 가기 더 힘들죠",
          "original_form": "아..ㅠ 요즘 코로나 때문에 가기 더 힘들죠",
          "speaker_id": "1",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100018843.1.5",
          "form": "저도 최근에 이건희 컬렉션 하나만 다녀왔어요",
          "original_form": "저도 최근에 이건희 컬렉션 하나만 다녀왔어요",
          "speaker_id": "1",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100018843.1.6",
          "form": "전 영화관도 안간지가 꽤 됐어요",
          "original_form": "전 영화관도 안간지가 꽤 됐어요",
          "speaker_id": "2",
          "time": "20210827 12:54"
        },
        {
          "id": "MDRW2100018843.1.7",
          "form": "아 저두요. 영화관 좋아했었는데 코로나 이후로 딱 접었어요ㅋㅋㅋ",
          "original_form": "아 저두요. 영화관 좋아했었는데 코로나 이후로 딱 접었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:56"
        },
        {
          "id": "MDRW2100018843.1.8",
          "form": "이제 집에서 보는게 훨씬 편하더라구요",
          "original_form": "이제 집에서 보는게 훨씬 편하더라구요",
          "speaker_id": "1",
          "time": "20210827 12:56"
        },
        {
          "id": "MDRW2100018843.1.9",
          "form": "가도 되는데 괜히 조심스러워지더라고요\r\n그러다 보니까 점점 관심도 안가지고..",
          "original_form": "가도 되는데 괜히 조심스러워지더라고요\r\n그러다 보니까 점점 관심도 안가지고..",
          "speaker_id": "2",
          "time": "20210827 12:56"
        },
        {
          "id": "MDRW2100018843.1.10",
          "form": "맞아요ㅋㅋㅋ 저도 초반에 안가다보니 그냥 귀찮아서....",
          "original_form": "맞아요ㅋㅋㅋ 저도 초반에 안가다보니 그냥 귀찮아서....",
          "speaker_id": "1",
          "time": "20210827 12:59"
        },
        {
          "id": "MDRW2100018843.1.11",
          "form": "코로나가 생활을 많이 바꾸네요.....",
          "original_form": "코로나가 생활을 많이 바꾸네요.....",
          "speaker_id": "1",
          "time": "20210827 12:59"
        },
        {
          "id": "MDRW2100018843.1.12",
          "form": "네 안익숙해질거 같앗는데\r\n또 어떻게 어떻게 적응되더라고요ㅋㅋ",
          "original_form": "네 안익숙해질거 같앗는데\r\n또 어떻게 어떻게 적응되더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:00"
        },
        {
          "id": "MDRW2100018843.1.13",
          "form": "ㅋㅋㅋ진짜 사람은 적응의 동물...",
          "original_form": "ㅋㅋㅋ진짜 사람은 적응의 동물...",
          "speaker_id": "1",
          "time": "20210827 13:01"
        },
        {
          "id": "MDRW2100018843.1.14",
          "form": "코로나 끝나면 뮤지컬은 보러가고 싶어요",
          "original_form": "코로나 끝나면 뮤지컬은 보러가고 싶어요",
          "speaker_id": "1",
          "time": "20210827 13:02"
        },
        {
          "id": "MDRW2100018843.1.15",
          "form": "아 나훈아 콘서트도...!",
          "original_form": "아 나훈아 콘서트도...!",
          "speaker_id": "1",
          "time": "20210827 13:02"
        },
        {
          "id": "MDRW2100018843.1.16",
          "form": "그런건 티비로는 맛이 안나져\r\n이상하게 영화도 티비로는 잘 안보게 되더라고요",
          "original_form": "그런건 티비로는 맛이 안나져\r\n이상하게 영화도 티비로는 잘 안보게 되더라고요",
          "speaker_id": "2",
          "time": "20210827 13:04"
        },
        {
          "id": "MDRW2100018843.1.17",
          "form": "맞아요 ㅠㅠ 직접가야 몸이 울려요",
          "original_form": "맞아요 ㅠㅠ 직접가야 몸이 울려요",
          "speaker_id": "1",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018843.1.18",
          "form": "전 요즘에 실내사이클탈 때 영화틀어놓으니까 보게되더라구요",
          "original_form": "전 요즘에 실내사이클탈 때 영화틀어놓으니까 보게되더라구요",
          "speaker_id": "1",
          "time": "20210827 13:05"
        },
        {
          "id": "MDRW2100018843.1.19",
          "form": "아 그런거 할때 보기는 좋겠네요\r\n가만히 보고있으면 자꾸 돌아다니고 그래서 집중 안되더라고요",
          "original_form": "아 그런거 할때 보기는 좋겠네요\r\n가만히 보고있으면 자꾸 돌아다니고 그래서 집중 안되더라고요",
          "speaker_id": "2",
          "time": "20210827 13:07"
        },
        {
          "id": "MDRW2100018843.1.20",
          "form": "그쵸 한곳에 붙들려있어야 끝까지 볼 수 있어요",
          "original_form": "그쵸 한곳에 붙들려있어야 끝까지 볼 수 있어요",
          "speaker_id": "1",
          "time": "20210827 13:09"
        },
        {
          "id": "MDRW2100018843.1.21",
          "form": "집중력이 언제 이렇게 짧아진건지..",
          "original_form": "집중력이 언제 이렇게 짧아진건지..",
          "speaker_id": "1",
          "time": "20210827 13:09"
        }
      ]
    }
