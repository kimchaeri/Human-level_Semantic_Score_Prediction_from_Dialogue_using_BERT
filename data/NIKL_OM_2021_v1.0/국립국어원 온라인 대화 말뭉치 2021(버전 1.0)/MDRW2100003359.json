      "utterance": [
        {
          "id": "MDRW2100003359.1.1",
          "form": "흑임자인절미 알아?",
          "original_form": "흑임자인절미 알아?",
          "speaker_id": "1",
          "time": "20210820 22:08"
        },
        {
          "id": "MDRW2100003359.1.2",
          "form": "알지 그거 엄청 맛있잖아",
          "original_form": "알지 그거 엄청 맛있잖아",
          "speaker_id": "2",
          "time": "20210820 22:08"
        },
        {
          "id": "MDRW2100003359.1.3",
          "form": "나 떡 별로 안좋아하는데 맛있더라",
          "original_form": "나 떡 별로 안좋아하는데 맛있더라",
          "speaker_id": "1",
          "time": "20210820 22:08"
        },
        {
          "id": "MDRW2100003359.1.4",
          "form": "방배떡집? 공주 떡집?",
          "original_form": "방배떡집? 공주 떡집?",
          "speaker_id": "2",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003359.1.5",
          "form": "압구정 공주 떡집",
          "original_form": "압구정 공주 떡집",
          "speaker_id": "1",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003359.1.6",
          "form": "아 흑임자인절미 얘기하니까 먹고 싶다",
          "original_form": "아 흑임자인절미 얘기하니까 먹고 싶다",
          "speaker_id": "2",
          "time": "20210820 22:09"
        },
        {
          "id": "MDRW2100003359.1.7",
          "form": "진짜 맛있어",
          "original_form": "진짜 맛있어",
          "speaker_id": "1",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003359.1.8",
          "form": "막한 떡이 진짜 맛있지",
          "original_form": "막한 떡이 진짜 맛있지",
          "speaker_id": "2",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003359.1.9",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003359.1.10",
          "form": "바로 먹는 게 맛있어",
          "original_form": "바로 먹는 게 맛있어",
          "speaker_id": "1",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003359.1.11",
          "form": "요즘 방앗간 잘 안보이는 것 같던데",
          "original_form": "요즘 방앗간 잘 안보이는 것 같던데",
          "speaker_id": "2",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003359.1.12",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003359.1.13",
          "form": "떡이랑 식혜랑 같이 먹으면 진짜 최고",
          "original_form": "떡이랑 식혜랑 같이 먹으면 진짜 최고",
          "speaker_id": "2",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003359.1.14",
          "form": "우와 나도 식혜 좋아",
          "original_form": "우와 나도 식혜 좋아",
          "speaker_id": "1",
          "time": "20210820 22:11"
        },
        {
          "id": "MDRW2100003359.1.15",
