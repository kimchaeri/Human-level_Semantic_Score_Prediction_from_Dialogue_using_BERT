      "utterance": [
        {
          "id": "MDRW2100027243.1.1",
          "form": "집안일은 진짜 누가 알아주지 않는 거라 더 힘드네요ㅠ",
          "original_form": "집안일은 진짜 누가 알아주지 않는 거라 더 힘드네요ㅠ",
          "speaker_id": "1",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027243.1.2",
          "form": "가족들은 집안일 잘 하는 편이신가요??",
          "original_form": "가족들은 집안일 잘 하는 편이신가요??",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027243.1.3",
          "form": "네네 다들 잘해요",
          "original_form": "네네 다들 잘해요",
          "speaker_id": "1",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027243.1.4",
          "form": "제가 제일 서툴러요 ㅋㅋㅋ",
          "original_form": "제가 제일 서툴러요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027243.1.5",
          "form": "ㅋㅋㅋ저랑 비슷하시네요",
          "original_form": "ㅋㅋㅋ저랑 비슷하시네요",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027243.1.6",
          "form": "맨날 혼납니다 ...",
          "original_form": "맨날 혼납니다 ...",
          "speaker_id": "2",
          "time": "20210902 10:44"
        },
        {
          "id": "MDRW2100027243.1.7",
          "form": "ㅋㅋㅋ으악 맞아요ㅠㅠ",
          "original_form": "ㅋㅋㅋ으악 맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027243.1.8",
          "form": "빡세요 집안일",
          "original_form": "빡세요 집안일",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027243.1.9",
          "form": "숙련도가 필요한 일이에여ㅜㅜ",
          "original_form": "숙련도가 필요한 일이에여ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027243.1.10",
          "form": "전 남동생이 있는데 취사병 출신이라서",
          "original_form": "전 남동생이 있는데 취사병 출신이라서",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027243.1.11",
          "form": "저보다 요리 잘하고 ...",
          "original_form": "저보다 요리 잘하고 ...",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027243.1.12",
          "form": "저만 뒤처졌어요",
          "original_form": "저만 뒤처졌어요",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027243.1.13",
          "form": "우와 대박 취사병이라니",
          "original_form": "우와 대박 취사병이라니",
          "speaker_id": "1",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100027243.1.14",
          "form": "요리잘하는 동생 저도 필요해요 크크",
          "original_form": "요리잘하는 동생 저도 필요해요 크크",
          "speaker_id": "1",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100027243.1.15",
          "form": "ㅋㅋ한번 얻어먹으려면 비위 맞추기 어렵습니다 ㅠ",
          "original_form": "ㅋㅋ한번 얻어먹으려면 비위 맞추기 어렵습니다 ㅠ",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027243.1.16",
          "form": "으악ㅋㅋㅋ 그건 싫네요",
          "original_form": "으악ㅋㅋㅋ 그건 싫네요",
          "speaker_id": "1",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027243.1.17",
          "form": "알아서 척척 해줬으면ㅋㅋㅋㅋ",
          "original_form": "알아서 척척 해줬으면ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027243.1.18",
          "form": "ㅋㅋㅋㅋ용돈주면 잘하더라구요",
          "original_form": "ㅋㅋㅋㅋ용돈주면 잘하더라구요",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027243.1.19",
          "form": "저는 그냥 집에 밥 없음 제가 하는거 같아요 ㅋㅋㅋ",
          "original_form": "저는 그냥 집에 밥 없음 제가 하는거 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027243.1.20",
          "form": "돈으로 움직이는 자들..",
          "original_form": "돈으로 움직이는 자들..",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027243.1.21",
          "form": "우와 좋은 언니네요",
          "original_form": "우와 좋은 언니네요",
          "speaker_id": "1",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027243.1.22",
          "form": "아니 누나구나",
          "original_form": "아니 누나구나",
          "speaker_id": "1",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027243.1.23",
          "form": "돈이 최고죠 크큭",
          "original_form": "돈이 최고죠 크큭",
          "speaker_id": "1",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027243.1.24",
          "form": "ㅋㅋㅋ맞아요 돈이 최고죠",
          "original_form": "ㅋㅋㅋ맞아요 돈이 최고죠",
          "speaker_id": "2",
          "time": "20210902 10:57"
        },
        {
          "id": "MDRW2100027243.1.25",
          "form": "이번에 그런 의미에서... 식기세척기를 샀습니다!!!! ",
          "original_form": "이번에 그런 의미에서... 식기세척기를 샀습니다!!!! ",
          "speaker_id": "2",
          "time": "20210902 10:57"
        }
      ]
