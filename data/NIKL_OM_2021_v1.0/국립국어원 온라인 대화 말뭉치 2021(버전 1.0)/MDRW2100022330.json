      "utterance": [
        {
          "id": "MDRW2100022330.1.1",
          "form": "저 가봤는데 좋았어요 ㅠ",
          "original_form": "저 가봤는데 좋았어요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022330.1.2",
          "form": "언제 한 번 가봐야겠어요",
          "original_form": "언제 한 번 가봐야겠어요",
          "speaker_id": "1",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022330.1.3",
          "form": "혼자 오신 남성분이",
          "original_form": "혼자 오신 남성분이",
          "speaker_id": "2",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022330.1.4",
          "form": "식어버린 피자 5조각 한꺼번에 가져가시는데",
          "original_form": "식어버린 피자 5조각 한꺼번에 가져가시는데",
          "speaker_id": "2",
          "time": "20210829 12:40"
        },
        {
          "id": "MDRW2100022330.1.5",
          "form": "너무 감사했어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무 감사했어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022330.1.6",
          "form": "누군가는 피자를 처리해줘야 새로운 피자가 나오거든여..",
          "original_form": "누군가는 피자를 처리해줘야 새로운 피자가 나오거든여..",
          "speaker_id": "2",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022330.1.7",
          "form": "ㅋㅋㅋㅋ 아 거기는 자기가 갖다먹는거에요?",
          "original_form": "ㅋㅋㅋㅋ 아 거기는 자기가 갖다먹는거에요?",
          "speaker_id": "1",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022330.1.8",
          "form": "네네 뷔페식이라서",
          "original_form": "네네 뷔페식이라서",
          "speaker_id": "2",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022330.1.9",
          "form": "피자헛은 갖다줬어요",
          "original_form": "피자헛은 갖다줬어요",
          "speaker_id": "1",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022330.1.10",
          "form": "랜덤식으로 막 구워서",
          "original_form": "랜덤식으로 막 구워서",
          "speaker_id": "1",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022330.1.11",
          "form": "오? 어?!",
          "original_form": "오? 어?!",
          "speaker_id": "2",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022330.1.12",
          "form": "저 그거 그때 가본 거 같아요 ㅋㅋㅋ",
          "original_form": "저 그거 그때 가본 거 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022330.1.13",
          "form": "앉아있음 갖다주는거",
          "original_form": "앉아있음 갖다주는거",
          "speaker_id": "1",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022330.1.14",
          "form": "먹어보셨구나",
          "original_form": "먹어보셨구나",
          "speaker_id": "1",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022330.1.15",
          "form": "피자 더 드시겠습니까~? 하면서 줬던거 같은데",
          "original_form": "피자 더 드시겠습니까~? 하면서 줬던거 같은데",
          "speaker_id": "2",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022330.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022330.1.17",
          "form": "근데 사람 너무 많아서 몇 테이블 주고 끝나버린..",
          "original_form": "근데 사람 너무 많아서 몇 테이블 주고 끝나버린..",
          "speaker_id": "2",
          "time": "20210829 12:41"
        },
        {
          "id": "MDRW2100022330.1.18",
          "form": "그 때 피자헛의 모든 피자를 다 먹어본거같아요",
          "original_form": "그 때 피자헛의 모든 피자를 다 먹어본거같아요",
          "speaker_id": "1",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022330.1.19",
          "form": "지금 와서 생각해보니 미피식이 좀 더 나은 거 같기도 해요",
          "original_form": "지금 와서 생각해보니 미피식이 좀 더 나은 거 같기도 해요",
          "speaker_id": "2",
          "time": "20210829 12:42"
        },
        {
          "id": "MDRW2100022330.1.20",
          "form": "아쉬웠겠다..",
          "original_form": "아쉬웠겠다..",
          "speaker_id": "1",
          "time": "20210829 12:42"
        }
      ]
