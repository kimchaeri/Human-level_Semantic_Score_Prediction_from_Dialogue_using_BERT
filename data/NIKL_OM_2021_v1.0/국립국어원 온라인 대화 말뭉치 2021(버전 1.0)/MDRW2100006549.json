      "utterance": [
        {
          "id": "MDRW2100006549.1.1",
          "form": "평소에 몇시에 주무시나요",
          "original_form": "평소에 몇시에 주무시나요",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100006549.1.2",
          "form": "보통 열두시에서 한시 사이요",
          "original_form": "보통 열두시에서 한시 사이요",
          "speaker_id": "2",
          "time": "20210908 07:18"
        },
        {
          "id": "MDRW2100006549.1.3",
          "form": "그럼 몇시에 일어나셔요?",
          "original_form": "그럼 몇시에 일어나셔요?",
          "speaker_id": "1",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100006549.1.4",
          "form": "보통 일곱시 반 쯤에 일어나요",
          "original_form": "보통 일곱시 반 쯤에 일어나요",
          "speaker_id": "2",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100006549.1.5",
          "form": "그럼 충분하시나요?수면시간?",
          "original_form": "그럼 충분하시나요?수면시간?",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100006549.1.6",
          "form": "살짝 졸려요ㅋ큐ㅠㅠ",
          "original_form": "살짝 졸려요ㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100006549.1.7",
          "form": "그렇죠 수면도 건강에 중요한듯요",
          "original_form": "그렇죠 수면도 건강에 중요한듯요",
          "speaker_id": "1",
          "time": "20210908 07:24"
        },
        {
          "id": "MDRW2100006549.1.8",
          "form": "그렇죠 수면 패턴 어떻게 되세요?",
          "original_form": "그렇죠 수면 패턴 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100006549.1.9",
          "form": "저는 9시전에 자서 6시 전에 일어나요",
          "original_form": "저는 9시전에 자서 6시 전에 일어나요",
          "speaker_id": "1",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100006549.1.10",
          "form": "일찍 주무시네요",
          "original_form": "일찍 주무시네요",
          "speaker_id": "2",
          "time": "20210908 07:26"
        },
        {
          "id": "MDRW2100006549.1.11",
          "form": "네 애들 재울때 같이 잠들때가 많아요",
          "original_form": "네 애들 재울때 같이 잠들때가 많아요",
          "speaker_id": "1",
          "time": "20210908 07:27"
        },
        {
          "id": "MDRW2100006549.1.12",
          "form": "미라클 모닝? 그런거 하시는 건가요?",
          "original_form": "미라클 모닝? 그런거 하시는 건가요?",
          "speaker_id": "2",
          "time": "20210908 07:27"
        },
        {
          "id": "MDRW2100006549.1.13",
          "form": "앗... 육아....",
          "original_form": "앗... 육아....",
          "speaker_id": "2",
          "time": "20210908 07:27"
        },
        {
          "id": "MDRW2100006549.1.14",
          "form": "아 전혀 아닙니다 재우고 일어나고싶은데.같이 잠들어버려요",
