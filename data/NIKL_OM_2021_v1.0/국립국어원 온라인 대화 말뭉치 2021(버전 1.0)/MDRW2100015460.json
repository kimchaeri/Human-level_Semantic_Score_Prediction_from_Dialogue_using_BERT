        },
        {
          "id": "MDRW2100015460.1.3",
          "form": "쵸등땐 합창단으로 바빴고",
          "original_form": "쵸등땐 합창단으로 바빴고",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015460.1.4",
          "form": "중고딩땐 공뷰로바빳고",
          "original_form": "중고딩땐 공뷰로바빳고",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015460.1.5",
          "form": "일쩜오진",
          "original_form": "일쩜오진",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015460.1.6",
          "form": "사실 중딩땐 연애로도 바빴어요 ㅋㅋ",
          "original_form": "사실 중딩땐 연애로도 바빴어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015460.1.7",
          "form": "대딩땐 좀 심심하게 보낸것같아요",
          "original_form": "대딩땐 좀 심심하게 보낸것같아요",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015460.1.8",
          "form": "ㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015460.1.9",
          "form": "name2는여?",
          "original_form": "&name2&는여?",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015460.1.10",
          "form": "역시인기녀다..",
          "original_form": "역시인기녀다..",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015460.1.11",
          "form": "피구하고 논거밖에생각이안나네..",
          "original_form": "피구하고 논거밖에생각이안나네..",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015460.1.12",
          "form": "우와 역시 피구왕의 명성",
          "original_form": "우와 역시 피구왕의 명성",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015460.1.13",
          "form": "통키 해키",
          "original_form": "통키 해키",
          "speaker_id": "1",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015460.1.14",
          "form": "피구왕ㅋㅋㅋㅋ",
          "original_form": "피구왕ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:10"
        },
        {
          "id": "MDRW2100015460.1.15",
          "form": "피구왕 해키",
          "original_form": "피구왕 해키",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015460.1.16",
          "form": "근데자꾸 name1님방 오류나요",
          "original_form": "근데자꾸 &name1&님방 오류나요",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015460.1.17",
          "form": "나만나나요?",
          "original_form": "나만나나요?",
          "speaker_id": "2",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015460.1.18",
          "form": "네 저 안나는데",
          "original_form": "네 저 안나는데",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015460.1.19",
          "form": "아까 다른방에 나더라고요",
          "original_form": "아까 다른방에 나더라고요",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015460.1.20",
          "form": "사람들 지금금 많이 모여 그런가",
          "original_form": "사람들 지금금 많이 모여 그런가",
          "speaker_id": "1",
          "time": "20210825 23:14"
        },
        {
          "id": "MDRW2100015460.1.21",
          "form": "아..다들참여해서그런가",
          "original_form": "아..다들참여해서그런가",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015460.1.22",
          "form": "학창시절에  젤기억남는거모예요?",
          "original_form": "학창시절에  젤기억남는거모예요?",
          "speaker_id": "2",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015460.1.23",
          "form": "오... 성적표가 벽에 붙었는데",
          "original_form": "오... 성적표가 벽에 붙었는데",
