      "utterance": [
        {
          "id": "MDRW2100026354.1.1",
          "form": "건강 어떻게 챙기세요?",
          "original_form": "건강 어떻게 챙기세요?",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026354.1.2",
          "form": "매일 운동하려고 하는거하구, 식자재 좋은거 사서 먹을려는거요",
          "original_form": "매일 운동하려고 하는거하구, 식자재 좋은거 사서 먹을려는거요",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026354.1.3",
          "form": "좋아하는운동 있으세요?",
          "original_form": "좋아하는운동 있으세요?",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100026354.1.4",
          "form": "와 대단하세요 매일 운동이라니~",
          "original_form": "와 대단하세요 매일 운동이라니~",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026354.1.5",
          "form": "전 매일 오천보 걷기도 겨우 해요 ㅋㅋㅋ",
          "original_form": "전 매일 오천보 걷기도 겨우 해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026354.1.6",
          "form": "저는 거의 안걷거든요ㅋㅋㅋ",
          "original_form": "저는 거의 안걷거든요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:46"
        },
        {
          "id": "MDRW2100026354.1.7",
          "form": "진짜 집에만있으니 걷는데 쓰는 근육들이 퇴화하고있어요",
          "original_form": "진짜 집에만있으니 걷는데 쓰는 근육들이 퇴화하고있어요",
          "speaker_id": "1",
          "time": "20210901 21:47"
        },
        {
          "id": "MDRW2100026354.1.8",
          "form": "저는 일부러 걷는거 하고 있어요",
          "original_form": "저는 일부러 걷는거 하고 있어요",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026354.1.9",
          "form": "그 캐시워크라고 어플 깔면 만보기 나와요",
          "original_form": "그 캐시워크라고 어플 깔면 만보기 나와요",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026354.1.10",
          "form": "걸으면 돈도 줘요~ㅎㅎ",
          "original_form": "걸으면 돈도 줘요~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026354.1.11",
          "form": "만보 걸으면 얼마 주나요?",
          "original_form": "만보 걸으면 얼마 주나요?",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026354.1.12",
          "form": "하루에만보걸어야 주나요?",
          "original_form": "하루에만보걸어야 주나요?",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026354.1.13",
          "form": "아뇨! 그냥 백걸음만 걸어도 돼요",
          "original_form": "아뇨! 그냥 백걸음만 걸어도 돼요",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026354.1.14",
          "form": "백걸음당 1원인가 그래요 ㅋㅋㅋ",
          "original_form": "백걸음당 1원인가 그래요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026354.1.15",
          "form": "아하!",
          "original_form": "아하!",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026354.1.16",
          "form": "저는 자주 걸으려고 하니까 그래도 만원씩은 쏠쏠히 모여요",
          "original_form": "저는 자주 걸으려고 하니까 그래도 만원씩은 쏠쏠히 모여요",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026354.1.17",
          "form": "나갈일있을때 켜야겠어요ㅋㅋ",
          "original_form": "나갈일있을때 켜야겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026354.1.18",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026354.1.19",
          "form": "애매한 친구들 생일선물은 그걸로 챙겨요 ㅋㅋ",
          "original_form": "애매한 친구들 생일선물은 그걸로 챙겨요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026354.1.20",
          "form": "지금은 하루에 천보도 안걸어요",
          "original_form": "지금은 하루에 천보도 안걸어요",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026354.1.21",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026354.1.22",
          "form": "괜찮네요~~~",
          "original_form": "괜찮네요~~~",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026354.1.23",
          "form": "칠천원 케이크 스벅 쿠폰 이런거 주고 생색내고 건강도 챙겨요 ㅋㅋ",
          "original_form": "칠천원 케이크 스벅 쿠폰 이런거 주고 생색내고 건강도 챙겨요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026354.1.24",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026354.1.25",
          "form": "확실히 걸으니까 배변도 잘되고 좋아요",
          "original_form": "확실히 걸으니까 배변도 잘되고 좋아요",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026354.1.26",
          "form": "ㅎㅎ 운동해야하는데ㅠㅠ",
          "original_form": "ㅎㅎ 운동해야하는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026354.1.27",
          "form": "원래수영좋아하거든요.  근데 1년넘게 못하고있어요",
          "original_form": "원래수영좋아하거든요.  근데 1년넘게 못하고있어요",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026354.1.28",
          "form": "진짜 걷기 추천해요~ 전신이 천천히 자극받는 느낌?",
          "original_form": "진짜 걷기 추천해요~ 전신이 천천히 자극받는 느낌?",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026354.1.29",
          "form": "와 저 코로나 끝나면 수영 배우고 싶어요~ 물이 너무 그리워요ㅠ",
          "original_form": "와 저 코로나 끝나면 수영 배우고 싶어요~ 물이 너무 그리워요ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026354.1.30",
          "form": "걷는거좋죠",
          "original_form": "걷는거좋죠",
          "speaker_id": "1",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026354.1.31",
          "form": "수영좋아요ㅠㅠ",
          "original_form": "수영좋아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:54"
        }
      ]
