      "utterance": [
        {
          "id": "MDRW2100026969.1.1",
          "form": "형님 전화왔었어",
          "original_form": "형님 전화왔었어",
          "speaker_id": "2",
          "time": "20210902 02:06"
        },
        {
          "id": "MDRW2100026969.1.2",
          "form": "왜? 무슨일있데?",
          "original_form": "왜? 무슨일있데?",
          "speaker_id": "1",
          "time": "20210902 02:06"
        },
        {
          "id": "MDRW2100026969.1.3",
          "form": "name3 학교 어떻게 된거냐고",
          "original_form": "&name3& 학교 어떻게 된거냐고",
          "speaker_id": "2",
          "time": "20210902 02:07"
        },
        {
          "id": "MDRW2100026969.1.4",
          "form": "전화 안했었어?",
          "original_form": "전화 안했었어?",
          "speaker_id": "1",
          "time": "20210902 02:08"
        },
        {
          "id": "MDRW2100026969.1.5",
          "form": "응 ㅎㅎ 할라고했는데 딱 먼저온거지",
          "original_form": "응 ㅎㅎ 할라고했는데 딱 먼저온거지",
          "speaker_id": "2",
          "time": "20210902 02:08"
        },
        {
          "id": "MDRW2100026969.1.6",
          "form": "먼저좀해라",
          "original_form": "먼저좀해라",
          "speaker_id": "1",
          "time": "20210902 02:08"
        },
        {
          "id": "MDRW2100026969.1.7",
          "form": "알았어",
          "original_form": "알았어",
          "speaker_id": "2",
          "time": "20210902 02:08"
        },
        {
          "id": "MDRW2100026969.1.8",
          "form": "비대면이리고 우선 중간고사까지만이라고 말했지",
          "original_form": "비대면이리고 우선 중간고사까지만이라고 말했지",
          "speaker_id": "2",
          "time": "20210902 02:09"
        },
        {
          "id": "MDRW2100026969.1.9",
          "form": "학교 가기도 그렇고 안가기도 그렇고",
          "original_form": "학교 가기도 그렇고 안가기도 그렇고",
          "speaker_id": "1",
          "time": "20210902 02:09"
        },
        {
          "id": "MDRW2100026969.1.10",
          "form": "그러게 등록금은 다 내고 이게 뭐하는건지",
          "original_form": "그러게 등록금은 다 내고 이게 뭐하는건지",
          "speaker_id": "2",
          "time": "20210902 02:10"
        },
        {
          "id": "MDRW2100026969.1.11",
          "form": "지금 중요한때인데 친구도 못사귀고 공부도 대강하고 큰일이네",
          "original_form": "지금 중요한때인데 친구도 못사귀고 공부도 대강하고 큰일이네",
          "speaker_id": "1",
          "time": "20210902 02:10"
        },
        {
          "id": "MDRW2100026969.1.12",
          "form": "그러게말야.  지긋한 코로나 언제 끝날지",
          "original_form": "그러게말야.  지긋한 코로나 언제 끝날지",
          "speaker_id": "2",
          "time": "20210902 02:11"
        },
        {
          "id": "MDRW2100026969.1.13",
          "form": "올해도 끝난듯하고... 내년엔 끋날까?",
          "original_form": "올해도 끝난듯하고... 내년엔 끋날까?",
          "speaker_id": "1",
          "time": "20210902 02:11"
        },
        {
          "id": "MDRW2100026969.1.14",
          "form": "백신도 맞으니 쫌 괜찮아지길 바래야지",
          "original_form": "백신도 맞으니 쫌 괜찮아지길 바래야지",
          "speaker_id": "2",
          "time": "20210902 02:12"
        },
        {
          "id": "MDRW2100026969.1.15",
          "form": "우리 명절엔 어떻게하지?",
          "original_form": "우리 명절엔 어떻게하지?",
          "speaker_id": "2",
