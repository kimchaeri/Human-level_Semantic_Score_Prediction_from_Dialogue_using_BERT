      "utterance": [
        {
          "id": "MDRW2100004276.1.1",
          "form": "영화 좋아하시나요",
          "original_form": "영화 좋아하시나요",
          "speaker_id": "1",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004276.1.2",
          "form": "좋아는 하는데 저 마지막으로 본 영화가 삼진그룹 영어토익반이에요",
          "original_form": "좋아는 하는데 저 마지막으로 본 영화가 삼진그룹 영어토익반이에요",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004276.1.3",
          "form": "오 저도 그거봣어용",
          "original_form": "오 저도 그거봣어용",
          "speaker_id": "1",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004276.1.4",
          "form": "보스베이비 다시 볼까하고있어요 지금 ㅋㅋ",
          "original_form": "보스베이비 다시 볼까하고있어요 지금 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004276.1.5",
          "form": "헐 그거 진짜 세상 귀엽잖아요!",
          "original_form": "헐 그거 진짜 세상 귀엽잖아요!",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004276.1.6",
          "form": "2도 나왓길래",
          "original_form": "2도 나왓길래",
          "speaker_id": "1",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004276.1.7",
          "form": "예전에 본거같은데 기억이잘안나서 ㅋㅋ",
          "original_form": "예전에 본거같은데 기억이잘안나서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004276.1.8",
          "form": "넷플로 보려구요",
          "original_form": "넷플로 보려구요",
          "speaker_id": "1",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004276.1.9",
          "form": "헐 1은 넷플에 올라왔어요? 날 잡고 한번 봐야겠네요",
          "original_form": "헐 1은 넷플에 올라왔어요? 날 잡고 한번 봐야겠네요",
          "speaker_id": "2",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004276.1.10",
          "form": "네네 있어용",
          "original_form": "네네 있어용",
          "speaker_id": "1",
          "time": "20210821 15:51"
        },
        {
          "id": "MDRW2100004276.1.11",
          "form": "2는 결제해서 봐야할듯용",
          "original_form": "2는 결제해서 봐야할듯용",
          "speaker_id": "1",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004276.1.12",
          "form": "혹시 공포영화 이런 거 잘 보세요?",
          "original_form": "혹시 공포영화 이런 거 잘 보세요?",
          "speaker_id": "2",
          "time": "20210821 15:52"
        },
        {
          "id": "MDRW2100004276.1.13",
          "form": "네네 공포영화 좋아해용 님은요?",
          "original_form": "네네 공포영화 좋아해용 님은요?",
          "speaker_id": "1",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004276.1.14",
          "form": "전 좋아하지는 않지만 나온 공포영화 얼결에 다 봤어요.. ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 좋아하지는 않지만 나온 공포영화 얼결에 다 봤어요.. ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004276.1.15",
          "form": "공포영화 보러가면 무서워서 화면은 못보고 소리만 들어요",
          "original_form": "공포영화 보러가면 무서워서 화면은 못보고 소리만 들어요",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004276.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ좋아하는거아닌가요 ..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ좋아하는거아닌가요 ..",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004276.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004276.1.18",
          "form": "아진짜요?",
          "original_form": "아진짜요?",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004276.1.19",
          "form": "친구들 때문에 같이 봐요... ㅎㅎ",
          "original_form": "친구들 때문에 같이 봐요... ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004276.1.20",
          "form": "콘서트는 가보셨어요?",
          "original_form": "콘서트는 가보셨어요?",
          "speaker_id": "1",
          "time": "20210821 15:59"
        },
        {
          "id": "MDRW2100004276.1.21",
          "form": "아 제가 또 한 때 열렬한 덕질 생활을 해서 많이 가봤아요",
          "original_form": "아 제가 또 한 때 열렬한 덕질 생활을 해서 많이 가봤아요",
