      "utterance": [
        {
          "id": "MDRW2100000584.1.1",
          "form": "앟ㅎㅎ 혹시 요리 하시는거 좋아하세요?",
          "original_form": "앟ㅎㅎ 혹시 요리 하시는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 17:00"
        },
        {
          "id": "MDRW2100000584.1.2",
          "form": "요리 잘 못해여ㅋㅋㅋㅋㅋㅋㅋ 해봤자 파스타정도..",
          "original_form": "요리 잘 못해여ㅋㅋㅋㅋㅋㅋㅋ 해봤자 파스타정도..",
          "speaker_id": "1",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000584.1.3",
          "form": "몇번망했더니 안하게되더라구요ㅋㅋㅋ",
          "original_form": "몇번망했더니 안하게되더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:01"
        },
        {
          "id": "MDRW2100000584.1.4",
          "form": "앟ㅠㅠㅠ 그렇군요 그러면 배달음식 좋아하시겠네요!",
          "original_form": "앟ㅠㅠㅠ 그렇군요 그러면 배달음식 좋아하시겠네요!",
          "speaker_id": "2",
          "time": "20210819 17:06"
        },
        {
          "id": "MDRW2100000584.1.5",
          "form": "넹ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 근데 아무래도 비싸니까 자주 안먹을려고해요",
          "original_form": "넹ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 근데 아무래도 비싸니까 자주 안먹을려고해요",
          "speaker_id": "1",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000584.1.6",
          "form": "최근에 배달시켜드신거 있어요?",
          "original_form": "최근에 배달시켜드신거 있어요?",
          "speaker_id": "1",
          "time": "20210819 17:08"
        },
        {
          "id": "MDRW2100000584.1.7",
          "form": "전 최근에 피나치공 먹었어요!",
          "original_form": "전 최근에 피나치공 먹었어요!",
          "speaker_id": "2",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000584.1.8",
          "form": "저는 푸라닭치킨!",
          "original_form": "저는 푸라닭치킨!",
          "speaker_id": "1",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000584.1.9",
          "form": "역시 치킨은 국룰..",
          "original_form": "역시 치킨은 국룰..",
          "speaker_id": "1",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000584.1.10",
          "form": "이야!ㅋㅋㅋㅋㅋ 역시 치킨이 제일이에요...",
          "original_form": "이야!ㅋㅋㅋㅋㅋ 역시 치킨이 제일이에요...",
          "speaker_id": "2",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000584.1.11",
          "form": "저는 치킨다음으로 곱창좋아하는데 너무 비싸여",
          "original_form": "저는 치킨다음으로 곱창좋아하는데 너무 비싸여",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000584.1.12",
          "form": "허어! 어쩜 저랑 입맛이 똑같을까요ㅋㅋㅋ",
          "original_form": "허어! 어쩜 저랑 입맛이 똑같을까요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000584.1.13",
          "form": "저도 곱창 진짜 좋아해요",
          "original_form": "저도 곱창 진짜 좋아해요",
          "speaker_id": "2",
          "time": "20210819 17:25"
        },
        {
          "id": "MDRW2100000584.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 근데 비싸기도하고 뭔가",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 근데 비싸기도하고 뭔가",
          "speaker_id": "1",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000584.1.15",
          "form": "자주먹으면 건강에 넘 안좋을거같잖아요",
          "original_form": "자주먹으면 건강에 넘 안좋을거같잖아요",
          "speaker_id": "1",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000584.1.16",
          "form": "흑흑 왜 맛있는건 몸에 안좋은거죠",
          "original_form": "흑흑 왜 맛있는건 몸에 안좋은거죠",
          "speaker_id": "1",
          "time": "20210819 17:27"
        },
        {
          "id": "MDRW2100000584.1.17",
          "form": "맞아요...ㅠㅠㅠ 기분 좋을 때 맛있게 먹는 정도가 딱 적당한 것 같아요ㅠㅠ",
          "original_form": "맞아요...ㅠㅠㅠ 기분 좋을 때 맛있게 먹는 정도가 딱 적당한 것 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000584.1.18",
          "form": "앜 여기 갑자기 비와요",
          "original_form": "앜 여기 갑자기 비와요",
          "speaker_id": "1",
          "time": "20210819 18:03"
        },
        {
          "id": "MDRW2100000584.1.19",
          "form": "비오는날은 칼국수인뎅",
          "original_form": "비오는날은 칼국수인뎅",
          "speaker_id": "1",
          "time": "20210819 18:04"
        },
        {
          "id": "MDRW2100000584.1.20",
          "form": "크흐 칼국수에 파전이면 비오는 날 최고죠ㅠㅠ",
          "original_form": "크흐 칼국수에 파전이면 비오는 날 최고죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 18:15"
        }
      ]
    }
