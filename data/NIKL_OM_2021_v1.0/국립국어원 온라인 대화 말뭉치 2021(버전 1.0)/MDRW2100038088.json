      "utterance": [
        {
          "id": "MDRW2100038088.1.1",
          "form": "중국에선",
          "original_form": "중국에선",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038088.1.2",
          "form": "감기기운이 있을대",
          "original_form": "감기기운이 있을대",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038088.1.3",
          "form": "콜라를 데워서 먹는다고 하더라구요",
          "original_form": "콜라를 데워서 먹는다고 하더라구요",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038088.1.4",
          "form": "드셔보셨나요?",
          "original_form": "드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100038088.1.5",
          "form": "헉 아니요!",
          "original_form": "헉 아니요!",
          "speaker_id": "1",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100038088.1.6",
          "form": "콜라를 따뜻하게해서 먹는다니",
          "original_form": "콜라를 따뜻하게해서 먹는다니",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038088.1.7",
          "form": "벌써 이질적인데요....",
          "original_form": "벌써 이질적인데요....",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100038088.1.8",
          "form": "콜라를 좋아하신다고 하셔서",
          "original_form": "콜라를 좋아하신다고 하셔서",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038088.1.9",
          "form": "중국식 콜라도 한 번 츄라이해보시라구",
          "original_form": "중국식 콜라도 한 번 츄라이해보시라구",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038088.1.10",
          "form": "얘길 꺼내 봤어요 ㅋㅋ",
          "original_form": "얘길 꺼내 봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100038088.1.11",
          "form": "앜ㅋㅋㅋㅋㅋ감사해요 상식이 늘었네요",
          "original_form": "앜ㅋㅋㅋㅋㅋ감사해요 상식이 늘었네요",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038088.1.12",
          "form": "과일은 좋아하세요?",
          "original_form": "과일은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100038088.1.13",
          "form": "과일도 좋아해요 오늘은 복숭아를 먹었어요",
          "original_form": "과일도 좋아해요 오늘은 복숭아를 먹었어요",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100038088.1.14",
          "form": "name1님은 뭐 드셨나요?",
          "original_form": "&name1&님은 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038088.1.15",
          "form": "저는 집에 있던  복숭아 다먹어서 과일 못 먹었어요ㅠㅠ",
          "original_form": "저는 집에 있던  복숭아 다먹어서 과일 못 먹었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038088.1.16",
          "form": "이번여름 복숭아 진짜 맛있었어요",
          "original_form": "이번여름 복숭아 진짜 맛있었어요",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038088.1.17",
          "form": "대부분 달아요 왜 이러죠 맛있어요",
          "original_form": "대부분 달아요 왜 이러죠 맛있어요",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100038088.1.18",
          "form": "저는 임실 말랑 복숭아 주로 사먹었는데",
          "original_form": "저는 임실 말랑 복숭아 주로 사먹었는데",
          "speaker_id": "1",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100038088.1.19",
          "form": "너무 달고 촉촉했어요",
          "original_form": "너무 달고 촉촉했어요",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100038088.1.20",
          "form": "브랜드가 있는걸 드셨군요 전 햇사래 먹었어요",
          "original_form": "브랜드가 있는걸 드셨군요 전 햇사래 먹었어요",
          "speaker_id": "2",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100038088.1.21",
          "form": "근데 그냥 길에서 파는 것도 엄청달아요",
          "original_form": "근데 그냥 길에서 파는 것도 엄청달아요",
          "speaker_id": "2",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100038088.1.22",
          "form": "햇사래도 먹었어요! 맛있죠 진짜요 다맛있어서 행복했어요",
          "original_form": "햇사래도 먹었어요! 맛있죠 진짜요 다맛있어서 행복했어요",
          "speaker_id": "1",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100038088.1.23",
          "form": "이제 나올 청포도 시즌도 기대돼요",
          "original_form": "이제 나올 청포도 시즌도 기대돼요",
          "speaker_id": "1",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100038088.1.24",
          "form": "올해 날씨가 복숭아에 맞나봐요",
          "original_form": "올해 날씨가 복숭아에 맞나봐요",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100038088.1.25",
          "form": "샤인 머스캣 좋아하시나요?",
          "original_form": "샤인 머스캣 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210901 23:37"
        },
        {
          "id": "MDRW2100038088.1.26",
          "form": "없어서 못먹죠",
          "original_form": "없어서 못먹죠",
          "speaker_id": "1",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100038088.1.27",
          "form": "청포도보다 샤인머스켓이 더 많이 보이는 느낌이에요",
          "original_form": "청포도보다 샤인머스켓이 더 많이 보이는 느낌이에요",
