      "utterance": [
        {
          "id": "MDRW2100023047.1.1",
          "form": "난 분모자가 떡인줄 알았는데",
          "original_form": "난 분모자가 떡인줄 알았는데",
          "speaker_id": "2",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100023047.1.2",
          "form": "당면이라더라 진짜니??",
          "original_form": "당면이라더라 진짜니??",
          "speaker_id": "2",
          "time": "20210829 21:04"
        },
        {
          "id": "MDRW2100023047.1.3",
          "form": "응 당면으로 만든 떡같은 맛이야",
          "original_form": "응 당면으로 만든 떡같은 맛이야",
          "speaker_id": "1",
          "time": "20210829 21:31"
        },
        {
          "id": "MDRW2100023047.1.4",
          "form": "쫄깃쫄깃해서 맛있어",
          "original_form": "쫄깃쫄깃해서 맛있어",
          "speaker_id": "1",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023047.1.5",
          "form": "신기한 식감일 것 같아ㅋㅋㅋㅋ",
          "original_form": "신기한 식감일 것 같아ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100023047.1.6",
          "form": "지금 뭐 먹고 있는 음식 있어?",
          "original_form": "지금 뭐 먹고 있는 음식 있어?",
          "speaker_id": "2",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100023047.1.7",
          "form": "지금부터는 내일아ㅊ ㅣㅁ까지 안먹으려고",
          "original_form": "지금부터는 내일아ㅊ ㅣㅁ까지 안먹으려고",
          "speaker_id": "1",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100023047.1.8",
          "form": "다이어트를 위하여",
          "original_form": "다이어트를 위하여",
          "speaker_id": "1",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100023047.1.9",
          "form": "지금 배는 안 고파?",
          "original_form": "지금 배는 안 고파?",
          "speaker_id": "2",
          "time": "20210829 21:34"
        },
        {
          "id": "MDRW2100023047.1.10",
          "form": "난 뭔가 허기져서 물 열심히 마시고 있어ㅋㅋㅋ",
          "original_form": "난 뭔가 허기져서 물 열심히 마시고 있어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:37"
        },
        {
          "id": "MDRW2100023047.1.11",
          "form": "헐 열심히 ㅌ살았구먼",
          "original_form": "헐 열심히 ㅌ살았구먼",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023047.1.12",
          "form": "난 왜 과일만 먹었는데도",
          "original_form": "난 왜 과일만 먹었는데도",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023047.1.13",
          "form": "배가부르지..?",
          "original_form": "배가부르지..?",
          "speaker_id": "1",
          "time": "20210829 21:40"
        },
        {
          "id": "MDRW2100023047.1.14",
          "form": "밥을 든든히 먹어서 그런가",
          "original_form": "밥을 든든히 먹어서 그런가",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023047.1.15",
          "form": "무슨 과일 먹었니?",
          "original_form": "무슨 과일 먹었니?",
          "speaker_id": "2",
          "time": "20210829 21:41"
        },
        {
          "id": "MDRW2100023047.1.16",
          "form": "바나나랑 복숭앋",
          "original_form": "바나나랑 복숭앋",
          "speaker_id": "1",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100023047.1.17",
          "form": "좀 많이 먹ㄷ었나..?",
          "original_form": "좀 많이 먹ㄷ었나..?",
          "speaker_id": "1",
          "time": "20210829 21:43"
        },
        {
          "id": "MDRW2100023047.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ많이 먹으면 좋지 뭐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ많이 먹으면 좋지 뭐",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023047.1.19",
          "form": "넌 토마토 좋아하니?",
          "original_form": "넌 토마토 좋아하니?",
          "speaker_id": "2",
          "time": "20210829 21:44"
        },
        {
          "id": "MDRW2100023047.1.20",
          "form": "으악 토마토는 스파게티소스와",
          "original_form": "으악 토마토는 스파게티소스와",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023047.1.21",
          "form": "케찹만 허락하라",
          "original_form": "케찹만 허락하라",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023047.1.22",
          "form": "타도 생토마토",
          "original_form": "타도 생토마토",
          "speaker_id": "1",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023047.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ타도래ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ타도래ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023047.1.24",
          "form": "설탕 뿌린 토마토도 타도파야?",
          "original_form": "설탕 뿌린 토마토도 타도파야?",
          "speaker_id": "2",
          "time": "20210829 21:47"
        },
        {
          "id": "MDRW2100023047.1.25",
          "form": "응 걍 먹을게없으면 먹겠는데",
          "original_form": "응 걍 먹을게없으면 먹겠는데",
          "speaker_id": "1",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023047.1.26",
          "form": "난 안먹을래",
          "original_form": "난 안먹을래",
          "speaker_id": "1",
          "time": "20210829 21:48"
        },
        {
          "id": "MDRW2100023047.1.27",
          "form": "이럴수가 난 토마토에 환장하는데",
          "original_form": "이럴수가 난 토마토에 환장하는데",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023047.1.28",
          "form": "근데 찰토마토는 싫어",
          "original_form": "근데 찰토마토는 싫어",
          "speaker_id": "2",
          "time": "20210829 21:49"
        },
        {
          "id": "MDRW2100023047.1.29",
          "form": "그런게있어???",
          "original_form": "그런게있어???",
          "speaker_id": "1",
          "time": "20210829 21:49"
        }
      ]
    }
