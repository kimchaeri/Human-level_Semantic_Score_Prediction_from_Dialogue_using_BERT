      "utterance": [
        {
          "id": "MDRW2100036871.1.1",
          "form": "안녕하세요~ 식사하셨나요?",
          "original_form": "안녕하세요~ 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210831 14:11"
        },
        {
          "id": "MDRW2100036871.1.2",
          "form": "네 밥이랑 나물이랑 간단하게 먹었어요 점심 드셨나요?",
          "original_form": "네 밥이랑 나물이랑 간단하게 먹었어요 점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 14:22"
        },
        {
          "id": "MDRW2100036871.1.3",
          "form": "네 좀 전에 먹었어요",
          "original_form": "네 좀 전에 먹었어요",
          "speaker_id": "1",
          "time": "20210831 14:22"
        },
        {
          "id": "MDRW2100036871.1.4",
          "form": "나물은 직접 만드신 건가요?",
          "original_form": "나물은 직접 만드신 건가요?",
          "speaker_id": "1",
          "time": "20210831 14:22"
        },
        {
          "id": "MDRW2100036871.1.5",
          "form": "아니요 엄마가 해주신거요 ㅋㅋ",
          "original_form": "아니요 엄마가 해주신거요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100036871.1.6",
          "form": "점심 뭐 드셨나요?",
          "original_form": "점심 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100036871.1.7",
          "form": "볶음밥이랑 미역국이랑 같이 먹었어요",
          "original_form": "볶음밥이랑 미역국이랑 같이 먹었어요",
          "speaker_id": "1",
          "time": "20210831 14:24"
        },
        {
          "id": "MDRW2100036871.1.8",
          "form": "와 너무 맛있었겠어요 직접하신건가요?",
          "original_form": "와 너무 맛있었겠어요 직접하신건가요?",
          "speaker_id": "2",
          "time": "20210831 14:25"
        },
        {
          "id": "MDRW2100036871.1.9",
          "form": "볶음밥은 제가 했고 미역국은 할머니가 끓여주셨어요",
          "original_form": "볶음밥은 제가 했고 미역국은 할머니가 끓여주셨어요",
          "speaker_id": "1",
          "time": "20210831 14:25"
        },
        {
          "id": "MDRW2100036871.1.10",
          "form": "볶음밥 무슨 볶음밥드셨나요?",
          "original_form": "볶음밥 무슨 볶음밥드셨나요?",
          "speaker_id": "2",
          "time": "20210831 14:26"
        },
        {
          "id": "MDRW2100036871.1.11",
          "form": "계란 볶음밥이요!",
          "original_form": "계란 볶음밥이요!",
          "speaker_id": "1",
          "time": "20210831 14:29"
        },
        {
          "id": "MDRW2100036871.1.12",
          "form": "귀찮아서 제일 간단한 걸로ㅎㅎ",
          "original_form": "귀찮아서 제일 간단한 걸로ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:29"
        },
        {
          "id": "MDRW2100036871.1.13",
          "form": "계란볶음밥 오랜만에 먹고싶네요 파기름내고 베이컨이나 새우 넣으면 진짜 맛있는데ㅜㅜ",
          "original_form": "계란볶음밥 오랜만에 먹고싶네요 파기름내고 베이컨이나 새우 넣으면 진짜 맛있는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210831 14:30"
        },
        {
          "id": "MDRW2100036871.1.14",
          "form": "파기름이 진짜 한 거랑 안 한 거랑 차이가 많이 나는 거 같아요",
          "original_form": "파기름이 진짜 한 거랑 안 한 거랑 차이가 많이 나는 거 같아요",
          "speaker_id": "1",
          "time": "20210831 14:32"
        },
        {
          "id": "MDRW2100036871.1.15",
          "form": "새우 넣으면 진짜 맛있죠ㅜㅜ",
          "original_form": "새우 넣으면 진짜 맛있죠ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 14:32"
        },
        {
          "id": "MDRW2100036871.1.16",
          "form": "파기름 진짜 파향 나서 너무 좋아요 풍미가 올라가는 느낌!!",
          "original_form": "파기름 진짜 파향 나서 너무 좋아요 풍미가 올라가는 느낌!!",
          "speaker_id": "2",
          "time": "20210831 14:33"
        },
        {
          "id": "MDRW2100036871.1.17",
          "form": "맞아요! 향이 너무 좋아져요",
          "original_form": "맞아요! 향이 너무 좋아져요",
          "speaker_id": "1",
          "time": "20210831 14:33"
        }
      ]
