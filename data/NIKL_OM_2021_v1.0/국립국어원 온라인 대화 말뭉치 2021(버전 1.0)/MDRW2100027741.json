      "utterance": [
        {
          "id": "MDRW2100027741.1.1",
          "form": "요즘 날씨 어떠신가요 그 지역은..!",
          "original_form": "요즘 날씨 어떠신가요 그 지역은..!",
          "speaker_id": "2",
          "time": "20210902 17:17"
        },
        {
          "id": "MDRW2100027741.1.2",
          "form": "저희 완전 흐려요 ㅋㅋ",
          "original_form": "저희 완전 흐려요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:26"
        },
        {
          "id": "MDRW2100027741.1.3",
          "form": "비올듯말듯~??",
          "original_form": "비올듯말듯~??",
          "speaker_id": "1",
          "time": "20210902 17:26"
        },
        {
          "id": "MDRW2100027741.1.4",
          "form": "그쪽은용??",
          "original_form": "그쪽은용??",
          "speaker_id": "1",
          "time": "20210902 17:27"
        },
        {
          "id": "MDRW2100027741.1.5",
          "form": "여기는 비 오다가 안 오다가 막 그러네요ㅜㅜ",
          "original_form": "여기는 비 오다가 안 오다가 막 그러네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100027741.1.6",
          "form": "요즘 날씨가 이상해요ㅋㅋ",
          "original_form": "요즘 날씨가 이상해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:28"
        },
        {
          "id": "MDRW2100027741.1.7",
          "form": "맞아용 ㅠㅠ",
          "original_form": "맞아용 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100027741.1.8",
          "form": "더웠다 추웠다가",
          "original_form": "더웠다 추웠다가",
          "speaker_id": "1",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100027741.1.9",
          "form": "난리에염ㅋㅋㅋ",
          "original_form": "난리에염ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100027741.1.10",
          "form": "이제.가을도 오나바야",
          "original_form": "이제.가을도 오나바야",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100027741.1.11",
          "form": "쌀쌀하니..",
          "original_form": "쌀쌀하니..",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100027741.1.12",
          "form": "ㅋㅋㅋㅋㅋ 맞아요 바람이 선선하게 불기 시작하더라구요",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요 바람이 선선하게 불기 시작하더라구요",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100027741.1.13",
          "form": "그래도 여름에 더운것보단",
          "original_form": "그래도 여름에 더운것보단",
          "speaker_id": "1",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100027741.1.14",
          "form": "겉옷입을수있는 가을이 좋은같아요 ..ㅎㅎ",
          "original_form": "겉옷입을수있는 가을이 좋은같아요 ..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 17:43"
        },
        {
          "id": "MDRW2100027741.1.15",
          "form": "맞아요! 여름 너무 덥고 습해서 별로 안 좋아하는 편입니다...ㅎ",
          "original_form": "맞아요! 여름 너무 덥고 습해서 별로 안 좋아하는 편입니다...ㅎ",
          "speaker_id": "2",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100027741.1.16",
          "form": "그래도 이번여름 엄청 더울거라고 했던거치곤",
          "original_form": "그래도 이번여름 엄청 더울거라고 했던거치곤",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100027741.1.17",
          "form": "잘넘어간거같아요",
          "original_form": "잘넘어간거같아요",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100027741.1.18",
          "form": "헉 그러게요",
          "original_form": "헉 그러게요",
          "speaker_id": "2",
          "time": "20210902 19:57"
        },
        {
          "id": "MDRW2100027741.1.19",
          "form": "이제 더웠는지 안 더웠는지 기억도 안 나네요",
          "original_form": "이제 더웠는지 안 더웠는지 기억도 안 나네요",
          "speaker_id": "2",
          "time": "20210902 19:57"
        },
        {
          "id": "MDRW2100027741.1.20",
          "form": "허헣 이제 겨울이오겠죵....ㅎ..",
          "original_form": "허헣 이제 겨울이오겠죵....ㅎ..",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100027741.1.21",
          "form": "가을이나 봄이 딱인뎅",
          "original_form": "가을이나 봄이 딱인뎅",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100027741.1.22",
          "form": "진짜요ㅜㅜ 옷도 봄 가을 옷이 예쁜데 말이에요ㅠㅠ",
          "original_form": "진짜요ㅜㅜ 옷도 봄 가을 옷이 예쁜데 말이에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100027741.1.23",
          "form": "제말이요 ㅠㅠ",
          "original_form": "제말이요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100027741.1.24",
          "form": "하지만 금방지나가서 ..",
          "original_form": "하지만 금방지나가서 ..",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100027741.1.25",
          "form": "진짜요! 날씨... 너무 아까워요... 좋은 계절이 점점 사라진다는 게",
          "original_form": "진짜요! 날씨... 너무 아까워요... 좋은 계절이 점점 사라진다는 게",
          "speaker_id": "2",
          "time": "20210902 20:42"
        },
        {
          "id": "MDRW2100027741.1.26",
          "form": "그런좋은날씨에",
          "original_form": "그런좋은날씨에",
          "speaker_id": "1",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100027741.1.27",
          "form": "놀러갈수가없다는게 아쉬워요",
          "original_form": "놀러갈수가없다는게 아쉬워요",
          "speaker_id": "1",
          "time": "20210902 20:51"
        }
      ]
