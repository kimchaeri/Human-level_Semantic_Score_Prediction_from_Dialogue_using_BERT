      "utterance": [
        {
          "id": "MDRW2100010726.1.1",
          "form": "난 영상보고 거르는데",
          "original_form": "난 영상보고 거르는데",
          "speaker_id": "2",
          "time": "20210823 21:13"
        },
        {
          "id": "MDRW2100010726.1.2",
          "form": "인기겜... 폰겜...",
          "original_form": "인기겜... 폰겜...",
          "speaker_id": "2",
          "time": "20210823 21:13"
        },
        {
          "id": "MDRW2100010726.1.3",
          "form": "오딘...? ㅋㅋㅋㅌㅋㅋㅋㅋㅋ",
          "original_form": "오딘...? ㅋㅋㅋㅌㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:13"
        },
        {
          "id": "MDRW2100010726.1.4",
          "form": "ㅋㅋㅋㅋㅋ그거 재미없어보여",
          "original_form": "ㅋㅋㅋㅋㅋ그거 재미없어보여",
          "speaker_id": "1",
          "time": "20210823 21:13"
        },
        {
          "id": "MDRW2100010726.1.5",
          "form": "힐링겜 하고싶다",
          "original_form": "힐링겜 하고싶다",
          "speaker_id": "1",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010726.1.6",
          "form": "ㅋㅋㅋ 리니지3임",
          "original_form": "ㅋㅋㅋ 리니지3임",
          "speaker_id": "2",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010726.1.7",
          "form": "망겜이네",
          "original_form": "망겜이네",
          "speaker_id": "1",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010726.1.8",
          "form": "게임으로 100개 채울 수 있을듯",
          "original_form": "게임으로 100개 채울 수 있을듯",
          "speaker_id": "2",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010726.1.9",
          "form": "남자분을 찾아봐야하나",
          "original_form": "남자분을 찾아봐야하나",
          "speaker_id": "2",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010726.1.10",
          "form": "잘 안보이던데",
          "original_form": "잘 안보이던데",
          "speaker_id": "1",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010726.1.11",
          "form": "아니다 트릭스터가",
          "original_form": "아니다 트릭스터가",
          "speaker_id": "2",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010726.1.12",
          "form": "리니지3이니",
          "original_form": "리니지3이니",
          "speaker_id": "2",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010726.1.13",
          "form": "오딘은 4네",
          "original_form": "오딘은 4네",
          "speaker_id": "2",
          "time": "20210823 21:14"
        },
        {
          "id": "MDRW2100010726.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100010726.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100010726.1.16",
          "form": "엔씨 리니지 또 내던데",
          "original_form": "엔씨 리니지 또 내던데",
          "speaker_id": "2",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100010726.1.17",
          "form": "w",
          "original_form": "w",
          "speaker_id": "2",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100010726.1.18",
          "form": "전세계 사람들이",
          "original_form": "전세계 사람들이",
          "speaker_id": "2",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100010726.1.19",
          "form": "왤캐 리니지 조아해",
          "original_form": "왤캐 리니지 조아해",
          "speaker_id": "1",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100010726.1.20",
          "form": "리니지를 할거라는 기대는",
          "original_form": "리니지를 할거라는 기대는",
          "speaker_id": "2",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100010726.1.21",
          "form": "어디서 오는거지",
          "original_form": "어디서 오는거지",
          "speaker_id": "2",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100010726.1.22",
          "form": "왜냐면",
          "original_form": "왜냐면",
          "speaker_id": "2",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100010726.1.23",
          "form": "리니지말고 살아남은게 없어서",
          "original_form": "리니지말고 살아남은게 없어서",
          "speaker_id": "2",
          "time": "20210823 21:16"
        },
        {
          "id": "MDRW2100010726.1.24",
          "form": "블소도 잘되나 싶더니 망했자너",
          "original_form": "블소도 잘되나 싶더니 망했자너",
          "speaker_id": "2",
          "time": "20210823 21:16"
        },
        {
          "id": "MDRW2100010726.1.25",
          "form": "현질유도만 적당히 하면 잘 갈건데",
          "original_form": "현질유도만 적당히 하면 잘 갈건데",
          "speaker_id": "1",
          "time": "20210823 21:16"
        },
        {
          "id": "MDRW2100010726.1.26",
          "form": "지들이 못만들어놓고 난리야",
          "original_form": "지들이 못만들어놓고 난리야",
          "speaker_id": "1",
          "time": "20210823 21:16"
        },
        {
          "id": "MDRW2100010726.1.27",
          "form": "리니지말고 다른겜을",
          "original_form": "리니지말고 다른겜을",
          "speaker_id": "2",
          "time": "20210823 21:17"
        },
        {
          "id": "MDRW2100010726.1.28",
          "form": "운용할 자재가 없는듯함",
          "original_form": "운용할 자재가 없는듯함",
          "speaker_id": "2",
          "time": "20210823 21:17"
        },
        {
          "id": "MDRW2100010726.1.29",
          "form": "3N은 이제 없는거같음",
          "original_form": "3N은 이제 없는거같음",
          "speaker_id": "2",
          "time": "20210823 21:17"
        },
        {
          "id": "MDRW2100010726.1.30",
          "form": "다들 카트 해줬으면 좋겠다",
          "original_form": "다들 카트 해줬으면 좋겠다",
          "speaker_id": "1",
          "time": "20210823 21:18"
        },
        {
          "id": "MDRW2100010726.1.31",
          "form": "본부장 갓인데",
          "original_form": "본부장 갓인데",
          "speaker_id": "1",
          "time": "20210823 21:18"
        },
        {
          "id": "MDRW2100010726.1.32",
          "form": "워후..",
          "original_form": "워후..",
          "speaker_id": "1",
          "time": "20210823 21:21"
        }
      ]
    }
