      "utterance": [
        {
          "id": "MDRW2100045798.1.1",
          "form": "직장은 만족스러운 가요",
          "original_form": "직장은 만족스러운 가요",
          "speaker_id": "2",
          "time": "20210908 07:18"
        },
        {
          "id": "MDRW2100045798.1.2",
          "form": "그냥 그래요",
          "original_form": "그냥 그래요",
          "speaker_id": "1",
          "time": "20210908 07:18"
        },
        {
          "id": "MDRW2100045798.1.3",
          "form": "행복하게 일하는 사람이 몇이나 되겠어요",
          "original_form": "행복하게 일하는 사람이 몇이나 되겠어요",
          "speaker_id": "2",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100045798.1.4",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100045798.1.5",
          "form": "그래도 속에서 소소한 행복을 찾아야 할 텐데",
          "original_form": "그래도 속에서 소소한 행복을 찾아야 할 텐데",
          "speaker_id": "2",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100045798.1.6",
          "form": "노력 중이에요",
          "original_form": "노력 중이에요",
          "speaker_id": "1",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100045798.1.7",
          "form": "같이 일하는 사람들은 어떤가요",
          "original_form": "같이 일하는 사람들은 어떤가요",
          "speaker_id": "2",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100045798.1.8",
          "form": "반 반이에요",
          "original_form": "반 반이에요",
          "speaker_id": "1",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100045798.1.9",
          "form": "좋은 사람도 있고 이상한 사람도 있군요",
          "original_form": "좋은 사람도 있고 이상한 사람도 있군요",
          "speaker_id": "2",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100045798.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100045798.1.11",
          "form": "이상한 사람과는 멀리해요",
          "original_form": "이상한 사람과는 멀리해요",
          "speaker_id": "2",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100045798.1.12",
          "form": "그렇지 않아도 그렇게 하려고 노력 중이에요",
          "original_form": "그렇지 않아도 그렇게 하려고 노력 중이에요",
          "speaker_id": "1",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100045798.1.13",
          "form": "좋은 사람들하고만 살기도 부족한 세상이니까",
          "original_form": "좋은 사람들하고만 살기도 부족한 세상이니까",
          "speaker_id": "2",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100045798.1.14",
          "form": "업무적으로 부딪힐 때가 있어요",
          "original_form": "업무적으로 부딪힐 때가 있어요",
          "speaker_id": "1",
          "time": "20210908 07:20"
        },
        {
          "id": "MDRW2100045798.1.15",
          "form": "저런 회사에서 힘들겠어요",
