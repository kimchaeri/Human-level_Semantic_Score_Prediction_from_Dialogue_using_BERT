      "utterance": [
        {
          "id": "MDRW2100039538.1.1",
          "form": "오늘 날씨 좋았어요",
          "original_form": "오늘 날씨 좋았어요",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100039538.1.2",
          "form": "거긴 어땠나요?",
          "original_form": "거긴 어땠나요?",
          "speaker_id": "2",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100039538.1.3",
          "form": "아침엔 추웠는데 점심때는 햇빛비추니까 덥더라구요!",
          "original_form": "아침엔 추웠는데 점심때는 햇빛비추니까 덥더라구요!",
          "speaker_id": "1",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039538.1.4",
          "form": "그래도 비 안오고 맑아서 좋았어요",
          "original_form": "그래도 비 안오고 맑아서 좋았어요",
          "speaker_id": "1",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100039538.1.5",
          "form": "대부분 지역이 날씨가 좋았네요",
          "original_form": "대부분 지역이 날씨가 좋았네요",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039538.1.6",
          "form": "저희도 비슷했어요 초여름 날씨 느낌이였어요",
          "original_form": "저희도 비슷했어요 초여름 날씨 느낌이였어요",
          "speaker_id": "2",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100039538.1.7",
          "form": "얼마전까진 점심땐 밖에 나가기도 싫을 정도였는데",
          "original_form": "얼마전까진 점심땐 밖에 나가기도 싫을 정도였는데",
          "speaker_id": "1",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100039538.1.8",
          "form": "오늘은 일교차는 살짝 있지만 야외활동하기 딱 좋은 날씨였던 것 같아요",
          "original_form": "오늘은 일교차는 살짝 있지만 야외활동하기 딱 좋은 날씨였던 것 같아요",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039538.1.9",
          "form": "그런데 금방 추워질 것 같아서 가을옷은 빨리 꺼내둬야겠어요",
          "original_form": "그런데 금방 추워질 것 같아서 가을옷은 빨리 꺼내둬야겠어요",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039538.1.10",
          "form": "그쵸 모처럼 좋은 날이었어요",
          "original_form": "그쵸 모처럼 좋은 날이었어요",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039538.1.11",
          "form": "전 어제 좀 더 두꺼운 이불 꺼냈어요",
          "original_form": "전 어제 좀 더 두꺼운 이불 꺼냈어요",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039538.1.12",
          "form": "앗 마침 저도요ㅋㅋㅋㅋㅋ",
          "original_form": "앗 마침 저도요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039538.1.13",
          "form": "슬슬 여름이불로는 못 버티겠더라구요",
          "original_form": "슬슬 여름이불로는 못 버티겠더라구요",
          "speaker_id": "1",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039538.1.14",
          "form": "그래도 푹신한 이불 덮어도 안 더워서 너무 좋아요",
          "original_form": "그래도 푹신한 이불 덮어도 안 더워서 너무 좋아요",
          "speaker_id": "1",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039538.1.15",
          "form": "새벽에 춥더라구요",
          "original_form": "새벽에 춥더라구요",
          "speaker_id": "2",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039538.1.16",
          "form": "포근한 이불 느낌 좋아요",
          "original_form": "포근한 이불 느낌 좋아요",
          "speaker_id": "2",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100039538.1.17",
          "form": "맞아요 여름엔 까슬까슬하고 얇은 이불 덮으니까",
          "original_form": "맞아요 여름엔 까슬까슬하고 얇은 이불 덮으니까",
          "speaker_id": "1",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100039538.1.18",
          "form": "무게감있는 이불이 너무 그리웠어요ㅋㅋㅋㅋ",
          "original_form": "무게감있는 이불이 너무 그리웠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039538.1.19",
          "form": "극세사 이불 꺼낼만큼 추워지기 전에 가을이불의 푹신함을 즐겨야겠어요",
          "original_form": "극세사 이불 꺼낼만큼 추워지기 전에 가을이불의 푹신함을 즐겨야겠어요",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100039538.1.20",
          "form": "맞아요 가을은 짧으니까 즐겨야죠 ㅎㅎ",
          "original_form": "맞아요 가을은 짧으니까 즐겨야죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100039538.1.21",
          "form": "봄가을은 너무 빨리가요",
          "original_form": "봄가을은 너무 빨리가요",
          "speaker_id": "2",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100039538.1.22",
          "form": "맞아요 날씨도 야외활동하기 제일 좋은데 아쉬워요",
          "original_form": "맞아요 날씨도 야외활동하기 제일 좋은데 아쉬워요",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100039538.1.23",
          "form": "올해도 너무 빨리 시원해져서",
          "original_form": "올해도 너무 빨리 시원해져서",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100039538.1.24",
          "form": "가을준비 미리해둘걸 그랬어요ㅋㅋㅋㅠ",
          "original_form": "가을준비 미리해둘걸 그랬어요ㅋㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100039538.1.25",
          "form": "항상 그런 것 같아요 준비되지 않았는데 찾아와요 겨울도 그렇게 올 것 같아요",
          "original_form": "항상 그런 것 같아요 준비되지 않았는데 찾아와요 겨울도 그렇게 올 것 같아요",
          "speaker_id": "2",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100039538.1.26",
          "form": "겨울엔 얼어죽지 않기 위해서",
          "original_form": "겨울엔 얼어죽지 않기 위해서",
          "speaker_id": "1",
          "time": "20210903 20:41"
        },
        {
          "id": "MDRW2100039538.1.27",
          "form": "패딩 잘 챙겨둬야겠어요ㅋㅋㅋㅠ",
          "original_form": "패딩 잘 챙겨둬야겠어요ㅋㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210903 20:41"
        }
      ]
