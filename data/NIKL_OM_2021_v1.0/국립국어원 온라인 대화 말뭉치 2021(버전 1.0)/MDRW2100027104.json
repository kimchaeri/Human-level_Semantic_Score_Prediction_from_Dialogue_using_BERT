      "utterance": [
        {
          "id": "MDRW2100027104.1.1",
          "form": "오늘은 핫도그를 주문했습니다",
          "original_form": "오늘은 핫도그를 주문했습니다",
          "speaker_id": "1",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100027104.1.2",
          "form": "오 맛있는 핫도그가 있나요",
          "original_form": "오 맛있는 핫도그가 있나요",
          "speaker_id": "2",
          "time": "20210902 09:30"
        },
        {
          "id": "MDRW2100027104.1.3",
          "form": "저는 빵을 샀습니다",
          "original_form": "저는 빵을 샀습니다",
          "speaker_id": "2",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100027104.1.4",
          "form": "파운드케익을 좋아합니다",
          "original_form": "파운드케익을 좋아합니다",
          "speaker_id": "2",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100027104.1.5",
          "form": "세일해서 싸게 샀습니다",
          "original_form": "세일해서 싸게 샀습니다",
          "speaker_id": "1",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100027104.1.6",
          "form": "파운드케익 뭐랑 먹으시낭ㅂ",
          "original_form": "파운드케익 뭐랑 먹으시낭ㅂ",
          "speaker_id": "1",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100027104.1.7",
          "form": "커피나 우유랑 드시나요",
          "original_form": "커피나 우유랑 드시나요",
          "speaker_id": "1",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100027104.1.8",
          "form": "파운드케익이랑",
          "original_form": "파운드케익이랑",
          "speaker_id": "2",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100027104.1.9",
          "form": "카스테라 세트에요",
          "original_form": "카스테라 세트에요",
          "speaker_id": "2",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100027104.1.10",
          "form": "진짜 맛있겠네이",
          "original_form": "진짜 맛있겠네이",
          "speaker_id": "1",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100027104.1.11",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100027104.1.12",
          "form": "달달한가요",
          "original_form": "달달한가요",
          "speaker_id": "1",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100027104.1.13",
          "form": "집에 홍초가 있어 그거랑 먹으면 맛있쭈게",
          "original_form": "집에 홍초가 있어 그거랑 먹으면 맛있쭈게",
          "speaker_id": "2",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100027104.1.14",
          "form": "새콤달콤이군요",
          "original_form": "새콤달콤이군요",
          "speaker_id": "1",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100027104.1.15",
          "form": "몬딱 먹겟수다",
          "original_form": "몬딱 먹겟수다",
          "speaker_id": "2",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100027104.1.16",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210902 09:33"
        },
        {
          "id": "MDRW2100027104.1.17",
          "form": "전 단짠좋아해요",
          "original_form": "전 단짠좋아해요",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100027104.1.18",
          "form": "진리지요. 단짠",
          "original_form": "진리지요. 단짠",
          "speaker_id": "2",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100027104.1.19",
          "form": "단짠단짠 무한으로 들어갑니다",
          "original_form": "단짠단짠 무한으로 들어갑니다",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100027104.1.20",
          "form": "빵은 우유랑 먹으면 맛있지요",
          "original_form": "빵은 우유랑 먹으면 맛있지요",
          "speaker_id": "2",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100027104.1.21",
          "form": "배가 큰가봅니다",
          "original_form": "배가 큰가봅니다",
          "speaker_id": "2",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100027104.1.22",
          "form": "커피랑 먹어도 맛있지요",
          "original_form": "커피랑 먹어도 맛있지요",
          "speaker_id": "1",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100027104.1.23",
          "form": "케이크도 좋아합니다",
          "original_form": "케이크도 좋아합니다",
          "speaker_id": "2",
          "time": "20210902 09:34"
        },
        {
          "id": "MDRW2100027104.1.24",
          "form": "닭발을 좋아합니다",
          "original_form": "닭발을 좋아합니다",
          "speaker_id": "2",
          "time": "20210902 09:38"
        }
