      "utterance": [
        {
          "id": "MDRW2100006144.1.1",
          "form": "책은 주로 사서보세요?",
          "original_form": "책은 주로 사서보세요?",
          "speaker_id": "1",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100006144.1.2",
          "form": "네 요새는 e북으로 사는 경우가 많은 것 같네요",
          "original_form": "네 요새는 e북으로 사는 경우가 많은 것 같네요",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100006144.1.3",
          "form": "아 그럼 태블릿으로 보세요?",
          "original_form": "아 그럼 태블릿으로 보세요?",
          "speaker_id": "1",
          "time": "20210907 20:58"
        },
        {
          "id": "MDRW2100006144.1.4",
          "form": "폰으로도 보고 리더기로도 봅니다!",
          "original_form": "폰으로도 보고 리더기로도 봅니다!",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006144.1.5",
          "form": "눈은 아프지않으세요?",
          "original_form": "눈은 아프지않으세요?",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006144.1.6",
          "form": "저도 컴으로보다가",
          "original_form": "저도 컴으로보다가",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006144.1.7",
          "form": "눈나빠져서 종이책으로 다시 돌아갔어요",
          "original_form": "눈나빠져서 종이책으로 다시 돌아갔어요",
          "speaker_id": "1",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006144.1.8",
          "form": "밝기 최대한 낮추고 하는데도 아프긴 아프더라고요 ㅠ 근데 리더기는 그 전용 잉크가 액정안에 들어가있어서",
          "original_form": "밝기 최대한 낮추고 하는데도 아프긴 아프더라고요 ㅠ 근데 리더기는 그 전용 잉크가 액정안에 들어가있어서",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006144.1.9",
          "form": "덜아파요!!",
          "original_form": "덜아파요!!",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006144.1.10",
          "form": "아 리더기도 있으신거보니",
          "original_form": "아 리더기도 있으신거보니",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006144.1.11",
          "form": "정말 책 엄청 읽으시나봐요",
          "original_form": "정말 책 엄청 읽으시나봐요",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100006144.1.12",
          "form": "엄청은 아닙니다 ㅋㅋㅋ 요새는 또 넷플만 봐대서 게을러졌어요",
          "original_form": "엄청은 아닙니다 ㅋㅋㅋ 요새는 또 넷플만 봐대서 게을러졌어요",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006144.1.13",
          "form": "책 을 고르는 기준이 있으세요?",
          "original_form": "책 을 고르는 기준이 있으세요?",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006144.1.14",
          "form": "스릴러나 미스터리같은 거 좋아해요",
          "original_form": "스릴러나 미스터리같은 거 좋아해요",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100006144.1.15",
          "form": "아 그리고 범죄같은 것도 좋아해서",
          "original_form": "아 그리고 범죄같은 것도 좋아해서",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100006144.1.16",
          "form": "권일용 프로파일러님 책을 본적도 있는 것 같아요",
          "original_form": "권일용 프로파일러님 책을 본적도 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100006144.1.17",
          "form": "아 저도 그런거 좋아해요",
          "original_form": "아 저도 그런거 좋아해요",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100006144.1.18",
          "form": "범죄심리학같은거",
          "original_form": "범죄심리학같은거",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100006144.1.19",
          "form": "과거 사건같은것도 찾아보구요",
          "original_form": "과거 사건같은것도 찾아보구요",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100006144.1.20",
          "form": "맞아맞아요 좀 무서우면서도 조심해야지 생각도 들고... 동시에 피해자분들 안타깝더라고요",
          "original_form": "맞아맞아요 좀 무서우면서도 조심해야지 생각도 들고... 동시에 피해자분들 안타깝더라고요",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100006144.1.21",
          "form": "네 트라우마 심할거같더라구요",
          "original_form": "네 트라우마 심할거같더라구요",
          "speaker_id": "1",
          "time": "20210907 21:21"
        }
      ]
