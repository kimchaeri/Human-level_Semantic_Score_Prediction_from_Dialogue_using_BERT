        },
        {
          "id": "MDRW2100023786.1.2",
          "form": "저녁뭐쇼핑합니까",
          "original_form": "저녁뭐쇼핑합니까",
          "speaker_id": "1",
          "time": "20210830 17:35"
        },
        {
          "id": "MDRW2100023786.1.3",
          "form": "저번주에 산 목살이 도착했다더라구요?",
          "original_form": "저번주에 산 목살이 도착했다더라구요?",
          "speaker_id": "2",
          "time": "20210830 17:37"
        },
        {
          "id": "MDRW2100023786.1.4",
          "form": "저번주에삿는데 왜 이제서야와요 ㅋㅋㅋㅋㅋ",
          "original_form": "저번주에삿는데 왜 이제서야와요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:38"
        },
        {
          "id": "MDRW2100023786.1.5",
          "form": "수욜인가 샀는데 주문이 밀린듯",
          "original_form": "수욜인가 샀는데 주문이 밀린듯",
          "speaker_id": "2",
          "time": "20210830 17:38"
        },
        {
          "id": "MDRW2100023786.1.6",
          "form": "싸게 샀으니 너그러운 마음으로 기다렸습니다",
          "original_form": "싸게 샀으니 너그러운 마음으로 기다렸습니다",
          "speaker_id": "2",
          "time": "20210830 17:39"
        },
        {
          "id": "MDRW2100023786.1.7",
          "form": "화요일 샀나?",
          "original_form": "화요일 샀나?",
          "speaker_id": "2",
          "time": "20210830 17:39"
        },
        {
          "id": "MDRW2100023786.1.8",
          "form": "암튼",
          "original_form": "암튼",
          "speaker_id": "2",
          "time": "20210830 17:39"
        },
        {
          "id": "MDRW2100023786.1.9",
          "form": "서비스로 주는건 담날 바로옴요",
          "original_form": "서비스로 주는건 담날 바로옴요",
          "speaker_id": "2",
          "time": "20210830 17:39"
        },
        {
          "id": "MDRW2100023786.1.10",
          "form": "뭐지?? 메인이 빠졌는데 하고 기다림",
          "original_form": "뭐지?? 메인이 빠졌는데 하고 기다림",
          "speaker_id": "2",
          "time": "20210830 17:39"
        },
        {
          "id": "MDRW2100023786.1.11",
          "form": "어디서삿는데여",
          "original_form": "어디서삿는데여",
          "speaker_id": "1",
          "time": "20210830 17:41"
        },
        {
          "id": "MDRW2100023786.1.12",
          "form": "농협이 고기사이트 새로 만들었거든요",
          "original_form": "농협이 고기사이트 새로 만들었거든요",
          "speaker_id": "2",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100023786.1.13",
          "form": "라이블리 << 여긴데",
          "original_form": "라이블리 << 여긴데",
          "speaker_id": "2",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100023786.1.14",
          "form": "할인 하나 모르겠네요",
          "original_form": "할인 하나 모르겠네요",
          "speaker_id": "2",
          "time": "20210830 17:43"
        },
        {
          "id": "MDRW2100023786.1.15",
          "form": "요즘 먹거리어플들이 하두많아가지고",
          "original_form": "요즘 먹거리어플들이 하두많아가지고",
          "speaker_id": "1",
          "time": "20210830 17:44"
        },
        {
          "id": "MDRW2100023786.1.16",
          "form": "아니 근데 모기세요?",
          "original_form": "아니 근데 모기세요?",
          "speaker_id": "2",
          "time": "20210830 17:44"
        },
        {
          "id": "MDRW2100023786.1.17",
          "form": "고기도 인터넷으로시켜요?",
          "original_form": "고기도 인터넷으로시켜요?",
          "speaker_id": "1",
          "time": "20210830 17:44"
        },
        {
          "id": "MDRW2100023786.1.18",
          "form": "정보를 다",
          "original_form": "정보를 다",
          "speaker_id": "2",
          "time": "20210830 17:44"
        },
        {
          "id": "MDRW2100023786.1.19",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210830 17:44"
        },
        {
          "id": "MDRW2100023786.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:44"
        },
        {
          "id": "MDRW2100023786.1.21",
          "form": "저 잘 빨,아먹죠",
          "original_form": "저 잘 빨,아먹죠",
          "speaker_id": "1",
          "time": "20210830 17:44"
        },
        {
          "id": "MDRW2100023786.1.22",
          "form": "정보를 다 빨.아가시네",
          "original_form": "정보를 다 빨.아가시네",
          "speaker_id": "2",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100023786.1.23",
          "form": "고기 인터넷으로",
          "original_form": "고기 인터넷으로",
          "speaker_id": "2",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100023786.1.24",
          "form": "가끔씩 시키는데",
          "original_form": "가끔씩 시키는데",
          "speaker_id": "2",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100023786.1.25",
          "form": "뭔데 짱나네",
          "original_form": "뭔데 짱나네",
          "speaker_id": "2",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100023786.1.26",
          "form": "잘되는구만",
          "original_form": "잘되는구만",
          "speaker_id": "2",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100023786.1.27",
          "form": "질이괜찮은가봐여",
          "original_form": "질이괜찮은가봐여",
          "speaker_id": "1",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100023786.1.28",
          "form": "암튼",
          "original_form": "암튼",
          "speaker_id": "2",
          "time": "20210830 17:45"
        },
        {
          "id": "MDRW2100023786.1.29",
          "form": "스티로폼에 잘오나",
          "original_form": "스티로폼에 잘오나",
          "speaker_id": "1",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023786.1.30",
          "form": "아뇨 질이 복불복",
          "original_form": "아뇨 질이 복불복",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023786.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023786.1.32",
          "form": "거진 다 잘 오거든요?",
          "original_form": "거진 다 잘 오거든요?",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023786.1.33",
          "form": "복불복인데 왜사요",
          "original_form": "복불복인데 왜사요",
          "speaker_id": "1",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023786.1.34",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023786.1.35",
          "form": "근데 삼겹. 이런거.시키면.",
          "original_form": "근데 삼겹. 이런거.시키면.",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023786.1.36",
          "form": "후 부들부들 띄어쓰기 쩜으로 합니다",
          "original_form": "후 부들부들 띄어쓰기 쩜으로 합니다",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023786.1.37",
          "form": "아 지방만, 엄청 오고 그러나",
          "original_form": "아 지방만, 엄청 오고 그러나",
          "speaker_id": "1",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023786.1.38",
          "form": "가끔씩 엄청ㅇㅇㅇ",
          "original_form": "가끔씩 엄청ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210830 17:46"
        },
        {
          "id": "MDRW2100023786.1.39",
          "form": "아 최악이다",
          "original_form": "아 최악이다",
          "speaker_id": "1",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023786.1.40",
          "form": "근데 그런건 또 반품 잘 해주긴해요",
          "original_form": "근데 그런건 또 반품 잘 해주긴해요",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023786.1.41",
          "form": "내가 짱.나서.그렇지",
          "original_form": "내가 짱.나서.그렇지",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023786.1.42",
          "form": "귀찮긴하겠네요",
          "original_form": "귀찮긴하겠네요",
          "speaker_id": "1",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023786.1.43",
          "form": "어지간해서는 괜찮게오고",
          "original_form": "어지간해서는 괜찮게오고",
          "speaker_id": "2",
          "time": "20210830 17:47"
        },
        {
          "id": "MDRW2100023786.1.44",
          "form": "특히 저런 오픈이벤트 이런거면",
          "original_form": "특히 저런 오픈이벤트 이런거면",
          "speaker_id": "2",
          "time": "20210830 17:47"
        }
