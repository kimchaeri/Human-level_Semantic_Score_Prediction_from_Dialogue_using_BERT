      "utterance": [
        {
          "id": "MDRW2100021880.1.1",
          "form": "ㅠㅠㅠ어쩔수없죠ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ어쩔수없죠ㅠㅠㅠ",
