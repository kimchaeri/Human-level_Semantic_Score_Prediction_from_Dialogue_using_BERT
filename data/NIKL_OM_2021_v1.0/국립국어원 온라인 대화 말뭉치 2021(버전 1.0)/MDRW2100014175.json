      "utterance": [
        {
          "id": "MDRW2100014175.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 17:36"
        },
        {
          "id": "MDRW2100014175.1.2",
          "form": "하이",
          "original_form": "하이",
          "speaker_id": "1",
          "time": "20210825 17:39"
        },
        {
          "id": "MDRW2100014175.1.3",
          "form": "개강 완전 마음에 들지않아",
          "original_form": "개강 완전 마음에 들지않아",
          "speaker_id": "2",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100014175.1.4",
          "form": "나도 개강의 존재를 잘 모르겠어 코로나 심하잖아 인정하지? 우리 모두 집에 틀어박혀서 싸이버강의를 진행해야돼.",
          "original_form": "나도 개강의 존재를 잘 모르겠어 코로나 심하잖아 인정하지? 우리 모두 집에 틀어박혀서 싸이버강의를 진행해야돼.",
          "speaker_id": "1",
          "time": "20210825 17:41"
        },
        {
          "id": "MDRW2100014175.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ정말이야",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ정말이야",
          "speaker_id": "2",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014175.1.6",
          "form": "비대면 추진!!!",
          "original_form": "비대면 추진!!!",
          "speaker_id": "2",
          "time": "20210825 17:42"
        },
        {
          "id": "MDRW2100014175.1.7",
          "form": "너 수강 신청은 다 했니?",
          "original_form": "너 수강 신청은 다 했니?",
          "speaker_id": "1",
          "time": "20210825 17:43"
        },
        {
          "id": "MDRW2100014175.1.8",
          "form": "그렇지. 너는?",
          "original_form": "그렇지. 너는?",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014175.1.9",
          "form": "나도. 완벽하진 않지만 어쩔 수 없지 내가 선택한 지잡대니까 악으로 깡으로 버틸거야",
          "original_form": "나도. 완벽하진 않지만 어쩔 수 없지 내가 선택한 지잡대니까 악으로 깡으로 버틸거야",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014175.1.10",
          "form": "그럼그럼. 화려한 에이쁠이 나를 비출때까짐ㅁ",
          "original_form": "그럼그럼. 화려한 에이쁠이 나를 비출때까짐ㅁ",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014175.1.11",
          "form": "학교 사람중에 이름에 국, 한이 들어가는 남성이 있는데 정말 별로야",
          "original_form": "학교 사람중에 이름에 국, 한이 들어가는 남성이 있는데 정말 별로야",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014175.1.12",
          "form": "그렇구나 국한이가 너를 괴롭히니?",
          "original_form": "그렇구나 국한이가 너를 괴롭히니?",
          "speaker_id": "2",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014175.1.13",
          "form": "oh 존재만으로도 충분히.",
          "original_form": "oh 존재만으로도 충분히.",
          "speaker_id": "1",
          "time": "20210825 17:46"
        },
        {
          "id": "MDRW2100014175.1.14",
          "form": "나도 그렇게생각해 정말 스트레스야",
          "original_form": "나도 그렇게생각해 정말 스트레스야",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014175.1.15",
          "form": "동아리 생각이 있니?",
          "original_form": "동아리 생각이 있니?",
          "speaker_id": "2",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014175.1.16",
          "form": "혹시 5시니?",
          "original_form": "혹시 5시니?",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014175.1.17",
          "form": "아 당연하지",
          "original_form": "아 당연하지",
          "speaker_id": "1",
          "time": "20210825 17:47"
        },
        {
          "id": "MDRW2100014175.1.18",
          "form": "모든 시간이 그런걸..",
          "original_form": "모든 시간이 그런걸..",
          "speaker_id": "2",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014175.1.19",
          "form": "어떤동아리?",
          "original_form": "어떤동아리?",
          "speaker_id": "2",
          "time": "20210825 17:48"
        },
        {
          "id": "MDRW2100014175.1.20",
          "form": "나는 봉사동아리 들어가고싶으네",
          "original_form": "나는 봉사동아리 들어가고싶으네",
          "speaker_id": "1",
          "time": "20210825 17:49"
        },
        {
          "id": "MDRW2100014175.1.21",
          "form": "정망 갓생이구나",
          "original_form": "정망 갓생이구나",
