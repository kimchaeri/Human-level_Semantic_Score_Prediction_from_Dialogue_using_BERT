      "utterance": [
        {
          "id": "MDRW2100038734.1.1",
          "form": "스크래쳐 는 뭐뭐쓰시나요?",
          "original_form": "스크래쳐 는 뭐뭐쓰시나요?",
          "speaker_id": "1",
          "time": "20210902 20:32"
        },
        {
          "id": "MDRW2100038734.1.2",
          "form": "키트니랑 캐토피아거 자주 사요 ㅎㅎ",
          "original_form": "키트니랑 캐토피아거 자주 사요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100038734.1.3",
          "form": "오 저는 수직기둥모양 잘쓰더라구요",
          "original_form": "오 저는 수직기둥모양 잘쓰더라구요",
          "speaker_id": "1",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100038734.1.4",
          "form": "그거랑 종이로된 쇼파모양 젤 좋아하던데",
          "original_form": "그거랑 종이로된 쇼파모양 젤 좋아하던데",
          "speaker_id": "1",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100038734.1.5",
          "form": "종이가 넘 날려요 ㅜㅜ",
          "original_form": "종이가 넘 날려요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100038734.1.6",
          "form": "맞아요 ㅋㅋㅋㅋㅋ 청소는 진짜... 무의식적으로 해요이제 ㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ 청소는 진짜... 무의식적으로 해요이제 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100038734.1.7",
          "form": "박스도 뜯고 비닐도 뜯고 뜯기면 다 뜯네요 ㅎㅎ",
          "original_form": "박스도 뜯고 비닐도 뜯고 뜯기면 다 뜯네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100038734.1.8",
          "form": "마져요 왜케 종이 찢는걸 좋아하는지 ㅋㅋㅋㅋ",
          "original_form": "마져요 왜케 종이 찢는걸 좋아하는지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100038734.1.9",
          "form": "비닐은 자뀨",
          "original_form": "비닐은 자뀨",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100038734.1.10",
          "form": "할ㅌ 는데",
          "original_form": "할ㅌ 는데",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100038734.1.11",
          "form": "그집 냥이도 그런가요?",
          "original_form": "그집 냥이도 그런가요?",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100038734.1.12",
          "form": "바스락 거리는 소리가 좋나봐요 ㅎㅎ",
          "original_form": "바스락 거리는 소리가 좋나봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100038734.1.13",
          "form": "비닐은 찢기만해요 ㅎㅎ",
          "original_form": "비닐은 찢기만해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100038734.1.14",
          "form": "오 저 이유몰랐는데 미야옹철님이 그렇다고 하더라구요",
          "original_form": "오 저 이유몰랐는데 미야옹철님이 그렇다고 하더라구요",
          "speaker_id": "1",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100038734.1.15",
          "form": "입에 안들어가게 조심해야겠어요",
          "original_form": "입에 안들어가게 조심해야겠어요",
          "speaker_id": "1",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100038734.1.16",
          "form": "미양옹철 채널 도움 많이 되는 것 같아요 ㅎㅎ",
          "original_form": "미양옹철 채널 도움 많이 되는 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100038734.1.17",
          "form": "저도 그 채널 자주 봐요!",
          "original_form": "저도 그 채널 자주 봐요!",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100038734.1.18",
          "form": "그집 애기씨 이뿌게 생겨써요 ㅎㅎ",
          "original_form": "그집 애기씨 이뿌게 생겨써요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100038734.1.19",
          "form": "맞아요ㅠ 그 집 애기들 다 미묘에요 ㅎㅎ",
          "original_form": "맞아요ㅠ 그 집 애기들 다 미묘에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100038734.1.20",
          "form": "영상 틀어 놓으면 저희 애도 눈을 못 떼요 ㅎㅎ",
          "original_form": "영상 틀어 놓으면 저희 애도 눈을 못 떼요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100038734.1.21",
          "form": "오오 우리애들은 새나 쥐 아님 안보더라구요",
          "original_form": "오오 우리애들은 새나 쥐 아님 안보더라구요",
          "speaker_id": "1",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100038734.1.22",
          "form": "사냥감에만 집중하네요 ㅋㅋㅋㅋ",
          "original_form": "사냥감에만 집중하네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100038734.1.23",
          "form": "40인치 샤오미 티비 부순 경력이있는데",
          "original_form": "40인치 샤오미 티비 부순 경력이있는데",
          "speaker_id": "1",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100038734.1.24",
          "form": "화면때릴때 넘 무서워요",
          "original_form": "화면때릴때 넘 무서워요",
          "speaker_id": "1",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100038734.1.25",
          "form": "저희 애는 맥을 두 번이나 부셨어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저희 애는 맥을 두 번이나 부셨어요 ㅋㅋㅋㅋㅋ",
