      "utterance": [
        {
          "id": "MDRW2100018474.1.1",
          "form": "아하 그래서",
          "original_form": "아하 그래서",
          "speaker_id": "2",
          "time": "20210827 02:34"
        },
        {
          "id": "MDRW2100018474.1.2",
          "form": "사서관련이겠군요",
          "original_form": "사서관련이겠군요",
          "speaker_id": "2",
          "time": "20210827 02:34"
        },
        {
          "id": "MDRW2100018474.1.3",
          "form": "그래서 학과에서 실습 한번 근로로 한번 대구시청 인턴으로 한번",
          "original_form": "그래서 학과에서 실습 한번 근로로 한번 대구시청 인턴으로 한번",
          "speaker_id": "1",
          "time": "20210827 02:34"
        },
        {
          "id": "MDRW2100018474.1.4",
          "form": "총 세번햇엇네요 ㅋㅋㅋ",
          "original_form": "총 세번햇엇네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:34"
        },
        {
          "id": "MDRW2100018474.1.5",
          "form": "넵! 공시도 사서직 공시 준비중이에요",
          "original_form": "넵! 공시도 사서직 공시 준비중이에요",
          "speaker_id": "1",
          "time": "20210827 02:34"
        },
        {
          "id": "MDRW2100018474.1.6",
          "form": "이번에 제가 일한 근로지에",
          "original_form": "이번에 제가 일한 근로지에",
          "speaker_id": "2",
          "time": "20210827 02:34"
        },
        {
          "id": "MDRW2100018474.1.7",
          "form": "사서분이",
          "original_form": "사서분이",
          "speaker_id": "2",
          "time": "20210827 02:34"
        },
        {
          "id": "MDRW2100018474.1.8",
          "form": "아시는분일수도 있겠네요",
          "original_form": "아시는분일수도 있겠네요",
          "speaker_id": "2",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018474.1.9",
          "form": "오오오 대구에요??",
          "original_form": "오오오 대구에요??",
          "speaker_id": "1",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018474.1.10",
          "form": "대학만같다면",
          "original_form": "대학만같다면",
          "speaker_id": "2",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018474.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018474.1.12",
          "form": "대구는 별로 뽑지도 않는데",
          "original_form": "대구는 별로 뽑지도 않는데",
          "speaker_id": "1",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018474.1.13",
          "form": "ㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018474.1.14",
          "form": "대구는 공무원 넘 적게뽑아요",
          "original_form": "대구는 공무원 넘 적게뽑아요",
          "speaker_id": "1",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018474.1.15",
          "form": "근데 그분은 14학번이신데",
          "original_form": "근데 그분은 14학번이신데",
          "speaker_id": "2",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018474.1.16",
          "form": "졸업하자마자 취업하신거라고",
          "original_form": "졸업하자마자 취업하신거라고",
          "speaker_id": "2",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018474.1.17",
          "form": "전 affiliation 졸업햇어요!!",
          "original_form": "전 &affiliation& 졸업햇어요!!",
          "speaker_id": "1",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018474.1.18",
          "form": "근데 저희 동기들은",
          "original_form": "근데 저희 동기들은",
          "speaker_id": "1",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018474.1.19",
          "form": "사서직 하는 친구 별로 없어요",
          "original_form": "사서직 하는 친구 별로 없어요",
          "speaker_id": "1",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018474.1.20",
          "form": "문헌정보면 사서로 빠지지않나요 대부분?",
          "original_form": "문헌정보면 사서로 빠지지않나요 대부분?",
          "speaker_id": "2",
          "time": "20210827 02:35"
        },
        {
          "id": "MDRW2100018474.1.21",
          "form": "그래서 사서 된 동기들 소식 별로 못들어봤어요",
          "original_form": "그래서 사서 된 동기들 소식 별로 못들어봤어요",
          "speaker_id": "1",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018474.1.22",
          "form": "근데 특이하게 제 동기들은",
          "original_form": "근데 특이하게 제 동기들은",
          "speaker_id": "1",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018474.1.23",
          "form": "사서교사도 많이 하고 공기업도 많이가고",
          "original_form": "사서교사도 많이 하고 공기업도 많이가고",
          "speaker_id": "1",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018474.1.24",
          "form": "사서직은 오히려 적어요",
          "original_form": "사서직은 오히려 적어요",
          "speaker_id": "1",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018474.1.25",
          "form": "이야기들어보면",
          "original_form": "이야기들어보면",
          "speaker_id": "2",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018474.1.26",
          "form": "확실히 사서직은",
          "original_form": "확실히 사서직은",
          "speaker_id": "2",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018474.1.27",
          "form": "사람도 적게뽑는데",
          "original_form": "사람도 적게뽑는데",
          "speaker_id": "2",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018474.1.28",
          "form": "자리가 안생긴다 하더라구여",
          "original_form": "자리가 안생긴다 하더라구여",
          "speaker_id": "2",
          "time": "20210827 02:36"
        },
        {
          "id": "MDRW2100018474.1.29",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018474.1.30",
          "form": "누군가가 퇴사하거나",
          "original_form": "누군가가 퇴사하거나",
          "speaker_id": "1",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018474.1.31",
          "form": "도서관이 새로지어지거나",
          "original_form": "도서관이 새로지어지거나",
          "speaker_id": "1",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018474.1.32",
          "form": "둘 중 하나가 잇어야해요 ㅠㅠㅠㅠㅋㅋㅋㅋㅋㅋ",
          "original_form": "둘 중 하나가 잇어야해요 ㅠㅠㅠㅠㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018474.1.33",
          "form": "마침 제가 간 도서관이 생긴지 얼마안됬어서",
          "original_form": "마침 제가 간 도서관이 생긴지 얼마안됬어서",
          "speaker_id": "2",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018474.1.34",
          "form": "사서분 두명계셨는데",
          "original_form": "사서분 두명계셨는데",
          "speaker_id": "2",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018474.1.35",
          "form": "근데 얘기듣기로는 전에 대명동에 미군부대잇던데 거기 새로 도서관 짓는다더라구요",
          "original_form": "근데 얘기듣기로는 전에 대명동에 미군부대잇던데 거기 새로 도서관 짓는다더라구요",
          "speaker_id": "1",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018474.1.36",
          "form": "거길 믿어봐야조 ㅋㅋㅋㅋ",
          "original_form": "거길 믿어봐야조 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018474.1.37",
          "form": "엥 진짜요?? 혹시 어딘지 물어봐도 될까요??",
          "original_form": "엥 진짜요?? 혹시 어딘지 물어봐도 될까요??",
          "speaker_id": "1",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018474.1.38",
          "form": "대구에 신설도서관 얘기 첨들어봐요",
          "original_form": "대구에 신설도서관 얘기 첨들어봐요",
          "speaker_id": "1",
          "time": "20210827 02:37"
        },
        {
          "id": "MDRW2100018474.1.39",
          "form": "2016년이면",
          "original_form": "2016년이면",
          "speaker_id": "2",
          "time": "20210827 02:38"
        },
        {
          "id": "MDRW2100018474.1.40",
          "form": "옛날인가요",
          "original_form": "옛날인가요",
          "speaker_id": "2",
          "time": "20210827 02:38"
        },
        {
          "id": "MDRW2100018474.1.41",
