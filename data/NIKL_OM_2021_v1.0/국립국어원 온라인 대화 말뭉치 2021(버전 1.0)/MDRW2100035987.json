      "utterance": [
        {
          "id": "MDRW2100035987.1.1",
          "form": "버거는 고기때메 먹어서",
          "original_form": "버거는 고기때메 먹어서",
          "speaker_id": "1",
          "time": "20210830 13:27"
        },
        {
          "id": "MDRW2100035987.1.2",
          "form": "저는 치킨버거 잘 안먹어요 ㅋㅋㅋ",
          "original_form": "저는 치킨버거 잘 안먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:27"
        },
        {
          "id": "MDRW2100035987.1.3",
          "form": "아하 고기를 좋아하시는구낭",
          "original_form": "아하 고기를 좋아하시는구낭",
          "speaker_id": "2",
          "time": "20210830 13:27"
        },
        {
          "id": "MDRW2100035987.1.4",
          "form": "소고기를 제일 좋아하시나용?",
          "original_form": "소고기를 제일 좋아하시나용?",
          "speaker_id": "2",
          "time": "20210830 13:27"
        },
        {
          "id": "MDRW2100035987.1.5",
          "form": "네 ㅋㅋㅋ 글구 소고기와 양파의 조화를 좋아해요",
          "original_form": "네 ㅋㅋㅋ 글구 소고기와 양파의 조화를 좋아해요",
          "speaker_id": "1",
          "time": "20210830 13:29"
        },
        {
          "id": "MDRW2100035987.1.6",
          "form": "베토디도 조화로운 맛 아닌가요? ㅎㅎ",
          "original_form": "베토디도 조화로운 맛 아닌가요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 13:29"
        },
        {
          "id": "MDRW2100035987.1.7",
          "form": "그쳐ㅋㅋㅋㅋ",
          "original_form": "그쳐ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:29"
        },
        {
          "id": "MDRW2100035987.1.8",
          "form": "토마토 베이컨 고기",
          "original_form": "토마토 베이컨 고기",
          "speaker_id": "2",
          "time": "20210830 13:29"
        },
        {
          "id": "MDRW2100035987.1.9",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:29"
        },
        {
          "id": "MDRW2100035987.1.10",
          "form": "소고기 드실때 양파 구워드시는구나",
          "original_form": "소고기 드실때 양파 구워드시는구나",
          "speaker_id": "2",
          "time": "20210830 13:29"
        },
        {
          "id": "MDRW2100035987.1.11",
          "form": "저도 아스파라거스 말고",
          "original_form": "저도 아스파라거스 말고",
          "speaker_id": "2",
          "time": "20210830 13:30"
        },
        {
          "id": "MDRW2100035987.1.12",
          "form": "양파 먹어용",
          "original_form": "양파 먹어용",
          "speaker_id": "2",
          "time": "20210830 13:30"
        },
        {
          "id": "MDRW2100035987.1.13",
          "form": "네 ㅎㅎㅎ 짱맛잇어요",
          "original_form": "네 ㅎㅎㅎ 짱맛잇어요",
          "speaker_id": "1",
          "time": "20210830 13:30"
        },
        {
          "id": "MDRW2100035987.1.14",
          "form": "양파랑 새송이버섯 구워먹는데 좋아요!",
          "original_form": "양파랑 새송이버섯 구워먹는데 좋아요!",
          "speaker_id": "1",
          "time": "20210830 13:30"
        },
        {
          "id": "MDRW2100035987.1.15",
          "form": "살짝 반투명해질때까지 구우면",
          "original_form": "살짝 반투명해질때까지 구우면",
          "speaker_id": "2",
          "time": "20210830 13:30"
        },
        {
          "id": "MDRW2100035987.1.16",
          "form": "맵지도않고",
          "original_form": "맵지도않고",
          "speaker_id": "2",
          "time": "20210830 13:30"
        },
        {
          "id": "MDRW2100035987.1.17",
          "form": "소고기 느끼함도 잡아주고ㅋㅋ",
          "original_form": "소고기 느끼함도 잡아주고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:30"
        },
        {
          "id": "MDRW2100035987.1.18",
          "form": "맞아요 맞아요 달달하고",
          "original_form": "맞아요 맞아요 달달하고",
          "speaker_id": "1",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100035987.1.19",
          "form": "고기 맛도 안 해쳐서 좋아요",
          "original_form": "고기 맛도 안 해쳐서 좋아요",
          "speaker_id": "1",
          "time": "20210830 13:31"
        },
        {
          "id": "MDRW2100035987.1.20",
          "form": "드실줄 아시는군여!ㅋㅋㅋ",
          "original_form": "드실줄 아시는군여!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100035987.1.21",
          "form": "소고기 어느부위 좋아하세요?",
          "original_form": "소고기 어느부위 좋아하세요?",
          "speaker_id": "2",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100035987.1.22",
          "form": "저는 부채살!",
          "original_form": "저는 부채살!",
          "speaker_id": "2",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100035987.1.23",
          "form": "오 사실 저 소ㅗㄱ기 부위는 잘 모르는데",
          "original_form": "오 사실 저 소ㅗㄱ기 부위는 잘 모르는데",
          "speaker_id": "1",
          "time": "20210830 13:32"
        },
        {
          "id": "MDRW2100035987.1.24",
          "form": "부채살에는 어떤 매력이 잇나요?",
          "original_form": "부채살에는 어떤 매력이 잇나요?",
          "speaker_id": "1",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100035987.1.25",
          "form": "적절한 마블링이 조화롭고",
          "original_form": "적절한 마블링이 조화롭고",
          "speaker_id": "2",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100035987.1.26",
          "form": "부드러워요!",
          "original_form": "부드러워요!",
          "speaker_id": "2",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100035987.1.27",
          "form": "제가 맛 표현을 잘 못해섴ㅋㅋㅋ",
          "original_form": "제가 맛 표현을 잘 못해섴ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:33"
        },
        {
          "id": "MDRW2100035987.1.28",
          "form": "마블링이 조화롭군요",
          "original_form": "마블링이 조화롭군요",
          "speaker_id": "1",
          "time": "20210830 13:35"
        },
        {
          "id": "MDRW2100035987.1.29",
          "form": "ㅈㅓ도 다음엔 부위 좀 체크하면서 먹어야겠어요 ㅋㅋㅋㅋ",
          "original_form": "ㅈㅓ도 다음엔 부위 좀 체크하면서 먹어야겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:35"
        },
        {
          "id": "MDRW2100035987.1.30",
          "form": "소고기 무한리필 얼마전에 갔는데",
          "original_form": "소고기 무한리필 얼마전에 갔는데",
          "speaker_id": "2",
          "time": "20210830 13:37"
        },
        {
          "id": "MDRW2100035987.1.31",
          "form": "부채살만 원없이 먹고왔어요ㅋㅋㅋ",
          "original_form": "부채살만 원없이 먹고왔어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:37"
        },
        {
          "id": "MDRW2100035987.1.32",
          "form": "name1님은 후식 드셨어요?",
          "original_form": "&name1&님은 후식 드셨어요?",
          "speaker_id": "2",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100035987.1.33",
          "form": "와 소고기 무한리필",
          "original_form": "와 소고기 무한리필",
          "speaker_id": "1",
          "time": "20210830 14:06"
        },
        {
          "id": "MDRW2100035987.1.34",
          "form": "전 지금 잠깨러 커피사러왓어요 ㅋㅋ",
          "original_form": "전 지금 잠깨러 커피사러왓어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:06"
        }
      ]
