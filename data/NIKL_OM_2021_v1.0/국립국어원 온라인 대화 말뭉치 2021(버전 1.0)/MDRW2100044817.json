      "utterance": [
        {
          "id": "MDRW2100044817.1.1",
          "form": "거북목님 많이 남으셨나요??",
          "original_form": "거북목님 많이 남으셨나요??",
          "speaker_id": "2",
          "time": "20210907 23:52"
        },
        {
          "id": "MDRW2100044817.1.2",
          "form": "약 28개쯤 남은 것 같아요 ㅋㅋㅋㅋ누누님은 몇 가 채우셨어뇨?",
          "original_form": "약 28개쯤 남은 것 같아요 ㅋㅋㅋㅋ누누님은 몇 가 채우셨어뇨?",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044817.1.3",
          "form": "저는 목표치만큼 채웠어요!!",
          "original_form": "저는 목표치만큼 채웠어요!!",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044817.1.4",
          "form": "엄청 많이하셨네요!!",
          "original_form": "엄청 많이하셨네요!!",
          "speaker_id": "2",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044817.1.5",
          "form": "대박 ㅜㅜㅠ축하드려요",
          "original_form": "대박 ㅜㅜㅠ축하드려요",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044817.1.6",
          "form": "저도 얼른 마저 하고 꿀잠 잘 것이야요",
          "original_form": "저도 얼른 마저 하고 꿀잠 잘 것이야요",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044817.1.7",
          "form": "아쉽네요ㅠㅠ",
          "original_form": "아쉽네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044817.1.8",
          "form": "누누님 덕분에 재밌게 했어요! 감사합니다",
          "original_form": "누누님 덕분에 재밌게 했어요! 감사합니다",
          "speaker_id": "1",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044817.1.9",
          "form": "이렇게된거 이 대화는",
          "original_form": "이렇게된거 이 대화는",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044817.1.10",
          "form": "끝내요ㅜㅠ",
          "original_form": "끝내요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044817.1.11",
          "form": "제가 아쉬워서요ㅜㅠ",
          "original_form": "제가 아쉬워서요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044817.1.12",
          "form": "ㅠㅜㅜㅠㅠㅠㅜㅜㅜㅜㅠㅠ",
          "original_form": "ㅠㅜㅜㅠㅠㅠㅜㅜㅜㅜㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044817.1.13",
          "form": "핑퐁도 수월하고 넘 좋았는데 정말 아쉬워요",
          "original_form": "핑퐁도 수월하고 넘 좋았는데 정말 아쉬워요",
          "speaker_id": "1",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044817.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044817.1.15",
          "form": "왜 기간을 단축해서",
          "original_form": "왜 기간을 단축해서",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044817.1.16",
          "form": "저희 스무고개할까요?",
          "original_form": "저희 스무고개할까요?",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044817.1.17",
          "form": "ㅠㅜㅠㅜㅜ",
          "original_form": "ㅠㅜㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044817.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044817.1.19",
          "form": "앗 저 도와주시는 거예요?",
          "original_form": "앗 저 도와주시는 거예요?",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044817.1.20",
          "form": "제가 너무아쉬워서요ㅠ",
          "original_form": "제가 너무아쉬워서요ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044817.1.21",
          "form": "(질 척 대 는 중)",
          "original_form": "(질 척 대 는 중)",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044817.1.22",
          "form": "ㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044817.1.23",
          "form": "넘 감사하고 해피해유 아이고",
          "original_form": "넘 감사하고 해피해유 아이고",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044817.1.24",
          "form": "그럼 뭐로 할까요!",
          "original_form": "그럼 뭐로 할까요!",
          "speaker_id": "1",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044817.1.25",
          "form": "제가 맞춰볼게요",
          "original_form": "제가 맞춰볼게요",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044817.1.26",
          "form": "매운음식인가요?",
          "original_form": "매운음식인가요?",
          "speaker_id": "2",
          "time": "20210907 23:59"
        },
        {
          "id": "MDRW2100044817.1.27",
          "form": "ㅋㅋㅋㅋ오늘 먹은 아침식사 맞춰보시기!",
          "original_form": "ㅋㅋㅋㅋ오늘 먹은 아침식사 맞춰보시기!",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044817.1.28",
          "form": "아니요! 안 매워요",
          "original_form": "아니요! 안 매워요",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044817.1.29",
          "form": "국물있나요?",
          "original_form": "국물있나요?",
          "speaker_id": "2",
