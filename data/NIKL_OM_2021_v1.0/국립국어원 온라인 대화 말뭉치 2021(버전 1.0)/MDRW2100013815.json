        {
          "id": "MDRW2100013815.1.1",
          "form": "네 해요 주부라 안할수가 없네요",
          "original_form": "네 해요 주부라 안할수가 없네요",
          "speaker_id": "2",
          "time": "20210825 16:21"
        },
        {
          "id": "MDRW2100013815.1.2",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210825 16:32"
        },
        {
          "id": "MDRW2100013815.1.3",
          "form": "청소팁이 있나요?",
          "original_form": "청소팁이 있나요?",
          "speaker_id": "2",
          "time": "20210825 16:33"
        },
        {
          "id": "MDRW2100013815.1.4",
          "form": "곰팡이 안 생기게 환기도",
          "original_form": "곰팡이 안 생기게 환기도",
          "speaker_id": "1",
          "time": "20210825 16:36"
        },
        {
          "id": "MDRW2100013815.1.5",
          "form": "가사분담은 잘되시나요?",
          "original_form": "가사분담은 잘되시나요?",
          "speaker_id": "2",
          "time": "20210825 16:37"
        },
        {
          "id": "MDRW2100013815.1.6",
          "form": "네 잘되여",
          "original_form": "네 잘되여",
          "speaker_id": "1",
          "time": "20210825 16:39"
        },
        {
          "id": "MDRW2100013815.1.7",
          "form": "가사는 뭐가제일힘드시나요?",
          "original_form": "가사는 뭐가제일힘드시나요?",
          "speaker_id": "2",
          "time": "20210825 16:48"
        },
        {
          "id": "MDRW2100013815.1.8",
          "form": "화장실청소가가장 힘들죠",
          "original_form": "화장실청소가가장 힘들죠",
          "speaker_id": "1",
          "time": "20210825 20:58"
        },
        {
          "id": "MDRW2100013815.1.9",
          "form": "가족들은 잘하나요?",
          "original_form": "가족들은 잘하나요?",
          "speaker_id": "2",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100013815.1.10",
          "form": "네 분담요",
          "original_form": "네 분담요",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100013815.1.11",
          "form": "질문할거 있으시나요?",
          "original_form": "질문할거 있으시나요?",
          "speaker_id": "2",
          "time": "20210826 06:05"
        },
        {
          "id": "MDRW2100013815.1.12",
          "form": "가족은 어떻게되요?",
          "original_form": "가족은 어떻게되요?",
          "speaker_id": "1",
          "time": "20210826 06:38"
