        },
        {
          "id": "MDRW2100046500.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211026 11:50"
        },
        {
          "id": "MDRW2100046500.1.3",
          "form": "글램핑 좋아하세요?",
          "original_form": "글램핑 좋아하세요?",
          "speaker_id": "2",
          "time": "20211026 11:50"
        },
        {
          "id": "MDRW2100046500.1.4",
          "form": "네 좋아하죠",
          "original_form": "네 좋아하죠",
          "speaker_id": "1",
          "time": "20211026 11:51"
        },
        {
          "id": "MDRW2100046500.1.5",
          "form": "다만 못가서 아쉽네요",
          "original_form": "다만 못가서 아쉽네요",
          "speaker_id": "1",
          "time": "20211026 11:51"
        },
        {
          "id": "MDRW2100046500.1.6",
          "form": "오 저도 좋아해요",
          "original_form": "오 저도 좋아해요",
          "speaker_id": "2",
          "time": "20211026 11:52"
        },
        {
          "id": "MDRW2100046500.1.7",
          "form": "그러니까요 ㅠㅠ",
          "original_form": "그러니까요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211026 11:52"
        },
        {
          "id": "MDRW2100046500.1.8",
          "form": "이제 슬슬 추워지면",
          "original_form": "이제 슬슬 추워지면",
          "speaker_id": "2",
          "time": "20211026 11:52"
        },
        {
          "id": "MDRW2100046500.1.9",
          "form": "불멍하러가야되는데~",
          "original_form": "불멍하러가야되는데~",
          "speaker_id": "2",
          "time": "20211026 11:52"
        },
        {
          "id": "MDRW2100046500.1.10",
          "form": "요즘 날씨가 가기 딱좋은데",
          "original_form": "요즘 날씨가 가기 딱좋은데",
          "speaker_id": "1",
          "time": "20211026 11:52"
        },
        {
          "id": "MDRW2100046500.1.11",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "2",
          "time": "20211026 11:53"
        },
        {
          "id": "MDRW2100046500.1.12",
          "form": "지금이 적긴데...",
          "original_form": "지금이 적긴데...",
          "speaker_id": "2",
          "time": "20211026 11:53"
        },
        {
          "id": "MDRW2100046500.1.13",
          "form": "인기많은곳은",
          "original_form": "인기많은곳은",
          "speaker_id": "2",
          "time": "20211026 11:53"
        },
        {
          "id": "MDRW2100046500.1.14",
          "form": "주말에 예약이 꽉꽉 찼더라구요 ㅋㅋ ㅠㅠ",
          "original_form": "주말에 예약이 꽉꽉 찼더라구요 ㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20211026 11:54"
        },
        {
          "id": "MDRW2100046500.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20211026 11:54"
        },
        {
          "id": "MDRW2100046500.1.16",
          "form": "혹시",
          "original_form": "혹시",
          "speaker_id": "2",
          "time": "20211026 11:54"
        },
        {
          "id": "MDRW2100046500.1.17",
          "form": "글램핑 명소 아시나요",
          "original_form": "글램핑 명소 아시나요",
          "speaker_id": "2",
          "time": "20211026 11:54"
        },
        {
          "id": "MDRW2100046500.1.18",
          "form": "저는 가평밖에 못가봐서..",
          "original_form": "저는 가평밖에 못가봐서..",
          "speaker_id": "2",
          "time": "20211026 11:54"
        },
        {
          "id": "MDRW2100046500.1.19",
          "form": "가평 좋은곳이 있나요,",
          "original_form": "가평 좋은곳이 있나요,",
          "speaker_id": "1",
          "time": "20211026 11:54"
        },
        {
          "id": "MDRW2100046500.1.20",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20211026 11:54"
        },
        {
          "id": "MDRW2100046500.1.21",
          "form": "저도 따라다니는 입장이라서요",
          "original_form": "저도 따라다니는 입장이라서요",
          "speaker_id": "1",
          "time": "20211026 11:54"
        },
        {
          "id": "MDRW2100046500.1.22",
          "form": "저는 플로렌스 글램핑 이라는 곳에 갔었는데",
          "original_form": "저는 플로렌스 글램핑 이라는 곳에 갔었는데",
          "speaker_id": "2",
          "time": "20211026 11:55"
        },
        {
          "id": "MDRW2100046500.1.23",
          "form": "깨끗하고 넓고 괜찮아서",
          "original_form": "깨끗하고 넓고 괜찮아서",
          "speaker_id": "2",
          "time": "20211026 11:56"
        },
        {
          "id": "MDRW2100046500.1.24",
          "form": "주변에 많이 추천하고 다녔어요 ㅋㅋㅋ",
          "original_form": "주변에 많이 추천하고 다녔어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211026 11:56"
        },
        {
          "id": "MDRW2100046500.1.25",
          "form": "오..다음에 한번 가봐야겠어요",
          "original_form": "오..다음에 한번 가봐야겠어요",
          "speaker_id": "1",
          "time": "20211026 11:56"
        },
        {
          "id": "MDRW2100046500.1.26",
          "form": "네네 추천드려용",
          "original_form": "네네 추천드려용",
          "speaker_id": "2",
          "time": "20211026 11:56"
        },
        {
          "id": "MDRW2100046500.1.27",
          "form": "추천 감사합니다",
          "original_form": "추천 감사합니다",
          "speaker_id": "1",
          "time": "20211026 11:57"
        },
        {
