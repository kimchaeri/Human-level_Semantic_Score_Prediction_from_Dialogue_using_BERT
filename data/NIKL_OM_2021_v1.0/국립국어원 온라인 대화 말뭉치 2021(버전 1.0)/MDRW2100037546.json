      "utterance": [
        {
          "id": "MDRW2100037546.1.1",
          "form": "아뇨 곧차려먹어야하는데 귀찮아서 빈둥빈둥 ㅋㅋ",
          "original_form": "아뇨 곧차려먹어야하는데 귀찮아서 빈둥빈둥 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 09:59"
        },
        {
          "id": "MDRW2100037546.1.2",
          "form": "뭐 드실거에요",
          "original_form": "뭐 드실거에요",
          "speaker_id": "1",
          "time": "20210901 10:01"
        },
        {
          "id": "MDRW2100037546.1.3",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "1",
          "time": "20210901 10:01"
        },
        {
          "id": "MDRW2100037546.1.4",
          "form": "저 도가니탕 먹어써욥",
          "original_form": "저 도가니탕 먹어써욥",
          "speaker_id": "2",
          "time": "20210901 16:30"
        },
        {
          "id": "MDRW2100037546.1.5",
          "form": "저녁은 비요뜨 먹게용 ㅎㅎ",
          "original_form": "저녁은 비요뜨 먹게용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 16:31"
        },
        {
          "id": "MDRW2100037546.1.6",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100037546.1.7",
          "form": "비요뜨",
          "original_form": "비요뜨",
          "speaker_id": "1",
          "time": "20210901 16:50"
        },
        {
          "id": "MDRW2100037546.1.8",
          "form": "저 정말 좋아해요",
          "original_form": "저 정말 좋아해요",
          "speaker_id": "1",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100037546.1.9",
          "form": "헉 저도 엄청요 ㅠ",
          "original_form": "헉 저도 엄청요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100037546.1.10",
          "form": "마트가면 대량구입ㅋㅋㅋㅋ",
          "original_form": "마트가면 대량구입ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100037546.1.11",
          "form": "비요뜨처럼 생긴 거",
          "original_form": "비요뜨처럼 생긴 거",
          "speaker_id": "1",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100037546.1.12",
          "form": "다양하잖아여",
          "original_form": "다양하잖아여",
          "speaker_id": "1",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100037546.1.13",
          "form": "비요뜨만 드세요?",
          "original_form": "비요뜨만 드세요?",
          "speaker_id": "1",
          "time": "20210901 16:52"
        },
        {
          "id": "MDRW2100037546.1.14",
          "form": "넹 소식하려구용 ㅋㄷ",
          "original_form": "넹 소식하려구용 ㅋㄷ",
          "speaker_id": "2",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100037546.1.15",
          "form": "아하 ㅎ",
          "original_form": "아하 ㅎ",
          "speaker_id": "1",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100037546.1.16",
          "form": "비요뜨 말고 야쿠르트에서 나온",
          "original_form": "비요뜨 말고 야쿠르트에서 나온",
          "speaker_id": "1",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100037546.1.17",
          "form": "팅글도 맛있어요",
          "original_form": "팅글도 맛있어요",
          "speaker_id": "1",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100037546.1.18",
          "form": "오호 함 먹어봐야겠네용",
          "original_form": "오호 함 먹어봐야겠네용",
          "speaker_id": "2",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100037546.1.19",
          "form": "풀무원꺼는",
          "original_form": "풀무원꺼는",
          "speaker_id": "1",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100037546.1.20",
          "form": "좀.. 제 스타일이 아니였습니다",
          "original_form": "좀.. 제 스타일이 아니였습니다",
          "speaker_id": "1",
          "time": "20210901 17:01"
        },
        {
          "id": "MDRW2100037546.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ요거트 엄청좋아하시나봐여",
          "original_form": "ㅋㅋㅋㅋㅋㅋ요거트 엄청좋아하시나봐여",
          "speaker_id": "2",
          "time": "20210901 17:02"
        },
        {
          "id": "MDRW2100037546.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ원래 안좋아했는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ원래 안좋아했는데",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100037546.1.23",
