      "utterance": [
        {
          "id": "MDRW2100044092.1.1",
          "form": "어떤 제품이요?",
          "original_form": "어떤 제품이요?",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044092.1.2",
          "form": "스마트워치랑 아이패드요",
          "original_form": "스마트워치랑 아이패드요",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044092.1.3",
          "form": "오 애플쪽 좋아하세요?",
          "original_form": "오 애플쪽 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044092.1.4",
          "form": "원래 갤럭시 쪽이었는데",
          "original_form": "원래 갤럭시 쪽이었는데",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044092.1.5",
          "form": "요즘 눈길이 가네요",
          "original_form": "요즘 눈길이 가네요",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044092.1.6",
          "form": "저도 갤럭시 이용해도 아이패드는 탐나더라구요",
          "original_form": "저도 갤럭시 이용해도 아이패드는 탐나더라구요",
          "speaker_id": "2",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100044092.1.7",
          "form": "네 저도 휴대폰은 갤럭시인데",
          "original_form": "네 저도 휴대폰은 갤럭시인데",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044092.1.8",
          "form": "아이패드는 탐나네요",
          "original_form": "아이패드는 탐나네요",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044092.1.9",
          "form": "전 다꾸랑 그림에 그렇게 좋다고 해서요",
          "original_form": "전 다꾸랑 그림에 그렇게 좋다고 해서요",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044092.1.10",
          "form": "아 다꾸랑 그림 좋아하시는구나",
          "original_form": "아 다꾸랑 그림 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044092.1.11",
          "form": "전 필기용으로 사고 싶더라고요",
          "original_form": "전 필기용으로 사고 싶더라고요",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044092.1.12",
          "form": "필기할 일이 많은데 책 다 들고 다니기도 힘들고",
          "original_form": "필기할 일이 많은데 책 다 들고 다니기도 힘들고",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044092.1.13",
          "form": "네 좋아해요 ㅎㅎ 필기감도 좋다고 하더라구요",
          "original_form": "네 좋아해요 ㅎㅎ 필기감도 좋다고 하더라구요",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044092.1.14",
          "form": "네 애플펜슬과 함께",
          "original_form": "네 애플펜슬과 함께",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044092.1.15",
          "form": "사고싶어요",
          "original_form": "사고싶어요",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044092.1.16",
          "form": "노트북 있어서 패드 욕심은 크게 없었는데",
          "original_form": "노트북 있어서 패드 욕심은 크게 없었는데",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044092.1.17",
          "form": "요즘 눈길이 가네요",
          "original_form": "요즘 눈길이 가네요",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044092.1.18",
          "form": "아이패드랑 펜슬은 세트죠 ㅎㅎ",
          "original_form": "아이패드랑 펜슬은 세트죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044092.1.19",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044092.1.20",
          "form": "꼭 사야한다더라고요",
          "original_form": "꼭 사야한다더라고요",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044092.1.21",
          "form": "아 막 사고 싶네요",
          "original_form": "아 막 사고 싶네요",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044092.1.22",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044092.1.23",
          "form": "추석때 보너스 받음 사려고 대기중이예요",
          "original_form": "추석때 보너스 받음 사려고 대기중이예요",
          "speaker_id": "1",
          "time": "20210907 21:58"
        }
      ]
