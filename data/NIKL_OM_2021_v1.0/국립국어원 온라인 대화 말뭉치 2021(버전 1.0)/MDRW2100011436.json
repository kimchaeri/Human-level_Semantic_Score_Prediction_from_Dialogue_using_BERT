      "utterance": [
        {
          "id": "MDRW2100011436.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100011436.1.2",
          "form": "최근 구매하신 물건 있으신가요",
          "original_form": "최근 구매하신 물건 있으신가요",
          "speaker_id": "2",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100011436.1.3",
          "form": "네 신발이요!",
          "original_form": "네 신발이요!",
          "speaker_id": "1",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100011436.1.4",
          "form": "오 어디서 사셨나요?",
          "original_form": "오 어디서 사셨나요?",
          "speaker_id": "2",
          "time": "20210824 14:35"
        },
        {
          "id": "MDRW2100011436.1.5",
          "form": "해외직구로 구매했어요!",
          "original_form": "해외직구로 구매했어요!",
          "speaker_id": "1",
          "time": "20210824 14:35"
        },
        {
          "id": "MDRW2100011436.1.6",
          "form": "해외직구면 어느 사이트 이용하셨나요?",
          "original_form": "해외직구면 어느 사이트 이용하셨나요?",
          "speaker_id": "2",
          "time": "20210824 14:36"
        },
        {
          "id": "MDRW2100011436.1.7",
          "form": "매치스패션이라는 사이트요!",
          "original_form": "매치스패션이라는 사이트요!",
          "speaker_id": "1",
          "time": "20210824 14:37"
        },
        {
          "id": "MDRW2100011436.1.8",
          "form": "아 저에겐 생소하네요! 배송은 빠른 편인가요?",
          "original_form": "아 저에겐 생소하네요! 배송은 빠른 편인가요?",
          "speaker_id": "2",
          "time": "20210824 14:38"
        },
        {
          "id": "MDRW2100011436.1.9",
          "form": "넵 빠르더라구요 최근에 구매하신거 있나요",
          "original_form": "넵 빠르더라구요 최근에 구매하신거 있나요",
          "speaker_id": "1",
          "time": "20210824 14:39"
        },
        {
          "id": "MDRW2100011436.1.10",
          "form": "네 저는 가족들 영양제를 구입했어요",
          "original_form": "네 저는 가족들 영양제를 구입했어요",
          "speaker_id": "2",
          "time": "20210824 14:40"
        },
        {
          "id": "MDRW2100011436.1.11",
          "form": "오 영양제 어떤거 드시나요",
          "original_form": "오 영양제 어떤거 드시나요",
          "speaker_id": "1",
          "time": "20210824 14:41"
        },
        {
          "id": "MDRW2100011436.1.12",
          "form": "아이들 오메가3오ㅓ 유산균을 샀오요",
          "original_form": "아이들 오메가3오ㅓ 유산균을 샀오요",
          "speaker_id": "2",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100011436.1.13",
          "form": "오 유산균 꾸준히 먹으니까 진짜 좋은것같아요",
          "original_form": "오 유산균 꾸준히 먹으니까 진짜 좋은것같아요",
          "speaker_id": "1",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011436.1.14",
          "form": "어떤제품으로 구입하시나요,",
          "original_form": "어떤제품으로 구입하시나요,",
          "speaker_id": "2",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011436.1.15",
          "form": "저는 락토핏 먹고있어요",
          "original_form": "저는 락토핏 먹고있어요",
