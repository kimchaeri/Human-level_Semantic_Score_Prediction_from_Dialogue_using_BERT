      "utterance": [
        {
          "id": "MDRW2100042953.1.1",
          "form": "안녕하세요 name2님!",
          "original_form": "안녕하세요 &name2&님!",
          "speaker_id": "1",
          "time": "20210907 11:59"
        },
        {
          "id": "MDRW2100042953.1.2",
          "form": "안녕하세요! 여행 좋아하세요?",
          "original_form": "안녕하세요! 여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 12:00"
        },
        {
          "id": "MDRW2100042953.1.3",
          "form": "저 랜선 여행 좋아해요 ㅋㅋㅋ 실제로 가 본 적이 없어서 좋다고 말을 못하겠어요ㅠ",
          "original_form": "저 랜선 여행 좋아해요 ㅋㅋㅋ 실제로 가 본 적이 없어서 좋다고 말을 못하겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210907 12:01"
        },
        {
          "id": "MDRW2100042953.1.4",
          "form": "가고는 싶어요!",
          "original_form": "가고는 싶어요!",
          "speaker_id": "1",
          "time": "20210907 12:01"
        },
        {
          "id": "MDRW2100042953.1.5",
          "form": "여행이 해외만 있는데 아니니까요 ㅋㅋㅋ 국내여행도 있잖아요!",
          "original_form": "여행이 해외만 있는데 아니니까요 ㅋㅋㅋ 국내여행도 있잖아요!",
          "speaker_id": "2",
          "time": "20210907 12:03"
        },
        {
          "id": "MDRW2100042953.1.6",
          "form": "해외는 어디가보고 싶으세요?",
          "original_form": "해외는 어디가보고 싶으세요?",
          "speaker_id": "2",
          "time": "20210907 12:03"
        },
        {
          "id": "MDRW2100042953.1.7",
          "form": "국내도 안가봤어요.......ㅠㅁㅠ",
          "original_form": "국내도 안가봤어요.......ㅠㅁㅠ",
          "speaker_id": "1",
          "time": "20210907 12:03"
        },
        {
          "id": "MDRW2100042953.1.8",
          "form": "스위스 가보고 싶어요",
          "original_form": "스위스 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210907 12:04"
        },
        {
          "id": "MDRW2100042953.1.9",
          "form": "앗 그럼 이제 코시국 끝나고 같 계획으로 짜보시는것도 좋죠!!",
          "original_form": "앗 그럼 이제 코시국 끝나고 같 계획으로 짜보시는것도 좋죠!!",
          "speaker_id": "2",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100042953.1.10",
          "form": "저도 유럽 너무 가보고싶어요 ㅋㅋㅋㅋㅋ 경제적문제때문에 아시아만 깔짝거렸네요 ㅠ",
          "original_form": "저도 유럽 너무 가보고싶어요 ㅋㅋㅋㅋㅋ 경제적문제때문에 아시아만 깔짝거렸네요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100042953.1.11",
          "form": "아시아 어디가보셨어요? 재밌으셨겠죠?",
          "original_form": "아시아 어디가보셨어요? 재밌으셨겠죠?",
          "speaker_id": "1",
          "time": "20210907 12:50"
        },
        {
          "id": "MDRW2100042953.1.12",
          "form": "저.. 학교에서 교류? 뭐 이런걸로 간건 필리핀이고..",
          "original_form": "저.. 학교에서 교류? 뭐 이런걸로 간건 필리핀이고..",
          "speaker_id": "2",
          "time": "20210907 12:51"
        },
        {
          "id": "MDRW2100042953.1.13",
          "form": "제돈주고 간건 일본이랑 상하이요!!",
          "original_form": "제돈주고 간건 일본이랑 상하이요!!",
          "speaker_id": "2",
          "time": "20210907 12:51"
        },
        {
          "id": "MDRW2100042953.1.14",
          "form": "재밌었어요 ㅠㅠㅠㅠㅠ",
          "original_form": "재밌었어요 ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 12:51"
        },
        {
          "id": "MDRW2100042953.1.15",
          "form": "우와 교환학생 그런거요?",
          "original_form": "우와 교환학생 그런거요?",
          "speaker_id": "1",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100042953.1.16",
          "form": "아 맞다 대만도 갔었거든요",
          "original_form": "아 맞다 대만도 갔었거든요",
          "speaker_id": "2",
          "time": "20210907 12:59"
        },
        {
          "id": "MDRW2100042953.1.17",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "1",
          "time": "20210907 12:59"
        },
        {
          "id": "MDRW2100042953.1.18",
          "form": "대만 좋아요 .... 나중에 기회되면 대만가보세요",
          "original_form": "대만 좋아요 .... 나중에 기회되면 대만가보세요",
          "speaker_id": "2",
          "time": "20210907 12:59"
        },
        {
          "id": "MDRW2100042953.1.19",
          "form": "대만 좋다고 많이 들었어요",
          "original_form": "대만 좋다고 많이 들었어요",
