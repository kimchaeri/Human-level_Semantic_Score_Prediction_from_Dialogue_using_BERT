      "utterance": [
        {
          "id": "MDRW2100041030.1.1",
          "form": "name2님 혹시",
          "original_form": "&name2&님 혹시",
          "speaker_id": "1",
          "time": "20210905 16:01"
        },
        {
          "id": "MDRW2100041030.1.2",
          "form": "<판의 미로> 라는 영화 보셨나요?!",
          "original_form": "<판의 미로> 라는 영화 보셨나요?!",
          "speaker_id": "1",
          "time": "20210905 16:01"
        },
        {
          "id": "MDRW2100041030.1.3",
          "form": "앗 네!!!!! 순수한 영화인줄 알고 봤다가",
          "original_form": "앗 네!!!!! 순수한 영화인줄 알고 봤다가",
          "speaker_id": "2",
          "time": "20210905 16:02"
        },
        {
          "id": "MDRW2100041030.1.4",
          "form": "뒷통수 제대로 맞었죠",
          "original_form": "뒷통수 제대로 맞었죠",
          "speaker_id": "2",
          "time": "20210905 16:02"
        },
        {
          "id": "MDRW2100041030.1.5",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "1",
          "time": "20210905 16:02"
        },
        {
          "id": "MDRW2100041030.1.6",
          "form": "역시 아시는군요!!!",
          "original_form": "역시 아시는군요!!!",
          "speaker_id": "1",
          "time": "20210905 16:02"
        },
        {
          "id": "MDRW2100041030.1.7",
          "form": "진도리님 정말 영상 콘텐츠를 다 아우르고 계시네요",
          "original_form": "진도리님 정말 영상 콘텐츠를 다 아우르고 계시네요",
          "speaker_id": "1",
          "time": "20210905 16:02"
        },
        {
          "id": "MDRW2100041030.1.8",
          "form": "앗 네 ㅋㅋㅋㅋㅋㅋㅋㅋ !!!!!!!!",
          "original_form": "앗 네 ㅋㅋㅋㅋㅋㅋㅋㅋ !!!!!!!!",
          "speaker_id": "2",
          "time": "20210905 16:03"
        },
        {
          "id": "MDRW2100041030.1.9",
          "form": "name1님도 보셨어요?!",
          "original_form": "&name1&님도 보셨어요?!",
          "speaker_id": "2",
          "time": "20210905 16:03"
        },
        {
          "id": "MDRW2100041030.1.10",
          "form": "네네네 얼마 전에 봤는데",
          "original_form": "네네네 얼마 전에 봤는데",
          "speaker_id": "1",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100041030.1.11",
          "form": "사실 감명깊게 봤어요",
          "original_form": "사실 감명깊게 봤어요",
          "speaker_id": "1",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100041030.1.12",
          "form": "그리고 마지막에 눈물이 나더라구요...",
          "original_form": "그리고 마지막에 눈물이 나더라구요...",
          "speaker_id": "1",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100041030.1.13",
          "form": "맞아요 ㅠ.ㅠ 그 주인공 죽을 때....",
          "original_form": "맞아요 ㅠ.ㅠ 그 주인공 죽을 때....",
          "speaker_id": "2",
          "time": "20210905 16:06"
        },
        {
          "id": "MDRW2100041030.1.14",
          "form": "그 동생 안 죽이려는 장면에서 ㅠㅠ",
          "original_form": "그 동생 안 죽이려는 장면에서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 16:06"
        },
        {
          "id": "MDRW2100041030.1.15",
          "form": "너무 슬펐어요 ㅠㅠ 주인공도 넘 불쌍하고...",
          "original_form": "너무 슬펐어요 ㅠㅠ 주인공도 넘 불쌍하고...",
          "speaker_id": "2",
          "time": "20210905 16:06"
        },
        {
          "id": "MDRW2100041030.1.16",
          "form": "아 맞아요 진짜",
          "original_form": "아 맞아요 진짜",
          "speaker_id": "1",
          "time": "20210905 16:13"
        },
        {
          "id": "MDRW2100041030.1.17",
          "form": "상상하는 장면도 그렇고ㅠ",
          "original_form": "상상하는 장면도 그렇고ㅠ",
          "speaker_id": "1",
          "time": "20210905 16:14"
        },
        {
          "id": "MDRW2100041030.1.18",
          "form": "이거 여러가지 해석이 있던데",
          "original_form": "이거 여러가지 해석이 있던데",
          "speaker_id": "1",
          "time": "20210905 16:14"
        },
        {
          "id": "MDRW2100041030.1.19",
          "form": "정말 모든 게 다 상상이었던걸까요ㅠ",
          "original_form": "정말 모든 게 다 상상이었던걸까요ㅠ",
          "speaker_id": "1",
          "time": "20210905 16:14"
        },
        {
          "id": "MDRW2100041030.1.20",
          "form": "전 다 상상이었던 것 같아요 ㅠ.^",
          "original_form": "전 다 상상이었던 것 같아요 ㅠ.^",
          "speaker_id": "2",
          "time": "20210905 16:16"
        },
        {
          "id": "MDRW2100041030.1.21",
          "form": "슬픈 결말의 해석이지만..",
          "original_form": "슬픈 결말의 해석이지만..",
          "speaker_id": "2",
          "time": "20210905 16:16"
        },
        {
          "id": "MDRW2100041030.1.22",
          "form": "아이고ㅠ",
          "original_form": "아이고ㅠ",
          "speaker_id": "1",
          "time": "20210905 16:16"
        },
        {
          "id": "MDRW2100041030.1.23",
          "form": "외로웠던 주인공의 상상같아요ㅠㅠ",
          "original_form": "외로웠던 주인공의 상상같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 16:16"
        },
        {
          "id": "MDRW2100041030.1.24",
          "form": "에휴 정말",
          "original_form": "에휴 정말",
          "speaker_id": "1",
          "time": "20210905 16:16"
        },
        {
          "id": "MDRW2100041030.1.25",
          "form": "감독이 너무 잔인하게",
          "original_form": "감독이 너무 잔인하게",
          "speaker_id": "1",
          "time": "20210905 16:16"
        },
        {
          "id": "MDRW2100041030.1.26",
          "form": "영화를 잘 만들었어요",
          "original_form": "영화를 잘 만들었어요",
          "speaker_id": "1",
          "time": "20210905 16:16"
        },
        {
          "id": "MDRW2100041030.1.27",
          "form": "그니까요 ㅠ ㅠ !",
          "original_form": "그니까요 ㅠ ㅠ !",
          "speaker_id": "2",
          "time": "20210905 16:17"
        },
        {
          "id": "MDRW2100041030.1.28",
          "form": "name1님 그럼 약간 좀 무서운 분위기의ㅡ영화?",
          "original_form": "&name1&님 그럼 약간 좀 무서운 분위기의ㅡ영화?",
          "speaker_id": "2",
          "time": "20210905 16:17"
        },
        {
          "id": "MDRW2100041030.1.29",
          "form": "도 잘 보시나요",
          "original_form": "도 잘 보시나요",
          "speaker_id": "2",
          "time": "20210905 16:17"
        },
        {
          "id": "MDRW2100041030.1.30",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100041030.1.31",
          "form": "사실 좋아해요",
          "original_form": "사실 좋아해요",
          "speaker_id": "1",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100041030.1.32",
          "form": "판의 미로도 사실...",
          "original_form": "판의 미로도 사실...",
          "speaker_id": "1",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100041030.1.33",
          "form": "엄청난 괴물들이 나온다고 해서",
          "original_form": "엄청난 괴물들이 나온다고 해서",
          "speaker_id": "1",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100041030.1.34",
          "form": "봤어요 ㅋㅋㅋㅋㅋㅌ 특이하죠?@",
          "original_form": "봤어요 ㅋㅋㅋㅋㅋㅌ 특이하죠?@",
          "speaker_id": "1",
          "time": "20210905 16:18"
        },
        {
          "id": "MDRW2100041030.1.35",
          "form": "헐 저도 그런 판타지? 좋아해요!",
          "original_form": "헐 저도 그런 판타지? 좋아해요!",
          "speaker_id": "2",
          "time": "20210905 16:19"
        },
        {
          "id": "MDRW2100041030.1.36",
          "form": "또 어떤 영화들 재밌게 보샸어요! 그런류의!",
          "original_form": "또 어떤 영화들 재밌게 보샸어요! 그런류의!",
          "speaker_id": "2",
          "time": "20210905 16:19"
        },
        {
          "id": "MDRW2100041030.1.37",
          "form": "미드소마요~~~!!!",
          "original_form": "미드소마요~~~!!!",
