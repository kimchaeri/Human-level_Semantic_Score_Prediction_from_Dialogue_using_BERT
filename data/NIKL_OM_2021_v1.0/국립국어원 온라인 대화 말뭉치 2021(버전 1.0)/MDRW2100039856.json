      "utterance": [
        {
          "id": "MDRW2100039856.1.1",
          "form": "이제 사실 연애도 지겨워요",
          "original_form": "이제 사실 연애도 지겨워요",
          "speaker_id": "2",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039856.1.2",
          "form": "나근데오늘좀",
          "original_form": "나근데오늘좀",
          "speaker_id": "1",
          "time": "20210903 22:07"
        },
        {
          "id": "MDRW2100039856.1.3",
          "form": "부러운거많이봤어요",
          "original_form": "부러운거많이봤어요",
          "speaker_id": "1",
          "time": "20210903 22:08"
        },
        {
          "id": "MDRW2100039856.1.4",
          "form": "오오 어떤거요?",
          "original_form": "오오 어떤거요?",
          "speaker_id": "2",
          "time": "20210903 22:09"
        },
        {
          "id": "MDRW2100039856.1.5",
          "form": "저는절뚝거리면서버스내리는듸ㅣ",
          "original_form": "저는절뚝거리면서버스내리는듸ㅣ",
          "speaker_id": "1",
          "time": "20210903 22:10"
        },
        {
          "id": "MDRW2100039856.1.6",
          "form": "어어",
          "original_form": "어어",
          "speaker_id": "2",
          "time": "20210903 22:10"
        },
        {
          "id": "MDRW2100039856.1.7",
          "form": "제옆에분이막급하게내리더니",
          "original_form": "제옆에분이막급하게내리더니",
          "speaker_id": "1",
          "time": "20210903 22:10"
        },
        {
          "id": "MDRW2100039856.1.8",
          "form": "버정에서있던여친이랑막포옹하고난리가낫져여",
          "original_form": "버정에서있던여친이랑막포옹하고난리가낫져여",
          "speaker_id": "1",
          "time": "20210903 22:10"
        },
        {
          "id": "MDRW2100039856.1.9",
          "form": "잉",
          "original_form": "잉",
          "speaker_id": "2",
          "time": "20210903 22:10"
        },
        {
          "id": "MDRW2100039856.1.10",
          "form": "난리네.ㅈ",
          "original_form": "난리네.ㅈ",
          "speaker_id": "1",
          "time": "20210903 22:10"
        },
        {
          "id": "MDRW2100039856.1.11",
          "form": "부럽던가요",
          "original_form": "부럽던가요",
          "speaker_id": "2",
          "time": "20210903 22:10"
        },
        {
          "id": "MDRW2100039856.1.12",
          "form": "제가 담에만나면 안아드릴게요ㅠㅠㅠ",
          "original_form": "제가 담에만나면 안아드릴게요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 22:10"
        },
        {
          "id": "MDRW2100039856.1.13",
          "form": "악ㄱㅈㅋㅈㄱ",
          "original_form": "악ㄱㅈㅋㅈㄱ",
          "speaker_id": "1",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039856.1.14",
          "form": "그건그냥",
          "original_form": "그건그냥",
          "speaker_id": "1",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039856.1.15",
          "form": "좀그랫는데",
          "original_form": "좀그랫는데",
          "speaker_id": "1",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039856.1.16",
          "form": "가방들어주는거부러웠어요",
          "original_form": "가방들어주는거부러웠어요",
          "speaker_id": "1",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039856.1.17",
          "form": "난발이아픈데",
          "original_form": "난발이아픈데",
          "speaker_id": "1",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039856.1.18",
          "form": "ㄱㅋㅈㄱㅈㄱㅈㄱㅈㅋㅈㄱㄷㄱ",
          "original_form": "ㄱㅋㅈㄱㅈㄱㅈㄱㅈㅋㅈㄱㄷㄱ",
          "speaker_id": "1",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039856.1.19",
          "form": "아ㅠㅠ그쵸",
          "original_form": "아ㅠㅠ그쵸",
          "speaker_id": "2",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039856.1.20",
          "form": "저도 누가 데리러오면 좋겠어요",
          "original_form": "저도 누가 데리러오면 좋겠어요",
          "speaker_id": "2",
          "time": "20210903 22:11"
        },
        {
          "id": "MDRW2100039856.1.21",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039856.1.22",
          "form": "그러고또짐이더늘엇는데",
          "original_form": "그러고또짐이더늘엇는데",
          "speaker_id": "1",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039856.1.23",
          "form": "자꾸내앞에",
          "original_form": "자꾸내앞에",
          "speaker_id": "1",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039856.1.24",
          "form": "오늘따라",
          "original_form": "오늘따라",
          "speaker_id": "1",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039856.1.25",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039856.1.26",
          "form": "커플들이",
          "original_form": "커플들이",
          "speaker_id": "1",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039856.1.27",
          "form": "연인들이...",
          "original_form": "연인들이...",
          "speaker_id": "2",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039856.1.28",
          "form": "손잡고행복하게가는거예요..",
          "original_form": "손잡고행복하게가는거예요..",
          "speaker_id": "1",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039856.1.29",
          "form": "그니까요 그런거 부러워요",
          "original_form": "그니까요 그런거 부러워요",
          "speaker_id": "2",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039856.1.30",
          "form": "가벼운차림으로",
          "original_form": "가벼운차림으로",
          "speaker_id": "1",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039856.1.31",
          "form": "난 맨날 혼자가요 ...",
          "original_form": "난 맨날 혼자가요 ...",
          "speaker_id": "2",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039856.1.32",
          "form": "네 그래서부러웠어요..",
          "original_form": "네 그래서부러웠어요..",
          "speaker_id": "1",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039856.1.33",
          "form": "해원이도 남친이 가방들어주고",
          "original_form": "해원이도 남친이 가방들어주고",
          "speaker_id": "2",
          "time": "20210903 22:12"
        },
        {
          "id": "MDRW2100039856.1.34",
          "form": "발 삐면 업어주고",
          "original_form": "발 삐면 업어주고",
          "speaker_id": "2",
          "time": "20210903 22:13"
        },
        {
          "id": "MDRW2100039856.1.35",
          "form": "ㄱㅈㅋㅈㄱㅈㄱㅈㅋ",
          "original_form": "ㄱㅈㅋㅈㄱㅈㄱㅈㅋ",
          "speaker_id": "1",
          "time": "20210903 22:13"
        },
        {
          "id": "MDRW2100039856.1.36",
          "form": "만들어볼 생각 없나요",
          "original_form": "만들어볼 생각 없나요",
          "speaker_id": "2",
          "time": "20210903 22:13"
        },
        {
          "id": "MDRW2100039856.1.37",
          "form": "업..어..?",
          "original_form": "업..어..?",
          "speaker_id": "1",
          "time": "20210903 22:13"
        },
        {
          "id": "MDRW2100039856.1.38",
          "form": "업어주라허세요",
          "original_form": "업어주라허세요",
          "speaker_id": "2",
          "time": "20210903 22:13"
        }
      ]
    }
