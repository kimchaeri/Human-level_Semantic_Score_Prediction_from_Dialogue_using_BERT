        },
        {
          "id": "MDRW2100028978.1.2",
          "form": "앗 안녕하세오 name1",
          "original_form": "앗 안녕하세오 &name1&",
          "speaker_id": "2",
          "time": "20210903 17:07"
        },
        {
          "id": "MDRW2100028978.1.3",
          "form": "음료 뭐 좋아하시나오?",
          "original_form": "음료 뭐 좋아하시나오?",
          "speaker_id": "2",
          "time": "20210903 17:08"
        },
        {
          "id": "MDRW2100028978.1.4",
          "form": "음료 요즘에는 제로칼로리 탄산 좋아합니다",
          "original_form": "음료 요즘에는 제로칼로리 탄산 좋아합니다",
          "speaker_id": "1",
          "time": "20210903 17:09"
        },
        {
          "id": "MDRW2100028978.1.5",
          "form": "님은 뭐 좋아 하시나오?",
          "original_form": "님은 뭐 좋아 하시나오?",
          "speaker_id": "1",
          "time": "20210903 17:10"
        },
        {
          "id": "MDRW2100028978.1.6",
          "form": "오오 저는 트로피카나 패션후르츠맛 탄산임미다",
          "original_form": "오오 저는 트로피카나 패션후르츠맛 탄산임미다",
          "speaker_id": "2",
          "time": "20210903 17:10"
        },
        {
          "id": "MDRW2100028978.1.7",
          "form": "제로칼로리 탄산이면",
          "original_form": "제로칼로리 탄산이면",
          "speaker_id": "2",
          "time": "20210903 17:10"
        },
        {
          "id": "MDRW2100028978.1.8",
          "form": "펩시 라임맛이요??",
          "original_form": "펩시 라임맛이요??",
          "speaker_id": "2",
          "time": "20210903 17:10"
        },
        {
          "id": "MDRW2100028978.1.9",
          "form": "네넹 펩시 라임 요즘 좋아합니다",
          "original_form": "네넹 펩시 라임 요즘 좋아합니다",
          "speaker_id": "1",
          "time": "20210903 17:10"
        },
        {
          "id": "MDRW2100028978.1.10",
          "form": "맛있더라구여",
          "original_form": "맛있더라구여",
          "speaker_id": "1",
          "time": "20210903 17:11"
        },
        {
          "id": "MDRW2100028978.1.11",
          "form": "아 님 맛잘알 이시군요",
          "original_form": "아 님 맛잘알 이시군요",
          "speaker_id": "2",
          "time": "20210903 17:11"
        },
        {
          "id": "MDRW2100028978.1.12",
          "form": "맛있어요 ㅈ2222",
          "original_form": "맛있어요 ㅈ2222",
          "speaker_id": "2",
          "time": "20210903 17:11"
        },
        {
          "id": "MDRW2100028978.1.13",
          "form": "원쁠원을 자주해서 잘 먹곤 합니다",
          "original_form": "원쁠원을 자주해서 잘 먹곤 합니다",
          "speaker_id": "1",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100028978.1.14",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100028978.1.15",
          "form": "꿀입니다",
          "original_form": "꿀입니다",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100028978.1.16",
          "form": "나랑드도 드시나요?",
          "original_form": "나랑드도 드시나요?",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100028978.1.17",
          "form": "트로피카나 패션후르츠 맛있나오",
          "original_form": "트로피카나 패션후르츠 맛있나오",
          "speaker_id": "1",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100028978.1.18",
          "form": "네 맛나요",
          "original_form": "네 맛나요",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100028978.1.19",
          "form": "ㄹㅇ 패션후르츠 맛이 납니다",
          "original_form": "ㄹㅇ 패션후르츠 맛이 납니다",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100028978.1.20",
          "form": "나랑드 제로 좋아합니댜",
          "original_form": "나랑드 제로 좋아합니댜",
          "speaker_id": "1",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100028978.1.21",
          "form": "제로칼로리는 아니지만",
          "original_form": "제로칼로리는 아니지만",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100028978.1.22",
          "form": "나랑드 깔끔해오",
          "original_form": "나랑드 깔끔해오",
          "speaker_id": "2",
          "time": "20210903 17:12"
        },
        {
          "id": "MDRW2100028978.1.23",
          "form": "노브랜드 탄산도 좋아하나오?",
          "original_form": "노브랜드 탄산도 좋아하나오?",
          "speaker_id": "2",
          "time": "20210903 17:13"
        },
        {
          "id": "MDRW2100028978.1.24",
          "form": "네넹 좋아합니다 그 노란색 말고 초록색 좋아해오",
          "original_form": "네넹 좋아합니다 그 노란색 말고 초록색 좋아해오",
          "speaker_id": "1",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100028978.1.25",
          "form": "어떤색 좋아하나요?",
          "original_form": "어떤색 좋아하나요?",
          "speaker_id": "1",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100028978.1.26",
          "form": "라임???",
          "original_form": "라임???",
          "speaker_id": "2",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100028978.1.27",
          "form": "저는 오렌지요",
          "original_form": "저는 오렌지요",
          "speaker_id": "2",
          "time": "20210903 17:14"
        },
        {
          "id": "MDRW2100028978.1.28",
          "form": "요새 샤인머스캣 맛도 나왔던데",
          "original_form": "요새 샤인머스캣 맛도 나왔던데",
          "speaker_id": "2",
          "time": "20210903 17:14"
        }
      ]
