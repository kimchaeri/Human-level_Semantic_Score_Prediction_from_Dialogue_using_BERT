      "utterance": [
        {
          "id": "MDRW2100035395.1.1",
          "form": "최근 보신 영화가 있으신가요?",
          "original_form": "최근 보신 영화가 있으신가요?",
          "speaker_id": "2",
          "time": "20210829 16:16"
        },
        {
          "id": "MDRW2100035395.1.2",
          "form": "아니요 최근엔 시간이없어서ㅜ",
          "original_form": "아니요 최근엔 시간이없어서ㅜ",
          "speaker_id": "1",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100035395.1.3",
          "form": "티비로 해리포터잠깐봤네요ㅋㅋ",
          "original_form": "티비로 해리포터잠깐봤네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100035395.1.4",
          "form": "name2님은요",
          "original_form": "&name2&님은요",
          "speaker_id": "1",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100035395.1.5",
          "form": "저는 크루엘라요ㅎㅎ",
          "original_form": "저는 크루엘라요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100035395.1.6",
          "form": "오 그거 재밌다던데",
          "original_form": "오 그거 재밌다던데",
          "speaker_id": "1",
          "time": "20210829 18:04"
        },
        {
          "id": "MDRW2100035395.1.7",
          "form": "추천합니당ㅎㅎ",
          "original_form": "추천합니당ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:25"
        },
        {
          "id": "MDRW2100035395.1.8",
          "form": "나중에 꼭봐야겠어요",
          "original_form": "나중에 꼭봐야겠어요",
          "speaker_id": "1",
          "time": "20210829 18:29"
        },
        {
          "id": "MDRW2100035395.1.9",
          "form": "넷플 보시마요",
          "original_form": "넷플 보시마요",
          "speaker_id": "1",
          "time": "20210829 18:29"
        },
        {
          "id": "MDRW2100035395.1.10",
          "form": "나",
          "original_form": "나",
          "speaker_id": "1",
          "time": "20210829 18:29"
        },
        {
          "id": "MDRW2100035395.1.11",
          "form": "네네! 넷플 자주 들어가요",
          "original_form": "네네! 넷플 자주 들어가요",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100035395.1.12",
          "form": "요즘은 뭐보세요?",
          "original_form": "요즘은 뭐보세요?",
          "speaker_id": "1",
          "time": "20210829 18:58"
        },
        {
          "id": "MDRW2100035395.1.13",
          "form": "슬의생 보고 있어요ㅎㅎ",
          "original_form": "슬의생 보고 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 18:59"
        },
        {
          "id": "MDRW2100035395.1.14",
          "form": "해외드라마도 보시나요",
          "original_form": "해외드라마도 보시나요",
          "speaker_id": "1",
          "time": "20210829 19:02"
        },
        {
          "id": "MDRW2100035395.1.15",
          "form": "해외드라마는 아직이요",
          "original_form": "해외드라마는 아직이요",
          "speaker_id": "2",
          "time": "20210829 19:16"
        },
        {
          "id": "MDRW2100035395.1.16",
          "form": "무슨장르조아하세요",
          "original_form": "무슨장르조아하세요",
          "speaker_id": "1",
          "time": "20210829 19:18"
        },
        {
          "id": "MDRW2100035395.1.17",
          "form": "주로 코미디 많이 봐여ㅋㅋ",
