      "utterance": [
        {
          "id": "MDRW2100024372.1.1",
          "form": "카오스냥이는 어떤건가요?",
          "original_form": "카오스냥이는 어떤건가요?",
          "speaker_id": "2",
          "time": "20210831 02:44"
        },
        {
          "id": "MDRW2100024372.1.2",
          "form": "그거 랜덤이라고도 들엇어요",
          "original_form": "그거 랜덤이라고도 들엇어요",
          "speaker_id": "1",
          "time": "20210831 02:44"
        },
        {
          "id": "MDRW2100024372.1.3",
          "form": "얼굴의 뱐이 검은냥이, 반이 치즈냥이엿어요",
          "original_form": "얼굴의 뱐이 검은냥이, 반이 치즈냥이엿어요",
          "speaker_id": "1",
          "time": "20210831 02:45"
        },
        {
          "id": "MDRW2100024372.1.4",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210831 02:45"
        },
        {
          "id": "MDRW2100024372.1.5",
          "form": "우와 매력있겠어요",
          "original_form": "우와 매력있겠어요",
          "speaker_id": "2",
          "time": "20210831 02:45"
        },
        {
          "id": "MDRW2100024372.1.6",
          "form": "목쪽에 흰색 냅킨? 손수건? 그런거잇고요",
          "original_form": "목쪽에 흰색 냅킨? 손수건? 그런거잇고요",
          "speaker_id": "1",
          "time": "20210831 02:45"
        },
        {
          "id": "MDRW2100024372.1.7",
          "form": "짱귀여워요...\r\n근데 사람들이 자꾸 사람먹는음식을줘서 ㅜㅜ",
          "original_form": "짱귀여워요...\r\n근데 사람들이 자꾸 사람먹는음식을줘서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 02:45"
        },
        {
          "id": "MDRW2100024372.1.8",
          "form": "아이구ㅠㅠ",
          "original_form": "아이구ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 02:45"
        },
        {
          "id": "MDRW2100024372.1.9",
          "form": "가끔 보면 부어잇더라고요",
          "original_form": "가끔 보면 부어잇더라고요",
          "speaker_id": "1",
          "time": "20210831 02:46"
        },
        {
          "id": "MDRW2100024372.1.10",
          "form": "그럼 안 되는데ㅠㅠ",
          "original_form": "그럼 안 되는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 02:46"
        },
        {
          "id": "MDRW2100024372.1.11",
          "form": "ㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 02:46"
        },
        {
          "id": "MDRW2100024372.1.12",
          "form": "맞아요 길냥이들 다",
          "original_form": "맞아요 길냥이들 다",
          "speaker_id": "2",
          "time": "20210831 02:46"
        },
        {
          "id": "MDRW2100024372.1.13",
          "form": "통통해보이는 이유가 잘먹어서가 아니라 나트륨 때문이라구....",
          "original_form": "통통해보이는 이유가 잘먹어서가 아니라 나트륨 때문이라구....",
          "speaker_id": "2",
          "time": "20210831 02:46"
        },
        {
          "id": "MDRW2100024372.1.14",
          "form": "그 가끔 강아지먹이는 어쩔수없다 이러면서 주듯..",
          "original_form": "그 가끔 강아지먹이는 어쩔수없다 이러면서 주듯..",
          "speaker_id": "1",
          "time": "20210831 02:46"
        },
        {
          "id": "MDRW2100024372.1.15",
          "form": "인간이먹능것도 주고",
          "original_form": "인간이먹능것도 주고",
          "speaker_id": "1",
          "time": "20210831 02:46"
        },
        {
          "id": "MDRW2100024372.1.16",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 02:46"
        },
        {
          "id": "MDRW2100024372.1.17",
