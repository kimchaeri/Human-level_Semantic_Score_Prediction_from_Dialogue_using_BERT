      "utterance": [
        {
          "id": "MDRW2100003721.1.1",
          "form": "주로 옷 쇼핑 어디서 하시는 편이세요?",
          "original_form": "주로 옷 쇼핑 어디서 하시는 편이세요?",
          "speaker_id": "1",
          "time": "20210821 11:10"
        },
        {
          "id": "MDRW2100003721.1.2",
          "form": "요즘에는 옷 쇼핑 어디서 하세요?",
          "original_form": "요즘에는 옷 쇼핑 어디서 하세요?",
          "speaker_id": "1",
          "time": "20210821 11:13"
        },
        {
          "id": "MDRW2100003721.1.3",
          "form": "저는 백화점 자주가요 체형이 좀 이상해서 기성품이 안맞거든요ㅠ",
          "original_form": "저는 백화점 자주가요 체형이 좀 이상해서 기성품이 안맞거든요ㅠ",
          "speaker_id": "2",
          "time": "20210821 11:13"
        },
        {
          "id": "MDRW2100003721.1.4",
          "form": "저는 핸드폰 어플로 많이 쇼핑하는 것 같아요",
          "original_form": "저는 핸드폰 어플로 많이 쇼핑하는 것 같아요",
          "speaker_id": "1",
          "time": "20210821 11:14"
        },
        {
          "id": "MDRW2100003721.1.5",
          "form": "기성품 사이즈가 안맞으시는 구나",
          "original_form": "기성품 사이즈가 안맞으시는 구나",
          "speaker_id": "1",
          "time": "20210821 11:14"
        },
        {
          "id": "MDRW2100003721.1.6",
          "form": "백화점은 좀 다른가요?",
          "original_form": "백화점은 좀 다른가요?",
          "speaker_id": "1",
          "time": "20210821 11:14"
        },
        {
          "id": "MDRW2100003721.1.7",
          "form": "백화점은 아무래도 수선이 가능하니까요ㅋㅋ!!",
          "original_form": "백화점은 아무래도 수선이 가능하니까요ㅋㅋ!!",
          "speaker_id": "2",
          "time": "20210821 11:15"
        },
        {
          "id": "MDRW2100003721.1.8",
          "form": "그리고 사이즈도 다양하구요",
          "original_form": "그리고 사이즈도 다양하구요",
          "speaker_id": "2",
          "time": "20210821 11:15"
        },
        {
          "id": "MDRW2100003721.1.9",
          "form": "아 그렇구나 백화점을 안가서 몰랐네요 ㅎㅎ",
          "original_form": "아 그렇구나 백화점을 안가서 몰랐네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:15"
        },
        {
          "id": "MDRW2100003721.1.10",
          "form": "요즘은 백화점 어플 잘돼있어서",
          "original_form": "요즘은 백화점 어플 잘돼있어서",
          "speaker_id": "2",
          "time": "20210821 11:15"
        },
        {
          "id": "MDRW2100003721.1.11",
          "form": "집에서 쇼핑 가능해요ㅋㅋ",
          "original_form": "집에서 쇼핑 가능해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 11:15"
        },
        {
          "id": "MDRW2100003721.1.12",
          "form": "최근에 뭐 사셨어요?",
          "original_form": "최근에 뭐 사셨어요?",
          "speaker_id": "2",
          "time": "20210821 11:15"
        },
        {
          "id": "MDRW2100003721.1.13",
          "form": "저는 기본 흰 반팔티 샀어요 ㅎㅎ",
          "original_form": "저는 기본 흰 반팔티 샀어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:16"
        },
        {
          "id": "MDRW2100003721.1.14",
          "form": "아 집에서 쇼핑해도 수선이 되나용?",
          "original_form": "아 집에서 쇼핑해도 수선이 되나용?",
          "speaker_id": "1",
          "time": "20210821 11:16"
        },
        {
          "id": "MDRW2100003721.1.15",
          "form": "네! 근처 백화점 가면 다 해줘요",
          "original_form": "네! 근처 백화점 가면 다 해줘요",
          "speaker_id": "2",
          "time": "20210821 11:19"
        },
        {
          "id": "MDRW2100003721.1.16",
          "form": "아 그렇군요 신기하네요",
          "original_form": "아 그렇군요 신기하네요",
          "speaker_id": "1",
          "time": "20210821 11:19"
        },
        {
          "id": "MDRW2100003721.1.17",
          "form": "브랜드수선이다보니 그런가봐요",
          "original_form": "브랜드수선이다보니 그런가봐요",
          "speaker_id": "2",
          "time": "20210821 11:19"
        },
        {
          "id": "MDRW2100003721.1.18",
          "form": "역시 옷은 브랜드 인가봐요 ㅎㅎ",
          "original_form": "역시 옷은 브랜드 인가봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 11:19"
        },
        {
          "id": "MDRW2100003721.1.19",
          "form": "아 기본티 사야하는데",
          "original_form": "아 기본티 사야하는데",
          "speaker_id": "2",
          "time": "20210821 11:19"
        },
        {
          "id": "MDRW2100003721.1.20",
          "form": "생각난김에 사야겠다",
          "original_form": "생각난김에 사야겠다",
