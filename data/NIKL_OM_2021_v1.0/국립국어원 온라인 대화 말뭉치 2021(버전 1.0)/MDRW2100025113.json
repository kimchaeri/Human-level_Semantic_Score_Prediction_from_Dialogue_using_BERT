      "utterance": [
        {
          "id": "MDRW2100025113.1.1",
          "form": "맞아여....",
          "original_form": "맞아여....",
          "speaker_id": "1",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100025113.1.2",
          "form": "그래서 전에 같이 살 때는 제가",
          "original_form": "그래서 전에 같이 살 때는 제가",
          "speaker_id": "1",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100025113.1.3",
          "form": "도시락싸서 들려보내고 그랬었어여",
          "original_form": "도시락싸서 들려보내고 그랬었어여",
          "speaker_id": "1",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100025113.1.4",
          "form": "백반하는곳도 없고...",
          "original_form": "백반하는곳도 없고...",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100025113.1.5",
          "form": "저도결국엔",
          "original_form": "저도결국엔",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100025113.1.6",
          "form": "그냥 진짜 냉동식품 튀겨서 싸줫는데 좋아하더라구여 그정도면 증말..",
          "original_form": "그냥 진짜 냉동식품 튀겨서 싸줫는데 좋아하더라구여 그정도면 증말..",
          "speaker_id": "1",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100025113.1.7",
          "form": "냉동도시락사서 먹었어요 ㅋㅋㄱㅋㅋ",
          "original_form": "냉동도시락사서 먹었어요 ㅋㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100025113.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100025113.1.9",
          "form": "그게오히려 덜질리더라구요",
          "original_form": "그게오히려 덜질리더라구요",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100025113.1.10",
          "form": "아니 진짜 음식 맛없게 하는 음식점들",
          "original_form": "아니 진짜 음식 맛없게 하는 음식점들",
          "speaker_id": "1",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100025113.1.11",
          "form": "요샌 맛도 다양하게 나와서...",
          "original_form": "요샌 맛도 다양하게 나와서...",
          "speaker_id": "2",
          "time": "20210831 23:14"
        },
        {
          "id": "MDRW2100025113.1.12",
          "form": "다",
          "original_form": "다",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100025113.1.13",
          "form": "벌.금",
          "original_form": "벌.금",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100025113.1.14",
          "form": "내고",
          "original_form": "내고",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100025113.1.15",
          "form": "문닫아ㅏ야돼요",
          "original_form": "문닫아ㅏ야돼요",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100025113.1.16",
          "form": "진짜 비싸기만하고 맛도없으면...",
          "original_form": "진짜 비싸기만하고 맛도없으면...",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100025113.1.17",
          "form": "화가나더라구요",
          "original_form": "화가나더라구요",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100025113.1.18",
          "form": "한두푼하는것도아니고",
          "original_form": "한두푼하는것도아니고",
          "speaker_id": "2",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100025113.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100025113.1.20",
          "form": "양심이읍서 양심이",
          "original_form": "양심이읍서 양심이",
          "speaker_id": "1",
          "time": "20210831 23:15"
        },
        {
          "id": "MDRW2100025113.1.21",
          "form": "암튼 점심먹는거 중요한데",
          "original_form": "암튼 점심먹는거 중요한데",
          "speaker_id": "2",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100025113.1.22",
          "form": "요샌 메뉴도 잘나오고",
          "original_form": "요샌 메뉴도 잘나오고",
          "speaker_id": "2",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100025113.1.23",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210831 23:16"
        },
        {
          "id": "MDRW2100025113.1.24",
          "form": "괜찮은곳이생기니까 스트레스도 덜받느느낌이에요",
          "original_form": "괜찮은곳이생기니까 스트레스도 덜받느느낌이에요",
          "speaker_id": "2",
          "time": "20210831 23:17"
        },
        {
          "id": "MDRW2100025113.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 다행이에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 다행이에요",
