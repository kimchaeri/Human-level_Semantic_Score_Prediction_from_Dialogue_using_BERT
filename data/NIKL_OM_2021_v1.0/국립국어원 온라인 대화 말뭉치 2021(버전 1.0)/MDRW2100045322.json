        {
          "id": "MDRW2100045322.1.1",
          "form": "저도 그거 사서 라면 끓여야겟어요 ㅋ",
          "original_form": "저도 그거 사서 라면 끓여야겟어요 ㅋ",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045322.1.2",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045322.1.3",
          "form": "네네 ㅋㅋㅋㅋ 솔직히 라면은",
          "original_form": "네네 ㅋㅋㅋㅋ 솔직히 라면은",
          "speaker_id": "2",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045322.1.4",
          "form": "쉽게 빨리 먹으려는건데",
          "original_form": "쉽게 빨리 먹으려는건데",
          "speaker_id": "2",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045322.1.5",
          "form": "물 애매하면 화나잖아여 ㅋㅋㅋㅋ",
          "original_form": "물 애매하면 화나잖아여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045322.1.6",
          "form": "맞아욬ㅋㅋㅋㅋ 물만 제대로 해도",
          "original_form": "맞아욬ㅋㅋㅋㅋ 물만 제대로 해도",
          "speaker_id": "1",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045322.1.7",
          "form": "기본은 하니까뇨 ㅠㅠ",
          "original_form": "기본은 하니까뇨 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100045322.1.8",
          "form": "어떤 라면 좋아하세요?",
          "original_form": "어떤 라면 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100045322.1.9",
          "form": "저는 너구리요!!!!",
          "original_form": "저는 너구리요!!!!",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045322.1.10",
          "form": "짜파게티도 ㅠㅠ",
          "original_form": "짜파게티도 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100045322.1.11",
          "form": "앗 너구리 맛있죠 ㅋㅋㅋㅋ",
          "original_form": "앗 너구리 맛있죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045322.1.12",
          "form": "짜파구리 좋아하시나요??",
          "original_form": "짜파구리 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100045322.1.13",
          "form": "오오 좋아해요 ㅋㅋ 가끔 만들어머겅요",
          "original_form": "오오 좋아해요 ㅋㅋ 가끔 만들어머겅요",
          "speaker_id": "1",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100045322.1.14",
          "form": "ㅋㅋㅋ 저도 좋아해서 만들어먹는데",
          "original_form": "ㅋㅋㅋ 저도 좋아해서 만들어먹는데",
          "speaker_id": "2",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045322.1.15",
          "form": "요즘은 아예 붙여서 팔더라구요 ㅋㅋㅋㅋ",
          "original_form": "요즘은 아예 붙여서 팔더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045322.1.16",
          "form": "우아 ㅋㅋㅋ 세트인가봐요",
          "original_form": "우아 ㅋㅋㅋ 세트인가봐요",
