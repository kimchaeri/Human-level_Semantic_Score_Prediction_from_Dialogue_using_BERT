      "utterance": [
        {
          "id": "MDRW2100007450.1.1",
          "form": "안녕하세요~ 반려동물 키우신 경험 있으신가요?",
          "original_form": "안녕하세요~ 반려동물 키우신 경험 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 02:11"
        },
        {
          "id": "MDRW2100007450.1.2",
          "form": "네 많이 키워봤어요 어렸을때",
          "original_form": "네 많이 키워봤어요 어렸을때",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007450.1.3",
          "form": "동물키우세요?",
          "original_form": "동물키우세요?",
          "speaker_id": "2",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007450.1.4",
          "form": "저도 키웠었는데 지금은 아니에요~",
          "original_form": "저도 키웠었는데 지금은 아니에요~",
          "speaker_id": "1",
          "time": "20210822 02:12"
        },
        {
          "id": "MDRW2100007450.1.5",
          "form": "고슴도치 두번 키워봤습니다",
          "original_form": "고슴도치 두번 키워봤습니다",
          "speaker_id": "1",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007450.1.6",
          "form": "오 특이하네요",
          "original_form": "오 특이하네요",
          "speaker_id": "2",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007450.1.7",
          "form": "키우기 어렵지않아요?",
          "original_form": "키우기 어렵지않아요?",
          "speaker_id": "2",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007450.1.8",
          "form": "음 처음에 경계가 많이 심해서요",
          "original_form": "음 처음에 경계가 많이 심해서요",
          "speaker_id": "1",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007450.1.9",
          "form": "3일에서 일주일 정도는",
          "original_form": "3일에서 일주일 정도는",
          "speaker_id": "1",
          "time": "20210822 02:13"
        },
        {
          "id": "MDRW2100007450.1.10",
          "form": "내리 밤송이모드로 쉭쉭거리기만 했었는데",
          "original_form": "내리 밤송이모드로 쉭쉭거리기만 했었는데",
          "speaker_id": "1",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007450.1.11",
          "form": "집에 풀어놓고 키우나요?",
          "original_form": "집에 풀어놓고 키우나요?",
          "speaker_id": "2",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007450.1.12",
          "form": "친해지면 정말 좋아요~",
          "original_form": "친해지면 정말 좋아요~",
          "speaker_id": "1",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007450.1.13",
          "form": "아뇨 케이지가 있는데 자주 풀어주긴 했어요",
          "original_form": "아뇨 케이지가 있는데 자주 풀어주긴 했어요",
          "speaker_id": "1",
          "time": "20210822 02:14"
        },
        {
          "id": "MDRW2100007450.1.14",
          "form": "첫번째 고슴도치가 유난히 활동적이었어서",
          "original_form": "첫번째 고슴도치가 유난히 활동적이었어서",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007450.1.15",
          "form": "귀여울것 같아요 ㅎ 말도 알아들어요?",
          "original_form": "귀여울것 같아요 ㅎ 말도 알아들어요?",
          "speaker_id": "2",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007450.1.16",
          "form": "밤에 다들 자는데 뚜껑 머리로 밀고 탈출해서 서랍 밑에서 발견하기도 했어요 ㅋㅋㅋ",
          "original_form": "밤에 다들 자는데 뚜껑 머리로 밀고 탈출해서 서랍 밑에서 발견하기도 했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007450.1.17",
          "form": "본인 이름은 알아듣는것같아요",
          "original_form": "본인 이름은 알아듣는것같아요",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007450.1.18",
          "form": "ㅎㅎ 가시박히면 엄청 따가울듯요",
          "original_form": "ㅎㅎ 가시박히면 엄청 따가울듯요",
          "speaker_id": "2",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007450.1.19",
          "form": "근데 뭐 강아지나 다른 동물들처럼 사람 좋아하고 친화력있는 아이는 아니라",
          "original_form": "근데 뭐 강아지나 다른 동물들처럼 사람 좋아하고 친화력있는 아이는 아니라",
          "speaker_id": "1",
          "time": "20210822 02:15"
        },
        {
          "id": "MDRW2100007450.1.20",
          "form": "혼자 잘 사는 동물이거든요 특성상",
          "original_form": "혼자 잘 사는 동물이거든요 특성상",
          "speaker_id": "1",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007450.1.21",
          "form": "그래서 들은 체도 잘 안해요",
          "original_form": "그래서 들은 체도 잘 안해요",
          "speaker_id": "1",
          "time": "20210822 02:16"
        },
        {
          "id": "MDRW2100007450.1.22",
          "form": "혹시 어떤 동물 키워보셨나요?",
          "original_form": "혹시 어떤 동물 키워보셨나요?",
          "speaker_id": "1",
          "time": "20210822 02:17"
        },
        {
          "id": "MDRW2100007450.1.23",
          "form": "전 고양이랑 개요",
          "original_form": "전 고양이랑 개요",
          "speaker_id": "2",
          "time": "20210822 02:18"
        },
        {
          "id": "MDRW2100007450.1.24",
          "form": "엄마집에 개한마리 키우고있어요",
          "original_form": "엄마집에 개한마리 키우고있어요",
          "speaker_id": "2",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007450.1.25",
          "form": "고양이는 늙어서 얼마전에 하늘나라로ㅠ",
          "original_form": "고양이는 늙어서 얼마전에 하늘나라로ㅠ",
          "speaker_id": "2",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007450.1.26",
          "form": "아ㅠ 얼마 전에 떠나보내셨구나",
          "original_form": "아ㅠ 얼마 전에 떠나보내셨구나",
          "speaker_id": "1",
          "time": "20210822 02:19"
        },
        {
          "id": "MDRW2100007450.1.27",
          "form": "잘 키워주셨으니 고양이도 잊지 않을거에요",
          "original_form": "잘 키워주셨으니 고양이도 잊지 않을거에요",
          "speaker_id": "1",
          "time": "20210822 02:19"
        }
      ]
    }
  ]
