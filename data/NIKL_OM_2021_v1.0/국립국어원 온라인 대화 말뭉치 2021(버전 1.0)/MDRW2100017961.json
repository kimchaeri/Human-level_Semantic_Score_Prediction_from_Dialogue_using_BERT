      "utterance": [
        {
          "id": "MDRW2100017961.1.1",
          "form": "천연화장품 써봄?",
          "original_form": "천연화장품 써봄?",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017961.1.2",
          "form": "ㄴㄴ 난 항상 로드샵 제품을 쓴다 ㅋㅋㅋ",
          "original_form": "ㄴㄴ 난 항상 로드샵 제품을 쓴다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:04"
        },
        {
          "id": "MDRW2100017961.1.3",
          "form": "ㅋㅋㅋㅋ 로드샵이라도",
          "original_form": "ㅋㅋㅋㅋ 로드샵이라도",
          "speaker_id": "2",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100017961.1.4",
          "form": "천연재료사용한게 있을때도 있잖아",
          "original_form": "천연재료사용한게 있을때도 있잖아",
          "speaker_id": "2",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100017961.1.5",
          "form": "그런거 딱히 안보구",
          "original_form": "그런거 딱히 안보구",
          "speaker_id": "1",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100017961.1.6",
          "form": "그냥 끌리는거 써",
          "original_form": "그냥 끌리는거 써",
          "speaker_id": "1",
          "time": "20210827 14:08"
        },
        {
          "id": "MDRW2100017961.1.7",
          "form": "그때 세일하는 상품중에 ㅋㅋㅋ",
          "original_form": "그때 세일하는 상품중에 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:08"
        },
        {
          "id": "MDRW2100017961.1.8",
          "form": "ㅋㅋㅋㅋㅋ그럼뭐",
          "original_form": "ㅋㅋㅋㅋㅋ그럼뭐",
          "speaker_id": "2",
          "time": "20210827 14:08"
        },
        {
          "id": "MDRW2100017961.1.9",
          "form": "재료별로안따지고 뭐든 잘받는 튼튼한 피부인가봐",
          "original_form": "재료별로안따지고 뭐든 잘받는 튼튼한 피부인가봐",
          "speaker_id": "2",
          "time": "20210827 14:08"
        },
        {
          "id": "MDRW2100017961.1.10",
          "form": "지금은 이니스프리 벚꽃라인 쓰는데",
          "original_form": "지금은 이니스프리 벚꽃라인 쓰는데",
          "speaker_id": "1",
          "time": "20210827 14:08"
        },
        {
          "id": "MDRW2100017961.1.11",
          "form": "다 써가서 다음 세일을 노려보고 있지",
          "original_form": "다 써가서 다음 세일을 노려보고 있지",
          "speaker_id": "1",
          "time": "20210827 14:09"
        },
        {
          "id": "MDRW2100017961.1.12",
          "form": "튼튼?은 아닌데 딱히 예민한 쪽은 아닌거 같음",
          "original_form": "튼튼?은 아닌데 딱히 예민한 쪽은 아닌거 같음",
          "speaker_id": "1",
          "time": "20210827 14:09"
        },
        {
          "id": "MDRW2100017961.1.13",
          "form": "천원 이천원 이런거 아니명 딱히 뭐 안올라오더라구",
          "original_form": "천원 이천원 이런거 아니명 딱히 뭐 안올라오더라구",
          "speaker_id": "1",
          "time": "20210827 14:09"
        },
        {
          "id": "MDRW2100017961.1.14",
          "form": "오..난 천연제품쓰면",
          "original_form": "오..난 천연제품쓰면",
          "speaker_id": "2",
          "time": "20210827 14:09"
        },
        {
          "id": "MDRW2100017961.1.15",
          "form": "무조건뒤집어져",
          "original_form": "무조건뒤집어져",
          "speaker_id": "2",
          "time": "20210827 14:09"
        },
        {
          "id": "MDRW2100017961.1.16",
          "form": "화학제품이 제일잘받음",
          "original_form": "화학제품이 제일잘받음",
          "speaker_id": "2",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100017961.1.17",
          "form": "헐...신기하네",
          "original_form": "헐...신기하네",
          "speaker_id": "1",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100017961.1.18",
          "form": "보통 천연제품 쓰는게 그런 이유 때문 아닌가",
          "original_form": "보통 천연제품 쓰는게 그런 이유 때문 아닌가",
          "speaker_id": "1",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100017961.1.19",
          "form": "천연제품 그자체가",
          "original_form": "천연제품 그자체가",
          "speaker_id": "2",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100017961.1.20",
          "form": "가지고있는 자연적인 독성이있는데",
          "original_form": "가지고있는 자연적인 독성이있는데",
          "speaker_id": "2",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100017961.1.21",
          "form": "내피부가 그걸 못이겨낸다",
          "original_form": "내피부가 그걸 못이겨낸다",
          "speaker_id": "2",
          "time": "20210827 14:10"
        },
        {
          "id": "MDRW2100017961.1.22",
          "form": "결론은 비싼게안맞는",
          "original_form": "결론은 비싼게안맞는",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100017961.1.23",
          "form": "고런 피부라는것이지",
          "original_form": "고런 피부라는것이지",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100017961.1.24",
          "form": "ㅇㅎ...음식이나 꽃 알레르기처럼 그런 거구만",
          "original_form": "ㅇㅎ...음식이나 꽃 알레르기처럼 그런 거구만",
          "speaker_id": "1",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100017961.1.25",
          "form": "ㅋㅋㅋㅋ 좋은건지 슬픈건지",
          "original_form": "ㅋㅋㅋㅋ 좋은건지 슬픈건지",
          "speaker_id": "1",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100017961.1.26",
          "form": "오 그래 알러지라고하는게",
          "original_form": "오 그래 알러지라고하는게",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100017961.1.27",
          "form": "정확할지도",
          "original_form": "정확할지도",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100017961.1.28",
          "form": "ㅋㅋㅋㅋ비싼게 비지떡",
          "original_form": "ㅋㅋㅋㅋ비싼게 비지떡",
          "speaker_id": "2",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100017961.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 보통 어디거 씀?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 보통 어디거 씀?",
          "speaker_id": "1",
          "time": "20210827 14:11"
        },
        {
          "id": "MDRW2100017961.1.30",
          "form": "이니스프리는 진심",
          "original_form": "이니스프리는 진심",
          "speaker_id": "2",
          "time": "20210827 14:12"
        },
        {
          "id": "MDRW2100017961.1.31",
          "form": "한번도뒤집어진적없음",
          "original_form": "한번도뒤집어진적없음",
          "speaker_id": "2",
          "time": "20210827 14:12"
        }
      ]
