      "utterance": [
        {
          "id": "MDRW2100017796.1.1",
          "form": "아 오늘 대박이 있었어요",
          "original_form": "아 오늘 대박이 있었어요",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017796.1.2",
          "form": "동물 좋아해?",
          "original_form": "동물 좋아해?",
          "speaker_id": "2",
          "time": "20210826 21:10"
        },
        {
          "id": "MDRW2100017796.1.3",
          "form": "랜선으로보는거좋아해",
          "original_form": "랜선으로보는거좋아해",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017796.1.4",
          "form": "키우는건?",
          "original_form": "키우는건?",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017796.1.5",
          "form": "키우는건안좋아",
          "original_form": "키우는건안좋아",
          "speaker_id": "1",
          "time": "20210826 21:13"
        },
        {
          "id": "MDRW2100017796.1.6",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100017796.1.7",
          "form": "생명책임진다는게 무서워",
          "original_form": "생명책임진다는게 무서워",
          "speaker_id": "1",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017796.1.8",
          "form": "그건 그래",
          "original_form": "그건 그래",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017796.1.9",
          "form": "나 최근 알러지 생겨서 알레르기 검사했는데",
          "original_form": "나 최근 알러지 생겨서 알레르기 검사했는데",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017796.1.10",
          "form": "고양이 알러지 있다더라고",
          "original_form": "고양이 알러지 있다더라고",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017796.1.11",
          "form": "헐 그러면 어떡해",
          "original_form": "헐 그러면 어떡해",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017796.1.12",
          "form": "아이고ㅜㅜ",
          "original_form": "아이고ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017796.1.13",
          "form": "웃겨서 엄청 웃었지ㅋㅋㅋ",
          "original_form": "웃겨서 엄청 웃었지ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017796.1.14",
          "form": "신기하더라구",
          "original_form": "신기하더라구",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017796.1.15",
          "form": "신기한데 그래두 심각한건아닌거지?ㅜㅜ",
          "original_form": "신기한데 그래두 심각한건아닌거지?ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017796.1.16",
          "form": "그럼ㅋㅋ",
          "original_form": "그럼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017796.1.17",
          "form": "이거때문에 3번 찔린 팔이 쓰릴뿐",
          "original_form": "이거때문에 3번 찔린 팔이 쓰릴뿐",
          "speaker_id": "2",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017796.1.18",
          "form": "아팠겠네요",
          "original_form": "아팠겠네요",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017796.1.19",
          "form": "완전요",
          "original_form": "완전요",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017796.1.20",
          "form": "동물알레르기 있으세요?",
          "original_form": "동물알레르기 있으세요?",
          "speaker_id": "2",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017796.1.21",
          "form": "아니요 알레르기없어요",
          "original_form": "아니요 알레르기없어요",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017796.1.22",
          "form": "반려동물키우시나요?",
          "original_form": "반려동물키우시나요?",
          "speaker_id": "1",
          "time": "20210826 21:30"
        },
        {
          "id": "MDRW2100017796.1.23",
          "form": "네 고양이요",
          "original_form": "네 고양이요",
          "speaker_id": "2",
          "time": "20210826 21:32"
        }
      ]
