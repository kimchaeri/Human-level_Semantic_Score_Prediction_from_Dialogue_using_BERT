      "utterance": [
        {
          "id": "MDRW2100010097.1.1",
          "form": "여기는 비와요",
          "original_form": "여기는 비와요",
          "speaker_id": "1",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100010097.1.2",
          "form": "여기두 비가오네요",
          "original_form": "여기두 비가오네요",
          "speaker_id": "2",
          "time": "20210823 14:19"
        },
        {
          "id": "MDRW2100010097.1.3",
          "form": "비오는 날 출근하기 너무 싫어요",
          "original_form": "비오는 날 출근하기 너무 싫어요",
          "speaker_id": "2",
          "time": "20210823 14:19"
        },
        {
          "id": "MDRW2100010097.1.4",
          "form": "저는오늘 재택이라ㅠㅠ",
          "original_form": "저는오늘 재택이라ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 14:19"
        },
        {
          "id": "MDRW2100010097.1.5",
          "form": "앗 너무 부럽네요",
          "original_form": "앗 너무 부럽네요",
          "speaker_id": "2",
          "time": "20210823 14:20"
        },
        {
          "id": "MDRW2100010097.1.6",
          "form": "내일 태풍때문에 비 진짜 많이온대요",
          "original_form": "내일 태풍때문에 비 진짜 많이온대요",
          "speaker_id": "2",
          "time": "20210823 14:20"
        },
        {
          "id": "MDRW2100010097.1.7",
          "form": "진짜요? 내일은 회사가야하는데",
          "original_form": "진짜요? 내일은 회사가야하는데",
          "speaker_id": "1",
          "time": "20210823 14:20"
        },
        {
          "id": "MDRW2100010097.1.8",
          "form": "날씨도 애매해서 입을 게 없어요",
          "original_form": "날씨도 애매해서 입을 게 없어요",
          "speaker_id": "1",
          "time": "20210823 14:20"
        },
        {
          "id": "MDRW2100010097.1.9",
          "form": "저도 그래서 요즘 가디건 들고다녀요",
          "original_form": "저도 그래서 요즘 가디건 들고다녀요",
          "speaker_id": "2",
          "time": "20210823 14:20"
        },
        {
          "id": "MDRW2100010097.1.10",
          "form": "에어컨 안틀어도 선선하니 좋죠",
          "original_form": "에어컨 안틀어도 선선하니 좋죠",
          "speaker_id": "1",
          "time": "20210823 14:21"
        },
        {
          "id": "MDRW2100010097.1.11",
          "form": "맞아요ㅋㅋ근데 꼭 덥다고 요즘도 에어컨 켜는 동료들이 있어서 가디건 필수예요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ근데 꼭 덥다고 요즘도 에어컨 켜는 동료들이 있어서 가디건 필수예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 14:21"
        },
        {
          "id": "MDRW2100010097.1.12",
          "form": "ㅋㅋ 내일 태풍 무탈히 지나갔음 좋겠네요",
          "original_form": "ㅋㅋ 내일 태풍 무탈히 지나갔음 좋겠네요",
          "speaker_id": "1",
          "time": "20210823 14:22"
        },
        {
          "id": "MDRW2100010097.1.13",
          "form": "그러게요 일기예보 안 맞길 기도합시다ㅋㅋ",
          "original_form": "그러게요 일기예보 안 맞길 기도합시다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 14:23"
        },
        {
          "id": "MDRW2100010097.1.14",
          "form": "내일 비오면 택시타고 출근할 거에요",
          "original_form": "내일 비오면 택시타고 출근할 거에요",
          "speaker_id": "1",
          "time": "20210823 14:23"
        },
        {
          "id": "MDRW2100010097.1.15",
          "form": "저는 오늘도 택시탔어요ㅜㅠ",
          "original_form": "저는 오늘도 택시탔어요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210823 14:24"
        },
        {
          "id": "MDRW2100010097.1.16",
          "form": "비에 젖는 거 너무 싫어해서...",
          "original_form": "비에 젖는 거 너무 싫어해서...",
          "speaker_id": "2",
          "time": "20210823 14:24"
        },
        {
          "id": "MDRW2100010097.1.17",
          "form": "동감입니다",
          "original_form": "동감입니다",
          "speaker_id": "1",
          "time": "20210823 14:25"
        },
        {
          "id": "MDRW2100010097.1.18",
          "form": "이번주만 버티면 완전 가을일 거같지않나요!",
          "original_form": "이번주만 버티면 완전 가을일 거같지않나요!",
          "speaker_id": "1",
          "time": "20210823 14:25"
        },
        {
          "id": "MDRW2100010097.1.19",
          "form": "네 엄청 시원해질 거같아요",
          "original_form": "네 엄청 시원해질 거같아요",
          "speaker_id": "2",
          "time": "20210823 14:25"
        },
        {
          "id": "MDRW2100010097.1.20",
          "form": "저는 여름 진짜 싫어해서",
          "original_form": "저는 여름 진짜 싫어해서",
          "speaker_id": "1",
          "time": "20210823 14:25"
        },
        {
          "id": "MDRW2100010097.1.21",
          "form": "빨리 겨울왔음 좋겠어요",
          "original_form": "빨리 겨울왔음 좋겠어요",
          "speaker_id": "1",
          "time": "20210823 14:26"
        },
        {
          "id": "MDRW2100010097.1.22",
          "form": "저도 겨울파긴한데",
          "original_form": "저도 겨울파긴한데",
          "speaker_id": "2",
          "time": "20210823 14:26"
        },
        {
          "id": "MDRW2100010097.1.23",
          "form": "가을을 제일 좋아해요",
          "original_form": "가을을 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210823 14:26"
        },
        {
          "id": "MDRW2100010097.1.24",
          "form": "가을 일주일은 되려나요... .",
          "original_form": "가을 일주일은 되려나요... .",
          "speaker_id": "1",
          "time": "20210823 14:27"
        },
        {
          "id": "MDRW2100010097.1.25",
          "form": "ㅠㅠ 바로 겨울오겠죠...",
          "original_form": "ㅠㅠ 바로 겨울오겠죠...",
          "speaker_id": "2",
          "time": "20210823 14:28"
        },
        {
          "id": "MDRW2100010097.1.26",
          "form": "여긴  비가많이와요",
          "original_form": "여긴  비가많이와요",
          "speaker_id": "2",
          "time": "20210823 23:36"
        }
      ]
    }
  ]
