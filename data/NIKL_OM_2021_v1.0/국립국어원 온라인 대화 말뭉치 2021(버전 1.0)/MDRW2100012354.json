      "utterance": [
        {
          "id": "MDRW2100012354.1.1",
          "form": "정자보다 작다는데",
          "original_form": "정자보다 작다는데",
          "speaker_id": "2",
          "time": "20210824 21:28"
        },
        {
          "id": "MDRW2100012354.1.2",
          "form": "복불복이죠",
          "original_form": "복불복이죠",
          "speaker_id": "2",
          "time": "20210824 21:28"
        },
        {
          "id": "MDRW2100012354.1.3",
          "form": "이런말하면 어떤애들은 보수적이라는데",
          "original_form": "이런말하면 어떤애들은 보수적이라는데",
          "speaker_id": "2",
          "time": "20210824 21:28"
        },
        {
          "id": "MDRW2100012354.1.4",
          "form": "아이러니하더라구욬ㅋㅋㅋㅋ",
          "original_form": "아이러니하더라구욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:29"
        },
        {
          "id": "MDRW2100012354.1.5",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "2",
          "time": "20210824 21:29"
        },
        {
          "id": "MDRW2100012354.1.6",
          "form": "그냥. 낳으면 키우는거 너무 무책임해보이더라구요..",
          "original_form": "그냥. 낳으면 키우는거 너무 무책임해보이더라구요..",
          "speaker_id": "1",
          "time": "20210824 21:29"
        },
        {
          "id": "MDRW2100012354.1.7",
          "form": "카우에서 봤는데",
          "original_form": "카우에서 봤는데",
          "speaker_id": "1",
          "time": "20210824 21:31"
        },
        {
          "id": "MDRW2100012354.1.8",
          "form": "가다실 안 맞았는데 성 ㄹㄹㄹ 폭 ㄹㄹㄹ 력 ㄹㄹㄹ 때문에",
          "original_form": "가다실 안 맞았는데 성 ㄹㄹㄹ 폭 ㄹㄹㄹ 력 ㄹㄹㄹ 때문에",
          "speaker_id": "1",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012354.1.9",
          "form": "성ㄹㄹ병이 옮았다는 분이 계신거보고",
          "original_form": "성ㄹㄹ병이 옮았다는 분이 계신거보고",
          "speaker_id": "1",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012354.1.10",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012354.1.11",
          "form": "진짜 통탄을 금ㅁ할수없었어요",
          "original_form": "진짜 통탄을 금ㅁ할수없었어요",
          "speaker_id": "1",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012354.1.12",
          "form": "진짜요",
          "original_form": "진짜요",
          "speaker_id": "2",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012354.1.13",
          "form": "뭐같은 경우가",
          "original_form": "뭐같은 경우가",
          "speaker_id": "2",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012354.1.14",
          "form": "성병은 진짜 선택할수있는게 아닌데 다들 쉽게 봐서 신기해요",
          "original_form": "성병은 진짜 선택할수있는게 아닌데 다들 쉽게 봐서 신기해요",
          "speaker_id": "1",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012354.1.15",
          "form": "진짜 그때 순간 말을 못했어요",
          "original_form": "진짜 그때 순간 말을 못했어요",
          "speaker_id": "2",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012354.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012354.1.17",
          "form": "임신 부 작 용처럼",
          "original_form": "임신 부 작 용처럼",
          "speaker_id": "2",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012354.1.18",
          "form": "너무 과소평과 됐어요",
          "original_form": "너무 과소평과 됐어요",
          "speaker_id": "2",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012354.1.19",
          "form": "부작용",
          "original_form": "부작용",
          "speaker_id": "1",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012354.1.20",
          "form": "아 맞아요 ㄹㅇ",
          "original_form": "아 맞아요 ㄹㅇ",
          "speaker_id": "1",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012354.1.21",
          "form": "남자가 손해안보니 다들 쉬쉬하는지",
          "original_form": "남자가 손해안보니 다들 쉬쉬하는지",
          "speaker_id": "1",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012354.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012354.1.23",
          "form": "선택을 더 쉽게하려고",
          "original_form": "선택을 더 쉽게하려고",
          "speaker_id": "2",
          "time": "20210824 21:34"
        },
        {
          "id": "MDRW2100012354.1.24",
          "form": "발악을 하는",
          "original_form": "발악을 하는",
          "speaker_id": "2",
          "time": "20210824 21:34"
        },
        {
          "id": "MDRW2100012354.1.25",
          "form": "진짜 짜증나요",
          "original_form": "진짜 짜증나요",
