      "utterance": [
        {
          "id": "MDRW2100020664.1.1",
          "form": "집안일 좋아하세요?",
          "original_form": "집안일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020664.1.2",
          "form": "집안일이요...?",
          "original_form": "집안일이요...?",
          "speaker_id": "2",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100020664.1.3",
          "form": "아뇽..",
          "original_form": "아뇽..",
          "speaker_id": "2",
          "time": "20210828 18:07"
        },
        {
          "id": "MDRW2100020664.1.4",
          "form": "저도..ㅎㅎ",
          "original_form": "저도..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 18:26"
        },
        {
          "id": "MDRW2100020664.1.5",
          "form": "가족과 함께 사시나요?",
          "original_form": "가족과 함께 사시나요?",
          "speaker_id": "1",
          "time": "20210828 18:26"
        },
        {
          "id": "MDRW2100020664.1.6",
          "form": "넹 그렇습니당",
          "original_form": "넹 그렇습니당",
          "speaker_id": "2",
          "time": "20210828 19:51"
        },
        {
          "id": "MDRW2100020664.1.7",
          "form": "학생이신가용??",
          "original_form": "학생이신가용??",
          "speaker_id": "2",
          "time": "20210828 19:52"
        },
        {
          "id": "MDRW2100020664.1.8",
          "form": "취준생이에요!",
          "original_form": "취준생이에요!",
          "speaker_id": "1",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100020664.1.9",
          "form": "학생이신가요??",
          "original_form": "학생이신가요??",
          "speaker_id": "1",
          "time": "20210828 19:59"
        },
        {
          "id": "MDRW2100020664.1.10",
          "form": "네 이제 막학기에요..!ㅠ",
          "original_form": "네 이제 막학기에요..!ㅠ",
          "speaker_id": "2",
          "time": "20210828 20:23"
        },
        {
          "id": "MDRW2100020664.1.11",
          "form": "바쁘시겠어요ㅜㅠ",
          "original_form": "바쁘시겠어요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100020664.1.12",
          "form": "자취하고 싶지는 않으신가요?",
          "original_form": "자취하고 싶지는 않으신가요?",
          "speaker_id": "1",
          "time": "20210828 20:28"
        },
        {
          "id": "MDRW2100020664.1.13",
          "form": "아 자취 너무 좋죠",
          "original_form": "아 자취 너무 좋죠",
          "speaker_id": "2",
          "time": "20210828 23:30"
        },
        {
          "id": "MDRW2100020664.1.14",
          "form": "근데 뭔가 자취할 필요성이 없달까...?",
          "original_form": "근데 뭔가 자취할 필요성이 없달까...?",
          "speaker_id": "2",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100020664.1.15",
          "form": "학교도 비대면이고 해서",
          "original_form": "학교도 비대면이고 해서",
          "speaker_id": "2",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100020664.1.16",
          "form": "ㅠㅜ",
          "original_form": "ㅠㅜ",
          "speaker_id": "2",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100020664.1.17",
          "form": "취업하면 하려구용",
          "original_form": "취업하면 하려구용",
          "speaker_id": "2",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100020664.1.18",
          "form": "자취 하고싶으신가용??",
          "original_form": "자취 하고싶으신가용??",
          "speaker_id": "2",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100020664.1.19",
          "form": "아 그렇네요ㅠ 줌 강의인가요??",
          "original_form": "아 그렇네요ㅠ 줌 강의인가요??",
          "speaker_id": "1",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100020664.1.20",
          "form": "네 아무래도 독립해야하는거 아닌가 싶은 생각이 들어요",
          "original_form": "네 아무래도 독립해야하는거 아닌가 싶은 생각이 들어요",
          "speaker_id": "1",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100020664.1.21",
          "form": "네 줌이용..ㅠ",
          "original_form": "네 줌이용..ㅠ",
          "speaker_id": "2",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100020664.1.22",
          "form": "독립 너무 좋은데..\r\n 저도 빨리 취업하면 독립하고 싶어요",
          "original_form": "독립 너무 좋은데..\r\n 저도 빨리 취업하면 독립하고 싶어요",
          "speaker_id": "2",
          "time": "20210829 16:39"
        },
        {
          "id": "MDRW2100020664.1.23",
          "form": "저두요! 빨리 자리잡고 싶다는 생각이 요즘 많이 드네요ㅠ",
          "original_form": "저두요! 빨리 자리잡고 싶다는 생각이 요즘 많이 드네요ㅠ",
          "speaker_id": "1",
          "time": "20210829 17:17"
        },
        {
          "id": "MDRW2100020664.1.24",
          "form": "형제가 어떻게 되세요??",
          "original_form": "형제가 어떻게 되세요??",
          "speaker_id": "2",
          "time": "20210830 11:52"
        },
        {
          "id": "MDRW2100020664.1.25",
          "form": "오빠있어요!",
          "original_form": "오빠있어요!",
          "speaker_id": "1",
          "time": "20210830 12:32"
        }
      ]
    }
