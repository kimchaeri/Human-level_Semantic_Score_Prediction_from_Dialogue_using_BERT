      "utterance": [
        {
          "id": "MDRW2100037814.1.1",
          "form": "너무 양이 적어요 ㅋㅋㅜㅜㅜ",
          "original_form": "너무 양이 적어요 ㅋㅋㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 18:49"
        },
        {
          "id": "MDRW2100037814.1.2",
          "form": "예전엔 이걸 누구 코에 붙이나 했는데",
          "original_form": "예전엔 이걸 누구 코에 붙이나 했는데",
          "speaker_id": "2",
          "time": "20210901 18:50"
        },
        {
          "id": "MDRW2100037814.1.3",
          "form": "국 대신이구나 생각하니까 괜찮더라구요 ㅋㅋ",
          "original_form": "국 대신이구나 생각하니까 괜찮더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:51"
        },
        {
          "id": "MDRW2100037814.1.4",
          "form": "아 국대신은 괜찮겠어요 ㅎㅎㅎ",
          "original_form": "아 국대신은 괜찮겠어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100037814.1.5",
          "form": "컵누들은 사이드로 먹어야해요 ㅋㅋㅋ",
          "original_form": "컵누들은 사이드로 먹어야해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:53"
        },
        {
          "id": "MDRW2100037814.1.6",
          "form": "맞아요. 그건 배부를때도 다 먹을수있는 양이에요",
          "original_form": "맞아요. 그건 배부를때도 다 먹을수있는 양이에요",
          "speaker_id": "2",
          "time": "20210901 18:56"
        },
        {
          "id": "MDRW2100037814.1.7",
          "form": "그걸로 다이어트를 어케하려고?",
          "original_form": "그걸로 다이어트를 어케하려고?",
          "speaker_id": "2",
          "time": "20210901 18:56"
        },
        {
          "id": "MDRW2100037814.1.8",
          "form": "딴거 먹다가 살 더 쪄요 ㅋㅋㅋ",
          "original_form": "딴거 먹다가 살 더 쪄요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:57"
        },
        {
          "id": "MDRW2100037814.1.9",
          "form": "마자옄ㅋㄲㅋㄱㄲ",
          "original_form": "마자옄ㅋㄲㅋㄱㄲ",
          "speaker_id": "1",
          "time": "20210901 18:59"
        },
        {
          "id": "MDRW2100037814.1.10",
          "form": "칼로리 낮은 이유가 양이 적어서라잖아요ㅋㅋㅋ",
          "original_form": "칼로리 낮은 이유가 양이 적어서라잖아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:00"
        },
        {
          "id": "MDRW2100037814.1.11",
          "form": "맞아요, 면이 당면인거 빼면",
          "original_form": "맞아요, 면이 당면인거 빼면",
          "speaker_id": "2",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100037814.1.12",
          "form": "솔직히 나트륨량도 그냥 라면이랑 비슷할듯",
          "original_form": "솔직히 나트륨량도 그냥 라면이랑 비슷할듯",
          "speaker_id": "2",
          "time": "20210901 19:01"
        },
        {
          "id": "MDRW2100037814.1.13",
          "form": "아 마자여 컵누들 나트륨 높더라구여 ??",
          "original_form": "아 마자여 컵누들 나트륨 높더라구여 ??",
          "speaker_id": "1",
          "time": "20210901 19:05"
        },
        {
          "id": "MDRW2100037814.1.14",
          "form": "배신이에요 ..",
          "original_form": "배신이에요 ..",
          "speaker_id": "1",
          "time": "20210901 19:05"
        },
        {
          "id": "MDRW2100037814.1.15",
          "form": "맛만 봐도 ㅋㅋㅋㅋ",
          "original_form": "맛만 봐도 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:05"
        },
        {
          "id": "MDRW2100037814.1.16",
          "form": "맛있잖아요...",
          "original_form": "맛있잖아요...",
          "speaker_id": "2",
          "time": "20210901 19:05"
        },
        {
          "id": "MDRW2100037814.1.17",
          "form": "맛과 칼로리는 비례하니까...",
          "original_form": "맛과 칼로리는 비례하니까...",
          "speaker_id": "2",
          "time": "20210901 19:06"
        },
        {
          "id": "MDRW2100037814.1.18",
          "form": "마자여 저 컵누들 한 5개는 먹을수 있을듯요...",
          "original_form": "마자여 저 컵누들 한 5개는 먹을수 있을듯요...",
          "speaker_id": "1",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100037814.1.19",
          "form": "그쵸, 사실 맛있게 다이어트라는건 있을 수 없음요 ㅠㅠ",
          "original_form": "그쵸, 사실 맛있게 다이어트라는건 있을 수 없음요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100037814.1.20",
          "form": "그냥 애초에 안찌게 노력하는게 제일 쉬운거 같아요",
          "original_form": "그냥 애초에 안찌게 노력하는게 제일 쉬운거 같아요",
          "speaker_id": "2",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100037814.1.21",
          "form": "마자여 ㅋㅋㅋㅋ .. 그게 쉽지 않지만 ...ㅜㅜ",
          "original_form": "마자여 ㅋㅋㅋㅋ .. 그게 쉽지 않지만 ...ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100037814.1.22",
          "form": "급찐급빠를 달고살아야죠 ㅋㅋ",
          "original_form": "급찐급빠를 달고살아야죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100037814.1.23",
          "form": "빼는 것보단 쉬워요.",
          "original_form": "빼는 것보단 쉬워요.",
          "speaker_id": "2",
          "time": "20210901 19:12"
        },
        {
          "id": "MDRW2100037814.1.24",
          "form": "급찐급빠는 건강에도 안좋지 않나요?",
          "original_form": "급찐급빠는 건강에도 안좋지 않나요?",
          "speaker_id": "2",
          "time": "20210901 19:12"
        },
        {
          "id": "MDRW2100037814.1.25",
          "form": "운동으로 조져야죠 !",
          "original_form": "운동으로 조져야죠 !",
          "speaker_id": "1",
          "time": "20210901 19:12"
        },
        {
          "id": "MDRW2100037814.1.26",
          "form": "유산소로 조져야죠 !!",
          "original_form": "유산소로 조져야죠 !!",
          "speaker_id": "1",
          "time": "20210901 19:12"
        },
        {
          "id": "MDRW2100037814.1.27",
          "form": "클린식 먹구요 하하",
          "original_form": "클린식 먹구요 하하",
