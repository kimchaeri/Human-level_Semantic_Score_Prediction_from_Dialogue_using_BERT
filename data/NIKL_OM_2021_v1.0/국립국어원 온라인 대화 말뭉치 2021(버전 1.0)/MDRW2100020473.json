      "utterance": [
        {
          "id": "MDRW2100020473.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 06:59"
        },
        {
          "id": "MDRW2100020473.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020473.1.3",
          "form": "햇빛이 눈부셔서 깼네요 ㅋㅋ 날씨가 어때요",
          "original_form": "햇빛이 눈부셔서 깼네요 ㅋㅋ 날씨가 어때요",
          "speaker_id": "2",
          "time": "20210828 08:15"
        },
        {
          "id": "MDRW2100020473.1.4",
          "form": "선선하고 좋네요ㅎㅎ",
          "original_form": "선선하고 좋네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 11:57"
        },
        {
          "id": "MDRW2100020473.1.5",
          "form": "비온다는 예보가 있었는데 맑아서 기분 좋네요",
          "original_form": "비온다는 예보가 있었는데 맑아서 기분 좋네요",
          "speaker_id": "2",
          "time": "20210828 11:59"
        },
        {
          "id": "MDRW2100020473.1.6",
          "form": "네 쨍쨍합니다",
          "original_form": "네 쨍쨍합니다",
          "speaker_id": "1",
          "time": "20210828 12:22"
        },
        {
          "id": "MDRW2100020473.1.7",
          "form": "날씨 굿",
          "original_form": "날씨 굿",
          "speaker_id": "1",
          "time": "20210828 12:23"
        },
        {
          "id": "MDRW2100020473.1.8",
          "form": "어디 나들이 가시나요",
          "original_form": "어디 나들이 가시나요",
          "speaker_id": "2",
          "time": "20210828 12:23"
        },
        {
          "id": "MDRW2100020473.1.9",
          "form": "아 처가요 ㅎㅎ",
          "original_form": "아 처가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100020473.1.10",
          "form": "오 처가ㅎㅎ 무슨 날이신가여?",
          "original_form": "오 처가ㅎㅎ 무슨 날이신가여?",
          "speaker_id": "2",
          "time": "20210828 17:58"
        },
        {
          "id": "MDRW2100020473.1.11",
          "form": "음 추석때 사람 많이 움직이니까 미리?ㅎㅎ",
          "original_form": "음 추석때 사람 많이 움직이니까 미리?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 18:00"
        },
        {
          "id": "MDRW2100020473.1.12",
          "form": "아 추석을 미리 준비하셨군요",
          "original_form": "아 추석을 미리 준비하셨군요",
          "speaker_id": "2",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100020473.1.13",
          "form": "빠르시다!",
          "original_form": "빠르시다!",
          "speaker_id": "2",
          "time": "20210828 18:02"
        },
        {
          "id": "MDRW2100020473.1.14",
          "form": "오늘은 비가 많이 오네용",
          "original_form": "오늘은 비가 많이 오네용",
          "speaker_id": "1",
          "time": "20210829 06:55"
        },
        {
          "id": "MDRW2100020473.1.15",
          "form": "앗 진짜용 여긴 아직 맑아요!",
          "original_form": "앗 진짜용 여긴 아직 맑아요!",
          "speaker_id": "2",
          "time": "20210829 08:17"
        },
        {
          "id": "MDRW2100020473.1.16",
          "form": "집콕하시나요",
          "original_form": "집콕하시나요",
          "speaker_id": "2",
          "time": "20210829 08:17"
        },
        {
          "id": "MDRW2100020473.1.17",
          "form": "비가 그쳤어요ㅋㅋㅋ",
          "original_form": "비가 그쳤어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 09:00"
        },
        {
          "id": "MDRW2100020473.1.18",
          "form": "진짜 날씨 오락가락하네유",
          "original_form": "진짜 날씨 오락가락하네유",
          "speaker_id": "1",
          "time": "20210829 09:00"
        },
        {
          "id": "MDRW2100020473.1.19",
          "form": "전 어제 비내리고 오늘 맑을 줄 알아서",
          "original_form": "전 어제 비내리고 오늘 맑을 줄 알아서",
          "speaker_id": "2",
          "time": "20210829 09:18"
        },
        {
          "id": "MDRW2100020473.1.20",
          "form": "어제 약속없고 오늘 약속있었는데 망했어요ㅋ큐ㅠ",
          "original_form": "어제 약속없고 오늘 약속있었는데 망했어요ㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210829 09:18"
        },
        {
          "id": "MDRW2100020473.1.21",
          "form": "오늘 날씨 괜찮아진듯 한데",
          "original_form": "오늘 날씨 괜찮아진듯 한데",
          "speaker_id": "1",
          "time": "20210829 11:45"
        },
        {
          "id": "MDRW2100020473.1.22",
          "form": "여긴 흐리꾸리하거든요",
          "original_form": "여긴 흐리꾸리하거든요",
          "speaker_id": "2",
          "time": "20210829 13:31"
        },
        {
          "id": "MDRW2100020473.1.23",
          "form": "그러게요 오락가락하네요",
          "original_form": "그러게요 오락가락하네요",
          "speaker_id": "1",
          "time": "20210829 22:09"
        },
        {
          "id": "MDRW2100020473.1.24",
          "form": "ㅋㅋㅋㅋㅋ오늘은 어디 안 다녀오셨나요",
          "original_form": "ㅋㅋㅋㅋㅋ오늘은 어디 안 다녀오셨나요",
          "speaker_id": "2",
          "time": "20210829 23:56"
        },
        {
          "id": "MDRW2100020473.1.25",
          "form": "ㅎㅎ 네 그냥그냥",
          "original_form": "ㅎㅎ 네 그냥그냥",
          "speaker_id": "1",
          "time": "20210830 01:24"
        },
        {
          "id": "MDRW2100020473.1.26",
          "form": "전 맛집 다녀왓는데 브레이크타임 끝나자마자 갔는데도 웨이팅 30분이나 했거든요",
          "original_form": "전 맛집 다녀왓는데 브레이크타임 끝나자마자 갔는데도 웨이팅 30분이나 했거든요",
          "speaker_id": "2",
          "time": "20210830 09:47"
        },
        {
          "id": "MDRW2100020473.1.27",
          "form": "웨이팅하기엔 너무 후텁지근한 날씨였어요",
          "original_form": "웨이팅하기엔 너무 후텁지근한 날씨였어요",
          "speaker_id": "2",
          "time": "20210830 09:48"
        },
        {
          "id": "MDRW2100020473.1.28",
          "form": "오우 웨이팅 지겹죠",
          "original_form": "오우 웨이팅 지겹죠",
          "speaker_id": "1",
