      "utterance": [
        {
          "id": "MDRW2100008748.1.1",
          "form": "가입되어있는 동호회가 있나요?",
          "original_form": "가입되어있는 동호회가 있나요?",
          "speaker_id": "1",
          "time": "20210822 18:01"
        },
        {
          "id": "MDRW2100008748.1.2",
          "form": "동호회는 없어요ㅜ",
          "original_form": "동호회는 없어요ㅜ",
          "speaker_id": "2",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008748.1.3",
          "form": "하시는거 있나요??",
          "original_form": "하시는거 있나요??",
          "speaker_id": "2",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008748.1.4",
          "form": "예전에 있었는데 요즘은 코로나때문에 없어요ㅜ",
          "original_form": "예전에 있었는데 요즘은 코로나때문에 없어요ㅜ",
          "speaker_id": "1",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008748.1.5",
          "form": "그럼 관심있는 분야라도 있으신가요?",
          "original_form": "그럼 관심있는 분야라도 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008748.1.6",
          "form": "클라이밍 같은거 해보곤 싶어요ㅜ",
          "original_form": "클라이밍 같은거 해보곤 싶어요ㅜ",
          "speaker_id": "2",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008748.1.7",
          "form": "헐 저도",
          "original_form": "헐 저도",
          "speaker_id": "1",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008748.1.8",
          "form": "클라이밍 해보고 싶었는데",
          "original_form": "클라이밍 해보고 싶었는데",
          "speaker_id": "1",
          "time": "20210822 18:06"
        },
        {
          "id": "MDRW2100008748.1.9",
          "form": "이번 올림필 보고 하고 싶더라구요!",
          "original_form": "이번 올림필 보고 하고 싶더라구요!",
          "speaker_id": "2",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008748.1.10",
          "form": "서채현선수!!!",
          "original_form": "서채현선수!!!",
          "speaker_id": "1",
          "time": "20210822 18:16"
        },
        {
          "id": "MDRW2100008748.1.11",
          "form": "네!! 진짜 멋있더라구요!!!",
          "original_form": "네!! 진짜 멋있더라구요!!!",
          "speaker_id": "2",
          "time": "20210822 18:17"
        },
        {
          "id": "MDRW2100008748.1.12",
          "form": "스피드도 멋있고 볼더링도 짱이더라구요!",
          "original_form": "스피드도 멋있고 볼더링도 짱이더라구요!",
          "speaker_id": "2",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008748.1.13",
          "form": "진짜 벽을 슉슉슉슉 타고 올라가는데",
          "original_form": "진짜 벽을 슉슉슉슉 타고 올라가는데",
          "speaker_id": "1",
          "time": "20210822 18:21"
        },
        {
          "id": "MDRW2100008748.1.14",
          "form": "작은체구에서 뿜어내는 에너지에 감탄했어요ㅜ",
          "original_form": "작은체구에서 뿜어내는 에너지에 감탄했어요ㅜ",
          "speaker_id": "1",
          "time": "20210822 18:22"
        },
        {
          "id": "MDRW2100008748.1.15",
          "form": "그래서 왠지 저도 할수있을거같은 느낌ㅎㅎ",
          "original_form": "그래서 왠지 저도 할수있을거같은 느낌ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008748.1.16",
          "form": "운동 원래 좋아하세요?",
          "original_form": "운동 원래 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 18:28"
        },
        {
          "id": "MDRW2100008748.1.17",
          "form": "좋아합니다ㅋㅋㅋ",
          "original_form": "좋아합니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:32"
        },
        {
          "id": "MDRW2100008748.1.18",
          "form": "기회가 없어서 잘 못할뿐ㅎ",
          "original_form": "기회가 없어서 잘 못할뿐ㅎ",
          "speaker_id": "2",
          "time": "20210822 18:32"
        },
        {
          "id": "MDRW2100008748.1.19",
          "form": "어떤운동 좋아하세요?",
          "original_form": "어떤운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 19:09"
        },
        {
          "id": "MDRW2100008748.1.20",
          "form": "그냥 조깅 같은것도 좋아하고ㅎㅎ",
          "original_form": "그냥 조깅 같은것도 좋아하고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:42"
        },
        {
          "id": "MDRW2100008748.1.21",
          "form": "필라테스도 조금 했어요ㅎㅎ",
          "original_form": "필라테스도 조금 했어요ㅎㅎ",
