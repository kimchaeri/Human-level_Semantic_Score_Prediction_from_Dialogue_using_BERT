      "utterance": [
        {
          "id": "MDRW2100042730.1.1",
          "form": "정성 그자체,,",
          "original_form": "정성 그자체,,",
          "speaker_id": "1",
          "time": "20210907 07:57"
        },
        {
          "id": "MDRW2100042730.1.2",
          "form": "그냥 넘어가는 법이 없는 ㅋㅋ",
          "original_form": "그냥 넘어가는 법이 없는 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 07:57"
        },
        {
          "id": "MDRW2100042730.1.3",
          "form": "you are what you eat 이라잖아요 그래서",
          "original_form": "you are what you eat 이라잖아요 그래서",
          "speaker_id": "1",
          "time": "20210907 07:58"
        },
        {
          "id": "MDRW2100042730.1.4",
          "form": "그렇게 먹는거 좋은거같아우",
          "original_form": "그렇게 먹는거 좋은거같아우",
          "speaker_id": "1",
          "time": "20210907 07:59"
        },
        {
          "id": "MDRW2100042730.1.5",
          "form": "아, 트위터에서 본 건데요,",
          "original_form": "아, 트위터에서 본 건데요,",
          "speaker_id": "2",
          "time": "20210907 07:59"
        },
        {
          "id": "MDRW2100042730.1.6",
          "form": "오 네네",
          "original_form": "오 네네",
          "speaker_id": "1",
          "time": "20210907 07:59"
        },
        {
          "id": "MDRW2100042730.1.7",
          "form": "유투브에 스로언_스페인로컬언니라는 채널에서",
          "original_form": "유투브에 스로언_스페인로컬언니라는 채널에서",
          "speaker_id": "2",
          "time": "20210907 07:59"
        },
        {
          "id": "MDRW2100042730.1.8",
          "form": "바게트 샌드위치 레시피를 올렸는데 그게 엄청 맛있나봐요",
          "original_form": "바게트 샌드위치 레시피를 올렸는데 그게 엄청 맛있나봐요",
          "speaker_id": "2",
          "time": "20210907 07:59"
        },
        {
          "id": "MDRW2100042730.1.9",
          "form": "수요일에 바게트 사서 저도 한 번 해 보려고요",
          "original_form": "수요일에 바게트 사서 저도 한 번 해 보려고요",
          "speaker_id": "2",
          "time": "20210907 08:00"
        },
        {
          "id": "MDRW2100042730.1.10",
          "form": "오 그 엄청 간단한 그건가요?",
          "original_form": "오 그 엄청 간단한 그건가요?",
          "speaker_id": "1",
          "time": "20210907 08:00"
        },
        {
          "id": "MDRW2100042730.1.11",
          "form": "저도 스쳐지나가면서 본거같은데",
          "original_form": "저도 스쳐지나가면서 본거같은데",
          "speaker_id": "1",
          "time": "20210907 08:00"
        },
        {
          "id": "MDRW2100042730.1.12",
          "form": "카라멜라이즈드 양파랑 달걀이랑 치즈랑 오믈렛팅 해서",
          "original_form": "카라멜라이즈드 양파랑 달걀이랑 치즈랑 오믈렛팅 해서",
          "speaker_id": "2",
          "time": "20210907 08:00"
        },
        {
          "id": "MDRW2100042730.1.13",
          "form": "바게트에 슝",
          "original_form": "바게트에 슝",
          "speaker_id": "2",
          "time": "20210907 08:00"
        },
        {
          "id": "MDRW2100042730.1.14",
          "form": "아 바게트에 토마토 슥슥 바르고",
          "original_form": "아 바게트에 토마토 슥슥 바르고",
          "speaker_id": "2",
          "time": "20210907 08:01"
        },
        {
          "id": "MDRW2100042730.1.15",
          "form": "올리브오일 뿌리고 올리더라고요",
          "original_form": "올리브오일 뿌리고 올리더라고요",
          "speaker_id": "2",
          "time": "20210907 08:01"
        },
        {
          "id": "MDRW2100042730.1.16",
          "form": "맛없을수 없는맛...",
          "original_form": "맛없을수 없는맛...",
          "speaker_id": "1",
          "time": "20210907 08:01"
        },
        {
          "id": "MDRW2100042730.1.17",
          "form": "양파 카라멜라이즈드 조만간 날잡고 한번에 엄청 많이 해둬야할거같아요 매번 하기가 너무귀찮더라구요",
          "original_form": "양파 카라멜라이즈드 조만간 날잡고 한번에 엄청 많이 해둬야할거같아요 매번 하기가 너무귀찮더라구요",
          "speaker_id": "1",
          "time": "20210907 08:02"
        },
        {
          "id": "MDRW2100042730.1.18",
          "form": "파르미지아노 레지아노를 사던가 해야겠어요",
          "original_form": "파르미지아노 레지아노를 사던가 해야겠어요",
          "speaker_id": "2",
          "time": "20210907 08:02"
        },
        {
          "id": "MDRW2100042730.1.19",
          "form": "그라나 파다노는 안 어울릴 것 같은데 ㅜ",
          "original_form": "그라나 파다노는 안 어울릴 것 같은데 ㅜ",
          "speaker_id": "2",
          "time": "20210907 08:02"
        },
        {
          "id": "MDRW2100042730.1.20",
          "form": "맞아요 카라멜라이즈드 양파 해서 얼려둬야하는데 그거 하려면 양파 따로 사러 가야하고 아악",
          "original_form": "맞아요 카라멜라이즈드 양파 해서 얼려둬야하는데 그거 하려면 양파 따로 사러 가야하고 아악",
          "speaker_id": "2",
          "time": "20210907 08:03"
        },
        {
          "id": "MDRW2100042730.1.21",
          "form": "그쵸 미쳐요",
          "original_form": "그쵸 미쳐요",
          "speaker_id": "1",
          "time": "20210907 08:03"
        },
        {
          "id": "MDRW2100042730.1.22",
          "form": "제 인생의 반은 양파까고 마늘까는데 보낸거같아요",
          "original_form": "제 인생의 반은 양파까고 마늘까는데 보낸거같아요",
          "speaker_id": "1",
          "time": "20210907 08:03"
        },
        {
          "id": "MDRW2100042730.1.23",
          "form": "양파 사서 가고 썰고 하루죙일볶고",
          "original_form": "양파 사서 가고 썰고 하루죙일볶고",
          "speaker_id": "1",
          "time": "20210907 08:04"
        },
        {
          "id": "MDRW2100042730.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:04"
        },
        {
          "id": "MDRW2100042730.1.25",
          "form": "눈물이 ㅋㅋㅋㅋㅋ",
          "original_form": "눈물이 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:04"
        },
        {
          "id": "MDRW2100042730.1.26",
          "form": "마늘도 좋은 거 먹으려면 따로 사서 다 까야하는 ㅜ",
          "original_form": "마늘도 좋은 거 먹으려면 따로 사서 다 까야하는 ㅜ",
          "speaker_id": "2",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100042730.1.27",
          "form": "은근 손이 많이가요 ㅜ",
          "original_form": "은근 손이 많이가요 ㅜ",
          "speaker_id": "1",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100042730.1.28",
          "form": "맞아요 마늘 진짜 까다보면 현타 오지고",
          "original_form": "맞아요 마늘 진짜 까다보면 현타 오지고",
          "speaker_id": "1",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100042730.1.29",
          "form": "햇마늘은 그래도 잘 까지는데",
          "original_form": "햇마늘은 그래도 잘 까지는데",
          "speaker_id": "2",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100042730.1.30",
          "form": "무슨 부귀영화를 누리겟다고 하루종일 이러고잇는가..",
          "original_form": "무슨 부귀영화를 누리겟다고 하루종일 이러고잇는가..",
          "speaker_id": "1",
          "time": "20210907 08:05"
        },
        {
          "id": "MDRW2100042730.1.31",
          "form": "햇마늘은 껍질이 과일처럼 촉촉해서 잘 까지지만 한 철이고",
          "original_form": "햇마늘은 껍질이 과일처럼 촉촉해서 잘 까지지만 한 철이고",
          "speaker_id": "2",
          "time": "20210907 08:06"
        },
        {
          "id": "MDRW2100042730.1.32",
          "form": "깐마늘은 마늘 같지도 않고",
          "original_form": "깐마늘은 마늘 같지도 않고",
          "speaker_id": "2",
          "time": "20210907 08:06"
        },
        {
          "id": "MDRW2100042730.1.33",
          "form": "맞아요 깐마늘 뭔가 불신",
          "original_form": "맞아요 깐마늘 뭔가 불신",
          "speaker_id": "1",
          "time": "20210907 08:06"
        },
        {
          "id": "MDRW2100042730.1.34",
          "form": "결국 시리즈물을 보며 마늘 까는 날을 정해야 하는",
          "original_form": "결국 시리즈물을 보며 마늘 까는 날을 정해야 하는",
          "speaker_id": "2",
          "time": "20210907 08:06"
        }
      ]
