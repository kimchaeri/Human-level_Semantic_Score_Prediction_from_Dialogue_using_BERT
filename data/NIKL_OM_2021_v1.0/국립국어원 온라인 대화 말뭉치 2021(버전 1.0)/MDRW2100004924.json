      "utterance": [
        {
          "id": "MDRW2100004924.1.1",
          "form": "라면하면 우리나라지",
          "original_form": "라면하면 우리나라지",
          "speaker_id": "2",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004924.1.2",
          "form": "응 우리나라 라면은 인트턴트 라면이래",
          "original_form": "응 우리나라 라면은 인트턴트 라면이래",
          "speaker_id": "1",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004924.1.3",
          "form": "된장 라면 해물 라면 맛있겠다",
          "original_form": "된장 라면 해물 라면 맛있겠다",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004924.1.4",
          "form": "티비에 라면 나오면 그렇게 먹고 싶 을수 가 없다",
          "original_form": "티비에 라면 나오면 그렇게 먹고 싶 을수 가 없다",
          "speaker_id": "1",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004924.1.5",
          "form": "응 특히 밤에 나오면",
          "original_form": "응 특히 밤에 나오면",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004924.1.6",
          "form": "맞아 야식으로 라면먹어서 살만 찐다",
          "original_form": "맞아 야식으로 라면먹어서 살만 찐다",
          "speaker_id": "1",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004924.1.7",
          "form": "라면에도 저기는 비법이 있네",
          "original_form": "라면에도 저기는 비법이 있네",
          "speaker_id": "2",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004924.1.8",
          "form": "라면 전문점인가보다 추가 양념장도 만들고 하는거보니",
          "original_form": "라면 전문점인가보다 추가 양념장도 만들고 하는거보니",
          "speaker_id": "1",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004924.1.9",
          "form": "칼칼하게 끓여서 먹는 라면 최고지",
          "original_form": "칼칼하게 끓여서 먹는 라면 최고지",
          "speaker_id": "2",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004924.1.10",
          "form": "난 버섯 넣어도 맛있더라",
          "original_form": "난 버섯 넣어도 맛있더라",
          "speaker_id": "2",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004924.1.11",
          "form": "청 양 고 추 넣어서 먹어도 맛있음",
          "original_form": "청 양 고 추 넣어서 먹어도 맛있음",
          "speaker_id": "1",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004924.1.12",
          "form": "그건 너무 맵지 않겠나",
          "original_form": "그건 너무 맵지 않겠나",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004924.1.13",
          "form": "좀 칼칼해야지 라면 먹는 재미가 있지",
          "original_form": "좀 칼칼해야지 라면 먹는 재미가 있지",
          "speaker_id": "1",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004924.1.14",
          "form": "해물넣으면 더 맛있음",
          "original_form": "해물넣으면 더 맛있음",
          "speaker_id": "2",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004924.1.15",
          "form": "그래 해물모듬 넣으면 엄청 맛있지",
          "original_form": "그래 해물모듬 넣으면 엄청 맛있지",
