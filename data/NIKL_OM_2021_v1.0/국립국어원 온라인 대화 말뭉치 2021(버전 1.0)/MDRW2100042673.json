      "utterance": [
        {
          "id": "MDRW2100042673.1.1",
          "form": "스타벅스에 신메뉴 나온대",
          "original_form": "스타벅스에 신메뉴 나온대",
          "speaker_id": "2",
          "time": "20210907 00:58"
        },
        {
          "id": "MDRW2100042673.1.2",
          "form": "어떤거 나오는데???",
          "original_form": "어떤거 나오는데???",
          "speaker_id": "1",
          "time": "20210907 00:58"
        },
        {
          "id": "MDRW2100042673.1.3",
          "form": "그린 글레이즈드 프라푸치노",
          "original_form": "그린 글레이즈드 프라푸치노",
          "speaker_id": "2",
          "time": "20210907 01:01"
        },
        {
          "id": "MDRW2100042673.1.4",
          "form": "블랙 글레이즈드 라떼",
          "original_form": "블랙 글레이즈드 라떼",
          "speaker_id": "2",
          "time": "20210907 01:01"
        },
        {
          "id": "MDRW2100042673.1.5",
          "form": "오텀 로드 애플 블랙티",
          "original_form": "오텀 로드 애플 블랙티",
          "speaker_id": "2",
          "time": "20210907 01:01"
        },
        {
          "id": "MDRW2100042673.1.6",
          "form": "블랙 글레이즈드 라떼가 엄청 맛있대",
          "original_form": "블랙 글레이즈드 라떼가 엄청 맛있대",
          "speaker_id": "2",
          "time": "20210907 01:02"
        },
        {
          "id": "MDRW2100042673.1.7",
          "form": "무슨 맛이래???",
          "original_form": "무슨 맛이래???",
          "speaker_id": "1",
          "time": "20210907 01:02"
        },
        {
          "id": "MDRW2100042673.1.8",
          "form": "흑당 밀크티같은 맛이라는디",
          "original_form": "흑당 밀크티같은 맛이라는디",
          "speaker_id": "2",
          "time": "20210907 01:09"
        },
        {
          "id": "MDRW2100042673.1.9",
          "form": "디카페인도 되려나",
          "original_form": "디카페인도 되려나",
          "speaker_id": "1",
          "time": "20210907 01:09"
        },
        {
          "id": "MDRW2100042673.1.10",
          "form": "디카페인도 된대",
          "original_form": "디카페인도 된대",
          "speaker_id": "2",
          "time": "20210907 01:09"
        },
        {
          "id": "MDRW2100042673.1.11",
          "form": "그럼 내일 한번 먹어볼까",
          "original_form": "그럼 내일 한번 먹어볼까",
          "speaker_id": "1",
          "time": "20210907 01:10"
        },
        {
          "id": "MDRW2100042673.1.12",
          "form": "나 기프티콘 두개있어",
          "original_form": "나 기프티콘 두개있어",
          "speaker_id": "2",
          "time": "20210907 01:10"
        },
        {
          "id": "MDRW2100042673.1.13",
          "form": "너는 뭐먹을거야",
          "original_form": "너는 뭐먹을거야",
          "speaker_id": "1",
          "time": "20210907 01:11"
        },
        {
          "id": "MDRW2100042673.1.14",
          "form": "나는 무조건 프라프치노 찬거",
          "original_form": "나는 무조건 프라프치노 찬거",
          "speaker_id": "2",
          "time": "20210907 01:11"
        },
        {
          "id": "MDRW2100042673.1.15",
          "form": "그럼 학원끝나고 집앞에서 사서 집에와",
          "original_form": "그럼 학원끝나고 집앞에서 사서 집에와",
          "speaker_id": "1",
          "time": "20210907 01:12"
        },
        {
          "id": "MDRW2100042673.1.16",
          "form": "귀찮지만 그렇게 할게",
          "original_form": "귀찮지만 그렇게 할게",
