      "utterance": [
        {
          "id": "MDRW2100043913.1.1",
          "form": "랍스타 살 먹었을때",
          "original_form": "랍스타 살 먹었을때",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043913.1.2",
          "form": "일반 게보다 많아서 넘 놀랐던",
          "original_form": "일반 게보다 많아서 넘 놀랐던",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043913.1.3",
          "form": "한번 뿐이지만..",
          "original_form": "한번 뿐이지만..",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043913.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 대게도 비슷하지 않아?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 대게도 비슷하지 않아?",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043913.1.5",
          "form": "이상하게 내가 먹은 대게는 그닥 살이 없던 느낌..",
          "original_form": "이상하게 내가 먹은 대게는 그닥 살이 없던 느낌..",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043913.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 뷔페에 대게 있는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 뷔페에 대게 있는데",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043913.1.7",
          "form": "평이 괜찮더라",
          "original_form": "평이 괜찮더라",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043913.1.8",
          "form": "이번 기회에 통통한 대게 함 먹어봐",
          "original_form": "이번 기회에 통통한 대게 함 먹어봐",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043913.1.9",
          "form": "오 조아조어",
          "original_form": "오 조아조어",
          "speaker_id": "2",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043913.1.10",
          "form": "맨날 티비에서만 통통한 대게 봤어",
          "original_form": "맨날 티비에서만 통통한 대게 봤어",
          "speaker_id": "2",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043913.1.11",
          "form": "남들은 젓가락으로 살 퍼먹던데",
          "original_form": "남들은 젓가락으로 살 퍼먹던데",
          "speaker_id": "2",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043913.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043913.1.13",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043913.1.14",
          "form": "나쁜말...",
          "original_form": "나쁜말...",
          "speaker_id": "2",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043913.1.15",
          "form": "대체 뭐가",
          "original_form": "대체 뭐가",
          "speaker_id": "2",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043913.1.16",
          "form": "찐으로 통통한 게는 살을 퍼먹지 않고",
          "original_form": "찐으로 통통한 게는 살을 퍼먹지 않고",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043913.1.17",
          "form": "껍데기 빼내면",
          "original_form": "껍데기 빼내면",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043913.1.18",
          "form": "게살을 나도 쭈욱 밀어내서",
          "original_form": "게살을 나도 쭈욱 밀어내서",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043913.1.19",
          "form": "먹고싶다",
          "original_form": "먹고싶다",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043913.1.20",
          "form": "살이 게맛살처럼 쏙 나오지",
          "original_form": "살이 게맛살처럼 쏙 나오지",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043913.1.21",
          "form": "ㅇㅇ...",
          "original_form": "ㅇㅇ...",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043913.1.22",
          "form": "그렇게 먹어보고 싶어",
          "original_form": "그렇게 먹어보고 싶어",
          "speaker_id": "2",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043913.1.23",
          "form": "그랭 그렇게 먹고",
          "original_form": "그랭 그렇게 먹고",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043913.1.24",
          "form": "집게발 크로스 샷도 기념으로 찍자 ㅋㅋㅋㅋ",
          "original_form": "집게발 크로스 샷도 기념으로 찍자 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043913.1.25",
          "form": "ㅋㅋㅋㅋ 조아조아",
          "original_form": "ㅋㅋㅋㅋ 조아조아",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043913.1.26",
          "form": "못 먹어봣던거 다 먹어야지",
          "original_form": "못 먹어봣던거 다 먹어야지",
          "speaker_id": "2",
          "time": "20210907 20:55"
        },
        {
          "id": "MDRW2100043913.1.27",
          "form": "아침은 뭐먹고잇어?",
          "original_form": "아침은 뭐먹고잇어?",
          "speaker_id": "1",
