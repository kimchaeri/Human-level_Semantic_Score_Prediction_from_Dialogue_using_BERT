      "utterance": [
        {
          "id": "MDRW2100030991.1.1",
          "form": "최근 산 물건 잇으신가요",
          "original_form": "최근 산 물건 잇으신가요",
          "speaker_id": "2",
          "time": "20210904 16:36"
        },
        {
          "id": "MDRW2100030991.1.2",
          "form": "최근에 요가메트 샀어요!",
          "original_form": "최근에 요가메트 샀어요!",
          "speaker_id": "1",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100030991.1.3",
          "form": "name2님은 최근 쇼핑 어떤거 하셨나요?",
          "original_form": "&name2&님은 최근 쇼핑 어떤거 하셨나요?",
          "speaker_id": "1",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100030991.1.4",
          "form": "오 운동하시게요?",
          "original_form": "오 운동하시게요?",
          "speaker_id": "2",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100030991.1.5",
          "form": "전 마사지볼 !",
          "original_form": "전 마사지볼 !",
          "speaker_id": "2",
          "time": "20210904 16:38"
        },
        {
          "id": "MDRW2100030991.1.6",
          "form": "홈트레이닝을 좀 할까해서 구매했어요",
          "original_form": "홈트레이닝을 좀 할까해서 구매했어요",
          "speaker_id": "1",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100030991.1.7",
          "form": "마사지볼!",
          "original_form": "마사지볼!",
          "speaker_id": "1",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100030991.1.8",
          "form": "어떤가요?",
          "original_form": "어떤가요?",
          "speaker_id": "1",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100030991.1.9",
          "form": "오땅콩볼이여",
          "original_form": "오땅콩볼이여",
          "speaker_id": "2",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100030991.1.10",
          "form": "아세요?",
          "original_form": "아세요?",
          "speaker_id": "2",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100030991.1.11",
          "form": "아뇨 처음들어봐요!",
          "original_form": "아뇨 처음들어봐요!",
          "speaker_id": "1",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100030991.1.12",
          "form": "ㅋㅋㅋㅋ 땅콩처럼 생겻는데 마사지할수잇어요 폼롤러처럼",
          "original_form": "ㅋㅋㅋㅋ 땅콩처럼 생겻는데 마사지할수잇어요 폼롤러처럼",
          "speaker_id": "2",
          "time": "20210904 17:19"
        },
        {
          "id": "MDRW2100030991.1.13",
          "form": "옹... 주로 어디를 안마하면 좋나요?",
          "original_form": "옹... 주로 어디를 안마하면 좋나요?",
          "speaker_id": "1",
          "time": "20210904 17:22"
        },
        {
          "id": "MDRW2100030991.1.14",
          "form": "전 어깨 등이요",
          "original_form": "전 어깨 등이요",
          "speaker_id": "2",
          "time": "20210904 17:39"
        },
        {
          "id": "MDRW2100030991.1.15",
          "form": "한곳 공략하기 조아요",
          "original_form": "한곳 공략하기 조아요",
          "speaker_id": "2",
          "time": "20210904 17:39"
        },
        {
          "id": "MDRW2100030991.1.16",
          "form": "집중 공략하는 목적이군요",
          "original_form": "집중 공략하는 목적이군요",
          "speaker_id": "1",
          "time": "20210904 17:43"
        },
        {
          "id": "MDRW2100030991.1.17",
          "form": "넹 ㅋㅋ 뭉칭근육풀기",
          "original_form": "넹 ㅋㅋ 뭉칭근육풀기",
          "speaker_id": "2",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100030991.1.18",
          "form": "요새 갖고 싶으셨던 물건은 어떤게 있으신가요?",
          "original_form": "요새 갖고 싶으셨던 물건은 어떤게 있으신가요?",
          "speaker_id": "1",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100030991.1.19",
          "form": "지플립이요",
          "original_form": "지플립이요",
          "speaker_id": "2",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100030991.1.20",
          "form": "넘이쁨",
          "original_form": "넘이쁨",
          "speaker_id": "2",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100030991.1.21",
          "form": "맞아요 진짜 예뻐요!",
          "original_form": "맞아요 진짜 예뻐요!",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100030991.1.22",
          "form": "어떤 색이 가장 예쁘던가요?",
          "original_form": "어떤 색이 가장 예쁘던가요?",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100030991.1.23",
          "form": "ㅠㅡㅜ 돈도없고 ㅋㅋㅋ구하기도힘들고",
          "original_form": "ㅠㅡㅜ 돈도없고 ㅋㅋㅋ구하기도힘들고",
          "speaker_id": "2",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100030991.1.24",
          "form": "크림 라벤더용",
          "original_form": "크림 라벤더용",
          "speaker_id": "2",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100030991.1.25",
          "form": "저는 크림이랑 라벤더요 ㅠ",
          "original_form": "저는 크림이랑 라벤더요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100030991.1.26",
          "form": "ㅋㅋㅋㅋ 앗",
          "original_form": "ㅋㅋㅋㅋ 앗",
          "speaker_id": "2",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100030991.1.27",
          "form": "사전 예약이라도 해서 살걸 싶었어요 ㅠ",
          "original_form": "사전 예약이라도 해서 살걸 싶었어요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100030991.1.28",
          "form": "ㅠㅠ탐나요",
          "original_form": "ㅠㅠ탐나요",
          "speaker_id": "2",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100030991.1.29",
          "form": "지플립 사실건가오",
          "original_form": "지플립 사실건가오",
          "speaker_id": "2",
          "time": "20210904 17:46"
        }
      ]
    }
  ]
