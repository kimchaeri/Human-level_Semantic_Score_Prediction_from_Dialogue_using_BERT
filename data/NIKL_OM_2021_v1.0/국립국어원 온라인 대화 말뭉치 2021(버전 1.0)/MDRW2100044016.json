      "utterance": [
        {
          "id": "MDRW2100044016.1.1",
          "form": "사실 다 좋아해요",
          "original_form": "사실 다 좋아해요",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100044016.1.2",
          "form": "까다로운 가족입맛에 맞춰야죠",
          "original_form": "까다로운 가족입맛에 맞춰야죠",
          "speaker_id": "2",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044016.1.3",
          "form": "ㅋㅋㅋㅋㅋ저도 시원한맛좋아하긴하는데",
          "original_form": "ㅋㅋㅋㅋㅋ저도 시원한맛좋아하긴하는데",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044016.1.4",
          "form": "젓갈만나는것도 땡길때있고그래여",
          "original_form": "젓갈만나는것도 땡길때있고그래여",
          "speaker_id": "1",
          "time": "20210907 21:17"
        },
        {
          "id": "MDRW2100044016.1.5",
          "form": "전라도 김치는 굴 넣는대섯",
          "original_form": "전라도 김치는 굴 넣는대섯",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044016.1.6",
          "form": "한번 먹어보고싶긴하네요",
          "original_form": "한번 먹어보고싶긴하네요",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100044016.1.7",
          "form": "경상도도 굴넣어여 ㅎㅎ저희집은 경상도인데 김치는 전라도거 사먹어요 ㅋㅋㅋㅋㅋ",
          "original_form": "경상도도 굴넣어여 ㅎㅎ저희집은 경상도인데 김치는 전라도거 사먹어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:21"
        },
        {
          "id": "MDRW2100044016.1.8",
          "form": "앗 저희집도 경상도인데 저말고는 굴 아무도 안좋아하더라구요",
          "original_form": "앗 저희집도 경상도인데 저말고는 굴 아무도 안좋아하더라구요",
          "speaker_id": "2",
          "time": "20210907 21:23"
        },
        {
          "id": "MDRW2100044016.1.9",
          "form": "젓갈도 싫어해서ㅋㄱᆞ",
          "original_form": "젓갈도 싫어해서ㅋㄱᆞ",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044016.1.10",
          "form": "아하 그래서 굴을안넣었군여",
          "original_form": "아하 그래서 굴을안넣었군여",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044016.1.11",
          "form": "네 아무래도 음식하는 엄마입맛위주라ㅋ",
          "original_form": "네 아무래도 음식하는 엄마입맛위주라ㅋ",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044016.1.12",
          "form": "최근에 뭐사셨어요?",
          "original_form": "최근에 뭐사셨어요?",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100044016.1.13",
          "form": "최근에 산건 옷이랑 조만간 버즈2살예정이에요",
          "original_form": "최근에 산건 옷이랑 조만간 버즈2살예정이에요",
          "speaker_id": "1",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100044016.1.14",
          "form": "와 버즈가 이어폰이죠?",
          "original_form": "와 버즈가 이어폰이죠?",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044016.1.15",
          "form": "1과 2의 차이점은 뭔가요?",
          "original_form": "1과 2의 차이점은 뭔가요?",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100044016.1.16",
          "form": "네 저도잘 ㅋㅋㅋㅋㅋㅋ전 버즈라이브쓰거든여 동생거사줄려다보니 그게젤최근에나왔대요",
          "original_form": "네 저도잘 ㅋㅋㅋㅋㅋㅋ전 버즈라이브쓰거든여 동생거사줄려다보니 그게젤최근에나왔대요",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100044016.1.17",
          "form": "아 동생선물이군요ㅋㅋ",
          "original_form": "아 동생선물이군요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044016.1.18",
          "form": "동생이 너무 부러운데요?",
          "original_form": "동생이 너무 부러운데요?",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044016.1.19",
          "form": "네네 ㅎㅎㅎ",
          "original_form": "네네 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044016.1.20",
          "form": "ㅋㅋㅋ돈받았어요 부러워안하샤도됩니다",
          "original_form": "ㅋㅋㅋ돈받았어요 부러워안하샤도됩니다",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100044016.1.21",
          "form": "ㅋㅋ아 구매대행이군요",
          "original_form": "ㅋㅋ아 구매대행이군요",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100044016.1.22",
          "form": "잠시 못된 누나인걸 반성할뻔했어요",
          "original_form": "잠시 못된 누나인걸 반성할뻔했어요",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100044016.1.23",
          "form": "네 ㅋㅋㅋㅋㅋㅋ대신구매해주는",
          "original_form": "네 ㅋㅋㅋㅋㅋㅋ대신구매해주는",
          "speaker_id": "1",
