        {
          "id": "MDRW2100044141.1.2",
          "form": "맞으셨어요??",
          "original_form": "맞으셨어요??",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044141.1.3",
          "form": "전 다 맞았어요",
          "original_form": "전 다 맞았어요",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044141.1.4",
          "form": "빠르시네요!",
          "original_form": "빠르시네요!",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044141.1.5",
          "form": "잔여요?",
          "original_form": "잔여요?",
          "speaker_id": "2",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044141.1.6",
          "form": "전 관련 업무 종사자라",
          "original_form": "전 관련 업무 종사자라",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044141.1.7",
          "form": "맞았어요",
          "original_form": "맞았어요",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044141.1.8",
          "form": "요즘 얼굴에 팩 올리고 이거 합니다 ㅎ",
          "original_form": "요즘 얼굴에 팩 올리고 이거 합니다 ㅎ",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100044141.1.9",
          "form": "ㅋㅋ진짜 아어이다할때 너무 신나요!!!",
          "original_form": "ㅋㅋ진짜 아어이다할때 너무 신나요!!!",
          "speaker_id": "2",
          "time": "20210907 22:12"
        },
        {
          "id": "MDRW2100044141.1.10",
          "form": "아어이다가 뭔가요?",
          "original_form": "아어이다가 뭔가요?",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044141.1.11",
          "form": "상대방과 대화가 딱딱 맞을때?",
          "original_form": "상대방과 대화가 딱딱 맞을때?",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044141.1.12",
          "form": "예전에 무도 상황극에서 나왔대요 유래는ㅋㅋ",
          "original_form": "예전에 무도 상황극에서 나왔대요 유래는ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:19"
        },
        {
          "id": "MDRW2100044141.1.13",
          "form": "아하!! 그렇군요 ㅎ",
          "original_form": "아하!! 그렇군요 ㅎ",
          "speaker_id": "1",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044141.1.14",
          "form": "ㅋㅋ 백신 그래도 다 맞으니 속 시원하시죠?",
          "original_form": "ㅋㅋ 백신 그래도 다 맞으니 속 시원하시죠?",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100044141.1.15",
          "form": "그럼요~ 후유증도 없었고요",
          "original_form": "그럼요~ 후유증도 없었고요",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044141.1.16",
          "form": "다행이에요!",
          "original_form": "다행이에요!",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044141.1.17",
          "form": "제주위도 아직 없어요 심각한 후유증은",
          "original_form": "제주위도 아직 없어요 심각한 후유증은",
