      "utterance": [
        {
          "id": "MDRW2100043267.1.1",
          "form": "아하 이동하려면 확실히 날이 좋아야죠!",
          "original_form": "아하 이동하려면 확실히 날이 좋아야죠!",
          "speaker_id": "2",
          "time": "20210907 17:08"
        },
        {
          "id": "MDRW2100043267.1.2",
          "form": "네네 ㅎㅎ 집앞 카페를 가도",
          "original_form": "네네 ㅎㅎ 집앞 카페를 가도",
          "speaker_id": "1",
          "time": "20210907 17:09"
        },
        {
          "id": "MDRW2100043267.1.3",
          "form": "날이 좋아야 기분이 좋자나용 ㅎㅎ",
          "original_form": "날이 좋아야 기분이 좋자나용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 17:09"
        },
        {
          "id": "MDRW2100043267.1.4",
          "form": "그쵸 ㅋㅋㅋ 맛집탐방 가시나여?",
          "original_form": "그쵸 ㅋㅋㅋ 맛집탐방 가시나여?",
          "speaker_id": "2",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100043267.1.5",
          "form": "네네 ~",
          "original_form": "네네 ~",
          "speaker_id": "1",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100043267.1.6",
          "form": "술도 한잔하구요",
          "original_form": "술도 한잔하구요",
          "speaker_id": "1",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100043267.1.7",
          "form": "ㅎㅎ 한잔 좋죵~",
          "original_form": "ㅎㅎ 한잔 좋죵~",
          "speaker_id": "2",
          "time": "20210907 19:30"
        },
        {
          "id": "MDRW2100043267.1.8",
          "form": "여행가고싶으신곳 있나요",
          "original_form": "여행가고싶으신곳 있나요",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043267.1.9",
          "form": "오늘 조개구이가 먹고싶더라구요 ㅋㅋㅋ",
          "original_form": "오늘 조개구이가 먹고싶더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100043267.1.10",
          "form": "부산이나 포항같은데 가고싶엇어요",
          "original_form": "부산이나 포항같은데 가고싶엇어요",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100043267.1.11",
          "form": "오 둘다 가보셨나요?",
          "original_form": "오 둘다 가보셨나요?",
          "speaker_id": "1",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100043267.1.12",
          "form": "예전에 한번씩 가서 조개구이 먹어봤어요 ㅎㅎ",
          "original_form": "예전에 한번씩 가서 조개구이 먹어봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100043267.1.13",
          "form": "오 ㅎㅎ 둘다 회 조개구이 먹기좋죠",
          "original_form": "오 ㅎㅎ 둘다 회 조개구이 먹기좋죠",
          "speaker_id": "1",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100043267.1.14",
          "form": "맞아요 ㅋㅋㅋ바닷가에서 먹는게 젤 맛있는것 같아요",
          "original_form": "맞아요 ㅋㅋㅋ바닷가에서 먹는게 젤 맛있는것 같아요",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100043267.1.15",
          "form": "진짜 그 바닷공기가 무시못해요",
          "original_form": "진짜 그 바닷공기가 무시못해요",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
