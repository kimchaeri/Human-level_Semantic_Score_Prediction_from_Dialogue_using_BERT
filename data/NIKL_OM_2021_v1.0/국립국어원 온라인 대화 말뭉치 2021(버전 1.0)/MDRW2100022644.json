      "utterance": [
        {
          "id": "MDRW2100022644.1.1",
          "form": "치킨좋아하세요?",
          "original_form": "치킨좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022644.1.2",
          "form": "네 ㅋㅋ 저 치킨 환장해요",
          "original_form": "네 ㅋㅋ 저 치킨 환장해요",
          "speaker_id": "1",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022644.1.3",
          "form": "1주일에 1번 이상 꼭 먹어요",
          "original_form": "1주일에 1번 이상 꼭 먹어요",
          "speaker_id": "1",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022644.1.4",
          "form": "무슨 치킨브랜드 좋아해요?",
          "original_form": "무슨 치킨브랜드 좋아해요?",
          "speaker_id": "1",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022644.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ저두요 ㅠㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저두요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022644.1.6",
          "form": "배달 최애음식 치킨",
          "original_form": "배달 최애음식 치킨",
          "speaker_id": "2",
          "time": "20210829 16:43"
        },
        {
          "id": "MDRW2100022644.1.7",
          "form": "저는 다양하게 먹긴하는데",
          "original_form": "저는 다양하게 먹긴하는데",
          "speaker_id": "2",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022644.1.8",
          "form": "요즘엔 바른치킨 맛잇더라구요",
          "original_form": "요즘엔 바른치킨 맛잇더라구요",
          "speaker_id": "2",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022644.1.9",
          "form": "바른치킨",
          "original_form": "바른치킨",
          "speaker_id": "1",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022644.1.10",
          "form": "배민에서 매주 수/일 할인 하는 거",
          "original_form": "배민에서 매주 수/일 할인 하는 거",
          "speaker_id": "1",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022644.1.11",
          "form": "그거 맞죠 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그거 맞죠 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:44"
        },
        {
          "id": "MDRW2100022644.1.12",
          "form": "아 할인도라더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 할인도라더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022644.1.13",
          "form": "네?ㅎㅎ",
          "original_form": "네?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022644.1.14",
          "form": "근데 할인은못먹어봣우요 ㅠㅠ 문닫혀잇어서요",
          "original_form": "근데 할인은못먹어봣우요 ㅠㅠ 문닫혀잇어서요",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022644.1.15",
          "form": "아 할인 매주 하죠 ㅎ",
          "original_form": "아 할인 매주 하죠 ㅎ",
          "speaker_id": "1",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022644.1.16",
          "form": "그 가게가 할인하는 날만 문 닫는거 아니에요?ㅜ",
          "original_form": "그 가게가 할인하는 날만 문 닫는거 아니에요?ㅜ",
          "speaker_id": "1",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022644.1.17",
          "form": "그런거같아녀 ㅋㅋㅋㅋ",
          "original_form": "그런거같아녀 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022644.1.18",
          "form": "저는 피피엘보고 맛잇어보여섴ㅋㅋㅋㅋㅋ",
          "original_form": "저는 피피엘보고 맛잇어보여섴ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022644.1.19",
          "form": "오늘 일요일인데 오늘 기회되면 시켜봐요",
          "original_form": "오늘 일요일인데 오늘 기회되면 시켜봐요",
          "speaker_id": "1",
          "time": "20210829 16:45"
        },
        {
          "id": "MDRW2100022644.1.20",
          "form": "아니 근데 죄다 배달비 붙어버리니 할인해도 하는 거 같지가 않아요..",
          "original_form": "아니 근데 죄다 배달비 붙어버리니 할인해도 하는 거 같지가 않아요..",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022644.1.21",
          "form": "아 그건그래요 ㅠ 교촌 삼천원으러 올앗다면서요?",
          "original_form": "아 그건그래요 ㅠ 교촌 삼천원으러 올앗다면서요?",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022644.1.22",
          "form": "교촌 ^^",
          "original_form": "교촌 ^^",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022644.1.23",
          "form": "하 배달비의 시초아닌가요",
          "original_form": "하 배달비의 시초아닌가요",
          "speaker_id": "2",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022644.1.24",
          "form": "이 프렌차이즈 덕분에 배달비라는 게 생긴거잖아요",
          "original_form": "이 프렌차이즈 덕분에 배달비라는 게 생긴거잖아요",
          "speaker_id": "1",
          "time": "20210829 16:46"
        },
        {
          "id": "MDRW2100022644.1.25",
          "form": "그때부터 지금까지 쭉 불매운동 중",
          "original_form": "그때부터 지금까지 쭉 불매운동 중",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022644.1.26",
          "form": "그러니까요 치킨도 보면 싼가격은아닌데 ㅠㅠ",
          "original_form": "그러니까요 치킨도 보면 싼가격은아닌데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022644.1.27",
          "form": "ㅠㅠ근데 집 창문 열면 교촌이 보이더라구요...?",
          "original_form": "ㅠㅠ근데 집 창문 열면 교촌이 보이더라구요...?",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022644.1.28",
          "form": "가끔 포장의 유혹이...",
          "original_form": "가끔 포장의 유혹이...",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022644.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ아 냄새유혹잇겟어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 냄새유혹잇겟어요",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022644.1.30",
          "form": "그래서 딱 한 번 포장주문 해보긴 했어요",
          "original_form": "그래서 딱 한 번 포장주문 해보긴 했어요",
          "speaker_id": "1",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022644.1.31",
          "form": "좋아하는 치킨 브랜드잇어요?",
          "original_form": "좋아하는 치킨 브랜드잇어요?",
          "speaker_id": "2",
          "time": "20210829 16:47"
        },
        {
          "id": "MDRW2100022644.1.32",
          "form": "역시 교촌은 교촌이구나 싶었어요 ㅋㅋㅋㅋ",
          "original_form": "역시 교촌은 교촌이구나 싶었어요 ㅋㅋㅋㅋ",
