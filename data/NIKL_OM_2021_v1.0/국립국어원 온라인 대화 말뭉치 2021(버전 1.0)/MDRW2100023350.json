      "utterance": [
        {
          "id": "MDRW2100023350.1.1",
          "form": "진짜 어릴때 고생해봐야하나봐요ㅎ",
          "original_form": "진짜 어릴때 고생해봐야하나봐요ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:36"
        },
        {
          "id": "MDRW2100023350.1.2",
          "form": "음 그건 아닌더 같아요!!",
          "original_form": "음 그건 아닌더 같아요!!",
          "speaker_id": "1",
          "time": "20210830 00:36"
        },
        {
          "id": "MDRW2100023350.1.3",
          "form": "아그래요?",
          "original_form": "아그래요?",
          "speaker_id": "2",
          "time": "20210830 00:36"
        },
        {
          "id": "MDRW2100023350.1.4",
          "form": "전 어릴때부터 고생안해봐서",
          "original_form": "전 어릴때부터 고생안해봐서",
          "speaker_id": "2",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100023350.1.5",
          "form": "어릴땐  그나이답게",
          "original_form": "어릴땐  그나이답게",
          "speaker_id": "1",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100023350.1.6",
          "form": "그게.좋은거 같아여 ㅎㅎ",
          "original_form": "그게.좋은거 같아여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100023350.1.7",
          "form": "철이없어요ㅠ",
          "original_form": "철이없어요ㅠ",
          "speaker_id": "2",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100023350.1.8",
          "form": "남자들은 다체로 없어요 ㅋ",
          "original_form": "남자들은 다체로 없어요 ㅋ",
          "speaker_id": "1",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100023350.1.9",
          "form": "음 이런이애기하기 좀 그런데",
          "original_form": "음 이런이애기하기 좀 그런데",
          "speaker_id": "2",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100023350.1.10",
          "form": "뚝해요ㅠㅋㅋ",
          "original_form": "뚝해요ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100023350.1.11",
          "form": "아버님이 대기업다니시고",
          "original_form": "아버님이 대기업다니시고",
          "speaker_id": "2",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100023350.1.12",
          "form": "어릴때부터 그렇게 부유하지는 않지만",
          "original_form": "어릴때부터 그렇게 부유하지는 않지만",
          "speaker_id": "2",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100023350.1.13",
          "form": "네넹~",
          "original_form": "네넹~",
          "speaker_id": "1",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023350.1.14",
          "form": "그냥 남들사는것 처럼 살았어요",
          "original_form": "그냥 남들사는것 처럼 살았어요",
          "speaker_id": "2",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023350.1.15",
          "form": "그래서 철이없고 아르바이트하면",
          "original_form": "그래서 철이없고 아르바이트하면",
          "speaker_id": "2",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023350.1.16",
          "form": "아버지가 엄청 혼냈었어요",
          "original_form": "아버지가 엄청 혼냈었어요",
          "speaker_id": "2",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023350.1.17",
          "form": "아 그러셧구낭",
          "original_form": "아 그러셧구낭",
          "speaker_id": "1",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023350.1.18",
          "form": "대학교때도 학비가 160이었는데 다 회사에서 지원됫구요",
          "original_form": "대학교때도 학비가 160이었는데 다 회사에서 지원됫구요",
          "speaker_id": "2",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023350.1.19",
          "form": "그래서 경제적으로 어려움없이 자랐는데",
          "original_form": "그래서 경제적으로 어려움없이 자랐는데",
          "speaker_id": "2",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100023350.1.20",
          "form": "일하고 결혼하고 하니깐 이제",
          "original_form": "일하고 결혼하고 하니깐 이제",
          "speaker_id": "2",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023350.1.21",
          "form": "철이 없다고 느낀게 그동안 경제적으로 너무 무지했던거에요",
          "original_form": "철이 없다고 느낀게 그동안 경제적으로 너무 무지했던거에요",
          "speaker_id": "2",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023350.1.22",
          "form": "어젠가 제가 이야기햇던게 있잖아요ㅎ 3개월 지출내역ㅎ",
          "original_form": "어젠가 제가 이야기햇던게 있잖아요ㅎ 3개월 지출내역ㅎ",
          "speaker_id": "2",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023350.1.23",
          "form": "음 그럴수 잇어요",
          "original_form": "음 그럴수 잇어요",
          "speaker_id": "1",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023350.1.24",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100023350.1.25",
          "form": "저축률나오는데... 정말 창피해서",
          "original_form": "저축률나오는데... 정말 창피해서",
          "speaker_id": "2",
          "time": "20210830 00:40"
        },
        {
          "id": "MDRW2100023350.1.26",
          "form": "저축률이 얼마나 되요",
          "original_form": "저축률이 얼마나 되요",
          "speaker_id": "1",
          "time": "20210830 00:40"
        },
        {
          "id": "MDRW2100023350.1.27",
          "form": "6월부터 8월까지 마이너스 120만원씩",
          "original_form": "6월부터 8월까지 마이너스 120만원씩",
          "speaker_id": "2",
          "time": "20210830 00:40"
        },
        {
          "id": "MDRW2100023350.1.28",
          "form": "흐규",
          "original_form": "흐규",
          "speaker_id": "1",
          "time": "20210830 00:40"
        },
        {
          "id": "MDRW2100023350.1.29",
          "form": "계속 적립됬더라구요",
          "original_form": "계속 적립됬더라구요",
          "speaker_id": "2",
          "time": "20210830 00:40"
        },
        {
          "id": "MDRW2100023350.1.30",
          "form": "뭐 제가 쓴것도 있고 아내가 쓴것도있고",
          "original_form": "뭐 제가 쓴것도 있고 아내가 쓴것도있고",
          "speaker_id": "2",
          "time": "20210830 00:40"
        },
        {
          "id": "MDRW2100023350.1.31",
          "form": "심각 하긴 해요",
          "original_form": "심각 하긴 해요",
          "speaker_id": "1",
          "time": "20210830 00:40"
        },
        {
          "id": "MDRW2100023350.1.32",
          "form": "전 진짜 뭘안사는데 아이에게 들어가는 돈이 엄청나요ㅠ",
          "original_form": "전 진짜 뭘안사는데 아이에게 들어가는 돈이 엄청나요ㅠ",
          "speaker_id": "2",
          "time": "20210830 00:41"
        },
        {
          "id": "MDRW2100023350.1.33",
          "form": "그럼 그 마이너스는 어디서 채워요?",
          "original_form": "그럼 그 마이너스는 어디서 채워요?",
          "speaker_id": "1",
          "time": "20210830 00:41"
        },
        {
          "id": "MDRW2100023350.1.34",
          "form": "그러니깐 힘들어요ㅠ",
          "original_form": "그러니깐 힘들어요ㅠ",
          "speaker_id": "2",
          "time": "20210830 00:41"
        },
        {
          "id": "MDRW2100023350.1.35",
          "form": "갚아야하는데 계속 늘어나니깐요ㅠ",
          "original_form": "갚아야하는데 계속 늘어나니깐요ㅠ",
          "speaker_id": "2",
          "time": "20210830 00:41"
        },
        {
          "id": "MDRW2100023350.1.36",
          "form": "그냥 빛?",
          "original_form": "그냥 빛?",
          "speaker_id": "1",
          "time": "20210830 00:41"
        },
        {
          "id": "MDRW2100023350.1.37",
          "form": "결국 연말에 보너스나 중간중간에 50프로 씩 성과금나오거든요",
          "original_form": "결국 연말에 보너스나 중간중간에 50프로 씩 성과금나오거든요",
          "speaker_id": "2",
          "time": "20210830 00:41"
        },
        {
          "id": "MDRW2100023350.1.38",
          "form": "네넹",
          "original_form": "네넹",
          "speaker_id": "1",
          "time": "20210830 00:41"
        },
        {
          "id": "MDRW2100023350.1.39",
          "form": "그걸로 일부 충당하는편이에요",
          "original_form": "그걸로 일부 충당하는편이에요",
          "speaker_id": "2",
          "time": "20210830 00:41"
        }
      ]
