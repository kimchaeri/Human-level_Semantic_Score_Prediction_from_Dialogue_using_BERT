      "utterance": [
        {
          "id": "MDRW2100016852.1.1",
          "form": "인공지능 앞으로 어떨까요?",
          "original_form": "인공지능 앞으로 어떨까요?",
          "speaker_id": "1",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016852.1.2",
          "form": "저도 잘 몰라요ㅋ",
          "original_form": "저도 잘 몰라요ㅋ",
          "speaker_id": "1",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016852.1.3",
          "form": "인공지능 교육이 활발한가봐요",
          "original_form": "인공지능 교육이 활발한가봐요",
          "speaker_id": "1",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100016852.1.4",
          "form": "따로 학과가 생길만큼 학문분과가 뚜렷한건지",
          "original_form": "따로 학과가 생길만큼 학문분과가 뚜렷한건지",
          "speaker_id": "2",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100016852.1.5",
          "form": "미용학과처럼 니즈에 따른건지 잘 모르겠어요",
          "original_form": "미용학과처럼 니즈에 따른건지 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100016852.1.6",
          "form": "10년뒤엔 인공지능세상이 될듯요",
          "original_form": "10년뒤엔 인공지능세상이 될듯요",
          "speaker_id": "1",
          "time": "20210826 15:54"
        },
        {
          "id": "MDRW2100016852.1.7",
          "form": "지금도 쇼핑몰등은 상담사를 대체 중인데요 뭐 ㅋㅋ",
          "original_form": "지금도 쇼핑몰등은 상담사를 대체 중인데요 뭐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:54"
        },
        {
          "id": "MDRW2100016852.1.8",
          "form": "근데 학과는 진짜 모르겠네요",
          "original_form": "근데 학과는 진짜 모르겠네요",
          "speaker_id": "2",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100016852.1.9",
          "form": "대학도 이제 과가 바뀔것같아요",
          "original_form": "대학도 이제 과가 바뀔것같아요",
          "speaker_id": "1",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100016852.1.10",
          "form": "그때 그때 유행따라 간판만 바꿔다는데가 마나서",
          "original_form": "그때 그때 유행따라 간판만 바꿔다는데가 마나서",
          "speaker_id": "2",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016852.1.11",
          "form": "저 학과들 커리를 봐야 알겠는데 지금은 잘모르겠어요",
          "original_form": "저 학과들 커리를 봐야 알겠는데 지금은 잘모르겠어요",
          "speaker_id": "2",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016852.1.12",
          "form": "앞으로 전망이 좋아서 우후죽순 생겨날듯요",
          "original_form": "앞으로 전망이 좋아서 우후죽순 생겨날듯요",
          "speaker_id": "1",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016852.1.13",
          "form": "뭘 가르치는지에 따라 다른데 농대가 생명공학과된것처럼 이름만 바꾼거면",
          "original_form": "뭘 가르치는지에 따라 다른데 농대가 생명공학과된것처럼 이름만 바꾼거면",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016852.1.14",
          "form": "신설이 아니라 상표갈이죠 ㅋㅋ 컴공과 같은걸 이름만 바꿈되니깐",
          "original_form": "신설이 아니라 상표갈이죠 ㅋㅋ 컴공과 같은걸 이름만 바꿈되니깐",
          "speaker_id": "2",
          "time": "20210826 15:58"
        },
