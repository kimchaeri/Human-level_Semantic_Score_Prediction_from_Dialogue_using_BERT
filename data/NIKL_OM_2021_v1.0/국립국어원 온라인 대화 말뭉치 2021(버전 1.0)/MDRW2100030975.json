      "utterance": [
        {
          "id": "MDRW2100030975.1.1",
          "form": "음..과연 다행일까요?ㅎㅎ",
          "original_form": "음..과연 다행일까요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 15:02"
        },
        {
          "id": "MDRW2100030975.1.2",
          "form": "ㅋㅋㅋ그런가요ㅋㅋㄱ",
          "original_form": "ㅋㅋㅋ그런가요ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210904 15:02"
        },
        {
          "id": "MDRW2100030975.1.3",
          "form": "반찬배달 시켜보셔어요?",
          "original_form": "반찬배달 시켜보셔어요?",
          "speaker_id": "1",
          "time": "20210904 15:02"
        },
        {
          "id": "MDRW2100030975.1.4",
          "form": "아니요~",
          "original_form": "아니요~",
          "speaker_id": "2",
          "time": "20210904 15:02"
        },
        {
          "id": "MDRW2100030975.1.5",
          "form": "하고싶은데 식구들이 눈치가 빨라서..",
          "original_form": "하고싶은데 식구들이 눈치가 빨라서..",
          "speaker_id": "2",
          "time": "20210904 15:03"
        },
        {
          "id": "MDRW2100030975.1.6",
          "form": "저는 한번 해봤는데",
          "original_form": "저는 한번 해봤는데",
          "speaker_id": "1",
          "time": "20210904 15:03"
        },
        {
          "id": "MDRW2100030975.1.7",
          "form": "장보는 비용이나 그거나 비슷한것 같죠?",
          "original_form": "장보는 비용이나 그거나 비슷한것 같죠?",
          "speaker_id": "2",
          "time": "20210904 15:03"
        },
        {
          "id": "MDRW2100030975.1.8",
          "form": "네 재료비에 씻고 조리하고 정리하는 수고비 생각하면",
          "original_form": "네 재료비에 씻고 조리하고 정리하는 수고비 생각하면",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100030975.1.9",
          "form": "오히려 그게 더 싼거같기도 해요",
          "original_form": "오히려 그게 더 싼거같기도 해요",
          "speaker_id": "1",
          "time": "20210904 15:04"
        },
        {
          "id": "MDRW2100030975.1.10",
          "form": "근데 또 어느정도 먹고나면 맛이 금방 질리더라구요",
          "original_form": "근데 또 어느정도 먹고나면 맛이 금방 질리더라구요",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100030975.1.11",
          "form": "입맛에 맞는곳을 찾기만 하면 그것도 한번씩 괜찮을듯 싶어요",
          "original_form": "입맛에 맞는곳을 찾기만 하면 그것도 한번씩 괜찮을듯 싶어요",
          "speaker_id": "2",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100030975.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100030975.1.13",
          "form": "주기적 정기적말고",
          "original_form": "주기적 정기적말고",
          "speaker_id": "1",
          "time": "20210904 15:05"
        },
        {
          "id": "MDRW2100030975.1.14",
          "form": "이따금씩 하면",
          "original_form": "이따금씩 하면",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100030975.1.15",
          "form": "진짜 메뉴선정이 어려울때 반찬가게를 가보거든요",
          "original_form": "진짜 메뉴선정이 어려울때 반찬가게를 가보거든요",
          "speaker_id": "2",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100030975.1.16",
          "form": "좋을듯해요",
          "original_form": "좋을듯해요",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100030975.1.17",
          "form": "오 굿팁!!!",
          "original_form": "오 굿팁!!!",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100030975.1.18",
          "form": "근데,늘 해먹던것 밖에 없어요",
          "original_form": "근데,늘 해먹던것 밖에 없어요",
          "speaker_id": "2",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100030975.1.19",
          "form": "거기도 다 우리랑 비슷비슷ㅋㅋ",
          "original_form": "거기도 다 우리랑 비슷비슷ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:06"
        },
        {
          "id": "MDRW2100030975.1.20",
          "form": "그래서 나도 다양하게 하는구만",
          "original_form": "그래서 나도 다양하게 하는구만",
          "speaker_id": "2",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100030975.1.21",
          "form": "뿌듯하게 나온다는 씁쓸한 아이러니..ㅋㅋ",
          "original_form": "뿌듯하게 나온다는 씁쓸한 아이러니..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100030975.1.22",
          "form": "그러네요 자부심을 가져야겠어요",
          "original_form": "그러네요 자부심을 가져야겠어요",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
        {
          "id": "MDRW2100030975.1.23",
          "form": "사실 학교급식도 애들말 들어보면",
          "original_form": "사실 학교급식도 애들말 들어보면",
          "speaker_id": "1",
          "time": "20210904 15:07"
        },
