      "utterance": [
        {
          "id": "MDRW2100041225.1.1",
          "form": "좋아하는 연예인 있으세용?",
          "original_form": "좋아하는 연예인 있으세용?",
          "speaker_id": "1",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100041225.1.2",
          "form": "김선호 요즘 좋아해요!",
          "original_form": "김선호 요즘 좋아해요!",
          "speaker_id": "2",
          "time": "20210905 20:34"
        },
        {
          "id": "MDRW2100041225.1.3",
          "form": "아 진짜 잘생겼죠...",
          "original_form": "아 진짜 잘생겼죠...",
          "speaker_id": "1",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100041225.1.4",
          "form": "진짜요...",
          "original_form": "진짜요...",
          "speaker_id": "2",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100041225.1.5",
          "form": "그리고 예능에서 빼질 않아요",
          "original_form": "그리고 예능에서 빼질 않아요",
          "speaker_id": "2",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100041225.1.6",
          "form": "드라마를 보는건아닌데 짤봤거든요\r\n어제 웃는 엔딩짱이던데",
          "original_form": "드라마를 보는건아닌데 짤봤거든요\r\n어제 웃는 엔딩짱이던데",
          "speaker_id": "1",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100041225.1.7",
          "form": "빼지도않는다니 성격도좋아보이구ㅜㅜ..",
          "original_form": "빼지도않는다니 성격도좋아보이구ㅜㅜ..",
          "speaker_id": "1",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100041225.1.8",
          "form": "아.. 얼굴 너무 잘해요ㅋㅋ",
          "original_form": "아.. 얼굴 너무 잘해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100041225.1.9",
          "form": "사기캐입니다...",
          "original_form": "사기캐입니다...",
          "speaker_id": "1",
          "time": "20210905 20:35"
        },
        {
          "id": "MDRW2100041225.1.10",
          "form": "저도모르게 저장한짤들..",
          "original_form": "저도모르게 저장한짤들..",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100041225.1.11",
          "form": "ㄱㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㄱㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100041225.1.12",
          "form": "딘딘때문에 보기 시작했는데",
          "original_form": "딘딘때문에 보기 시작했는데",
          "speaker_id": "2",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100041225.1.13",
          "form": "김선호한테 빠졌구요",
          "original_form": "김선호한테 빠졌구요",
          "speaker_id": "2",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100041225.1.14",
          "form": "ㅋㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100041225.1.15",
          "form": "딘딘도 재밌긴재밌어여",
          "original_form": "딘딘도 재밌긴재밌어여",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100041225.1.16",
          "form": "귀여워서 봤는데 넘 얄밉고?.. ㅋㅋ",
          "original_form": "귀여워서 봤는데 넘 얄밉고?.. ㅋㅋ",
