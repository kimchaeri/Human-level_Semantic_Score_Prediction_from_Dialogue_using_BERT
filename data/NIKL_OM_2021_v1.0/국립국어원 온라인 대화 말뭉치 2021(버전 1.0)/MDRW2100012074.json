      "utterance": [
        {
          "id": "MDRW2100012074.1.1",
          "form": "책 좋아하시나요",
          "original_form": "책 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 19:01"
        },
        {
          "id": "MDRW2100012074.1.2",
          "form": "전 로설만요",
          "original_form": "전 로설만요",
          "speaker_id": "2",
          "time": "20210824 19:05"
        },
        {
          "id": "MDRW2100012074.1.3",
          "form": "무슨책 좋아하세요",
          "original_form": "무슨책 좋아하세요",
          "speaker_id": "2",
          "time": "20210824 19:05"
        },
        {
          "id": "MDRW2100012074.1.4",
          "form": "카카페에서 매일 한편씩봐요 ㅋㅋ",
          "original_form": "카카페에서 매일 한편씩봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:05"
        },
        {
          "id": "MDRW2100012074.1.5",
          "form": "음 저는 수필이나 육아서 많이 봐요",
          "original_form": "음 저는 수필이나 육아서 많이 봐요",
          "speaker_id": "1",
          "time": "20210824 19:06"
        },
        {
          "id": "MDRW2100012074.1.6",
          "form": "아 네",
          "original_form": "아 네",
          "speaker_id": "2",
          "time": "20210824 19:07"
        },
        {
          "id": "MDRW2100012074.1.7",
          "form": "전 사실 요새 육아서 봐야하나 싶어요",
          "original_form": "전 사실 요새 육아서 봐야하나 싶어요",
          "speaker_id": "2",
          "time": "20210824 19:07"
        },
        {
          "id": "MDRW2100012074.1.8",
          "form": "사춘기 딸을 어찌 키울지 모르겠네요",
          "original_form": "사춘기 딸을 어찌 키울지 모르겠네요",
          "speaker_id": "2",
          "time": "20210824 19:07"
        },
        {
          "id": "MDRW2100012074.1.9",
          "form": "큰애랑 너무달라요",
          "original_form": "큰애랑 너무달라요",
          "speaker_id": "2",
          "time": "20210824 19:07"
        },
        {
          "id": "MDRW2100012074.1.10",
          "form": "큰애는 고3이라 터울이 너무지니 다생각도 안나고",
          "original_form": "큰애는 고3이라 터울이 너무지니 다생각도 안나고",
          "speaker_id": "2",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012074.1.11",
          "form": "육아서 도움 되나요?",
          "original_form": "육아서 도움 되나요?",
          "speaker_id": "2",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012074.1.12",
          "form": "딸과 싸우지 않기 요런거 봐야하나 싶어요",
          "original_form": "딸과 싸우지 않기 요런거 봐야하나 싶어요",
          "speaker_id": "2",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012074.1.13",
          "form": "어머어머 고3이요? 다 키우셨어요 저는 3살..6살이라.ㅡ육아서가 절실해요",
          "original_form": "어머어머 고3이요? 다 키우셨어요 저는 3살..6살이라.ㅡ육아서가 절실해요",
          "speaker_id": "1",
          "time": "20210824 19:08"
        },
        {
          "id": "MDRW2100012074.1.14",
          "form": "네 다키웠는데 초등딸이 있어서 아직멀었어요 ㅜㅜ",
          "original_form": "네 다키웠는데 초등딸이 있어서 아직멀었어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 19:09"
        },
        {
          "id": "MDRW2100012074.1.15",
          "form": "그렇죠 그맘때 알아야 할것도 정보도 많고 그렇죠",
          "original_form": "그렇죠 그맘때 알아야 할것도 정보도 많고 그렇죠",
          "speaker_id": "2",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100012074.1.16",
          "form": "저도 그때 좀 봤던거 같아요",
          "original_form": "저도 그때 좀 봤던거 같아요",
          "speaker_id": "2",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100012074.1.17",
          "form": "이제 전 애들이 크니 제가 좋아하는 로맨스 보네요 ㅋㅋ",
          "original_form": "이제 전 애들이 크니 제가 좋아하는 로맨스 보네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100012074.1.18",
          "form": "저희 딸이 오히려 책수준이 높아요 ㅋㅋ",
          "original_form": "저희 딸이 오히려 책수준이 높아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:11"
        },
        {
          "id": "MDRW2100012074.1.19",
          "form": "인문학서 읽고 그럴듯요?^^",
          "original_form": "인문학서 읽고 그럴듯요?^^",
          "speaker_id": "1",
          "time": "20210824 19:11"
        },
        {
          "id": "MDRW2100012074.1.20",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:11"
        },
        {
          "id": "MDRW2100012074.1.21",
          "form": "전 안읽고요",
          "original_form": "전 안읽고요",
          "speaker_id": "2",
          "time": "20210824 19:11"
        },
        {
          "id": "MDRW2100012074.1.22",
          "form": "로맨스 소설 보면 설레시나요 ㅎㅎ",
          "original_form": "로맨스 소설 보면 설레시나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100012074.1.23",
          "form": "네 저 아직 소녀예요 ㅋㅋ",
          "original_form": "네 저 아직 소녀예요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:13"
        },
        {
          "id": "MDRW2100012074.1.24",
          "form": "귀여우세요^^ 소설이 감정을 말랑말랑하게 해주죠",
          "original_form": "귀여우세요^^ 소설이 감정을 말랑말랑하게 해주죠",
          "speaker_id": "1",
          "time": "20210824 19:14"
        },
        {
          "id": "MDRW2100012074.1.25",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100012074.1.26",
          "form": "육아물 봐요 ㅋㅋ",
          "original_form": "육아물 봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:15"
        },
        {
          "id": "MDRW2100012074.1.27",
          "form": "소설에서 귀요미 아가 키우는 ㅋㅋ",
          "original_form": "소설에서 귀요미 아가 키우는 ㅋㅋ",
