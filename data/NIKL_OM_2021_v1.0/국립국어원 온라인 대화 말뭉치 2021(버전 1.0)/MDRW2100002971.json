      "utterance": [
        {
          "id": "MDRW2100002971.1.1",
          "form": "안녕하세요 저랑 대화 해보실래요?",
          "original_form": "안녕하세요 저랑 대화 해보실래요?",
          "speaker_id": "1",
          "time": "20210820 17:11"
        },
        {
          "id": "MDRW2100002971.1.2",
          "form": "만나서 반가워요",
          "original_form": "만나서 반가워요",
          "speaker_id": "1",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002971.1.3",
          "form": "안녕하세요! 집안일좋아하세요?",
          "original_form": "안녕하세요! 집안일좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002971.1.4",
          "form": "힘들어서 좋아하지 않아요",
          "original_form": "힘들어서 좋아하지 않아요",
          "speaker_id": "1",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002971.1.5",
          "form": "name2님은 어떠세요?",
          "original_form": "&name2&님은 어떠세요?",
          "speaker_id": "1",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002971.1.6",
          "form": "저도 좋아하지는  않지만 생존을 위해서 억지로 하고있어요ㅠㅠ",
          "original_form": "저도 좋아하지는  않지만 생존을 위해서 억지로 하고있어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 17:13"
        },
        {
          "id": "MDRW2100002971.1.7",
          "form": "주고 어떤 집안일을 하시나요",
          "original_form": "주고 어떤 집안일을 하시나요",
          "speaker_id": "1",
          "time": "20210820 17:14"
        },
        {
          "id": "MDRW2100002971.1.8",
          "form": "저는 청소기돌리기와 먼지털기 설거지밖에안해요거의ㅠㅠ",
          "original_form": "저는 청소기돌리기와 먼지털기 설거지밖에안해요거의ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 17:15"
        },
        {
          "id": "MDRW2100002971.1.9",
          "form": "저도 청소기를 돌리고 쓰레기를 버려요",
          "original_form": "저도 청소기를 돌리고 쓰레기를 버려요",
          "speaker_id": "1",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002971.1.10",
          "form": "쓰레기가 왜이렇게 많이 생기는지 모르겠어요",
          "original_form": "쓰레기가 왜이렇게 많이 생기는지 모르겠어요",
          "speaker_id": "1",
          "time": "20210820 17:16"
        },
        {
          "id": "MDRW2100002971.1.11",
          "form": "ㅠㅠㅋㅋ쓰레기는 버려도 자꾸 생기는 마법걸려있죠... 집안일은 가족분들이랑 분배하시나요?",
          "original_form": "ㅠㅠㅋㅋ쓰레기는 버려도 자꾸 생기는 마법걸려있죠... 집안일은 가족분들이랑 분배하시나요?",
          "speaker_id": "2",
          "time": "20210820 17:17"
        },
        {
          "id": "MDRW2100002971.1.12",
          "form": "가족들과 같이 사시나요",
          "original_form": "가족들과 같이 사시나요",
          "speaker_id": "1",
          "time": "20210820 17:19"
        },
        {
          "id": "MDRW2100002971.1.13",
          "form": "넹! 그래서 저는 집안일을 나눠해요",
          "original_form": "넹! 그래서 저는 집안일을 나눠해요",
          "speaker_id": "2",
          "time": "20210820 17:20"
        },
        {
          "id": "MDRW2100002971.1.14",
          "form": "누구랑 나눠서 하나요",
          "original_form": "누구랑 나눠서 하나요",
          "speaker_id": "1",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002971.1.15",
          "form": "저는부모님과 나눠해용!",
          "original_form": "저는부모님과 나눠해용!",
          "speaker_id": "2",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002971.1.16",
          "form": "권호국님은 어떠신가요?",
          "original_form": "권호국님은 어떠신가요?",
          "speaker_id": "2",
          "time": "20210820 17:21"
        },
        {
          "id": "MDRW2100002971.1.17",
          "form": "저는 거의 정리를 하지않아요",
          "original_form": "저는 거의 정리를 하지않아요",
          "speaker_id": "1",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002971.1.18",
          "form": "일이 늦게끝나요",
          "original_form": "일이 늦게끝나요",
          "speaker_id": "1",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002971.1.19",
          "form": "집안일을 할 시간이 없어요",
          "original_form": "집안일을 할 시간이 없어요",
          "speaker_id": "1",
          "time": "20210820 17:22"
        },
        {
          "id": "MDRW2100002971.1.20",
          "form": "ㅠㅠ시간이없으면 어쩔수없는일이죠! 주말밖에 시간이 없겠네요! 집안일도 시간이 꽤 걸리더라구요",
          "original_form": "ㅠㅠ시간이없으면 어쩔수없는일이죠! 주말밖에 시간이 없겠네요! 집안일도 시간이 꽤 걸리더라구요",
          "speaker_id": "2",
          "time": "20210820 17:23"
        }
      ]
