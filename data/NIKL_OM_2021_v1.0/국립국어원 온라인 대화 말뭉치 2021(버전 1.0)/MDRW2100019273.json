      "utterance": [
        {
          "id": "MDRW2100019273.1.1",
          "form": "헐 사라졌나요??",
          "original_form": "헐 사라졌나요??",
          "speaker_id": "2",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100019273.1.2",
          "form": "네 ㅠㅠㅠ",
          "original_form": "네 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019273.1.3",
          "form": "라임 들어간 레샤로 나와서 밀렸대요",
          "original_form": "라임 들어간 레샤로 나와서 밀렸대요",
          "speaker_id": "1",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019273.1.4",
          "form": "헐..",
          "original_form": "헐..",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019273.1.5",
          "form": "너무 슬퍼요",
          "original_form": "너무 슬퍼요",
          "speaker_id": "1",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019273.1.6",
          "form": "원래 드셨었어요??",
          "original_form": "원래 드셨었어요??",
          "speaker_id": "1",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019273.1.7",
          "form": "가끔 사먹었어요!",
          "original_form": "가끔 사먹었어요!",
          "speaker_id": "2",
          "time": "20210827 16:53"
        },
        {
          "id": "MDRW2100019273.1.8",
          "form": "그거도 호불호갈리던데 호셨군요 ㅌㅋㅋ",
          "original_form": "그거도 호불호갈리던데 호셨군요 ㅌㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100019273.1.9",
          "form": "혹시 민초드세요?",
          "original_form": "혹시 민초드세요?",
          "speaker_id": "1",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100019273.1.10",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100019273.1.11",
          "form": "헉 전 반민초파에요 ㅋㅋㅋ",
          "original_form": "헉 전 반민초파에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100019273.1.12",
          "form": "요새 민초로 없는 음식이 없더라구요",
          "original_form": "요새 민초로 없는 음식이 없더라구요",
          "speaker_id": "1",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100019273.1.13",
          "form": "헐.. 전 민초파요..",
          "original_form": "헐.. 전 민초파요..",
          "speaker_id": "2",
          "time": "20210827 16:54"
        },
        {
          "id": "MDRW2100019273.1.14",
          "form": "하지만 디저트가 아닌 민초는 안먹어요",
          "original_form": "하지만 디저트가 아닌 민초는 안먹어요",
          "speaker_id": "2",
          "time": "20210827 16:55"
        },
        {
          "id": "MDRW2100019273.1.15",
          "form": "헉 디저트아닌 민초도 있나요?",
          "original_form": "헉 디저트아닌 민초도 있나요?",
          "speaker_id": "1",
          "time": "20210827 16:55"
        },
        {
          "id": "MDRW2100019273.1.16",
          "form": "얼마전에 나온 민초소주요...",
          "original_form": "얼마전에 나온 민초소주요...",
          "speaker_id": "2",
          "time": "20210827 16:55"
        },
        {
          "id": "MDRW2100019273.1.17",
          "form": "아 들어본거같아요",
          "original_form": "아 들어본거같아요",
          "speaker_id": "1",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100019273.1.18",
          "form": "정말 별게다있네요",
          "original_form": "정말 별게다있네요",
          "speaker_id": "1",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100019273.1.19",
          "form": "마자요 그거 맛없다는 평을 많이 들었어요",
          "original_form": "마자요 그거 맛없다는 평을 많이 들었어요",
          "speaker_id": "2",
          "time": "20210827 16:56"
        }
      ]
