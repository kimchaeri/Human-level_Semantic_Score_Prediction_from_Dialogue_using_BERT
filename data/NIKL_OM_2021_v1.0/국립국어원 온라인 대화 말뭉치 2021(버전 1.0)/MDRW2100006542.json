      "utterance": [
        {
          "id": "MDRW2100006542.1.1",
          "form": "귀신이네",
          "original_form": "귀신이네",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100006542.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:53"
        },
        {
          "id": "MDRW2100006542.1.3",
          "form": "근데 지금 내 머리가 마음에 들어서 괜한 도전 안할까 생각 중이기도 하고",
          "original_form": "근데 지금 내 머리가 마음에 들어서 괜한 도전 안할까 생각 중이기도 하고",
          "speaker_id": "2",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100006542.1.4",
          "form": "그러해",
          "original_form": "그러해",
          "speaker_id": "2",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100006542.1.5",
          "form": "먼 머린데 맘에들어ㅋㅋㅋ",
          "original_form": "먼 머린데 맘에들어ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:54"
        },
        {
          "id": "MDRW2100006542.1.6",
          "form": "그냥 투블럭 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그냥 투블럭 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100006542.1.7",
          "form": "흔한 머리인데 마음에 드는 미용사 분을 찾았어",
          "original_form": "흔한 머리인데 마음에 드는 미용사 분을 찾았어",
          "speaker_id": "2",
          "time": "20210908 01:55"
        },
        {
          "id": "MDRW2100006542.1.8",
          "form": "아 투블럭? 그 원래 투블럭 아니야 모든 남자갘ㅋㅋㅋ",
          "original_form": "아 투블럭? 그 원래 투블럭 아니야 모든 남자갘ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100006542.1.9",
          "form": "흔하지 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "흔하지 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100006542.1.10",
          "form": "그냥 뭐 마음에 들더라 그리고 조금 객관적인 눈이 필요해서",
          "original_form": "그냥 뭐 마음에 들더라 그리고 조금 객관적인 눈이 필요해서",
          "speaker_id": "2",
          "time": "20210908 01:57"
        },
        {
          "id": "MDRW2100006542.1.11",
          "form": "나중에 다른 사람한테 물어보고 새로운거 도전해볼까 싶기도하고",
          "original_form": "나중에 다른 사람한테 물어보고 새로운거 도전해볼까 싶기도하고",
          "speaker_id": "2",
          "time": "20210908 01:58"
        },
        {
          "id": "MDRW2100006542.1.12",
          "form": "ㅇㅎ 그래도 주위 반응 있을거아냐 지금은 어떤데?",
          "original_form": "ㅇㅎ 그래도 주위 반응 있을거아냐 지금은 어떤데?",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100006542.1.13",
          "form": "평범하지 뭐 ㅋㅋㅋㅋㅋ",
          "original_form": "평범하지 뭐 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100006542.1.14",
          "form": "나 요즘 사람을 잘 안 만나서 반응을 잘 모르긴하겠다",
          "original_form": "나 요즘 사람을 잘 안 만나서 반응을 잘 모르긴하겠다",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100006542.1.15",
          "form": "얼마 들었어??",
          "original_form": "얼마 들었어??",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100006542.1.16",
          "form": "머리하는데",
          "original_form": "머리하는데",
          "speaker_id": "1",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100006542.1.17",
          "form": "18000원",
          "original_form": "18000원",
          "speaker_id": "2",
          "time": "20210908 01:59"
        },
        {
          "id": "MDRW2100006542.1.18",
          "form": "별로 안비싼데 맘에 들었다니 놀랍군",
          "original_form": "별로 안비싼데 맘에 들었다니 놀랍군",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100006542.1.19",
          "form": "서울이 좀 비싼가? 비슷한가??",
          "original_form": "서울이 좀 비싼가? 비슷한가??",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100006542.1.20",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100006542.1.21",
          "form": "그래서 미용사가 마음에 들어서 자주 가려고 해",
          "original_form": "그래서 미용사가 마음에 들어서 자주 가려고 해",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100006542.1.22",
          "form": "다행이다 ㅋㅋㅋ 맘에 드는 머리라서",
          "original_form": "다행이다 ㅋㅋㅋ 맘에 드는 머리라서",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100006542.1.23",
          "form": "서울이지만",
          "original_form": "서울이지만",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100006542.1.24",
          "form": "응응 언젠가 자랑할게 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "응응 언젠가 자랑할게 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100006542.1.25",
          "form": "뭐 어차피 서울 매번 가면섴ㅋㅋㅋ",
          "original_form": "뭐 어차피 서울 매번 가면섴ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:00"
        },
        {
          "id": "MDRW2100006542.1.26",
          "form": "그냥 평범하잖아? 싶겠지만ㅋㅋㅋ",
          "original_form": "그냥 평범하잖아? 싶겠지만ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 02:01"
        },
        {
          "id": "MDRW2100006542.1.27",
          "form": "서울...추석 때 가고 나서 좀 자제하려고",
          "original_form": "서울...추석 때 가고 나서 좀 자제하려고",
          "speaker_id": "2",
          "time": "20210908 02:01"
        },
        {
