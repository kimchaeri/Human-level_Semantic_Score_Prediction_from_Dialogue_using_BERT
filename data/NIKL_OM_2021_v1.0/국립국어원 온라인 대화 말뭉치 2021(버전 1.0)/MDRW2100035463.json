      "utterance": [
        {
          "id": "MDRW2100035463.1.1",
          "form": "거긴 사장님 나오는 프론데",
          "original_form": "거긴 사장님 나오는 프론데",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100035463.1.2",
          "form": "게속 운동하는데",
          "original_form": "게속 운동하는데",
          "speaker_id": "2",
          "time": "20210829 17:47"
        },
        {
          "id": "MDRW2100035463.1.3",
          "form": "박하나는 왜 나왔지?",
          "original_form": "박하나는 왜 나왔지?",
          "speaker_id": "1",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100035463.1.4",
          "form": "kbs드라마 여주인공으로 드라마한데",
          "original_form": "kbs드라마 여주인공으로 드라마한데",
          "speaker_id": "2",
          "time": "20210829 17:48"
        },
        {
          "id": "MDRW2100035463.1.5",
          "form": "밀어주는거네",
          "original_form": "밀어주는거네",
          "speaker_id": "1",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100035463.1.6",
          "form": "양치승관장하고 거의 10년 가까이 아는사이",
          "original_form": "양치승관장하고 거의 10년 가까이 아는사이",
          "speaker_id": "2",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100035463.1.7",
          "form": "그래서 나오는거네",
          "original_form": "그래서 나오는거네",
          "speaker_id": "1",
          "time": "20210829 17:49"
        },
        {
          "id": "MDRW2100035463.1.8",
          "form": "엉 드라마에서 운동 마스터로 나오는갑더라",
          "original_form": "엉 드라마에서 운동 마스터로 나오는갑더라",
          "speaker_id": "2",
          "time": "20210829 17:50"
        },
        {
          "id": "MDRW2100035463.1.9",
          "form": "많이 물어봐야겠네",
          "original_form": "많이 물어봐야겠네",
          "speaker_id": "1",
          "time": "20210829 17:51"
        },
        {
          "id": "MDRW2100035463.1.10",
          "form": "그런데 운동하는건 별로 안좋아한다고 한다",
          "original_form": "그런데 운동하는건 별로 안좋아한다고 한다",
          "speaker_id": "2",
          "time": "20210829 17:52"
        },
        {
          "id": "MDRW2100035463.1.11",
          "form": "이제 요리사 정호영으로 화면 바뀜",
          "original_form": "이제 요리사 정호영으로 화면 바뀜",
          "speaker_id": "2",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100035463.1.12",
          "form": "안 좋아해도 어쩔수 없이 해야지",
          "original_form": "안 좋아해도 어쩔수 없이 해야지",
          "speaker_id": "1",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100035463.1.13",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "2",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100035463.1.14",
          "form": "정호영 나오면 허재 등 나올텐데",
          "original_form": "정호영 나오면 허재 등 나올텐데",
