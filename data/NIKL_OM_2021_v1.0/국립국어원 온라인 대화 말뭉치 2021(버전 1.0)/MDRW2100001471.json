          "speaker_id": "1",
          "time": "20211015 10:27"
        },
        {
          "id": "MDRW2100001471.1.27",
          "form": "알죠알죠",
          "original_form": "알죠알죠",
          "speaker_id": "1",
          "time": "20211015 10:27"
        },
        {
          "id": "MDRW2100001471.1.28",
          "form": "한국인인게 또 자랑스러워지는...",
          "original_form": "한국인인게 또 자랑스러워지는...",
          "speaker_id": "2",
          "time": "20211015 10:27"
        },
        {
          "id": "MDRW2100001471.1.29",
          "form": "문화강국이에여 이제 진짜",
          "original_form": "문화강국이에여 이제 진짜",
          "speaker_id": "2",
          "time": "20211015 10:27"
        },
        {
          "id": "MDRW2100001471.1.30",
          "form": "후후",
          "original_form": "후후",
          "speaker_id": "2",
          "time": "20211015 10:27"
        },
        {
          "id": "MDRW2100001471.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211015 10:28"
        },
        {
          "id": "MDRW2100001471.1.32",
          "form": "혹시 넷플릭스도 좋아하시나요",
          "original_form": "혹시 넷플릭스도 좋아하시나요",
          "speaker_id": "1",
          "time": "20211015 10:28"
        },
        {
          "id": "MDRW2100001471.1.33",
          "form": "보고싶은건 많은데",
          "original_form": "보고싶은건 많은데",
          "speaker_id": "2",
          "time": "20211015 10:28"
        },
        {
          "id": "MDRW2100001471.1.34",
          "form": "시간이 없어서..",
          "original_form": "시간이 없어서..",
          "speaker_id": "2",
          "time": "20211015 10:28"
        },
        {
          "id": "MDRW2100001471.1.35",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20211015 10:28"
        },
        {
          "id": "MDRW2100001471.1.36",
          "form": "아앗..",
          "original_form": "아앗..",
          "speaker_id": "1",
          "time": "20211015 10:28"
        },
        {
          "id": "MDRW2100001471.1.37",
          "form": "그렇군요 ㅠㅠㅠ",
          "original_form": "그렇군요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20211015 10:28"
        },
        {
          "id": "MDRW2100001471.1.38",
