        {
          "id": "MDRW2100001471.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211015 10:26"
        },
        {
          "id": "MDRW2100001471.1.3",
          "form": "네~",
          "original_form": "네~",
          "speaker_id": "1",
          "time": "20211015 10:26"
        },
        {
          "id": "MDRW2100001471.1.4",
          "form": "혹시 오징어게임 보셨어요?",
          "original_form": "혹시 오징어게임 보셨어요?",
          "speaker_id": "2",
          "time": "20211015 10:26"
        },
        {
          "id": "MDRW2100001471.1.5",
          "form": "오..",
          "original_form": "오..",
          "speaker_id": "1",
          "time": "20211015 10:26"
        },
        {
          "id": "MDRW2100001471.1.6",
          "form": "짧게짧게",
          "original_form": "짧게짧게",
          "speaker_id": "1",
          "time": "20211015 10:26"
        },
        {
          "id": "MDRW2100001471.1.7",
          "form": "유튜브로 봤어요",
          "original_form": "유튜브로 봤어요",
          "speaker_id": "1",
          "time": "20211015 10:26"
        },
        {
          "id": "MDRW2100001471.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ와",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ와",
          "speaker_id": "2",
          "time": "20211015 10:26"
        },
        {
          "id": "MDRW2100001471.1.9",
          "form": "사실 저도..",
          "original_form": "사실 저도..",
          "speaker_id": "2",
          "time": "20211015 10:26"
        },
        {
          "id": "MDRW2100001471.1.10",
          "form": "보셨나요?",
          "original_form": "보셨나요?",
          "speaker_id": "1",
          "time": "20211015 10:26"
        },
        {
          "id": "MDRW2100001471.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211015 10:26"
        },
        {
          "id": "MDRW2100001471.1.12",
          "form": "유튜브 요약본으로..ㅎㅎ",
          "original_form": "유튜브 요약본으로..ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 10:26"
        },
        {
          "id": "MDRW2100001471.1.13",
          "form": "스포 포함인지 모르고",
          "original_form": "스포 포함인지 모르고",
          "speaker_id": "2",
          "time": "20211015 10:27"
        },
        {
          "id": "MDRW2100001471.1.14",
          "form": "재밌어서 볼려고했는데",
          "original_form": "재밌어서 볼려고했는데",
          "speaker_id": "1",
          "time": "20211015 10:27"
        },
        {
          "id": "MDRW2100001471.1.15",
          "form": "이미 다 본거같아서...",
          "original_form": "이미 다 본거같아서...",
          "speaker_id": "1",
          "time": "20211015 10:27"
        },
        {
          "id": "MDRW2100001471.1.16",
          "form": "봤다가 결말까지 알아버려서...",
          "original_form": "봤다가 결말까지 알아버려서...",
          "speaker_id": "2",
          "time": "20211015 10:27"
        },
        {
          "id": "MDRW2100001471.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저도요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ저도요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 10:27"
        },
        {
          "id": "MDRW2100001471.1.18",
          "form": "근데 재밌더라고요..요약본도",
          "original_form": "근데 재밌더라고요..요약본도",
          "speaker_id": "2",
          "time": "20211015 10:27"
        },
        {
          "id": "MDRW2100001471.1.19",
          "form": "그만해...",
          "original_form": "그만해...",
          "speaker_id": "1",
          "time": "20211015 10:27"
        },
        {
          "id": "MDRW2100001471.1.20",
          "form": "킄ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "킄ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 10:27"
        },
        {
          "id": "MDRW2100001471.1.21",
          "form": "이러다 죽어~",
          "original_form": "이러다 죽어~",
          "speaker_id": "1",
          "time": "20211015 10:27"
        },
        {
          "id": "MDRW2100001471.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ다보셨네!!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ다보셨네!!!",
          "speaker_id": "2",
          "time": "20211015 10:27"
        },
        {
          "id": "MDRW2100001471.1.23",
          "form": "그니까요..",
          "original_form": "그니까요..",
          "speaker_id": "1",
          "time": "20211015 10:27"
        },
        {
          "id": "MDRW2100001471.1.24",
          "form": "오징어게임이 넷플릭스 시청자수 역대 1위하신거 아세요?",
          "original_form": "오징어게임이 넷플릭스 시청자수 역대 1위하신거 아세요?",
          "speaker_id": "2",
          "time": "20211015 10:27"
        },
        {
          "id": "MDRW2100001471.1.25",
          "form": "전 세계에서!",
          "original_form": "전 세계에서!",
          "speaker_id": "2",
          "time": "20211015 10:27"
        },
        {
