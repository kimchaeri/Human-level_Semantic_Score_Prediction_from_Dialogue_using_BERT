      "utterance": [
        {
          "id": "MDRW2100012649.1.1",
          "form": "아뇨 먹고 살려고 하는거죠ㅎㅎ",
          "original_form": "아뇨 먹고 살려고 하는거죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100012649.1.2",
          "form": "또 뭐하실줄 아시나요?",
          "original_form": "또 뭐하실줄 아시나요?",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100012649.1.3",
          "form": "계란찜도 합니다",
          "original_form": "계란찜도 합니다",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012649.1.4",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012649.1.5",
          "form": "주로 계란요리네요",
          "original_form": "주로 계란요리네요",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100012649.1.6",
          "form": "찜 은근 어려워요ㅎ",
          "original_form": "찜 은근 어려워요ㅎ",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100012649.1.7",
          "form": "촉촉하게 하기 힘든던데",
          "original_form": "촉촉하게 하기 힘든던데",
          "speaker_id": "1",
          "time": "20210824 23:53"
        },
        {
          "id": "MDRW2100012649.1.8",
          "form": "중탕하면 촉촉해집니다ㅎㅎ",
          "original_form": "중탕하면 촉촉해집니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:31"
        },
        {
          "id": "MDRW2100012649.1.9",
          "form": "아 중탕을해야되는군요",
          "original_form": "아 중탕을해야되는군요",
          "speaker_id": "1",
          "time": "20210825 00:32"
        },
        {
          "id": "MDRW2100012649.1.10",
          "form": "그럼닭발집 계란찜처럼되나요?ㅎㅎ",
          "original_form": "그럼닭발집 계란찜처럼되나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 00:33"
        },
        {
          "id": "MDRW2100012649.1.11",
          "form": "네 맞아요ㅋㅋ",
          "original_form": "네 맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100012649.1.12",
          "form": "제 주특기죠",
          "original_form": "제 주특기죠",
          "speaker_id": "2",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100012649.1.13",
          "form": "오ㅎ 조리법좀",
          "original_form": "오ㅎ 조리법좀",
          "speaker_id": "1",
          "time": "20210825 00:53"
        },
        {
          "id": "MDRW2100012649.1.14",
          "form": "중탕을 어떻게 설명하죠ㅋㅋㅋㅋㅋ",
          "original_form": "중탕을 어떻게 설명하죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012649.1.15",
          "form": "일단 물 끓일것이랑",
          "original_form": "일단 물 끓일것이랑",
          "speaker_id": "2",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012649.1.16",
          "form": "계란찜 만들 녀석 두개",
          "original_form": "계란찜 만들 녀석 두개",
          "speaker_id": "2",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012649.1.17",
          "form": "중간에 물 넣고 중탕하시면 돼요",
          "original_form": "중간에 물 넣고 중탕하시면 돼요",
          "speaker_id": "2",
          "time": "20210825 01:03"
        },
        {
          "id": "MDRW2100012649.1.18",
          "form": "계란찜 몇개에 물 얼마나넣어요?ㅎ",
          "original_form": "계란찜 몇개에 물 얼마나넣어요?ㅎ",
          "speaker_id": "1",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012649.1.19",
          "form": "아 우유 넣어요",
          "original_form": "아 우유 넣어요",
          "speaker_id": "2",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012649.1.20",
          "form": "그래야 부드러워요",
          "original_form": "그래야 부드러워요",
          "speaker_id": "2",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012649.1.21",
          "form": "아하ㅎ 물대신 우유요ㅎ",
          "original_form": "아하ㅎ 물대신 우유요ㅎ",
          "speaker_id": "1",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012649.1.22",
          "form": "비율은요?",
          "original_form": "비율은요?",
          "speaker_id": "1",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100012649.1.23",
          "form": "계란은 네개정도 넣고 우유는 200~300ml 정도",
          "original_form": "계란은 네개정도 넣고 우유는 200~300ml 정도",
          "speaker_id": "2",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100012649.1.24",
          "form": "넣었던거같애요",
          "original_form": "넣었던거같애요",
          "speaker_id": "2",
          "time": "20210825 01:35"
        },
        {
          "id": "MDRW2100012649.1.25",
          "form": "담에 함 해볼게요",
          "original_form": "담에 함 해볼게요",
          "speaker_id": "1",
          "time": "20210825 01:36"
        }
      ]
