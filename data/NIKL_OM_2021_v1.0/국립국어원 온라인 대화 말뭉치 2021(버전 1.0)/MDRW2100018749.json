        },
        {
          "id": "MDRW2100018749.1.2",
          "form": "가인핛ㅇ 하는거본",
          "original_form": "가인핛ㅇ 하는거본",
          "speaker_id": "1",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018749.1.3",
          "form": "보니",
          "original_form": "보니",
          "speaker_id": "1",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018749.1.4",
          "form": "마자재밋어보여",
          "original_form": "마자재밋어보여",
          "speaker_id": "2",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018749.1.5",
          "form": "그니께",
          "original_form": "그니께",
          "speaker_id": "1",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018749.1.6",
          "form": "완전 궁금해",
          "original_form": "완전 궁금해",
          "speaker_id": "1",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018749.1.7",
          "form": "액션도 대박이야",
          "original_form": "액션도 대박이야",
          "speaker_id": "2",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018749.1.8",
          "form": "캐릭터는 무슨캐로?",
          "original_form": "캐릭터는 무슨캐로?",
          "speaker_id": "1",
          "time": "20210827 11:44"
        },
        {
          "id": "MDRW2100018749.1.9",
          "form": "나는 남캐",
          "original_form": "나는 남캐",
          "speaker_id": "2",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018749.1.10",
          "form": "오 나도",
          "original_form": "오 나도",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018749.1.11",
          "form": "너는 왜?",
          "original_form": "너는 왜?",
          "speaker_id": "2",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018749.1.12",
          "form": "난 여캐보다",
          "original_form": "난 여캐보다",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018749.1.13",
          "form": "남캐로 해보고싶어서",
          "original_form": "남캐로 해보고싶어서",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018749.1.14",
          "form": "나도 매번 여캐만 해서",
          "original_form": "나도 매번 여캐만 해서",
          "speaker_id": "2",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018749.1.15",
          "form": "기분전환이지",
          "original_form": "기분전환이지",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018749.1.16",
          "form": "그릏지",
          "original_form": "그릏지",
          "speaker_id": "2",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018749.1.17",
          "form": "글고 나도 전사 해보고싶어",
          "original_form": "글고 나도 전사 해보고싶어",
          "speaker_id": "1",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100018749.1.18",
          "form": "그 직업??",
          "original_form": "그 직업??",
          "speaker_id": "2",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100018749.1.19",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100018749.1.20",
          "form": "캐릭터",
          "original_form": "캐릭터",
          "speaker_id": "1",
          "time": "20210827 11:47"
        },
        {
          "id": "MDRW2100018749.1.21",
          "form": "나도 망치로 때리는 캐릭터",
          "original_form": "나도 망치로 때리는 캐릭터",
