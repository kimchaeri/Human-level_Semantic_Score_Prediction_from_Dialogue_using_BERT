      "utterance": [
        {
          "id": "MDRW2100009101.1.1",
          "form": "아샌드위치랑",
          "original_form": "아샌드위치랑",
          "speaker_id": "2",
          "time": "20210822 21:15"
        },
        {
          "id": "MDRW2100009101.1.2",
          "form": "쿠키머핀요",
          "original_form": "쿠키머핀요",
          "speaker_id": "2",
          "time": "20210822 21:16"
        },
        {
          "id": "MDRW2100009101.1.3",
          "form": "새로나온 음료드셔보셨어요?",
          "original_form": "새로나온 음료드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210822 21:16"
        },
        {
          "id": "MDRW2100009101.1.4",
          "form": "요머핀 블루베리 잼이 안에서 흐르는데 넘 맛나요",
          "original_form": "요머핀 블루베리 잼이 안에서 흐르는데 넘 맛나요",
          "speaker_id": "2",
          "time": "20210822 21:16"
        },
        {
          "id": "MDRW2100009101.1.5",
          "form": "저 유자요",
          "original_form": "저 유자요",
          "speaker_id": "2",
          "time": "20210822 21:16"
        },
        {
          "id": "MDRW2100009101.1.6",
          "form": "아이스크림 블렌딩 콜드브루였던거같아요",
          "original_form": "아이스크림 블렌딩 콜드브루였던거같아요",
          "speaker_id": "1",
          "time": "20210822 21:16"
        },
        {
          "id": "MDRW2100009101.1.7",
          "form": "자주 마셔요",
          "original_form": "자주 마셔요",
          "speaker_id": "2",
          "time": "20210822 21:16"
        },
        {
          "id": "MDRW2100009101.1.8",
          "form": "아 보긴했는데",
          "original_form": "아 보긴했는데",
          "speaker_id": "2",
          "time": "20210822 21:16"
        },
        {
          "id": "MDRW2100009101.1.9",
          "form": "맛이 비싼 더위사냥 맛인데 맛있어요 ㅎ",
          "original_form": "맛이 비싼 더위사냥 맛인데 맛있어요 ㅎ",
          "speaker_id": "1",
          "time": "20210822 21:17"
        },
        {
          "id": "MDRW2100009101.1.10",
          "form": "못먹었어요 ㅜㅜ",
          "original_form": "못먹었어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 21:17"
        },
        {
          "id": "MDRW2100009101.1.11",
          "form": "옾",
          "original_form": "옾",
          "speaker_id": "2",
          "time": "20210822 21:17"
        },
        {
          "id": "MDRW2100009101.1.12",
          "form": "땡기네요 ㅎㅎ",
          "original_form": "땡기네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 21:17"
        },
        {
          "id": "MDRW2100009101.1.13",
          "form": "더위사냥 진짜 많이 먹었었어요",
          "original_form": "더위사냥 진짜 많이 먹었었어요",
          "speaker_id": "2",
          "time": "20210822 21:17"
        },
        {
          "id": "MDRW2100009101.1.14",
          "form": "그맛이라니 낼 먹어 볼께요 ㅋㅋ",
          "original_form": "그맛이라니 낼 먹어 볼께요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009101.1.15",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009101.1.16",
          "form": "궁금해요",
          "original_form": "궁금해요",
          "speaker_id": "2",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009101.1.17",
          "form": "저도 좋아하는 아이스크림이라 입맛에 맞더라구요",
          "original_form": "저도 좋아하는 아이스크림이라 입맛에 맞더라구요",
          "speaker_id": "1",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009101.1.18",
          "form": "진한거 좋아하심 샷추가 하심좋구요",
          "original_form": "진한거 좋아하심 샷추가 하심좋구요",
          "speaker_id": "1",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009101.1.19",
          "form": "네 그거 싫어하는 사람 없을거예요",
          "original_form": "네 그거 싫어하는 사람 없을거예요",
          "speaker_id": "2",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009101.1.20",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 21:18"
        },
        {
          "id": "MDRW2100009101.1.21",
          "form": "ㅋㅋ세상엔 맛난거 천지네요",
          "original_form": "ㅋㅋ세상엔 맛난거 천지네요",
          "speaker_id": "2",
          "time": "20210822 21:19"
        },
        {
          "id": "MDRW2100009101.1.22",
          "form": "친구는 먹으러가니 품절이래서 못먹었데요",
          "original_form": "친구는 먹으러가니 품절이래서 못먹었데요",
          "speaker_id": "1",
          "time": "20210822 21:19"
        },
        {
          "id": "MDRW2100009101.1.23",
          "form": "세상에 품절",
          "original_form": "세상에 품절",
          "speaker_id": "2",
          "time": "20210822 21:19"
        },
        {
          "id": "MDRW2100009101.1.24",
          "form": "일찍 가야겠어요",
          "original_form": "일찍 가야겠어요",
          "speaker_id": "2",
          "time": "20210822 21:19"
        },
        {
          "id": "MDRW2100009101.1.25",
          "form": "여긴 스벅 적아요",
          "original_form": "여긴 스벅 적아요",
          "speaker_id": "2",
          "time": "20210822 21:19"
        },
        {
          "id": "MDRW2100009101.1.26",
          "form": "작아요",
          "original_form": "작아요",
          "speaker_id": "2",
          "time": "20210822 21:19"
        },
        {
          "id": "MDRW2100009101.1.27",
          "form": "사람은 많고",
          "original_form": "사람은 많고",
          "speaker_id": "2",
          "time": "20210822 21:19"
        },
        {
          "id": "MDRW2100009101.1.28",
          "form": "품절 될수도 있겠어요",
          "original_form": "품절 될수도 있겠어요",
          "speaker_id": "2",
          "time": "20210822 21:19"
        },
        {
          "id": "MDRW2100009101.1.29",
          "form": "아마 그땐 쓰리별준다고 품절됐던거같기도해요",
          "original_form": "아마 그땐 쓰리별준다고 품절됐던거같기도해요",
          "speaker_id": "1",
          "time": "20210822 21:20"
        },
        {
          "id": "MDRW2100009101.1.30",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210822 21:20"
        },
        {
          "id": "MDRW2100009101.1.31",
          "form": "쓰리별 바로 품절이죠",
          "original_form": "쓰리별 바로 품절이죠",
          "speaker_id": "2",
          "time": "20210822 21:20"
        },
        {
          "id": "MDRW2100009101.1.32",
          "form": "스벅은 진짜 어딜가나 인기인듯해요",
          "original_form": "스벅은 진짜 어딜가나 인기인듯해요",
          "speaker_id": "1",
          "time": "20210822 21:20"
        },
        {
          "id": "MDRW2100009101.1.33",
          "form": "멎아요",
          "original_form": "멎아요",
          "speaker_id": "2",
          "time": "20210822 21:20"
        },
        {
          "id": "MDRW2100009101.1.34",
          "form": "자리가 없어요",
          "original_form": "자리가 없어요",
          "speaker_id": "2",
          "time": "20210822 21:21"
        },
        {
          "id": "MDRW2100009101.1.35",
          "form": "코로나도 비껴가요",
          "original_form": "코로나도 비껴가요",
          "speaker_id": "2",
          "time": "20210822 21:21"
        },
        {
          "id": "MDRW2100009101.1.36",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 21:21"
        },
        {
          "id": "MDRW2100009101.1.37",
          "form": "그때 캐리어인가?",
          "original_form": "그때 캐리어인가?",
          "speaker_id": "1",
          "time": "20210822 21:21"
        },
        {
          "id": "MDRW2100009101.1.38",
          "form": "진짜 딴세상이얘요",
          "original_form": "진짜 딴세상이얘요",
          "speaker_id": "2",
          "time": "20210822 21:21"
        },
        {
          "id": "MDRW2100009101.1.39",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 21:21"
        },
        {
          "id": "MDRW2100009101.1.40",
          "form": "줄서는거 보고 깜놀했어요 ㅎㅎ",
          "original_form": "줄서는거 보고 깜놀했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:21"
        },
        {
          "id": "MDRW2100009101.1.41",
          "form": "그렇죠 ㅋㅋ",
          "original_form": "그렇죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:21"
        },
        {
          "id": "MDRW2100009101.1.42",
          "form": "경쟁 장난 아니예요",
          "original_form": "경쟁 장난 아니예요",
          "speaker_id": "2",
          "time": "20210822 21:22"
        },
        {
          "id": "MDRW2100009101.1.43",
          "form": "여긴 인기 커피숍인게",
          "original_form": "여긴 인기 커피숍인게",
          "speaker_id": "2",
          "time": "20210822 21:22"
        },
        {
          "id": "MDRW2100009101.1.44",
          "form": "이런 제품들 때문이죠",
          "original_form": "이런 제품들 때문이죠",
          "speaker_id": "2",
          "time": "20210822 21:22"
        },
        {
          "id": "MDRW2100009101.1.45",
          "form": "저는 그인기덕에 한판모아서 잘팔았어요",
          "original_form": "저는 그인기덕에 한판모아서 잘팔았어요",
          "speaker_id": "1",
          "time": "20210822 21:24"
        }
      ]
