        },
        {
          "id": "MDRW2100020070.1.3",
          "form": "좀비 나오는 거 좀비?",
          "original_form": "좀비 나오는 거 좀비?",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020070.1.4",
          "form": "맞아 시즌이 두 개나 돼",
          "original_form": "맞아 시즌이 두 개나 돼",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020070.1.5",
          "form": "그거 보려고 넷플릭스 가입 함 가입 함",
          "original_form": "그거 보려고 넷플릭스 가입 함 가입 함",
          "speaker_id": "2",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020070.1.6",
          "form": "이번에 나온 킹덤 아 신전 도 봤니",
          "original_form": "이번에 나온 킹덤 아 신전 도 봤니",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020070.1.7",
          "form": "배우 전지현 나온다 그래서 챙겨 봄",
          "original_form": "배우 전지현 나온다 그래서 챙겨 봄",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020070.1.8",
          "form": "나는 기대보단 별로였어",
          "original_form": "나는 기대보단 별로였어",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020070.1.9",
          "form": "나도 졸리더라",
          "original_form": "나도 졸리더라",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020070.1.10",
          "form": "다음에도 그렇게 스피노프를 만든다던데",
          "original_form": "다음에도 그렇게 스피노프를 만든다던데",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020070.1.11",
          "form": "진짜 시즌 쓰리 나오는 줄",
          "original_form": "진짜 시즌 쓰리 나오는 줄",
          "speaker_id": "2",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020070.1.12",
          "form": "그러게 나도 시즌 쓰리가 더 기대되는데",
          "original_form": "그러게 나도 시즌 쓰리가 더 기대되는데",
          "speaker_id": "1",
          "time": "20210827 23:35"
        },
        {
          "id": "MDRW2100020070.1.13",
          "form": "김은희 작가 거다 재미있긴 함",
          "original_form": "김은희 작가 거다 재미있긴 함",
