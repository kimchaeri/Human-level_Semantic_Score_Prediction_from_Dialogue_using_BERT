      "utterance": [
        {
          "id": "MDRW2100039606.1.1",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210903 19:32"
        },
        {
          "id": "MDRW2100039606.1.2",
          "form": "또",
          "original_form": "또",
          "speaker_id": "1",
          "time": "20210903 19:32"
        },
        {
          "id": "MDRW2100039606.1.3",
          "form": "부인 허리띠로 때리는거 나왓어요",
          "original_form": "부인 허리띠로 때리는거 나왓어요",
          "speaker_id": "1",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039606.1.4",
          "form": "진짜 불쾌해요",
          "original_form": "진짜 불쾌해요",
          "speaker_id": "2",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039606.1.5",
          "form": "네 예전에 한번 보여준 건데",
          "original_form": "네 예전에 한번 보여준 건데",
          "speaker_id": "1",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039606.1.6",
          "form": "또 보여줘요",
          "original_form": "또 보여줘요",
          "speaker_id": "1",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039606.1.7",
          "form": "여혐인가요?",
          "original_form": "여혐인가요?",
          "speaker_id": "2",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039606.1.8",
          "form": "그건 아닌데",
          "original_form": "그건 아닌데",
          "speaker_id": "1",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039606.1.9",
          "form": "좀 감수성이 부족해보여요",
          "original_form": "좀 감수성이 부족해보여요",
          "speaker_id": "1",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039606.1.10",
          "form": "네 굳이 보여줄 필요 있나요",
          "original_form": "네 굳이 보여줄 필요 있나요",
          "speaker_id": "2",
          "time": "20210903 19:33"
        },
        {
          "id": "MDRW2100039606.1.11",
          "form": "네 이런거 때문애",
          "original_form": "네 이런거 때문애",
          "speaker_id": "1",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100039606.1.12",
          "form": "보기 불편해요",
          "original_form": "보기 불편해요",
          "speaker_id": "1",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100039606.1.13",
          "form": "전 안 볼거에요 ㅋ",
          "original_form": "전 안 볼거에요 ㅋ",
          "speaker_id": "2",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100039606.1.14",
          "form": "네 ㅋㅋ 보지마세요 ㅠ",
          "original_form": "네 ㅋㅋ 보지마세요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100039606.1.15",
          "form": "계속 보세요?",
          "original_form": "계속 보세요?",
          "speaker_id": "2",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100039606.1.16",
          "form": "보긴 봐요.",
          "original_form": "보긴 봐요.",
          "speaker_id": "1",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100039606.1.17",
          "form": "완결 봐야 직성이 풀리는 타입이라.. ㅜㅜ",
          "original_form": "완결 봐야 직성이 풀리는 타입이라.. ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100039606.1.18",
          "form": "아 그러시군요",
          "original_form": "아 그러시군요",
          "speaker_id": "2",
          "time": "20210903 19:34"
        },
        {
          "id": "MDRW2100039606.1.19",
          "form": "네.. 계속 선넘네요",
          "original_form": "네.. 계속 선넘네요",
          "speaker_id": "1",
