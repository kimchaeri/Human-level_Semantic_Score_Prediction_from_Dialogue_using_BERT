      "utterance": [
        {
          "id": "MDRW2100005635.1.1",
          "form": "언니 이제 정리하고 자요.ㅋ저도 정리하고 잘게요ㅋㅋ",
          "original_form": "언니 이제 정리하고 자요.ㅋ저도 정리하고 잘게요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005635.1.2",
          "form": "그래 집 대충 치우고 자자",
          "original_form": "그래 집 대충 치우고 자자",
          "speaker_id": "2",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005635.1.3",
          "form": "내일 이어도 되재?",
          "original_form": "내일 이어도 되재?",
          "speaker_id": "2",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005635.1.4",
          "form": "이거?",
          "original_form": "이거?",
          "speaker_id": "2",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005635.1.5",
          "form": "얍얍",
          "original_form": "얍얍",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005635.1.6",
          "form": "잘자요ㅋ",
          "original_form": "잘자요ㅋ",
          "speaker_id": "1",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005635.1.7",
          "form": "오키~",
          "original_form": "오키~",
          "speaker_id": "2",
          "time": "20210821 23:29"
        },
        {
          "id": "MDRW2100005635.1.8",
          "form": "니도ㅋㅋ",
          "original_form": "니도ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005635.1.9",
          "form": "우리가족은 아빠생일 전야재 축하하러간다ㅋ",
          "original_form": "우리가족은 아빠생일 전야재 축하하러간다ㅋ",
          "speaker_id": "2",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100005635.1.10",
          "form": "울신랑 내일생일ㅡㅡ",
          "original_form": "울신랑 내일생일ㅡㅡ",
          "speaker_id": "2",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100005635.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ대박ㅋㄱ생일이 다 몰려있네요ㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ대박ㅋㄱ생일이 다 몰려있네요ㅋ",
          "speaker_id": "1",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100005635.1.12",
          "form": "좋다ㅋㅋㅋㄱㄲ",
          "original_form": "좋다ㅋㅋㅋㄱㄲ",
          "speaker_id": "1",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100005635.1.13",
          "form": "다시다시ㅋ 우리가족의아빠=신랑",
          "original_form": "다시다시ㅋ 우리가족의아빠=신랑",
          "speaker_id": "2",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100005635.1.14",
          "form": "울신랑 내일생일 오늘도 축하한다고ㅋㅋ",
          "original_form": "울신랑 내일생일 오늘도 축하한다고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:32"
        },
        {
          "id": "MDRW2100005635.1.15",
          "form": "이런가족이 있나?ㅋ",
          "original_form": "이런가족이 있나?ㅋ",
          "speaker_id": "2",
          "time": "20210822 12:33"
        },
        {
          "id": "MDRW2100005635.1.16",
          "form": "아아ㅋㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "아아ㅋㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100005635.1.17",
          "form": "웃겨ㅋㅋㅋㅋㅋㅋㅋㅋㅋ언니 저희신랑은",
          "original_form": "웃겨ㅋㅋㅋㅋㅋㅋㅋㅋㅋ언니 저희신랑은",
          "speaker_id": "1",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100005635.1.18",
          "form": "3일연속 생일파티했어요",
          "original_form": "3일연속 생일파티했어요",
          "speaker_id": "1",
          "time": "20210822 12:36"
        },
        {
          "id": "MDRW2100005635.1.19",
          "form": "하루ㅡ우리집, 그다음날ㅡ시댁, 그다다음날ㅡ큰형님댁",
          "original_form": "하루ㅡ우리집, 그다음날ㅡ시댁, 그다다음날ㅡ큰형님댁",
          "speaker_id": "1",
          "time": "20210822 12:37"
        },
        {
          "id": "MDRW2100005635.1.20",
          "form": "와우ㅋㅋ",
          "original_form": "와우ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100005635.1.21",
          "form": "대단한 가족들임ㅋㅋ 애들은 뭐하노",
          "original_form": "대단한 가족들임ㅋㅋ 애들은 뭐하노",
          "speaker_id": "2",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100005635.1.22",
          "form": "ㅋㅋㅋ저희 애들은 지금 티비보고있죠ㅋ",
          "original_form": "ㅋㅋㅋ저희 애들은 지금 티비보고있죠ㅋ",
          "speaker_id": "1",
          "time": "20210822 12:38"
        },
        {
          "id": "MDRW2100005635.1.23",
          "form": "우린 석경밥먹으러",
          "original_form": "우린 석경밥먹으러",
          "speaker_id": "2",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100005635.1.24",
          "form": "어디갈지 고민이네요.ㅋㅋ",
          "original_form": "어디갈지 고민이네요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100005635.1.25",
          "form": "오오 언니네 외식 잘하는듯ㅋㅋ",
          "original_form": "오오 언니네 외식 잘하는듯ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100005635.1.26",
          "form": "집안일은 다했나?",
          "original_form": "집안일은 다했나?",
          "speaker_id": "2",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100005635.1.27",
          "form": "난 거지꼴ㅋㅋ",
          "original_form": "난 거지꼴ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100005635.1.28",
          "form": "거지꼴ㅋㅋ저는 늘 그래요ㅋㅋ",
          "original_form": "거지꼴ㅋㅋ저는 늘 그래요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100005635.1.29",
          "form": "생일전날ㅋㅋ울신랑  최애 식당",
          "original_form": "생일전날ㅋㅋ울신랑  최애 식당",
          "speaker_id": "2",
          "time": "20210822 12:39"
        },
        {
          "id": "MDRW2100005635.1.30",
          "form": "아 그렇구나 생일날에는 또 뭐드실라고ㅋㅋ",
          "original_form": "아 그렇구나 생일날에는 또 뭐드실라고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:40"
        },
        {
          "id": "MDRW2100005635.1.31",
          "form": "생일상 차리셔야되나",
          "original_form": "생일상 차리셔야되나",
          "speaker_id": "1",
          "time": "20210822 12:40"
        }
      ]
    }
