      "utterance": [
        {
          "id": "MDRW2100016760.1.1",
          "form": "오늘은 뭐드셨어요?",
          "original_form": "오늘은 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016760.1.2",
          "form": "오늘 오랜만에 마라탕 시켜먹었어요~~",
          "original_form": "오늘 오랜만에 마라탕 시켜먹었어요~~",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100016760.1.3",
          "form": "한번씯 땡길 때가 있더라고요#",
          "original_form": "한번씯 땡길 때가 있더라고요#",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100016760.1.4",
          "form": "마라탕에 중국당면 넣어드시나요?",
          "original_form": "마라탕에 중국당면 넣어드시나요?",
          "speaker_id": "1",
          "time": "20210826 21:14"
        },
        {
          "id": "MDRW2100016760.1.5",
          "form": "네~ 당면 좋아해서 종류별로 다 넣어먹어요~",
          "original_form": "네~ 당면 좋아해서 종류별로 다 넣어먹어요~",
          "speaker_id": "2",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100016760.1.6",
          "form": "마라탕 좋아하세요??",
          "original_form": "마라탕 좋아하세요??",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100016760.1.7",
          "form": "완전 좋아해요!!",
          "original_form": "완전 좋아해요!!",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100016760.1.8",
          "form": "네모난 두부볼이랑 포두부 제일 좋아해요",
          "original_form": "네모난 두부볼이랑 포두부 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100016760.1.9",
          "form": "어떤거 제일 좋아하세요?",
          "original_form": "어떤거 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100016760.1.10",
          "form": "일단 당면이 제일 좋고",
          "original_form": "일단 당면이 제일 좋고",
          "speaker_id": "2",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100016760.1.11",
          "form": "그다음은 두부류요~",
          "original_form": "그다음은 두부류요~",
          "speaker_id": "2",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100016760.1.12",
          "form": "야채도 좋아하기는 하는데",
          "original_form": "야채도 좋아하기는 하는데",
          "speaker_id": "2",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100016760.1.13",
          "form": "당면하고 두부류 비율이 제일 높은 거 같아요~",
          "original_form": "당면하고 두부류 비율이 제일 높은 거 같아요~",
          "speaker_id": "2",
          "time": "20210827 10:28"
        },
        {
          "id": "MDRW2100016760.1.14",
          "form": "저두 두부류 젤 많이 넣어요ㅋㅋ",
          "original_form": "저두 두부류 젤 많이 넣어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100016760.1.15",
          "form": "그리고 소고기추가...",
          "original_form": "그리고 소고기추가...",
          "speaker_id": "1",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100016760.1.16",
          "form": "오~ 저는 이상하게 고기추가를 안하게 되더라고요~",
          "original_form": "오~ 저는 이상하게 고기추가를 안하게 되더라고요~",
          "speaker_id": "2",
          "time": "20210828 09:34"
        },
        {
          "id": "MDRW2100016760.1.17",
          "form": "이미 맑지 않지만, 그나마 맑은 국물을 먹고 싶어서.ㅎㅎ",
          "original_form": "이미 맑지 않지만, 그나마 맑은 국물을 먹고 싶어서.ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100016760.1.18",
          "form": "아하 고기추가 안하는분 첨봐요 신기하네용ㅎㅎ",
          "original_form": "아하 고기추가 안하는분 첨봐요 신기하네용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100016760.1.19",
          "form": "초반에는 고기 넣어먹었었는데",
          "original_form": "초반에는 고기 넣어먹었었는데",
          "speaker_id": "2",
          "time": "20210828 23:44"
        },
        {
          "id": "MDRW2100016760.1.20",
          "form": "어느 순간 부터 안 먹게 되더라고요~",
          "original_form": "어느 순간 부터 안 먹게 되더라고요~",
          "speaker_id": "2",
          "time": "20210828 23:44"
        },
        {
          "id": "MDRW2100016760.1.21",
          "form": "원래 소고기를 별로 안 좋아하기도 했어가지고..ㅎㅎ",
          "original_form": "원래 소고기를 별로 안 좋아하기도 했어가지고..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:44"
        },
        {
          "id": "MDRW2100016760.1.22",
          "form": "아하 소고기 안좋아하시는구나",
          "original_form": "아하 소고기 안좋아하시는구나",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100016760.1.23",
          "form": "돼지고기는 좋아하세요?",
          "original_form": "돼지고기는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100016760.1.24",
          "form": "네~ 돼지고기는 좋아해요~",
          "original_form": "네~ 돼지고기는 좋아해요~",
          "speaker_id": "2",
          "time": "20210829 01:04"
        },
        {
          "id": "MDRW2100016760.1.25",
          "form": "제일좋아하는 건 닭고기고요~ㅎㅎ",
          "original_form": "제일좋아하는 건 닭고기고요~ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 01:04"
        },
        {
          "id": "MDRW2100016760.1.26",
          "form": "닭고기 맛있죠ㅎㅎ",
          "original_form": "닭고기 맛있죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100016760.1.27",
          "form": "닭은 언제나 옳아요",
          "original_form": "닭은 언제나 옳아요",
          "speaker_id": "1",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100016760.1.28",
          "form": "맞아요~ 어떤 요리를 해도 닭은 항상 맛있는 거 같아요~",
          "original_form": "맞아요~ 어떤 요리를 해도 닭은 항상 맛있는 거 같아요~",
          "speaker_id": "2",
          "time": "20210830 11:27"
        },
        {
          "id": "MDRW2100016760.1.29",
          "form": "간만에 이삭토스트 먹고싶네요",
          "original_form": "간만에 이삭토스트 먹고싶네요",
          "speaker_id": "1",
          "time": "20210830 18:40"
        }
      ]
