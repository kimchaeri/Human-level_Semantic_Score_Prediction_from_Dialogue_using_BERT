      "utterance": [
        {
          "id": "MDRW2100023965.1.1",
          "form": "저녁 식사하셨나요~?",
          "original_form": "저녁 식사하셨나요~?",
          "speaker_id": "1",
          "time": "20210830 20:40"
        },
        {
          "id": "MDRW2100023965.1.2",
          "form": "네 먹었어요~~~드셨나요?",
          "original_form": "네 먹었어요~~~드셨나요?",
          "speaker_id": "2",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100023965.1.3",
          "form": "전 오늘 저녁 굶었어요~",
          "original_form": "전 오늘 저녁 굶었어요~",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100023965.1.4",
          "form": "저녁 메뉴 뭐드셨어요?",
          "original_form": "저녁 메뉴 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100023965.1.5",
          "form": "다욧 하시나여? 조금이라더 드셔야죵 ㅠ",
          "original_form": "다욧 하시나여? 조금이라더 드셔야죵 ㅠ",
          "speaker_id": "2",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100023965.1.6",
          "form": "저는 오늘 제사라 ㅋㅋㅋㅋ탕국 먹었어요",
          "original_form": "저는 오늘 제사라 ㅋㅋㅋㅋ탕국 먹었어요",
          "speaker_id": "2",
          "time": "20210830 21:51"
        },
        {
          "id": "MDRW2100023965.1.7",
          "form": "헉  저 탕국 완전 좋아해요ㅠㅠ",
          "original_form": "헉  저 탕국 완전 좋아해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100023965.1.8",
          "form": "저녁 굶고 대신 내일 아침에 스테이크 먹을까 고민중이예요 ㅎㅎ",
          "original_form": "저녁 굶고 대신 내일 아침에 스테이크 먹을까 고민중이예요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 21:58"
        },
        {
          "id": "MDRW2100023965.1.9",
          "form": "아우 고급져 스테이크 아침 먹을시간 없는데 전 부러워요",
          "original_form": "아우 고급져 스테이크 아침 먹을시간 없는데 전 부러워요",
          "speaker_id": "2",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100023965.1.10",
          "form": "저도 막상 아침되면 늦잠자기 바쁠수도 있어요 ㅎㅎㅎㅋㅋㅋㅋㅋ 스테이크가 너무 먹고싶은데.. 지금은 못먹으니까 일단 아침으로 미뤄봅니다ㅠㅠ",
          "original_form": "저도 막상 아침되면 늦잠자기 바쁠수도 있어요 ㅎㅎㅎㅋㅋㅋㅋㅋ 스테이크가 너무 먹고싶은데.. 지금은 못먹으니까 일단 아침으로 미뤄봅니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100023965.1.11",
          "form": "양파랑 딱 구워가지고...",
          "original_form": "양파랑 딱 구워가지고...",
          "speaker_id": "1",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100023965.1.12",
          "form": "아 양파...아스파라거스 마늘도 궈서 어우.... 침고여",
          "original_form": "아 양파...아스파라거스 마늘도 궈서 어우.... 침고여",
          "speaker_id": "2",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100023965.1.13",
          "form": "소스랑해도 맛있고 히말라야 핑크소금과 와사비랑 먹어도 맛있고 그 겨자랑 먹어도 맛나요",
          "original_form": "소스랑해도 맛있고 히말라야 핑크소금과 와사비랑 먹어도 맛있고 그 겨자랑 먹어도 맛나요",
          "speaker_id": "2",
          "time": "20210830 22:11"
        },
        {
          "id": "MDRW2100023965.1.14",
          "form": "아악 맛있겠다ㅠㅠㅋㅋㅋㅋㅋㅋ 지금 당장 먹고싶네요",
          "original_form": "아악 맛있겠다ㅠㅠㅋㅋㅋㅋㅋㅋ 지금 당장 먹고싶네요",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100023965.1.15",
          "form": "전 와사비가 원픽이예요 ㅋㅋ",
          "original_form": "전 와사비가 원픽이예요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100023965.1.16",
          "form": "내일 일찍 일어나서 아침먹기 도전해봅니당",
          "original_form": "내일 일찍 일어나서 아침먹기 도전해봅니당",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100023965.1.17",
          "form": "후기 기대합니다",
          "original_form": "후기 기대합니다{emoji:♥}",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100023965.1.18",
          "form": "저도 와사비 원픽요 ㅋㅋ",
          "original_form": "저도 와사비 원픽요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100023965.1.19",
          "form": "결국 늦잠자서 못먹었네요ㅠㅋㅋㅋㅋㅋ",
          "original_form": "결국 늦잠자서 못먹었네요ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 08:42"
        },
        {
          "id": "MDRW2100023965.1.20",
          "form": "헐 ㅠㅠㅠ 왜요 기대햇는데 ㅠㅠ 낼 먹어줘요 ㅠㅠ",
          "original_form": "헐 ㅠㅠㅠ 왜요 기대햇는데 ㅠㅠ 낼 먹어줘요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 22:39"
        },
        {
          "id": "MDRW2100023965.1.21",
          "form": "오늘도 늦게 자서.. 낼도 왠지 못먹을굿 같은 예감 ㅠㅠㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오늘도 늦게 자서.. 낼도 왠지 못먹을굿 같은 예감 ㅠㅠㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 00:30"
        },
        {
          "id": "MDRW2100023965.1.22",
          "form": "주말에 제대로 먹을까싶기도해요ㅠㅠ배고파라..",
          "original_form": "주말에 제대로 먹을까싶기도해요ㅠㅠ배고파라..",
          "speaker_id": "1",
          "time": "20210901 00:30"
        },
        {
          "id": "MDRW2100023965.1.23",
          "form": "ㅋㅋㅋㅋ 주말에 먹는게 맘  편해요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ 주말에 먹는게 맘  편해요 ㅠㅠ",
