      "utterance": [
        {
          "id": "MDRW2100014577.1.1",
          "form": "어떤 택배 받으셨는지 궁금해요 ㅎㅎ",
          "original_form": "어떤 택배 받으셨는지 궁금해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014577.1.2",
          "form": "저는 알리에서 온 캠핑용전구도 오늘 왔어요 ㅋㅋ",
          "original_form": "저는 알리에서 온 캠핑용전구도 오늘 왔어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014577.1.3",
          "form": "아직도 도착하지 않은 택배들이 산더미인데...",
          "original_form": "아직도 도착하지 않은 택배들이 산더미인데...",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014577.1.4",
          "form": "남편이 머라할 듯 해요 ㅋㅋㅋ",
          "original_form": "남편이 머라할 듯 해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014577.1.5",
          "form": "아 저도 알리 사용해요",
          "original_form": "아 저도 알리 사용해요",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014577.1.6",
          "form": "뭐언젠가 오더라구요 ㅋㅋ",
          "original_form": "뭐언젠가 오더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014577.1.7",
          "form": "그렇죠 어서 까서 숨기세요",
          "original_form": "그렇죠 어서 까서 숨기세요",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014577.1.8",
          "form": "증거인멸 ㅋㅋ",
          "original_form": "증거인멸 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014577.1.9",
          "form": "알리 너무 싸요 ㅎㅎ",
          "original_form": "알리 너무 싸요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014577.1.10",
          "form": "ㅋㅋㅋㅋ 근데.. 쌓여있는 택배상자들.... ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 근데.. 쌓여있는 택배상자들.... ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014577.1.11",
          "form": "같은 건데도.. 알리가 훨 싸더라구요..ㅎㅎ",
          "original_form": "같은 건데도.. 알리가 훨 싸더라구요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014577.1.12",
          "form": "단지 오래기다려야된다는 단점이....",
          "original_form": "단지 오래기다려야된다는 단점이....",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014577.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014577.1.14",
          "form": "분실위험도 있구..ㅋㅋㅋ",
          "original_form": "분실위험도 있구..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014577.1.15",
          "form": "저 한달지났는데 안온거 있어요 ㅋㅋ",
          "original_form": "저 한달지났는데 안온거 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014577.1.16",
          "form": "아직까지 안온거는 저는 없었어요",
          "original_form": "아직까지 안온거는 저는 없었어요",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014577.1.17",
          "form": "네 분실 몇번되 봤어요",
          "original_form": "네 분실 몇번되 봤어요",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014577.1.18",
          "form": "전 있었어요",
          "original_form": "전 있었어요",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014577.1.19",
          "form": "두달 지나니깐.. 우편함에 있더라구요",
          "original_form": "두달 지나니깐.. 우편함에 있더라구요",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014577.1.20",
          "form": "환불해주던데요",
          "original_form": "환불해주던데요",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014577.1.21",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014577.1.22",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014577.1.23",
          "form": "저도 그때쯤 오면좋겠어요",
          "original_form": "저도 그때쯤 오면좋겠어요",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014577.1.24",
          "form": "환불보다",
          "original_form": "환불보다",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014577.1.25",
          "form": "친구도 디스풋걸어서 환불받았는데",
          "original_form": "친구도 디스풋걸어서 환불받았는데",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014577.1.26",
          "form": "도착했다더라구요 ㅋㅋㅋ",
          "original_form": "도착했다더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014577.1.27",
          "form": "한참후에",
          "original_form": "한참후에",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014577.1.28",
          "form": "다시 돌려주기도.. 힘들고 ㅋㅋㅋ그래서",
          "original_form": "다시 돌려주기도.. 힘들고 ㅋㅋㅋ그래서",
          "speaker_id": "1",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014577.1.29",
          "form": "아 그렇죠 ㅋㅋ돌려줄수가 없어요",
          "original_form": "아 그렇죠 ㅋㅋ돌려줄수가 없어요",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014577.1.30",
          "form": "항공비가 더들겠죠 ㅋㅋ",
          "original_form": "항공비가 더들겠죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014577.1.31",
          "form": "서로 거부하지 싶어욯ㅎ",
          "original_form": "서로 거부하지 싶어욯ㅎ",
          "speaker_id": "1",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014577.1.32",
          "form": "인사 하려 했는데",
          "original_form": "인사 하려 했는데",
          "speaker_id": "2",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100014577.1.33",
          "form": "그동안 감사합니다 ㅎㅎ",
          "original_form": "그동안 감사합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 16:16"
        },
        {
          "id": "MDRW2100014577.1.34",
          "form": "이거하느라 넘 신경써서 ㅋㅋ힘들었지만 잼났어요",
          "original_form": "이거하느라 넘 신경써서 ㅋㅋ힘들었지만 잼났어요",
          "speaker_id": "2",
