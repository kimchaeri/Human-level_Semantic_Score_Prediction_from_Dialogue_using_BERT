      "utterance": [
        {
          "id": "MDRW2100016047.1.1",
          "form": "안녕하세요 아침에 식사는 하셨나요??",
          "original_form": "안녕하세요 아침에 식사는 하셨나요??",
          "speaker_id": "1",
          "time": "20210826 08:46"
        },
        {
          "id": "MDRW2100016047.1.2",
          "form": "네 전복죽과 복슝아 스무디요 name1님은요?",
          "original_form": "네 전복죽과 복슝아 스무디요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 09:21"
        },
        {
          "id": "MDRW2100016047.1.3",
          "form": "집에서 해서 드신거에요??",
          "original_form": "집에서 해서 드신거에요??",
          "speaker_id": "1",
          "time": "20210826 09:22"
        },
        {
          "id": "MDRW2100016047.1.4",
          "form": "전 지금 곤드레볶음밥 볶구있는중이요",
          "original_form": "전 지금 곤드레볶음밥 볶구있는중이요",
          "speaker_id": "1",
          "time": "20210826 09:22"
        },
        {
          "id": "MDRW2100016047.1.5",
          "form": "복숭아 스무디 맛있겠어요",
          "original_form": "복숭아 스무디 맛있겠어요",
          "speaker_id": "1",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016047.1.6",
          "form": "네 어머니가 해주셨어요",
          "original_form": "네 어머니가 해주셨어요",
          "speaker_id": "2",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016047.1.7",
          "form": "곤드레 맛있겠어요",
          "original_form": "곤드레 맛있겠어요",
          "speaker_id": "2",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016047.1.8",
          "form": "와 부러워요 미혼이시죠?",
          "original_form": "와 부러워요 미혼이시죠?",
          "speaker_id": "1",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100016047.1.9",
          "form": "어머니가 옆에서 챙겨주실때가...가장 좋아요^^",
          "original_form": "어머니가 옆에서 챙겨주실때가...가장 좋아요^^",
          "speaker_id": "1",
          "time": "20210826 11:05"
        },
        {
          "id": "MDRW2100016047.1.10",
          "form": "네 미혼이에여  ㅋㅋ 그렇군요 ㅠㅠ",
          "original_form": "네 미혼이에여  ㅋㅋ 그렇군요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 11:36"
        },
        {
          "id": "MDRW2100016047.1.11",
          "form": "부러워요 ㅎ 많이 누리고 결혼하세요",
          "original_form": "부러워요 ㅎ 많이 누리고 결혼하세요",
          "speaker_id": "1",
          "time": "20210826 11:37"
        },
        {
          "id": "MDRW2100016047.1.12",
          "form": "네 ㅋ 아직 좀 남아서 많이 누려야겠네요",
          "original_form": "네 ㅋ 아직 좀 남아서 많이 누려야겠네요",
          "speaker_id": "2",
          "time": "20210826 11:38"
        },
        {
          "id": "MDRW2100016047.1.13",
          "form": "점심은 뭐드세요?",
          "original_form": "점심은 뭐드세요?",
          "speaker_id": "2",
          "time": "20210826 11:38"
        },
        {
          "id": "MDRW2100016047.1.14",
          "form": "빵먹을려구요",
          "original_form": "빵먹을려구요",
          "speaker_id": "1",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016047.1.15",
          "form": "지금은 아메리카노 한잔중입니다",
          "original_form": "지금은 아메리카노 한잔중입니다",
          "speaker_id": "1",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100016047.1.16",
          "form": "오 저는 점심 먹는중이에요 나중에 한잔해야겠어요",
          "original_form": "오 저는 점심 먹는중이에요 나중에 한잔해야겠어요",
          "speaker_id": "2",
          "time": "20210826 12:00"
        },
        {
          "id": "MDRW2100016047.1.17",
          "form": "메뉴 뭔가요??",
          "original_form": "메뉴 뭔가요??",
          "speaker_id": "1",
          "time": "20210826 12:01"
        },
        {
          "id": "MDRW2100016047.1.18",
          "form": "점심드셨어요?",
          "original_form": "점심드셨어요?",
          "speaker_id": "1",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016047.1.19",
          "form": "네 굴림만두국이요 점심 뭐드셨어요",
          "original_form": "네 굴림만두국이요 점심 뭐드셨어요",
          "speaker_id": "2",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100016047.1.20",
          "form": "첨들어봐요 맛나겠어요",
          "original_form": "첨들어봐요 맛나겠어요",
          "speaker_id": "1",
          "time": "20210826 17:35"
        },
