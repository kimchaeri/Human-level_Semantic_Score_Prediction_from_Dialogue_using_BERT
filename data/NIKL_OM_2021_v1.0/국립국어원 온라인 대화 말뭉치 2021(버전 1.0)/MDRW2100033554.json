      "utterance": [
        {
          "id": "MDRW2100033554.1.1",
          "form": "저 카레요 ㅎ",
          "original_form": "저 카레요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 14:08"
        },
        {
          "id": "MDRW2100033554.1.2",
          "form": "머드셨어요?",
          "original_form": "머드셨어요?",
          "speaker_id": "2",
          "time": "20210824 14:08"
        },
        {
          "id": "MDRW2100033554.1.3",
          "form": "전 바지락칼국수ㅎ",
          "original_form": "전 바지락칼국수ㅎ",
          "speaker_id": "1",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100033554.1.4",
          "form": "비오는날딱이네요",
          "original_form": "비오는날딱이네요",
          "speaker_id": "2",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100033554.1.5",
          "form": "저는 애들땜에 맨날 짜장만하다가",
          "original_form": "저는 애들땜에 맨날 짜장만하다가",
          "speaker_id": "2",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100033554.1.6",
          "form": "카레가 먹고싶어서 조금만해야지했는데",
          "original_form": "카레가 먹고싶어서 조금만해야지했는데",
          "speaker_id": "2",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100033554.1.7",
          "form": "한솥만들어졌네요 ㅋ",
          "original_form": "한솥만들어졌네요 ㅋ",
          "speaker_id": "2",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100033554.1.8",
          "form": "ㅎㅎ 카레가 항상그렇더라구요ㅎ",
          "original_form": "ㅎㅎ 카레가 항상그렇더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100033554.1.9",
          "form": "채소 넣음 냄비가 넘쳐요",
          "original_form": "채소 넣음 냄비가 넘쳐요",
          "speaker_id": "1",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100033554.1.10",
          "form": "애들안먹으면 제가 다 먹어야되는데",
          "original_form": "애들안먹으면 제가 다 먹어야되는데",
          "speaker_id": "2",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100033554.1.11",
          "form": "오늘 저녁메뉴로 줘볼라구요",
          "original_form": "오늘 저녁메뉴로 줘볼라구요",
          "speaker_id": "2",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100033554.1.12",
          "form": "ㅎㅎ 사과좀 넣고 달달하게",
          "original_form": "ㅎㅎ 사과좀 넣고 달달하게",
          "speaker_id": "1",
          "time": "20210824 14:12"
        },
        {
          "id": "MDRW2100033554.1.13",
          "form": "감자대신 고구마넣었어요",
          "original_form": "감자대신 고구마넣었어요",
          "speaker_id": "2",
          "time": "20210824 14:12"
        },
        {
          "id": "MDRW2100033554.1.14",
          "form": "달달하라고요",
          "original_form": "달달하라고요",
          "speaker_id": "2",
          "time": "20210824 14:12"
        },
        {
          "id": "MDRW2100033554.1.15",
          "form": "사과도 넣나요?",
          "original_form": "사과도 넣나요?",
          "speaker_id": "2",
          "time": "20210824 14:12"
        },
        {
          "id": "MDRW2100033554.1.16",
          "form": "사과넣은 카레도 있자나요",
          "original_form": "사과넣은 카레도 있자나요",
          "speaker_id": "1",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100033554.1.17",
          "form": "3분카레 가루",
          "original_form": "3분카레 가루",
          "speaker_id": "1",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100033554.1.18",
          "form": "그래요? 사과넣는건 생각못했네요",
          "original_form": "그래요? 사과넣는건 생각못했네요",
          "speaker_id": "2",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100033554.1.19",
          "form": "지금이라도 다시 끓여야하나",
          "original_form": "지금이라도 다시 끓여야하나",
          "speaker_id": "2",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100033554.1.20",
          "form": "고민되네요 ㅎ",
          "original_form": "고민되네요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100033554.1.21",
          "form": "검색해보세요ㅎ 오뚜기3분카레 사과그림있는데ㅋ",
          "original_form": "검색해보세요ㅎ 오뚜기3분카레 사과그림있는데ㅋ",
          "speaker_id": "1",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100033554.1.22",
          "form": "카레의 여왕만사다보니 ㅋ",
          "original_form": "카레의 여왕만사다보니 ㅋ",
          "speaker_id": "2",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100033554.1.23",
          "form": "ㅎㅎ 고구마도 맛있을듯",
          "original_form": "ㅎㅎ 고구마도 맛있을듯",
          "speaker_id": "1",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100033554.1.24",
          "form": "카레에 김싸서 먹었더니 너무 많이 먹었나봐요 ㅎ",
          "original_form": "카레에 김싸서 먹었더니 너무 많이 먹었나봐요 ㅎ",
          "speaker_id": "2",
          "time": "20210824 14:17"
        }
      ]
    }
