        {
          "id": "MDRW2100035443.1.1",
          "form": "저녁 메뉴 정하셨어요?",
          "original_form": "저녁 메뉴 정하셨어요?",
          "speaker_id": "2",
          "time": "20210829 17:23"
        },
        {
          "id": "MDRW2100035443.1.2",
          "form": "저는 아직요ㅠ",
          "original_form": "저는 아직요ㅠ",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100035443.1.3",
          "form": "name2님은 정하셨나요?",
          "original_form": "&name2&님은 정하셨나요?",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100035443.1.4",
          "form": "아직요ㅠ",
          "original_form": "아직요ㅠ",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100035443.1.5",
          "form": "점심은",
          "original_form": "점심은",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100035443.1.6",
          "form": "김치찌개 싸악",
          "original_form": "김치찌개 싸악",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100035443.1.7",
          "form": "했습니다",
          "original_form": "했습니다",
          "speaker_id": "1",
          "time": "20210829 17:31"
        },
        {
          "id": "MDRW2100035443.1.8",
          "form": "오오 한식!",
          "original_form": "오오 한식!",
          "speaker_id": "2",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100035443.1.9",
          "form": "지금은 돈까스가 먹고싶어요",
          "original_form": "지금은 돈까스가 먹고싶어요",
          "speaker_id": "1",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100035443.1.10",
          "form": "저는 열무국수 뚝딱 했습니닷",
          "original_form": "저는 열무국수 뚝딱 했습니닷",
          "speaker_id": "2",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100035443.1.11",
          "form": "아 돈까스!!",
          "original_form": "아 돈까스!!",
          "speaker_id": "2",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100035443.1.12",
          "form": "어떤 돈까스요??",
          "original_form": "어떤 돈까스요??",
          "speaker_id": "2",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100035443.1.13",
          "form": "저 약간 옛날돈까스 스타일 좋아합니다",
          "original_form": "저 약간 옛날돈까스 스타일 좋아합니다",
          "speaker_id": "1",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100035443.1.14",
          "form": "갈색소스부먹",
          "original_form": "갈색소스부먹",
          "speaker_id": "1",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100035443.1.15",
          "form": "정석이 최고죠",
          "original_form": "정석이 최고죠",
          "speaker_id": "2",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100035443.1.16",
          "form": "전 치즈도 좋아해요!",
          "original_form": "전 치즈도 좋아해요!",
          "speaker_id": "2",
          "time": "20210829 17:53"
        },
        {
          "id": "MDRW2100035443.1.17",
          "form": "최고네요",
          "original_form": "최고네요",
          "speaker_id": "1",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100035443.1.18",
          "form": "허기지네요",
          "original_form": "허기지네요",
          "speaker_id": "1",
          "time": "20210829 17:54"
        },
        {
          "id": "MDRW2100035443.1.19",
          "form": "이제 슬슬 정할때인가봐요",
          "original_form": "이제 슬슬 정할때인가봐요",
          "speaker_id": "2",
          "time": "20210829 17:59"
        },
        {
          "id": "MDRW2100035443.1.20",
          "form": "스팸도 살짝 굽고싶고",
          "original_form": "스팸도 살짝 굽고싶고",
          "speaker_id": "1",
          "time": "20210829 18:00"
        },
        {
          "id": "MDRW2100035443.1.21",
          "form": "아 그것도 좋네요",
          "original_form": "아 그것도 좋네요",
          "speaker_id": "2",
          "time": "20210829 18:01"
        },
        {
          "id": "MDRW2100035443.1.22",
          "form": "김하고 싸먹어도 맛있죠",
          "original_form": "김하고 싸먹어도 맛있죠",
          "speaker_id": "2",
          "time": "20210829 18:01"
        },
        {
          "id": "MDRW2100035443.1.23",
          "form": "계란후라이랑 스팸해서 김찌랑 김싸서",
          "original_form": "계란후라이랑 스팸해서 김찌랑 김싸서",
          "speaker_id": "1",
          "time": "20210829 18:02"
        },
