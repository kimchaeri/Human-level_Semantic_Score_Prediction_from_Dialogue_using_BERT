      "utterance": [
        {
          "id": "MDRW2100041269.1.1",
          "form": "일하러갔나?",
          "original_form": "일하러갔나?",
          "speaker_id": "2",
          "time": "20210906 10:13"
        },
        {
          "id": "MDRW2100041269.1.2",
          "form": "오늘시원하네ㅋ",
          "original_form": "오늘시원하네ㅋ",
          "speaker_id": "2",
          "time": "20210906 10:13"
        },
        {
          "id": "MDRW2100041269.1.3",
          "form": "난 차에있으니 덥다",
          "original_form": "난 차에있으니 덥다",
          "speaker_id": "2",
          "time": "20210906 10:13"
        },
        {
          "id": "MDRW2100041269.1.4",
          "form": "응  알바다녀왔다ㅋㅋ",
          "original_form": "응  알바다녀왔다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:05"
        },
        {
          "id": "MDRW2100041269.1.5",
          "form": "ㅋㄱㄲㄱㅋ언니 점심은요?",
          "original_form": "ㅋㄱㄲㄱㅋ언니 점심은요?",
          "speaker_id": "1",
          "time": "20210906 12:37"
        },
        {
          "id": "MDRW2100041269.1.6",
          "form": "떡먹어서ㅋ 셀러드먹을라고",
          "original_form": "떡먹어서ㅋ 셀러드먹을라고",
          "speaker_id": "2",
          "time": "20210906 12:48"
        },
        {
          "id": "MDRW2100041269.1.7",
          "form": "안먹지는않음",
          "original_form": "안먹지는않음",
          "speaker_id": "2",
          "time": "20210906 12:48"
        },
        {
          "id": "MDRW2100041269.1.8",
          "form": "큰일임ㅋ",
          "original_form": "큰일임ㅋ",
          "speaker_id": "2",
          "time": "20210906 12:49"
        },
        {
          "id": "MDRW2100041269.1.9",
          "form": "ㅋㅋ언니 나두요",
          "original_form": "ㅋㅋ언니 나두요",
          "speaker_id": "1",
          "time": "20210906 12:49"
        },
        {
          "id": "MDRW2100041269.1.10",
          "form": "진짜 배고파서 이제 밥 좀 해먹으려구욬ㅋ",
          "original_form": "진짜 배고파서 이제 밥 좀 해먹으려구욬ㅋ",
          "speaker_id": "1",
          "time": "20210906 12:49"
        },
        {
          "id": "MDRW2100041269.1.11",
          "form": "지금은 커피타임",
          "original_form": "지금은 커피타임",
          "speaker_id": "2",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100041269.1.12",
          "form": "애들올때까지 쫌쉬어야긋다",
          "original_form": "애들올때까지 쫌쉬어야긋다",
          "speaker_id": "2",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100041269.1.13",
          "form": "ㅋㅋㅋㅋ저두요 언니 조금이따가 옷 정리해야되는데 ㅋㅋ 귀찮네요 ㅋ",
          "original_form": "ㅋㅋㅋㅋ저두요 언니 조금이따가 옷 정리해야되는데 ㅋㅋ 귀찮네요 ㅋ",
          "speaker_id": "1",
          "time": "20210906 14:19"
        },
        {
          "id": "MDRW2100041269.1.14",
          "form": "나도 버릴것도 많고ㅡㅡ",
          "original_form": "나도 버릴것도 많고ㅡㅡ",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100041269.1.15",
          "form": "작아진옷들 정리좀 해야되는데 ㅋㅋ",
          "original_form": "작아진옷들 정리좀 해야되는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100041269.1.16",
          "form": "진짜 ㅋㅋㅋㅋ",
          "original_form": "진짜 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100041269.1.17",
          "form": "언니 첫째 집에 왔는데 간식으로 김치볶음밥을 해달래요",
          "original_form": "언니 첫째 집에 왔는데 간식으로 김치볶음밥을 해달래요",
          "speaker_id": "1",
          "time": "20210906 15:28"
        },
        {
          "id": "MDRW2100041269.1.18",
          "form": "그게간식이가?ㅅ",
          "original_form": "그게간식이가?ㅅ",
          "speaker_id": "2",
          "time": "20210906 15:34"
        },
        {
          "id": "MDRW2100041269.1.19",
          "form": "대박ㅋㅋ",
          "original_form": "대박ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 15:34"
        },
        {
          "id": "MDRW2100041269.1.20",
          "form": "잘먹으니 좋으네ㅋ",
          "original_form": "잘먹으니 좋으네ㅋ",
          "speaker_id": "2",
          "time": "20210906 15:34"
        },
        {
          "id": "MDRW2100041269.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ밥이 없어서 ㅋㅋㅋ 못해줬어요 ㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ밥이 없어서 ㅋㅋㅋ 못해줬어요 ㅋ",
          "speaker_id": "1",
          "time": "20210906 15:35"
        },
        {
          "id": "MDRW2100041269.1.22",
          "form": "그러니깐요 그게 간식인가봐요",
          "original_form": "그러니깐요 그게 간식인가봐요",
          "speaker_id": "1",
          "time": "20210906 15:35"
        },
        {
          "id": "MDRW2100041269.1.23",
          "form": "첵스초코 주려구요 ㅋ",
          "original_form": "첵스초코 주려구요 ㅋ",
          "speaker_id": "1",
          "time": "20210906 15:35"
        },
        {
          "id": "MDRW2100041269.1.24",
          "form": "오늘 애들 병원갔다가 간다고",
          "original_form": "오늘 애들 병원갔다가 간다고",
          "speaker_id": "2",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100041269.1.25",
          "form": "너의첫째 혼자서있길래이야기해줬다",
          "original_form": "너의첫째 혼자서있길래이야기해줬다",
          "speaker_id": "2",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100041269.1.26",
          "form": "비염에 알레르기ㅡㅡ",
          "original_form": "비염에 알레르기ㅡㅡ",
          "speaker_id": "2",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100041269.1.27",
          "form": "계절성",
          "original_form": "계절성",
          "speaker_id": "2",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100041269.1.28",
          "form": "아이고 비염 때문에 고생하네요",
          "original_form": "아이고 비염 때문에 고생하네요",
          "speaker_id": "1",
          "time": "20210906 16:28"
        },
        {
          "id": "MDRW2100041269.1.29",
          "form": "계절바뀔때마다 그렇네ㅋ",
          "original_form": "계절바뀔때마다 그렇네ㅋ",
          "speaker_id": "2",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100041269.1.30",
          "form": "이번엔 지나가나 했더니",
          "original_form": "이번엔 지나가나 했더니",
          "speaker_id": "2",
          "time": "20210906 16:51"
        }
      ]
    }
  ]
