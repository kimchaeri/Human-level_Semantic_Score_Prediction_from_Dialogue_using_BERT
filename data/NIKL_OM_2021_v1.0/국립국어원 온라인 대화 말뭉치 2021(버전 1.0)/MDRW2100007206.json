      "utterance": [
        {
          "id": "MDRW2100007206.1.1",
          "form": "혹시 만두는 어떤거 드세요??",
          "original_form": "혹시 만두는 어떤거 드세요??",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007206.1.2",
          "form": "저 근데 요리를 거의 안해서",
          "original_form": "저 근데 요리를 거의 안해서",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007206.1.3",
          "form": "군만두 찐만두 거의다...",
          "original_form": "군만두 찐만두 거의다...",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007206.1.4",
          "form": "딤섬류 좋아해요",
          "original_form": "딤섬류 좋아해요",
          "speaker_id": "2",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007206.1.5",
          "form": "집에서 구우시는 브랜드 ㅋㅋㅋ",
          "original_form": "집에서 구우시는 브랜드 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007206.1.6",
          "form": "아하ㅋㅋㅋ비비고죠",
          "original_form": "아하ㅋㅋㅋ비비고죠",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007206.1.7",
          "form": "냉동 만두요",
          "original_form": "냉동 만두요",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007206.1.8",
          "form": "무난의극치 비비고",
          "original_form": "무난의극치 비비고",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007206.1.9",
          "form": "그거말곤모르겠네요...",
          "original_form": "그거말곤모르겠네요...",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007206.1.10",
          "form": "아니 고향만두를 왜 모르시나요 ㅋㅋㅋ",
          "original_form": "아니 고향만두를 왜 모르시나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007206.1.11",
          "form": "악ㅋㅋㅋ그건어디서출시된건가요?",
          "original_form": "악ㅋㅋㅋ그건어디서출시된건가요?",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007206.1.12",
          "form": "비비고가 소가 많이 들고 ㅋㅋ 맛있긴 한데 ㅋㅋㅋ",
          "original_form": "비비고가 소가 많이 들고 ㅋㅋ 맛있긴 한데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007206.1.13",
          "form": "고기가 많이 들어서 ㅋㅋㅋ",
          "original_form": "고기가 많이 들어서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007206.1.14",
          "form": "고향만두가 더 맛잇나요",
          "original_form": "고향만두가 더 맛잇나요",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007206.1.15",
          "form": "피가 얇진않나요?",
          "original_form": "피가 얇진않나요?",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007206.1.16",
          "form": "저한테는 안맞더라구요",
          "original_form": "저한테는 안맞더라구요",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007206.1.17",
          "form": "전 너무얇으면싫더라구요ㅠ",
          "original_form": "전 너무얇으면싫더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007206.1.18",
          "form": "고향만두 피 얇아여ㅠㅠ",
          "original_form": "고향만두 피 얇아여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007206.1.19",
          "form": "아하ㅠ그럼저는",
          "original_form": "아하ㅠ그럼저는",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007206.1.20",
          "form": "비비고계속.....",
          "original_form": "비비고계속.....",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007206.1.21",
          "form": "더 바사크 한데요 ㅋㅋㅋㅋㅋ",
          "original_form": "더 바사크 한데요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007206.1.22",
          "form": "잡채만두좋아하시나요?",
          "original_form": "잡채만두좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007206.1.23",
          "form": "요즘유행이던데",
          "original_form": "요즘유행이던데",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007206.1.24",
          "form": "네 ㅋㅋㅋㅋㅋ 좋아해용ㅋ",
          "original_form": "네 ㅋㅋㅋㅋㅋ 좋아해용ㅋ",
          "speaker_id": "1",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007206.1.25",
          "form": "맛있죠ㅎㅎ향긋",
          "original_form": "맛있죠ㅎㅎ향긋",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007206.1.26",
          "form": "부추넣은것도있던데",
          "original_form": "부추넣은것도있던데",
          "speaker_id": "2",
          "time": "20210822 00:55"
        }
      ]
    }
