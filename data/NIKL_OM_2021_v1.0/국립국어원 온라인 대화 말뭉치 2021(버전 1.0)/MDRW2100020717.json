      "utterance": [
        {
          "id": "MDRW2100020717.1.1",
          "form": "뭔가 굉장히 화학적인 맛",
          "original_form": "뭔가 굉장히 화학적인 맛",
          "speaker_id": "2",
          "time": "20210828 11:33"
        },
        {
          "id": "MDRW2100020717.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 11:33"
        },
        {
          "id": "MDRW2100020717.1.3",
          "form": "그거 잘먹는사람은",
          "original_form": "그거 잘먹는사람은",
          "speaker_id": "1",
          "time": "20210828 11:33"
        },
        {
          "id": "MDRW2100020717.1.4",
          "form": "포카리맛난다던데",
          "original_form": "포카리맛난다던데",
          "speaker_id": "1",
          "time": "20210828 11:33"
        },
        {
          "id": "MDRW2100020717.1.5",
          "form": "와 세상에..",
          "original_form": "와 세상에..",
          "speaker_id": "2",
          "time": "20210828 11:33"
        },
        {
          "id": "MDRW2100020717.1.6",
          "form": "이게?  포카리..?",
          "original_form": "이게?  포카리..?",
          "speaker_id": "1",
          "time": "20210828 11:33"
        },
        {
          "id": "MDRW2100020717.1.7",
          "form": "믿을 수 없죠",
          "original_form": "믿을 수 없죠",
          "speaker_id": "1",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100020717.1.8",
          "form": "청량갑 포카리를 여기에 댈 수 있나",
          "original_form": "청량갑 포카리를 여기에 댈 수 있나",
          "speaker_id": "2",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100020717.1.9",
          "form": "그니까요ㅋㅋㅋㅋㅋ",
          "original_form": "그니까요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100020717.1.10",
          "form": "아 그런데 자꾸 도전의식이 생겨서",
          "original_form": "아 그런데 자꾸 도전의식이 생겨서",
          "speaker_id": "2",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100020717.1.11",
          "form": "나 진짜 살면서 먹어본적도 없는 맛인데",
          "original_form": "나 진짜 살면서 먹어본적도 없는 맛인데",
          "speaker_id": "1",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100020717.1.12",
          "form": "드시나요",
          "original_form": "드시나요",
          "speaker_id": "1",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100020717.1.13",
          "form": "고수 보일 때마다 먹고 후회하고의 반복이에요",
          "original_form": "고수 보일 때마다 먹고 후회하고의 반복이에요",
          "speaker_id": "2",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100020717.1.14",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100020717.1.15",
          "form": "계속해도.. 적응은 안되더라구왜",
          "original_form": "계속해도.. 적응은 안되더라구왜",
          "speaker_id": "2",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100020717.1.16",
          "form": "ㅋㅋㅋ 뭔 느낌인지 알아요",
          "original_form": "ㅋㅋㅋ 뭔 느낌인지 알아요",
          "speaker_id": "1",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100020717.1.17",
          "form": "하루종일 입안에서 맴도는 고수향..",
          "original_form": "하루종일 입안에서 맴도는 고수향..",
          "speaker_id": "2",
          "time": "20210828 11:35"
        },
        {
          "id": "MDRW2100020717.1.18",
          "form": "우리는 도저히 적응불가 유전자를 갖고있나봐요",
          "original_form": "우리는 도저히 적응불가 유전자를 갖고있나봐요",
          "speaker_id": "1",
          "time": "20210828 11:35"
        },
        {
          "id": "MDRW2100020717.1.19",
          "form": "그 향 진짜 오래가긴하더라구요",
          "original_form": "그 향 진짜 오래가긴하더라구요",
          "speaker_id": "1",
          "time": "20210828 11:35"
        },
        {
          "id": "MDRW2100020717.1.20",
          "form": "그런가봐요ㅋㅋㅋㅋㅋㅋㅠㅠ",
          "original_form": "그런가봐요ㅋㅋㅋㅋㅋㅋㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 11:35"
        }
      ]
