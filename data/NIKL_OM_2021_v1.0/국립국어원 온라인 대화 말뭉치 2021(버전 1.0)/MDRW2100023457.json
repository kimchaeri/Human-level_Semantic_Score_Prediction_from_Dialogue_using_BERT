      "utterance": [
        {
          "id": "MDRW2100023457.1.1",
          "form": "국내산 수제맥주 만이 나오더라 편의점에",
          "original_form": "국내산 수제맥주 만이 나오더라 편의점에",
          "speaker_id": "2",
          "time": "20210830 03:35"
        },
        {
          "id": "MDRW2100023457.1.2",
          "form": "ㅇㅇ로제 맛있어 ㄹㅇ 맛으로 먹는 맥주",
          "original_form": "ㅇㅇ로제 맛있어 ㄹㅇ 맛으로 먹는 맥주",
          "speaker_id": "2",
          "time": "20210830 03:36"
        },
        {
          "id": "MDRW2100023457.1.3",
          "form": "약간 음료수 같음",
          "original_form": "약간 음료수 같음",
          "speaker_id": "2",
          "time": "20210830 03:36"
        },
        {
          "id": "MDRW2100023457.1.4",
          "form": "탄산은...글캐 쎄진 않았던듯??",
          "original_form": "탄산은...글캐 쎄진 않았던듯??",
          "speaker_id": "2",
          "time": "20210830 03:36"
        },
        {
          "id": "MDRW2100023457.1.5",
          "form": "먹고 크으으 하는 그런느낌은 ㅂㄹ 없어",
          "original_form": "먹고 크으으 하는 그런느낌은 ㅂㄹ 없어",
          "speaker_id": "2",
          "time": "20210830 03:36"
        },
        {
          "id": "MDRW2100023457.1.6",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210830 03:36"
        },
        {
          "id": "MDRW2100023457.1.7",
          "form": "달달한맥주야?신기하네",
          "original_form": "달달한맥주야?신기하네",
          "speaker_id": "1",
          "time": "20210830 03:36"
        },
        {
          "id": "MDRW2100023457.1.8",
          "form": "담에먹어봐야겠다",
          "original_form": "담에먹어봐야겠다",
          "speaker_id": "1",
          "time": "20210830 03:36"
        },
        {
          "id": "MDRW2100023457.1.9",
          "form": "난탄산강한게조아",
          "original_form": "난탄산강한게조아",
          "speaker_id": "1",
          "time": "20210830 03:36"
        },
        {
          "id": "MDRW2100023457.1.10",
          "form": "칭따오같은",
          "original_form": "칭따오같은",
          "speaker_id": "1",
          "time": "20210830 03:36"
        },
        {
          "id": "MDRW2100023457.1.11",
          "form": "ㅇㅇ달달하고",
          "original_form": "ㅇㅇ달달하고",
          "speaker_id": "2",
          "time": "20210830 03:36"
        },
        {
          "id": "MDRW2100023457.1.12",
          "form": "과일맛나",
          "original_form": "과일맛나",
          "speaker_id": "2",
          "time": "20210830 03:36"
        },
        {
          "id": "MDRW2100023457.1.13",
          "form": "과일소주맨치로?",
          "original_form": "과일소주맨치로?",
          "speaker_id": "1",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100023457.1.14",
          "form": "아하 칭따오는 탄산이 강하구나",
          "original_form": "아하 칭따오는 탄산이 강하구나",
          "speaker_id": "2",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100023457.1.15",
          "form": "오...신기하군",
          "original_form": "오...신기하군",
          "speaker_id": "1",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100023457.1.16",
          "form": "ㅇㅇ과일소주만큼 독하진않고",
          "original_form": "ㅇㅇ과일소주만큼 독하진않고",
          "speaker_id": "2",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100023457.1.17",
          "form": "암튼 로제 맛잇어",
          "original_form": "암튼 로제 맛잇어",
          "speaker_id": "2",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100023457.1.18",
          "form": "제일세지",
          "original_form": "제일세지",
          "speaker_id": "1",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100023457.1.19",
          "form": "양꼬치에칭따오아이가",
          "original_form": "양꼬치에칭따오아이가",
          "speaker_id": "1",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100023457.1.20",
          "form": "ㅋㅋㅋ나 진짜 맥주를 맛구분 못해",
          "original_form": "ㅋㅋㅋ나 진짜 맥주를 맛구분 못해",
          "speaker_id": "2",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100023457.1.21",
          "form": "로제도4캔만원하나?",
          "original_form": "로제도4캔만원하나?",
          "speaker_id": "1",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100023457.1.22",
          "form": "ㅋㅋ나도그래 근데",
          "original_form": "ㅋㅋ나도그래 근데",
          "speaker_id": "1",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100023457.1.23",
          "form": "맥주 같이 많이 마셔본 친구가 업어서 긍가바",
          "original_form": "맥주 같이 많이 마셔본 친구가 업어서 긍가바",
          "speaker_id": "2",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100023457.1.24",
          "form": "기네스랑 칭따오",
          "original_form": "기네스랑 칭따오",
          "speaker_id": "1",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100023457.1.25",
          "form": "그런건구분하지",
          "original_form": "그런건구분하지",
          "speaker_id": "1",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100023457.1.26",
          "form": "나랑...마셔줘...친구야....",
          "original_form": "나랑...마셔줘...친구야....",
          "speaker_id": "2",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100023457.1.27",
          "form": "제발...!!!!!",
          "original_form": "제발...!!!!!",
          "speaker_id": "1",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100023457.1.28",
          "form": "기네스는 하지 흑맥주잔아",
          "original_form": "기네스는 하지 흑맥주잔아",
          "speaker_id": "2",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100023457.1.29",
          "form": "기네스는특히",
          "original_form": "기네스는특히",
          "speaker_id": "1",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100023457.1.30",
          "form": "로제 4캔 만원 ㅇㅇ 근데 파는데 잘 업음",
          "original_form": "로제 4캔 만원 ㅇㅇ 근데 파는데 잘 업음",
          "speaker_id": "2",
          "time": "20210830 03:37"
        },
        {
          "id": "MDRW2100023457.1.31",
          "form": "공이들어가잇어서",
          "original_form": "공이들어가잇어서",
          "speaker_id": "1",
          "time": "20210830 03:38"
        },
        {
          "id": "MDRW2100023457.1.32",
          "form": "왜 제발이야",
          "original_form": "왜 제발이야",
          "speaker_id": "2",
          "time": "20210830 03:38"
        },
        {
          "id": "MDRW2100023457.1.33",
          "form": "공?",
          "original_form": "공?",
          "speaker_id": "2",
          "time": "20210830 03:38"
        },
        {
          "id": "MDRW2100023457.1.34",
          "form": "처음마실때불량인줄앎",
          "original_form": "처음마실때불량인줄앎",
          "speaker_id": "1",
          "time": "20210830 03:38"
        },
        {
          "id": "MDRW2100023457.1.35",
          "form": "ㅇㅇ아닌가?",
          "original_form": "ㅇㅇ아닌가?",
          "speaker_id": "1",
          "time": "20210830 03:38"
        },
        {
          "id": "MDRW2100023457.1.36",
          "form": "맥주안에 공이잇어??",
          "original_form": "맥주안에 공이잇어??",
          "speaker_id": "2",
          "time": "20210830 03:38"
        },
        {
          "id": "MDRW2100023457.1.37",
          "form": "첨들어봐",
          "original_form": "첨들어봐",
          "speaker_id": "2",
          "time": "20210830 03:38"
        }
      ]
    }
