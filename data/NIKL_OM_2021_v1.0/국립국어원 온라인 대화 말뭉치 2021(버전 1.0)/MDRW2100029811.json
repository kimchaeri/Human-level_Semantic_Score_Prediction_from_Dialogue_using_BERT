      "utterance": [
        {
          "id": "MDRW2100029811.1.1",
          "form": "배달음식 좋아하시나요?",
          "original_form": "배달음식 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210903 14:10"
        },
        {
          "id": "MDRW2100029811.1.2",
          "form": "아 좋아합니다",
          "original_form": "아 좋아합니다",
          "speaker_id": "2",
          "time": "20210903 14:15"
        },
        {
          "id": "MDRW2100029811.1.3",
          "form": "저는 어제 쉬는날인데 안나가고 배달음식 먹었어요^^",
          "original_form": "저는 어제 쉬는날인데 안나가고 배달음식 먹었어요^^",
          "speaker_id": "1",
          "time": "20210903 14:23"
        },
        {
          "id": "MDRW2100029811.1.4",
          "form": "자주드시는편인가요?",
          "original_form": "자주드시는편인가요?",
          "speaker_id": "1",
          "time": "20210903 14:23"
        },
        {
          "id": "MDRW2100029811.1.5",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210903 14:23"
        },
        {
          "id": "MDRW2100029811.1.6",
          "form": "근데 저는",
          "original_form": "근데 저는",
          "speaker_id": "2",
          "time": "20210903 14:23"
        },
        {
          "id": "MDRW2100029811.1.7",
          "form": "좋아하기는 하는데",
          "original_form": "좋아하기는 하는데",
          "speaker_id": "2",
          "time": "20210903 14:23"
        },
        {
          "id": "MDRW2100029811.1.8",
          "form": "자주 먹진 않아요",
          "original_form": "자주 먹진 않아요",
          "speaker_id": "2",
          "time": "20210903 14:23"
        },
        {
          "id": "MDRW2100029811.1.9",
          "form": "어떤거 드셨나요",
          "original_form": "어떤거 드셨나요",
          "speaker_id": "2",
          "time": "20210903 14:24"
        },
        {
          "id": "MDRW2100029811.1.10",
          "form": "저도 일주일에 한번 안넘어가려고 노력해요 식비가 너무 많이들어서요",
          "original_form": "저도 일주일에 한번 안넘어가려고 노력해요 식비가 너무 많이들어서요",
          "speaker_id": "1",
          "time": "20210903 14:24"
        },
        {
          "id": "MDRW2100029811.1.11",
          "form": "주로 해드시는편인가요?",
          "original_form": "주로 해드시는편인가요?",
          "speaker_id": "1",
          "time": "20210903 14:24"
        },
        {
          "id": "MDRW2100029811.1.12",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210903 14:24"
        },
        {
          "id": "MDRW2100029811.1.13",
          "form": "다만 요리를 잘하는 건 물론 아닙니다",
          "original_form": "다만 요리를 잘하는 건 물론 아닙니다",
          "speaker_id": "2",
          "time": "20210903 14:25"
        },
        {
          "id": "MDRW2100029811.1.14",
          "form": "요새는 유튜브 레시피가 잘나와있어서 저도 잘하는편은 아닌데 잘따라하긴해요",
          "original_form": "요새는 유튜브 레시피가 잘나와있어서 저도 잘하는편은 아닌데 잘따라하긴해요",
          "speaker_id": "1",
          "time": "20210903 14:49"
        },
        {
          "id": "MDRW2100029811.1.15",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100029811.1.16",
          "form": "옛날 요리 프로처럼",
          "original_form": "옛날 요리 프로처럼",
          "speaker_id": "2",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100029811.1.17",
          "form": "로즈마리를 넣어주세요",
          "original_form": "로즈마리를 넣어주세요",
          "speaker_id": "2",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100029811.1.18",
          "form": "이런 것도 아니고요",
          "original_form": "이런 것도 아니고요",
          "speaker_id": "2",
          "time": "20210903 14:50"
        },
        {
          "id": "MDRW2100029811.1.19",
          "form": "맞어요 없으면 안넣어도되요~ 바닐라에센스나 생강이나 맛술 이런거 없어도 된다고 해서 좋아요 있는재료로 넣어요",
          "original_form": "맞어요 없으면 안넣어도되요~ 바닐라에센스나 생강이나 맛술 이런거 없어도 된다고 해서 좋아요 있는재료로 넣어요",
          "speaker_id": "1",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100029811.1.20",
          "form": "특히 백선생님, 최고에요",
          "original_form": "특히 백선생님, 최고에요",
          "speaker_id": "1",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100029811.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100029811.1.22",
          "form": "저도 구독 중이에요",
          "original_form": "저도 구독 중이에요",
          "speaker_id": "2",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100029811.1.23",
          "form": "항상",
          "original_form": "항상",
          "speaker_id": "2",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100029811.1.24",
          "form": "이건 없어두 되유",
          "original_form": "이건 없어두 되유",
          "speaker_id": "2",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100029811.1.25",
          "form": "이렇게 말해주셔서",
          "original_form": "이렇게 말해주셔서",
          "speaker_id": "2",
          "time": "20210903 15:01"
        },
        {
          "id": "MDRW2100029811.1.26",
          "form": "레시피도 간단하기도 하구욬ㅋㅋ 이건 없으면 안넣어두 되유~ 그냥집에있는걸루 하시면 되유",
          "original_form": "레시피도 간단하기도 하구욬ㅋㅋ 이건 없으면 안넣어두 되유~ 그냥집에있는걸루 하시면 되유",
          "speaker_id": "1",
          "time": "20210903 15:11"
        },
        {
          "id": "MDRW2100029811.1.27",
          "form": "네 맞아요 ㅎㅎ",
          "original_form": "네 맞아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 15:24"
        },
        {
          "id": "MDRW2100029811.1.28",
          "form": "물론 그와중에",
          "original_form": "물론 그와중에",
          "speaker_id": "2",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100029811.1.29",
          "form": "이건 꼭 있어야 되요",
          "original_form": "이건 꼭 있어야 되요",
          "speaker_id": "2",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100029811.1.30",
          "form": "하면서 기본은 지키고요",
          "original_form": "하면서 기본은 지키고요",
          "speaker_id": "2",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100029811.1.31",
          "form": "맞어요 요알못 헤깔릴까봐ㅋㅋㅋㅋ",
          "original_form": "맞어요 요알못 헤깔릴까봐ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100029811.1.32",
          "form": "그 요리 못하는 사람 특징 중 하나가",
          "original_form": "그 요리 못하는 사람 특징 중 하나가",
          "speaker_id": "2",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100029811.1.33",
          "form": "제멋대로 딴 거 넣거나",
          "original_form": "제멋대로 딴 거 넣거나",
          "speaker_id": "2",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100029811.1.34",
          "form": "센불 중간불 안 지키는건데",
          "original_form": "센불 중간불 안 지키는건데",
          "speaker_id": "2",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100029811.1.35",
          "form": "거기서 선을 잡아주셔서 좋아요",
          "original_form": "거기서 선을 잡아주셔서 좋아요",
          "speaker_id": "2",
          "time": "20210903 15:26"
        }
      ]
