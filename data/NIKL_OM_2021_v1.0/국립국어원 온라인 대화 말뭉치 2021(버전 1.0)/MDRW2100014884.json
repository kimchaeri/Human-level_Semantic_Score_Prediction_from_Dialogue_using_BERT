      "utterance": [
        {
          "id": "MDRW2100014884.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210825 21:03"
        },
        {
          "id": "MDRW2100014884.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014884.1.3",
          "form": "name2님은 하시는 운동 있으신가요~?",
          "original_form": "&name2&님은 하시는 운동 있으신가요~?",
          "speaker_id": "1",
          "time": "20210825 21:04"
        },
        {
          "id": "MDRW2100014884.1.4",
          "form": "전 없어요ㅠㅠ 하지만 해야할 필요성은 느껴요!",
          "original_form": "전 없어요ㅠㅠ 하지만 해야할 필요성은 느껴요!",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014884.1.5",
          "form": "name1님이 하는 운동이 있으시다면 추천부탁드릴게요!",
          "original_form": "&name1&님이 하는 운동이 있으시다면 추천부탁드릴게요!",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014884.1.6",
          "form": "건강이 참 중요한것같아요ㅎㅎ",
          "original_form": "건강이 참 중요한것같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014884.1.7",
          "form": "저는 집에서 유튜브 틀어놓고 홈트레이닝을 합니다",
          "original_form": "저는 집에서 유튜브 틀어놓고 홈트레이닝을 합니다",
          "speaker_id": "1",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014884.1.8",
          "form": "홈트레이닝 좋죠!",
          "original_form": "홈트레이닝 좋죠!",
          "speaker_id": "2",
          "time": "20210825 21:05"
        },
        {
          "id": "MDRW2100014884.1.9",
          "form": "전 예전에 땅끄부부봤었엉ㅅ",
          "original_form": "전 예전에 땅끄부부봤었엉ㅅ",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014884.1.10",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014884.1.11",
          "form": "오 추천 감사해요",
          "original_form": "오 추천 감사해요",
          "speaker_id": "1",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014884.1.12",
          "form": "요즘은 꼭 집밖에 안나가도",
          "original_form": "요즘은 꼭 집밖에 안나가도",
          "speaker_id": "1",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014884.1.13",
          "form": "집에서 할 수 있는 운동법이 많더라구요",
          "original_form": "집에서 할 수 있는 운동법이 많더라구요",
          "speaker_id": "1",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014884.1.14",
          "form": "맞아요. 특히 코로나때문에 헬스장 가기에도 애매한 것 같아요ㅠㅠ",
          "original_form": "맞아요. 특히 코로나때문에 헬스장 가기에도 애매한 것 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014884.1.15",
          "form": "그러게요ㅠㅠ 집에서하면 원하는 음악 틀어놓을수도 있고",
          "original_form": "그러게요ㅠㅠ 집에서하면 원하는 음악 틀어놓을수도 있고",
          "speaker_id": "1",
          "time": "20210825 21:07"
        },
        {
          "id": "MDRW2100014884.1.16",
          "form": "바로 샤워할 수 있어서 좋은것같아요",
          "original_form": "바로 샤워할 수 있어서 좋은것같아요",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014884.1.17",
          "form": "맞아요!! 저는 코로나 끝나면 유도나 태권도를 배워보고싶어요",
          "original_form": "맞아요!! 저는 코로나 끝나면 유도나 태권도를 배워보고싶어요",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014884.1.18",
          "form": "얼마전 올림픽에서 경기하는걸 봤는데",
          "original_form": "얼마전 올림픽에서 경기하는걸 봤는데",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014884.1.19",
          "form": "도쿄 올림픽보면서 운동선수들이 너무 멋지더라고요~",
          "original_form": "도쿄 올림픽보면서 운동선수들이 너무 멋지더라고요~",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014884.1.20",
          "form": "정말 멋진 운동같아요",
          "original_form": "정말 멋진 운동같아요",
          "speaker_id": "1",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014884.1.21",
          "form": "어렸을 때 태권도 다니는 게 너무 싫었는데 안배운게 아쉬워요",
          "original_form": "어렸을 때 태권도 다니는 게 너무 싫었는데 안배운게 아쉬워요",
          "speaker_id": "2",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014884.1.22",
          "form": "어렸을 때 태권도라든지 검도라든지 다니셨었나요?",
          "original_form": "어렸을 때 태권도라든지 검도라든지 다니셨었나요?",
          "speaker_id": "2",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014884.1.23",
          "form": "저도 name2님처럼 안다녔어요ㅠㅠㅋㅋ",
          "original_form": "저도 &name2&님처럼 안다녔어요ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:09"
        },
        {
          "id": "MDRW2100014884.1.24",
          "form": "요즘엔 태권도장에서 예절교육까지 가르친다고 하더라구요",
          "original_form": "요즘엔 태권도장에서 예절교육까지 가르친다고 하더라구요",
          "speaker_id": "1",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014884.1.25",
          "form": "해외에서도 인기가 많나봐요",
          "original_form": "해외에서도 인기가 많나봐요",
          "speaker_id": "1",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014884.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요. 너무 신기해요! 더 인기 많아졌으면 좋겠어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요. 너무 신기해요! 더 인기 많아졌으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014884.1.27",
          "form": "저는 사실 하는 것보다는 보는 게 좋아요.",
          "original_form": "저는 사실 하는 것보다는 보는 게 좋아요.",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014884.1.28",
          "form": "축구보는 걸 좋아하는 편이에요.",
          "original_form": "축구보는 걸 좋아하는 편이에요.",
          "speaker_id": "2",
          "time": "20210825 21:10"
        },
        {
          "id": "MDRW2100014884.1.29",
          "form": "ㅋㅋㅋ 저두요 저랑 좋아하는 종목이 겹치시네요",
          "original_form": "ㅋㅋㅋ 저두요 저랑 좋아하는 종목이 겹치시네요",
          "speaker_id": "1",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014884.1.30",
          "form": "앗 축구보는 거 좋아하세요? 어느 팀 좋아하세요?",
          "original_form": "앗 축구보는 거 좋아하세요? 어느 팀 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014884.1.31",
          "form": "저는 해외 첼시 경기 주로 봐용",
          "original_form": "저는 해외 첼시 경기 주로 봐용",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014884.1.32",
          "form": "팬까지는 아닌데",
          "original_form": "팬까지는 아닌데",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014884.1.33",
          "form": "종종 챙겨봅니다",
          "original_form": "종종 챙겨봅니다",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014884.1.34",
          "form": "경기를 새벽에 해서 자주는 못봐요ㅠㅠ",
          "original_form": "경기를 새벽에 해서 자주는 못봐요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014884.1.35",
          "form": "첼시 경기를 챙겨보시는군요! 저번 시즌 너무 좋으셨겠어요ㅠㅠ",
          "original_form": "첼시 경기를 챙겨보시는군요! 저번 시즌 너무 좋으셨겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:12"
        }
      ]
    }
