      "utterance": [
        {
          "id": "MDRW2100015761.1.1",
          "form": "강아지 간식 추천할 만한 게 있을까요?",
          "original_form": "강아지 간식 추천할 만한 게 있을까요?",
          "speaker_id": "1",
          "time": "20210826 00:54"
        },
        {
          "id": "MDRW2100015761.1.2",
          "form": "듀먼 닭가슴살이요",
          "original_form": "듀먼 닭가슴살이요",
          "speaker_id": "2",
          "time": "20210827 07:37"
        },
        {
          "id": "MDRW2100015761.1.3",
          "form": "오오 강아지들이 좋아하던가요",
          "original_form": "오오 강아지들이 좋아하던가요",
          "speaker_id": "1",
          "time": "20210827 07:37"
        },
        {
          "id": "MDRW2100015761.1.4",
          "form": "엄청 좋아해요",
          "original_form": "엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210827 07:38"
        },
        {
          "id": "MDRW2100015761.1.5",
          "form": "염지 안돼 있죠?",
          "original_form": "염지 안돼 있죠?",
          "speaker_id": "1",
          "time": "20210827 07:38"
        },
        {
          "id": "MDRW2100015761.1.6",
          "form": "거의 환장합니다",
          "original_form": "거의 환장합니다",
          "speaker_id": "2",
          "time": "20210827 07:38"
        },
        {
          "id": "MDRW2100015761.1.7",
          "form": "집에서 닭 삶아주는 것과 다른게 있나요",
          "original_form": "집에서 닭 삶아주는 것과 다른게 있나요",
          "speaker_id": "1",
          "time": "20210827 07:38"
        },
        {
          "id": "MDRW2100015761.1.8",
          "form": "네. 닭가슴살을 진공포장해서 오래 먹일 수 있어요",
          "original_form": "네. 닭가슴살을 진공포장해서 오래 먹일 수 있어요",
          "speaker_id": "2",
          "time": "20210827 07:38"
        },
        {
          "id": "MDRW2100015761.1.9",
          "form": "뻑뻑하지는 않은가요",
          "original_form": "뻑뻑하지는 않은가요",
          "speaker_id": "1",
          "time": "20210827 07:39"
        },
        {
          "id": "MDRW2100015761.1.10",
          "form": "전자렌지에 데우면 살짝 촉촉",
          "original_form": "전자렌지에 데우면 살짝 촉촉",
          "speaker_id": "2",
          "time": "20210827 07:39"
        },
        {
          "id": "MDRW2100015761.1.11",
          "form": "추천해요",
          "original_form": "추천해요",
          "speaker_id": "2",
          "time": "20210827 07:39"
        },
        {
          "id": "MDRW2100015761.1.12",
          "form": "name1씨는 추천간식?",
          "original_form": "&name1&씨는 추천간식?",
          "speaker_id": "2",
          "time": "20210827 07:40"
        },
        {
          "id": "MDRW2100015761.1.13",
          "form": "오오. 신기하네요. 진공포장이라 그런가",
          "original_form": "오오. 신기하네요. 진공포장이라 그런가",
          "speaker_id": "1",
          "time": "20210827 07:40"
        },
        {
          "id": "MDRW2100015761.1.14",
          "form": "추천간식이라기보다ㅎ 애들이 우유 좋아해요",
          "original_form": "추천간식이라기보다ㅎ 애들이 우유 좋아해요",
          "speaker_id": "1",
          "time": "20210827 07:40"
        },
        {
          "id": "MDRW2100015761.1.15",
          "form": "우유도 좋아하죠",
          "original_form": "우유도 좋아하죠",
          "speaker_id": "2",
          "time": "20210827 07:40"
        },
        {
          "id": "MDRW2100015761.1.16",
          "form": "펫우유로 주나요?",
          "original_form": "펫우유로 주나요?",
          "speaker_id": "1",
          "time": "20210827 07:41"
        },
        {
          "id": "MDRW2100015761.1.17",
          "form": "우리애들은 산책후 우유 좋아해요",
          "original_form": "우리애들은 산책후 우유 좋아해요",
          "speaker_id": "2",
          "time": "20210827 07:41"
        },
        {
          "id": "MDRW2100015761.1.18",
          "form": "저유당우유",
          "original_form": "저유당우유",
          "speaker_id": "2",
          "time": "20210827 07:41"
        },
        {
          "id": "MDRW2100015761.1.19",
          "form": "펫우유로 주시나요",
          "original_form": "펫우유로 주시나요",
