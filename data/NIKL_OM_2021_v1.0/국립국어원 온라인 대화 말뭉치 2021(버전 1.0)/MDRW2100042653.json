      "utterance": [
        {
          "id": "MDRW2100042653.1.1",
          "form": "완전기쁘지만 하루를 기다려야함ㅠㅜ",
          "original_form": "완전기쁘지만 하루를 기다려야함ㅠㅜ",
          "speaker_id": "2",
          "time": "20210907 00:26"
        },
        {
          "id": "MDRW2100042653.1.2",
          "form": "쿠팡은 자정에 잘봐야됨ㅋㅋㅋ",
          "original_form": "쿠팡은 자정에 잘봐야됨ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:26"
        },
        {
          "id": "MDRW2100042653.1.3",
          "form": "네이버도 어차피 하루 기다려야되는거 아니야?",
          "original_form": "네이버도 어차피 하루 기다려야되는거 아니야?",
          "speaker_id": "1",
          "time": "20210907 00:27"
        },
        {
          "id": "MDRW2100042653.1.4",
          "form": "무슨오류가발생했능데 주문하다가?",
          "original_form": "무슨오류가발생했능데 주문하다가?",
          "speaker_id": "1",
          "time": "20210907 00:28"
        },
        {
          "id": "MDRW2100042653.1.5",
          "form": "쿠팡은 기대치가 있잖아ㅋㅋ열두시 전에 시킬껄! 하는ㅋㅋ",
          "original_form": "쿠팡은 기대치가 있잖아ㅋㅋ열두시 전에 시킬껄! 하는ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 00:28"
        },
        {
          "id": "MDRW2100042653.1.6",
          "form": "ㅋㅋㅋㅋ 그래서 열시이후로는 품절 너무많아",
          "original_form": "ㅋㅋㅋㅋ 그래서 열시이후로는 품절 너무많아",
          "speaker_id": "1",
          "time": "20210907 00:29"
        },
        {
          "id": "MDRW2100042653.1.7",
          "form": "응응 맛잇는 과자 사고싶다",
          "original_form": "응응 맛잇는 과자 사고싶다",
          "speaker_id": "2",
          "time": "20210907 00:30"
        },
        {
          "id": "MDRW2100042653.1.8",
          "form": "내일은 후라이펜오겠지",
          "original_form": "내일은 후라이펜오겠지",
          "speaker_id": "2",
          "time": "20210907 00:30"
        },
        {
          "id": "MDRW2100042653.1.9",
          "form": "난지금미쯔먹음",
          "original_form": "난지금미쯔먹음",
          "speaker_id": "1",
          "time": "20210907 00:30"
        },
        {
          "id": "MDRW2100042653.1.10",
          "form": "난 오늘 왔어 쿠폰 적용못한 후라이팬 ^^  ...",
          "original_form": "난 오늘 왔어 쿠폰 적용못한 후라이팬 ^^  ...",
          "speaker_id": "1",
          "time": "20210907 00:32"
        },
        {
          "id": "MDRW2100042653.1.11",
          "form": "그래도 빨리왔네",
          "original_form": "그래도 빨리왔네",
          "speaker_id": "2",
          "time": "20210907 00:33"
        },
        {
          "id": "MDRW2100042653.1.12",
          "form": "ㅋㅋㅋㅋ 난금요일에 시켰더라구",
          "original_form": "ㅋㅋㅋㅋ 난금요일에 시켰더라구",
          "speaker_id": "1",
          "time": "20210907 00:34"
        },
        {
          "id": "MDRW2100042653.1.13",
          "form": "아ㅋㅋ그래도 빠르다 토요일에도 일하는구나",
          "original_form": "아ㅋㅋ그래도 빠르다 토요일에도 일하는구나",
          "speaker_id": "2",
          "time": "20210907 00:34"
        },
        {
          "id": "MDRW2100042653.1.14",
          "form": "내가시킬때는 쿠폰이없었다고 믿어야지 ^^",
          "original_form": "내가시킬때는 쿠폰이없었다고 믿어야지 ^^",
          "speaker_id": "1",
          "time": "20210907 00:34"
        },
        {
          "id": "MDRW2100042653.1.15",
          "form": "그래그러자ㅋㅋ",
          "original_form": "그래그러자ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 00:35"
        },
        {
          "id": "MDRW2100042653.1.16",
          "form": "열두시만되면 뭐살거없나 쇼핑몰봄ㅋㅋㅋ",
          "original_form": "열두시만되면 뭐살거없나 쇼핑몰봄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:37"
        },
        {
          "id": "MDRW2100042653.1.17",
          "form": "ㅋㅋㅋ나는 시시때때로 쿠팡ㅋㅋ",
          "original_form": "ㅋㅋㅋ나는 시시때때로 쿠팡ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 00:37"
        },
        {
          "id": "MDRW2100042653.1.18",
          "form": "가격이 자꾸 바뀌어?",
          "original_form": "가격이 자꾸 바뀌어?",
          "speaker_id": "1",
          "time": "20210907 00:38"
        },
        {
          "id": "MDRW2100042653.1.19",
          "form": "아니ㅋㄱ 그냥 볼때마다 사고싶은게 생겨서ㅋㅋ",
          "original_form": "아니ㅋㄱ 그냥 볼때마다 사고싶은게 생겨서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 00:39"
        },
        {
          "id": "MDRW2100042653.1.20",
          "form": "나도먹고싶은거 맨날담잖아 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "나도먹고싶은거 맨날담잖아 ㅋㅋㅋㅋㅋㅋ",
