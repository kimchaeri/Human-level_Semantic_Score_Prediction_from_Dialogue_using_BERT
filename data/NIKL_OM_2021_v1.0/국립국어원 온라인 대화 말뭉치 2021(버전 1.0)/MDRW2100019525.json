      "utterance": [
        {
          "id": "MDRW2100019525.1.1",
          "form": "제철 채소로 하는거 정말 싱그러워보여요",
          "original_form": "제철 채소로 하는거 정말 싱그러워보여요",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019525.1.2",
          "form": "꽃 들어가면 되게 보기 좋구",
          "original_form": "꽃 들어가면 되게 보기 좋구",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019525.1.3",
          "form": "그 리틀포레스트에 꽃 파스타인가 나온다던데",
          "original_form": "그 리틀포레스트에 꽃 파스타인가 나온다던데",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019525.1.4",
          "form": "리틀포레스트에도 꽃파스타인가 나온다던데",
          "original_form": "리틀포레스트에도 꽃파스타인가 나온다던데",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019525.1.5",
          "form": "꽃 활용한 요리 되게 향긋할거같아요",
          "original_form": "꽃 활용한 요리 되게 향긋할거같아요",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019525.1.6",
          "form": "오 깻잎이랑 아카시아랑 이것저것 넣은 거예요",
          "original_form": "오 깻잎이랑 아카시아랑 이것저것 넣은 거예요",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019525.1.7",
          "form": "와 진짜 향긋하겠어요",
          "original_form": "와 진짜 향긋하겠어요",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019525.1.8",
          "form": "꽃을 구하기가 어려워서",
          "original_form": "꽃을 구하기가 어려워서",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019525.1.9",
          "form": "안해봤지만",
          "original_form": "안해봤지만",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019525.1.10",
          "form": "맛있을 것 같아요 ㅎㅎ",
          "original_form": "맛있을 것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019525.1.11",
          "form": "맞아요 ㅋㅋㅋㅋㅋ꽃은 마켓컬리에도 안팔고",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ꽃은 마켓컬리에도 안팔고",
          "speaker_id": "1",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019525.1.12",
          "form": "연희동 사러가 쇼핑센터에 팔아요",
          "original_form": "연희동 사러가 쇼핑센터에 팔아요",
          "speaker_id": "2",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019525.1.13",
          "form": "호박꽃전 하려면 호박을 키워야겠네요",
          "original_form": "호박꽃전 하려면 호박을 키워야겠네요",
          "speaker_id": "1",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019525.1.14",
          "form": "식용꽃 종류별로",
          "original_form": "식용꽃 종류별로",
          "speaker_id": "2",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019525.1.15",
          "form": "앜ㅋㅋㅋㅋ 거기에 식용꽃을",
          "original_form": "앜ㅋㅋㅋㅋ 거기에 식용꽃을",
          "speaker_id": "1",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019525.1.16",
          "form": "지도에 또 별표를..",
          "original_form": "지도에 또 별표를..",
          "speaker_id": "1",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019525.1.17",
          "form": "호박꽃을 밤에 서리했어요",
          "original_form": "호박꽃을 밤에 서리했어요",
          "speaker_id": "2",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019525.1.18",
          "form": "name2님이요?",
