      "utterance": [
        {
          "id": "MDRW2100022869.1.1",
          "form": "음 난 초밥",
          "original_form": "음 난 초밥",
          "speaker_id": "2",
          "time": "20210829 18:44"
        },
        {
          "id": "MDRW2100022869.1.2",
          "form": "너는 뭐가 좋아?",
          "original_form": "너는 뭐가 좋아?",
          "speaker_id": "2",
          "time": "20210829 18:44"
        },
        {
          "id": "MDRW2100022869.1.3",
          "form": "난 스팸김치찌개가 제일 먼저 떠올랐어....",
          "original_form": "난 스팸김치찌개가 제일 먼저 떠올랐어....",
          "speaker_id": "1",
          "time": "20210829 18:44"
        },
        {
          "id": "MDRW2100022869.1.4",
          "form": "아 밥도둑..",
          "original_form": "아 밥도둑..",
          "speaker_id": "2",
          "time": "20210829 18:44"
        },
        {
          "id": "MDRW2100022869.1.5",
          "form": "진짜 맛있지..",
          "original_form": "진짜 맛있지..",
          "speaker_id": "1",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022869.1.6",
          "form": "근데 김치찌개는 먹고나면 달달한거 먹고싶어...",
          "original_form": "근데 김치찌개는 먹고나면 달달한거 먹고싶어...",
          "speaker_id": "2",
          "time": "20210829 18:45"
        },
        {
          "id": "MDRW2100022869.1.7",
          "form": "맞아ㅋㅋㅋㅋㅋ원래 매운거 먹고나면 단거 땡기잖아",
          "original_form": "맞아ㅋㅋㅋㅋㅋ원래 매운거 먹고나면 단거 땡기잖아",
          "speaker_id": "1",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022869.1.8",
          "form": "초코우유 같은거..그치 ㅋㅋㅋ",
          "original_form": "초코우유 같은거..그치 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022869.1.9",
          "form": "맞앜ㅋㅋ그래서 단거 먹으면 또 좀 짠거 땡기잖아",
          "original_form": "맞앜ㅋㅋ그래서 단거 먹으면 또 좀 짠거 땡기잖아",
          "speaker_id": "1",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022869.1.10",
          "form": "맞아 ㅋㅋ무한굴레야",
          "original_form": "맞아 ㅋㅋ무한굴레야",
          "speaker_id": "2",
          "time": "20210829 18:46"
        },
        {
          "id": "MDRW2100022869.1.11",
          "form": "ㅋ맞아 단짠느매 단짠느매",
          "original_form": "ㅋ맞아 단짠느매 단짠느매",
          "speaker_id": "1",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022869.1.12",
          "form": "단짠맵 말하는거지?",
          "original_form": "단짠맵 말하는거지?",
          "speaker_id": "2",
          "time": "20210829 18:48"
        },
        {
          "id": "MDRW2100022869.1.13",
          "form": "달고 짜고 느끼하고 맵고ㅋㅋ",
          "original_form": "달고 짜고 느끼하고 맵고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:49"
        },
        {
          "id": "MDRW2100022869.1.14",
          "form": "아아 ㅋㅋㅋㅋㅋ느가 오타가 아니었구낰ㅋㅋ",
          "original_form": "아아 ㅋㅋㅋㅋㅋ느가 오타가 아니었구낰ㅋㅋ",
