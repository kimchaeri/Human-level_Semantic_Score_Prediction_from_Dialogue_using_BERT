      "utterance": [
        {
          "id": "MDRW2100031902.1.1",
          "form": "협회에서 하는 거라서",
          "original_form": "협회에서 하는 거라서",
          "speaker_id": "2",
          "time": "20210905 08:01"
        },
        {
          "id": "MDRW2100031902.1.2",
          "form": "할만 하실 거예요",
          "original_form": "할만 하실 거예요",
          "speaker_id": "2",
          "time": "20210905 08:01"
        },
        {
          "id": "MDRW2100031902.1.3",
          "form": "네 저도 관심은 있어요",
          "original_form": "네 저도 관심은 있어요",
          "speaker_id": "1",
          "time": "20210905 08:01"
        },
        {
          "id": "MDRW2100031902.1.4",
          "form": "전 아예 그 생활체육지도사 그 자격증 따고 싶더라구요",
          "original_form": "전 아예 그 생활체육지도사 그 자격증 따고 싶더라구요",
          "speaker_id": "1",
          "time": "20210905 08:01"
        },
        {
          "id": "MDRW2100031902.1.5",
          "form": "수영 대회도 나가고 싶은데",
          "original_form": "수영 대회도 나가고 싶은데",
          "speaker_id": "1",
          "time": "20210905 08:02"
        },
        {
          "id": "MDRW2100031902.1.6",
          "form": "아직 실력이 그 정도는 안 되는 거 같아요",
          "original_form": "아직 실력이 그 정도는 안 되는 거 같아요",
          "speaker_id": "1",
          "time": "20210905 08:02"
        },
        {
          "id": "MDRW2100031902.1.7",
          "form": "아 그 과정이 따로 있을 겁니다",
          "original_form": "아 그 과정이 따로 있을 겁니다",
          "speaker_id": "2",
          "time": "20210905 08:06"
        },
        {
          "id": "MDRW2100031902.1.8",
          "form": "그에 맞춰서 훈련을 하고",
          "original_form": "그에 맞춰서 훈련을 하고",
          "speaker_id": "2",
          "time": "20210905 08:06"
        },
        {
          "id": "MDRW2100031902.1.9",
          "form": "접수해서 진행하시면 됩니다",
          "original_form": "접수해서 진행하시면 됩니다",
          "speaker_id": "2",
          "time": "20210905 08:06"
        },
        {
          "id": "MDRW2100031902.1.10",
          "form": "자신의 건강도 지키고",
          "original_form": "자신의 건강도 지키고",
          "speaker_id": "2",
          "time": "20210905 08:06"
        },
        {
          "id": "MDRW2100031902.1.11",
          "form": "개인 강습 받아야 될 거 같은데요",
          "original_form": "개인 강습 받아야 될 거 같은데요",
          "speaker_id": "1",
          "time": "20210905 08:06"
        },
        {
          "id": "MDRW2100031902.1.12",
          "form": "가족의 건강도 지킬 수 있어요",
          "original_form": "가족의 건강도 지킬 수 있어요",
          "speaker_id": "2",
          "time": "20210905 08:06"
        },
        {
          "id": "MDRW2100031902.1.13",
          "form": "사실 그렇게 배우긴 했는데 자격증 과정은 더 힘든 거 같더라고요",
          "original_form": "사실 그렇게 배우긴 했는데 자격증 과정은 더 힘든 거 같더라고요",
          "speaker_id": "1",
          "time": "20210905 08:06"
        },
        {
          "id": "MDRW2100031902.1.14",
          "form": "단체로 같이 하는 게 있습니다",
          "original_form": "단체로 같이 하는 게 있습니다",
          "speaker_id": "2",
          "time": "20210905 08:06"
        },
        {
          "id": "MDRW2100031902.1.15",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "1",
          "time": "20210905 08:06"
        },
        {
          "id": "MDRW2100031902.1.16",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 08:06"
        },
        {
          "id": "MDRW2100031902.1.17",
          "form": "요즘",
          "original_form": "요즘",
          "speaker_id": "2",
          "time": "20210905 08:07"
        },
        {
          "id": "MDRW2100031902.1.18",
          "form": "필라테스나",
          "original_form": "필라테스나",
          "speaker_id": "2",
          "time": "20210905 08:07"
        },
        {
          "id": "MDRW2100031902.1.19",
          "form": "폴댄스",
          "original_form": "폴댄스",
          "speaker_id": "2",
          "time": "20210905 08:07"
        },
        {
          "id": "MDRW2100031902.1.20",
          "form": "이런거 많이 하던데",
          "original_form": "이런거 많이 하던데",
          "speaker_id": "2",
          "time": "20210905 08:07"
        },
        {
          "id": "MDRW2100031902.1.21",
          "form": "관심 안가시나요?",
          "original_form": "관심 안가시나요?",
          "speaker_id": "2",
          "time": "20210905 08:07"
        },
        {
          "id": "MDRW2100031902.1.22",
          "form": "네 제가 운동을 잘 하지 못하고 수영만 할 줄 알아서",
          "original_form": "네 제가 운동을 잘 하지 못하고 수영만 할 줄 알아서",
          "speaker_id": "1",
          "time": "20210905 08:08"
        },
        {
          "id": "MDRW2100031902.1.23",
          "form": "필라테스 이런 거 하면 못 할 거 같아요",
          "original_form": "필라테스 이런 거 하면 못 할 거 같아요",
          "speaker_id": "1",
          "time": "20210905 08:08"
        },
        {
          "id": "MDRW2100031902.1.24",
          "form": "아닙니다 그런 생각하지 마세요",
          "original_form": "아닙니다 그런 생각하지 마세요",
          "speaker_id": "2",
          "time": "20210905 08:08"
        },
        {
          "id": "MDRW2100031902.1.25",
          "form": "해보면 다 금방 배우실 겁니다",
          "original_form": "해보면 다 금방 배우실 겁니다",
          "speaker_id": "2",
          "time": "20210905 08:08"
        },
        {
          "id": "MDRW2100031902.1.26",
          "form": "기구를 이용해서 하다보니깐",
          "original_form": "기구를 이용해서 하다보니깐",
          "speaker_id": "2",
          "time": "20210905 08:09"
        },
        {
          "id": "MDRW2100031902.1.27",
          "form": "힘도 좀 들어가고",
          "original_form": "힘도 좀 들어가고",
          "speaker_id": "2",
          "time": "20210905 08:09"
        },
        {
          "id": "MDRW2100031902.1.28",
          "form": "유연성도 길어진다고 하더라고요",
          "original_form": "유연성도 길어진다고 하더라고요",
          "speaker_id": "2",
          "time": "20210905 08:09"
        },
        {
          "id": "MDRW2100031902.1.29",
          "form": "저도 배우려고 했는데",
          "original_form": "저도 배우려고 했는데",
          "speaker_id": "2",
          "time": "20210905 08:09"
        },
        {
          "id": "MDRW2100031902.1.30",
          "form": "요가랑 일맥상통한 거 아니에요?",
          "original_form": "요가랑 일맥상통한 거 아니에요?",
          "speaker_id": "1",
          "time": "20210905 08:09"
        },
        {
          "id": "MDRW2100031902.1.31",
          "form": "여자 단체 반은 같이 할 수 없다고 하셔서",
          "original_form": "여자 단체 반은 같이 할 수 없다고 하셔서",
          "speaker_id": "2",
          "time": "20210905 08:09"
        },
        {
          "id": "MDRW2100031902.1.32",
          "form": "남자 네 명 모아 오래요",
          "original_form": "남자 네 명 모아 오래요",
          "speaker_id": "2",
          "time": "20210905 08:09"
        },
        {
          "id": "MDRW2100031902.1.33",
          "form": "전 요가 하러 가면 진짜 뻣뻣 돼서요",
          "original_form": "전 요가 하러 가면 진짜 뻣뻣 돼서요",
          "speaker_id": "1",
          "time": "20210905 08:09"
        },
        {
          "id": "MDRW2100031902.1.34",
          "form": "네 ㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 08:09"
        },
        {
          "id": "MDRW2100031902.1.35",
          "form": "어 비슷한데 약간 다르더라구요",
          "original_form": "어 비슷한데 약간 다르더라구요",
          "speaker_id": "2",
          "time": "20210905 08:09"
        },
        {
          "id": "MDRW2100031902.1.36",
          "form": "그 옷 의상이",
          "original_form": "그 옷 의상이",
          "speaker_id": "2",
          "time": "20210905 08:09"
        },
        {
          "id": "MDRW2100031902.1.37",
          "form": "민망해서 그런지",
          "original_form": "민망해서 그런지",
          "speaker_id": "2",
          "time": "20210905 08:09"
        }
      ]
    }
