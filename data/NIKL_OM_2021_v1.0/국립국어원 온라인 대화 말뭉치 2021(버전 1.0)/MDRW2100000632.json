      "utterance": [
        {
          "id": "MDRW2100000632.1.1",
          "form": "안녕하삼나꺼]",
          "original_form": "안녕하삼나꺼]",
          "speaker_id": "1",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000632.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000632.1.3",
          "form": "게임 좋아하시나여",
          "original_form": "게임 좋아하시나여",
          "speaker_id": "1",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000632.1.4",
          "form": "전 롤 좋아하는데",
          "original_form": "전 롤 좋아하는데",
          "speaker_id": "1",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000632.1.5",
          "form": "저는 페이커보고 롤 입문했어요",
          "original_form": "저는 페이커보고 롤 입문했어요",
          "speaker_id": "2",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000632.1.6",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000632.1.7",
          "form": "저는 슼빠에요",
          "original_form": "저는 슼빠에요",
          "speaker_id": "1",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000632.1.8",
          "form": "롤말고 다른 게임은 안 하나요?",
          "original_form": "롤말고 다른 게임은 안 하나요?",
          "speaker_id": "2",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000632.1.9",
          "form": "무슨게임좋아하시나요?",
          "original_form": "무슨게임좋아하시나요?",
          "speaker_id": "1",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000632.1.10",
          "form": "저는 롤 하다가 오버워치 하다가 요즘엔 유튜브로 보기만 해요",
          "original_form": "저는 롤 하다가 오버워치 하다가 요즘엔 유튜브로 보기만 해요",
          "speaker_id": "2",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000632.1.11",
          "form": "오 오버워치~ 저도 예전에 했었는데",
          "original_form": "오 오버워치~ 저도 예전에 했었는데",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000632.1.12",
          "form": "예전에 엄청 팬이었거든요 루나틱하이랑 뉴욕엑셀시오르",
          "original_form": "예전에 엄청 팬이었거든요 루나틱하이랑 뉴욕엑셀시오르",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000632.1.13",
          "form": "이스포츠 방송 자주 보나봐요?",
          "original_form": "이스포츠 방송 자주 보나봐요?",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000632.1.14",
          "form": "넵 저는 리그 자주 봐요 ㅎ",
          "original_form": "넵 저는 리그 자주 봐요 ㅎ",
          "speaker_id": "1",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000632.1.15",
          "form": "리그 자주 안보시고 유튜브 주로 보시나봐요?",
          "original_form": "리그 자주 안보시고 유튜브 주로 보시나봐요?",
          "speaker_id": "1",
          "time": "20210819 17:28"
        },
        {
          "id": "MDRW2100000632.1.16",
          "form": "네 직접 하는 것보다 보는게 속 편해요",
          "original_form": "네 직접 하는 것보다 보는게 속 편해요",
          "speaker_id": "2",
          "time": "20210819 17:29"
        },
        {
          "id": "MDRW2100000632.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ명언이시네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ명언이시네요",
          "speaker_id": "1",
          "time": "20210819 17:30"
        },
        {
          "id": "MDRW2100000632.1.18",
          "form": "게임 유튜버 누구보시나요",
          "original_form": "게임 유튜버 누구보시나요",
          "speaker_id": "1",
          "time": "20210819 17:30"
        },
        {
          "id": "MDRW2100000632.1.19",
          "form": "예전엔 대도서관이 유명하다길래 봤는데",
          "original_form": "예전엔 대도서관이 유명하다길래 봤는데",
          "speaker_id": "2",
          "time": "20210819 17:30"
        },
        {
          "id": "MDRW2100000632.1.20",
          "form": "요즘엔 알고리즘을 받아들여요",
          "original_form": "요즘엔 알고리즘을 받아들여요",
          "speaker_id": "2",
          "time": "20210819 17:30"
        },
        {
          "id": "MDRW2100000632.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ알고리즘에 무슨 게임이 나오나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ알고리즘에 무슨 게임이 나오나요",
          "speaker_id": "1",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000632.1.22",
          "form": "여러 게임 많더라고요",
          "original_form": "여러 게임 많더라고요",
          "speaker_id": "2",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000632.1.23",
          "form": "특히 옛날에 하던 플래쉬게임같은거",
          "original_form": "특히 옛날에 하던 플래쉬게임같은거",
          "speaker_id": "2",
          "time": "20210819 17:32"
        }
      ]
    }
