      "utterance": [
        {
          "id": "MDRW2100030742.1.1",
          "form": "잘 다려집니다",
          "original_form": "잘 다려집니다",
          "speaker_id": "2",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100030742.1.2",
          "form": "오늘의 집에서 파는 거 중에 싼 제품이 많은 것 같아요 다른 쇼핑몰과 비교해 봤을 때요",
          "original_form": "오늘의 집에서 파는 거 중에 싼 제품이 많은 것 같아요 다른 쇼핑몰과 비교해 봤을 때요",
          "speaker_id": "2",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100030742.1.3",
          "form": "브랜드 아닌 거 사 줬나요",
          "original_form": "브랜드 아닌 거 사 줬나요",
          "speaker_id": "1",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100030742.1.4",
          "form": "좌식 테이블을 샀는데 요즘에는 디자인이 예쁜 것도 많이 팔더라고요",
          "original_form": "좌식 테이블을 샀는데 요즘에는 디자인이 예쁜 것도 많이 팔더라고요",
          "speaker_id": "2",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100030742.1.5",
          "form": "오늘의 집 관련주는",
          "original_form": "오늘의 집 관련주는",
          "speaker_id": "1",
          "time": "20210904 12:12"
        },
        {
          "id": "MDRW2100030742.1.6",
          "form": "모네 그림이 그려진 좌식 테이블도 거기에서 구매했어요",
          "original_form": "모네 그림이 그려진 좌식 테이블도 거기에서 구매했어요",
          "speaker_id": "2",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030742.1.7",
          "form": "오하임 아이엔티입니다",
          "original_form": "오하임 아이엔티입니다",
          "speaker_id": "1",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030742.1.8",
          "form": "뭐가 있나요",
          "original_form": "뭐가 있나요",
          "speaker_id": "2",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030742.1.9",
          "form": "레이디가구 회사지요",
          "original_form": "레이디가구 회사지요",
          "speaker_id": "1",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030742.1.10",
          "form": "좋은 정보 감사합니다",
          "original_form": "좋은 정보 감사합니다",
          "speaker_id": "2",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030742.1.11",
          "form": "오하임아이엔티 제가 좋아하는 종목이죠",
          "original_form": "오하임아이엔티 제가 좋아하는 종목이죠",
          "speaker_id": "1",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030742.1.12",
          "form": "최근에 좀 올랐어요",
          "original_form": "최근에 좀 올랐어요",
          "speaker_id": "1",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030742.1.13",
          "form": "그리고 커피 타 먹을 때 편하라고",
          "original_form": "그리고 커피 타 먹을 때 편하라고",
          "speaker_id": "2",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030742.1.14",
          "form": "다음에 검색해 보도록 할게요",
          "original_form": "다음에 검색해 보도록 할게요",
          "speaker_id": "2",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030742.1.15",
          "form": "가장 재밌는 쇼핑은 주식 쇼핑입니다",
          "original_form": "가장 재밌는 쇼핑은 주식 쇼핑입니다",
          "speaker_id": "1",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030742.1.16",
          "form": "커피포트 중에",
          "original_form": "커피포트 중에",
          "speaker_id": "2",
          "time": "20210904 12:13"
        },
        {
          "id": "MDRW2100030742.1.17",
          "form": "맞아요 그건 정말 인정입니다",
          "original_form": "맞아요 그건 정말 인정입니다",
          "speaker_id": "2",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100030742.1.18",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100030742.1.19",
          "form": "커피 물 내리기 쉽게 주둥이가 매우 얇은 커피포트가 있거든요",
          "original_form": "커피 물 내리기 쉽게 주둥이가 매우 얇은 커피포트가 있거든요",
          "speaker_id": "2",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100030742.1.20",
          "form": "전에 커피포트 고장 난 김에 그런 걸로 장만했어요",
          "original_form": "전에 커피포트 고장 난 김에 그런 걸로 장만했어요",
          "speaker_id": "2",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100030742.1.21",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "1",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100030742.1.22",
          "form": "확실히 맛있나요",
          "original_form": "확실히 맛있나요",
          "speaker_id": "1",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100030742.1.23",
          "form": "물이 끓는 속도도 빠르고 디자인이 예뻐서 너무 너무 마음에 들더라구요",
          "original_form": "물이 끓는 속도도 빠르고 디자인이 예뻐서 너무 너무 마음에 들더라구요",
          "speaker_id": "2",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100030742.1.24",
          "form": "드립으로 마시나 봐요",
          "original_form": "드립으로 마시나 봐요",
          "speaker_id": "1",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100030742.1.25",
          "form": "요즘에는 디자인 예쁜 물건을 사야지 만족도가 높은 것 같아요",
          "original_form": "요즘에는 디자인 예쁜 물건을 사야지 만족도가 높은 것 같아요",
          "speaker_id": "2",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100030742.1.26",
          "form": "디자인도 참 중요하죠",
          "original_form": "디자인도 참 중요하죠",
          "speaker_id": "1",
          "time": "20210904 12:14"
        },
        {
          "id": "MDRW2100030742.1.27",
          "form": "네 그립으로 마시고 있습니다",
          "original_form": "네 그립으로 마시고 있습니다",
          "speaker_id": "2",
          "time": "20210904 12:14"
        }
      ]
    }
