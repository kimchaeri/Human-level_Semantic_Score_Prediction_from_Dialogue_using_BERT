      "utterance": [
        {
          "id": "MDRW2100002083.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002083.1.2",
          "form": "반가워요!",
          "original_form": "반가워요!",
          "speaker_id": "2",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002083.1.3",
          "form": "오늘 저녁 뭐 드셨나요?",
          "original_form": "오늘 저녁 뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210819 23:26"
        },
        {
          "id": "MDRW2100002083.1.4",
          "form": "오늘은 순대국밥먹었어요 든든하게~",
          "original_form": "오늘은 순대국밥먹었어요 든든하게~",
          "speaker_id": "2",
          "time": "20210819 23:27"
        },
        {
          "id": "MDRW2100002083.1.5",
          "form": "오 따근한 국물!",
          "original_form": "오 따근한 국물!",
          "speaker_id": "1",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002083.1.6",
          "form": "전 된장찌개 자박자박 끓여서",
          "original_form": "전 된장찌개 자박자박 끓여서",
          "speaker_id": "1",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002083.1.7",
          "form": "상추에 쌈싸먹었어요ㅋㅋㅋ",
          "original_form": "상추에 쌈싸먹었어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002083.1.8",
          "form": "요즘 배달도 많이 시켜드시죠?",
          "original_form": "요즘 배달도 많이 시켜드시죠?",
          "speaker_id": "1",
          "time": "20210819 23:28"
        },
        {
          "id": "MDRW2100002083.1.9",
          "form": "오 맛있겠다 된찌",
          "original_form": "오 맛있겠다 된찌",
          "speaker_id": "2",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100002083.1.10",
          "form": "배달은 일상이죠ㅋㅋ",
          "original_form": "배달은 일상이죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100002083.1.11",
          "form": "어젠 아구찜 시켜먹었어요",
          "original_form": "어젠 아구찜 시켜먹었어요",
          "speaker_id": "2",
          "time": "20210819 23:30"
        },
        {
          "id": "MDRW2100002083.1.12",
          "form": "헐... 진짜 밤에 음식 이야기하기",
          "original_form": "헐... 진짜 밤에 음식 이야기하기",
          "speaker_id": "1",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002083.1.13",
          "form": "힘드네요..ㅜ",
          "original_form": "힘드네요..ㅜ",
          "speaker_id": "1",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002083.1.14",
          "form": "전 점심에 중식 시켜먹었어요ㅋㅋ",
          "original_form": "전 점심에 중식 시켜먹었어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:31"
        },
        {
          "id": "MDRW2100002083.1.15",
          "form": "점심은 김치나베우동...",
          "original_form": "점심은 김치나베우동...",
          "speaker_id": "2",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100002083.1.16",
          "form": "낼은 돈까스 먹을거예요",
          "original_form": "낼은 돈까스 먹을거예요",
          "speaker_id": "2",
          "time": "20210819 23:32"
        },
        {
          "id": "MDRW2100002083.1.17",
          "form": "와.. 진짜 맛있는 게 많아서 행복합니다",
          "original_form": "와.. 진짜 맛있는 게 많아서 행복합니다",
          "speaker_id": "1",
          "time": "20210819 23:33"
        },
        {
          "id": "MDRW2100002083.1.18",
          "form": "내일은 뭐 먹지..",
          "original_form": "내일은 뭐 먹지..",
          "speaker_id": "1",
          "time": "20210819 23:33"
        },
        {
          "id": "MDRW2100002083.1.19",
          "form": "매일 고민이네요ㅠ",
          "original_form": "매일 고민이네요ㅠ",
          "speaker_id": "1",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002083.1.20",
          "form": "ㅋㅋㅋㅋㅋ맞아요 매일 맛집만 찾아다니고싶은 마음..",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 매일 맛집만 찾아다니고싶은 마음..",
          "speaker_id": "2",
          "time": "20210819 23:34"
        },
        {
          "id": "MDRW2100002083.1.21",
          "form": "진짜 돈만 많으면 맛집투어하면서 살고 싶네요ㅎㅎ",
          "original_form": "진짜 돈만 많으면 맛집투어하면서 살고 싶네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100002083.1.22",
          "form": "맛집 일주일에 몇 번 가시나요?",
          "original_form": "맛집 일주일에 몇 번 가시나요?",
          "speaker_id": "1",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100002083.1.23",
          "form": "흠 한달에 한두번정도..? 가는것같아요!",
          "original_form": "흠 한달에 한두번정도..? 가는것같아요!",
          "speaker_id": "2",
          "time": "20210819 23:35"
        },
        {
          "id": "MDRW2100002083.1.24",
          "form": "오 맛집가는 거 진짜 좋아하는데",
          "original_form": "오 맛집가는 거 진짜 좋아하는데",
          "speaker_id": "1",
          "time": "20210819 23:37"
        },
        {
          "id": "MDRW2100002083.1.25",
          "form": "요즘 너무 더워서 저도 그 정도 가는 거 같아요",
          "original_form": "요즘 너무 더워서 저도 그 정도 가는 거 같아요",
          "speaker_id": "1",
          "time": "20210819 23:37"
        }
      ]
    }
  ]
