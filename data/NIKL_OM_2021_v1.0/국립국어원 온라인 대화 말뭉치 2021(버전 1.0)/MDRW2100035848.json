      "utterance": [
        {
          "id": "MDRW2100035848.1.1",
          "form": "안녕하세용 아침 드셨어요~?",
          "original_form": "안녕하세용 아침 드셨어요~?",
          "speaker_id": "1",
          "time": "20210830 09:05"
        },
        {
          "id": "MDRW2100035848.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210830 09:05"
        },
        {
          "id": "MDRW2100035848.1.3",
          "form": "아니요...ㅠㅠ",
          "original_form": "아니요...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 09:05"
        },
        {
          "id": "MDRW2100035848.1.4",
          "form": "배고프네요",
          "original_form": "배고프네요",
          "speaker_id": "2",
          "time": "20210830 09:05"
        },
        {
          "id": "MDRW2100035848.1.5",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210830 09:05"
        },
        {
          "id": "MDRW2100035848.1.6",
          "form": "앗ㅠㅠ 뭐 드실예정이세용?",
          "original_form": "앗ㅠㅠ 뭐 드실예정이세용?",
          "speaker_id": "1",
          "time": "20210830 09:05"
        },
        {
          "id": "MDRW2100035848.1.7",
          "form": "저는 거봉 한다발을 다 먹어서",
          "original_form": "저는 거봉 한다발을 다 먹어서",
          "speaker_id": "1",
          "time": "20210830 09:05"
        },
        {
          "id": "MDRW2100035848.1.8",
          "form": "배부르네요ㅋㅋㅋ",
          "original_form": "배부르네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 09:05"
        },
        {
          "id": "MDRW2100035848.1.9",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210830 09:05"
        },
        {
          "id": "MDRW2100035848.1.10",
          "form": "침나오네요",
          "original_form": "침나오네요",
          "speaker_id": "2",
          "time": "20210830 09:05"
        },
        {
          "id": "MDRW2100035848.1.11",
          "form": "ㅎㅎ 요즘 철이라 달더라구요",
          "original_form": "ㅎㅎ 요즘 철이라 달더라구요",
          "speaker_id": "1",
          "time": "20210830 09:06"
        },
        {
          "id": "MDRW2100035848.1.12",
          "form": "ㅠㅠ과일 너무 좋아오",
          "original_form": "ㅠㅠ과일 너무 좋아오",
          "speaker_id": "2",
          "time": "20210830 09:06"
        },
        {
          "id": "MDRW2100035848.1.13",
          "form": "주말엔 뭐 드셨어용?",
          "original_form": "주말엔 뭐 드셨어용?",
          "speaker_id": "1",
          "time": "20210830 09:06"
        },
        {
          "id": "MDRW2100035848.1.14",
          "form": "주말엔 돼지였습니다",
          "original_form": "주말엔 돼지였습니다",
          "speaker_id": "2",
          "time": "20210830 09:06"
        },
        {
          "id": "MDRW2100035848.1.15",
          "form": "고기궈먹고",
          "original_form": "고기궈먹고",
          "speaker_id": "2",
          "time": "20210830 09:06"
        },
        {
          "id": "MDRW2100035848.1.16",
          "form": "치킨먹고",
          "original_form": "치킨먹고",
          "speaker_id": "2",
          "time": "20210830 09:06"
        },
        {
          "id": "MDRW2100035848.1.17",
          "form": "저두요ㅋㅋㅋㅋ",
          "original_form": "저두요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 09:06"
        },
        {
          "id": "MDRW2100035848.1.18",
          "form": "복숭아먹고",
          "original_form": "복숭아먹고",
          "speaker_id": "2",
          "time": "20210830 09:06"
        },
        {
          "id": "MDRW2100035848.1.19",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:06"
        },
        {
          "id": "MDRW2100035848.1.20",
          "form": "치킨먹었어요!",
          "original_form": "치킨먹었어요!",
          "speaker_id": "1",
          "time": "20210830 09:06"
        },
        {
          "id": "MDRW2100035848.1.21",
          "form": "어디 치킨 드셨어요?",
          "original_form": "어디 치킨 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 09:06"
        },
        {
