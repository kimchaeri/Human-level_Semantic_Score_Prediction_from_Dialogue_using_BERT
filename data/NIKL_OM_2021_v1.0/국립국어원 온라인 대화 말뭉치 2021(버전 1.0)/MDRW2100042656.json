      "utterance": [
        {
          "id": "MDRW2100042656.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 00:28"
        },
        {
          "id": "MDRW2100042656.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210907 00:29"
        },
        {
          "id": "MDRW2100042656.1.3",
          "form": "최근에 구매하신물건있으신가여",
          "original_form": "최근에 구매하신물건있으신가여",
          "speaker_id": "1",
          "time": "20210907 00:29"
        },
        {
          "id": "MDRW2100042656.1.4",
          "form": "최근에 좋아하는 아이돌 관련된 상품을 하나 샀어요",
          "original_form": "최근에 좋아하는 아이돌 관련된 상품을 하나 샀어요",
          "speaker_id": "2",
          "time": "20210907 00:30"
        },
        {
          "id": "MDRW2100042656.1.5",
          "form": "오 아이돌누구여?",
          "original_form": "오 아이돌누구여?",
          "speaker_id": "1",
          "time": "20210907 00:30"
        },
        {
          "id": "MDRW2100042656.1.6",
          "form": "엑소 백현이요",
          "original_form": "엑소 백현이요",
          "speaker_id": "2",
          "time": "20210907 00:30"
        },
        {
          "id": "MDRW2100042656.1.7",
          "form": "아하 어떤 상품사셨나여",
          "original_form": "아하 어떤 상품사셨나여",
          "speaker_id": "1",
          "time": "20210907 00:32"
        },
        {
          "id": "MDRW2100042656.1.8",
          "form": "무슨 포토북? 같은 상품이었어요",
          "original_form": "무슨 포토북? 같은 상품이었어요",
          "speaker_id": "2",
          "time": "20210907 00:32"
        },
        {
          "id": "MDRW2100042656.1.9",
          "form": "포토북이요!",
          "original_form": "포토북이요!",
          "speaker_id": "2",
          "time": "20210907 00:33"
        },
        {
          "id": "MDRW2100042656.1.10",
          "form": "사진왕창 들어있는거말이져?",
          "original_form": "사진왕창 들어있는거말이져?",
          "speaker_id": "1",
          "time": "20210907 00:33"
        },
        {
          "id": "MDRW2100042656.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210907 00:34"
        },
        {
          "id": "MDRW2100042656.1.12",
          "form": "언제부터 좋아하셨어요?",
          "original_form": "언제부터 좋아하셨어요?",
          "speaker_id": "1",
          "time": "20210907 00:35"
        },
        {
          "id": "MDRW2100042656.1.13",
          "form": "14년도?부터 좋아했으니까 7년 넘게 좋아했어요",
          "original_form": "14년도?부터 좋아했으니까 7년 넘게 좋아했어요",
          "speaker_id": "2",
          "time": "20210907 00:38"
        },
        {
          "id": "MDRW2100042656.1.14",
          "form": "오오 오래되셨네요 백현이 최애신가봐여",
          "original_form": "오오 오래되셨네요 백현이 최애신가봐여",
          "speaker_id": "1",
          "time": "20210907 00:43"
        },
        {
          "id": "MDRW2100042656.1.15",
          "form": "네네 요즘은 아이돌 전반적으로 두루두루 좋아하지만 남돌 중엔 백현이 최애예요!",
          "original_form": "네네 요즘은 아이돌 전반적으로 두루두루 좋아하지만 남돌 중엔 백현이 최애예요!",
          "speaker_id": "2",
          "time": "20210907 00:44"
        }
      ]
