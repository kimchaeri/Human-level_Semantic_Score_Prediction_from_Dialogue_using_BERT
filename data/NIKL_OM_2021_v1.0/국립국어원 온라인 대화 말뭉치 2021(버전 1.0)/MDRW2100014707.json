          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014707.1.3",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014707.1.4",
          "form": "좋아해요ㅎㅎ 축구ㅎ",
          "original_form": "좋아해요ㅎㅎ 축구ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014707.1.5",
          "form": "뭐좋아하세요?",
          "original_form": "뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014707.1.6",
          "form": "배구 좋아해요",
          "original_form": "배구 좋아해요",
          "speaker_id": "1",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014707.1.7",
          "form": "배구는 여자배구 보는것만ㅎㅎ",
          "original_form": "배구는 여자배구 보는것만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014707.1.8",
          "form": "배구좋아하시나봐요ㅎㅎ",
          "original_form": "배구좋아하시나봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:27"
        },
        {
          "id": "MDRW2100014707.1.9",
          "form": "저도 여자배구 좋아해요",
          "original_form": "저도 여자배구 좋아해요",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014707.1.10",
          "form": "여자배구 좋아하죠ㅎ",
          "original_form": "여자배구 좋아하죠ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014707.1.11",
          "form": "올림픽때문에요ㅎ",
          "original_form": "올림픽때문에요ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014707.1.12",
          "form": "저와 계기가 같네요",
          "original_form": "저와 계기가 같네요",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014707.1.13",
          "form": "아무래도 일본전이 컸던것같애요ㅎ",
          "original_form": "아무래도 일본전이 컸던것같애요ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014707.1.14",
          "form": "김연경 메시누나가 하는거보고ㅎㅎ",
          "original_form": "김연경 메시누나가 하는거보고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014707.1.15",
          "form": "터키전도 박진감 넘쳤죠",
          "original_form": "터키전도 박진감 넘쳤죠",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014707.1.16",
          "form": "맞아요ㅎ",
          "original_form": "맞아요ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014707.1.17",
          "form": "터키전 승리 정말 값진경기죠ㅎㅎ",
          "original_form": "터키전 승리 정말 값진경기죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014707.1.18",
          "form": "직관 해보신적 있으신가요",
          "original_form": "직관 해보신적 있으신가요",
          "speaker_id": "1",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014707.1.19",
          "form": "배구는 아니고 축구는 직관해봐ㅛ어요ㅎㅎ",
          "original_form": "배구는 아니고 축구는 직관해봐ㅛ어요ㅎㅎ",
