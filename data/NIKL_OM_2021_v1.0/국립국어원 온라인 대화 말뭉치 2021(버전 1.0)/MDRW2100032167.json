      "utterance": [
        {
          "id": "MDRW2100032167.1.1",
          "form": "안물렸니 ㅋㅋ",
          "original_form": "안물렸니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100032167.1.2",
          "form": "밥 시간이 가까워져서 그런지 온순한...",
          "original_form": "밥 시간이 가까워져서 그런지 온순한...",
          "speaker_id": "2",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100032167.1.3",
          "form": "ㅋㅋㅋㅋ 밥 언제언제먹는데?",
          "original_form": "ㅋㅋㅋㅋ 밥 언제언제먹는데?",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032167.1.4",
          "form": "오전에 한 번 먹고 오후에 두 번 먹는데 보통 2시에 줘",
          "original_form": "오전에 한 번 먹고 오후에 두 번 먹는데 보통 2시에 줘",
          "speaker_id": "2",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100032167.1.5",
          "form": "밥 시간 20분 전ㅋㅋㅋㅋ안절부절 못하는 중",
          "original_form": "밥 시간 20분 전ㅋㅋㅋㅋ안절부절 못하는 중",
          "speaker_id": "2",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100032167.1.6",
          "form": "ㅋㅋㅋㅋ 고양이들 진짜 찌끔 먹는데 엄청 열심히 와구와구 먹어 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 고양이들 진짜 찌끔 먹는데 엄청 열심히 와구와구 먹어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032167.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ헐 맞아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ헐 맞아",
          "speaker_id": "2",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032167.1.8",
          "form": "고양이잘알",
          "original_form": "고양이잘알",
          "speaker_id": "2",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032167.1.9",
          "form": "물도 그렇게 먹어서 입에 들어가는게 있나싶어 ㅋㅋㅋ",
          "original_form": "물도 그렇게 먹어서 입에 들어가는게 있나싶어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:45"
        },
        {
          "id": "MDRW2100032167.1.10",
          "form": "그런데 그렇게 먹으면 기분 좋다",
          "original_form": "그런데 그렇게 먹으면 기분 좋다",
          "speaker_id": "2",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100032167.1.11",
          "form": "꼭 대접에 물을 받아서 쳡쳡 먹어 봐",
          "original_form": "꼭 대접에 물을 받아서 쳡쳡 먹어 봐",
          "speaker_id": "2",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100032167.1.12",
          "form": "인간이..?",
          "original_form": "인간이..?",
          "speaker_id": "1",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100032167.1.13",
          "form": "그래.",
          "original_form": "그래.",
          "speaker_id": "2",
          "time": "20210905 13:49"
        },
        {
          "id": "MDRW2100032167.1.14",
          "form": "존엄성은 잠시 내려 놓고",
          "original_form": "존엄성은 잠시 내려 놓고",
          "speaker_id": "2",
          "time": "20210905 13:49"
        },
        {
          "id": "MDRW2100032167.1.15",
          "form": "아 쉽지 않은일인데? 그렇게 고양이에게 한발짝 다가서는?",
          "original_form": "아 쉽지 않은일인데? 그렇게 고양이에게 한발짝 다가서는?",
          "speaker_id": "1",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100032167.1.16",
          "form": "어? 어...",
          "original_form": "어? 어...",
          "speaker_id": "2",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100032167.1.17",
          "form": "고양이랑은 한 발짝 가까워지고 방금 너랑은 한 발짝 멀어진 것 같지만...",
          "original_form": "고양이랑은 한 발짝 가까워지고 방금 너랑은 한 발짝 멀어진 것 같지만...",
          "speaker_id": "2",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100032167.1.18",
          "form": "언젠가 해볼일이 없으면 좋겠구나 ㅋㅋㅋ",
          "original_form": "언젠가 해볼일이 없으면 좋겠구나 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:52"
        }
      ]
