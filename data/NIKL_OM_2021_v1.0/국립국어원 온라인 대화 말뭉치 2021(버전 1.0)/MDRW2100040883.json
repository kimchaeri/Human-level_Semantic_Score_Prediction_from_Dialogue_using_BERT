      "utterance": [
        {
          "id": "MDRW2100040883.1.1",
          "form": "쭈꾸미",
          "original_form": "쭈꾸미",
          "speaker_id": "2",
          "time": "20210905 11:05"
        },
        {
          "id": "MDRW2100040883.1.2",
          "form": "주꾸미 아닙니까",
          "original_form": "주꾸미 아닙니까",
          "speaker_id": "2",
          "time": "20210905 11:06"
        },
        {
          "id": "MDRW2100040883.1.3",
          "form": "쭈꾸미 맛집이 근처에 있어요?",
          "original_form": "쭈꾸미 맛집이 근처에 있어요?",
          "speaker_id": "1",
          "time": "20210905 11:06"
        },
        {
          "id": "MDRW2100040883.1.4",
          "form": "찾아본 곳이 있는지",
          "original_form": "찾아본 곳이 있는지",
          "speaker_id": "1",
          "time": "20210905 11:07"
        },
        {
          "id": "MDRW2100040883.1.5",
          "form": "찾아봤슈",
          "original_form": "찾아봤슈",
          "speaker_id": "2",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100040883.1.6",
          "form": "주꾸미박사와 주꾸미아가씨가 있습니다",
          "original_form": "주꾸미박사와 주꾸미아가씨가 있습니다",
          "speaker_id": "2",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100040883.1.7",
          "form": "어디서 배달합니까",
          "original_form": "어디서 배달합니까",
          "speaker_id": "1",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100040883.1.8",
          "form": "ㅋㅋㅋ박사님과 아가씨라니",
          "original_form": "ㅋㅋㅋ박사님과 아가씨라니",
          "speaker_id": "1",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100040883.1.9",
          "form": "하나는 상동 하나는 삼산동임다",
          "original_form": "하나는 상동 하나는 삼산동임다",
          "speaker_id": "2",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100040883.1.10",
          "form": "배민참고하세용",
          "original_form": "배민참고하세용",
          "speaker_id": "2",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100040883.1.11",
          "form": "주꾸미 검색 ㄱㄱ",
          "original_form": "주꾸미 검색 ㄱㄱ",
          "speaker_id": "2",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100040883.1.12",
          "form": "둘다 12시쯤 영업하나",
          "original_form": "둘다 12시쯤 영업하나",
          "speaker_id": "1",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100040883.1.13",
          "form": "알아봐야겠군요",
          "original_form": "알아봐야겠군요",
          "speaker_id": "1",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100040883.1.14",
          "form": "11시 시작이유",
          "original_form": "11시 시작이유",
          "speaker_id": "2",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100040883.1.15",
          "form": "어렵게 주꾸미 협상을 끝냈는데",
          "original_form": "어렵게 주꾸미 협상을 끝냈는데",
          "speaker_id": "1",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100040883.1.16",
          "form": "아 배달가능시간이?",
          "original_form": "아 배달가능시간이?",
          "speaker_id": "1",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100040883.1.17",
          "form": "다행이군요",
          "original_form": "다행이군요",
          "speaker_id": "1",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100040883.1.18",
          "form": "네 그렇습니다",
          "original_form": "네 그렇습니다",
          "speaker_id": "2",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100040883.1.19",
          "form": "한번 보시쥬",
          "original_form": "한번 보시쥬",
          "speaker_id": "2",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100040883.1.20",
          "form": "천사채주고 마요소스 없음 (박사) vs 천사채 없고 마요소스에 날치알 (아가씨)",
          "original_form": "천사채주고 마요소스 없음 (박사) vs 천사채 없고 마요소스에 날치알 (아가씨)",
          "speaker_id": "2",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100040883.1.21",
          "form": "네 맛집 검색해볼게유",
          "original_form": "네 맛집 검색해볼게유",
          "speaker_id": "1",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100040883.1.22",
          "form": "요차이같은..?",
          "original_form": "요차이같은..?",
          "speaker_id": "2",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100040883.1.23",
          "form": "ㅋㅋㅋ천사채를 마요소스 없이라",
          "original_form": "ㅋㅋㅋ천사채를 마요소스 없이라",
          "speaker_id": "1",
          "time": "20210905 11:10"
        },
        {
          "id": "MDRW2100040883.1.24",
          "form": "얘기만 들음 천사채 쪽인디",
          "original_form": "얘기만 들음 천사채 쪽인디",
          "speaker_id": "1",
          "time": "20210905 11:10"
        },
        {
          "id": "MDRW2100040883.1.25",
          "form": "함 보십쇼 천사채는 마요에 버무렸는데",
          "original_form": "함 보십쇼 천사채는 마요에 버무렸는데",
          "speaker_id": "2",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100040883.1.26",
          "form": "찍는소스가 땅콩소스래",
          "original_form": "찍는소스가 땅콩소스래",
          "speaker_id": "2",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100040883.1.27",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "1",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100040883.1.28",
          "form": "보겠습니다",
          "original_form": "보겠습니다",
          "speaker_id": "1",
