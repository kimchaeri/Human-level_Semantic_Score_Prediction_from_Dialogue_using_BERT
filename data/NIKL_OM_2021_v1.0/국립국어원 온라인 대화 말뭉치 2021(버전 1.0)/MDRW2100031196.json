      "utterance": [
        {
          "id": "MDRW2100031196.1.1",
          "form": "라면 자주 드시나요?",
          "original_form": "라면 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210904 18:32"
        },
        {
          "id": "MDRW2100031196.1.2",
          "form": "아뇨ㅜ 그냥 가끔 먹는거같아요",
          "original_form": "아뇨ㅜ 그냥 가끔 먹는거같아요",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031196.1.3",
          "form": "컵라면은 괜찮은데",
          "original_form": "컵라면은 괜찮은데",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031196.1.4",
          "form": "봉지라면은 이상하게 입에 잘 안맞더라구요",
          "original_form": "봉지라면은 이상하게 입에 잘 안맞더라구요",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031196.1.5",
          "form": "면 성분이 달라서 그런 건지...",
          "original_form": "면 성분이 달라서 그런 건지...",
          "speaker_id": "2",
          "time": "20210904 23:47"
        },
        {
          "id": "MDRW2100031196.1.6",
          "form": "뭔가 몸에 안 맞으시나 봐요",
          "original_form": "뭔가 몸에 안 맞으시나 봐요",
          "speaker_id": "1",
          "time": "20210904 23:48"
        },
        {
          "id": "MDRW2100031196.1.7",
          "form": "그런거같기도 해요",
          "original_form": "그런거같기도 해요",
          "speaker_id": "2",
          "time": "20210905 00:18"
        },
        {
          "id": "MDRW2100031196.1.8",
          "form": "아님 그냥 맛 문제인가 싶기도하고...?",
          "original_form": "아님 그냥 맛 문제인가 싶기도하고...?",
          "speaker_id": "2",
          "time": "20210905 00:18"
        },
        {
          "id": "MDRW2100031196.1.9",
          "form": "불면 그때부터 잘 못먹거든요",
          "original_form": "불면 그때부터 잘 못먹거든요",
          "speaker_id": "2",
          "time": "20210905 00:18"
        },
        {
          "id": "MDRW2100031196.1.10",
          "form": "국수는 괜찮으세요?",
          "original_form": "국수는 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210905 00:18"
        },
        {
          "id": "MDRW2100031196.1.11",
          "form": "네! 국수는 괜찮아요ㅋㅋㅋ",
          "original_form": "네! 국수는 괜찮아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:48"
        },
        {
          "id": "MDRW2100031196.1.12",
          "form": "국수는 좋아해요",
          "original_form": "국수는 좋아해요",
          "speaker_id": "2",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100031196.1.13",
          "form": "칼국수도 좋고 잔치국수도 좋고...",
          "original_form": "칼국수도 좋고 잔치국수도 좋고...",
          "speaker_id": "2",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100031196.1.14",
          "form": "우동도 좋아합니당",
          "original_form": "우동도 좋아합니당",
          "speaker_id": "2",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100031196.1.15",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100031196.1.16",
          "form": "저도 국수 좋아해요",
          "original_form": "저도 국수 좋아해요",
          "speaker_id": "1",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100031196.1.17",
          "form": "기름기가 없어 깔끔하니 좋아요",
          "original_form": "기름기가 없어 깔끔하니 좋아요",
          "speaker_id": "1",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100031196.1.18",
          "form": "오 국수중에는 그럼",
          "original_form": "오 국수중에는 그럼",
          "speaker_id": "2",
          "time": "20210905 16:40"
        },
        {
          "id": "MDRW2100031196.1.19",
          "form": "어떤국수 젤 조아하세요?",
          "original_form": "어떤국수 젤 조아하세요?",
          "speaker_id": "2",
          "time": "20210905 16:40"
        },
        {
          "id": "MDRW2100031196.1.20",
          "form": "콩국수 좋아해요",
          "original_form": "콩국수 좋아해요",
          "speaker_id": "1",
          "time": "20210905 16:45"
        },
        {
          "id": "MDRW2100031196.1.21",
          "form": "나이 들수록 맛있더라고요",
          "original_form": "나이 들수록 맛있더라고요",
          "speaker_id": "1",
          "time": "20210905 16:51"
        },
        {
          "id": "MDRW2100031196.1.22",
          "form": "콩국수 보면",
          "original_form": "콩국수 보면",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100031196.1.23",
          "form": "우묵가사리 든 것도 콩국수라 하는것같고",
          "original_form": "우묵가사리 든 것도 콩국수라 하는것같고",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100031196.1.24",
          "form": "아 국수대신요?",
          "original_form": "아 국수대신요?",
          "speaker_id": "1",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100031196.1.25",
          "form": "밀가루 면 든것도 콩국수라하는거같던데",
          "original_form": "밀가루 면 든것도 콩국수라하는거같던데",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100031196.1.26",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "2",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100031196.1.27",
          "form": "그것도 콩국수라하나봐요 ㅋㅋ",
          "original_form": "그것도 콩국수라하나봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100031196.1.28",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210905 20:57"
        },
        {
          "id": "MDRW2100031196.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ둘다 맛있죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ둘다 맛있죠",
          "speaker_id": "2",
          "time": "20210905 20:58"
        }
      ]
    }
