      "utterance": [
        {
          "id": "MDRW2100027715.1.1",
          "form": "우린 역시 이런얘기?",
          "original_form": "우린 역시 이런얘기?",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027715.1.2",
          "form": "ㅋㅋㅋㅋ 그런가",
          "original_form": "ㅋㅋㅋㅋ 그런가",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027715.1.3",
          "form": "오늘 집 가면 빨래부터 좀 처리해야겠어..",
          "original_form": "오늘 집 가면 빨래부터 좀 처리해야겠어..",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027715.1.4",
          "form": "슬슬 청소기도 돌려야 할거 같더라는........",
          "original_form": "슬슬 청소기도 돌려야 할거 같더라는........",
          "speaker_id": "1",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027715.1.5",
          "form": "3일치..",
          "original_form": "3일치..",
          "speaker_id": "2",
          "time": "20210902 14:26"
        },
        {
          "id": "MDRW2100027715.1.6",
          "form": "응응 저번주에 내가 아파서 못 돌렸더니...",
          "original_form": "응응 저번주에 내가 아파서 못 돌렸더니...",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027715.1.7",
          "form": "어제 분명히 빨래 갠거같은데 말이지",
          "original_form": "어제 분명히 빨래 갠거같은데 말이지",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027715.1.8",
          "form": "그게 벌써 화욜입니당",
          "original_form": "그게 벌써 화욜입니당",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027715.1.9",
          "form": "오늘은 내가 먼저가서 청소기돌릴게요!",
          "original_form": "오늘은 내가 먼저가서 청소기돌릴게요!",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027715.1.10",
          "form": "아냐 빨래부터 해야 청소기 돌리니까",
          "original_form": "아냐 빨래부터 해야 청소기 돌리니까",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027715.1.11",
          "form": "빨래만 해줘요",
          "original_form": "빨래만 해줘요",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027715.1.12",
          "form": "청소긴 내가 할께",
          "original_form": "청소긴 내가 할께",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027715.1.13",
          "form": "빨래 개서 넣어놓기 알겟습니당",
          "original_form": "빨래 개서 넣어놓기 알겟습니당",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027715.1.14",
          "form": "오홍홍 좋아용.",
          "original_form": "오홍홍 좋아용.",
          "speaker_id": "2",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027715.1.15",
          "form": "냉장고에 저번에 못버린것도 떠오르네..",
          "original_form": "냉장고에 저번에 못버린것도 떠오르네..",
          "speaker_id": "1",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027715.1.16",
          "form": "아직 싱크대서랍에 쓰봉 많죠?",
          "original_form": "아직 싱크대서랍에 쓰봉 많죠?",
          "speaker_id": "1",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027715.1.17",
          "form": "넹 꽤 있더라구요",
          "original_form": "넹 꽤 있더라구요",
          "speaker_id": "2",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027715.1.18",
