      "utterance": [
        {
          "id": "MDRW2100046081.1.1",
          "form": "저 지금 화상수업중인데ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 지금 화상수업중인데ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046081.1.2",
          "form": "이게 접속자도 많아서",
          "original_form": "이게 접속자도 많아서",
          "speaker_id": "1",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046081.1.3",
          "form": "계속 렉걸려요ㅋ",
          "original_form": "계속 렉걸려요ㅋ",
          "speaker_id": "1",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046081.1.4",
          "form": "ㅋㅋㅋㅋㅋ그러면 어떻게해요?",
          "original_form": "ㅋㅋㅋㅋㅋ그러면 어떻게해요?",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046081.1.5",
          "form": "나갔다 들어가야하나",
          "original_form": "나갔다 들어가야하나",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100046081.1.6",
          "form": "교수님이 렉걸린거라",
          "original_form": "교수님이 렉걸린거라",
          "speaker_id": "1",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100046081.1.7",
          "form": "교수님나가면 다시 못들어오ㅏ욬ㅋㅋ",
          "original_form": "교수님나가면 다시 못들어오ㅏ욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100046081.1.8",
          "form": "헐ㅋㅋㅋㅋ그럼어캐요?",
          "original_form": "헐ㅋㅋㅋㅋ그럼어캐요?",
          "speaker_id": "2",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100046081.1.9",
          "form": "교수님이 방을 새로만드시는데",
          "original_form": "교수님이 방을 새로만드시는데",
          "speaker_id": "1",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100046081.1.10",
          "form": "지금은그것마저 렉이라",
          "original_form": "지금은그것마저 렉이라",
          "speaker_id": "1",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100046081.1.11",
          "form": "강제휴식중",
          "original_form": "강제휴식중",
          "speaker_id": "1",
          "time": "20210908 10:10"
        },
        {
          "id": "MDRW2100046081.1.12",
          "form": "좋다ㅎㅎ",
          "original_form": "좋다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046081.1.13",
          "form": "컴퓨터 성능안조ㅎ으면",
          "original_form": "컴퓨터 성능안조ㅎ으면",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046081.1.14",
          "form": "수업듣기 힘들거같아요",
          "original_form": "수업듣기 힘들거같아요",
          "speaker_id": "2",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046081.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046081.1.16",
          "form": "그거때문에 엄청스트레스 받았어요",
          "original_form": "그거때문에 엄청스트레스 받았어요",
          "speaker_id": "1",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100046081.1.17",
          "form": "자꾸 렉걸려서요?",
          "original_form": "자꾸 렉걸려서요?",
          "speaker_id": "2",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046081.1.18",
          "form": "그것도 있고",
          "original_form": "그것도 있고",
          "speaker_id": "1",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046081.1.19",
          "form": "웹캠 사서설치했는데",
          "original_form": "웹캠 사서설치했는데",
          "speaker_id": "1",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046081.1.20",
          "form": "자체오류가 계속 나서요",
          "original_form": "자체오류가 계속 나서요",
          "speaker_id": "1",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046081.1.21",
          "form": "아..그러면",
          "original_form": "아..그러면",
          "speaker_id": "2",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046081.1.22",
          "form": "그냥 카메라끄면안되나?",
          "original_form": "그냥 카메라끄면안되나?",
          "speaker_id": "2",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046081.1.23",
          "form": "안돼요",
          "original_form": "안돼요",
          "speaker_id": "1",
          "time": "20210908 10:12"
        },
        {
          "id": "MDRW2100046081.1.24",
          "form": "싫어한교수님이 있어서...",
          "original_form": "싫어한교수님이 있어서...",
          "speaker_id": "1",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046081.1.25",
          "form": "그래서 웹캠도 두번샀어요",
          "original_form": "그래서 웹캠도 두번샀어요",
          "speaker_id": "1",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046081.1.26",
          "form": "그것도 다 돈인데..",
          "original_form": "그것도 다 돈인데..",
          "speaker_id": "2",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046081.1.27",
          "form": "지원해주는것도 아니고ㅋㅋㅋㅋ",
          "original_form": "지원해주는것도 아니고ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046081.1.28",
          "form": "그러니까뇨",
          "original_form": "그러니까뇨",
          "speaker_id": "1",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100046081.1.29",
          "form": "학교 다닐 때",
          "original_form": "학교 다닐 때",
          "speaker_id": "1",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100046081.1.30",
          "form": "동아리 하셨어요",
          "original_form": "동아리 하셨어요",
          "speaker_id": "1",
          "time": "20210908 10:14"
        }
      ]
    }
  ]
