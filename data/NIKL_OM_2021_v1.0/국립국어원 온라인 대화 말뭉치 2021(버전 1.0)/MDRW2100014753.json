      "utterance": [
        {
          "id": "MDRW2100014753.1.1",
          "form": "그럼요 수술도 한번했었어요",
          "original_form": "그럼요 수술도 한번했었어요",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014753.1.2",
          "form": "어떤 수술인가요",
          "original_form": "어떤 수술인가요",
          "speaker_id": "1",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014753.1.3",
          "form": "장기내에 혹이 있어서 혹을 잘라냈어요",
          "original_form": "장기내에 혹이 있어서 혹을 잘라냈어요",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014753.1.4",
          "form": "큰 수술인가요?",
          "original_form": "큰 수술인가요?",
          "speaker_id": "1",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014753.1.5",
          "form": "위험하지는 않구요 전신마취는 하는 수술이였어요",
          "original_form": "위험하지는 않구요 전신마취는 하는 수술이였어요",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014753.1.6",
          "form": "name1님은 아파본적 있으세요?",
          "original_form": "&name1&님은 아파본적 있으세요?",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014753.1.7",
          "form": "독감걸려서 죽는줄알았어요",
          "original_form": "독감걸려서 죽는줄알았어요",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014753.1.8",
          "form": "독감힘들죠...",
          "original_form": "독감힘들죠...",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014753.1.9",
          "form": "고생하셨네요",
          "original_form": "고생하셨네요",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014753.1.10",
          "form": "며칠 앓으셨겠다",
          "original_form": "며칠 앓으셨겠다",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014753.1.11",
          "form": "의외로 타미플루 먹고 30분만에 안정되었어요",
          "original_form": "의외로 타미플루 먹고 30분만에 안정되었어요",
          "speaker_id": "1",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014753.1.12",
          "form": "다행이였네요",
          "original_form": "다행이였네요",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014753.1.13",
          "form": "다이어트해보셨나요?",
          "original_form": "다이어트해보셨나요?",
          "speaker_id": "1",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014753.1.14",
          "form": "네 학생때 해봤어요",
          "original_form": "네 학생때 해봤어요",
          "speaker_id": "2",
          "time": "20210825 20:51"
        },
        {
          "id": "MDRW2100014753.1.15",
          "form": "어떻게 하셨나요?",
          "original_form": "어떻게 하셨나요?",
