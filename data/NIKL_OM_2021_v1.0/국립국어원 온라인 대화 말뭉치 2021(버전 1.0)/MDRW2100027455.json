      "utterance": [
        {
          "id": "MDRW2100027455.1.1",
          "form": "요즘 또 뭐 읽으시는거 있으세요?",
          "original_form": "요즘 또 뭐 읽으시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210902 13:19"
        },
        {
          "id": "MDRW2100027455.1.2",
          "form": "저 최근에 천개의 파랑이라는 한국 소설 읽었어요!",
          "original_form": "저 최근에 천개의 파랑이라는 한국 소설 읽었어요!",
          "speaker_id": "1",
          "time": "20210902 13:27"
        },
        {
          "id": "MDRW2100027455.1.3",
          "form": "오오 천선란님!",
          "original_form": "오오 천선란님!",
          "speaker_id": "2",
          "time": "20210902 13:32"
        },
        {
          "id": "MDRW2100027455.1.4",
          "form": "저도 사놓고는 안읽어봤네요 ㅋㅋㅋ",
          "original_form": "저도 사놓고는 안읽어봤네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027455.1.5",
          "form": "저는 재밌었어요!! 추천추천",
          "original_form": "저는 재밌었어요!! 추천추천",
          "speaker_id": "1",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027455.1.6",
          "form": "오 사놓은거 읽어볼게요!!",
          "original_form": "오 사놓은거 읽어볼게요!!",
          "speaker_id": "2",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027455.1.7",
          "form": "책 좋아하시면 보통 어떤 거 보세요?",
          "original_form": "책 좋아하시면 보통 어떤 거 보세요?",
          "speaker_id": "1",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027455.1.8",
          "form": "추천 감사합니다",
          "original_form": "추천 감사합니다{emoji:❤}",
          "speaker_id": "2",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027455.1.9",
          "form": "저는 진짜 잡식으로 읽어요",
          "original_form": "저는 진짜 잡식으로 읽어요",
          "speaker_id": "2",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100027455.1.10",
          "form": "챌린지하며 읽는건 시공디스커버리 총서예요",
          "original_form": "챌린지하며 읽는건 시공디스커버리 총서예요",
          "speaker_id": "2",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100027455.1.11",
          "form": "오 시공서쪽은 잘 모르긴 하는데 추천해주세요!",
          "original_form": "오 시공서쪽은 잘 모르긴 하는데 추천해주세요!",
          "speaker_id": "1",
          "time": "20210902 13:35"
        },
        {
          "id": "MDRW2100027455.1.12",
          "form": "저도 좀 잡식이에요 ㅋㅋ",
          "original_form": "저도 좀 잡식이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:36"
        },
        {
          "id": "MDRW2100027455.1.13",
          "form": "시공서는 약간 복불복이에요 ㅋㅋㅋ",
          "original_form": "시공서는 약간 복불복이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100027455.1.14",
          "form": "신화나 종교 혹은 위인전 이런건 그 분야 덕후박사가 열심히 써서 재밌는데",
          "original_form": "신화나 종교 혹은 위인전 이런건 그 분야 덕후박사가 열심히 써서 재밌는데",
          "speaker_id": "2",
          "time": "20210902 13:45"
        },
        {
          "id": "MDRW2100027455.1.15",
          "form": "건물 이런 주제는 건물도 보여주고 각도가 몇이고 이래버려요",
          "original_form": "건물 이런 주제는 건물도 보여주고 각도가 몇이고 이래버려요",
          "speaker_id": "2",
          "time": "20210902 13:46"
        },
        {
          "id": "MDRW2100027455.1.16",
          "form": "아 비문학 좋아하시는거에요 그럼?ㅋㅋ",
          "original_form": "아 비문학 좋아하시는거에요 그럼?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:48"
        },
        {
          "id": "MDRW2100027455.1.17",
          "form": "소설도 좋아해요 사실 소설 전공이거든요!ㅋㅋ",
          "original_form": "소설도 좋아해요 사실 소설 전공이거든요!ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:48"
        },
        {
          "id": "MDRW2100027455.1.18",
          "form": "근데 약간 이게 전공이니까 미묘해요 전공서 읽는 느낌이라 ㅋㅋㅋ 즐기면서 읽는건 비문학이에요",
          "original_form": "근데 약간 이게 전공이니까 미묘해요 전공서 읽는 느낌이라 ㅋㅋㅋ 즐기면서 읽는건 비문학이에요",
          "speaker_id": "2",
          "time": "20210902 13:49"
        },
        {
          "id": "MDRW2100027455.1.19",
          "form": "아하 그마음 좀 이해돼요 ㅋㅋㅋ",
          "original_form": "아하 그마음 좀 이해돼요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100027455.1.20",
          "form": "뭔가 분석해야할거같은..",
          "original_form": "뭔가 분석해야할거같은..",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027455.1.21",
          "form": "지금 읽는 책은 뭔가요?",
          "original_form": "지금 읽는 책은 뭔가요?",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100027455.1.22",
          "form": "요즘 매일 읽는 건 하루 쓰기 공부요+",
          "original_form": "요즘 매일 읽는 건 하루 쓰기 공부요+",
          "speaker_id": "2",
          "time": "20210902 15:00"
        },
        {
          "id": "MDRW2100027455.1.23",
          "form": "힐링책 재질이에요 ㅋㅋㅋ",
          "original_form": "힐링책 재질이에요 ㅋㅋㅋ",
