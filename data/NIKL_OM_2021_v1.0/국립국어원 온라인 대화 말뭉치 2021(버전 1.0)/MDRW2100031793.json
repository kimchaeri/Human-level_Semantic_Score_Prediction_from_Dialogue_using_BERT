      "utterance": [
        {
          "id": "MDRW2100031793.1.1",
          "form": "저는 갈비먹었어요",
          "original_form": "저는 갈비먹었어요",
          "speaker_id": "1",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100031793.1.2",
          "form": "갈비 맛있었겠네요 ㅎㅎ",
          "original_form": "갈비 맛있었겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100031793.1.3",
          "form": "네 정말 또가서 먹고싶어요",
          "original_form": "네 정말 또가서 먹고싶어요",
          "speaker_id": "1",
          "time": "20210905 00:30"
        },
        {
          "id": "MDRW2100031793.1.4",
          "form": "그럼 여행계획은 있으세요?!!",
          "original_form": "그럼 여행계획은 있으세요?!!",
          "speaker_id": "1",
          "time": "20210905 00:31"
        },
        {
          "id": "MDRW2100031793.1.5",
          "form": "딱히 계획은 없어요 .. 님은요 ?",
          "original_form": "딱히 계획은 없어요 .. 님은요 ?",
          "speaker_id": "2",
          "time": "20210905 00:31"
        },
        {
          "id": "MDRW2100031793.1.6",
          "form": "저도 계획은 딱히 없어요",
          "original_form": "저도 계획은 딱히 없어요",
          "speaker_id": "1",
          "time": "20210905 00:31"
        },
        {
          "id": "MDRW2100031793.1.7",
          "form": "좋아하질않아서",
          "original_form": "좋아하질않아서",
          "speaker_id": "1",
          "time": "20210905 00:31"
        },
        {
          "id": "MDRW2100031793.1.8",
          "form": "가고 싶은 곳은 있나요 ?",
          "original_form": "가고 싶은 곳은 있나요 ?",
          "speaker_id": "2",
          "time": "20210905 00:31"
        },
        {
          "id": "MDRW2100031793.1.9",
          "form": "아 제가 제주도를 안가봐서",
          "original_form": "아 제가 제주도를 안가봐서",
          "speaker_id": "1",
          "time": "20210905 00:31"
        },
        {
          "id": "MDRW2100031793.1.10",
          "form": "가보고싶긴해요!",
          "original_form": "가보고싶긴해요!",
          "speaker_id": "1",
          "time": "20210905 00:31"
        },
        {
          "id": "MDRW2100031793.1.11",
          "form": "아 제주도 많이들 가죠",
          "original_form": "아 제주도 많이들 가죠",
          "speaker_id": "2",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031793.1.12",
          "form": "네네 바다가 그렇게 이쁘다고 하더라고요ㅠ",
          "original_form": "네네 바다가 그렇게 이쁘다고 하더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031793.1.13",
          "form": "바다좋아하세요?",
          "original_form": "바다좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031793.1.14",
          "form": "네 막상 가면 좋아해요",
          "original_form": "네 막상 가면 좋아해요",
          "speaker_id": "2",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031793.1.15",
          "form": "요즘 코로나로 못가니까 가보고싶네요",
          "original_form": "요즘 코로나로 못가니까 가보고싶네요",
          "speaker_id": "2",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031793.1.16",
          "form": "역시ㅠ",
          "original_form": "역시ㅠ",
          "speaker_id": "1",
          "time": "20210905 00:32"
        },
        {
          "id": "MDRW2100031793.1.17",
          "form": "코로나",
          "original_form": "코로나",
          "speaker_id": "1",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100031793.1.18",
          "form": "코로나가 얼른끝나야 여행도 다닐텐데..",
          "original_form": "코로나가 얼른끝나야 여행도 다닐텐데..",
          "speaker_id": "2",
          "time": "20210905 00:33"
        },
        {
          "id": "MDRW2100031793.1.19",
          "form": "맞아요 얼른끝났으면좋겠네요ㅠ",
          "original_form": "맞아요 얼른끝났으면좋겠네요ㅠ",
          "speaker_id": "1",
          "time": "20210905 00:33"
        }
      ]
