      "utterance": [
        {
          "id": "MDRW2100043836.1.1",
          "form": "여행갔던 곳 중 어디가 좋으셨나요?",
          "original_form": "여행갔던 곳 중 어디가 좋으셨나요?",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043836.1.2",
          "form": "해외요",
          "original_form": "해외요",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100043836.1.3",
          "form": "어디가좋으셨나요?",
          "original_form": "어디가좋으셨나요?",
          "speaker_id": "2",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100043836.1.4",
          "form": "전 뉴질랜즈랑 호주요",
          "original_form": "전 뉴질랜즈랑 호주요",
          "speaker_id": "1",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043836.1.5",
          "form": "호주가고싶네요",
          "original_form": "호주가고싶네요",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100043836.1.6",
          "form": "자연경관이 너무 좋았고 스테이크가 어딜 가도 다 맛있었어요",
          "original_form": "자연경관이 너무 좋았고 스테이크가 어딜 가도 다 맛있었어요",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100043836.1.7",
          "form": "와.멋지네요",
          "original_form": "와.멋지네요",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043836.1.8",
          "form": "호주 자외선이 쎄서 썬글라스 필수예요",
          "original_form": "호주 자외선이 쎄서 썬글라스 필수예요",
          "speaker_id": "1",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100043836.1.9",
          "form": "가보고싶어요",
          "original_form": "가보고싶어요",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043836.1.10",
          "form": "그냥 자연이 넘 좋아요 바닷길 걷는데 그것도 너무 좋았어요",
          "original_form": "그냥 자연이 넘 좋아요 바닷길 걷는데 그것도 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043836.1.11",
          "form": "와!!!그렇군요.한번가면 다시 한국에 오고싶지않다더니",
          "original_form": "와!!!그렇군요.한번가면 다시 한국에 오고싶지않다더니",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100043836.1.12",
          "form": "맞아요. 한국으로 오고 싶지 않았어요",
          "original_form": "맞아요. 한국으로 오고 싶지 않았어요",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100043836.1.13",
          "form": "다들 그러더라구요",
          "original_form": "다들 그러더라구요",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100043836.1.14",
          "form": "유학다녀오신거예요?",
          "original_form": "유학다녀오신거예요?",
          "speaker_id": "2",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100043836.1.15",
          "form": "아뇨 호주는 여행간거고 뉴질랜드는 한달 어학연수 갔다가 좋아서 다시 여행으로 또 갔어요",
          "original_form": "아뇨 호주는 여행간거고 뉴질랜드는 한달 어학연수 갔다가 좋아서 다시 여행으로 또 갔어요",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100043836.1.16",
          "form": "아..한달만 어학연수도 가요?",
          "original_form": "아..한달만 어학연수도 가요?",
          "speaker_id": "2",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100043836.1.17",
          "form": "1년씩 가야하는줄알았네요..몰라서~",
          "original_form": "1년씩 가야하는줄알았네요..몰라서~",
          "speaker_id": "2",
          "time": "20210907 21:30"
        }
      ]
