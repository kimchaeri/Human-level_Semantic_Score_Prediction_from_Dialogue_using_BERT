      "utterance": [
        {
          "id": "MDRW2100010622.1.1",
          "form": "저의 가족께서 오늘 큰힘쓰셨습니다",
          "original_form": "저의 가족께서 오늘 큰힘쓰셨습니다",
          "speaker_id": "1",
          "time": "20210823 20:08"
        },
        {
          "id": "MDRW2100010622.1.2",
          "form": "저는 눈물을 흘리며 두그릇 순삭했습죠",
          "original_form": "저는 눈물을 흘리며 두그릇 순삭했습죠",
          "speaker_id": "1",
          "time": "20210823 20:08"
        },
        {
          "id": "MDRW2100010622.1.3",
          "form": "부럽습니다 크..",
          "original_form": "부럽습니다 크..",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010622.1.4",
          "form": "민어는 회, 전, 맑은탕으로만 먹어봤어요",
          "original_form": "민어는 회, 전, 맑은탕으로만 먹어봤어요",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010622.1.5",
          "form": "고오급 생선..",
          "original_form": "고오급 생선..",
          "speaker_id": "2",
          "time": "20210823 20:13"
        },
        {
          "id": "MDRW2100010622.1.6",
          "form": "요즘은 생선도 조리가 쉽게 나온답니다",
          "original_form": "요즘은 생선도 조리가 쉽게 나온답니다",
          "speaker_id": "1",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010622.1.7",
          "form": "반건조돼서 밀봉포장된거 먹었어요",
          "original_form": "반건조돼서 밀봉포장된거 먹었어요",
          "speaker_id": "1",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010622.1.8",
          "form": "name1 요리왕같아요!",
          "original_form": "&name1& 요리왕같아요!",
          "speaker_id": "2",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010622.1.9",
          "form": "요새 삼치도 제철이래요 왕삼치들 이마트에도 나왔답니다",
          "original_form": "요새 삼치도 제철이래요 왕삼치들 이마트에도 나왔답니다",
          "speaker_id": "2",
          "time": "20210823 20:14"
        },
        {
          "id": "MDRW2100010622.1.10",
          "form": "요리왕 됐으면좋겠어요..! 요즘은 요리잘하는사람이멋지더라구요",
          "original_form": "요리왕 됐으면좋겠어요..! 요즘은 요리잘하는사람이멋지더라구요",
          "speaker_id": "1",
          "time": "20210823 20:15"
        },
        {
          "id": "MDRW2100010622.1.11",
          "form": "삼치구이 존맛인데요",
          "original_form": "삼치구이 존맛인데요",
          "speaker_id": "1",
          "time": "20210823 20:15"
        },
        {
          "id": "MDRW2100010622.1.12",
          "form": "혹쉬 즐기는 요리가있으신가요",
          "original_form": "혹쉬 즐기는 요리가있으신가요",
          "speaker_id": "1",
          "time": "20210823 20:19"
        },
        {
          "id": "MDRW2100010622.1.13",
          "form": "즐겨하시는",
          "original_form": "즐겨하시는",
          "speaker_id": "1",
          "time": "20210823 20:19"
        },
        {
          "id": "MDRW2100010622.1.14",
          "form": "음 즐겨서 하는거는 닭감자탕이요",
          "original_form": "음 즐겨서 하는거는 닭감자탕이요",
          "speaker_id": "2",
          "time": "20210823 20:24"
        },
        {
          "id": "MDRW2100010622.1.15",
          "form": "닭 감자 물 소금+ 압력솥만 씁니당",
          "original_form": "닭 감자 물 소금+ 압력솥만 씁니당",
          "speaker_id": "2",
          "time": "20210823 20:25"
        },
        {
          "id": "MDRW2100010622.1.16",
          "form": "오 name2이 요리왕이신데요..!",
          "original_form": "오 &name2&이 요리왕이신데요..!",
          "speaker_id": "1",
          "time": "20210823 20:25"
        },
        {
          "id": "MDRW2100010622.1.17",
          "form": "빨간 닭 탕 말씀하시는거 맞나요?",
          "original_form": "빨간 닭 탕 말씀하시는거 맞나요?",
          "speaker_id": "1",
          "time": "20210823 20:25"
        },
        {
          "id": "MDRW2100010622.1.18",
          "form": "아니에요! 맑은탕이에요",
          "original_form": "아니에요! 맑은탕이에요",
          "speaker_id": "2",
          "time": "20210823 20:25"
        },
        {
          "id": "MDRW2100010622.1.19",
          "form": "오왕 대박이에요 맛있나요? 삼계탕맛 날것 같아요",
          "original_form": "오왕 대박이에요 맛있나요? 삼계탕맛 날것 같아요",
          "speaker_id": "1",
          "time": "20210823 20:26"
        },
        {
          "id": "MDRW2100010622.1.20",
          "form": "압력솥에 감자통채로 통닭이나 조각낸닭을 넣고 찰랑찰랑할 정도 물 넣고 소금간한후 압력솥으로 끓여내기만 하는 초간단 요리에요",
          "original_form": "압력솥에 감자통채로 통닭이나 조각낸닭을 넣고 찰랑찰랑할 정도 물 넣고 소금간한후 압력솥으로 끓여내기만 하는 초간단 요리에요",
          "speaker_id": "2",
          "time": "20210823 20:26"
        },
        {
          "id": "MDRW2100010622.1.21",
          "form": "감자가 진짜",
          "original_form": "감자가 진짜",
          "speaker_id": "2",
          "time": "20210823 20:26"
        },
        {
          "id": "MDRW2100010622.1.22",
          "form": "미친맛이에요",
          "original_form": "미친맛이에요",
          "speaker_id": "2",
          "time": "20210823 20:26"
        },
        {
          "id": "MDRW2100010622.1.23",
          "form": "닭은 백숙느낌인데",
          "original_form": "닭은 백숙느낌인데",
          "speaker_id": "2",
          "time": "20210823 20:26"
        },
        {
          "id": "MDRW2100010622.1.24",
          "form": "헙",
          "original_form": "헙",
          "speaker_id": "1",
          "time": "20210823 20:27"
        },
        {
          "id": "MDRW2100010622.1.25",
          "form": "기름진 닭육수에 삶겨진 감자가..",
          "original_form": "기름진 닭육수에 삶겨진 감자가..",
          "speaker_id": "2",
          "time": "20210823 20:27"
        },
        {
          "id": "MDRW2100010622.1.26",
          "form": "맑은 닭육수에 끓여진 감자가",
          "original_form": "맑은 닭육수에 끓여진 감자가",
          "speaker_id": "1",
          "time": "20210823 20:27"
        },
        {
          "id": "MDRW2100010622.1.27",
          "form": "맛없을리가 없잖아요",
          "original_form": "맛없을리가 없잖아요",
          "speaker_id": "1",
          "time": "20210823 20:27"
        },
        {
          "id": "MDRW2100010622.1.28",
          "form": "닭보다 감자를 먹고싶어서 끓이게 됩니다",
          "original_form": "닭보다 감자를 먹고싶어서 끓이게 됩니다",
          "speaker_id": "2",
          "time": "20210823 20:27"
        },
        {
          "id": "MDRW2100010622.1.29",
          "form": "쌀쌀한 날씨에 최고에요",
          "original_form": "쌀쌀한 날씨에 최고에요",
          "speaker_id": "2",
          "time": "20210823 20:27"
        },
        {
          "id": "MDRW2100010622.1.30",
          "form": "냄비론 안되더라고요 꼭 압력솥!",
          "original_form": "냄비론 안되더라고요 꼭 압력솥!",
          "speaker_id": "2",
          "time": "20210823 20:27"
        },
        {
          "id": "MDRW2100010622.1.31",
          "form": "와 진짜 배운분",
          "original_form": "와 진짜 배운분",
          "speaker_id": "1",
          "time": "20210823 20:27"
        },
        {
          "id": "MDRW2100010622.1.32",
          "form": "가방끈 삼메다",
          "original_form": "가방끈 삼메다",
          "speaker_id": "1",
          "time": "20210823 20:28"
        },
        {
          "id": "MDRW2100010622.1.33",
          "form": "그리고 뼈에서 육수가 나와야해서 얘는 뼈있는 닭으로 해야해요",
          "original_form": "그리고 뼈에서 육수가 나와야해서 얘는 뼈있는 닭으로 해야해요",
          "speaker_id": "2",
          "time": "20210823 20:28"
        },
        {
          "id": "MDRW2100010622.1.34",
          "form": "옛날 82쿡 네임드분 레시피였어요 ㅋㅋ",
