        },
        {
          "id": "MDRW2100012796.1.2",
          "form": "네ㄹㅎㅎㅎ",
          "original_form": "네ㄹㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:32"
        },
        {
          "id": "MDRW2100012796.1.3",
          "form": "퇴근하셨어요?ㅎㅎ",
          "original_form": "퇴근하셨어요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:32"
        },
        {
          "id": "MDRW2100012796.1.4",
          "form": "전 아따 음악들으면서 퇴근했네요ㅎㅎ",
          "original_form": "전 아따 음악들으면서 퇴근했네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:32"
        },
        {
          "id": "MDRW2100012796.1.5",
          "form": "네넹 ㅎㅎ 저도 음악 들으면서",
          "original_form": "네넹 ㅎㅎ 저도 음악 들으면서",
          "speaker_id": "1",
          "time": "20210825 19:34"
        },
        {
          "id": "MDRW2100012796.1.6",
          "form": "비와서 \r\n지아 노래 들으면서  술한잔해요 ㅎㅎ",
          "original_form": "비와서 \r\n지아 노래 들으면서  술한잔해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:34"
        },
        {
          "id": "MDRW2100012796.1.7",
          "form": "우와ㅎㅎ",
          "original_form": "우와ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100012796.1.8",
          "form": "갬성인가요?ㄹㄹ",
          "original_form": "갬성인가요?ㄹㄹ",
          "speaker_id": "2",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100012796.1.9",
          "form": "지아 노래좋죠ㅎ",
          "original_form": "지아 노래좋죠ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100012796.1.10",
          "form": "뭐듣고계세요?",
          "original_form": "뭐듣고계세요?",
          "speaker_id": "2",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100012796.1.11",
          "form": "ㅎㅎㅎ 지금은 윤하 노래 듣고 잇어요 ㅎㅎ",
          "original_form": "ㅎㅎㅎ 지금은 윤하 노래 듣고 잇어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100012796.1.12",
          "form": "오호ㄹ",
          "original_form": "오호ㄹ",
          "speaker_id": "2",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100012796.1.13",
          "form": "근데 왠 술이세요?ㅎ",
          "original_form": "근데 왠 술이세요?ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100012796.1.14",
          "form": "아 ㅋㅋ 지아노래 제목이",
          "original_form": "아 ㅋㅋ 지아노래 제목이",
          "speaker_id": "1",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100012796.1.15",
          "form": "술한잔해요   에요 ㅋ",
          "original_form": "술한잔해요   에요 ㅋ",
          "speaker_id": "1",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100012796.1.16",
          "form": "저 술 못마셔요 ㅎㅎ",
          "original_form": "저 술 못마셔요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100012796.1.17",
          "form": "ㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100012796.1.18",
          "form": "아 ㅎㅎㅎㅎㅎㄹㄹㄹ",
          "original_form": "아 ㅎㅎㅎㅎㅎㄹㄹㄹ",
          "speaker_id": "2",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100012796.1.19",
          "form": "난 술한잔 하신다고ㅎㅎㅎ",
          "original_form": "난 술한잔 하신다고ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100012796.1.20",
          "form": "비오니 들을만한 곡이 넘 많네요 ㅎㅎ",
          "original_form": "비오니 들을만한 곡이 넘 많네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100012796.1.21",
          "form": "노래제목이었는데ㅎㅎ",
          "original_form": "노래제목이었는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100012796.1.22",
          "form": "글죠ㅎ",
          "original_form": "글죠ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100012796.1.23",
          "form": "헤이즈노래도좋고ㅎ",
          "original_form": "헤이즈노래도좋고ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100012796.1.24",
          "form": "헤이즈 노래도 들어야겠네요",
          "original_form": "헤이즈 노래도 들어야겠네요",
          "speaker_id": "1",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100012796.1.25",
          "form": "음악이 주는 힘이있어요ㅎㅎ",
          "original_form": "음악이 주는 힘이있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100012796.1.26",
          "form": "전지금 심심이한다고 싄나는 음악을ㅎㅎㅎ",
          "original_form": "전지금 심심이한다고 싄나는 음악을ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100012796.1.27",
          "form": "브레이브걸스 롤린듣고있네요ㅎㅎ",
          "original_form": "브레이브걸스 롤린듣고있네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100012796.1.28",
          "form": "오홍 뭐들어요 ㅎㅎ",
          "original_form": "오홍 뭐들어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100012796.1.29",
          "form": "앗 롤린 좋죵~~",
          "original_form": "앗 롤린 좋죵~~",
          "speaker_id": "1",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100012796.1.30",
          "form": "요즘 똥밟았네 인가",
          "original_form": "요즘 똥밟았네 인가",
          "speaker_id": "1",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100012796.1.31",
          "form": "노래 예전에도 듣는거 좋아했는데ㅎㄹ",
          "original_form": "노래 예전에도 듣는거 좋아했는데ㅎㄹ",
          "speaker_id": "2",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100012796.1.32",
          "form": "아ㅎㅎ 그것도나왔어요?ㅎㅎ",
