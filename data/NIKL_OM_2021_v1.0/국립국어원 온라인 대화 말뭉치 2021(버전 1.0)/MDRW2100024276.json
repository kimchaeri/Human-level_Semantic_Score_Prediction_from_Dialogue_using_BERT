      "utterance": [
        {
          "id": "MDRW2100024276.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210830 23:57"
        },
        {
          "id": "MDRW2100024276.1.2",
          "form": "저는 name2입니다~",
          "original_form": "저는 &name2&입니다~",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024276.1.3",
          "form": "안녕하세요~!~! 나는 name1 푸하하",
          "original_form": "안녕하세요~!~! 나는 &name1& 푸하하",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024276.1.4",
          "form": "언니 나는 바디미스트 하나 살구야",
          "original_form": "언니 나는 바디미스트 하나 살구야",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024276.1.5",
          "form": "4만7천원짜리 찜꽁해놨어!!",
          "original_form": "4만7천원짜리 찜꽁해놨어!!",
          "speaker_id": "1",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100024276.1.6",
          "form": "나도 요즘 물욕이 너~~무 많아",
          "original_form": "나도 요즘 물욕이 너~~무 많아",
          "speaker_id": "2",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024276.1.7",
          "form": "사고싶은거 다말해줘!!!!",
          "original_form": "사고싶은거 다말해줘!!!!",
          "speaker_id": "1",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100024276.1.8",
          "form": "알았어... 기다려봐...",
          "original_form": "알았어... 기다려봐...{emoji:☆}",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024276.1.9",
          "form": "나는 일단",
          "original_form": "나는 일단",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024276.1.10",
          "form": "까리한 운동화를 사고싶어,,,",
          "original_form": "까리한 운동화를 사고싶어,,,",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024276.1.11",
          "form": "내가 올해 초에 나이키 데이브레이크 운동화를 샀는데",
          "original_form": "내가 올해 초에 나이키 데이브레이크 운동화를 샀는데",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024276.1.12",
          "form": "알고보니 가품이었던거야",
          "original_form": "알고보니 가품이었던거야",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024276.1.13",
          "form": "그래서 바닥이 반으로 갈라졌어!!!",
          "original_form": "그래서 바닥이 반으로 갈라졌어!!!",
          "speaker_id": "2",
          "time": "20210831 00:05"
        },
        {
          "id": "MDRW2100024276.1.14",
          "form": "할 수 없이 새 운동화가 필요한데...",
          "original_form": "할 수 없이 새 운동화가 필요한데...",
          "speaker_id": "2",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100024276.1.15",
          "form": "name1는?!",
          "original_form": "&name1&는?!",
          "speaker_id": "2",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100024276.1.16",
          "form": "사고싶은 바디미스트 어디꺼야???",
          "original_form": "사고싶은 바디미스트 어디꺼야???",
          "speaker_id": "2",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100024276.1.17",
          "form": "이이이이이런 갈라졌다고?!!?! 어찌이럴수가!!! 얼른 새거 사야겠구만!!!!!!",
          "original_form": "이이이이이런 갈라졌다고?!!?! 어찌이럴수가!!! 얼른 새거 사야겠구만!!!!!!",
          "speaker_id": "1",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100024276.1.18",
          "form": "나는 아베다꺼!",
          "original_form": "나는 아베다꺼!",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024276.1.19",
          "form": "바디미스트!",
          "original_form": "바디미스트!",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024276.1.20",
          "form": "요가원에서 선생님이 칙칙 뿌려주시는데 와우 냄새가 너무 황홀해",
          "original_form": "요가원에서 선생님이 칙칙 뿌려주시는데 와우 냄새가 너무 황홀해",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024276.1.21",
          "form": "헉 너무 궁금하다",
          "original_form": "헉 너무 궁금하다",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024276.1.22",
          "form": "어떤 향이길래 황홀한걸까?!",
          "original_form": "어떤 향이길래 황홀한걸까?!",
          "speaker_id": "2",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024276.1.23",
          "form": "운동화 쇼핑 너무 신나겠다ㅠㅠ 난 최근에 에어포스 샀는데 아주 만족스러워",
          "original_form": "운동화 쇼핑 너무 신나겠다ㅠㅠ 난 최근에 에어포스 샀는데 아주 만족스러워",
          "speaker_id": "1",
          "time": "20210831 00:07"
        },
        {
          "id": "MDRW2100024276.1.24",
          "form": "헉 궁금하다!!!!",
          "original_form": "헉 궁금하다!!!!",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100024276.1.25",
          "form": "첨엔 무겁고 불편한데",
          "original_form": "첨엔 무겁고 불편한데",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100024276.1.26",
          "form": "아니근데 운동화는 한 번 사려면",
          "original_form": "아니근데 운동화는 한 번 사려면",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100024276.1.27",
          "form": "나중엔 이뻐서 용서돼!!",
          "original_form": "나중엔 이뻐서 용서돼!!",
          "speaker_id": "1",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100024276.1.28",
          "form": "십 만원 가까이 넘어서",
          "original_form": "십 만원 가까이 넘어서",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100024276.1.29",
          "form": "통장이 써늘해지는 게",
          "original_form": "통장이 써늘해지는 게",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100024276.1.30",
          "form": "서글프달까.....",
          "original_form": "서글프달까.....",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100024276.1.31",
          "form": "맞아ㅠ 하지만 운동화 하나만 잘 신어도 힙해보이는데!!!!!",
          "original_form": "맞아ㅠ 하지만 운동화 하나만 잘 신어도 힙해보이는데!!!!!",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100024276.1.32",
          "form": "아완전 맞아...............",
          "original_form": "아완전 맞아...............",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024276.1.33",
          "form": "나도 그래서 신발을 막 자주 사진 않아... 비싸...큭..",
          "original_form": "나도 그래서 신발을 막 자주 사진 않아... 비싸...큭..",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100024276.1.34",
          "form": "가슴아파..",
          "original_form": "가슴아파..",
          "speaker_id": "1",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100024276.1.35",
          "form": "사실근데 그 바디미스트 향이 막",
          "original_form": "사실근데 그 바디미스트 향이 막",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100024276.1.36",
          "form": "기억이 잘 나진 않아.... 그냥",
          "original_form": "기억이 잘 나진 않아.... 그냥",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100024276.1.37",
          "form": "그 왜 요가 끝나면 회원들 다같이 누워서 쉬는거 있거든? 그거할때 쌤이 지나다니시면서 칙칙 뿌려주시는데",
          "original_form": "그 왜 요가 끝나면 회원들 다같이 누워서 쉬는거 있거든? 그거할때 쌤이 지나다니시면서 칙칙 뿌려주시는데",
          "speaker_id": "1",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100024276.1.38",
          "form": "그때의 그.... 릴뤡스...한.. 분위기.. 그게 황홀해...",
          "original_form": "그때의 그.... 릴뤡스...한.. 분위기.. 그게 황홀해...",
          "speaker_id": "1",
          "time": "20210831 00:14"
        },
        {
          "id": "MDRW2100024276.1.39",
          "form": "와 정말 신기하다....",
          "original_form": "와 정말 신기하다....",
          "speaker_id": "2",
          "time": "20210831 00:20"
        },
        {
          "id": "MDRW2100024276.1.40",
          "form": "우리 필라테스 학원은 왜 그런거 안뿌려주지? 흥",
          "original_form": "우리 필라테스 학원은 왜 그런거 안뿌려주지? 흥",
          "speaker_id": "2",
          "time": "20210831 00:20"
        },
        {
          "id": "MDRW2100024276.1.41",
          "form": "그리고 나는 사고싶은 게 항상 많아...",
          "original_form": "그리고 나는 사고싶은 게 항상 많아...",
          "speaker_id": "2",
          "time": "20210831 00:21"
        },
        {
          "id": "MDRW2100024276.1.42",
          "form": "내가 스타일이 많이 달라졌다 보니",
          "original_form": "내가 스타일이 많이 달라졌다 보니",
          "speaker_id": "2",
          "time": "20210831 00:21"
        },
        {
          "id": "MDRW2100024276.1.43",
          "form": "옷도 싹 다 새로 사고싶고....!!!!",
          "original_form": "옷도 싹 다 새로 사고싶고....!!!!",
          "speaker_id": "2",
          "time": "20210831 00:21"
        },
        {
          "id": "MDRW2100024276.1.44",
          "form": "와~~ 무슨 말인지 알거같아...... 나도나도 멋쟁이 옷 갖고 싶고 그렇다",
          "original_form": "와~~ 무슨 말인지 알거같아...... 나도나도 멋쟁이 옷 갖고 싶고 그렇다",
          "speaker_id": "1",
          "time": "20210831 00:23"
        },
        {
          "id": "MDRW2100024276.1.45",
          "form": "나도나도",
          "original_form": "나도나도",
          "speaker_id": "2",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100024276.1.46",
          "form": "심지어 검정색 가죽자켓도",
          "original_form": "심지어 검정색 가죽자켓도",
          "speaker_id": "2",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100024276.1.47",
          "form": "왠지 지금 머리랑은 좀 안어울리는 느낌!?!",
          "original_form": "왠지 지금 머리랑은 좀 안어울리는 느낌!?!",
          "speaker_id": "2",
          "time": "20210831 00:25"
        },
        {
          "id": "MDRW2100024276.1.48",
