      "utterance": [
        {
          "id": "MDRW2100037944.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100037944.1.2",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037944.1.3",
          "form": "제가 지금 나가볼건데 글 남겨놓으신 답 할게여",
          "original_form": "제가 지금 나가볼건데 글 남겨놓으신 답 할게여",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100037944.1.4",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100037944.1.5",
          "form": "네 간단하게 햇반에 반찬 먹았어요",
          "original_form": "네 간단하게 햇반에 반찬 먹았어요",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100037944.1.6",
          "form": "그렇군요 그럼 좋아하시는 음식 있으신가요?",
          "original_form": "그렇군요 그럼 좋아하시는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037944.1.7",
          "form": "오 전 기본적으로 채색선호하구요",
          "original_form": "오 전 기본적으로 채색선호하구요",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100037944.1.8",
          "form": "얼마전에 마라탕 마라샹궈 맛있게 먹었아요",
          "original_form": "얼마전에 마라탕 마라샹궈 맛있게 먹었아요",
          "speaker_id": "2",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100037944.1.9",
          "form": "매운거 잘드시나봐요",
          "original_form": "매운거 잘드시나봐요",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100037944.1.10",
          "form": "아뇨 맴찔인데",
          "original_form": "아뇨 맴찔인데",
          "speaker_id": "2",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100037944.1.11",
          "form": "제주가 날씨가 습하고 비오고 그러니까",
          "original_form": "제주가 날씨가 습하고 비오고 그러니까",
          "speaker_id": "2",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100037944.1.12",
          "form": "자연스레 땡기더라구요",
          "original_form": "자연스레 땡기더라구요",
          "speaker_id": "2",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100037944.1.13",
          "form": "제주에 마라 잘하는  추천할만한집 있나요?",
          "original_form": "제주에 마라 잘하는  추천할만한집 있나요?",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100037944.1.14",
          "form": "저도 추천 좀 해주세여",
          "original_form": "저도 추천 좀 해주세여",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100037944.1.15",
          "form": "얼마전에 시켰는데 맛없어서 혼났어요",
          "original_form": "얼마전에 시켰는데 맛없어서 혼났어요",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100037944.1.16",
          "form": "제가 제주에는 연이 없어서요ㅠㅠ\r\n한번도 안가봤네요ㅠㅠ",
          "original_form": "제가 제주에는 연이 없어서요ㅠㅠ\r\n한번도 안가봤네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100037944.1.17",
          "form": "프렌차이즈 매장 가는 게 안전한가 같애요",
          "original_form": "프렌차이즈 매장 가는 게 안전한가 같애요",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100037944.1.18",
          "form": "마라탕은 유명한 프렌차이즈가 아직 없군여",
          "original_form": "마라탕은 유명한 프렌차이즈가 아직 없군여",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100037944.1.19",
          "form": "그러게요\r\n제주도에서는 어떤 음식이 제일 맛있으신가요?",
          "original_form": "그러게요\r\n제주도에서는 어떤 음식이 제일 맛있으신가요?",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100037944.1.20",
          "form": "고기국수랑 회",
          "original_form": "고기국수랑 회",
          "speaker_id": "2",
          "time": "20210901 22:27"
        },
        {
          "id": "MDRW2100037944.1.21",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100037944.1.22",
          "form": "고기국수 맛집 추천받아도 될까요?",
          "original_form": "고기국수 맛집 추천받아도 될까요?",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100037944.1.23",
          "form": "제가 좀더 살아보고 추천할데요",
          "original_form": "제가 좀더 살아보고 추천할데요",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100037944.1.24",
          "form": "자신이 없네여 ㅋㅋㅌ",
          "original_form": "자신이 없네여 ㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100037944.1.25",
          "form": "앗!!네ㅋㅋ",
          "original_form": "앗!!네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:33"
        }
      ]
    }
  ]
