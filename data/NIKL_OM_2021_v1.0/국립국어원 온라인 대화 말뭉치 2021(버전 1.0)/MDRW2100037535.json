      "utterance": [
        {
          "id": "MDRW2100037535.1.1",
          "form": "먹거 싶은거 있니??",
          "original_form": "먹거 싶은거 있니??",
          "speaker_id": "1",
          "time": "20210901 07:25"
        },
        {
          "id": "MDRW2100037535.1.2",
          "form": "돼지갈비요",
          "original_form": "돼지갈비요",
          "speaker_id": "2",
          "time": "20210901 07:27"
        },
        {
          "id": "MDRW2100037535.1.3",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210901 10:05"
        },
        {
          "id": "MDRW2100037535.1.4",
          "form": "오늘 먹나요?",
          "original_form": "오늘 먹나요?",
          "speaker_id": "2",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100037535.1.5",
          "form": "아니 내일 동생 오면 먹을거야",
          "original_form": "아니 내일 동생 오면 먹을거야",
          "speaker_id": "1",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100037535.1.6",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210901 20:50"
        },
        {
          "id": "MDRW2100037535.1.7",
          "form": "돼지양념갈비라서 맛있을거야",
          "original_form": "돼지양념갈비라서 맛있을거야",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037535.1.8",
          "form": "와~ 양념갈비 좋아요",
          "original_form": "와~ 양념갈비 좋아요",
          "speaker_id": "2",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037535.1.9",
          "form": "그래.. 너 양념한 고기 좋아하잖아.. 엄마가 알지",
          "original_form": "그래.. 너 양념한 고기 좋아하잖아.. 엄마가 알지",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037535.1.10",
          "form": "역시 엄마 최고!",
          "original_form": "역시 엄마 최고!",
          "speaker_id": "2",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100037535.1.11",
          "form": "ㅎㅎㅎ 엄마밖에 없지?",
          "original_form": "ㅎㅎㅎ 엄마밖에 없지?",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037535.1.12",
          "form": "네~ 온가족이 고기파티 하겠네요",
          "original_form": "네~ 온가족이 고기파티 하겠네요",
          "speaker_id": "2",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037535.1.13",
          "form": "응 7키로 샀으니까 아주 실컷 먹을 수 있을거야",
          "original_form": "응 7키로 샀으니까 아주 실컷 먹을 수 있을거야",
