      "utterance": [
        {
          "id": "MDRW2100013204.1.1",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100013204.1.2",
          "form": "보는건 좋아해요",
          "original_form": "보는건 좋아해요",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013204.1.3",
          "form": "올림픽보셨나요?",
          "original_form": "올림픽보셨나요?",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013204.1.4",
          "form": "저는 짤로 돌아다니는것밖에 못봤어요",
          "original_form": "저는 짤로 돌아다니는것밖에 못봤어요",
          "speaker_id": "1",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100013204.1.5",
          "form": "이번에 엄청 대단했다고 하더라고요ㅠ",
          "original_form": "이번에 엄청 대단했다고 하더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100013204.1.6",
          "form": "아  저는 집에 있느라 다봤어요",
          "original_form": "아  저는 집에 있느라 다봤어요",
          "speaker_id": "2",
          "time": "20210825 14:33"
        },
        {
          "id": "MDRW2100013204.1.7",
          "form": "어떤 종목이 제일 좋으셨나요?",
          "original_form": "어떤 종목이 제일 좋으셨나요?",
          "speaker_id": "1",
          "time": "20210825 14:34"
        },
        {
          "id": "MDRW2100013204.1.8",
          "form": "양궁이요",
          "original_form": "양궁이요",
          "speaker_id": "2",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100013204.1.9",
          "form": "보신건 어떤건가요?",
          "original_form": "보신건 어떤건가요?",
          "speaker_id": "2",
          "time": "20210825 14:38"
        },
        {
          "id": "MDRW2100013204.1.10",
          "form": "아무래도 짤이다보니 명중하는 장면이랑 기뻐하는모습 이정도였어요",
          "original_form": "아무래도 짤이다보니 명중하는 장면이랑 기뻐하는모습 이정도였어요",
          "speaker_id": "1",
          "time": "20210825 14:39"
        },
        {
          "id": "MDRW2100013204.1.11",
          "form": "다보셨으면 감동도 크셨을것같아요",
          "original_form": "다보셨으면 감동도 크셨을것같아요",
          "speaker_id": "1",
          "time": "20210825 14:40"
        },
        {
          "id": "MDRW2100013204.1.12",
          "form": "다이빙도 재밌고",
          "original_form": "다이빙도 재밌고",
          "speaker_id": "2",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100013204.1.13",
          "form": "육상도 재밌더라고요",
          "original_form": "육상도 재밌더라고요",
          "speaker_id": "2",
          "time": "20210825 14:41"
        },
        {
          "id": "MDRW2100013204.1.14",
          "form": "다이빙 왠지 재밌을것같아요",
          "original_form": "다이빙 왠지 재밌을것같아요",
          "speaker_id": "1",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013204.1.15",
          "form": "묘한 중독이",
          "original_form": "묘한 중독이",
          "speaker_id": "1",
          "time": "20210825 14:47"
        },
        {
          "id": "MDRW2100013204.1.16",
          "form": "한국선수도 나와서 재밌게 봤네요",
          "original_form": "한국선수도 나와서 재밌게 봤네요",
          "speaker_id": "2",
          "time": "20210825 14:54"
        },
        {
          "id": "MDRW2100013204.1.17",
          "form": "오.. 찾아봐야겠네요",
          "original_form": "오.. 찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013204.1.18",
          "form": "한국선수 잘했나요?",
          "original_form": "한국선수 잘했나요?",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013204.1.19",
          "form": "결선엔 올랐었어요",
          "original_form": "결선엔 올랐었어요",
          "speaker_id": "2",
          "time": "20210825 14:57"
        },
        {
          "id": "MDRW2100013204.1.20",
          "form": "와 왜 몰랐지 대박이네요",
          "original_form": "와 왜 몰랐지 대박이네요",
          "speaker_id": "1",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013204.1.21",
          "form": "엄청 잘한거네요",
          "original_form": "엄청 잘한거네요",
          "speaker_id": "1",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013204.1.22",
          "form": "네 육상도 잘했잖아요 높이뛰기 ㅎ",
          "original_form": "네 육상도 잘했잖아요 높이뛰기 ㅎ",
          "speaker_id": "2",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013204.1.23",
          "form": "안본거 후회되네요 티비 사야하낰ㅋㅋ",
          "original_form": "안본거 후회되네요 티비 사야하낰ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 15:00"
        },
        {
          "id": "MDRW2100013204.1.24",
          "form": "전 하루종일 집에 있느라 봤네요 ㅎ",
          "original_form": "전 하루종일 집에 있느라 봤네요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 15:04"
        }
      ]
