      "utterance": [
        {
          "id": "MDRW2100046169.1.1",
          "form": "오늘 날씨 너무좋네요",
          "original_form": "오늘 날씨 너무좋네요",
          "speaker_id": "2",
          "time": "20210908 11:26"
        },
        {
          "id": "MDRW2100046169.1.2",
          "form": "그러게요 화창하네요",
          "original_form": "그러게요 화창하네요",
          "speaker_id": "1",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046169.1.3",
          "form": "요며칠 계속 비오더니",
          "original_form": "요며칠 계속 비오더니",
          "speaker_id": "2",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046169.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046169.1.5",
          "form": "비오는날 맑은날 뭐가더좋나요?",
          "original_form": "비오는날 맑은날 뭐가더좋나요?",
          "speaker_id": "2",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046169.1.6",
          "form": "전 맑은날이요",
          "original_form": "전 맑은날이요",
          "speaker_id": "1",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046169.1.7",
          "form": "맑으면 기분도 좋아지죠",
          "original_form": "맑으면 기분도 좋아지죠",
          "speaker_id": "2",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046169.1.8",
          "form": "맞어요 그래서좋아요",
          "original_form": "맞어요 그래서좋아요",
          "speaker_id": "1",
          "time": "20210908 11:27"
        },
        {
          "id": "MDRW2100046169.1.9",
          "form": "비오는날도 나름 운치있어요",
          "original_form": "비오는날도 나름 운치있어요",
          "speaker_id": "2",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046169.1.10",
          "form": "빗소리 들으면 분위기있죠",
          "original_form": "빗소리 들으면 분위기있죠",
          "speaker_id": "1",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046169.1.11",
          "form": "빗소리가 너무 좋아요",
          "original_form": "빗소리가 너무 좋아요",
          "speaker_id": "2",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046169.1.12",
          "form": "그래서 비오는날도 괜찮아요",
          "original_form": "그래서 비오는날도 괜찮아요",
          "speaker_id": "1",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046169.1.13",
          "form": "하지만 비오는날 나가는건 싫죠?",
          "original_form": "하지만 비오는날 나가는건 싫죠?",
          "speaker_id": "2",
          "time": "20210908 11:28"
        },
        {
          "id": "MDRW2100046169.1.14",
          "form": "네 다 버려서요",
          "original_form": "네 다 버려서요",
          "speaker_id": "1",
          "time": "20210908 11:29"
        },
        {
          "id": "MDRW2100046169.1.15",
          "form": "신발이랑 양말...",
          "original_form": "신발이랑 양말...",
          "speaker_id": "2",
          "time": "20210908 11:29"
        },
        {
          "id": "MDRW2100046169.1.16",
          "form": "ㅠㅠ진짜 싫어요",
          "original_form": "ㅠㅠ진짜 싫어요",
          "speaker_id": "1",
          "time": "20210908 11:29"
        }
      ]
