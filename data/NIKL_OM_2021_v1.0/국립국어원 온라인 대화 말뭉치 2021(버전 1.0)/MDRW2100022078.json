      "utterance": [
        {
          "id": "MDRW2100022078.1.1",
          "form": "ㅎㅎ 그래도 오늘 name1님에대해서 많은 걸알앜ㅅ네요ㅎ",
          "original_form": "ㅎㅎ 그래도 오늘 &name1&님에대해서 많은 걸알앜ㅅ네요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:16"
        },
        {
          "id": "MDRW2100022078.1.2",
          "form": "조금 물어보기가 그래요ㅎ",
          "original_form": "조금 물어보기가 그래요ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:16"
        },
        {
          "id": "MDRW2100022078.1.3",
          "form": "어 헉교",
          "original_form": "어 헉교",
          "speaker_id": "1",
          "time": "20210829 02:17"
        },
        {
          "id": "MDRW2100022078.1.4",
          "form": "눌르라 한건데",
          "original_form": "눌르라 한건데",
          "speaker_id": "1",
          "time": "20210829 02:17"
        },
        {
          "id": "MDRW2100022078.1.5",
          "form": "직업도 그렇고ㅎ 왠지 실례일것같고ㅎㅎ",
          "original_form": "직업도 그렇고ㅎ 왠지 실례일것같고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:17"
        },
        {
          "id": "MDRW2100022078.1.6",
          "form": "ㅋㅋㅋ 손이 에러 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 손이 에러 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:17"
        },
        {
          "id": "MDRW2100022078.1.7",
          "form": "뭐 주제야 ㅎㅎㅎ",
          "original_form": "뭐 주제야 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:17"
        },
        {
          "id": "MDRW2100022078.1.8",
          "form": "일임 ㅋ",
          "original_form": "일임 ㅋ",
          "speaker_id": "1",
          "time": "20210829 02:17"
        },
        {
          "id": "MDRW2100022078.1.9",
          "form": "저에겐 의미없죠ㄹㄹㄹ",
          "original_form": "저에겐 의미없죠ㄹㄹㄹ",
          "speaker_id": "2",
          "time": "20210829 02:17"
        },
        {
          "id": "MDRW2100022078.1.10",
          "form": "뭐 그냥다 옛날 추억 ㅋ",
          "original_form": "뭐 그냥다 옛날 추억 ㅋ",
          "speaker_id": "1",
          "time": "20210829 02:17"
        },
        {
          "id": "MDRW2100022078.1.11",
          "form": "너무 많은거 이야기안해주셔도되요ㄹㄹ",
          "original_form": "너무 많은거 이야기안해주셔도되요ㄹㄹ",
          "speaker_id": "2",
          "time": "20210829 02:17"
        },
        {
          "id": "MDRW2100022078.1.12",
          "form": "음 간만에 추억 이야기 한거죠 ㄹㅎ",
          "original_form": "음 간만에 추억 이야기 한거죠 ㄹㅎ",
          "speaker_id": "1",
          "time": "20210829 02:18"
        },
        {
          "id": "MDRW2100022078.1.13",
          "form": "저희는 오픈채팅방임ㅎㅎ 항상비밀보장ㅎ",
          "original_form": "저희는 오픈채팅방임ㅎㅎ 항상비밀보장ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:18"
        },
        {
          "id": "MDRW2100022078.1.14",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:18"
        },
        {
          "id": "MDRW2100022078.1.15",
          "form": "ㅋㅋㅋㅋ",
