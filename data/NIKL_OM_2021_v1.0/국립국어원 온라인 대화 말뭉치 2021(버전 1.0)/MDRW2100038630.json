        },
        {
          "id": "MDRW2100038630.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100038630.1.3",
          "form": "점심 뭐드셨나요?",
          "original_form": "점심 뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100038630.1.4",
          "form": "닭볶음탕 먹었습니다!",
          "original_form": "닭볶음탕 먹었습니다!",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100038630.1.5",
          "form": "뭐 드셨어요??",
          "original_form": "뭐 드셨어요??",
          "speaker_id": "1",
          "time": "20210902 17:45"
        },
        {
          "id": "MDRW2100038630.1.6",
          "form": "저는 조금 바빠서 컵라면 먹었어요",
          "original_form": "저는 조금 바빠서 컵라면 먹었어요",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100038630.1.7",
          "form": "닭볶음탕! 해드셨나요?",
          "original_form": "닭볶음탕! 해드셨나요?",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100038630.1.8",
          "form": "나가서 먹었어요! 구내식당 밥이 오늘 안좋아하는거여서 ㅎㅎ",
          "original_form": "나가서 먹었어요! 구내식당 밥이 오늘 안좋아하는거여서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100038630.1.9",
          "form": "컵라면 하나만 드셨어요?? 핫바같은거 같이 먹으면 배 든든하던데!",
          "original_form": "컵라면 하나만 드셨어요?? 핫바같은거 같이 먹으면 배 든든하던데!",
          "speaker_id": "1",
          "time": "20210902 18:02"
        },
        {
          "id": "MDRW2100038630.1.10",
          "form": "헉안좋아하시는거였구나 메뉴가 뭐였어요?",
          "original_form": "헉안좋아하시는거였구나 메뉴가 뭐였어요?",
          "speaker_id": "2",
          "time": "20210902 18:02"
        },
        {
          "id": "MDRW2100038630.1.11",
          "form": "네 하나만 호다닥먹고 끝냈어요",
          "original_form": "네 하나만 호다닥먹고 끝냈어요",
          "speaker_id": "2",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100038630.1.12",
          "form": "추어탕이었어요 ㅎㅎ",
          "original_form": "추어탕이었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100038630.1.13",
          "form": "저녁은 든든하게 드셔야죠!",
          "original_form": "저녁은 든든하게 드셔야죠!",
          "speaker_id": "1",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100038630.1.14",
          "form": "저녁은 고기구워 먹을려구요!",
          "original_form": "저녁은 고기구워 먹을려구요!",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100038630.1.15",
          "form": "오! 저는 파기름 내서 굽는거 좋아하는데 따로 굽는 방법이 있나요??",
          "original_form": "오! 저는 파기름 내서 굽는거 좋아하는데 따로 굽는 방법이 있나요??",
          "speaker_id": "1",
          "time": "20210902 19:18"
        },
        {
          "id": "MDRW2100038630.1.16",
          "form": "저는 마늘분이랑 맛소금뿌리구요  마늘편을 같이 구워요",
          "original_form": "저는 마늘분이랑 맛소금뿌리구요  마늘편을 같이 구워요",
          "speaker_id": "2",
          "time": "20210902 19:20"
        },
        {
          "id": "MDRW2100038630.1.17",
          "form": "마늘분을 생각을 못했네요",
          "original_form": "마늘분을 생각을 못했네요",
          "speaker_id": "1",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100038630.1.18",
          "form": "기름맛 안나고 맛있을 것 같아요!",
          "original_form": "기름맛 안나고 맛있을 것 같아요!",
          "speaker_id": "1",
          "time": "20210902 19:22"
        },
        {
          "id": "MDRW2100038630.1.19",
          "form": "마늘분을 넣으면 맛이 깔끔해져요",
          "original_form": "마늘분을 넣으면 맛이 깔끔해져요",
          "speaker_id": "2",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100038630.1.20",
          "form": "그리고 파채무침하고 양파절임을 꼭 같이해먹어요",
          "original_form": "그리고 파채무침하고 양파절임을 꼭 같이해먹어요",
          "speaker_id": "2",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100038630.1.21",
          "form": "웬만한 식당보다 더 잘 차려 드시는 것 같아요 ㅎㅎ",
          "original_form": "웬만한 식당보다 더 잘 차려 드시는 것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 19:33"
        }
      ]
