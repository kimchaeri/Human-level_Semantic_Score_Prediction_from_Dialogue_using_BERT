      "utterance": [
        {
          "id": "MDRW2100021923.1.1",
          "form": "다이어트 언제부터 시작하셨나요?",
          "original_form": "다이어트 언제부터 시작하셨나요?",
          "speaker_id": "1",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021923.1.2",
          "form": "아직 열흘차에요ㅎㅎㅎ",
          "original_form": "아직 열흘차에요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 11:34"
        },
        {
          "id": "MDRW2100021923.1.3",
          "form": "식이만 하시나요?",
          "original_form": "식이만 하시나요?",
          "speaker_id": "1",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100021923.1.4",
          "form": "홈트해요 먹을건 그냥 평소보다 줄이구요ㅜㅜ",
          "original_form": "홈트해요 먹을건 그냥 평소보다 줄이구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100021923.1.5",
          "form": "홈트 영상 참고해서 하시나요?",
          "original_form": "홈트 영상 참고해서 하시나요?",
          "speaker_id": "1",
          "time": "20210829 12:47"
        },
        {
          "id": "MDRW2100021923.1.6",
          "form": "그럴때도있구요 헬스장 다닐때 배웠던걸루 하기도해요ㅎㅎ",
          "original_form": "그럴때도있구요 헬스장 다닐때 배웠던걸루 하기도해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 12:57"
        },
        {
          "id": "MDRW2100021923.1.7",
          "form": "헬스장에서 피티 하셨나여?",
          "original_form": "헬스장에서 피티 하셨나여?",
          "speaker_id": "1",
          "time": "20210829 13:01"
        },
        {
          "id": "MDRW2100021923.1.8",
          "form": "피티 안해도 잘 알려주시더라구용",
          "original_form": "피티 안해도 잘 알려주시더라구용",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100021923.1.9",
          "form": "삼개월만에 10키로 빠졌었어요ㅋㅋ",
          "original_form": "삼개월만에 10키로 빠졌었어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:09"
        },
        {
          "id": "MDRW2100021923.1.10",
          "form": "좋은 곳으로 다니셨나봐요! 제가 다니는 곳은 피티 안 하면 2번 정도만 간단한 피티 도와주고 끝이거든요ㅠ",
          "original_form": "좋은 곳으로 다니셨나봐요! 제가 다니는 곳은 피티 안 하면 2번 정도만 간단한 피티 도와주고 끝이거든요ㅠ",
          "speaker_id": "1",
          "time": "20210829 13:28"
        },
        {
          "id": "MDRW2100021923.1.11",
          "form": "약간 한가한 동네 헬스장 다녀보세요ㅜㅜ잘알려줄수도..",
          "original_form": "약간 한가한 동네 헬스장 다녀보세요ㅜㅜ잘알려줄수도..",
          "speaker_id": "2",
          "time": "20210829 13:50"
        },
        {
          "id": "MDRW2100021923.1.12",
          "form": "참고할게요!! 어떤 동작 많이하셨나요?",
          "original_form": "참고할게요!! 어떤 동작 많이하셨나요?",
          "speaker_id": "1",
          "time": "20210829 14:52"
        },
        {
          "id": "MDRW2100021923.1.13",
          "form": "보통 많이들 하는거 해요 스쿼트 런지 이런아이들ㅎㅎ",
          "original_form": "보통 많이들 하는거 해요 스쿼트 런지 이런아이들ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 16:42"
        },
        {
          "id": "MDRW2100021923.1.14",
          "form": "스쿼트 너무 힘들어요...ㅠ",
          "original_form": "스쿼트 너무 힘들어요...ㅠ",
          "speaker_id": "1",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100021923.1.15",
          "form": "그쵸..하다보면 토할것같아요",
          "original_form": "그쵸..하다보면 토할것같아요",
          "speaker_id": "2",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100021923.1.16",
          "form": "백신 예약하셨나요?",
