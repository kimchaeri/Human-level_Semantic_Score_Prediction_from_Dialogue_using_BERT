      "utterance": [
        {
          "id": "MDRW2100035760.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210830 00:53"
        },
        {
          "id": "MDRW2100035760.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210830 00:53"
        },
        {
          "id": "MDRW2100035760.1.3",
          "form": "저녁 드셨나요 ㅎㅎ ?",
          "original_form": "저녁 드셨나요 ㅎㅎ ?",
          "speaker_id": "1",
          "time": "20210830 00:54"
        },
        {
          "id": "MDRW2100035760.1.4",
          "form": "그럼요ㅎㅎ 멸치 볶음이랑 생선구이 해서 먹었어요ㅎㅎ 님은요?",
          "original_form": "그럼요ㅎㅎ 멸치 볶음이랑 생선구이 해서 먹었어요ㅎㅎ 님은요?",
          "speaker_id": "2",
          "time": "20210830 00:56"
        },
        {
          "id": "MDRW2100035760.1.5",
          "form": "저는 치킨 먹었어요 ㅎㅎ",
          "original_form": "저는 치킨 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:57"
        },
        {
          "id": "MDRW2100035760.1.6",
          "form": "아 치킨 좋네요ㅎㅎ  맥주는요?ㅎㅎ",
          "original_form": "아 치킨 좋네요ㅎㅎ  맥주는요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:01"
        },
        {
          "id": "MDRW2100035760.1.7",
          "form": "맥주도 먹었습니다 ㅎㅎ ! 치맥 좋아하시나요?",
          "original_form": "맥주도 먹었습니다 ㅎㅎ ! 치맥 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210830 01:02"
        },
        {
          "id": "MDRW2100035760.1.8",
          "form": "치맥 좋아하지만 병원에서 금주령 선고 받는바람에 치킨만 가끔 먹어요ㅎㅎ",
          "original_form": "치맥 좋아하지만 병원에서 금주령 선고 받는바람에 치킨만 가끔 먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:09"
        },
        {
          "id": "MDRW2100035760.1.9",
          "form": "헐 왜 금주인가요 ㅜㅜ ?",
          "original_form": "헐 왜 금주인가요 ㅜㅜ ?",
          "speaker_id": "1",
          "time": "20210830 01:10"
        },
        {
          "id": "MDRW2100035760.1.10",
          "form": "몸이 안좋아서 먹는 약들이 많아요ㅎㅎ 그렇다보니 술은 못먹게 하더라구요ㅋㅋㅋ 그래두 몰래몰래 가끔 한잔 씩 해요ㅋㅋ",
          "original_form": "몸이 안좋아서 먹는 약들이 많아요ㅎㅎ 그렇다보니 술은 못먹게 하더라구요ㅋㅋㅋ 그래두 몰래몰래 가끔 한잔 씩 해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 01:16"
        },
        {
          "id": "MDRW2100035760.1.11",
          "form": "아 ㅠㅠ 슬프네요 ㅠㅠㅠ",
          "original_form": "아 ㅠㅠ 슬프네요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 01:18"
        },
        {
          "id": "MDRW2100035760.1.12",
          "form": "꼭 건강 회복하셔서 즐겁게 치맥합시다 ㅎㅎ !",
          "original_form": "꼭 건강 회복하셔서 즐겁게 치맥합시다 ㅎㅎ !",
          "speaker_id": "1",
          "time": "20210830 01:18"
        },
        {
          "id": "MDRW2100035760.1.13",
          "form": "그래야죠ㅎㅎㅎ 아 치맥 얘기 하니까 진짜 치맥 먹고싶네여ㅋㅋ",
          "original_form": "그래야죠ㅎㅎㅎ 아 치맥 얘기 하니까 진짜 치맥 먹고싶네여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 01:22"
        },
        {
          "id": "MDRW2100035760.1.14",
          "form": "무알콜맥주라도 괜찮지않나요??!",
          "original_form": "무알콜맥주라도 괜찮지않나요??!",
          "speaker_id": "1",
          "time": "20210830 01:25"
        },
        {
          "id": "MDRW2100035760.1.15",
          "form": "아 근데 또 무알콜 맥주는 찐 맥주의 그 시원함을 못따라가더라구요ㅋㅋㅋㅋ",
          "original_form": "아 근데 또 무알콜 맥주는 찐 맥주의 그 시원함을 못따라가더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100035760.1.16",
          "form": "이미 드셔보셨군요 ! 맞아요 ㅋㅋ ㅜㅜ",
          "original_form": "이미 드셔보셨군요 ! 맞아요 ㅋㅋ ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 01:33"
        },
        {
          "id": "MDRW2100035760.1.17",
          "form": "찐은 못따라가는 ㅠㅠ",
          "original_form": "찐은 못따라가는 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 01:33"
