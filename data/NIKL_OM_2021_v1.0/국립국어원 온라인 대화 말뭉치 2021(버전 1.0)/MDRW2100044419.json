      "utterance": [
        {
          "id": "MDRW2100044419.1.1",
          "form": "투탕카멘 점시회 언제 가지?",
          "original_form": "투탕카멘 점시회 언제 가지?",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044419.1.2",
          "form": "내년쯤 갈 수 있겠지...",
          "original_form": "내년쯤 갈 수 있겠지...",
          "speaker_id": "2",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044419.1.3",
          "form": "아 여름에 갔어야 했는데",
          "original_form": "아 여름에 갔어야 했는데",
          "speaker_id": "1",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044419.1.4",
          "form": "그러게... 기념품도 사고 싶었어",
          "original_form": "그러게... 기념품도 사고 싶었어",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044419.1.5",
          "form": "아 미술 전시회도 봄에 간 걸로 끝이네",
          "original_form": "아 미술 전시회도 봄에 간 걸로 끝이네",
          "speaker_id": "1",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044419.1.6",
          "form": "모네 전시회?! 고흐보단 부족했어",
          "original_form": "모네 전시회?! 고흐보단 부족했어",
          "speaker_id": "2",
          "time": "20210907 22:39"
        },
        {
          "id": "MDRW2100044419.1.7",
          "form": "맞아 코로나 때문에 그랬지",
          "original_form": "맞아 코로나 때문에 그랬지",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044419.1.8",
          "form": "그래서 무하 전시회도 못 갔지 ㅠㅠ",
          "original_form": "그래서 무하 전시회도 못 갔지 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044419.1.9",
          "form": "코로나가 정말 문제네 지방이라 힘든데",
          "original_form": "코로나가 정말 문제네 지방이라 힘든데",
          "speaker_id": "1",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044419.1.10",
          "form": "교통비가 만만치 않지",
          "original_form": "교통비가 만만치 않지",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044419.1.11",
          "form": "전시회 가까운 곳이라도 가자",
          "original_form": "전시회 가까운 곳이라도 가자",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044419.1.12",
          "form": "그럴까? 마음에 드는 것 있었으면 좋겠다",
          "original_form": "그럴까? 마음에 드는 것 있었으면 좋겠다",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044419.1.13",
          "form": "하나정도는 있겠지 ㅋㅋ 검색해 보자!",
          "original_form": "하나정도는 있겠지 ㅋㅋ 검색해 보자!",
