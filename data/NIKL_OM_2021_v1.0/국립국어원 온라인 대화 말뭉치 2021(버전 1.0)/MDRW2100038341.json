      "utterance": [
        {
          "id": "MDRW2100038341.1.1",
          "form": "이만큼 해낸게 정말 대단했어요",
          "original_form": "이만큼 해낸게 정말 대단했어요",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100038341.1.2",
          "form": "존경스러워요 우리 선수들",
          "original_form": "존경스러워요 우리 선수들",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038341.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038341.1.4",
          "form": "조만간 예능에 또 출연하신다던데",
          "original_form": "조만간 예능에 또 출연하신다던데",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038341.1.5",
          "form": "전부 챙겨볼려고요",
          "original_form": "전부 챙겨볼려고요",
          "speaker_id": "1",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038341.1.6",
          "form": "아 진짜 기댜돼요 그거!",
          "original_form": "아 진짜 기댜돼요 그거!",
          "speaker_id": "2",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038341.1.7",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038341.1.8",
          "form": "이번이 마지막인 국대선수들",
          "original_form": "이번이 마지막인 국대선수들",
          "speaker_id": "2",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100038341.1.9",
          "form": "많이 아쉽지만",
          "original_form": "많이 아쉽지만",
          "speaker_id": "2",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100038341.1.10",
          "form": "제가 뒤늦게 빠진게 잘못이라",
          "original_form": "제가 뒤늦게 빠진게 잘못이라",
          "speaker_id": "2",
          "time": "20210902 10:34"
        },
        {
          "id": "MDRW2100038341.1.11",
          "form": "예전 경기들 찾아보기 쉬워요",
          "original_form": "예전 경기들 찾아보기 쉬워요",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100038341.1.12",
          "form": "꼭 결과 모르시는 상태에서 보세요",
          "original_form": "꼭 결과 모르시는 상태에서 보세요",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100038341.1.13",
          "form": "그래볼까요?",
          "original_form": "그래볼까요?",
          "speaker_id": "2",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100038341.1.14",
          "form": "올림픽 끝난 이후로는 안보긴했거든요",
          "original_form": "올림픽 끝난 이후로는 안보긴했거든요",
          "speaker_id": "2",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100038341.1.15",
          "form": "10월에 국내에서 한다는 거 기대하고있어요",
          "original_form": "10월에 국내에서 한다는 거 기대하고있어요",
          "speaker_id": "2",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100038341.1.16",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100038341.1.17",
          "form": "꼭 볼려구요",
          "original_form": "꼭 볼려구요",
          "speaker_id": "1",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100038341.1.18",
          "form": "네 진짜 재밌을것 같아요",
          "original_form": "네 진짜 재밌을것 같아요",
          "speaker_id": "2",
          "time": "20210902 10:40"
        },
        {
          "id": "MDRW2100038341.1.19",
          "form": "어느 팀 응원하실거예요?",
          "original_form": "어느 팀 응원하실거예요?",
          "speaker_id": "2",
          "time": "20210902 10:40"
        },
        {
          "id": "MDRW2100038341.1.20",
          "form": "저는 오지영 선수 있는 팀 응원할거 같아요",
          "original_form": "저는 오지영 선수 있는 팀 응원할거 같아요",
          "speaker_id": "1",
          "time": "20210902 10:41"
        },
        {
          "id": "MDRW2100038341.1.21",
          "form": "오지영선수",
          "original_form": "오지영선수",
          "speaker_id": "2",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100038341.1.22",
          "form": "gs군요",
          "original_form": "gs군요",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100038341.1.23",
          "form": "전 현대건설 혹은 ibk응원할것 같아요",
          "original_form": "전 현대건설 혹은 ibk응원할것 같아요",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100038341.1.24",
          "form": "오지영선수와 경기하게되면 엄청 흥미진진할것같아요",
          "original_form": "오지영선수와 경기하게되면 엄청 흥미진진할것같아요",
          "speaker_id": "2",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100038341.1.25",
          "form": "수비가 되면 확실히 더 재밌어지더라구요",
          "original_form": "수비가 되면 확실히 더 재밌어지더라구요",
          "speaker_id": "2",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100038341.1.26",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100038341.1.27",
          "form": "막강한 공격수와 막강한 수비수...",
          "original_form": "막강한 공격수와 막강한 수비수...",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100038341.1.28",
          "form": "생각만 해도 떨리네요",
          "original_form": "생각만 해도 떨리네요",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100038341.1.29",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100038341.1.30",
          "form": "흥미진진",
          "original_form": "흥미진진",
          "speaker_id": "2",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100038341.1.31",
          "form": "배구에서 세터가 많이 중요하다는데",
          "original_form": "배구에서 세터가 많이 중요하다는데",
          "speaker_id": "2",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100038341.1.32",
          "form": "다른 세터들도 궁금해요",
          "original_form": "다른 세터들도 궁금해요",
          "speaker_id": "2",
          "time": "20210902 11:10"
        },
        {
          "id": "MDRW2100038341.1.33",
          "form": "요즘엔 세터가 어떨지 모르겠어요",
          "original_form": "요즘엔 세터가 어떨지 모르겠어요",
          "speaker_id": "1",
          "time": "20210902 11:13"
        },
        {
          "id": "MDRW2100038341.1.34",
          "form": "세터가 좋았던 시대는 간거 같아서요",
          "original_form": "세터가 좋았던 시대는 간거 같아서요",
          "speaker_id": "1",
          "time": "20210902 11:13"
        },
        {
          "id": "MDRW2100038341.1.35",
          "form": "아 그런가요?",
          "original_form": "아 그런가요?",
          "speaker_id": "2",
          "time": "20210902 11:30"
        },
        {
          "id": "MDRW2100038341.1.36",
          "form": "어떤 양상이 일어날지 궁금하네요!",
          "original_form": "어떤 양상이 일어날지 궁금하네요!",
          "speaker_id": "2",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100038341.1.37",
          "form": "잘하는 세터가 나타났으면 좋겠어요",
          "original_form": "잘하는 세터가 나타났으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100038341.1.38",
          "form": "수비에서도 김혜란 선수가 나온다고 해서 기대돼요",
          "original_form": "수비에서도 김혜란 선수가 나온다고 해서 기대돼요",
          "speaker_id": "2",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100038341.1.39",
          "form": "오호 기대되는군요",
          "original_form": "오호 기대되는군요",
          "speaker_id": "1",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100038341.1.40",
          "form": "네 김혜란 선수 되게 유명하시더라구요",
          "original_form": "네 김혜란 선수 되게 유명하시더라구요",
          "speaker_id": "2",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100038341.1.41",
          "form": "직접 뛰는 모습 보고싶어요",
          "original_form": "직접 뛰는 모습 보고싶어요",
          "speaker_id": "2",
          "time": "20210902 14:31"
        },
        {
          "id": "MDRW2100038341.1.42",
          "form": "선수들 뛰는 모습 보면 더 스펙타클해요",
          "original_form": "선수들 뛰는 모습 보면 더 스펙타클해요",
          "speaker_id": "1",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100038341.1.43",
          "form": "오 기대돼요",
          "original_form": "오 기대돼요",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100038341.1.44",
          "form": "지금은 코로나라 무관중이지만",
          "original_form": "지금은 코로나라 무관중이지만",
          "speaker_id": "2",
          "time": "20210902 14:34"
        },
        {
          "id": "MDRW2100038341.1.45",
          "form": "풀리면 직접 가보려구요!",
          "original_form": "풀리면 직접 가보려구요!",
