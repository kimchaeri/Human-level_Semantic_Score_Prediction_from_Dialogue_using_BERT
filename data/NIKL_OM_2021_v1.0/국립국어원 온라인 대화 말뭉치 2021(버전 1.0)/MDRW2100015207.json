      "utterance": [
        {
          "id": "MDRW2100015207.1.1",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015207.1.2",
          "form": "배스킨라빈스 가시면 뭐 주로드세요",
          "original_form": "배스킨라빈스 가시면 뭐 주로드세요",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015207.1.3",
          "form": "그린티요",
          "original_form": "그린티요",
          "speaker_id": "2",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015207.1.4",
          "form": "삽사름한 맛 좋아하시는그누나",
          "original_form": "삽사름한 맛 좋아하시는그누나",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015207.1.5",
          "form": "는구나",
          "original_form": "는구나",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015207.1.6",
          "form": "그럼 파인트에 3가지 맛을 담는다면?",
          "original_form": "그럼 파인트에 3가지 맛을 담는다면?",
          "speaker_id": "1",
          "time": "20210825 22:16"
        },
        {
          "id": "MDRW2100015207.1.7",
          "form": "딱히 그런건 아니에요",
          "original_form": "딱히 그런건 아니에요",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015207.1.8",
          "form": "앗..",
          "original_form": "앗..",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015207.1.9",
          "form": "사빠딸 그린티 바람과 함께 사라지다 뉴욕치즈케이크 레인보우 샤베트",
          "original_form": "사빠딸 그린티 바람과 함께 사라지다 뉴욕치즈케이크 레인보우 샤베트",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015207.1.10",
          "form": "돌아가면서 먹어요 ㅎ",
          "original_form": "돌아가면서 먹어요 ㅎ",
          "speaker_id": "2",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015207.1.11",
          "form": "레인보우 샤베트의 매력은?",
          "original_form": "레인보우 샤베트의 매력은?",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015207.1.12",
          "form": "좀 물릴때 다시 시작할수 있게 해줘서 좋아요",
          "original_form": "좀 물릴때 다시 시작할수 있게 해줘서 좋아요",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015207.1.13",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015207.1.14",
          "form": "공감합니다",
          "original_form": "공감합니다",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015207.1.15",
          "form": "저는 요거트 좋아해요",
          "original_form": "저는 요거트 좋아해요",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015207.1.16",
          "form": "레인보우 샤베트 드세요?",
          "original_form": "레인보우 샤베트 드세요?",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015207.1.17",
          "form": "선호하지는 않는데 있으면 먹어요",
          "original_form": "선호하지는 않는데 있으면 먹어요",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015207.1.18",
          "form": "그럼 난 민초다 반민초다",
          "original_form": "그럼 난 민초다 반민초다",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015207.1.19",
          "form": "있으면 먹어요 근데 제가 주문하진 않아요",
          "original_form": "있으면 먹어요 근데 제가 주문하진 않아요",
          "speaker_id": "2",
          "time": "20210825 22:19"
        }
      ]
