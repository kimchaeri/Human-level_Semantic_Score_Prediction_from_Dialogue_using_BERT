      "utterance": [
        {
          "id": "MDRW2100039978.1.1",
          "form": "전 이직하고 싶은  마음은 굴뚝같은데",
          "original_form": "전 이직하고 싶은  마음은 굴뚝같은데",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039978.1.2",
          "form": "이 업종이 어딜 가도 고생길이라ㅜ",
          "original_form": "이 업종이 어딜 가도 고생길이라ㅜ",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039978.1.3",
          "form": "정년은 보장이 되나요",
          "original_form": "정년은 보장이 되나요",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039978.1.4",
          "form": "언제까지 일을 하며 살게 될까 그것도 고민이 되더라고요",
          "original_form": "언제까지 일을 하며 살게 될까 그것도 고민이 되더라고요",
          "speaker_id": "2",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039978.1.5",
          "form": "정년.. 그렇게 철밥통은 아닙니다ㅎㅎ",
          "original_form": "정년.. 그렇게 철밥통은 아닙니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039978.1.6",
          "form": "능력은 엄청 따지면서",
          "original_form": "능력은 엄청 따지면서",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039978.1.7",
          "form": "결국 줄 잘 탄 사람들이 주로 승진하는 거 보면 자괴감 들어요",
          "original_form": "결국 줄 잘 탄 사람들이 주로 승진하는 거 보면 자괴감 들어요",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039978.1.8",
          "form": "앞으로 50까진 일을 할수 있을까",
          "original_form": "앞으로 50까진 일을 할수 있을까",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039978.1.9",
          "form": "아니면 해야하는 걸까",
          "original_form": "아니면 해야하는 걸까",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039978.1.10",
          "form": "맞아요 용비어천가",
          "original_form": "맞아요 용비어천가",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039978.1.11",
          "form": "그런사람들이 승진하죠",
          "original_form": "그런사람들이 승진하죠",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039978.1.12",
          "form": "어디가나 그렇군요ㅎㅎ",
          "original_form": "어디가나 그렇군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039978.1.13",
          "form": "비하인드 앱 들어가시나요",
          "original_form": "비하인드 앱 들어가시나요",
          "speaker_id": "1",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100039978.1.14",
          "form": "그렇죠 뭐 저렇게까지 하는데 승진해라 해 싶다가도 ㅋㅋ 어우",
          "original_form": "그렇죠 뭐 저렇게까지 하는데 승진해라 해 싶다가도 ㅋㅋ 어우",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100039978.1.15",
          "form": "아니요 처음 들어요",
          "original_form": "아니요 처음 들어요",
          "speaker_id": "2",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100039978.1.16",
          "form": "문제는 승진하면 정말 아랫사람이 너무 피곤해진다는거",
          "original_form": "문제는 승진하면 정말 아랫사람이 너무 피곤해진다는거",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100039978.1.17",
          "form": "앗 블라인드앱이요",
          "original_form": "앗 블라인드앱이요",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100039978.1.18",
          "form": "비하인드라닠ㅋ",
          "original_form": "비하인드라닠ㅋ",
          "speaker_id": "1",
          "time": "20210903 23:52"
        },
        {
          "id": "MDRW2100039978.1.19",
          "form": "비하인드 라고 하셔서 무슨 숨겨진 이야기 앱인줄알았어요",
          "original_form": "비하인드 라고 하셔서 무슨 숨겨진 이야기 앱인줄알았어요",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100039978.1.20",
          "form": "블라인드앱도안들어가요 ㅎㅎ",
          "original_form": "블라인드앱도안들어가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100039978.1.21",
          "form": "전 최근에 깔았는데",
          "original_form": "전 최근에 깔았는데",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100039978.1.22",
          "form": "한탄들 보면 자괴감만 들어서 지웠어요ㅎㅎ",
          "original_form": "한탄들 보면 자괴감만 들어서 지웠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100039978.1.23",
          "form": "동종업계 라운지  보면 더 회의감 커지고ㅎㅎ",
          "original_form": "동종업계 라운지  보면 더 회의감 커지고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100039978.1.24",
          "form": "이직할 맘이 아니라 이 직종을 뜨고ㅇ싶네요",
          "original_form": "이직할 맘이 아니라 이 직종을 뜨고ㅇ싶네요",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100039978.1.25",
          "form": "이번 생은 글렀지만ㅎ",
          "original_form": "이번 생은 글렀지만ㅎ",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039978.1.26",
          "form": "아 그렇구나...",
          "original_form": "아 그렇구나...",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039978.1.27",
          "form": "들어가볼생각도 하지않을래요",
          "original_form": "들어가볼생각도 하지않을래요",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039978.1.28",
          "form": "유리멘탈이라 ㅋㅋ",
          "original_form": "유리멘탈이라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100039978.1.29",
          "form": "훌륭한 생각입니다!",
          "original_form": "훌륭한 생각입니다!",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100039978.1.30",
          "form": "네 ㅎㅎㅎ",
          "original_form": "네 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100039978.1.31",
          "form": "다들 비슷한 삶을 살텐데",
          "original_form": "다들 비슷한 삶을 살텐데",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100039978.1.32",
          "form": "그래도 직장이라도 있음을 다행으로 여기며",
          "original_form": "그래도 직장이라도 있음을 다행으로 여기며",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100039978.1.33",
          "form": "지내려고 주문을 걸어야죠",
          "original_form": "지내려고 주문을 걸어야죠",
          "speaker_id": "2",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100039978.1.34",
          "form": "떡값 현찰로 나오고 구내식당 있는 회사인걸요!",
          "original_form": "떡값 현찰로 나오고 구내식당 있는 회사인걸요!",
          "speaker_id": "1",
