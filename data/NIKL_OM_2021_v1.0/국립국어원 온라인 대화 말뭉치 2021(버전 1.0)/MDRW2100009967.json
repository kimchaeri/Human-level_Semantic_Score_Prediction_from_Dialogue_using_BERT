      "utterance": [
        {
          "id": "MDRW2100009967.1.1",
          "form": "그 인형의집을 남자애가 선물로 줘요",
          "original_form": "그 인형의집을 남자애가 선물로 줘요",
          "speaker_id": "2",
          "time": "20210823 12:27"
        },
        {
          "id": "MDRW2100009967.1.2",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "1",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009967.1.3",
          "form": "그걸 옮길 수 있나요?",
          "original_form": "그걸 옮길 수 있나요?",
          "speaker_id": "1",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009967.1.4",
          "form": "그리고 사과도 했던거 같아요",
          "original_form": "그리고 사과도 했던거 같아요",
          "speaker_id": "2",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009967.1.5",
          "form": "아리에티 엄마 구해서 풀어준거도 그남자애였고",
          "original_form": "아리에티 엄마 구해서 풀어준거도 그남자애였고",
          "speaker_id": "2",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009967.1.6",
          "form": "그부분이 정확히 기억이안나네요",
          "original_form": "그부분이 정확히 기억이안나네요",
          "speaker_id": "2",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009967.1.7",
          "form": "어떻게준거지",
          "original_form": "어떻게준거지",
          "speaker_id": "2",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009967.1.8",
          "form": "저도 기억이안나네요 ㅋㅋㅋ",
          "original_form": "저도 기억이안나네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009967.1.9",
          "form": "쨌든 주기는 줬어요",
          "original_form": "쨌든 주기는 줬어요",
          "speaker_id": "2",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009967.1.10",
          "form": "다시봐야겠어요",
          "original_form": "다시봐야겠어요",
          "speaker_id": "1",
          "time": "20210823 12:28"
        },
        {
          "id": "MDRW2100009967.1.11",
          "form": "이랬는데 제 기억이 틀릴수도ㅋ",
          "original_form": "이랬는데 제 기억이 틀릴수도ㅋ",
          "speaker_id": "2",
          "time": "20210823 12:29"
        },
        {
          "id": "MDRW2100009967.1.12",
          "form": "뭐 ㅋㅋㅋ 이거 오래 전에 보신거 아니에요?",
          "original_form": "뭐 ㅋㅋㅋ 이거 오래 전에 보신거 아니에요?",
          "speaker_id": "1",
          "time": "20210823 12:29"
        },
        {
          "id": "MDRW2100009967.1.13",
          "form": "아리에티 진짜 흥행은 못했지만 좋은영화였어요 ㅋㅋ",
          "original_form": "아리에티 진짜 흥행은 못했지만 좋은영화였어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:29"
        },
        {
          "id": "MDRW2100009967.1.14",
          "form": "생각해보니 그러네요 ㅋㅋ",
          "original_form": "생각해보니 그러네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:29"
        },
        {
          "id": "MDRW2100009967.1.15",
          "form": "맞아요 저도 폼포코 너구리 대작전 좋아했는데",
          "original_form": "맞아요 저도 폼포코 너구리 대작전 좋아했는데",
          "speaker_id": "1",
          "time": "20210823 12:30"
        },
        {
          "id": "MDRW2100009967.1.16",
          "form": "흥행은 못한거같아요",
          "original_form": "흥행은 못한거같아요",
          "speaker_id": "1",
          "time": "20210823 12:30"
        },
        {
          "id": "MDRW2100009967.1.17",
          "form": "그거랑 추억은 방울방울",
          "original_form": "그거랑 추억은 방울방울",
          "speaker_id": "1",
          "time": "20210823 12:30"
        },
        {
          "id": "MDRW2100009967.1.18",
          "form": "지브리 그 특유의 감성 좋죠",
          "original_form": "지브리 그 특유의 감성 좋죠",
          "speaker_id": "2",
          "time": "20210823 12:34"
        },
        {
          "id": "MDRW2100009967.1.19",
          "form": "저는 다운받아놓은 제 인생에서 제일 많이본건 음 짱구같아요ㅋㅋ",
          "original_form": "저는 다운받아놓은 제 인생에서 제일 많이본건 음 짱구같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:34"
        },
        {
          "id": "MDRW2100009967.1.20",
          "form": "ㅋㅋㅋㅋㅋ아 짱구!",
          "original_form": "ㅋㅋㅋㅋㅋ아 짱구!",
          "speaker_id": "1",
          "time": "20210823 12:47"
        },
        {
          "id": "MDRW2100009967.1.21",
          "form": "짱구 지금 스무살 넘었다면서요?",
          "original_form": "짱구 지금 스무살 넘었다면서요?",
          "speaker_id": "1",
          "time": "20210823 12:47"
        },
        {
          "id": "MDRW2100009967.1.22",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210823 12:48"
        },
        {
          "id": "MDRW2100009967.1.23",
          "form": "어른들의 제국 극장판ㅋㅋ인생 애니같아요",
          "original_form": "어른들의 제국 극장판ㅋㅋ인생 애니같아요",
          "speaker_id": "2",
          "time": "20210823 12:48"
        },
        {
          "id": "MDRW2100009967.1.24",
          "form": "와 그거 칭찬 많이 들었는데",
          "original_form": "와 그거 칭찬 많이 들었는데",
          "speaker_id": "1",
          "time": "20210823 12:49"
        },
        {
          "id": "MDRW2100009967.1.25",
          "form": "어른들 완전 공감하며 볼 수 있다면서요",
          "original_form": "어른들 완전 공감하며 볼 수 있다면서요",
          "speaker_id": "1",
          "time": "20210823 12:49"
        },
        {
          "id": "MDRW2100009967.1.26",
          "form": "꼭 어른이 아니라도 뭉클해요 ㅋㅋ",
          "original_form": "꼭 어른이 아니라도 뭉클해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:52"
        }
      ]
    }
