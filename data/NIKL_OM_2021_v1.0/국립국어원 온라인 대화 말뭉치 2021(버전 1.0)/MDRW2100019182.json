      "utterance": [
        {
          "id": "MDRW2100019182.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210827 16:00"
        },
        {
          "id": "MDRW2100019182.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210827 16:05"
        },
        {
          "id": "MDRW2100019182.1.3",
          "form": "운동하시고 계신가요?",
          "original_form": "운동하시고 계신가요?",
          "speaker_id": "2",
          "time": "20210827 16:05"
        },
        {
          "id": "MDRW2100019182.1.4",
          "form": "지금은 운동은 안하는데 예전에 홈트 했었어요!",
          "original_form": "지금은 운동은 안하는데 예전에 홈트 했었어요!",
          "speaker_id": "1",
          "time": "20210827 16:07"
        },
        {
          "id": "MDRW2100019182.1.5",
          "form": "별님은요?",
          "original_form": "별님은요?",
          "speaker_id": "1",
          "time": "20210827 16:07"
        },
        {
          "id": "MDRW2100019182.1.6",
          "form": "저는 링피트로 조금 하고있어요",
          "original_form": "저는 링피트로 조금 하고있어요",
          "speaker_id": "2",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100019182.1.7",
          "form": "오 링피트 재밋을거같아요!",
          "original_form": "오 링피트 재밋을거같아요!",
          "speaker_id": "1",
          "time": "20210827 21:15"
        },
        {
          "id": "MDRW2100019182.1.8",
          "form": "운동이 다양해서 덜 지루해요",
          "original_form": "운동이 다양해서 덜 지루해요",
          "speaker_id": "2",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100019182.1.9",
          "form": "저도 유튜브에서 봤는데 해보고 싶더라구용",
          "original_form": "저도 유튜브에서 봤는데 해보고 싶더라구용",
          "speaker_id": "1",
          "time": "20210828 01:49"
        },
        {
          "id": "MDRW2100019182.1.10",
          "form": "헬스비라고 생각하고 한번 해보는거 추천드려요",
          "original_form": "헬스비라고 생각하고 한번 해보는거 추천드려요",
          "speaker_id": "2",
          "time": "20210828 09:42"
        },
        {
          "id": "MDRW2100019182.1.11",
          "form": "홈트가 아무래도 지겨우니까요...",
          "original_form": "홈트가 아무래도 지겨우니까요...",
          "speaker_id": "2",
          "time": "20210828 09:42"
        },
        {
          "id": "MDRW2100019182.1.12",
          "form": "오오 링피트가 게임같아서 더 재밋을거같아요",
          "original_form": "오오 링피트가 게임같아서 더 재밋을거같아요",
          "speaker_id": "1",
          "time": "20210828 22:31"
        },
        {
          "id": "MDRW2100019182.1.13",
          "form": "그죠 보스 쓰러트리는 재미 새로운 운동(스킬)배우는 재미가 있어요",
          "original_form": "그죠 보스 쓰러트리는 재미 새로운 운동(스킬)배우는 재미가 있어요",
          "speaker_id": "2",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100019182.1.14",
          "form": "집에 스위치도 잇어서 나중에 살만할거같아요 ㅋㅋ",
          "original_form": "집에 스위치도 잇어서 나중에 살만할거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:35"
        },
        {
          "id": "MDRW2100019182.1.15",
          "form": "헬스비라고 생각하면 할만한거 같아요!",
          "original_form": "헬스비라고 생각하면 할만한거 같아요!",
          "speaker_id": "2",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100019182.1.16",
          "form": "중고로는 사시지 마시구...",
          "original_form": "중고로는 사시지 마시구...",
          "speaker_id": "2",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100019182.1.17",
          "form": "링피트는 중고는 아니래요...",
          "original_form": "링피트는 중고는 아니래요...",
          "speaker_id": "2",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100019182.1.18",
          "form": "아 진짜요??",
          "original_form": "아 진짜요??",
          "speaker_id": "1",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100019182.1.19",
          "form": "달라요?",
          "original_form": "달라요?",
          "speaker_id": "1",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100019182.1.20",
          "form": "이게 운동이다보니까 땀냄새가 난대요",
          "original_form": "이게 운동이다보니까 땀냄새가 난대요",
          "speaker_id": "2",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100019182.1.21",
          "form": "들고하는 컨트롤러 링이 있거든요...",
          "original_form": "들고하는 컨트롤러 링이 있거든요...",
          "speaker_id": "2",
          "time": "20210828 22:37"
        },
        {
          "id": "MDRW2100019182.1.22",
          "form": "아아..",
          "original_form": "아아..",
          "speaker_id": "1",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100019182.1.23",
          "form": "좀 그러겟네요 진짜루..",
          "original_form": "좀 그러겟네요 진짜루..",
          "speaker_id": "1",
          "time": "20210828 22:40"
        }
      ]
    }
