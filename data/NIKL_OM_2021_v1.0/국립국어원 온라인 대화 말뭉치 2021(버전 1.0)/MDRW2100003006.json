      "utterance": [
        {
          "id": "MDRW2100003006.1.1",
          "form": "흠.. 오늘 4단계 발표했으니",
          "original_form": "흠.. 오늘 4단계 발표했으니",
          "speaker_id": "2",
          "time": "20210820 17:27"
        },
        {
          "id": "MDRW2100003006.1.2",
          "form": "2학기 내내 못가는 건 아닌지",
          "original_form": "2학기 내내 못가는 건 아닌지",
          "speaker_id": "1",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100003006.1.3",
          "form": "색시가 힘들겠네...",
          "original_form": "색시가 힘들겠네...",
          "speaker_id": "2",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100003006.1.4",
          "form": "힘들긴 해도 안전이 우선이니까 ㅠ",
          "original_form": "힘들긴 해도 안전이 우선이니까 ㅠ",
          "speaker_id": "1",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100003006.1.5",
          "form": "또 줌 지옥이 시작되겠네",
          "original_form": "또 줌 지옥이 시작되겠네",
          "speaker_id": "2",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100003006.1.6",
          "form": "글치.. 오전에는 청소기도 못돌려",
          "original_form": "글치.. 오전에는 청소기도 못돌려",
          "speaker_id": "1",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100003006.1.7",
          "form": "ㅎㅎ 각 방마다 수업 중이니",
          "original_form": "ㅎㅎ 각 방마다 수업 중이니",
          "speaker_id": "2",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100003006.1.8",
          "form": "난 있을 곳도 없어 ㅋㅋ",
          "original_form": "난 있을 곳도 없어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100003006.1.9",
          "form": "떠들지도 못하고 숨막히겠네 ㅎㅎ",
          "original_form": "떠들지도 못하고 숨막히겠네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100003006.1.10",
          "form": "ㅇㅇ 설거지랑 하는데도 엄청 신경쓰임 ㅋ",
          "original_form": "ㅇㅇ 설거지랑 하는데도 엄청 신경쓰임 ㅋ",
          "speaker_id": "1",
          "time": "20210820 17:29"
        },
        {
          "id": "MDRW2100003006.1.11",
          "form": "이렇게 계속 온라인 수업하고 나중에 애들이 적응할 수 있을런지",
          "original_form": "이렇게 계속 온라인 수업하고 나중에 애들이 적응할 수 있을런지",
          "speaker_id": "2",
          "time": "20210820 17:30"
        },
        {
          "id": "MDRW2100003006.1.12",
          "form": "진짜 걱정이야",
          "original_form": "진짜 걱정이야",
          "speaker_id": "1",
          "time": "20210820 17:30"
        },
        {
          "id": "MDRW2100003006.1.13",
          "form": "몇 단계부터 정상수업이지?",
          "original_form": "몇 단계부터 정상수업이지?",
          "speaker_id": "2",
          "time": "20210820 17:30"
        },
        {
          "id": "MDRW2100003006.1.14",
          "form": "여긴 3단계부터",
          "original_form": "여긴 3단계부터",
          "speaker_id": "1",
          "time": "20210820 17:30"
        },
        {
          "id": "MDRW2100003006.1.15",
          "form": "요즘 확진자가 넘 많아서 흠..",
          "original_form": "요즘 확진자가 넘 많아서 흠..",
