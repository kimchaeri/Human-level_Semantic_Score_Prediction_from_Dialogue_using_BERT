      "utterance": [
        {
          "id": "MDRW2100027240.1.1",
          "form": "아니아니",
          "original_form": "아니아니",
          "speaker_id": "2",
          "time": "20210902 10:43"
        },
        {
          "id": "MDRW2100027240.1.2",
          "form": "스마트도서관은 음",
          "original_form": "스마트도서관은 음",
          "speaker_id": "1",
          "time": "20210902 10:47"
        },
        {
          "id": "MDRW2100027240.1.3",
          "form": "책자판기처럼생김",
          "original_form": "책자판기처럼생김",
          "speaker_id": "1",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100027240.1.4",
          "form": "키오스크화면으로보고",
          "original_form": "키오스크화면으로보고",
          "speaker_id": "1",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100027240.1.5",
          "form": "도서관증 바코드 찍으면",
          "original_form": "도서관증 바코드 찍으면",
          "speaker_id": "1",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100027240.1.6",
          "form": "책이 자판기에서 커피나오듯나옴!",
          "original_form": "책이 자판기에서 커피나오듯나옴!",
          "speaker_id": "1",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100027240.1.7",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027240.1.8",
          "form": "근데 안에 책이 다양한가유?",
          "original_form": "근데 안에 책이 다양한가유?",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027240.1.9",
          "form": "이북 데이터 대출도아닌것이 다양한게 있으려면 커야하잖아",
          "original_form": "이북 데이터 대출도아닌것이 다양한게 있으려면 커야하잖아",
          "speaker_id": "2",
          "time": "20210902 10:50"
        },
        {
          "id": "MDRW2100027240.1.10",
          "form": "엄청 많지는않음 그래도",
          "original_form": "엄청 많지는않음 그래도",
          "speaker_id": "1",
          "time": "20210902 10:52"
        },
        {
          "id": "MDRW2100027240.1.11",
          "form": "새로들어온 책이나",
          "original_form": "새로들어온 책이나",
          "speaker_id": "1",
          "time": "20210902 10:52"
        },
        {
          "id": "MDRW2100027240.1.12",
          "form": "유명한책위주로",
          "original_form": "유명한책위주로",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027240.1.13",
          "form": "나름 다양하게들어있음",
          "original_form": "나름 다양하게들어있음",
          "speaker_id": "1",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027240.1.14",
          "form": "사람들은 베스트셀러를 원하니까",
          "original_form": "사람들은 베스트셀러를 원하니까",
          "speaker_id": "2",
          "time": "20210902 10:53"
        },
        {
          "id": "MDRW2100027240.1.15",
          "form": "편리하네",
          "original_form": "편리하네",
          "speaker_id": "2",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100027240.1.16",
          "form": "사서도 일덜하고",
          "original_form": "사서도 일덜하고",
          "speaker_id": "2",
          "time": "20210902 10:54"
        },
        {
          "id": "MDRW2100027240.1.17",
          "form": "ㅇㅇ게다가 여기도서관 지금이벤트해서",
          "original_form": "ㅇㅇ게다가 여기도서관 지금이벤트해서",
          "speaker_id": "1",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027240.1.18",
          "form": "스마트 도서관 사용하면 유리컵주는 이벤트함",
          "original_form": "스마트 도서관 사용하면 유리컵주는 이벤트함",
          "speaker_id": "1",
          "time": "20210902 10:58"
        },
        {
          "id": "MDRW2100027240.1.19",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "2",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100027240.1.20",
          "form": "거 어데고 나도좀 쓰자",
          "original_form": "거 어데고 나도좀 쓰자",
          "speaker_id": "2",
          "time": "20210902 11:00"
        },
        {
          "id": "MDRW2100027240.1.21",
          "form": "유리컵 사진보여주겠음 ㅋㅋ",
          "original_form": "유리컵 사진보여주겠음 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:03"
        },
        {
          "id": "MDRW2100027240.1.22",
          "form": "엄마한테말하니까",
          "original_form": "엄마한테말하니까",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027240.1.23",
          "form": "컵은 2개있어야한다며 엄마도 책빌리러감ㅋㅣㅣ",
          "original_form": "컵은 2개있어야한다며 엄마도 책빌리러감ㅋㅣㅣ",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027240.1.24",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:04"
        },
        {
          "id": "MDRW2100027240.1.25",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027240.1.26",
          "form": "차가운거담아도 그 물자국안생김",
          "original_form": "차가운거담아도 그 물자국안생김",
          "speaker_id": "1",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027240.1.27",
          "form": "니 동생명의로 또 빌려바",
          "original_form": "니 동생명의로 또 빌려바",
          "speaker_id": "2",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027240.1.28",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027240.1.29",
          "form": "넘 귀엽다",
          "original_form": "넘 귀엽다",
          "speaker_id": "2",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027240.1.30",
          "form": "우린 저런컵은 없었던지라 맨날카페에서만보고",
          "original_form": "우린 저런컵은 없었던지라 맨날카페에서만보고",
          "speaker_id": "1",
          "time": "20210902 11:06"
        },
        {
          "id": "MDRW2100027240.1.31",
          "form": "오좋다 이러고있음 ㅋㅋ",
          "original_form": "오좋다 이러고있음 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:07"
        },
        {
          "id": "MDRW2100027240.1.32",
          "form": "언니 책이음카드있어요?",
          "original_form": "언니 책이음카드있어요?",
          "speaker_id": "1",
          "time": "20210902 11:07"
        },
        {
          "id": "MDRW2100027240.1.33",
          "form": "울동네는 왜저런거안함ㅜㅜ",
          "original_form": "울동네는 왜저런거안함ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 11:07"
        },
        {
          "id": "MDRW2100027240.1.34",
          "form": "아뉘",
          "original_form": "아뉘",
          "speaker_id": "2",
          "time": "20210902 11:07"
        },
        {
          "id": "MDRW2100027240.1.35",
          "form": "일단 책이음카드를 만들어야함",
          "original_form": "일단 책이음카드를 만들어야함",
          "speaker_id": "1",
          "time": "20210902 11:08"
        },
        {
          "id": "MDRW2100027240.1.36",
          "form": "그럼 전국도서관에서 다 호환가능함",
          "original_form": "그럼 전국도서관에서 다 호환가능함",
          "speaker_id": "1",
          "time": "20210902 11:08"
        },
        {
          "id": "MDRW2100027240.1.37",
          "form": "울동네도 컵주능가",
          "original_form": "울동네도 컵주능가",
          "speaker_id": "2",
          "time": "20210902 11:09"
        },
        {
          "id": "MDRW2100027240.1.38",
          "form": "알아봐야지",
          "original_form": "알아봐야지",
          "speaker_id": "2",
          "time": "20210902 11:09"
        },
        {
          "id": "MDRW2100027240.1.39",
          "form": "ㅇㅇ내동생은 본인 책이음카드 본인이부산에서 책빌릴때 쓰고있어서 ㅎㅎ",
          "original_form": "ㅇㅇ내동생은 본인 책이음카드 본인이부산에서 책빌릴때 쓰고있어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027240.1.40",
          "form": "여하간 득템했고",
          "original_form": "여하간 득템했고",
          "speaker_id": "1",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027240.1.41",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027240.1.42",
          "form": "책도 재밌음",
          "original_form": "책도 재밌음",
          "speaker_id": "1",
          "time": "20210902 11:15"
        },
        {
          "id": "MDRW2100027240.1.43",
          "form": "꿀정보 감사해용",
          "original_form": "꿀정보 감사해용",
          "speaker_id": "2",
          "time": "20210902 11:16"
        }
      ]
    }
