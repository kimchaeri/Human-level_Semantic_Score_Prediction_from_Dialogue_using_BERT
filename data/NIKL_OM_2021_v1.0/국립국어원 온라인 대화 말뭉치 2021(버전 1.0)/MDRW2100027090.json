      "utterance": [
        {
          "id": "MDRW2100027090.1.1",
          "form": "음식 레시피를 많이 봅니다",
          "original_form": "음식 레시피를 많이 봅니다",
          "speaker_id": "2",
          "time": "20210902 09:22"
        },
        {
          "id": "MDRW2100027090.1.2",
          "form": "곱창 맛있지요",
          "original_form": "곱창 맛있지요",
          "speaker_id": "1",
          "time": "20210902 09:22"
        },
        {
          "id": "MDRW2100027090.1.3",
          "form": "곱창 먹고싶네요 크",
          "original_form": "곱창 먹고싶네요 크",
          "speaker_id": "2",
          "time": "20210902 09:22"
        },
        {
          "id": "MDRW2100027090.1.4",
          "form": "주로 식당에서 드시겠네요",
          "original_form": "주로 식당에서 드시겠네요",
          "speaker_id": "1",
          "time": "20210902 09:22"
        },
        {
          "id": "MDRW2100027090.1.5",
          "form": "소주랑 같이먹음 좋은데요",
          "original_form": "소주랑 같이먹음 좋은데요",
          "speaker_id": "2",
          "time": "20210902 09:22"
        },
        {
          "id": "MDRW2100027090.1.6",
          "form": "양념구이 소금구이중 어느걸 선호하세요",
          "original_form": "양념구이 소금구이중 어느걸 선호하세요",
          "speaker_id": "1",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100027090.1.7",
          "form": "최근에 데워먹는걸로 먹었는데",
          "original_form": "최근에 데워먹는걸로 먹었는데",
          "speaker_id": "2",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100027090.1.8",
          "form": "맛이좋더군요",
          "original_form": "맛이좋더군요",
          "speaker_id": "2",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100027090.1.9",
          "form": "전 둘다 좋아합니나",
          "original_form": "전 둘다 좋아합니나",
          "speaker_id": "2",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100027090.1.10",
          "form": "다",
          "original_form": "다",
          "speaker_id": "2",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100027090.1.11",
          "form": "저는 좀전에 장어조림 먹었습니다",
          "original_form": "저는 좀전에 장어조림 먹었습니다",
          "speaker_id": "1",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100027090.1.12",
          "form": "없어서못먹죠",
          "original_form": "없어서못먹죠",
          "speaker_id": "2",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100027090.1.13",
          "form": "장어조림 맛있죠",
          "original_form": "장어조림 맛있죠",
          "speaker_id": "2",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100027090.1.14",
          "form": "곱창도 전골이 맛있지요",
          "original_form": "곱창도 전골이 맛있지요",
          "speaker_id": "1",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100027090.1.15",
          "form": "맞아요 볶음밥까지 먹음",
          "original_form": "맞아요 볶음밥까지 먹음",
          "speaker_id": "2",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100027090.1.16",
          "form": "국물에 밥을 비벼먹어요",
          "original_form": "국물에 밥을 비벼먹어요",
          "speaker_id": "1",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100027090.1.17",
          "form": "완벽하지요",
          "original_form": "완벽하지요",
          "speaker_id": "2",
          "time": "20210902 09:23"
        },
        {
          "id": "MDRW2100027090.1.18",
          "form": "밀가루도 좋아하나요",
          "original_form": "밀가루도 좋아하나요",
          "speaker_id": "1",
          "time": "20210902 09:24"
        }
