      "utterance": [
        {
          "id": "MDRW2100005032.1.1",
          "form": "오늘 비 엄청왔죠",
          "original_form": "오늘 비 엄청왔죠",
          "speaker_id": "2",
          "time": "20210821 20:04"
        },
        {
          "id": "MDRW2100005032.1.2",
          "form": "저 그비를 뚫고 서울갔다 오느라 힘들었네요",
          "original_form": "저 그비를 뚫고 서울갔다 오느라 힘들었네요",
          "speaker_id": "2",
          "time": "20210821 20:04"
        },
        {
          "id": "MDRW2100005032.1.3",
          "form": "고생하셨겠네요",
          "original_form": "고생하셨겠네요",
          "speaker_id": "1",
          "time": "20210821 20:05"
        },
        {
          "id": "MDRW2100005032.1.4",
          "form": "전 어제 베란다 문열고 자다가 아침에 비가..ㅠ",
          "original_form": "전 어제 베란다 문열고 자다가 아침에 비가..ㅠ",
          "speaker_id": "1",
          "time": "20210821 20:05"
        },
        {
          "id": "MDRW2100005032.1.5",
          "form": "네 돌아오는데 하늘이 맑아서 참 그랬네요",
          "original_form": "네 돌아오는데 하늘이 맑아서 참 그랬네요",
          "speaker_id": "2",
          "time": "20210821 20:05"
        },
        {
          "id": "MDRW2100005032.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 20:05"
        },
        {
          "id": "MDRW2100005032.1.7",
          "form": "갑자기 쏟아져서",
          "original_form": "갑자기 쏟아져서",
          "speaker_id": "2",
          "time": "20210821 20:06"
        },
        {
          "id": "MDRW2100005032.1.8",
          "form": "오후에 그쳐서 쫌 늦게 가면 안맞았을텐데 괜히 고생했어요",
          "original_form": "오후에 그쳐서 쫌 늦게 가면 안맞았을텐데 괜히 고생했어요",
          "speaker_id": "2",
          "time": "20210821 20:06"
        },
        {
          "id": "MDRW2100005032.1.9",
          "form": "무슨계절 좋으세요?",
          "original_form": "무슨계절 좋으세요?",
          "speaker_id": "2",
          "time": "20210821 20:06"
        },
        {
          "id": "MDRW2100005032.1.10",
          "form": "전 봄은 꽃가루 알러지 때문에 힘들고 가을이 좋아요 ㅎㅎ",
          "original_form": "전 봄은 꽃가루 알러지 때문에 힘들고 가을이 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:11"
        },
        {
          "id": "MDRW2100005032.1.11",
          "form": "여름은 너무 덥고 습해서 싫고 겨울은 추워서 싫고",
          "original_form": "여름은 너무 덥고 습해서 싫고 겨울은 추워서 싫고",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005032.1.12",
          "form": "저도 봄 가을 좋아해요",
          "original_form": "저도 봄 가을 좋아해요",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005032.1.13",
          "form": "딱 적당한 날씨라 좋아요",
          "original_form": "딱 적당한 날씨라 좋아요",
          "speaker_id": "2",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005032.1.14",
          "form": "미세먼지 때문에 없는 계절인 여름도 그런면에선 좋아요",
          "original_form": "미세먼지 때문에 없는 계절인 여름도 그런면에선 좋아요",
          "speaker_id": "2",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005032.1.15",
          "form": "아 그건 좋네요",
          "original_form": "아 그건 좋네요",
          "speaker_id": "1",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100005032.1.16",
          "form": "맑은 날씨 그립네요",
          "original_form": "맑은 날씨 그립네요",
          "speaker_id": "1",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005032.1.17",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005032.1.18",
          "form": "겨울엔 난방비가",
          "original_form": "겨울엔 난방비가",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005032.1.19",
          "form": "작년에 대박이였어요",
          "original_form": "작년에 대박이였어요",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005032.1.20",
          "form": "작년 겨울 너무 추웠죠",
          "original_form": "작년 겨울 너무 추웠죠",
          "speaker_id": "1",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005032.1.21",
          "form": "네 올해 겁나요 ㅜㅜ",
          "original_form": "네 올해 겁나요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005032.1.22",
          "form": "일기예보 보면 잘 안맞아서ㅎ 더 정확했음 좋겠어요",
          "original_form": "일기예보 보면 잘 안맞아서ㅎ 더 정확했음 좋겠어요",
          "speaker_id": "1",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005032.1.23",
          "form": "맞아요 넘 틀려요 ㅡㅡ",
          "original_form": "맞아요 넘 틀려요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005032.1.24",
          "form": "기상청 반성해야 해요",
          "original_form": "기상청 반성해야 해요",
          "speaker_id": "2",
          "time": "20210821 20:41"
        },
        {
          "id": "MDRW2100005032.1.25",
          "form": "겨울에 준비좀 단단히 해야겠어요 딸 패딩도 사고요",
          "original_form": "겨울에 준비좀 단단히 해야겠어요 딸 패딩도 사고요",
          "speaker_id": "2",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005032.1.26",
          "form": "옷이 계절을 말해주죠 ㅎㅎ",
          "original_form": "옷이 계절을 말해주죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:42"
        },
        {
          "id": "MDRW2100005032.1.27",
          "form": "맞아요 따뜻한 겨울을 보낼 준비 해야겠어요",
          "original_form": "맞아요 따뜻한 겨울을 보낼 준비 해야겠어요",
          "speaker_id": "1",
          "time": "20210821 20:43"
        },
        {
          "id": "MDRW2100005032.1.28",
          "form": "네 그럼요 ㅎㅎ",
          "original_form": "네 그럼요 ㅎㅎ",
          "speaker_id": "2",
