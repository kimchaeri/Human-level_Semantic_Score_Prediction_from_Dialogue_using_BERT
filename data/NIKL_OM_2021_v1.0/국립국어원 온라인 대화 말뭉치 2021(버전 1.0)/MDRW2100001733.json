      "utterance": [
        {
          "id": "MDRW2100001733.1.1",
          "form": "전시같은거 많이 보러 가여?",
          "original_form": "전시같은거 많이 보러 가여?",
          "speaker_id": "1",
          "time": "20210819 18:25"
        },
        {
          "id": "MDRW2100001733.1.2",
          "form": "전 지방이라 그런게 잘 없더라고요",
          "original_form": "전 지방이라 그런게 잘 없더라고요",
          "speaker_id": "2",
          "time": "20210819 18:25"
        },
        {
          "id": "MDRW2100001733.1.3",
          "form": "아하 그렇구나 최근에 영화는 뭐 봤어요?",
          "original_form": "아하 그렇구나 최근에 영화는 뭐 봤어요?",
          "speaker_id": "1",
          "time": "20210819 18:33"
        },
        {
          "id": "MDRW2100001733.1.4",
          "form": "3년전에 어벤져스본게 마지막이었어요",
          "original_form": "3년전에 어벤져스본게 마지막이었어요",
          "speaker_id": "2",
          "time": "20210819 18:38"
        },
        {
          "id": "MDRW2100001733.1.5",
          "form": "어벤져스 재밌게 봤었는데",
          "original_form": "어벤져스 재밌게 봤었는데",
          "speaker_id": "1",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100001733.1.6",
          "form": "뭐 재밌는 영화 없어요!??",
          "original_form": "뭐 재밌는 영화 없어요!??",
          "speaker_id": "1",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100001733.1.7",
          "form": "곧 11월에 디즈니플러스가 들어온다던데 거기 볼게 많다더라고요",
          "original_form": "곧 11월에 디즈니플러스가 들어온다던데 거기 볼게 많다더라고요",
          "speaker_id": "2",
          "time": "20210819 18:45"
        },
        {
          "id": "MDRW2100001733.1.8",
          "form": "디즈니플러스?그런게 있어요?",
          "original_form": "디즈니플러스?그런게 있어요?",
          "speaker_id": "1",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100001733.1.9",
          "form": "네 11월에 한국 온데요",
          "original_form": "네 11월에 한국 온데요",
          "speaker_id": "2",
          "time": "20210819 18:59"
        },
        {
          "id": "MDRW2100001733.1.10",
          "form": "오 나오면 관심있게 좀 찾아봐야겠네요",
          "original_form": "오 나오면 관심있게 좀 찾아봐야겠네요",
          "speaker_id": "1",
          "time": "20210819 19:17"
        },
        {
          "id": "MDRW2100001733.1.11",
          "form": "나중에 개봉하는 시리즈랑 연관성도 있다더라고요",
          "original_form": "나중에 개봉하는 시리즈랑 연관성도 있다더라고요",
          "speaker_id": "2",
          "time": "20210819 19:19"
        },
        {
          "id": "MDRW2100001733.1.12",
          "form": "아하 크게 하나보네용 꼭 찾아봐야겠어요ㅋㅋ",
          "original_form": "아하 크게 하나보네용 꼭 찾아봐야겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100001733.1.13",
          "form": "네 그거 봐야 후속작이 이해된데요",
          "original_form": "네 그거 봐야 후속작이 이해된데요",
          "speaker_id": "2",
          "time": "20210819 20:46"
        },
        {
          "id": "MDRW2100001733.1.14",
          "form": "떡밥 완전 많이 나오겠네요!",
          "original_form": "떡밥 완전 많이 나오겠네요!",
