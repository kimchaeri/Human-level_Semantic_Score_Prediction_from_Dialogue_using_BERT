      "utterance": [
        {
          "id": "MDRW2100040859.1.1",
          "form": "name2님 아침 드셨나요?!",
          "original_form": "&name2&님 아침 드셨나요?!",
          "speaker_id": "1",
          "time": "20210905 10:54"
        },
        {
          "id": "MDRW2100040859.1.2",
          "form": "이제 일어나려고 꾸물대고 있어용ㅋㅋㅋㅋ",
          "original_form": "이제 일어나려고 꾸물대고 있어용ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100040859.1.3",
          "form": "name1님은용?",
          "original_form": "&name1&님은용?",
          "speaker_id": "2",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100040859.1.4",
          "form": "저는 대충",
          "original_form": "저는 대충",
          "speaker_id": "1",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100040859.1.5",
          "form": "과일 갈아마시고 성당에 왔답니다,,",
          "original_form": "과일 갈아마시고 성당에 왔답니다,,",
          "speaker_id": "1",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100040859.1.6",
          "form": "바나나 갈아마시면 너무 맛있어요~~!",
          "original_form": "바나나 갈아마시면 너무 맛있어요~~!",
          "speaker_id": "1",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100040859.1.7",
          "form": "부지런하시네요!! 바나나랑 우유 넣으신거죠?",
          "original_form": "부지런하시네요!! 바나나랑 우유 넣으신거죠?",
          "speaker_id": "2",
          "time": "20210905 11:15"
        },
        {
          "id": "MDRW2100040859.1.8",
          "form": "우유는 안넣고,,,",
          "original_form": "우유는 안넣고,,,",
          "speaker_id": "1",
          "time": "20210905 11:23"
        },
        {
          "id": "MDRW2100040859.1.9",
          "form": "사실 저희 아빠가 만들어주신건뎈ㅋㅋㅋㅋㅋ",
          "original_form": "사실 저희 아빠가 만들어주신건뎈ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:23"
        },
        {
          "id": "MDRW2100040859.1.10",
          "form": "수박... 체리..... 바나나... 등등,,,",
          "original_form": "수박... 체리..... 바나나... 등등,,,",
          "speaker_id": "1",
          "time": "20210905 11:23"
        },
        {
          "id": "MDRW2100040859.1.11",
          "form": "다 때려넣은 거랍니다~!",
          "original_form": "다 때려넣은 거랍니다~!",
          "speaker_id": "1",
          "time": "20210905 11:23"
        },
        {
          "id": "MDRW2100040859.1.12",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ 맛있으면 된거죠!! 아버지가 스윗하시네용",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋ 맛있으면 된거죠!! 아버지가 스윗하시네용",
          "speaker_id": "2",
          "time": "20210905 11:31"
        },
        {
          "id": "MDRW2100040859.1.13",
          "form": "각각 재료들의 맛이 느껴지나요?",
          "original_form": "각각 재료들의 맛이 느껴지나요?",
          "speaker_id": "2",
          "time": "20210905 11:31"
        },
        {
          "id": "MDRW2100040859.1.14",
          "form": "음음 저는 바나나만 있으면",
          "original_form": "음음 저는 바나나만 있으면",
          "speaker_id": "1",
          "time": "20210905 11:42"
        },
        {
          "id": "MDRW2100040859.1.15",
          "form": "다 맛나요,,",
          "original_form": "다 맛나요,,",
          "speaker_id": "1",
          "time": "20210905 11:42"
        },
        {
          "id": "MDRW2100040859.1.16",
          "form": "근데 저희 아빠는... 삶은 달걀도 넣어요,,,",
          "original_form": "근데 저희 아빠는... 삶은 달걀도 넣어요,,,",
          "speaker_id": "1",
          "time": "20210905 11:43"
        },
        {
          "id": "MDRW2100040859.1.17",
          "form": "아하 ㅋㅋㅋㅋ그래도 다 몸에 좋은거 듬뿍 먹이고 싶은 마음에 그러시는거겠죠 ㅎㅎㅎ",
          "original_form": "아하 ㅋㅋㅋㅋ그래도 다 몸에 좋은거 듬뿍 먹이고 싶은 마음에 그러시는거겠죠 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 12:39"
        },
        {
          "id": "MDRW2100040859.1.18",
          "form": "바나나 엄청 좋아하시나봐요!",
          "original_form": "바나나 엄청 좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20210905 12:39"
        },
        {
          "id": "MDRW2100040859.1.19",
          "form": "헤헤 맞아요!!",
          "original_form": "헤헤 맞아요!!",
          "speaker_id": "1",
          "time": "20210905 12:41"
        },
        {
          "id": "MDRW2100040859.1.20",
          "form": "그리고 달걀 넣어 먹으면",
          "original_form": "그리고 달걀 넣어 먹으면",
          "speaker_id": "1",
          "time": "20210905 12:41"
        },
        {
          "id": "MDRW2100040859.1.21",
          "form": "그렇게 이상하지도 않더라구요?!",
          "original_form": "그렇게 이상하지도 않더라구요?!",
          "speaker_id": "1",
          "time": "20210905 12:41"
        },
        {
          "id": "MDRW2100040859.1.22",
          "form": "오 그래요? 저도 다음에 넣어볼까봐요",
          "original_form": "오 그래요? 저도 다음에 넣어볼까봐요",
          "speaker_id": "2",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100040859.1.23",
          "form": "목 막히지 않아요?",
          "original_form": "목 막히지 않아요?",
          "speaker_id": "2",
          "time": "20210905 12:49"
        },
        {
          "id": "MDRW2100040859.1.24",
          "form": "네네 근데 믹서기가 갈아줘서",
          "original_form": "네네 근데 믹서기가 갈아줘서",
          "speaker_id": "1",
          "time": "20210905 13:07"
        },
        {
          "id": "MDRW2100040859.1.25",
          "form": "그냥 좀 식감이",
          "original_form": "그냥 좀 식감이",
          "speaker_id": "1",
          "time": "20210905 13:07"
        },
        {
          "id": "MDRW2100040859.1.26",
          "form": "단백질이 느껴지는 것 외에는",
          "original_form": "단백질이 느껴지는 것 외에는",
          "speaker_id": "1",
          "time": "20210905 13:07"
        },
        {
          "id": "MDRW2100040859.1.27",
          "form": "아무렇지도 않아요!",
          "original_form": "아무렇지도 않아요!",
          "speaker_id": "1",
          "time": "20210905 13:08"
        },
        {
          "id": "MDRW2100040859.1.28",
          "form": "믹서기가 엄청 좋은게 필요할것 같아요",
          "original_form": "믹서기가 엄청 좋은게 필요할것 같아요",
          "speaker_id": "2",
          "time": "20210905 13:10"
        },
        {
          "id": "MDRW2100040859.1.29",
          "form": "뭐쓰세요?",
          "original_form": "뭐쓰세요?",
          "speaker_id": "2",
          "time": "20210905 13:10"
        },
        {
          "id": "MDRW2100040859.1.30",
          "form": "믹사기 바꾸고 싶어요 ㅠ",
          "original_form": "믹사기 바꾸고 싶어요 ㅠ",
          "speaker_id": "2",
          "time": "20210905 13:10"
        },
        {
          "id": "MDRW2100040859.1.31",
          "form": "아 놀라실거예요 저희집에서 쓰는 믹서기",
          "original_form": "아 놀라실거예요 저희집에서 쓰는 믹서기",
          "speaker_id": "1",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100040859.1.32",
          "form": "진짜 오래된 거예요",
          "original_form": "진짜 오래된 거예요",
          "speaker_id": "1",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100040859.1.33",
          "form": "완전 투박한...",
          "original_form": "완전 투박한...",
          "speaker_id": "1",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100040859.1.34",
          "form": "거의.... 10년 넘었을걸요?!",
          "original_form": "거의.... 10년 넘었을걸요?!",
          "speaker_id": "1",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100040859.1.35",
          "form": "근데 그렇게 잘 갈려요???",
          "original_form": "근데 그렇게 잘 갈려요???",
          "speaker_id": "2",
          "time": "20210905 13:12"
        },
        {
          "id": "MDRW2100040859.1.36",
          "form": "저희가 쓰는 믹서기",
          "original_form": "저희가 쓰는 믹서기",
          "speaker_id": "1",
          "time": "20210905 13:13"
        },
        {
          "id": "MDRW2100040859.1.37",
          "form": "완전 오래된 거라",
