      "utterance": [
        {
          "id": "MDRW2100035960.1.1",
          "form": "안녕하세요! 식사하셨나요?",
          "original_form": "안녕하세요! 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210830 12:41"
        },
        {
          "id": "MDRW2100035960.1.2",
          "form": "안녕하세요! 방금 먹었어요!",
          "original_form": "안녕하세요! 방금 먹었어요!",
          "speaker_id": "2",
          "time": "20210830 12:52"
        },
        {
          "id": "MDRW2100035960.1.3",
          "form": "드셨어요?",
          "original_form": "드셨어요?",
          "speaker_id": "2",
          "time": "20210830 12:52"
        },
        {
          "id": "MDRW2100035960.1.4",
          "form": "아직이요ㅠㅠ",
          "original_form": "아직이요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 12:52"
        },
        {
          "id": "MDRW2100035960.1.5",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "1",
          "time": "20210830 12:52"
        },
        {
          "id": "MDRW2100035960.1.6",
          "form": "바베큐 치킨 먹었어요!",
          "original_form": "바베큐 치킨 먹었어요!",
          "speaker_id": "2",
          "time": "20210830 12:54"
        },
        {
          "id": "MDRW2100035960.1.7",
          "form": "언제드세요ㅜㅜ",
          "original_form": "언제드세요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 12:54"
        },
        {
          "id": "MDRW2100035960.1.8",
          "form": "맛있으셨겠어요ㅠㅠ 은행일 보러 왔는데 대기가 길어져서..ㅠㅠ",
          "original_form": "맛있으셨겠어요ㅠㅠ 은행일 보러 왔는데 대기가 길어져서..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 12:55"
        },
        {
          "id": "MDRW2100035960.1.9",
          "form": "음료는 뭐랑 드셨나요?",
          "original_form": "음료는 뭐랑 드셨나요?",
          "speaker_id": "1",
          "time": "20210830 12:55"
        },
        {
          "id": "MDRW2100035960.1.10",
          "form": "아ㅜㅜ 속상하네요ㅠ",
          "original_form": "아ㅜㅜ 속상하네요ㅠ",
          "speaker_id": "2",
          "time": "20210830 12:56"
        },
        {
          "id": "MDRW2100035960.1.11",
          "form": "점심은 맞춰 드셔야하는데ㅠ",
          "original_form": "점심은 맞춰 드셔야하는데ㅠ",
          "speaker_id": "2",
          "time": "20210830 12:57"
        },
        {
          "id": "MDRW2100035960.1.12",
          "form": "콜라요! 코카콜라!",
          "original_form": "콜라요! 코카콜라!",
          "speaker_id": "2",
          "time": "20210830 12:57"
        },
        {
          "id": "MDRW2100035960.1.13",
          "form": "콜라 맛있죠~",
          "original_form": "콜라 맛있죠~",
          "speaker_id": "1",
          "time": "20210830 12:58"
        },
        {
          "id": "MDRW2100035960.1.14",
          "form": "사이다보다 콜라파이신가요?",
          "original_form": "사이다보다 콜라파이신가요?",
          "speaker_id": "1",
          "time": "20210830 12:58"
        },
        {
          "id": "MDRW2100035960.1.15",
          "form": "사이다파인데 딱 떨어져서..",
          "original_form": "사이다파인데 딱 떨어져서..",
          "speaker_id": "2",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100035960.1.16",
          "form": "같이 온 게 콜라였어요ㅠ",
          "original_form": "같이 온 게 콜라였어요ㅠ",
          "speaker_id": "2",
          "time": "20210830 13:01"
        },
        {
          "id": "MDRW2100035960.1.17",
          "form": "아하 선택지가 없었군요ㅜㅜ",
          "original_form": "아하 선택지가 없었군요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 13:07"
        },
        {
          "id": "MDRW2100035960.1.18",
          "form": "다른 닭요리도 좋아하시나요?",
          "original_form": "다른 닭요리도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210830 13:08"
        },
        {
          "id": "MDRW2100035960.1.19",
          "form": "네ㅠㅠ",
          "original_form": "네ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 13:53"
        },
        {
          "id": "MDRW2100035960.1.20",
          "form": "네! 닭발도 좋아하고 닭볶음탕도 좋아해요!",
          "original_form": "네! 닭발도 좋아하고 닭볶음탕도 좋아해요!",
          "speaker_id": "2",
          "time": "20210830 13:54"
        },
        {
          "id": "MDRW2100035960.1.21",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 13:54"
        },
        {
          "id": "MDRW2100035960.1.22",
          "form": "저도 닭요리 안 가리고 다 먹어요!",
          "original_form": "저도 닭요리 안 가리고 다 먹어요!",
          "speaker_id": "1",
          "time": "20210830 14:19"
        },
        {
          "id": "MDRW2100035960.1.23",
          "form": "고기 중에 닭을 제일 좋아해요ㅎㅎ",
          "original_form": "고기 중에 닭을 제일 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 14:19"
        },
        {
          "id": "MDRW2100035960.1.24",
          "form": "최애 닭요리 있으세요!?",
          "original_form": "최애 닭요리 있으세요!?",
          "speaker_id": "2",
          "time": "20210830 15:04"
        },
        {
          "id": "MDRW2100035960.1.25",
          "form": "치즈불닭이요!",
          "original_form": "치즈불닭이요!",
          "speaker_id": "1",
          "time": "20210830 15:09"
        },
        {
          "id": "MDRW2100035960.1.26",
          "form": "배우신분이시네요",
          "original_form": "배우신분이시네요",
          "speaker_id": "2",
          "time": "20210830 15:31"
        },
        {
          "id": "MDRW2100035960.1.27",
          "form": "진짜 맛없을수 없는걸요?!",
          "original_form": "진짜 맛없을수 없는걸요?!",
          "speaker_id": "2",
          "time": "20210830 15:31"
        },
        {
          "id": "MDRW2100035960.1.28",
          "form": "매운 음식에 치즈 조합은 맛이 없을 수 없죠!",
          "original_form": "매운 음식에 치즈 조합은 맛이 없을 수 없죠!",
          "speaker_id": "1",
          "time": "20210830 15:35"
        }
      ]
    }
  ]
