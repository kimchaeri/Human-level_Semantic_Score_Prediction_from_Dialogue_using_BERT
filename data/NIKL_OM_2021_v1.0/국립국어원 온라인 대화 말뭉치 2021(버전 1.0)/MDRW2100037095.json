      "utterance": [
        {
          "id": "MDRW2100037095.1.1",
          "form": "안녕하세요 먹짱이신가요",
          "original_form": "안녕하세요 먹짱이신가요",
          "speaker_id": "1",
          "time": "20210831 19:31"
        },
        {
          "id": "MDRW2100037095.1.2",
          "form": "ㅋㅋㅋㅋ 먹는걸 조아합니다",
          "original_form": "ㅋㅋㅋㅋ 먹는걸 조아합니다",
          "speaker_id": "2",
          "time": "20210831 19:32"
        },
        {
          "id": "MDRW2100037095.1.3",
          "form": "저도 먹는거 좋아해요",
          "original_form": "저도 먹는거 좋아해요",
          "speaker_id": "1",
          "time": "20210831 19:32"
        },
        {
          "id": "MDRW2100037095.1.4",
          "form": "머조아하세용!",
          "original_form": "머조아하세용!",
          "speaker_id": "2",
          "time": "20210831 19:32"
        },
        {
          "id": "MDRW2100037095.1.5",
          "form": "전 오늘",
          "original_form": "전 오늘",
          "speaker_id": "1",
          "time": "20210831 19:32"
        },
        {
          "id": "MDRW2100037095.1.6",
          "form": "초밥이 먹고싶네요",
          "original_form": "초밥이 먹고싶네요",
          "speaker_id": "1",
          "time": "20210831 19:32"
        },
        {
          "id": "MDRW2100037095.1.7",
          "form": "헐 저도그랫는데",
          "original_form": "헐 저도그랫는데",
          "speaker_id": "2",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100037095.1.8",
          "form": "근데 비싸서 그냥 샌드위치먹음",
          "original_form": "근데 비싸서 그냥 샌드위치먹음",
          "speaker_id": "2",
          "time": "20210831 19:33"
        },
        {
          "id": "MDRW2100037095.1.9",
          "form": "저도 아직 저녁못먹었어요 귀가중 ㅜㅜ",
          "original_form": "저도 아직 저녁못먹었어요 귀가중 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100037095.1.10",
          "form": "너무 비싸서 차마",
          "original_form": "너무 비싸서 차마",
          "speaker_id": "1",
          "time": "20210831 19:34"
        },
        {
          "id": "MDRW2100037095.1.11",
          "form": "샌드위치뭐드셧어요?",
          "original_form": "샌드위치뭐드셧어요?",
          "speaker_id": "1",
          "time": "20210831 19:35"
        },
        {
          "id": "MDRW2100037095.1.12",
          "form": "불고기 샌드위치요 ㅋㅋ  캬페서파는더",
          "original_form": "불고기 샌드위치요 ㅋㅋ  캬페서파는더",
          "speaker_id": "2",
          "time": "20210831 19:40"
        },
        {
          "id": "MDRW2100037095.1.13",
          "form": "너무 귀찮아서 올만에 배달시킨건데",
          "original_form": "너무 귀찮아서 올만에 배달시킨건데",
          "speaker_id": "2",
          "time": "20210831 19:40"
        },
        {
          "id": "MDRW2100037095.1.14",
          "form": "걍그랬어용",
          "original_form": "걍그랬어용",
          "speaker_id": "2",
          "time": "20210831 19:40"
        },
        {
          "id": "MDRW2100037095.1.15",
          "form": "배달시켯는데 실패하면 실망스러워요",
          "original_form": "배달시켯는데 실패하면 실망스러워요",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100037095.1.16",
          "form": "저는 고등어 구워둔거 데워서 먹엇는데 역시 갓 구운게 맛잇는거같아요",
          "original_form": "저는 고등어 구워둔거 데워서 먹엇는데 역시 갓 구운게 맛잇는거같아요",
          "speaker_id": "1",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037095.1.17",
          "form": "생선은 특히그런거같아용",
          "original_form": "생선은 특히그런거같아용",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037095.1.18",
          "form": "생선구이 갓구운게 넘 맛있는데",
          "original_form": "생선구이 갓구운게 넘 맛있는데",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100037095.1.19",
          "form": "그쵸 갓구우면 촉촉하고맛잇는데 ㅜㅜ",
          "original_form": "그쵸 갓구우면 촉촉하고맛잇는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 10:08"
        },
        {
          "id": "MDRW2100037095.1.20",
          "form": "저 학교에서일해서 급식먹는데 오늘 수요일이라 기대중이에요",
          "original_form": "저 학교에서일해서 급식먹는데 오늘 수요일이라 기대중이에요",
          "speaker_id": "1",
          "time": "20210901 10:08"
        },
        {
          "id": "MDRW2100037095.1.21",
          "form": "아 수요일 급식 ! ㅋㅋㅋ",
          "original_form": "아 수요일 급식 ! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:20"
        },
        {
          "id": "MDRW2100037095.1.22",
          "form": "메뉴가뭔가요",
          "original_form": "메뉴가뭔가요",
          "speaker_id": "2",
          "time": "20210901 12:20"
        },
        {
          "id": "MDRW2100037095.1.23",
          "form": "오늘 짜장면이랑 탕수육 나왔어요",
          "original_form": "오늘 짜장면이랑 탕수육 나왔어요",
          "speaker_id": "1",
          "time": "20210901 12:53"
        },
        {
          "id": "MDRW2100037095.1.24",
          "form": "대박적",
          "original_form": "대박적",
          "speaker_id": "1",
          "time": "20210901 12:53"
        },
        {
          "id": "MDRW2100037095.1.25",
          "form": "대박 ㅋㅋㅋㅋㅋㅋ 급식먹을때생각나네요... 수요일이면빨리달려가던 그시절",
          "original_form": "대박 ㅋㅋㅋㅋㅋㅋ 급식먹을때생각나네요... 수요일이면빨리달려가던 그시절",
          "speaker_id": "2",
          "time": "20210901 12:57"
        },
        {
          "id": "MDRW2100037095.1.26",
          "form": "맞아요 수요일마다 ㅋㅋㅋㅋ 뭐가 최애메뉴셨어요?",
          "original_form": "맞아요 수요일마다 ㅋㅋㅋㅋ 뭐가 최애메뉴셨어요?",
          "speaker_id": "1",
          "time": "20210901 12:57"
        },
        {
          "id": "MDRW2100037095.1.27",
          "form": "삼겹살이여 ㅋㅋㅋㅋㅋ",
          "original_form": "삼겹살이여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:22"
        },
        {
          "id": "MDRW2100037095.1.28",
          "form": "와 급식에 삼겹살 ㅜ",
          "original_form": "와 급식에 삼겹살 ㅜ",
          "speaker_id": "1",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100037095.1.29",
          "form": "저희는 보쌈이 가끔 나오는데 그게 진짜 맛있어요 쌈장이랑 보쌈김치랑",
          "original_form": "저희는 보쌈이 가끔 나오는데 그게 진짜 맛있어요 쌈장이랑 보쌈김치랑",
          "speaker_id": "1",
          "time": "20210901 13:46"
        }
      ]
    }
