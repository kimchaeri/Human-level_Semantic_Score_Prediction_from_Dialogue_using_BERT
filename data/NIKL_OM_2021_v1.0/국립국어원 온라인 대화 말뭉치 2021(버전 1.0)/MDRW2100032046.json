      "utterance": [
        {
          "id": "MDRW2100032046.1.1",
          "form": "점심",
          "original_form": "점심",
          "speaker_id": "1",
          "time": "20210905 11:25"
        },
        {
          "id": "MDRW2100032046.1.2",
          "form": "준비하셔야죠 ㅎㅎ",
          "original_form": "준비하셔야죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 11:25"
        },
        {
          "id": "MDRW2100032046.1.3",
          "form": "그러게요ㅋㅋㅋㅋㅋ 요즘은 뭐가 맛있을까요",
          "original_form": "그러게요ㅋㅋㅋㅋㅋ 요즘은 뭐가 맛있을까요",
          "speaker_id": "2",
          "time": "20210905 11:26"
        },
        {
          "id": "MDRW2100032046.1.4",
          "form": "냉동실에 넣어놓은 치킨텐더 데워먹을까도 싶은데",
          "original_form": "냉동실에 넣어놓은 치킨텐더 데워먹을까도 싶은데",
          "speaker_id": "2",
          "time": "20210905 11:27"
        },
        {
          "id": "MDRW2100032046.1.5",
          "form": "유통기한 임박제품으로 싸게 사서 쟁여놨거든요!",
          "original_form": "유통기한 임박제품으로 싸게 사서 쟁여놨거든요!",
          "speaker_id": "2",
          "time": "20210905 11:27"
        },
        {
          "id": "MDRW2100032046.1.6",
          "form": "ㅋㅋ치킨텐더 맛있겠네요",
          "original_form": "ㅋㅋ치킨텐더 맛있겠네요",
          "speaker_id": "1",
          "time": "20210905 11:37"
        },
        {
          "id": "MDRW2100032046.1.7",
          "form": "저도 고민중이에요",
          "original_form": "저도 고민중이에요",
          "speaker_id": "1",
          "time": "20210905 11:37"
        },
        {
          "id": "MDRW2100032046.1.8",
          "form": "간단히 할지",
          "original_form": "간단히 할지",
          "speaker_id": "1",
          "time": "20210905 11:37"
        },
        {
          "id": "MDRW2100032046.1.9",
          "form": "ㅋㅋㅋㅋㅋ점심 후보로 뭐뭐 정해두셨나요?",
          "original_form": "ㅋㅋㅋㅋㅋ점심 후보로 뭐뭐 정해두셨나요?",
          "speaker_id": "2",
          "time": "20210905 11:38"
        },
        {
          "id": "MDRW2100032046.1.10",
          "form": "닭가.슴살 도시락",
          "original_form": "닭가.슴살 도시락",
          "speaker_id": "1",
          "time": "20210905 11:42"
        },
        {
          "id": "MDRW2100032046.1.11",
          "form": "해야지 싶어요ㅋㅋ",
          "original_form": "해야지 싶어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:42"
        },
        {
          "id": "MDRW2100032046.1.12",
          "form": "이따 저녁에 나갈거라",
          "original_form": "이따 저녁에 나갈거라",
          "speaker_id": "1",
          "time": "20210905 11:42"
        },
        {
          "id": "MDRW2100032046.1.13",
          "form": "도시락 좋죠ㅋㅋㅋㅋㅋㅋ",
          "original_form": "도시락 좋죠ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:42"
        },
        {
          "id": "MDRW2100032046.1.14",
          "form": "간편하게 먹을 수 있는게 역시 최고인 것 같아요",
          "original_form": "간편하게 먹을 수 있는게 역시 최고인 것 같아요",
          "speaker_id": "2",
          "time": "20210905 11:42"
        },
        {
          "id": "MDRW2100032046.1.15",
          "form": "도시락은 미리 사둔 냉동도시락 같은건가요?",
          "original_form": "도시락은 미리 사둔 냉동도시락 같은건가요?",
          "speaker_id": "2",
          "time": "20210905 11:43"
        },
        {
          "id": "MDRW2100032046.1.16",
          "form": "네 허닭이요 ㅎㅎ",
          "original_form": "네 허닭이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 11:45"
        },
        {
          "id": "MDRW2100032046.1.17",
          "form": "앗 허닭 맛있나요ㅋㅋㅋㅋㅋ",
          "original_form": "앗 허닭 맛있나요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:45"
        },
        {
          "id": "MDRW2100032046.1.18",
          "form": "워낙 닭가슴살 브랜드들이 많이 나와서",
          "original_form": "워낙 닭가슴살 브랜드들이 많이 나와서",
          "speaker_id": "2",
          "time": "20210905 11:45"
        },
        {
          "id": "MDRW2100032046.1.19",
          "form": "뭐가 맛있는지 모르겠더라구요",
          "original_form": "뭐가 맛있는지 모르겠더라구요",
          "speaker_id": "2",
          "time": "20210905 11:45"
        },
        {
          "id": "MDRW2100032046.1.20",
          "form": "전 아임닭 큐브닭가슴살 먹어봤는데 이건 맛있었고",
          "original_form": "전 아임닭 큐브닭가슴살 먹어봤는데 이건 맛있었고",
          "speaker_id": "2",
          "time": "20210905 11:46"
        },
        {
          "id": "MDRW2100032046.1.21",
          "form": "요새는 뭐",
          "original_form": "요새는 뭐",
          "speaker_id": "1",
          "time": "20210905 11:46"
        },
        {
          "id": "MDRW2100032046.1.22",
          "form": "왠만하면",
          "original_form": "왠만하면",
          "speaker_id": "1",
          "time": "20210905 11:46"
        },
        {
          "id": "MDRW2100032046.1.23",
          "form": "다 맛있죠!ㅋㅋ",
          "original_form": "다 맛있죠!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:46"
        },
        {
          "id": "MDRW2100032046.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 다이어트 식품 시장이 커지면서 웬만한건 다 맛있게 나오더라구요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 다이어트 식품 시장이 커지면서 웬만한건 다 맛있게 나오더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:47"
        },
        {
          "id": "MDRW2100032046.1.25",
          "form": "얼마전에 수비드 닭가.슴살 먹어봤는데 이것도 너무 맛있었어요",
          "original_form": "얼마전에 수비드 닭가.슴살 먹어봤는데 이것도 너무 맛있었어요",
          "speaker_id": "2",
          "time": "20210905 11:48"
        },
        {
          "id": "MDRW2100032046.1.26",
          "form": "ㅋㅋ예전엔 뻑뻑했던거 같은데",
          "original_form": "ㅋㅋ예전엔 뻑뻑했던거 같은데",
          "speaker_id": "1",
          "time": "20210905 11:48"
        },
        {
          "id": "MDRW2100032046.1.27",
          "form": "다행이에요",
          "original_form": "다행이에요",
          "speaker_id": "1",
          "time": "20210905 11:48"
        },
        {
          "id": "MDRW2100032046.1.28",
          "form": "맞아요ㅋㅋㅋㅋㅋㅋ 수비드 진짜 너무 부드럽고 맛있더라구요",
          "original_form": "맞아요ㅋㅋㅋㅋㅋㅋ 수비드 진짜 너무 부드럽고 맛있더라구요",
          "speaker_id": "2",
          "time": "20210905 11:51"
        },
        {
          "id": "MDRW2100032046.1.29",
          "form": "예전에는 닭찌하면 뻑뻑한 걸로 유명했는데 사람들이 못해낼 일은 없나봐요",
          "original_form": "예전에는 닭찌하면 뻑뻑한 걸로 유명했는데 사람들이 못해낼 일은 없나봐요",
          "speaker_id": "2",
          "time": "20210905 11:51"
        },
        {
          "id": "MDRW2100032046.1.30",
          "form": "너무 맛있어서 수비드 기계도 좀 관심가더라구요 예전에는 왜 번거롭게 저런걸 하나 싶은 마음도 있었는데 직접 먹어보니 알 것 같아요ㅋㅋㅋㅋㅋ 너무 맛있어요",
          "original_form": "너무 맛있어서 수비드 기계도 좀 관심가더라구요 예전에는 왜 번거롭게 저런걸 하나 싶은 마음도 있었는데 직접 먹어보니 알 것 같아요ㅋㅋㅋㅋㅋ 너무 맛있어요",
          "speaker_id": "2",
          "time": "20210905 11:54"
        },
        {
          "id": "MDRW2100032046.1.31",
          "form": "수비드기계까지 사시려고요?ㅋㅋ",
          "original_form": "수비드기계까지 사시려고요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:22"
        },
        {
          "id": "MDRW2100032046.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아뇨 아무래도 공간을 많이 잡아먹어서 사긴 힘들겠지만 잠깐 뽐뿌가 왔었어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아뇨 아무래도 공간을 많이 잡아먹어서 사긴 힘들겠지만 잠깐 뽐뿌가 왔었어요",
          "speaker_id": "2",
          "time": "20210905 12:23"
        },
        {
          "id": "MDRW2100032046.1.33",
          "form": "사실 에어프라이어 조차도 놓을 공간이 부족해서 못 사겠더라구요",
          "original_form": "사실 에어프라이어 조차도 놓을 공간이 부족해서 못 사겠더라구요",
          "speaker_id": "2",
          "time": "20210905 12:23"
        }
      ]
    }
