      "utterance": [
        {
          "id": "MDRW2100002723.1.1",
          "form": "날씨가 그곳은 어떤가요",
          "original_form": "날씨가 그곳은 어떤가요",
          "speaker_id": "2",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002723.1.2",
          "form": "여긴 좀 흐려요",
          "original_form": "여긴 좀 흐려요",
          "speaker_id": "1",
          "time": "20210820 15:03"
        },
        {
          "id": "MDRW2100002723.1.3",
          "form": "여기는 맑지만 덥네요",
          "original_form": "여기는 맑지만 덥네요",
          "speaker_id": "2",
          "time": "20210820 15:03"
        },
        {
          "id": "MDRW2100002723.1.4",
          "form": "그러게요 낮에는 덥네요",
          "original_form": "그러게요 낮에는 덥네요",
          "speaker_id": "1",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002723.1.5",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002723.1.6",
          "form": "밤에는 춥던데",
          "original_form": "밤에는 춥던데",
          "speaker_id": "2",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002723.1.7",
          "form": "그러게요 여름이 거의 다 갔나봐요",
          "original_form": "그러게요 여름이 거의 다 갔나봐요",
          "speaker_id": "1",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002723.1.8",
          "form": "역시 입추매직",
          "original_form": "역시 입추매직",
          "speaker_id": "2",
          "time": "20210820 15:04"
        },
        {
          "id": "MDRW2100002723.1.9",
          "form": "맞아요 입추 지나니까 서늘해진 기분이네요",
          "original_form": "맞아요 입추 지나니까 서늘해진 기분이네요",
          "speaker_id": "1",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002723.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002723.1.11",
          "form": "가을 좋아하세요?",
          "original_form": "가을 좋아하세요?",
          "speaker_id": "2",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002723.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210820 15:05"
        },
        {
          "id": "MDRW2100002723.1.13",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "1",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002723.1.14",
          "form": "저는 봄이요",
          "original_form": "저는 봄이요",
          "speaker_id": "2",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002723.1.15",
          "form": "저도 봄도 좋아요 봄가을",
          "original_form": "저도 봄도 좋아요 봄가을",
          "speaker_id": "1",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002723.1.16",
          "form": "꽃이랑 잎 색들이 예뻐요",
          "original_form": "꽃이랑 잎 색들이 예뻐요",
          "speaker_id": "2",
          "time": "20210820 15:06"
        },
        {
          "id": "MDRW2100002723.1.17",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002723.1.18",
          "form": "시원하기도 하고",
          "original_form": "시원하기도 하고",
          "speaker_id": "1",
          "time": "20210820 15:07"
        },
        {
          "id": "MDRW2100002723.1.19",
          "form": "우와 덥다",
          "original_form": "우와 덥다",
          "speaker_id": "2",
