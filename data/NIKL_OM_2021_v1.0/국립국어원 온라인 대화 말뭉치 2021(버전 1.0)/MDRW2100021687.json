      "utterance": [
        {
          "id": "MDRW2100021687.1.1",
          "form": "걍 시끄러웠음",
          "original_form": "걍 시끄러웠음",
          "speaker_id": "2",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100021687.1.2",
          "form": "지금도잖아요",
          "original_form": "지금도잖아요",
          "speaker_id": "1",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021687.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021687.1.4",
          "form": "그럼 초딩때는요",
          "original_form": "그럼 초딩때는요",
          "speaker_id": "1",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021687.1.5",
          "form": "초딩때는ㅋㅋ",
          "original_form": "초딩때는ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021687.1.6",
          "form": "조용했음",
          "original_form": "조용했음",
          "speaker_id": "2",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021687.1.7",
          "form": "의외네요",
          "original_form": "의외네요",
          "speaker_id": "1",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021687.1.8",
          "form": "초4때까지는ㅇㅇ",
          "original_form": "초4때까지는ㅇㅇ",
          "speaker_id": "2",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021687.1.9",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021687.1.10",
          "form": "너무조용해서",
          "original_form": "너무조용해서",
          "speaker_id": "2",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100021687.1.11",
          "form": "엄마가 저 처음에 병있는줄 알았다함",
          "original_form": "엄마가 저 처음에 병있는줄 알았다함",
          "speaker_id": "2",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021687.1.12",
          "form": "왜 그땐 말안하셧나요",
          "original_form": "왜 그땐 말안하셧나요",
          "speaker_id": "1",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021687.1.13",
          "form": "아직 세상에 적응하기 전인건가",
          "original_form": "아직 세상에 적응하기 전인건가",
          "speaker_id": "1",
          "time": "20210828 21:40"
        },
        {
          "id": "MDRW2100021687.1.14",
          "form": "글쎄요ㅋㅋ",
          "original_form": "글쎄요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021687.1.15",
          "form": "기억안나네요",
          "original_form": "기억안나네요",
          "speaker_id": "2",
          "time": "20210828 22:08"
        },
        {
          "id": "MDRW2100021687.1.16",
          "form": "초딩때 공부잘하셨나요",
          "original_form": "초딩때 공부잘하셨나요",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100021687.1.17",
          "form": "초딩때는 잘 했는데",
          "original_form": "초딩때는 잘 했는데",
          "speaker_id": "2",
          "time": "20210828 22:51"
        },
        {
          "id": "MDRW2100021687.1.18",
          "form": "중딩때 망햇죠",
          "original_form": "중딩때 망햇죠",
          "speaker_id": "2",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100021687.1.19",
          "form": "이씨는요",
          "original_form": "이씨는요",
          "speaker_id": "2",
          "time": "20210828 22:52"
        },
        {
          "id": "MDRW2100021687.1.20",
          "form": "초딩때부터",
          "original_form": "초딩때부터",
          "speaker_id": "1",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021687.1.21",
          "form": "조져버림",
          "original_form": "조져버림",
          "speaker_id": "1",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021687.1.22",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021687.1.23",
          "form": "사건이 있었나요",
          "original_form": "사건이 있었나요",
          "speaker_id": "2",
          "time": "20210829 00:27"
        },
        {
          "id": "MDRW2100021687.1.24",
          "form": "그냥 멍청한애중하나였어요",
          "original_form": "그냥 멍청한애중하나였어요",
          "speaker_id": "1",
          "time": "20210829 14:58"
        },
        {
          "id": "MDRW2100021687.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ사건따윈없고",
          "original_form": "ㅋㅋㅋㅋㅋㅋ사건따윈없고",
          "speaker_id": "1",
          "time": "20210829 14:58"
        },
        {
          "id": "MDRW2100021687.1.26",
          "form": "ㅋㄱㅋㄱㄱ자기비하하지마여",
          "original_form": "ㅋㄱㅋㄱㄱ자기비하하지마여",
          "speaker_id": "2",
          "time": "20210829 15:06"
        },
        {
          "id": "MDRW2100021687.1.27",
          "form": "ㅋㅋㅋ암튼 그냥 타고나게 공부를 못하",
          "original_form": "ㅋㅋㅋ암튼 그냥 타고나게 공부를 못하",
          "speaker_id": "1",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100021687.1.28",
          "form": "ㅋㅋㅋ딴거잘하믄대죠",
          "original_form": "ㅋㅋㅋ딴거잘하믄대죠",
          "speaker_id": "2",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100021687.1.29",
          "form": "딴거잘했음?",
          "original_form": "딴거잘했음?",
          "speaker_id": "2",
          "time": "20210829 23:12"
        },
        {
          "id": "MDRW2100021687.1.30",
          "form": "오 놀랍게도",
          "original_form": "오 놀랍게도",
          "speaker_id": "1",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100021687.1.31",
          "form": "게임잘함ㅋ",
          "original_form": "게임잘함ㅋ",
          "speaker_id": "1",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100021687.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ공부에도움안되는거 잘했음",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ공부에도움안되는거 잘했음",
          "speaker_id": "1",
          "time": "20210829 23:47"
        },
        {
          "id": "MDRW2100021687.1.33",
          "form": "ㅋㅋㅋㅋ게임이라도 잘하면 뭐",
          "original_form": "ㅋㅋㅋㅋ게임이라도 잘하면 뭐",
          "speaker_id": "2",
          "time": "20210830 01:42"
        },
        {
          "id": "MDRW2100021687.1.34",
          "form": "저도 피방 죽돌이임",
          "original_form": "저도 피방 죽돌이임",
