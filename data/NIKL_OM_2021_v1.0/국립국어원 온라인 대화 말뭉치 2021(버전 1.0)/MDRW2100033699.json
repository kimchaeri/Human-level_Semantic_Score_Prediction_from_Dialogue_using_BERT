      "utterance": [
        {
          "id": "MDRW2100033699.1.1",
          "form": "공연 보는 거 좋아하세요?",
          "original_form": "공연 보는 거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100033699.1.2",
          "form": "저는 경험이 별로 없어요",
          "original_form": "저는 경험이 별로 없어요",
          "speaker_id": "2",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100033699.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 23:18"
        },
        {
          "id": "MDRW2100033699.1.4",
          "form": "전 콘서트 자주갔어요 ㅎ",
          "original_form": "전 콘서트 자주갔어요 ㅎ",
          "speaker_id": "1",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100033699.1.5",
          "form": "어떤 콘서트였나요",
          "original_form": "어떤 콘서트였나요",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033699.1.6",
          "form": "아이돌 콘서트였어오 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아이돌 콘서트였어오 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033699.1.7",
          "form": "뮤지컬은 관심 있으신가요?",
          "original_form": "뮤지컬은 관심 있으신가요?",
          "speaker_id": "1",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033699.1.8",
          "form": "누구인가요ㅋㅋㅋㅋ",
          "original_form": "누구인가요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100033699.1.9",
          "form": "관심은 있어요",
          "original_form": "관심은 있어요",
          "speaker_id": "2",
          "time": "20210824 23:21"
        },
        {
          "id": "MDRW2100033699.1.10",
          "form": "저도요 ㅋㅋㅋㅋㅋㅋㅋ 가고싶어요...",
          "original_form": "저도요 ㅋㅋㅋㅋㅋㅋㅋ 가고싶어요...",
          "speaker_id": "1",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100033699.1.11",
          "form": "완전 예전에 엑소랑 그나마 최근이",
          "original_form": "완전 예전에 엑소랑 그나마 최근이",
          "speaker_id": "1",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100033699.1.12",
          "form": "엔시티요ㅋㅋㅋㅋㅋ 아시나요?",
          "original_form": "엔시티요ㅋㅋㅋㅋㅋ 아시나요?",
          "speaker_id": "1",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100033699.1.13",
          "form": "알아요!",
          "original_form": "알아요!",
          "speaker_id": "2",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100033699.1.14",
          "form": "친구가 엑소 좋아했고",
          "original_form": "친구가 엑소 좋아했고",
          "speaker_id": "2",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100033699.1.15",
          "form": "엔시티 노래는 나올때마다 타이틀은 들어보거든요",
          "original_form": "엔시티 노래는 나올때마다 타이틀은 들어보거든요",
          "speaker_id": "2",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100033699.1.16",
          "form": "첫 타이틀이 신비롭고 좋아서요",
          "original_form": "첫 타이틀이 신비롭고 좋아서요",
          "speaker_id": "2",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100033699.1.17",
          "form": "오 아셔서 다행이네요 ㅎ ㅎㅎㅎㅎ",
          "original_form": "오 아셔서 다행이네요 ㅎ ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100033699.1.18",
          "form": "두가수 콘서트 가봤어요",
          "original_form": "두가수 콘서트 가봤어요",
          "speaker_id": "1",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100033699.1.19",
          "form": "아이돌 콘서트 가본적 없는데 어때요?",
          "original_form": "아이돌 콘서트 가본적 없는데 어때요?",
          "speaker_id": "2",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100033699.1.20",
          "form": "티켓구하기힘들어서 친구랑 같이 감상하기 힘들죠...?",
          "original_form": "티켓구하기힘들어서 친구랑 같이 감상하기 힘들죠...?",
          "speaker_id": "2",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100033699.1.21",
          "form": "솔플 하는건가요?",
          "original_form": "솔플 하는건가요?",
          "speaker_id": "2",
          "time": "20210824 23:24"
        },
        {
          "id": "MDRW2100033699.1.22",
          "form": "네 솔플이나 동행을 구해요!!!",
          "original_form": "네 솔플이나 동행을 구해요!!!",
          "speaker_id": "1",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100033699.1.23",
          "form": "친구랑 가기는 보통 힘들긴하죠",
          "original_form": "친구랑 가기는 보통 힘들긴하죠",
          "speaker_id": "1",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100033699.1.24",
          "form": "동행은 어떻게 구하나요?",
          "original_form": "동행은 어떻게 구하나요?",
          "speaker_id": "2",
          "time": "20210824 23:25"
        },
        {
          "id": "MDRW2100033699.1.25",
          "form": "전 그냥 트위터 이런 곳에서요 !!!",
          "original_form": "전 그냥 트위터 이런 곳에서요 !!!",
          "speaker_id": "1",
          "time": "20210824 23:26"
        }
      ]
    }
