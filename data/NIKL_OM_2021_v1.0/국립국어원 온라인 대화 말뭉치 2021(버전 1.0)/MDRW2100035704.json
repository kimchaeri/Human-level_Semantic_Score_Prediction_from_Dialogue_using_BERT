      "utterance": [
        {
          "id": "MDRW2100035704.1.1",
          "form": "맥주좋아하세요~?",
          "original_form": "맥주좋아하세요~?",
          "speaker_id": "2",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100035704.1.2",
          "form": "즐겨먹진 않아요",
          "original_form": "즐겨먹진 않아요",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100035704.1.3",
          "form": "회식땐 먹어요",
          "original_form": "회식땐 먹어요",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100035704.1.4",
          "form": "피쳐잔에 나온걸 먹어요",
          "original_form": "피쳐잔에 나온걸 먹어요",
          "speaker_id": "1",
          "time": "20210829 22:53"
        },
        {
          "id": "MDRW2100035704.1.5",
          "form": "그러시군요~",
          "original_form": "그러시군요~",
          "speaker_id": "2",
          "time": "20210829 22:54"
        },
        {
          "id": "MDRW2100035704.1.6",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100035704.1.7",
          "form": "전 술중엔 맥주를 좋아해요~",
          "original_form": "전 술중엔 맥주를 좋아해요~",
          "speaker_id": "2",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100035704.1.8",
          "form": "더운여름엔 많이마셨네요ㅠ",
          "original_form": "더운여름엔 많이마셨네요ㅠ",
          "speaker_id": "2",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100035704.1.9",
          "form": "어떤 종류를 좋아하세요?",
          "original_form": "어떤 종류를 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100035704.1.10",
          "form": "라거가 제일 인거같아요 ! 톡소는 보리맛 ㅋㅋ",
          "original_form": "라거가 제일 인거같아요 ! 톡소는 보리맛 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:57"
        },
        {
          "id": "MDRW2100035704.1.11",
          "form": "와..전 솔직히 맛 차이를 잘 머르겠어요",
          "original_form": "와..전 솔직히 맛 차이를 잘 머르겠어요",
          "speaker_id": "1",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100035704.1.12",
          "form": "다 좀 찝찔한 탄산 맛이에요",
          "original_form": "다 좀 찝찔한 탄산 맛이에요",
          "speaker_id": "1",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100035704.1.13",
          "form": "죄송 ㅎㅎ",
          "original_form": "죄송 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100035704.1.14",
          "form": "ㅋㅋㅋㅋ 그쵸 저도 맛을 잘아는건아니에요",
          "original_form": "ㅋㅋㅋㅋ 그쵸 저도 맛을 잘아는건아니에요",
          "speaker_id": "2",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100035704.1.15",
          "form": "여러종류의 맥주를 드시나봐요",
          "original_form": "여러종류의 맥주를 드시나봐요",
          "speaker_id": "1",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100035704.1.16",
          "form": "라거종류가 오비라거는 아니죠?",
          "original_form": "라거종류가 오비라거는 아니죠?",
          "speaker_id": "1",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100035704.1.17",
          "form": "보통 카스맥주류 ㅋㅋㅋ",
          "original_form": "보통 카스맥주류 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100035704.1.18",
          "form": "카스맥주도 라거에요?",
          "original_form": "카스맥주도 라거에요?",
          "speaker_id": "1",
          "time": "20210829 23:00"
        },
        {
          "id": "MDRW2100035704.1.19",
          "form": "알못이에욬ㅋㅋ",
          "original_form": "알못이에욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:00"
        },
        {
          "id": "MDRW2100035704.1.20",
          "form": "맥주 종류 분류에서 라거죠!",
          "original_form": "맥주 종류 분류에서 라거죠!",
