        {
          "id": "MDRW2100024422.1.28",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210831 08:59"
        },
        {
          "id": "MDRW2100024422.1.29",
          "form": "저 원래 운동 안하는데",
          "original_form": "저 원래 운동 안하는데",
          "speaker_id": "1",
          "time": "20210831 09:03"
        },
        {
          "id": "MDRW2100024422.1.30",
          "form": "요새 걷기 하고 있어요",
          "original_form": "요새 걷기 하고 있어요",
          "speaker_id": "1",
          "time": "20210831 09:03"
        },
        {
          "id": "MDRW2100024422.1.31",
          "form": "살기 위해서 ㅎㅎㅎ",
          "original_form": "살기 위해서 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 09:03"
        },
        {
          "id": "MDRW2100024422.1.32",
          "form": "아ㅋㅋㅋ 삼성헬스로 걷는거 측정하면 좋더라구요",
          "original_form": "아ㅋㅋㅋ 삼성헬스로 걷는거 측정하면 좋더라구요",
          "speaker_id": "2",
          "time": "20210831 09:05"
        },
        {
          "id": "MDRW2100024422.1.33",
          "form": "저도 하루에 6000보 이상씩 걸어요!!",
          "original_form": "저도 하루에 6000보 이상씩 걸어요!!",
          "speaker_id": "2",
          "time": "20210831 09:06"
        },
        {
          "id": "MDRW2100024422.1.34",
          "form": "와우 전 걸음수는 모르겠고",
          "original_form": "와우 전 걸음수는 모르겠고",
          "speaker_id": "1",
          "time": "20210831 09:16"
        },
        {
          "id": "MDRW2100024422.1.35",
          "form": "1시간정도 걸어요",
          "original_form": "1시간정도 걸어요",
          "speaker_id": "1",
