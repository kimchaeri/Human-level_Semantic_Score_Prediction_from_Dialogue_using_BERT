      "utterance": [
        {
          "id": "MDRW2100024422.1.1",
          "form": "name2님은 백신 맞았나요?",
          "original_form": "&name2&님은 백신 맞았나요?",
          "speaker_id": "1",
          "time": "20210831 08:21"
        },
        {
          "id": "MDRW2100024422.1.2",
          "form": "앗 아뇨ㅠ 까먹고 신청을 못했어요",
          "original_form": "앗 아뇨ㅠ 까먹고 신청을 못했어요",
          "speaker_id": "2",
          "time": "20210831 08:22"
        },
        {
          "id": "MDRW2100024422.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 08:22"
        },
        {
          "id": "MDRW2100024422.1.4",
          "form": "헛 신청 놓첬어요?",
          "original_form": "헛 신청 놓첬어요?",
          "speaker_id": "1",
          "time": "20210831 08:22"
        },
        {
          "id": "MDRW2100024422.1.5",
          "form": "전 추석 전에 맞아요",
          "original_form": "전 추석 전에 맞아요",
          "speaker_id": "1",
          "time": "20210831 08:22"
        },
        {
          "id": "MDRW2100024422.1.6",
          "form": "네ㅠㅠㅜ",
          "original_form": "네ㅠㅠㅜ",
          "speaker_id": "2",
          "time": "20210831 08:22"
        },
        {
          "id": "MDRW2100024422.1.7",
          "form": "오오 다행이네요!!",
          "original_form": "오오 다행이네요!!",
          "speaker_id": "2",
          "time": "20210831 08:23"
        },
        {
          "id": "MDRW2100024422.1.8",
          "form": "그럼 재신청해야 하셔야죠",
          "original_form": "그럼 재신청해야 하셔야죠",
          "speaker_id": "1",
          "time": "20210831 08:23"
        },
        {
          "id": "MDRW2100024422.1.9",
          "form": "맞아요ㅜ 근데 병원가면",
          "original_form": "맞아요ㅜ 근데 병원가면",
          "speaker_id": "2",
          "time": "20210831 08:23"
        },
        {
          "id": "MDRW2100024422.1.10",
          "form": "날짜잡아준다고 그래서",
          "original_form": "날짜잡아준다고 그래서",
          "speaker_id": "2",
          "time": "20210831 08:23"
        },
        {
          "id": "MDRW2100024422.1.11",
          "form": "내일 가보려고요!!",
          "original_form": "내일 가보려고요!!",
          "speaker_id": "2",
          "time": "20210831 08:23"
        },
        {
          "id": "MDRW2100024422.1.12",
          "form": "재신청하는건 아마... 추석때였나요? 또 헷갈리네요",
          "original_form": "재신청하는건 아마... 추석때였나요? 또 헷갈리네요",
          "speaker_id": "2",
          "time": "20210831 08:23"
        },
        {
          "id": "MDRW2100024422.1.13",
          "form": "아~ 잔여백신도 있으니까요",
          "original_form": "아~ 잔여백신도 있으니까요",
          "speaker_id": "1",
          "time": "20210831 08:32"
        },
        {
          "id": "MDRW2100024422.1.14",
          "form": "어쩌면 저보다 빨리 맞을 수도 있게네요",
          "original_form": "어쩌면 저보다 빨리 맞을 수도 있게네요",
          "speaker_id": "1",
          "time": "20210831 08:32"
        },
        {
          "id": "MDRW2100024422.1.15",
          "form": "엇ㅎㅎ 그럴수도 있나요?!!",
          "original_form": "엇ㅎㅎ 그럴수도 있나요?!!",
          "speaker_id": "2",
          "time": "20210831 08:33"
        },
        {
          "id": "MDRW2100024422.1.16",
          "form": "백신 맞으면 코로나 그래도...약화되겠죠?",
          "original_form": "백신 맞으면 코로나 그래도...약화되겠죠?",
          "speaker_id": "2",
          "time": "20210831 08:33"
        },
        {
          "id": "MDRW2100024422.1.17",
          "form": "빨리 마스크 벗고 싶어요ㅠㅠ",
          "original_form": "빨리 마스크 벗고 싶어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 08:33"
        },
        {
          "id": "MDRW2100024422.1.18",
          "form": "일단 위드코로나는 될거 같아야",
          "original_form": "일단 위드코로나는 될거 같아야",
          "speaker_id": "1",
          "time": "20210831 08:42"
        },
        {
          "id": "MDRW2100024422.1.19",
          "form": "마스크 쓰더라도 좀 마음놓고 돌아디닐수 있었으면 해요",
          "original_form": "마스크 쓰더라도 좀 마음놓고 돌아디닐수 있었으면 해요",
          "speaker_id": "1",
          "time": "20210831 08:43"
        },
        {
          "id": "MDRW2100024422.1.20",
          "form": "ㅜㅠ 맞아요 코로나때문에 정말...너무 힘들어요",
          "original_form": "ㅜㅠ 맞아요 코로나때문에 정말...너무 힘들어요",
          "speaker_id": "2",
          "time": "20210831 08:43"
        },
        {
          "id": "MDRW2100024422.1.21",
          "form": "사실 저 지금 감기에 심하게 걸렸거든요",
          "original_form": "사실 저 지금 감기에 심하게 걸렸거든요",
          "speaker_id": "2",
          "time": "20210831 08:43"
        },
        {
          "id": "MDRW2100024422.1.22",
          "form": "에어컨바람때문에ㅜ",
          "original_form": "에어컨바람때문에ㅜ",
          "speaker_id": "2",
          "time": "20210831 08:43"
        },
        {
          "id": "MDRW2100024422.1.23",
          "form": "그래서 코로나가 더 무서워요",
          "original_form": "그래서 코로나가 더 무서워요",
          "speaker_id": "2",
          "time": "20210831 08:44"
        },
        {
          "id": "MDRW2100024422.1.24",
          "form": "헐 백신 맞기 전에 컨디션 조절해야 해요",
          "original_form": "헐 백신 맞기 전에 컨디션 조절해야 해요",
          "speaker_id": "1",
          "time": "20210831 08:45"
        },
        {
          "id": "MDRW2100024422.1.25",
          "form": "ㅜㅠㅠ 그쵸 그래서 빨리 감기 나으려고",
          "original_form": "ㅜㅠㅠ 그쵸 그래서 빨리 감기 나으려고",
          "speaker_id": "2",
          "time": "20210831 08:59"
        },
        {
          "id": "MDRW2100024422.1.26",
          "form": "지금도 꿀차 타먹고 있습니다ㅋㅋ",
          "original_form": "지금도 꿀차 타먹고 있습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 08:59"
        },
        {
          "id": "MDRW2100024422.1.27",
          "form": "name1님은 특별히 하는 운동같은거 있으세요(",
          "original_form": "&name1&님은 특별히 하는 운동같은거 있으세요(",
          "speaker_id": "2",
