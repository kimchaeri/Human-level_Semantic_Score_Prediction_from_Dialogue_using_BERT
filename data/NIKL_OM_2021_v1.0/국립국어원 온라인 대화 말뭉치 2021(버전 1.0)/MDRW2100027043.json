      "utterance": [
        {
          "id": "MDRW2100027043.1.1",
          "form": "요즘은 슬의 봐요",
          "original_form": "요즘은 슬의 봐요",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027043.1.2",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210902 10:29"
        },
        {
          "id": "MDRW2100027043.1.3",
          "form": "오 저는 그 시리즈 한번도 안봤는데",
          "original_form": "오 저는 그 시리즈 한번도 안봤는데",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027043.1.4",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "1",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100027043.1.5",
          "form": "잔잔하게 보기에 좋은거 같아요!",
          "original_form": "잔잔하게 보기에 좋은거 같아요!",
          "speaker_id": "2",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100027043.1.6",
          "form": "막 자극적이지 않아서 좋아요",
          "original_form": "막 자극적이지 않아서 좋아요",
          "speaker_id": "2",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100027043.1.7",
          "form": "저는 기대작은 있어요",
          "original_form": "저는 기대작은 있어요",
          "speaker_id": "1",
          "time": "20210902 17:41"
        },
        {
          "id": "MDRW2100027043.1.8",
          "form": "오 뭐에요??",
          "original_form": "오 뭐에요??",
          "speaker_id": "2",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100027043.1.9",
          "form": "옷소매붉은끝동?",
          "original_form": "옷소매붉은끝동?",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100027043.1.10",
          "form": "준호 나오는거래요",
          "original_form": "준호 나오는거래요",
          "speaker_id": "1",
          "time": "20210902 17:48"
        },
        {
          "id": "MDRW2100027043.1.11",
          "form": "아 들어본거 같아요!",
          "original_form": "아 들어본거 같아요!",
          "speaker_id": "2",
          "time": "20210902 17:53"
        },
        {
          "id": "MDRW2100027043.1.12",
          "form": "투피엠 준호죠?",
          "original_form": "투피엠 준호죠?",
          "speaker_id": "2",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100027043.1.13",
          "form": "밎아요 사극이라 또 기대돼요",
          "original_form": "밎아요 사극이라 또 기대돼요",
          "speaker_id": "1",
          "time": "20210902 17:54"
        },
        {
          "id": "MDRW2100027043.1.14",
          "form": "준호도 연기 잘하는거 같더라고요",
          "original_form": "준호도 연기 잘하는거 같더라고요",
          "speaker_id": "2",
          "time": "20210902 17:57"
        },
        {
          "id": "MDRW2100027043.1.15",
          "form": "맞아요 그 요리하는 드라마 봤었어요",
          "original_form": "맞아요 그 요리하는 드라마 봤었어요",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100027043.1.16",
          "form": "저는 추격자들? 이라는 영화로 봤는데 귀엽더라고요!",
          "original_form": "저는 추격자들? 이라는 영화로 봤는데 귀엽더라고요!",
          "speaker_id": "2",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100027043.1.17",
          "form": "오 저도 봤을꺼에요 ㅋㅋ",
          "original_form": "오 저도 봤을꺼에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100027043.1.18",
          "form": "축구 아까 했는데 동점으로 끝났네요",
          "original_form": "축구 아까 했는데 동점으로 끝났네요",
          "speaker_id": "1",
          "time": "20210902 22:12"
        },
        {
          "id": "MDRW2100027043.1.19",
          "form": "볼려고 했는데 놓쳤네요ㅠㅠ",
          "original_form": "볼려고 했는데 놓쳤네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 22:13"
        },
        {
          "id": "MDRW2100027043.1.20",
          "form": "축구 오늘 중계한다고 슬기로운 의사생활 오늘 안했다더라고요",
          "original_form": "축구 오늘 중계한다고 슬기로운 의사생활 오늘 안했다더라고요",
          "speaker_id": "2",
          "time": "20210902 22:14"
        },
        {
          "id": "MDRW2100027043.1.21",
          "form": "아 오늘 하는날이였군요!!",
          "original_form": "아 오늘 하는날이였군요!!",
          "speaker_id": "1",
          "time": "20210902 22:16"
        },
        {
          "id": "MDRW2100027043.1.22",
          "form": "슬의가 한주에 한번만 해서 2주를 기다려야 되네요ㅠ",
          "original_form": "슬의가 한주에 한번만 해서 2주를 기다려야 되네요ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:17"
        },
        {
          "id": "MDRW2100027043.1.23",
          "form": "그런거 정말 짜증나지요 ㅜㅜㅠㅠ",
          "original_form": "그런거 정말 짜증나지요 ㅜㅜㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 22:18"
        },
        {
          "id": "MDRW2100027043.1.24",
          "form": "예전에는 월화, 수목, 주말 드라마 이렇게 했는데 말이에요",
          "original_form": "예전에는 월화, 수목, 주말 드라마 이렇게 했는데 말이에요",
          "speaker_id": "2",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100027043.1.25",
          "form": "요즘엔 맘대로네요 ㅜㅜ",
          "original_form": "요즘엔 맘대로네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100027043.1.26",
          "form": "주1회도하고",
          "original_form": "주1회도하고",
          "speaker_id": "1",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100027043.1.27",
          "form": "맞아요 매번 편성 시간도 바뀌고",
          "original_form": "맞아요 매번 편성 시간도 바뀌고",
          "speaker_id": "2",
          "time": "20210902 22:22"
        },
        {
          "id": "MDRW2100027043.1.28",
          "form": "제가 드라마 봤던 때에는 다 10시에 시작했었거든요",
          "original_form": "제가 드라마 봤던 때에는 다 10시에 시작했었거든요",
          "speaker_id": "2",
          "time": "20210902 22:23"
        },
        {
          "id": "MDRW2100027043.1.29",
          "form": "그러게 말이에요 ㅜㅜ",
          "original_form": "그러게 말이에요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100027043.1.30",
          "form": "요즘엔 예능도하고 그시간에",
          "original_form": "요즘엔 예능도하고 그시간에",
          "speaker_id": "1",
          "time": "20210902 22:24"
        },
        {
          "id": "MDRW2100027043.1.31",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100027043.1.32",
          "form": "요즘은 뭐 챙겨보려고 해도",
          "original_form": "요즘은 뭐 챙겨보려고 해도",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100027043.1.33",
          "form": "시간이 헷갈리더라고요",
          "original_form": "시간이 헷갈리더라고요",
          "speaker_id": "2",
          "time": "20210902 22:25"
        },
        {
          "id": "MDRW2100027043.1.34",
          "form": "그러니까요... 통일할수없는지...",
          "original_form": "그러니까요... 통일할수없는지...",
          "speaker_id": "1",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100027043.1.35",
          "form": "그냥 다 통일해버렸으면 좋겠어요",
          "original_form": "그냥 다 통일해버렸으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210902 22:36"
        },
        {
          "id": "MDRW2100027043.1.36",
          "form": "그리고 요즘은 11시에도 하더라고요?",
          "original_form": "그리고 요즘은 11시에도 하더라고요?",
          "speaker_id": "2",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100027043.1.37",
          "form": "와 가지가지 하네요",
          "original_form": "와 가지가지 하네요",
          "speaker_id": "1",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100027043.1.38",
          "form": "시청률이 잘 나올려나요",
          "original_form": "시청률이 잘 나올려나요",
          "speaker_id": "1",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100027043.1.39",
          "form": "오히려 너무 늦어서 안보게되던데 도대체 뭘위해 그렇게 늦게 하는건지 모르겠어요",
          "original_form": "오히려 너무 늦어서 안보게되던데 도대체 뭘위해 그렇게 늦게 하는건지 모르겠어요",
          "speaker_id": "2",
          "time": "20210902 22:38"
        },
        {
          "id": "MDRW2100027043.1.40",
          "form": "중간 광고도 짜증나는데...",
          "original_form": "중간 광고도 짜증나는데...",
          "speaker_id": "1",
          "time": "20210902 22:39"
        }
      ]
    }
  ]
