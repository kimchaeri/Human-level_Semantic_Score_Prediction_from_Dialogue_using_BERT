        },
        {
          "id": "MDRW2100041735.1.2",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "2",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100041735.1.3",
          "form": "지난주부터",
          "original_form": "지난주부터",
          "speaker_id": "1",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100041735.1.4",
          "form": "다른 데서 알아보고 수술할걸",
          "original_form": "다른 데서 알아보고 수술할걸",
          "speaker_id": "2",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100041735.1.5",
          "form": "근데 보험회사에 전화하니깐 만원인가 빼고 다 나온다고 했다더라구",
          "original_form": "근데 보험회사에 전화하니깐 만원인가 빼고 다 나온다고 했다더라구",
          "speaker_id": "1",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100041735.1.6",
          "form": "나도 보험사 다시 전화해볼까",
          "original_form": "나도 보험사 다시 전화해볼까",
          "speaker_id": "2",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041735.1.7",
          "form": "그래서 신랑이 도수치료 좀 받아야되겠다곤 하더라구, 나는 도수치료는 당연히 다 나오는줄알았지",
          "original_form": "그래서 신랑이 도수치료 좀 받아야되겠다곤 하더라구, 나는 도수치료는 당연히 다 나오는줄알았지",
          "speaker_id": "1",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041735.1.8",
          "form": "그래그래 다시 전화해봐, 그냥 도수치료 받아도 나오는데",
          "original_form": "그래그래 다시 전화해봐, 그냥 도수치료 받아도 나오는데",
          "speaker_id": "1",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041735.1.9",
          "form": "그 신랑 보험설계해준 사람한테",
          "original_form": "그 신랑 보험설계해준 사람한테",
          "speaker_id": "2",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041735.1.10",
          "form": "소류 보냈거든",
          "original_form": "소류 보냈거든",
          "speaker_id": "2",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041735.1.11",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041735.1.12",
          "form": "보험 회사에 전 홤해봐야겠다",
          "original_form": "보험 회사에 전 홤해봐야겠다",
          "speaker_id": "2",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041735.1.13",
          "form": "좀 그렇다 ㅠ",
          "original_form": "좀 그렇다 ㅠ",
          "speaker_id": "1",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041735.1.14",
          "form": "이런 거 정말 짜증난다",
          "original_form": "이런 거 정말 짜증난다",
          "speaker_id": "2",
          "time": "20210906 15:14"
