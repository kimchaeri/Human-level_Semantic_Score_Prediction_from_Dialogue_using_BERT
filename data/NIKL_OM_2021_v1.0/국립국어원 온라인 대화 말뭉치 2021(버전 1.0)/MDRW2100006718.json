      "utterance": [
        {
          "id": "MDRW2100006718.1.1",
          "form": "가을 좋아하세요?",
          "original_form": "가을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100006718.1.2",
          "form": "제일 좋아하는 계절이죠",
          "original_form": "제일 좋아하는 계절이죠",
          "speaker_id": "2",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100006718.1.3",
          "form": "시원하고 가디건 입을 수 있고",
          "original_form": "시원하고 가디건 입을 수 있고",
          "speaker_id": "2",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100006718.1.4",
          "form": "딱 옷 입기 좋은 거 같아요",
          "original_form": "딱 옷 입기 좋은 거 같아요",
          "speaker_id": "1",
          "time": "20210908 07:31"
        },
        {
          "id": "MDRW2100006718.1.5",
          "form": "겨울이 오기 전까지...",
          "original_form": "겨울이 오기 전까지...",
          "speaker_id": "1",
          "time": "20210908 07:31"
        },
        {
          "id": "MDRW2100006718.1.6",
          "form": "올해는 지난달말부터 비가 오더니 가을이 일찍 온것 같아요",
          "original_form": "올해는 지난달말부터 비가 오더니 가을이 일찍 온것 같아요",
          "speaker_id": "2",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100006718.1.7",
          "form": "항상 가을이 짧아 슬펐는데",
          "original_form": "항상 가을이 짧아 슬펐는데",
          "speaker_id": "2",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100006718.1.8",
          "form": "맞아요! 너무 좋아요ㅎㅎ",
          "original_form": "맞아요! 너무 좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100006718.1.9",
          "form": "올 가을은 길 것 같네요",
          "original_form": "올 가을은 길 것 같네요",
          "speaker_id": "2",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100006718.1.10",
          "form": "가을은 배랑 사과의 계절이라 더 좋네요",
          "original_form": "가을은 배랑 사과의 계절이라 더 좋네요",
          "speaker_id": "1",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100006718.1.11",
          "form": "과일 좋아하세요?",
          "original_form": "과일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100006718.1.12",
          "form": "네 너무 좋아하죠",
          "original_form": "네 너무 좋아하죠",
          "speaker_id": "1",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100006718.1.13",
          "form": "코스트코에서 복숭아 먹었는데 정말 앗있어요",
          "original_form": "코스트코에서 복숭아 먹었는데 정말 앗있어요",
          "speaker_id": "2",
          "time": "20210908 07:35"
        },
        {
          "id": "MDRW2100006718.1.14",
          "form": "올해 복숭아가 맛있더라고요",
          "original_form": "올해 복숭아가 맛있더라고요",
          "speaker_id": "1",
          "time": "20210908 07:37"
        },
        {
          "id": "MDRW2100006718.1.15",
          "form": "네네 정말 맛있더라고요",
          "original_form": "네네 정말 맛있더라고요",
          "speaker_id": "2",
          "time": "20210908 07:38"
        },
        {
          "id": "MDRW2100006718.1.16",
          "form": "곧 추석도 오네요",
          "original_form": "곧 추석도 오네요",
          "speaker_id": "2",
          "time": "20210908 07:38"
        },
        {
          "id": "MDRW2100006718.1.17",
          "form": "네 추석에도 먹거리가 많아 좋아요",
          "original_form": "네 추석에도 먹거리가 많아 좋아요",
          "speaker_id": "1",
          "time": "20210908 07:39"
        }
