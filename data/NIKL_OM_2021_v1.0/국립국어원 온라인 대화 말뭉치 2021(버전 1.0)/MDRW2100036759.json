      "utterance": [
        {
          "id": "MDRW2100036759.1.1",
          "form": "와 힘들게하는만큼 효과가 나타나길",
          "original_form": "와 힘들게하는만큼 효과가 나타나길",
          "speaker_id": "2",
          "time": "20210831 11:32"
        },
        {
          "id": "MDRW2100036759.1.2",
          "form": "근데 이틀만에 그거 효과가있을까 싶기도하고 ㅋㅋ",
          "original_form": "근데 이틀만에 그거 효과가있을까 싶기도하고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:32"
        },
        {
          "id": "MDRW2100036759.1.3",
          "form": "그래도 다른거안먹고  그걸로  독소빼내는거면",
          "original_form": "그래도 다른거안먹고  그걸로  독소빼내는거면",
          "speaker_id": "2",
          "time": "20210831 11:33"
        },
        {
          "id": "MDRW2100036759.1.4",
          "form": "뭔가 독소라도 빠지길 바라고있어요 ㅋㅋ",
          "original_form": "뭔가 독소라도 빠지길 바라고있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:37"
        },
        {
          "id": "MDRW2100036759.1.5",
          "form": "그럼 커피같은것도안되는거죠?",
          "original_form": "그럼 커피같은것도안되는거죠?",
          "speaker_id": "2",
          "time": "20210831 11:40"
        },
        {
          "id": "MDRW2100036759.1.6",
          "form": "되지만 최대한의 효과를 위해",
          "original_form": "되지만 최대한의 효과를 위해",
          "speaker_id": "1",
          "time": "20210831 11:41"
        },
        {
          "id": "MDRW2100036759.1.7",
          "form": "공복을 유지하래요ㅜㅜ",
          "original_form": "공복을 유지하래요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 11:42"
        },
        {
          "id": "MDRW2100036759.1.8",
          "form": "ㅠㅠ이런",
          "original_form": "ㅠㅠ이런",
          "speaker_id": "2",
          "time": "20210831 11:48"
        },
        {
          "id": "MDRW2100036759.1.9",
          "form": "아니 그럼물도 마시지 말라는건가",
          "original_form": "아니 그럼물도 마시지 말라는건가",
          "speaker_id": "1",
          "time": "20210831 11:54"
        },
        {
          "id": "MDRW2100036759.1.10",
          "form": "이 제품이름 레디톡 48시간이거든요",
          "original_form": "이 제품이름 레디톡 48시간이거든요",
          "speaker_id": "1",
          "time": "20210831 11:54"
        },
        {
          "id": "MDRW2100036759.1.11",
          "form": "절대 사지마세요 ㅋㅋ",
          "original_form": "절대 사지마세요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:54"
        },
        {
          "id": "MDRW2100036759.1.12",
          "form": "ㅋㅋㅋㅋ 이런ㅋㅋㄱㄱ절대로",
          "original_form": "ㅋㅋㅋㅋ 이런ㅋㅋㄱㄱ절대로",
          "speaker_id": "2",
          "time": "20210831 12:05"
        },
        {
          "id": "MDRW2100036759.1.13",
          "form": "물은되겠져ㅠㅠ",
          "original_form": "물은되겠져ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 12:05"
        },
        {
          "id": "MDRW2100036759.1.14",
          "form": "공복의 의미를 생각해보고있어요",
          "original_form": "공복의 의미를 생각해보고있어요",
          "speaker_id": "1",
          "time": "20210831 12:15"
        },
        {
          "id": "MDRW2100036759.1.15",
          "form": "보통 병원에서 공복 어쩌고할때는 물도 안되는거던데",
          "original_form": "보통 병원에서 공복 어쩌고할때는 물도 안되는거던데",
          "speaker_id": "1",
          "time": "20210831 12:16"
        },
        {
          "id": "MDRW2100036759.1.16",
          "form": "수분부족되는거 아니구요??/ㅠㅠ",
          "original_form": "수분부족되는거 아니구요??/ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 12:27"
        },
        {
          "id": "MDRW2100036759.1.17",
          "form": "인간적으로 물은 마셔야할거같아요ㅜㅜ",
          "original_form": "인간적으로 물은 마셔야할거같아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 13:01"
        },
        {
          "id": "MDRW2100036759.1.18",
          "form": "와 이게  뭐라고 이렇게 괴롭지",
          "original_form": "와 이게  뭐라고 이렇게 괴롭지",
          "speaker_id": "1",
          "time": "20210831 13:01"
        },
        {
          "id": "MDRW2100036759.1.19",
          "form": "이날씨에 수분 보충은 되야져 ㅠㅠ 물드세여 ㅠㅠ",
          "original_form": "이날씨에 수분 보충은 되야져 ㅠㅠ 물드세여 ㅠㅠ",
