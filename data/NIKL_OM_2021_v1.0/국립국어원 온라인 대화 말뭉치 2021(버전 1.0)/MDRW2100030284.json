      "utterance": [
        {
          "id": "MDRW2100030284.1.1",
          "form": "아 맞다",
          "original_form": "아 맞다",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030284.1.2",
          "form": "진짜로 과제 한다",
          "original_form": "진짜로 과제 한다",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030284.1.3",
          "form": "선배님들 졸업 선물로",
          "original_form": "선배님들 졸업 선물로",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030284.1.4",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030284.1.5",
          "form": "각인 텀블러 준다고?",
          "original_form": "각인 텀블러 준다고?",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030284.1.6",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030284.1.7",
          "form": "1만원~2만원정도 모아서",
          "original_form": "1만원~2만원정도 모아서",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030284.1.8",
          "form": "26기랑 27기랑",
          "original_form": "26기랑 27기랑",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030284.1.9",
          "form": "응응 어떤 문구 써?",
          "original_form": "응응 어떤 문구 써?",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030284.1.10",
          "form": "아직 그것까진...",
          "original_form": "아직 그것까진...",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030284.1.11",
          "form": "정하진 않았어",
          "original_form": "정하진 않았어",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030284.1.12",
          "form": "인당 1~2만원이야??",
          "original_form": "인당 1~2만원이야??",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030284.1.13",
          "form": "우리 동아리가 조금 일찍 정했긴 했음",
          "original_form": "우리 동아리가 조금 일찍 정했긴 했음",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030284.1.14",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030284.1.15",
          "form": "막 작년에는 패딩도 나오고",
          "original_form": "막 작년에는 패딩도 나오고",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030284.1.16",
          "form": "실제로 이번에도 조끼패딩도 나오고",
          "original_form": "실제로 이번에도 조끼패딩도 나오고",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030284.1.17",
          "form": "아하 와..패딩 비쌀텐데",
          "original_form": "아하 와..패딩 비쌀텐데",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030284.1.18",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030284.1.19",
          "form": "동아리회비가 있어서",
          "original_form": "동아리회비가 있어서",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030284.1.20",
          "form": "매년 100만원씩 지급되는 걸로 알고있어",
          "original_form": "매년 100만원씩 지급되는 걸로 알고있어",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030284.1.21",
          "form": "근데 코로나때문에 만날 일이 없어서",
          "original_form": "근데 코로나때문에 만날 일이 없어서",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030284.1.22",
          "form": "회비가 남아돌아",
          "original_form": "회비가 남아돌아",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030284.1.23",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030284.1.24",
          "form": "와..동아리 지원 빵빵하게 해준다..부럽네ㅜㅠㅠ",
          "original_form": "와..동아리 지원 빵빵하게 해준다..부럽네ㅜㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030284.1.25",
          "form": "맨날 아이스크림 사먹고 과자 사먹고",
          "original_form": "맨날 아이스크림 사먹고 과자 사먹고",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030284.1.26",
          "form": "우리 동아리는 고작 10인거 20이야...",
          "original_form": "우리 동아리는 고작 10인거 20이야...",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030284.1.27",
          "form": "우리 동아리가 조금 커서",
          "original_form": "우리 동아리가 조금 커서",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030284.1.28",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030284.1.29",
          "form": "운영하기 힘들겠다ㅠㅠ",
          "original_form": "운영하기 힘들겠다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030284.1.30",
          "form": "할 수 잇눈게 없어...디자인부가 맨투맨 뽑으면",
          "original_form": "할 수 잇눈게 없어...디자인부가 맨투맨 뽑으면",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030284.1.31",
          "form": "인터랙트가 우리 학교만 있는 동아리가 아니고",
          "original_form": "인터랙트가 우리 학교만 있는 동아리가 아니고",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030284.1.32",
          "form": "연합 동아리여서",
          "original_form": "연합 동아리여서",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030284.1.33",
          "form": "우리 일러북 못 뽑음...",
          "original_form": "우리 일러북 못 뽑음...",
          "speaker_id": "1",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030284.1.34",
          "form": "조금 더 주는 걸 수도 있어",
          "original_form": "조금 더 주는 걸 수도 있어",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030284.1.35",
          "form": "ㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030284.1.36",
          "form": "와... 부러워ㅜㅜㅜ",
          "original_form": "와... 부러워ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030284.1.37",
          "form": "어떡해ㅠㅠㅠ",
          "original_form": "어떡해ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 23:33"
        }
      ]
