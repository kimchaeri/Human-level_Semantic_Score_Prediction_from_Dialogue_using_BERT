      "utterance": [
        {
          "id": "MDRW2100031634.1.1",
          "form": "게임 하시나요?",
          "original_form": "게임 하시나요?",
          "speaker_id": "1",
          "time": "20210904 23:16"
        },
        {
          "id": "MDRW2100031634.1.2",
          "form": "저는 모바일게임만 종종 해요!",
          "original_form": "저는 모바일게임만 종종 해요!",
          "speaker_id": "2",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031634.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031634.1.4",
          "form": "아 저도요 모바일만",
          "original_form": "아 저도요 모바일만",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031634.1.5",
          "form": "어떤 게임하세요?",
          "original_form": "어떤 게임하세요?",
          "speaker_id": "1",
          "time": "20210904 23:23"
        },
        {
          "id": "MDRW2100031634.1.6",
          "form": "종종 배그해요!",
          "original_form": "종종 배그해요!",
          "speaker_id": "2",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031634.1.7",
          "form": "저처럼 손느린사람은 못하는 게임이네요 ㅎ",
          "original_form": "저처럼 손느린사람은 못하는 게임이네요 ㅎ",
          "speaker_id": "1",
          "time": "20210904 23:26"
        },
        {
          "id": "MDRW2100031634.1.8",
          "form": "저는 그냥 숨어있어요ㅋㅋㅋ무서워서",
          "original_form": "저는 그냥 숨어있어요ㅋㅋㅋ무서워서",
          "speaker_id": "2",
          "time": "20210904 23:27"
        },
        {
          "id": "MDRW2100031634.1.9",
          "form": "ㅎㅎㅎ 그것도 전략이죠",
          "original_form": "ㅎㅎㅎ 그것도 전략이죠",
          "speaker_id": "1",
          "time": "20210904 23:28"
        },
        {
          "id": "MDRW2100031634.1.10",
          "form": "하다가 사람보면 심장마비 걸릴거 같긴해요ㅋㅋㅋ",
          "original_form": "하다가 사람보면 심장마비 걸릴거 같긴해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100031634.1.11",
          "form": "그래도 흥미진진한가봐요 ㅎㅎㅎ",
          "original_form": "그래도 흥미진진한가봐요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 23:31"
        },
        {
          "id": "MDRW2100031634.1.12",
          "form": "쓰릴을 느끼고 싶을때 종종 켜요ㅋㅋ",
          "original_form": "쓰릴을 느끼고 싶을때 종종 켜요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031634.1.13",
          "form": "ㅋㅋㅋㅋㅋ 스릴 좋죠",
          "original_form": "ㅋㅋㅋㅋㅋ 스릴 좋죠",
          "speaker_id": "1",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031634.1.14",
          "form": "저는 혼자서 하는게임만 주로해요",
          "original_form": "저는 혼자서 하는게임만 주로해요",
          "speaker_id": "1",
          "time": "20210904 23:44"
        },
        {
          "id": "MDRW2100031634.1.15",
          "form": "게임 못해서요ㅜ",
          "original_form": "게임 못해서요ㅜ",
          "speaker_id": "1",
          "time": "20210904 23:45"
        },
        {
          "id": "MDRW2100031634.1.16",
          "form": "뭐뭐해보셨어요?",
          "original_form": "뭐뭐해보셨어요?",
          "speaker_id": "2",
          "time": "20210904 23:45"
        },
        {
          "id": "MDRW2100031634.1.17",
          "form": "저는 캐주얼게임이나 방탈출게임같은거요",
          "original_form": "저는 캐주얼게임이나 방탈출게임같은거요",
