      "utterance": [
        {
          "id": "MDRW2100043032.1.1",
          "form": "연휴전날 바로 본가가세요?",
          "original_form": "연휴전날 바로 본가가세요?",
          "speaker_id": "1",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100043032.1.2",
          "form": "아마 토요일에 갈 것 같습니다ㅋㅋㄱ",
          "original_form": "아마 토요일에 갈 것 같습니다ㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043032.1.3",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043032.1.4",
          "form": "거리가얼마나되나용?",
          "original_form": "거리가얼마나되나용?",
          "speaker_id": "1",
          "time": "20210907 13:36"
        },
        {
          "id": "MDRW2100043032.1.5",
          "form": "가고싶지않습니다최대한늦게가구싶습니다",
          "original_form": "가고싶지않습니다최대한늦게가구싶습니다",
          "speaker_id": "2",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043032.1.6",
          "form": "동감",
          "original_form": "동감",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043032.1.7",
          "form": "완전",
          "original_form": "완전",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043032.1.8",
          "form": "동감이에요",
          "original_form": "동감이에요",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043032.1.9",
          "form": "하....",
          "original_form": "하....",
          "speaker_id": "1",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043032.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ휴..주말에붙으니 이거 피할방법이..ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ휴..주말에붙으니 이거 피할방법이..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:37"
        },
        {
          "id": "MDRW2100043032.1.11",
          "form": "저는 음 한 일박이일정도만",
          "original_form": "저는 음 한 일박이일정도만",
          "speaker_id": "1",
          "time": "20210907 13:38"
        },
        {
          "id": "MDRW2100043032.1.12",
          "form": "잇다가올꺼에요",
          "original_form": "잇다가올꺼에요",
          "speaker_id": "1",
          "time": "20210907 13:38"
        },
        {
          "id": "MDRW2100043032.1.13",
          "form": "혼자쉬고싶어요.....",
          "original_form": "혼자쉬고싶어요.....",
          "speaker_id": "1",
          "time": "20210907 13:38"
        },
        {
          "id": "MDRW2100043032.1.14",
          "form": "어짜피 근처라서 자주 소환 당하니까....",
          "original_form": "어짜피 근처라서 자주 소환 당하니까....",
          "speaker_id": "1",
          "time": "20210907 13:38"
        },
        {
          "id": "MDRW2100043032.1.15",
          "form": "오ㅋㅋㅋㅋ괜찮네요 가볍게떠나있기",
          "original_form": "오ㅋㅋㅋㅋ괜찮네요 가볍게떠나있기",
          "speaker_id": "2",
          "time": "20210907 16:18"
        },
        {
          "id": "MDRW2100043032.1.16",
          "form": "퇴근하셧어욤?",
          "original_form": "퇴근하셧어욤?",
          "speaker_id": "1",
          "time": "20210907 16:20"
        },
        {
          "id": "MDRW2100043032.1.17",
          "form": "네 하구있습니당! ㅋㅋㅋ 비가내렸다안내렸다  \r\n제 빨래 잘 말라야하는데ㅜㅜㅜ",
          "original_form": "네 하구있습니당! ㅋㅋㅋ 비가내렸다안내렸다  \r\n제 빨래 잘 말라야하는데ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 16:23"
        },
        {
          "id": "MDRW2100043032.1.18",
          "form": "ㅜㅜ언능 에어컨 제습모드 고고하세요",
          "original_form": "ㅜㅜ언능 에어컨 제습모드 고고하세요",
          "speaker_id": "1",
          "time": "20210907 16:26"
        },
        {
          "id": "MDRW2100043032.1.19",
          "form": "저희집 에어컨 너무옛날꺼고고장나서 제습모드안돼요 흑흑ㅋㅋㅋㅋ그냥켜두는거뿐..흑흑흑",
          "original_form": "저희집 에어컨 너무옛날꺼고고장나서 제습모드안돼요 흑흑ㅋㅋㅋㅋ그냥켜두는거뿐..흑흑흑",
          "speaker_id": "2",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100043032.1.20",
          "form": "ㅋㄱㅈㅋㅈㅋㅈㄱㅈㅋ",
          "original_form": "ㅋㄱㅈㅋㅈㅋㅈㄱㅈㅋ",
          "speaker_id": "2",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100043032.1.21",
          "form": "이정도면그냥하나사라 싶은 에어컨",
          "original_form": "이정도면그냥하나사라 싶은 에어컨",
          "speaker_id": "2",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100043032.1.22",
          "form": "ㄱㅋㅈㅋㄱㅈㄱㅈㅋㅈㄱ",
          "original_form": "ㄱㅋㅈㅋㄱㅈㄱㅈㅋㅈㄱ",
          "speaker_id": "2",
          "time": "20210907 16:29"
        },
        {
          "id": "MDRW2100043032.1.23",
          "form": "앗 ㅋㅋ그래도 에어컨 시원하기만하면 되죱",
          "original_form": "앗 ㅋㅋ그래도 에어컨 시원하기만하면 되죱",
          "speaker_id": "1",
          "time": "20210907 16:30"
        },
        {
          "id": "MDRW2100043032.1.24",
          "form": "네ㅋㅋㅋㅋ그걸로만족하면서씁니다ㅋㅋㄲㅋ",
          "original_form": "네ㅋㅋㅋㅋ그걸로만족하면서씁니다ㅋㅋㄲㅋ",
