      "utterance": [
        {
          "id": "MDRW2100018086.1.1",
          "form": "음 샐러드 좋아하는데 맛있는 소스가 듬뿍 있어야 해요 ㅋㅋㅋㅋ",
          "original_form": "음 샐러드 좋아하는데 맛있는 소스가 듬뿍 있어야 해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018086.1.2",
          "form": "과일 좋아해요 복숭아 멜론 자두 이런거요",
          "original_form": "과일 좋아해요 복숭아 멜론 자두 이런거요",
          "speaker_id": "2",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018086.1.3",
          "form": "ㅠㅠ 그냥 상추 같은거 씹아먹음 맛있 잖아여..",
          "original_form": "ㅠㅠ 그냥 상추 같은거 씹아먹음 맛있 잖아여..",
          "speaker_id": "1",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018086.1.4",
          "form": "파프리카는 뭐 과일수준이고..",
          "original_form": "파프리카는 뭐 과일수준이고..",
          "speaker_id": "1",
          "time": "20210826 22:29"
        },
        {
          "id": "MDRW2100018086.1.5",
          "form": "헉 충격 맛있다구여? 그냥 상추가여?",
          "original_form": "헉 충격 맛있다구여? 그냥 상추가여?",
          "speaker_id": "2",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100018086.1.6",
          "form": "파프리카는 카레에 넣으면 맛있어요",
          "original_form": "파프리카는 카레에 넣으면 맛있어요",
          "speaker_id": "2",
          "time": "20210826 22:30"
        },
        {
          "id": "MDRW2100018086.1.7",
          "form": "배고프면 오이 까먹고 그러지 않아영?",
          "original_form": "배고프면 오이 까먹고 그러지 않아영?",
          "speaker_id": "1",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100018086.1.8",
          "form": "헉 저희 가족은 그러던데 저는..",
          "original_form": "헉 저희 가족은 그러던데 저는..",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100018086.1.9",
          "form": "배고프면 과자나 단거 찾아먹어요",
          "original_form": "배고프면 과자나 단거 찾아먹어요",
          "speaker_id": "2",
          "time": "20210826 22:31"
        },
        {
          "id": "MDRW2100018086.1.10",
          "form": "이제보니 무조건 단맛이 들어간걸 좋아하시네여..",
          "original_form": "이제보니 무조건 단맛이 들어간걸 좋아하시네여..",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018086.1.11",
          "form": "혹시.. 백종원이세요? ㅋㅋㅋㅋ",
          "original_form": "혹시.. 백종원이세요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018086.1.12",
          "form": "헉 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "헉 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018086.1.13",
          "form": "차라리 백종원이면 만들어먹을 수 있어서 좋겠네요",
          "original_form": "차라리 백종원이면 만들어먹을 수 있어서 좋겠네요",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100018086.1.14",
          "form": "주로 배달?",
          "original_form": "주로 배달?",
          "speaker_id": "1",
          "time": "20210826 22:33"
        },
        {
          "id": "MDRW2100018086.1.15",
          "form": "네 ㅋㅋㅋ 사먹거나 배달이요 줄여야하는데 ㅠ",
          "original_form": "네 ㅋㅋㅋ 사먹거나 배달이요 줄여야하는데 ㅠ",
          "speaker_id": "2",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018086.1.16",
          "form": "배민 브이아이피신가여?",
          "original_form": "배민 브이아이피신가여?",
          "speaker_id": "1",
          "time": "20210826 22:34"
        },
        {
          "id": "MDRW2100018086.1.17",
          "form": "아닌거같아요 포인트가 항상 없더라구여 ㅠ",
          "original_form": "아닌거같아요 포인트가 항상 없더라구여 ㅠ",
          "speaker_id": "2",
          "time": "20210826 22:35"
        },
        {
          "id": "MDRW2100018086.1.18",
          "form": "ㅋㅋㅋ 야식은 뭐 좋아하세여ㅡ",
          "original_form": "ㅋㅋㅋ 야식은 뭐 좋아하세여ㅡ",
          "speaker_id": "1",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100018086.1.19",
          "form": "음 야식도 역시..달달한",
          "original_form": "음 야식도 역시..달달한",
          "speaker_id": "2",
          "time": "20210826 22:36"
        },
        {
          "id": "MDRW2100018086.1.20",
          "form": "빵이 좋아요",
          "original_form": "빵이 좋아요",
          "speaker_id": "2",
