      "utterance": [
        {
          "id": "MDRW2100005776.1.1",
          "form": "좀전에 왜 안오냐고 전화옴",
          "original_form": "좀전에 왜 안오냐고 전화옴",
          "speaker_id": "2",
          "time": "20210907 11:34"
        },
        {
          "id": "MDRW2100005776.1.2",
          "form": "왜케 재촉? ㅋㅋㅋㅋㅋ",
          "original_form": "왜케 재촉? ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:35"
        },
        {
          "id": "MDRW2100005776.1.3",
          "form": "기다리라고하셈",
          "original_form": "기다리라고하셈",
          "speaker_id": "1",
          "time": "20210907 11:35"
        },
        {
          "id": "MDRW2100005776.1.4",
          "form": "현장 밥먹으러 가야한다고 빨리 해달라함ㅋㅋㅋ",
          "original_form": "현장 밥먹으러 가야한다고 빨리 해달라함ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:35"
        },
        {
          "id": "MDRW2100005776.1.5",
          "form": "후",
          "original_form": "후",
          "speaker_id": "2",
          "time": "20210907 11:35"
        },
        {
          "id": "MDRW2100005776.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:35"
        },
        {
          "id": "MDRW2100005776.1.7",
          "form": "저희도 곧 밥먹는데요 ㅡㅡ",
          "original_form": "저희도 곧 밥먹는데요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210907 11:36"
        },
        {
          "id": "MDRW2100005776.1.8",
          "form": "우리부서가 힘이 없음ㅠ",
          "original_form": "우리부서가 힘이 없음ㅠ",
          "speaker_id": "2",
          "time": "20210907 11:37"
        },
        {
          "id": "MDRW2100005776.1.9",
          "form": "부서 윗대,가리가 누구임",
          "original_form": "부서 윗대,가리가 누구임",
          "speaker_id": "1",
          "time": "20210907 11:39"
        },
        {
          "id": "MDRW2100005776.1.10",
          "form": "그사람 파워가없나봄",
          "original_form": "그사람 파워가없나봄",
          "speaker_id": "1",
          "time": "20210907 11:39"
        },
        {
          "id": "MDRW2100005776.1.11",
          "form": "ㅇㅇ 우리 차장님ㅋㅋㅋ",
          "original_form": "ㅇㅇ 우리 차장님ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:39"
        },
        {
          "id": "MDRW2100005776.1.12",
          "form": "우리팀은 젊은 팀이라ㅋㅋ",
          "original_form": "우리팀은 젊은 팀이라ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:40"
        },
        {
          "id": "MDRW2100005776.1.13",
          "form": "아 그럼 진짜어쩔수가없네",
          "original_form": "아 그럼 진짜어쩔수가없네",
          "speaker_id": "1",
          "time": "20210907 11:40"
        },
        {
          "id": "MDRW2100005776.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:40"
        },
        {
          "id": "MDRW2100005776.1.15",
          "form": "대신 터치가 거의 없음ㅋㅋ",
          "original_form": "대신 터치가 거의 없음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 11:40"
        },
        {
          "id": "MDRW2100005776.1.16",
          "form": "내할거만,하면 댐",
          "original_form": "내할거만,하면 댐",
          "speaker_id": "2",
          "time": "20210907 11:40"
        },
        {
          "id": "MDRW2100005776.1.17",
          "form": "일도 나름 자유롭고?? 말이 이상한데",
          "original_form": "일도 나름 자유롭고?? 말이 이상한데",
          "speaker_id": "2",
          "time": "20210907 11:41"
        },
        {
          "id": "MDRW2100005776.1.18",
          "form": "저도하긴 그런듯",
          "original_form": "저도하긴 그런듯",
          "speaker_id": "1",
          "time": "20210907 11:41"
        },
        {
          "id": "MDRW2100005776.1.19",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:41"
        },
        {
          "id": "MDRW2100005776.1.20",
          "form": "머리만 잘 굴리면 되요",
          "original_form": "머리만 잘 굴리면 되요",
          "speaker_id": "2",
          "time": "20210907 11:43"
        },
        {
          "id": "MDRW2100005776.1.21",
          "form": "주말에도 거의 안나오고",
          "original_form": "주말에도 거의 안나오고",
          "speaker_id": "2",
          "time": "20210907 11:43"
        },
        {
          "id": "MDRW2100005776.1.22",
          "form": "저는 오늘 운영위원회 준비하라고",
          "original_form": "저는 오늘 운영위원회 준비하라고",
          "speaker_id": "1",
          "time": "20210907 11:43"
        },
        {
          "id": "MDRW2100005776.1.23",
          "form": "추경하래요",
          "original_form": "추경하래요",
          "speaker_id": "1",
          "time": "20210907 11:43"
        },
        {
          "id": "MDRW2100005776.1.24",
          "form": "윽 하기싫어",
          "original_form": "윽 하기싫어",
          "speaker_id": "1",
          "time": "20210907 11:44"
        },
        {
          "id": "MDRW2100005776.1.25",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210907 11:44"
        },
        {
          "id": "MDRW2100005776.1.26",
          "form": "대신해줄 조대리구함",
          "original_form": "대신해줄 조대리구함",
          "speaker_id": "1",
          "time": "20210907 11:44"
        },
        {
          "id": "MDRW2100005776.1.27",
          "form": "추경은 조은거아님?",
          "original_form": "추경은 조은거아님?",
          "speaker_id": "2",
          "time": "20210907 11:44"
        },
        {
          "id": "MDRW2100005776.1.28",
          "form": "저 모름",
          "original_form": "저 모름",
          "speaker_id": "2",
          "time": "20210907 11:44"
        },
        {
          "id": "MDRW2100005776.1.29",
          "form": "ㅅㄱㅇ",
          "original_form": "ㅅㄱㅇ",
          "speaker_id": "2",
          "time": "20210907 11:44"
        },
        {
          "id": "MDRW2100005776.1.30",
          "form": "예산 다시짠다고해야하나",
          "original_form": "예산 다시짠다고해야하나",
          "speaker_id": "1",
          "time": "20210907 11:44"
        },
        {
          "id": "MDRW2100005776.1.31",
          "form": "뉴스에서 추가경정예산으로 뭐 재난지원금 얼마 해놨따",
          "original_form": "뉴스에서 추가경정예산으로 뭐 재난지원금 얼마 해놨따",
          "speaker_id": "1",
          "time": "20210907 11:45"
        },
        {
          "id": "MDRW2100005776.1.32",
          "form": "이런거 많이말하는데",
          "original_form": "이런거 많이말하는데",
          "speaker_id": "1",
          "time": "20210907 11:45"
        },
        {
          "id": "MDRW2100005776.1.33",
          "form": "추석선물세트 예산 좋게 ㄱㄱ",
          "original_form": "추석선물세트 예산 좋게 ㄱㄱ",
          "speaker_id": "2",
          "time": "20210907 11:45"
        },
        {
          "id": "MDRW2100005776.1.34",
          "form": "여태까지 돈 얼마썻나 그리고 얼마남았나 계산하는거임 ㅋㅋㅋㅋ",
          "original_form": "여태까지 돈 얼마썻나 그리고 얼마남았나 계산하는거임 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:46"
        },
        {
          "id": "MDRW2100005776.1.35",
          "form": "남은거 앞으로 어따쓸건지 계획하고",
          "original_form": "남은거 앞으로 어따쓸건지 계획하고",
          "speaker_id": "1",
          "time": "20210907 11:46"
        },
        {
          "id": "MDRW2100005776.1.36",
          "form": "단위가 억이 넘어가니까 힘들어짐",
          "original_form": "단위가 억이 넘어가니까 힘들어짐",
          "speaker_id": "1",
          "time": "20210907 11:46"
        },
        {
          "id": "MDRW2100005776.1.37",
          "form": "아 뭐여.. 재고조사였네요",
          "original_form": "아 뭐여.. 재고조사였네요",
          "speaker_id": "2",
          "time": "20210907 11:46"
        },
        {
          "id": "MDRW2100005776.1.38",
          "form": "재고조사라니 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "재고조사라니 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:47"
        },
        {
          "id": "MDRW2100005776.1.39",
          "form": "돈 재고조사",
          "original_form": "돈 재고조사",
          "speaker_id": "2",
          "time": "20210907 11:48"
        },
        {
          "id": "MDRW2100005776.1.40",
          "form": "ㅇㅋ ㅇㅈ",
          "original_form": "ㅇㅋ ㅇㅈ",
          "speaker_id": "1",
          "time": "20210907 11:49"
        },
        {
          "id": "MDRW2100005776.1.41",
          "form": "돈이라서 계산잘못하믄안댐",
          "original_form": "돈이라서 계산잘못하믄안댐",
          "speaker_id": "1",
          "time": "20210907 11:49"
        },
        {
          "id": "MDRW2100005776.1.42",
          "form": "1원도 그래서 문제",
          "original_form": "1원도 그래서 문제",
          "speaker_id": "1",
          "time": "20210907 11:49"
        },
        {
          "id": "MDRW2100005776.1.43",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 11:49"
        },
        {
          "id": "MDRW2100005776.1.44",
          "form": "윽 하기시러",
          "original_form": "윽 하기시러",
          "speaker_id": "1",
          "time": "20210907 11:49"
        },
        {
          "id": "MDRW2100005776.1.45",
          "form": "월급으로 메워야죠뭐",
          "original_form": "월급으로 메워야죠뭐",
          "speaker_id": "2",
          "time": "20210907 11:49"
        },
        {
          "id": "MDRW2100005776.1.46",
          "form": "돈남으면 월급올릴텐데 안남겟죠?",
          "original_form": "돈남으면 월급올릴텐데 안남겟죠?",
          "speaker_id": "1",
          "time": "20210907 11:49"
        },
        {
          "id": "MDRW2100005776.1.47",
          "form": "최저시급 인상률만큼은 올려야죠",
          "original_form": "최저시급 인상률만큼은 올려야죠",
          "speaker_id": "2",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100005776.1.48",
          "form": "포케값도 오르는 마당에",
          "original_form": "포케값도 오르는 마당에",
          "speaker_id": "2",
          "time": "20210907 11:50"
        },
        {
          "id": "MDRW2100005776.1.49",
          "form": "원래 사회복지가 최저받고 일하는수준이라,,",
          "original_form": "원래 사회복지가 최저받고 일하는수준이라,,",
          "speaker_id": "1",
          "time": "20210907 11:51"
        },
        {
          "id": "MDRW2100005776.1.50",
          "form": "근데 그거보단 많이받았음 전부터",
          "original_form": "근데 그거보단 많이받았음 전부터",
          "speaker_id": "1",
          "time": "20210907 11:51"
        },
        {
          "id": "MDRW2100005776.1.51",
          "form": "제 세.금 잘 써주세요",
          "original_form": "제 세.금 잘 써주세요",
          "speaker_id": "2",
          "time": "20210907 11:53"
        },
        {
          "id": "MDRW2100005776.1.52",
          "form": "참나",
          "original_form": "참나",
          "speaker_id": "2",
          "time": "20210907 11:53"
        },
        {
          "id": "MDRW2100005776.1.53",
          "form": "3금도 금지어네",
          "original_form": "3금도 금지어네",
          "speaker_id": "2",
          "time": "20210907 11:53"
        },
        {
          "id": "MDRW2100005776.1.54",
          "form": "이미 글러먹었음 ㅋㅋㅋㅈㅅ",
          "original_form": "이미 글러먹었음 ㅋㅋㅋㅈㅅ",
          "speaker_id": "1",
          "time": "20210907 11:55"
        },
        {
          "id": "MDRW2100005776.1.55",
          "form": "하",
          "original_form": "하",
          "speaker_id": "2",
          "time": "20210907 11:56"
        },
        {
          "id": "MDRW2100005776.1.56",
          "form": "요새 3금 미쳤어요",
          "original_form": "요새 3금 미쳤어요",
          "speaker_id": "2",
          "time": "20210907 11:56"
        },
        {
          "id": "MDRW2100005776.1.57",
          "form": "2~3년째 실수령이 그대로임",
          "original_form": "2~3년째 실수령이 그대로임",
          "speaker_id": "2",
          "time": "20210907 11:56"
        },
        {
          "id": "MDRW2100005776.1.58",
          "form": "맛점하세요",
          "original_form": "맛점하세요",
          "speaker_id": "2",
          "time": "20210907 12:10"
        },
        {
          "id": "MDRW2100005776.1.59",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100005776.1.60",
          "form": "3금인정,,,",
          "original_form": "3금인정,,,",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100005776.1.61",
          "form": "맛점ㅎㅏ셨나요",
          "original_form": "맛점ㅎㅏ셨나요",
          "speaker_id": "1",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100005776.1.62",
          "form": "그냥저냥 뭐 회사밥이 다 그렇죠",
          "original_form": "그냥저냥 뭐 회사밥이 다 그렇죠",
          "speaker_id": "2",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100005776.1.63",
          "form": "메뉴뭐임?",
          "original_form": "메뉴뭐임?",
          "speaker_id": "1",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100005776.1.64",
          "form": "오늘은",
          "original_form": "오늘은",
          "speaker_id": "2",
          "time": "20210907 13:35"
        },
        {
          "id": "MDRW2100005776.1.65",
          "form": "닭도리탕 깻잎장아찌 김이랑부추무친이상한거랑 미역국이랑 콩나물무침",
