      "utterance": [
        {
          "id": "MDRW2100009295.1.1",
          "form": "김밥 귀찮은데 그냥 사먹지",
          "original_form": "김밥 귀찮은데 그냥 사먹지",
          "speaker_id": "2",
          "time": "20210822 22:36"
        },
        {
          "id": "MDRW2100009295.1.2",
          "form": "얼마전에 김밥집 집단 식중독 기사보니 불안해서 ㅎㅎ",
          "original_form": "얼마전에 김밥집 집단 식중독 기사보니 불안해서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009295.1.3",
          "form": "겁도 많아 ㅎㅎ",
          "original_form": "겁도 많아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009295.1.4",
          "form": "계란이랑 상온에 놓고 계속 싸면 안좋다고 하니 그냥 내가 싸지 뭐 ㅋ",
          "original_form": "계란이랑 상온에 놓고 계속 싸면 안좋다고 하니 그냥 내가 싸지 뭐 ㅋ",
          "speaker_id": "1",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009295.1.5",
          "form": "손이 많이 가니까 그렇지",
          "original_form": "손이 많이 가니까 그렇지",
          "speaker_id": "2",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009295.1.6",
          "form": "그래도 재료 3~4줄 가격이면 10줄은 만드니까 ㅎㅎ",
          "original_form": "그래도 재료 3~4줄 가격이면 10줄은 만드니까 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100009295.1.7",
          "form": "ㅎㅎ 마음 편한 대로 햐",
          "original_form": "ㅎㅎ 마음 편한 대로 햐",
          "speaker_id": "2",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100009295.1.8",
          "form": "계란이랑 직접 지단 부쳐서 김밥 먹어야겠다. 우동도 같이 ㅎㅎ",
          "original_form": "계란이랑 직접 지단 부쳐서 김밥 먹어야겠다. 우동도 같이 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100009295.1.9",
          "form": "우동도 만들게?",
          "original_form": "우동도 만들게?",
          "speaker_id": "2",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009295.1.10",
          "form": "ㅇㅇ 김밥하고 같이 먹으면 맛있을 것 같아",
          "original_form": "ㅇㅇ 김밥하고 같이 먹으면 맛있을 것 같아",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009295.1.11",
          "form": "좋네~ 최고의 궁합이지 ㅎㅎ",
          "original_form": "좋네~ 최고의 궁합이지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009295.1.12",
          "form": "집에서 만들면 아무 재료나 넣어도 맛있지 뭐 ㅎㅎ",
          "original_form": "집에서 만들면 아무 재료나 넣어도 맛있지 뭐 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009295.1.13",
          "form": "작은 아들이 맨날 엄마 요리가 최고라잖아",
          "original_form": "작은 아들이 맨날 엄마 요리가 최고라잖아",
          "speaker_id": "2",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009295.1.14",
          "form": "내 최고의 팬 ㅎㅎ",
          "original_form": "내 최고의 팬 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:40"
        },
        {
