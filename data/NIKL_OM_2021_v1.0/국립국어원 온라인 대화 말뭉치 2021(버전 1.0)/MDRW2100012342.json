      "utterance": [
        {
          "id": "MDRW2100012342.1.1",
          "form": "근디...재택근무 일자리가 있긴해요??",
          "original_form": "근디...재택근무 일자리가 있긴해요??",
          "speaker_id": "1",
          "time": "20210824 21:24"
        },
        {
          "id": "MDRW2100012342.1.2",
          "form": "없지ㅋㅇㅋ",
          "original_form": "없지ㅋㅇㅋ",
          "speaker_id": "2",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012342.1.3",
          "form": "재택근무 좋네ㅋㅋ",
          "original_form": "재택근무 좋네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012342.1.4",
          "form": "나도 코로나땜에 비대면",
          "original_form": "나도 코로나땜에 비대면",
          "speaker_id": "2",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012342.1.5",
          "form": "그것도 재택근무네요~",
          "original_form": "그것도 재택근무네요~",
          "speaker_id": "1",
          "time": "20210824 21:26"
        },
        {
          "id": "MDRW2100012342.1.6",
          "form": "언닌 코로나로 비대면 되면 더 좋겠네뇨 ㅋㅋㅋㅋㅋ",
          "original_form": "언닌 코로나로 비대면 되면 더 좋겠네뇨 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012342.1.7",
          "form": "아니야ㅜ 중점은가야되",
          "original_form": "아니야ㅜ 중점은가야되",
          "speaker_id": "2",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012342.1.8",
          "form": "이게더싫음ㅡㅡ",
          "original_form": "이게더싫음ㅡㅡ",
          "speaker_id": "2",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012342.1.9",
          "form": "반반 재택근무ㅡㅡ",
          "original_form": "반반 재택근무ㅡㅡ",
          "speaker_id": "2",
          "time": "20210824 21:27"
        },
        {
          "id": "MDRW2100012342.1.10",
          "form": "아..맞다 이건이거대로 저건저거대로 신경쓰임 ㅋㅋㅋㅋ",
          "original_form": "아..맞다 이건이거대로 저건저거대로 신경쓰임 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:28"
        },
        {
          "id": "MDRW2100012342.1.11",
          "form": "금쪽이 보니 그엄마가 건축설계하던데~",
          "original_form": "금쪽이 보니 그엄마가 건축설계하던데~",
          "speaker_id": "1",
          "time": "20210824 21:29"
        },
        {
          "id": "MDRW2100012342.1.12",
          "form": "재택이더라구요~그런 전문직 아니고는..",
          "original_form": "재택이더라구요~그런 전문직 아니고는..",
          "speaker_id": "1",
          "time": "20210824 21:29"
        },
        {
          "id": "MDRW2100012342.1.13",
          "form": "오ㅋ",
          "original_form": "오ㅋ",
          "speaker_id": "2",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100012342.1.14",
          "form": "금쪽이 보나?",
          "original_form": "금쪽이 보나?",
          "speaker_id": "2",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100012342.1.15",
          "form": "난요즘 잘안보네ㅋㅋㅋ",
          "original_form": "난요즘 잘안보네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100012342.1.16",
          "form": "말고는 뭐 다 블로그 알바고~",
          "original_form": "말고는 뭐 다 블로그 알바고~",
          "speaker_id": "1",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100012342.1.17",
          "form": "재택근무하고 월급도 잘받으면좋지",
          "original_form": "재택근무하고 월급도 잘받으면좋지",
          "speaker_id": "2",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100012342.1.18",
          "form": "저도요~다 엄마탓이라함 ㅋㅋㅋㅋ",
          "original_form": "저도요~다 엄마탓이라함 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:30"
        },
        {
          "id": "MDRW2100012342.1.19",
          "form": "엄마탓 맞다",
          "original_form": "엄마탓 맞다",
          "speaker_id": "2",
          "time": "20210824 21:31"
        },
        {
          "id": "MDRW2100012342.1.20",
          "form": "애들은 잘못없다",
          "original_form": "애들은 잘못없다",
          "speaker_id": "2",
          "time": "20210824 21:31"
        },
        {
          "id": "MDRW2100012342.1.21",
          "form": "ㅋㅋㅋㅋ왜 기질이 그런애들도 있잖아요#",
          "original_form": "ㅋㅋㅋㅋ왜 기질이 그런애들도 있잖아요#",
          "speaker_id": "1",
          "time": "20210824 21:31"
        },
        {
          "id": "MDRW2100012342.1.22",
          "form": "엄마도 사람인데 감당안됨 ㅋㅋ",
          "original_form": "엄마도 사람인데 감당안됨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:31"
        },
        {
          "id": "MDRW2100012342.1.23",
          "form": "그래 우리집도 나가고싶노ㅋㅋ",
          "original_form": "그래 우리집도 나가고싶노ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:31"
        },
        {
          "id": "MDRW2100012342.1.24",
          "form": "그니깐요ㅋㅋㅋ",
          "original_form": "그니깐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012342.1.25",
          "form": "재택근무를 하면서~",
          "original_form": "재택근무를 하면서~",
          "speaker_id": "1",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012342.1.26",
          "form": "사무실늘 하나 얻는거죠 ㅋㅋㅋㅋ",
          "original_form": "사무실늘 하나 얻는거죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012342.1.27",
          "form": "시간에 구애받지않고 장소에 구애받지않고~",
          "original_form": "시간에 구애받지않고 장소에 구애받지않고~",
          "speaker_id": "1",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012342.1.28",
          "form": "ㅋㅋ 미국은 재택근무많이한단다",
          "original_form": "ㅋㅋ 미국은 재택근무많이한단다",
          "speaker_id": "2",
          "time": "20210824 21:32"
        },
        {
          "id": "MDRW2100012342.1.29",
          "form": "근데 진짜 우리나라는 없는듯~",
          "original_form": "근데 진짜 우리나라는 없는듯~",
          "speaker_id": "1",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012342.1.30",
          "form": "주변에 들어보질 못했어요~",
          "original_form": "주변에 들어보질 못했어요~",
          "speaker_id": "1",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100012342.1.31",
          "form": "나돚ㅋㅇㅋ",
          "original_form": "나돚ㅋㅇㅋ",
          "speaker_id": "2",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100012342.1.32",
          "form": "내가 재택근무임ㅋ",
          "original_form": "내가 재택근무임ㅋ",
          "speaker_id": "2",
          "time": "20210824 21:35"
        },
        {
          "id": "MDRW2100012342.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ하고싶네요~블로그 알바 보이스피싱 말고 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ하고싶네요~블로그 알바 보이스피싱 말고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:36"
        }
      ]
