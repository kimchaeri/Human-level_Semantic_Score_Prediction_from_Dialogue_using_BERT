      "utterance": [
        {
          "id": "MDRW2100021289.1.1",
          "form": "저는 게임을 잘 못하고 해봤자 모바일카트 이정도라",
          "original_form": "저는 게임을 잘 못하고 해봤자 모바일카트 이정도라",
          "speaker_id": "2",
          "time": "20210828 17:19"
        },
        {
          "id": "MDRW2100021289.1.2",
          "form": "게임유투브는 안봐봤는데 인기가 정말 많은 장르더라구요",
          "original_form": "게임유투브는 안봐봤는데 인기가 정말 많은 장르더라구요",
          "speaker_id": "2",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100021289.1.3",
          "form": "저는 게임 플레이 영상보다는",
          "original_form": "저는 게임 플레이 영상보다는",
          "speaker_id": "1",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100021289.1.4",
          "form": "분석 영상을 많이봐요",
          "original_form": "분석 영상을 많이봐요",
          "speaker_id": "1",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100021289.1.5",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210828 17:20"
        },
        {
          "id": "MDRW2100021289.1.6",
          "form": "인기많죠...겜하는사람들이 많아서 그런가봐요",
          "original_form": "인기많죠...겜하는사람들이 많아서 그런가봐요",
          "speaker_id": "1",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100021289.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100021289.1.8",
          "form": "그래서 그런지 제가 종종 보던 유투버도 최근에 게임영상 종종 찍더라고요",
          "original_form": "그래서 그런지 제가 종종 보던 유투버도 최근에 게임영상 종종 찍더라고요",
          "speaker_id": "2",
          "time": "20210828 17:21"
        },
        {
          "id": "MDRW2100021289.1.9",
          "form": "그 유투버도 게임을 잘하는 편은 아니라서 맨날 시청자들이랑 같이 게임하고 맨날 지는거같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "그 유투버도 게임을 잘하는 편은 아니라서 맨날 시청자들이랑 같이 게임하고 맨날 지는거같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100021289.1.10",
          "form": "앜ㅋㅋㄲㅋ게임은 못하는것도 컨텐츠니까요ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㄲㅋ게임은 못하는것도 컨텐츠니까요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:22"
        },
        {
          "id": "MDRW2100021289.1.11",
          "form": "드라마 클립은어떤거  보셨나요?",
          "original_form": "드라마 클립은어떤거  보셨나요?",
          "speaker_id": "1",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100021289.1.12",
          "form": "맞아요 ㅋㅋㅋㅋㅋ 너무 웃기더라고요 진짜 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ 너무 웃기더라고요 진짜 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100021289.1.13",
          "form": "저 최근에는 블랙독이 유투브에 뜨길래 자주 보고",
          "original_form": "저 최근에는 블랙독이 유투브에 뜨길래 자주 보고",
          "speaker_id": "2",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100021289.1.14",
          "form": "예능이긴 하지만... 연애의참견도 자주 봐요",
          "original_form": "예능이긴 하지만... 연애의참견도 자주 봐요",
          "speaker_id": "2",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100021289.1.15",
          "form": "연참도 클립으로 보면 참 자극적이죠",
          "original_form": "연참도 클립으로 보면 참 자극적이죠",
          "speaker_id": "1",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100021289.1.16",
          "form": "그맛에 봐요",
          "original_form": "그맛에 봐요",
          "speaker_id": "1",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100021289.1.17",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋ 머 이런 사람이 다 있나 싶은",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋ 머 이런 사람이 다 있나 싶은",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100021289.1.18",
          "form": "근데 항상 현실이 드라마보다 더하다잖아요...",
          "original_form": "근데 항상 현실이 드라마보다 더하다잖아요...",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100021289.1.19",
          "form": "정말 잘 보던 연참 배우가 불륜터졌을때 띠용..",
          "original_form": "정말 잘 보던 연참 배우가 불륜터졌을때 띠용..",
          "speaker_id": "2",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100021289.1.20",
          "form": "헉 누구예요?",
          "original_form": "헉 누구예요?",
          "speaker_id": "1",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100021289.1.21",
          "form": "첨듣는 이야기예요",
          "original_form": "첨듣는 이야기예요",
          "speaker_id": "1",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100021289.1.22",
          "form": "그 연참2 인가 나오던 여자배우",
          "original_form": "그 연참2 인가 나오던 여자배우",
          "speaker_id": "2",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100021289.1.23",
          "form": "사촌네 형부랑 ... 바람나서..",
          "original_form": "사촌네 형부랑 ... 바람나서..",
          "speaker_id": "2",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100021289.1.24",
          "form": "헉.....",
          "original_form": "헉.....",
          "speaker_id": "1",
          "time": "20210828 17:43"
        },
        {
          "id": "MDRW2100021289.1.25",
          "form": "하핫...,",
          "original_form": "하핫...,",
          "speaker_id": "2",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100021289.1.26",
          "form": "최근에 영화 보셨어요?",
          "original_form": "최근에 영화 보셨어요?",
          "speaker_id": "2",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100021289.1.27",
          "form": "블랙위도우 봤어요",
          "original_form": "블랙위도우 봤어요",
          "speaker_id": "1",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100021289.1.28",
          "form": "보셨나요?",
          "original_form": "보셨나요?",
          "speaker_id": "1",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100021289.1.29",
          "form": "보고싶은데 아직 못봤어요ㅠ 재밌나용?",
          "original_form": "보고싶은데 아직 못봤어요ㅠ 재밌나용?",
          "speaker_id": "2",
          "time": "20210828 17:46"
        },
        {
          "id": "MDRW2100021289.1.30",
          "form": "네 저는 오랜만에 너무 잘봤어요",
          "original_form": "네 저는 오랜만에 너무 잘봤어요",
          "speaker_id": "1",
          "time": "20210828 17:46"
        },
        {
          "id": "MDRW2100021289.1.31",
          "form": "킬링타임용 영화로 최고인거 같아요",
          "original_form": "킬링타임용 영화로 최고인거 같아요",
          "speaker_id": "1",
          "time": "20210828 17:47"
        },
        {
          "id": "MDRW2100021289.1.32",
          "form": "배우 좋아하시면 더 좋구요",
          "original_form": "배우 좋아하시면 더 좋구요",
          "speaker_id": "1",
          "time": "20210828 17:47"
        },
        {
          "id": "MDRW2100021289.1.33",
          "form": "다음에 기회되면 봐야겠어요",
          "original_form": "다음에 기회되면 봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 17:47"
        }
      ]
