      "utterance": [
        {
          "id": "MDRW2100037900.1.1",
          "form": "집안일 자주 하세요?",
          "original_form": "집안일 자주 하세요?",
          "speaker_id": "1",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100037900.1.2",
          "form": "1인가구라서 자주 할 수 밖에 없어요ㅋㅋ",
          "original_form": "1인가구라서 자주 할 수 밖에 없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100037900.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100037900.1.4",
          "form": "전 4인 가구요ㅠ",
          "original_form": "전 4인 가구요ㅠ",
          "speaker_id": "1",
          "time": "20210901 20:33"
        },
        {
          "id": "MDRW2100037900.1.5",
          "form": "돌아서는 빨래 돌아서면 밥 돌아서면 청소ㅠ",
          "original_form": "돌아서는 빨래 돌아서면 밥 돌아서면 청소ㅠ",
          "speaker_id": "1",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100037900.1.6",
          "form": "일인가구로 돌아가고 싶어요;",
          "original_form": "일인가구로 돌아가고 싶어요;",
          "speaker_id": "1",
          "time": "20210901 20:34"
        },
        {
          "id": "MDRW2100037900.1.7",
          "form": "혼자 있어도 그건 마찬가지예요ㅋㅋ",
          "original_form": "혼자 있어도 그건 마찬가지예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100037900.1.8",
          "form": "혼자서 다하니 더 벅차요\r\n4명은 분담이 가능하잖아요",
          "original_form": "혼자서 다하니 더 벅차요\r\n4명은 분담이 가능하잖아요",
          "speaker_id": "2",
          "time": "20210901 20:35"
        },
        {
          "id": "MDRW2100037900.1.9",
          "form": "분담할 수가 없네요 애기가 둘이에요ㅠ",
          "original_form": "분담할 수가 없네요 애기가 둘이에요ㅠ",
          "speaker_id": "1",
          "time": "20210901 20:36"
        },
        {
          "id": "MDRW2100037900.1.10",
          "form": "아직 15년은 더 기다려야 할 것 같아요ㅋㅋ",
          "original_form": "아직 15년은 더 기다려야 할 것 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:37"
        },
        {
          "id": "MDRW2100037900.1.11",
          "form": "부부 분담하면 될 것 같아요",
          "original_form": "부부 분담하면 될 것 같아요",
          "speaker_id": "2",
          "time": "20210901 20:38"
        },
        {
          "id": "MDRW2100037900.1.12",
          "form": "신랑은 직업 특성상 새벽에 나가서 저녁 늦게 들어와요~",
          "original_form": "신랑은 직업 특성상 새벽에 나가서 저녁 늦게 들어와요~",
          "speaker_id": "1",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100037900.1.13",
          "form": "아 ... 진정한 독박육아시네요",
          "original_form": "아 ... 진정한 독박육아시네요",
          "speaker_id": "2",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100037900.1.14",
          "form": "대단하세요",
          "original_form": "대단하세요",
          "speaker_id": "2",
          "time": "20210901 20:40"
        },
        {
          "id": "MDRW2100037900.1.15",
          "form": "끝이 있으려나 싶네요;;",
          "original_form": "끝이 있으려나 싶네요;;",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100037900.1.16",
          "form": "어떤 집안일 이제 일하기 싫으세요?",
          "original_form": "어떤 집안일 이제 일하기 싫으세요?",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100037900.1.17",
          "form": "제일~",
          "original_form": "제일~",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100037900.1.18",
          "form": "저는 설거지요ㅋㅋ",
          "original_form": "저는 설거지요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100037900.1.19",
          "form": "먹고 바로하기가 진짜 귀찮아요",
          "original_form": "먹고 바로하기가 진짜 귀찮아요",
          "speaker_id": "2",
          "time": "20210901 20:42"
        },
        {
          "id": "MDRW2100037900.1.20",
          "form": "설거지는 미뤄 둬야 미덕이죠ㅋㅋㅋ",
          "original_form": "설거지는 미뤄 둬야 미덕이죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100037900.1.21",
          "form": "ㅋㅋ 결혼해도 그건 마찬가지군요ㅋㅋ\r\n전 결혼하면 밥먹자마자 하는 줄 알았네요",
          "original_form": "ㅋㅋ 결혼해도 그건 마찬가지군요ㅋㅋ\r\n전 결혼하면 밥먹자마자 하는 줄 알았네요",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100037900.1.22",
          "form": "신혼 때는 그랬던 거 같아요ㅋㅋ",
          "original_form": "신혼 때는 그랬던 거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100037900.1.23",
          "form": "지금은 거의 애들과 같이 있으니",
          "original_form": "지금은 거의 애들과 같이 있으니",
          "speaker_id": "1",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100037900.1.24",
          "form": "가사에 치어사시니 설거지도 쉽지 않나보네요",
          "original_form": "가사에 치어사시니 설거지도 쉽지 않나보네요",
          "speaker_id": "2",
          "time": "20210901 20:44"
        },
        {
          "id": "MDRW2100037900.1.25",
          "form": "몰아서 하게 되네요 ㅋㅋㅋ",
          "original_form": "몰아서 하게 되네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:44"
        }
      ]
    }
