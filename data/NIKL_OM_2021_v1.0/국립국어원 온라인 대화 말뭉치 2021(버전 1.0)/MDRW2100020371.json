      "utterance": [
        {
          "id": "MDRW2100020371.1.1",
          "form": "오 이북",
          "original_form": "오 이북",
          "speaker_id": "2",
          "time": "20210828 02:23"
        },
        {
          "id": "MDRW2100020371.1.2",
          "form": "저는 이북으로는 책 한번읽어봤는데",
          "original_form": "저는 이북으로는 책 한번읽어봤는데",
          "speaker_id": "2",
          "time": "20210828 02:23"
        },
        {
          "id": "MDRW2100020371.1.3",
          "form": "종이책이 더좋더라고요",
          "original_form": "종이책이 더좋더라고요",
          "speaker_id": "2",
          "time": "20210828 02:23"
        },
        {
          "id": "MDRW2100020371.1.4",
          "form": "맞아요! 종이책을 완전 대체할 수는 없더라구요",
          "original_form": "맞아요! 종이책을 완전 대체할 수는 없더라구요",
          "speaker_id": "1",
          "time": "20210828 02:23"
        },
        {
          "id": "MDRW2100020371.1.5",
          "form": "저는 이북리더기가 있어서 그걸로 읽는데",
          "original_form": "저는 이북리더기가 있어서 그걸로 읽는데",
          "speaker_id": "1",
          "time": "20210828 02:23"
        },
        {
          "id": "MDRW2100020371.1.6",
          "form": "보관하기는 편해서ㅠㅠ",
          "original_form": "보관하기는 편해서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 02:24"
        },
        {
          "id": "MDRW2100020371.1.7",
          "form": "그쵸완전",
          "original_form": "그쵸완전",
          "speaker_id": "2",
          "time": "20210828 02:24"
        },
        {
          "id": "MDRW2100020371.1.8",
          "form": "하나에 다들어가니까",
          "original_form": "하나에 다들어가니까",
          "speaker_id": "2",
          "time": "20210828 02:24"
        },
        {
          "id": "MDRW2100020371.1.9",
          "form": "줄치면서 메모하면서 읽기에도 이북이 좋은것같아요",
          "original_form": "줄치면서 메모하면서 읽기에도 이북이 좋은것같아요",
          "speaker_id": "2",
          "time": "20210828 02:24"
        },
        {
          "id": "MDRW2100020371.1.10",
          "form": "아맞아요! 책갈피 기능이 편하긴해요ㅋㅋㅋ",
          "original_form": "아맞아요! 책갈피 기능이 편하긴해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:25"
        },
        {
          "id": "MDRW2100020371.1.11",
          "form": "근데 정가로 따지면 종이책보다 싼지 잘 모르겠고",
          "original_form": "근데 정가로 따지면 종이책보다 싼지 잘 모르겠고",
          "speaker_id": "1",
          "time": "20210828 02:25"
        },
        {
          "id": "MDRW2100020371.1.12",
          "form": "저는 적립금 영끌해서 사거든요... 그래야 좀 살만한..",
          "original_form": "저는 적립금 영끌해서 사거든요... 그래야 좀 살만한..",
          "speaker_id": "1",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020371.1.13",
          "form": "아 이북이 싸진않아요?!",
          "original_form": "아 이북이 싸진않아요?!",
          "speaker_id": "2",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020371.1.14",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020371.1.15",
          "form": "실망스럽네요",
          "original_form": "실망스럽네요",
          "speaker_id": "2",
          "time": "20210828 02:26"
        },
        {
          "id": "MDRW2100020371.1.16",
          "form": "종이책보다 30퍼정도 저렴하다고 하더라고요?",
          "original_form": "종이책보다 30퍼정도 저렴하다고 하더라고요?",
          "speaker_id": "1",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020371.1.17",
          "form": "헉 생각보다 그렇게 싸진않네요,,",
          "original_form": "헉 생각보다 그렇게 싸진않네요,,",
          "speaker_id": "2",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020371.1.18",
          "form": "이북리더기가 있지않는 한 메리트는 없을듯해요",
          "original_form": "이북리더기가 있지않는 한 메리트는 없을듯해요",
          "speaker_id": "1",
          "time": "20210828 02:27"
        },
        {
          "id": "MDRW2100020371.1.19",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020371.1.20",
          "form": "가격으로라도 진입장벽을 낮춰야 많이 유입될텐데 말이죠",
          "original_form": "가격으로라도 진입장벽을 낮춰야 많이 유입될텐데 말이죠",
          "speaker_id": "1",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020371.1.21",
          "form": "그렇군요,,.",
          "original_form": "그렇군요,,.",
          "speaker_id": "2",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020371.1.22",
          "form": "그러네요 진짜",
          "original_form": "그러네요 진짜",
          "speaker_id": "2",
          "time": "20210828 02:28"
        },
        {
          "id": "MDRW2100020371.1.23",
          "form": "가격을 낮춰야할거같아요이북리더기도",
          "original_form": "가격을 낮춰야할거같아요이북리더기도",
          "speaker_id": "2",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100020371.1.24",
          "form": "싼가격이 아니니까",
          "original_form": "싼가격이 아니니까",
          "speaker_id": "2",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100020371.1.25",
          "form": "일단 기계사고 책사려면",
          "original_form": "일단 기계사고 책사려면",
          "speaker_id": "2",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100020371.1.26",
          "form": "그 돈에 종이책 사거나 빌리는 비용이 더 낫죠ㅋㅋㅋㅋㅋ",
          "original_form": "그 돈에 종이책 사거나 빌리는 비용이 더 낫죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:29"
        },
        {
          "id": "MDRW2100020371.1.27",
          "form": "요즘엔 중고서점도 잘되어 있으니까요",
          "original_form": "요즘엔 중고서점도 잘되어 있으니까요",
          "speaker_id": "1",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100020371.1.28",
          "form": "어 중고서점 책 잘해놨더라고요",
          "original_form": "어 중고서점 책 잘해놨더라고요",
          "speaker_id": "2",
          "time": "20210828 02:31"
        },
        {
          "id": "MDRW2100020371.1.29",
          "form": "검수도 빡세게해서 책 상태도 좋던데요ㅋㅋㅋ",
          "original_form": "검수도 빡세게해서 책 상태도 좋던데요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:32"
        }
      ]
    }
  ]
