          "time": "20210826 21:04"
        },
        {
          "id": "MDRW2100017735.1.13",
          "form": "네 좋아요",
          "original_form": "네 좋아요",
          "speaker_id": "1",
          "time": "20210826 22:37"
        },
        {
          "id": "MDRW2100017735.1.14",
          "form": "좋네요 부럽기도 하고요 ㅎㅎ",
          "original_form": "좋네요 부럽기도 하고요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 22:44"
        },
