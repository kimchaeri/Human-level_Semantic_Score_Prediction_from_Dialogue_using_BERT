          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100012904.1.6",
          "form": "습해서 싫어요",
          "original_form": "습해서 싫어요",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100012904.1.7",
          "form": "빨래도 넘 안 말라요",
