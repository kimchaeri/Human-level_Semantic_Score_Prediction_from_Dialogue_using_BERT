        },
        {
          "id": "MDRW2100016170.1.3",
          "form": "식사하셨나요",
          "original_form": "식사하셨나요",
          "speaker_id": "2",
          "time": "20210826 10:38"
        },
        {
          "id": "MDRW2100016170.1.4",
          "form": "네 먹었습니다",
          "original_form": "네 먹었습니다",
          "speaker_id": "1",
          "time": "20210826 10:45"
        },
        {
          "id": "MDRW2100016170.1.5",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210826 10:46"
        },
        {
          "id": "MDRW2100016170.1.6",
          "form": "볶음밥이요!",
          "original_form": "볶음밥이요!",
          "speaker_id": "1",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100016170.1.7",
          "form": "너무 맛잇었겠어요",
          "original_form": "너무 맛잇었겠어요",
          "speaker_id": "2",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100016170.1.8",
          "form": "어떤 볶음밥을 해드셨나요",
          "original_form": "어떤 볶음밥을 해드셨나요",
          "speaker_id": "2",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100016170.1.9",
          "form": "그냥 있는 재료로 대강 하셨나요? 아.. 혹시 배달드신건가",
          "original_form": "그냥 있는 재료로 대강 하셨나요? 아.. 혹시 배달드신건가",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016170.1.10",
          "form": "해먹었습니닿ㅎ",
          "original_form": "해먹었습니닿ㅎ",
          "speaker_id": "1",
          "time": "20210826 10:54"
        },
        {
          "id": "MDRW2100016170.1.11",
          "form": "해드셨구나 ㅎㅎ",
          "original_form": "해드셨구나 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016170.1.12",
          "form": "김치넣고 볶아드셨나요",
          "original_form": "김치넣고 볶아드셨나요",
          "speaker_id": "2",
          "time": "20210826 10:55"
        },
        {
          "id": "MDRW2100016170.1.13",
          "form": "오 맞아요ㅎㅎㅎ",
          "original_form": "오 맞아요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016170.1.14",
          "form": "그럼 햄이나 고기도 넣으셨구요?",
          "original_form": "그럼 햄이나 고기도 넣으셨구요?",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016170.1.15",
          "form": "참치 넣었습니다~",
          "original_form": "참치 넣었습니다~",
          "speaker_id": "1",
          "time": "20210826 11:01"
        },
        {
          "id": "MDRW2100016170.1.16",
          "form": "아~참치김치볶음밥을 해드셨구나",
          "original_form": "아~참치김치볶음밥을 해드셨구나",
          "speaker_id": "2",
          "time": "20210826 11:02"
        },
        {
          "id": "MDRW2100016170.1.17",
          "form": "다른 야채 뭐 넣으셨나요",
          "original_form": "다른 야채 뭐 넣으셨나요",
