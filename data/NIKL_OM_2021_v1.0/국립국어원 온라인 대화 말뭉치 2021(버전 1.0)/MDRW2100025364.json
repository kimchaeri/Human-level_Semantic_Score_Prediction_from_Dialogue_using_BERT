      "utterance": [
        {
          "id": "MDRW2100025364.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210901 10:13"
        },
        {
          "id": "MDRW2100025364.1.2",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210901 10:15"
        },
        {
          "id": "MDRW2100025364.1.3",
          "form": "여행좋아하시나요?",
          "original_form": "여행좋아하시나요?",
          "speaker_id": "2",
          "time": "20210901 10:16"
        },
        {
          "id": "MDRW2100025364.1.4",
          "form": "어디 가고싶으세요?",
          "original_form": "어디 가고싶으세요?",
          "speaker_id": "2",
          "time": "20210901 10:16"
        },
        {
          "id": "MDRW2100025364.1.5",
          "form": "유럽이요",
          "original_form": "유럽이요",
          "speaker_id": "1",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100025364.1.6",
          "form": "가보신적은 있으세요?",
          "original_form": "가보신적은 있으세요?",
          "speaker_id": "2",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100025364.1.7",
          "form": "저도 미국쪽 가보고싶네요 ㅠ",
          "original_form": "저도 미국쪽 가보고싶네요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100025364.1.8",
          "form": "파리요",
          "original_form": "파리요",
          "speaker_id": "1",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025364.1.9",
          "form": "좋더라구요",
          "original_form": "좋더라구요",
          "speaker_id": "1",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025364.1.10",
          "form": "아진짜요? 저는 영어권쪽은 한번도 못가봤어요가까운",
          "original_form": "아진짜요? 저는 영어권쪽은 한번도 못가봤어요가까운",
          "speaker_id": "2",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025364.1.11",
          "form": "중국이나 일본??",
          "original_form": "중국이나 일본??",
          "speaker_id": "2",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025364.1.12",
          "form": "일본도 나름 재미있더라구요",
          "original_form": "일본도 나름 재미있더라구요",
          "speaker_id": "2",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025364.1.13",
          "form": "방사능이걱정이요",
          "original_form": "방사능이걱정이요",
          "speaker_id": "1",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025364.1.14",
          "form": "ㅋㅋㅋㅋㅋ 그런거 걱정한다고 시도 못하면 아쉬울거같아서 저는 다녀왔어요 코로나전에요!",
          "original_form": "ㅋㅋㅋㅋㅋ 그런거 걱정한다고 시도 못하면 아쉬울거같아서 저는 다녀왔어요 코로나전에요!",
          "speaker_id": "2",
          "time": "20210901 10:23"
        },
        {
          "id": "MDRW2100025364.1.15",
          "form": "재미있었오요!",
          "original_form": "재미있었오요!",
          "speaker_id": "2",
          "time": "20210901 10:23"
        },
        {
          "id": "MDRW2100025364.1.16",
          "form": "가면 기념품 같은건 사나요?",
          "original_form": "가면 기념품 같은건 사나요?",
          "speaker_id": "2",
          "time": "20210901 10:23"
        },
        {
          "id": "MDRW2100025364.1.17",
          "form": "에펠탑 흑이이 팔더라구요",
          "original_form": "에펠탑 흑이이 팔더라구요",
          "speaker_id": "1",
          "time": "20210901 10:23"
        },
        {
          "id": "MDRW2100025364.1.18",
          "form": "진짜 돈 많이 모았겠어요 ㅠㅠ그쪽은 좀 비싸자나요",
          "original_form": "진짜 돈 많이 모았겠어요 ㅠㅠ그쪽은 좀 비싸자나요",
          "speaker_id": "2",
          "time": "20210901 10:24"
        },
        {
          "id": "MDRW2100025364.1.19",
          "form": "한반뿐이라",
          "original_form": "한반뿐이라",
          "speaker_id": "1",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100025364.1.20",
          "form": "주로 계획을 짜고 가시나요?",
          "original_form": "주로 계획을 짜고 가시나요?",
          "speaker_id": "2",
          "time": "20210901 10:26"
        },
        {
          "id": "MDRW2100025364.1.21",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210901 11:02"
        }
      ]
