      "utterance": [
        {
          "id": "MDRW2100039377.1.1",
          "form": "몰랑이 좋아하긴 하는데",
          "original_form": "몰랑이 좋아하긴 하는데",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039377.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039377.1.3",
          "form": "즙 뚝뚝 떨어지는 거",
          "original_form": "즙 뚝뚝 떨어지는 거",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039377.1.4",
          "form": "복숭아 과즙이 참 맛있죠",
          "original_form": "복숭아 과즙이 참 맛있죠",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039377.1.5",
          "form": "근데 단단한 복숭아도 좋아해요 아삭거리는 맛",
          "original_form": "근데 단단한 복숭아도 좋아해요 아삭거리는 맛",
          "speaker_id": "2",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039377.1.6",
          "form": "요즘 신비 복숭아가 새로 나왔던데",
          "original_form": "요즘 신비 복숭아가 새로 나왔던데",
          "speaker_id": "1",
          "time": "20210903 16:53"
        },
        {
          "id": "MDRW2100039377.1.7",
          "form": "아 맞아요 먹어 봤어요",
          "original_form": "아 맞아요 먹어 봤어요",
          "speaker_id": "2",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039377.1.8",
          "form": "어땠어요?",
          "original_form": "어땠어요?",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039377.1.9",
          "form": "괜찮았어요 근데 저는 그레이트 복숭아 나 장호원황도",
          "original_form": "괜찮았어요 근데 저는 그레이트 복숭아 나 장호원황도",
          "speaker_id": "2",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039377.1.10",
          "form": "복숭아를 정말 좋아하네요 품종까지 알고",
          "original_form": "복숭아를 정말 좋아하네요 품종까지 알고",
          "speaker_id": "1",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039377.1.11",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210903 16:54"
        },
        {
          "id": "MDRW2100039377.1.12",
          "form": "그레이트 나도 들어본 거 같아요",
          "original_form": "그레이트 나도 들어본 거 같아요",
          "speaker_id": "1",
          "time": "20210903 16:55"
        },
        {
          "id": "MDRW2100039377.1.13",
          "form": "사실 품종도 품종인데 원산지가 중요한 듯해요",
          "original_form": "사실 품종도 품종인데 원산지가 중요한 듯해요",
          "speaker_id": "2",
          "time": "20210903 16:55"
        },
        {
          "id": "MDRW2100039377.1.14",
          "form": "이천이 유명하죠?",
          "original_form": "이천이 유명하죠?",
          "speaker_id": "1",
          "time": "20210903 16:55"
        },
        {
          "id": "MDRW2100039377.1.15",
          "form": "임실도 나름 유명했던 거 같은데",
          "original_form": "임실도 나름 유명했던 거 같은데",
          "speaker_id": "1",
          "time": "20210903 16:55"
        },
        {
          "id": "MDRW2100039377.1.16",
          "form": "아 맞아요 근데 저는 충청도 많이 먹어요 햇사레 브랜드",
          "original_form": "아 맞아요 근데 저는 충청도 많이 먹어요 햇사레 브랜드",
          "speaker_id": "2",
          "time": "20210903 16:56"
        },
        {
          "id": "MDRW2100039377.1.17",
          "form": "맞아요 충주 조치원 세종 여기도 복숭아로 유명하죠",
          "original_form": "맞아요 충주 조치원 세종 여기도 복숭아로 유명하죠",
          "speaker_id": "1",
          "time": "20210903 16:56"
