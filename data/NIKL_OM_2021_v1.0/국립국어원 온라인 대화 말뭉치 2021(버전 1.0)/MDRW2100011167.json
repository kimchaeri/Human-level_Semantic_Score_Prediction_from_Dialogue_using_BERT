      "utterance": [
        {
          "id": "MDRW2100011167.1.1",
          "form": "안녕하세요 게임 좋아하세요?",
          "original_form": "안녕하세요 게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 02:18"
        },
        {
          "id": "MDRW2100011167.1.2",
          "form": "게임 보는 거 좋아해서 스트리밍 자주 봐요!",
          "original_form": "게임 보는 거 좋아해서 스트리밍 자주 봐요!",
          "speaker_id": "2",
          "time": "20210824 02:20"
        },
        {
          "id": "MDRW2100011167.1.3",
          "form": "누구 자주보세요?",
          "original_form": "누구 자주보세요?",
          "speaker_id": "1",
          "time": "20210824 02:31"
        },
        {
          "id": "MDRW2100011167.1.4",
          "form": "자빱이요!",
          "original_form": "자빱이요!",
          "speaker_id": "2",
          "time": "20210824 02:31"
        },
        {
          "id": "MDRW2100011167.1.5",
          "form": "가장 재밌었던 컨텐츠 추천해주세요!",
          "original_form": "가장 재밌었던 컨텐츠 추천해주세요!",
          "speaker_id": "1",
          "time": "20210824 02:39"
        },
        {
          "id": "MDRW2100011167.1.6",
          "form": "마인 크래프트 좋아하세요?",
          "original_form": "마인 크래프트 좋아하세요?",
          "speaker_id": "2",
          "time": "20210824 02:45"
        },
        {
          "id": "MDRW2100011167.1.7",
          "form": "어렸을때 많이 해봤어요",
          "original_form": "어렸을때 많이 해봤어요",
          "speaker_id": "1",
          "time": "20210824 02:48"
        },
        {
          "id": "MDRW2100011167.1.8",
          "form": "오 그럼 서리사막의 도둑들 한 번 봐보세요 ㅎㅎ 띵작이에요",
          "original_form": "오 그럼 서리사막의 도둑들 한 번 봐보세요 ㅎㅎ 띵작이에요",
          "speaker_id": "2",
          "time": "20210824 02:49"
        },
        {
          "id": "MDRW2100011167.1.9",
          "form": "추천해주신 걸로 입문? 해볼게요",
          "original_form": "추천해주신 걸로 입문? 해볼게요",
          "speaker_id": "1",
          "time": "20210824 02:57"
        },
        {
          "id": "MDRW2100011167.1.10",
          "form": "네 ㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 03:01"
        },
        {
          "id": "MDRW2100011167.1.11",
          "form": "좋아하는 게임 있으세요?",
          "original_form": "좋아하는 게임 있으세요?",
          "speaker_id": "2",
          "time": "20210824 03:01"
        },
        {
          "id": "MDRW2100011167.1.12",
          "form": "예전에 coc랑 데바데 자주했어요",
          "original_form": "예전에 coc랑 데바데 자주했어요",
          "speaker_id": "1",
          "time": "20210824 03:03"
        },
        {
          "id": "MDRW2100011167.1.13",
          "form": "오 데바데 자주 보는데 어렵더라고요",
          "original_form": "오 데바데 자주 보는데 어렵더라고요",
          "speaker_id": "2",
          "time": "20210824 03:03"
        },
        {
          "id": "MDRW2100011167.1.14",
          "form": "맞아요 그래서 그만뒀어요ㅋㅋ",
          "original_form": "맞아요 그래서 그만뒀어요ㅋㅋ",
