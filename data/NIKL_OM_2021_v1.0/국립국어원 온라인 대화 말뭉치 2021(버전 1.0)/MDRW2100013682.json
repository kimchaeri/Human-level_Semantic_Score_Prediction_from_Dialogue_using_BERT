      "utterance": [
        {
          "id": "MDRW2100013682.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013682.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013682.1.3",
          "form": "올림픽 재밌게보셨나요",
          "original_form": "올림픽 재밌게보셨나요",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013682.1.4",
          "form": "네! 좋아하는 스포츠 종목 있으세요?",
          "original_form": "네! 좋아하는 스포츠 종목 있으세요?",
          "speaker_id": "2",
          "time": "20210825 15:37"
        },
        {
          "id": "MDRW2100013682.1.5",
          "form": "솔직히 스포츠에 대해 잘모르는데",
          "original_form": "솔직히 스포츠에 대해 잘모르는데",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013682.1.6",
          "form": "이번에 여자배구는 멋있다고 생각했어요",
          "original_form": "이번에 여자배구는 멋있다고 생각했어요",
          "speaker_id": "1",
          "time": "20210825 15:38"
        },
        {
          "id": "MDRW2100013682.1.7",
          "form": "맞아요ㅎㅎ 메달은 아쉬웠지만 4강 진출해서 좋았어요",
          "original_form": "맞아요ㅎㅎ 메달은 아쉬웠지만 4강 진출해서 좋았어요",
          "speaker_id": "2",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100013682.1.8",
          "form": "양궁은 볼 때 저도 긴장되더라구요",
          "original_form": "양궁은 볼 때 저도 긴장되더라구요",
          "speaker_id": "2",
          "time": "20210825 15:40"
        },
        {
          "id": "MDRW2100013682.1.9",
          "form": "모든 스포츠가 그렇지만",
          "original_form": "모든 스포츠가 그렇지만",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013682.1.10",
          "form": "양궁선수들은  집중력이 대단할꺼같아요",
          "original_form": "양궁선수들은  집중력이 대단할꺼같아요",
          "speaker_id": "1",
          "time": "20210825 15:42"
        },
        {
          "id": "MDRW2100013682.1.11",
          "form": "바람이 많이 불었는데도 다들 침착해서 멋졌어요~",
          "original_form": "바람이 많이 불었는데도 다들 침착해서 멋졌어요~",
          "speaker_id": "2",
          "time": "20210825 15:44"
        },
        {
          "id": "MDRW2100013682.1.12",
          "form": "그러니까요 저라면 바람 불면 집중력이 흩어질껏같은데",
          "original_form": "그러니까요 저라면 바람 불면 집중력이 흩어질껏같은데",
          "speaker_id": "1",
          "time": "20210825 15:46"
        },
        {
          "id": "MDRW2100013682.1.13",
          "form": "선수들은 진짜 대단한거같아요",
          "original_form": "선수들은 진짜 대단한거같아요",
          "speaker_id": "1",
          "time": "20210825 15:47"
        },
        {
          "id": "MDRW2100013682.1.14",
          "form": "경기를 위해서 몇 년동안 노력한다는게 정말 대단한 것 같아요",
          "original_form": "경기를 위해서 몇 년동안 노력한다는게 정말 대단한 것 같아요",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013682.1.15",
          "form": "달리기 때 출발실수로 실격당한 선수가 있어서 안타까웠어요 ㅠ",
          "original_form": "달리기 때 출발실수로 실격당한 선수가 있어서 안타까웠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100013682.1.16",
          "form": "헉 그런일이 있었군요",
          "original_form": "헉 그런일이 있었군요",
          "speaker_id": "1",
          "time": "20210825 15:50"
        },
        {
          "id": "MDRW2100013682.1.17",
          "form": "처음부터 실격이라니 너무 마음아프네요",
          "original_form": "처음부터 실격이라니 너무 마음아프네요",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100013682.1.18",
          "form": "영광스러운 일이지만 선수들 스트레스도 대단할꺼같아요",
          "original_form": "영광스러운 일이지만 선수들 스트레스도 대단할꺼같아요",
          "speaker_id": "1",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100013682.1.19",
          "form": "결승이었는데 담담하게 인정하고 들어가서 더 안타깝더라구요 ㅠ",
          "original_form": "결승이었는데 담담하게 인정하고 들어가서 더 안타깝더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210825 15:52"
        },
        {
          "id": "MDRW2100013682.1.20",
          "form": "아 결승에서ㅠ...",
          "original_form": "아 결승에서ㅠ...",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013682.1.21",
          "form": "실력이 좋은 선순거같은데 너무 안타깝네요",
          "original_form": "실력이 좋은 선순거같은데 너무 안타깝네요",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013682.1.22",
          "form": "맞아요ㅠ 한순간에 기회가 사라져서 너무 허무할 것 같아요",
          "original_form": "맞아요ㅠ 한순간에 기회가 사라져서 너무 허무할 것 같아요",
          "speaker_id": "2",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013682.1.23",
          "form": "이번 올림픽은 어린 선수들이 좋은 성적을 많이 내서 대견했어요 ㅎㅎ!",
          "original_form": "이번 올림픽은 어린 선수들이 좋은 성적을 많이 내서 대견했어요 ㅎㅎ!",
          "speaker_id": "2",
          "time": "20210825 15:55"
        },
        {
          "id": "MDRW2100013682.1.24",
          "form": "어린나이에 대단한거같아요",
          "original_form": "어린나이에 대단한거같아요",
          "speaker_id": "1",
          "time": "20210825 15:58"
        }
      ]
    }
