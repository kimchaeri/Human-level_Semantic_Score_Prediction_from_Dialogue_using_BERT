      "utterance": [
        {
          "id": "MDRW2100003342.1.1",
          "form": "여름휴가는 다녀오셨나요?",
          "original_form": "여름휴가는 다녀오셨나요?",
          "speaker_id": "1",
          "time": "20210823 10:45"
        },
        {
          "id": "MDRW2100003342.1.2",
          "form": "아니요  가을에 강원도 강 생각이에요",
          "original_form": "아니요  가을에 강원도 강 생각이에요",
          "speaker_id": "2",
          "time": "20210823 10:46"
        },
        {
          "id": "MDRW2100003342.1.3",
          "form": "여름휴가 다녀오셨어요?",
          "original_form": "여름휴가 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210823 10:46"
        },
        {
          "id": "MDRW2100003342.1.4",
          "form": "저희도 그냥 시골 다녀왔습니다.",
          "original_form": "저희도 그냥 시골 다녀왔습니다.",
          "speaker_id": "1",
          "time": "20210823 10:51"
        },
        {
          "id": "MDRW2100003342.1.5",
          "form": "강원도가 확실히 좋지요?",
          "original_form": "강원도가 확실히 좋지요?",
          "speaker_id": "1",
          "time": "20210823 10:51"
        },
        {
          "id": "MDRW2100003342.1.6",
          "form": "네 영월 횡성 이쪽을 한번도 안가봤어서 가보려고요 ㅋ",
          "original_form": "네 영월 횡성 이쪽을 한번도 안가봤어서 가보려고요 ㅋ",
          "speaker_id": "2",
          "time": "20210823 11:19"
        },
        {
          "id": "MDRW2100003342.1.7",
          "form": "횡성은 스키장이죠!",
          "original_form": "횡성은 스키장이죠!",
          "speaker_id": "1",
          "time": "20210823 11:59"
        },
        {
          "id": "MDRW2100003342.1.8",
          "form": "ㅎㅎ 한우먹으러가요 저는 ㅋㅋㅋ",
          "original_form": "ㅎㅎ 한우먹으러가요 저는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 12:58"
        },
        {
          "id": "MDRW2100003342.1.9",
          "form": "으아 횡성 하면 한우죠ㅋㅋㅋ",
          "original_form": "으아 횡성 하면 한우죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:16"
        },
        {
          "id": "MDRW2100003342.1.10",
          "form": "부모님 모시고 바람쐬러 가는거죠 ㅎ",
          "original_form": "부모님 모시고 바람쐬러 가는거죠 ㅎ",
          "speaker_id": "2",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100003342.1.11",
          "form": "코로나때문에 유명한 관광지는 못가겠더라고요",
          "original_form": "코로나때문에 유명한 관광지는 못가겠더라고요",
          "speaker_id": "2",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100003342.1.12",
          "form": "횡성에 월정사 숲 좋아요!",
          "original_form": "횡성에 월정사 숲 좋아요!",
          "speaker_id": "1",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100003342.1.13",
          "form": "오 꼭 가볼게요 ㅎㅎ",
          "original_form": "오 꼭 가볼게요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 13:38"
        },
        {
          "id": "MDRW2100003342.1.14",
          "form": "우리나라 여행가본데중에 어디가 제일 좋았어요??",
          "original_form": "우리나라 여행가본데중에 어디가 제일 좋았어요??",
          "speaker_id": "2",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100003342.1.15",
          "form": "그리고 강원도는 송어회도 좋아요",
          "original_form": "그리고 강원도는 송어회도 좋아요",
          "speaker_id": "1",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100003342.1.16",
          "form": "제일 좋은 데는 모르겠어요",
          "original_form": "제일 좋은 데는 모르겠어요",
          "speaker_id": "1",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100003342.1.17",
          "form": "그냥 놀면 다 좋아서ㅋㅋ",
          "original_form": "그냥 놀면 다 좋아서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100003342.1.18",
          "form": "ㅎㅎㅎ 맞아요 일안하고 놀면 그거만큼 좋은게없죠",
          "original_form": "ㅎㅎㅎ 맞아요 일안하고 놀면 그거만큼 좋은게없죠",
          "speaker_id": "2",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100003342.1.19",
          "form": "저는 겨울에 동유럽 여행가보는게 소원이에요  크리스마스시즌에 ㅎ",
          "original_form": "저는 겨울에 동유럽 여행가보는게 소원이에요  크리스마스시즌에 ㅎ",
          "speaker_id": "2",
          "time": "20210823 15:08"
        },
        {
          "id": "MDRW2100003342.1.20",
          "form": "으아 저는 체코 다녀왔었어요",
          "original_form": "으아 저는 체코 다녀왔었어요",
          "speaker_id": "1",
          "time": "20210823 15:09"
        }
      ]
