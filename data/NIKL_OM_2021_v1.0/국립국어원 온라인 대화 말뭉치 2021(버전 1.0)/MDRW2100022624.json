      "utterance": [
        {
          "id": "MDRW2100022624.1.1",
          "form": "저는 감자튀김좋아해요 ㅋㅋㅋㅋ",
          "original_form": "저는 감자튀김좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:26"
        },
        {
          "id": "MDRW2100022624.1.2",
          "form": "생맥을좋아해서그런가",
          "original_form": "생맥을좋아해서그런가",
          "speaker_id": "2",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100022624.1.3",
          "form": "아 감자튀김 ㅋㅋㅋㅋ",
          "original_form": "아 감자튀김 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100022624.1.4",
          "form": "감자튀김도 종류가 여러가지던데 어떤거 좋아해요?",
          "original_form": "감자튀김도 종류가 여러가지던데 어떤거 좋아해요?",
          "speaker_id": "1",
          "time": "20210829 16:27"
        },
        {
          "id": "MDRW2100022624.1.5",
          "form": "저는 그 얇은기본적인거 좋아해요 ㅋㅋㅋㅋ아니면 그 울퉁불퉁항거아세요?",
          "original_form": "저는 그 얇은기본적인거 좋아해요 ㅋㅋㅋㅋ아니면 그 울퉁불퉁항거아세요?",
          "speaker_id": "2",
          "time": "20210829 16:28"
        },
        {
          "id": "MDRW2100022624.1.6",
          "form": "두툼한것두좋아흐구욬ㅋㅋㅋ",
          "original_form": "두툼한것두좋아흐구욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100022624.1.7",
          "form": "네 알아요 그거",
          "original_form": "네 알아요 그거",
          "speaker_id": "1",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100022624.1.8",
          "form": "스틸링컷인가 이름이 뭐지 하튼 그거요",
          "original_form": "스틸링컷인가 이름이 뭐지 하튼 그거요",
          "speaker_id": "1",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100022624.1.9",
          "form": "사실 감튀라면 다..",
          "original_form": "사실 감튀라면 다..",
          "speaker_id": "2",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100022624.1.10",
          "form": "되게 디테일하게 잘아시네욬ㅋㅋㅋㅋ",
          "original_form": "되게 디테일하게 잘아시네욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100022624.1.11",
          "form": "해쉬브라운도 좋아요?",
          "original_form": "해쉬브라운도 좋아요?",
          "speaker_id": "1",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100022624.1.12",
          "form": "최애 안주잇으세요?",
          "original_form": "최애 안주잇으세요?",
          "speaker_id": "2",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100022624.1.13",
          "form": "저는 술을 얼마나 먹느냐에 따라 다른 것 같아요",
          "original_form": "저는 술을 얼마나 먹느냐에 따라 다른 것 같아요",
          "speaker_id": "1",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100022624.1.14",
          "form": "해쉬브라운은 그냥 있으면 먹는평이에요",
          "original_form": "해쉬브라운은 그냥 있으면 먹는평이에요",
          "speaker_id": "2",
          "time": "20210829 16:29"
        },
        {
          "id": "MDRW2100022624.1.15",
          "form": "오 되게 술잘알 느낌",
          "original_form": "오 되게 술잘알 느낌",
          "speaker_id": "2",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100022624.1.16",
          "form": "기준이 딱 있어요!",
          "original_form": "기준이 딱 있어요!",
          "speaker_id": "1",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100022624.1.17",
          "form": "오래 죽치고 앉아서 먹는다 = 무조건 찌개나 탕, 국 시켜요",
          "original_form": "오래 죽치고 앉아서 먹는다 = 무조건 찌개나 탕, 국 시켜요",
          "speaker_id": "1",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100022624.1.18",
          "form": "아직 초저녁이고 밥 안먹은 사람 많다 = 고기류, 튀김류",
          "original_form": "아직 초저녁이고 밥 안먹은 사람 많다 = 고기류, 튀김류",
          "speaker_id": "1",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100022624.1.19",
          "form": "ㅋㅋㅋㅋㅋ디테일하네욬ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ디테일하네욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100022624.1.20",
          "form": "한번에 둘다 시키면 되지않아요..?",
          "original_form": "한번에 둘다 시키면 되지않아요..?",
          "speaker_id": "2",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100022624.1.21",
          "form": "안그러면 안주가 거덜나버려요",
          "original_form": "안그러면 안주가 거덜나버려요",
          "speaker_id": "1",
          "time": "20210829 16:30"
        },
        {
          "id": "MDRW2100022624.1.22",
          "form": "저는 좀 사실 안주파",
          "original_form": "저는 좀 사실 안주파",
          "speaker_id": "2",
          "time": "20210829 16:31"
        },
        {
          "id": "MDRW2100022624.1.23",
          "form": "ㅋㅋㅋ제가 안주를 잘 안먹는 편이에요",
          "original_form": "ㅋㅋㅋ제가 안주를 잘 안먹는 편이에요",
          "speaker_id": "1",
          "time": "20210829 16:31"
        },
        {
          "id": "MDRW2100022624.1.24",
          "form": "그리고 우리들은 술먹으러 왔지 안주 먹으러왔냐고 핀잔줘요",
          "original_form": "그리고 우리들은 술먹으러 왔지 안주 먹으러왔냐고 핀잔줘요",
          "speaker_id": "1",
          "time": "20210829 16:31"
        },
        {
          "id": "MDRW2100022624.1.25",
          "form": "앜ㅋㅋㅋㅋㅋ너무하시네여 ㅠ",
          "original_form": "앜ㅋㅋㅋㅋㅋ너무하시네여 ㅠ",
          "speaker_id": "2",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100022624.1.26",
          "form": "안주먹을겸 술 곁들이는거죠!",
          "original_form": "안주먹을겸 술 곁들이는거죠!",
          "speaker_id": "2",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100022624.1.27",
          "form": "서울은 워낙 안주값이 비싸서요 ㅜㅜ",
          "original_form": "서울은 워낙 안주값이 비싸서요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100022624.1.28",
          "form": "하나만 시켜요 2만오천원 정도 하니 ㅜ",
          "original_form": "하나만 시켜요 2만오천원 정도 하니 ㅜ",
          "speaker_id": "1",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100022624.1.29",
          "form": "아 근데 그거 뭔지는알거같아요",
          "original_form": "아 근데 그거 뭔지는알거같아요",
          "speaker_id": "2",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100022624.1.30",
          "form": "안주 진짜비싸요 ㅠ",
          "original_form": "안주 진짜비싸요 ㅠ",
          "speaker_id": "2",
          "time": "20210829 16:32"
        },
        {
          "id": "MDRW2100022624.1.31",
          "form": "그래서 그 안주 사먹을 돈으로 술먹네요...",
          "original_form": "그래서 그 안주 사먹을 돈으로 술먹네요...",
          "speaker_id": "1",
          "time": "20210829 16:32"
        }
      ]
    }
