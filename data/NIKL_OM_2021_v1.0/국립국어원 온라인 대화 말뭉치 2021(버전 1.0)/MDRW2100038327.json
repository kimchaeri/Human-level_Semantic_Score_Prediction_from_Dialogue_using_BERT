      "utterance": [
        {
          "id": "MDRW2100038327.1.1",
          "form": "name1님말 찰떡이네요",
          "original_form": "&name1&님말 찰떡이네요",
          "speaker_id": "2",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100038327.1.2",
          "form": "이런게 핑크텍스인가싶음",
          "original_form": "이런게 핑크텍스인가싶음",
          "speaker_id": "1",
          "time": "20210902 10:10"
        },
        {
          "id": "MDRW2100038327.1.3",
          "form": "프로그램이 세개있는데",
          "original_form": "프로그램이 세개있는데",
          "speaker_id": "1",
          "time": "20210902 10:10"
        },
        {
          "id": "MDRW2100038327.1.4",
          "form": "그렇겠다",
          "original_form": "그렇겠다",
          "speaker_id": "2",
          "time": "20210902 10:10"
        },
        {
          "id": "MDRW2100038327.1.5",
          "form": "젤유명한게 48시간프로그램임",
          "original_form": "젤유명한게 48시간프로그램임",
          "speaker_id": "1",
          "time": "20210902 10:10"
        },
        {
          "id": "MDRW2100038327.1.6",
          "form": "실제 제품이름이 레디톡 48시간 이기도하고",
          "original_form": "실제 제품이름이 레디톡 48시간 이기도하고",
          "speaker_id": "1",
          "time": "20210902 10:10"
        },
        {
          "id": "MDRW2100038327.1.7",
          "form": "어떤건데",
          "original_form": "어떤건데",
          "speaker_id": "2",
          "time": "20210902 10:10"
        },
        {
          "id": "MDRW2100038327.1.8",
          "form": "말그대로 48시간동안 저거만먹음",
          "original_form": "말그대로 48시간동안 저거만먹음",
          "speaker_id": "1",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100038327.1.9",
          "form": "이외 최대한공복유지",
          "original_form": "이외 최대한공복유지",
          "speaker_id": "1",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100038327.1.10",
          "form": "니가 맛있다카니 건강식아닐확률이 높음ㅋㅋ",
          "original_form": "니가 맛있다카니 건강식아닐확률이 높음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100038327.1.11",
          "form": "운동도 병행할것",
          "original_form": "운동도 병행할것",
          "speaker_id": "1",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100038327.1.12",
          "form": "10개인데 한세트에",
          "original_form": "10개인데 한세트에",
          "speaker_id": "1",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100038327.1.13",
          "form": "운동하고굶어서빠지겠네",
          "original_form": "운동하고굶어서빠지겠네",
          "speaker_id": "2",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100038327.1.14",
          "form": "하루다섯개씩먹음",
          "original_form": "하루다섯개씩먹음",
          "speaker_id": "1",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100038327.1.15",
          "form": "세시간 간격으로",
          "original_form": "세시간 간격으로",
          "speaker_id": "1",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100038327.1.16",
          "form": "ㅜㅜ배고파 듣기만해도",
          "original_form": "ㅜㅜ배고파 듣기만해도",
          "speaker_id": "2",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100038327.1.17",
          "form": "드래곤볼에나오는 선인콩암? 한알에 배부름",
          "original_form": "드래곤볼에나오는 선인콩암? 한알에 배부름",
          "speaker_id": "2",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100038327.1.18",
          "form": "그래봐야",
          "original_form": "그래봐야",
          "speaker_id": "1",
          "time": "20210902 10:17"
        },
        {
          "id": "MDRW2100038327.1.19",
          "form": "여하간 내 위는 지금 뭐가 지나갔나 하고있을든",
          "original_form": "여하간 내 위는 지금 뭐가 지나갔나 하고있을든",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100038327.1.20",
          "form": "근데 그거는 니동생이 사서 널준거니",
          "original_form": "근데 그거는 니동생이 사서 널준거니",
          "speaker_id": "2",
          "time": "20210902 10:38"
        },
        {
          "id": "MDRW2100038327.1.21",
          "form": "니가 돈나간건아니지않아? 동생 감사사~",
          "original_form": "니가 돈나간건아니지않아? 동생 감사사~",
