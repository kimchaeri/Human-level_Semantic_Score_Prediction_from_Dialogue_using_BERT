      "utterance": [
        {
          "id": "MDRW2100024705.1.1",
          "form": "빨간머리앤 좋아하시는건가요? ㅎㅎ",
          "original_form": "빨간머리앤 좋아하시는건가요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024705.1.2",
          "form": "네! 좋아해요 그냥 어릴때 보던 애니를 좋아했던",
          "original_form": "네! 좋아해요 그냥 어릴때 보던 애니를 좋아했던",
          "speaker_id": "2",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100024705.1.3",
          "form": "책은 안읽어봤어여 ㅎ ㅠ..",
          "original_form": "책은 안읽어봤어여 ㅎ ㅠ..",
          "speaker_id": "2",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100024705.1.4",
          "form": "좋아하는 작가님 계세요?",
          "original_form": "좋아하는 작가님 계세요?",
          "speaker_id": "2",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100024705.1.5",
          "form": "저는 무라카미하루키 아시나요 상실의 시대 쓴",
          "original_form": "저는 무라카미하루키 아시나요 상실의 시대 쓴",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100024705.1.6",
          "form": "그 사람이 쓴 소설말고 에세이 재밌어서 잘봐요 ㅋㅋ",
          "original_form": "그 사람이 쓴 소설말고 에세이 재밌어서 잘봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100024705.1.7",
          "form": "아! 제목은 들어봤어요!",
          "original_form": "아! 제목은 들어봤어요!",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100024705.1.8",
          "form": "책을 즐겨읽으시나봐요",
          "original_form": "책을 즐겨읽으시나봐요",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100024705.1.9",
          "form": "저는... 진짜 오랫동안 책 안읽고산거같아여",
          "original_form": "저는... 진짜 오랫동안 책 안읽고산거같아여",
          "speaker_id": "2",
          "time": "20210907 19:32"
        },
        {
          "id": "MDRW2100024705.1.10",
          "form": "사실 저도 그래요 ㅋㅋ 1년에 진짜 몇권 읽을까말까한 ㅠㅠ",
          "original_form": "사실 저도 그래요 ㅋㅋ 1년에 진짜 몇권 읽을까말까한 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:33"
        },
        {
          "id": "MDRW2100024705.1.11",
          "form": "그죠ㅠ",
          "original_form": "그죠ㅠ",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100024705.1.12",
          "form": "최근에 주식 책 읽다가 포기했네요",
          "original_form": "최근에 주식 책 읽다가 포기했네요",
          "speaker_id": "2",
          "time": "20210907 19:36"
        },
        {
          "id": "MDRW2100024705.1.13",
          "form": "헐 주식책.. 어려울거 같아요",
          "original_form": "헐 주식책.. 어려울거 같아요",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100024705.1.14",
          "form": "용어 많이 나오죠?",
          "original_form": "용어 많이 나오죠?",
          "speaker_id": "1",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100024705.1.15",
          "form": "네 ㅎ 어려워서 보다가 포기했습니다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 ㅎ 어려워서 보다가 포기했습니다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100024705.1.16",
          "form": "봐도 이해가안가서요",
          "original_form": "봐도 이해가안가서요",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100024705.1.17",
          "form": "ㅋㅋㅋㅋㅋ 그럴거 같아요 약간 책을 이해하기 위해서 또 뭘 찾아야되는 그런 책 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 그럴거 같아요 약간 책을 이해하기 위해서 또 뭘 찾아야되는 그런 책 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100024705.1.18",
          "form": "맞아요ㅋㅋㄱ안그래도 피곤한데 더피곤해져서 빠른포기....",
          "original_form": "맞아요ㅋㅋㄱ안그래도 피곤한데 더피곤해져서 빠른포기....",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100024705.1.19",
          "form": "ㅋㅋㅋㅋ 그럼 살면서 젤 기억에 남는 책은 뭐였어요?",
          "original_form": "ㅋㅋㅋㅋ 그럼 살면서 젤 기억에 남는 책은 뭐였어요?",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100024705.1.20",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100024705.1.21",
          "form": "저,,,",
          "original_form": "저,,,",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100024705.1.22",
          "form": "내용은 기억안나고요",
          "original_form": "내용은 기억안나고요",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100024705.1.23",
          "form": "사신치바?",
          "original_form": "사신치바?",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100024705.1.24",
          "form": "그거 재밌었어요",
          "original_form": "그거 재밌었어요",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100024705.1.25",
          "form": "오 첨들어봐요 어느 나라 작가인가용",
          "original_form": "오 첨들어봐요 어느 나라 작가인가용",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100024705.1.26",
          "form": "일본작가였어요 ㅎ",
          "original_form": "일본작가였어요 ㅎ",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100024705.1.27",
          "form": "재밌게 읽었던 기억이 ㅎ",
          "original_form": "재밌게 읽었던 기억이 ㅎ",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100024705.1.28",
          "form": "오오 장르가 뭐였어요? 소설인가요",
          "original_form": "오오 장르가 뭐였어요? 소설인가요",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100024705.1.29",
          "form": "네 소설이에요!!!!",
          "original_form": "네 소설이에요!!!!",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100024705.1.30",
          "form": "이사카 코타로",
          "original_form": "이사카 코타로",
          "speaker_id": "2",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100024705.1.31",
          "form": "라네요 저자가",
          "original_form": "라네요 저자가",
          "speaker_id": "2",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100024705.1.32",
          "form": "오 찾아봐야겠어요",
          "original_form": "오 찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100024705.1.33",
          "form": "저는 재밌게 읽었던 기억이 ㅎ",
          "original_form": "저는 재밌게 읽었던 기억이 ㅎ",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100024705.1.34",
          "form": "한창 그때 일본소설에 빠졌던터라",
          "original_form": "한창 그때 일본소설에 빠졌던터라",
          "speaker_id": "2",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100024705.1.35",
          "form": "오 한때 유행할때가 있었잖아요 저도 작가는 기억 안나는데 많이 본거 같네요 ㅋㅋ",
          "original_form": "오 한때 유행할때가 있었잖아요 저도 작가는 기억 안나는데 많이 본거 같네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:59"
        }
      ]
