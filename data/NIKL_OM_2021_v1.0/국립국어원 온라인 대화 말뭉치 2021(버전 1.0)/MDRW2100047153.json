      "utterance": [
        {
          "id": "MDRW2100047153.1.1",
          "form": "왠지 그럴거 같았어요 ㅋㅋㅋ 메뉴 하면 뚝딱 나오길래",
          "original_form": "왠지 그럴거 같았어요 ㅋㅋㅋ 메뉴 하면 뚝딱 나오길래",
          "speaker_id": "2",
          "time": "20210901 15:00"
        },
        {
          "id": "MDRW2100047153.1.2",
          "form": "서울쪽 사는데 서울쪽에서 맛집 찾아다니느걸 참 좋아해요 ㅋㅋㄱㄱ",
          "original_form": "서울쪽 사는데 서울쪽에서 맛집 찾아다니느걸 참 좋아해요 ㅋㅋㄱㄱ",
          "speaker_id": "1",
          "time": "20210901 15:02"
        },
        {
          "id": "MDRW2100047153.1.3",
          "form": "요샌자주못가지만",
          "original_form": "요샌자주못가지만",
          "speaker_id": "1",
          "time": "20210901 15:02"
        },
        {
          "id": "MDRW2100047153.1.4",
          "form": "오 순대맛집도 아시나요 저 전통순대 좋아하는데 ㅋㅋ",
          "original_form": "오 순대맛집도 아시나요 저 전통순대 좋아하는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:04"
        },
        {
          "id": "MDRW2100047153.1.5",
          "form": "오 순대",
          "original_form": "오 순대",
          "speaker_id": "1",
          "time": "20210901 15:08"
        },
        {
          "id": "MDRW2100047153.1.6",
          "form": "선릉역쪽ㅇ에 농민백암순대",
          "original_form": "선릉역쪽ㅇ에 농민백암순대",
          "speaker_id": "1",
          "time": "20210901 15:09"
        },
        {
          "id": "MDRW2100047153.1.7",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "1",
          "time": "20210901 15:09"
        },
        {
          "id": "MDRW2100047153.1.8",
          "form": "강남역에도 분점있는데 평일은 진짜 직장인들 줄서서 먹어용",
          "original_form": "강남역에도 분점있는데 평일은 진짜 직장인들 줄서서 먹어용",
          "speaker_id": "1",
          "time": "20210901 15:09"
        },
        {
          "id": "MDRW2100047153.1.9",
          "form": "아 저 다른 지점 지나가다 본거 같아요",
          "original_form": "아 저 다른 지점 지나가다 본거 같아요",
          "speaker_id": "2",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100047153.1.10",
          "form": "가보려고 했는데 그때 뭐때문인지 못 갔네요",
          "original_form": "가보려고 했는데 그때 뭐때문인지 못 갔네요",
          "speaker_id": "2",
          "time": "20210901 15:49"
        },
        {
          "id": "MDRW2100047153.1.11",
          "form": "거기가 정식도 그렇고 양도많고 맛있어요",
          "original_form": "거기가 정식도 그렇고 양도많고 맛있어요",
          "speaker_id": "1",
          "time": "20210901 15:51"
        },
        {
          "id": "MDRW2100047153.1.12",
          "form": "수요미식회 나온 덴가봐요 찾아보니까",
          "original_form": "수요미식회 나온 덴가봐요 찾아보니까",
          "speaker_id": "2",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100047153.1.13",
          "form": "아하 거기가 나왔나보네요",
          "original_form": "아하 거기가 나왔나보네요",
          "speaker_id": "1",
          "time": "20210901 15:52"
        },
        {
          "id": "MDRW2100047153.1.14",
          "form": "그거나오기전부터도 진짜 맛집으로 유명했었는데",
          "original_form": "그거나오기전부터도 진짜 맛집으로 유명했었는데",
          "speaker_id": "1",
          "time": "20210901 15:53"
        },
        {
          "id": "MDRW2100047153.1.15",
          "form": "오 찐 맛집인가봐요",
          "original_form": "오 찐 맛집인가봐요",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100047153.1.16",
          "form": "네네 ㅋㅋㄱㅋㅋ가끔 사람너무 많으면 못가는 그런곳..",
          "original_form": "네네 ㅋㅋㄱㅋㅋ가끔 사람너무 많으면 못가는 그런곳..",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100047153.1.17",
          "form": "제가 좋아하는 순대타입이에요 ㅋㅋㅋ",
          "original_form": "제가 좋아하는 순대타입이에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100047153.1.18",
          "form": "깔끔하고 맛있어요 ㅋㄱㅋㅋㄱㅋ",
          "original_form": "깔끔하고 맛있어요 ㅋㄱㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100047153.1.19",
          "form": "거기도괜찮고",
          "original_form": "거기도괜찮고",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100047153.1.20",
          "form": "체인이긴하지만 제주은희네해장국도 맛있어요",
          "original_form": "체인이긴하지만 제주은희네해장국도 맛있어요",
          "speaker_id": "1",
          "time": "20210901 15:54"
        },
        {
          "id": "MDRW2100047153.1.21",
          "form": "헐 저희동네에 하나 있네요 ㅋㅋ",
          "original_form": "헐 저희동네에 하나 있네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:56"
        },
        {
          "id": "MDRW2100047153.1.22",
          "form": "오 ㅋㄱㅋㅋㅋㅋ 거기 정말 건더기가 많아요",
          "original_form": "오 ㅋㄱㅋㅋㅋㅋ 거기 정말 건더기가 많아요",
          "speaker_id": "1",
          "time": "20210901 15:56"
        },
        {
          "id": "MDRW2100047153.1.23",
          "form": "꼭 가보셔요",
          "original_form": "꼭 가보셔요",
          "speaker_id": "1",
          "time": "20210901 15:56"
        }
      ]
