      "utterance": [
        {
          "id": "MDRW2100029782.1.1",
          "form": "배달이 다른집에 갔네요ㅜ",
          "original_form": "배달이 다른집에 갔네요ㅜ",
          "speaker_id": "1",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100029782.1.2",
          "form": "배민원 별로인게 라이더 연락처도 안 뜨고",
          "original_form": "배민원 별로인게 라이더 연락처도 안 뜨고",
          "speaker_id": "1",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100029782.1.3",
          "form": "가게서도 연락처 모른대서ㅜ",
          "original_form": "가게서도 연락처 모른대서ㅜ",
          "speaker_id": "1",
          "time": "20210903 13:36"
        },
        {
          "id": "MDRW2100029782.1.4",
          "form": "연결도 늦는 배민 채팅방 대기중ㅜ",
          "original_form": "연결도 늦는 배민 채팅방 대기중ㅜ",
          "speaker_id": "1",
          "time": "20210903 13:37"
        },
        {
          "id": "MDRW2100029782.1.5",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100029782.1.6",
          "form": "그런 불상사가",
          "original_form": "그런 불상사가",
          "speaker_id": "2",
          "time": "20210903 15:19"
        },
        {
          "id": "MDRW2100029782.1.7",
          "form": "다 드셨게죠?",
          "original_form": "다 드셨게죠?",
          "speaker_id": "2",
          "time": "20210903 15:20"
        },
        {
          "id": "MDRW2100029782.1.8",
          "form": "인사철이라 밖에서 커피한잔하면서 시간보내고 왓네요",
          "original_form": "인사철이라 밖에서 커피한잔하면서 시간보내고 왓네요",
          "speaker_id": "2",
          "time": "20210903 15:20"
        },
        {
          "id": "MDRW2100029782.1.9",
          "form": "어수선합니다",
          "original_form": "어수선합니다",
          "speaker_id": "2",
          "time": "20210903 15:20"
        },
        {
          "id": "MDRW2100029782.1.10",
          "form": "멍게 신선했나요?",
          "original_form": "멍게 신선했나요?",
          "speaker_id": "2",
          "time": "20210903 15:20"
        },
        {
          "id": "MDRW2100029782.1.11",
          "form": "주문한지 한시간40분만에 받아서",
          "original_form": "주문한지 한시간40분만에 받아서",
          "speaker_id": "1",
          "time": "20210903 15:20"
        },
        {
          "id": "MDRW2100029782.1.12",
          "form": "아직 식사중이요ㅜ",
          "original_form": "아직 식사중이요ㅜ",
          "speaker_id": "1",
          "time": "20210903 15:21"
        },
        {
          "id": "MDRW2100029782.1.13",
          "form": "쏘쏘였어요",
          "original_form": "쏘쏘였어요",
          "speaker_id": "1",
          "time": "20210903 15:21"
        },
        {
          "id": "MDRW2100029782.1.14",
          "form": "울적",
          "original_form": "울적",
          "speaker_id": "1",
          "time": "20210903 15:21"
        },
        {
          "id": "MDRW2100029782.1.15",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210903 15:21"
        },
        {
          "id": "MDRW2100029782.1.16",
          "form": "인사철이라니ㄷㄷ",
          "original_form": "인사철이라니ㄷㄷ",
          "speaker_id": "1",
          "time": "20210903 15:21"
        },
        {
          "id": "MDRW2100029782.1.17",
          "form": "저는 관계없는데 실장들 인사철이라 ㅋㅋ",
          "original_form": "저는 관계없는데 실장들 인사철이라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:21"
        },
        {
          "id": "MDRW2100029782.1.18",
          "form": "이동이 큰가요?",
          "original_form": "이동이 큰가요?",
          "speaker_id": "1",
          "time": "20210903 15:21"
        },
        {
          "id": "MDRW2100029782.1.19",
          "form": "행정쪽은 다 바뀌더라구요",
          "original_form": "행정쪽은 다 바뀌더라구요",
          "speaker_id": "2",
          "time": "20210903 15:22"
        },
        {
          "id": "MDRW2100029782.1.20",
          "form": "원장바뀌어서",
          "original_form": "원장바뀌어서",
          "speaker_id": "2",
          "time": "20210903 15:22"
        },
        {
          "id": "MDRW2100029782.1.21",
          "form": "싹다 바뀝니다",
          "original_form": "싹다 바뀝니다",
          "speaker_id": "2",
          "time": "20210903 15:22"
        },
        {
          "id": "MDRW2100029782.1.22",
          "form": "아...ㅎ 더 뒤숭숭하겠네요 실장급이 그러니",
          "original_form": "아...ㅎ 더 뒤숭숭하겠네요 실장급이 그러니",
          "speaker_id": "1",
          "time": "20210903 15:22"
        },
        {
          "id": "MDRW2100029782.1.23",
          "form": "아이고 ㅜ 경험상 큰이동은 대개 하향평준화로 귀결되던데",
          "original_form": "아이고 ㅜ 경험상 큰이동은 대개 하향평준화로 귀결되던데",
          "speaker_id": "1",
          "time": "20210903 15:23"
        },
        {
          "id": "MDRW2100029782.1.24",
          "form": "좋은 결론 났음 좋겠네요",
          "original_form": "좋은 결론 났음 좋겠네요",
          "speaker_id": "1",
          "time": "20210903 15:23"
        },
        {
          "id": "MDRW2100029782.1.25",
          "form": "그럴것 같습니다 ㅋㅋ",
          "original_form": "그럴것 같습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:23"
        },
        {
          "id": "MDRW2100029782.1.26",
          "form": "새우튀김은 어떤 결론인가요?",
          "original_form": "새우튀김은 어떤 결론인가요?",
          "speaker_id": "2",
          "time": "20210903 15:24"
        },
        {
          "id": "MDRW2100029782.1.27",
          "form": "얘는 합격",
          "original_form": "얘는 합격",
          "speaker_id": "1",
          "time": "20210903 15:24"
        },
        {
          "id": "MDRW2100029782.1.28",
          "form": "오 다행",
          "original_form": "오 다행",
          "speaker_id": "2",
          "time": "20210903 15:24"
        },
        {
          "id": "MDRW2100029782.1.29",
          "form": "광어!",
          "original_form": "광어!",
          "speaker_id": "2",
          "time": "20210903 15:24"
        },
        {
          "id": "MDRW2100029782.1.30",
          "form": "그리고 멍게는 뭐...쏘쏘라도 특유의 향이 있으니 ㅎ",
          "original_form": "그리고 멍게는 뭐...쏘쏘라도 특유의 향이 있으니 ㅎ",
          "speaker_id": "1",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100029782.1.31",
          "form": "광어 지느러미 짱!",
          "original_form": "광어 지느러미 짱!",
          "speaker_id": "1",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100029782.1.32",
          "form": "두툼하게 썰었네요",
          "original_form": "두툼하게 썰었네요",
          "speaker_id": "1",
          "time": "20210903 15:25"
        },
        {
          "id": "MDRW2100029782.1.33",
          "form": "오징어초무침도 굿굿",
          "original_form": "오징어초무침도 굿굿",
          "speaker_id": "1",
          "time": "20210903 15:26"
        },
        {
          "id": "MDRW2100029782.1.34",
          "form": "사랑하는 번데기와 샐러드도 밑반찬으로 왔네요",
          "original_form": "사랑하는 번데기와 샐러드도 밑반찬으로 왔네요",
          "speaker_id": "1",
          "time": "20210903 15:26"
        }
      ]
    }
