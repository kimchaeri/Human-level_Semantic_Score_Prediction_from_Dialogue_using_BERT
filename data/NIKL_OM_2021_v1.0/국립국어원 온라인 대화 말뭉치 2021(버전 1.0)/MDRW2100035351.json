      "utterance": [
        {
          "id": "MDRW2100035351.1.1",
          "form": "저 과일 좋아하는데 꼭 한번 시도해볼게요",
          "original_form": "저 과일 좋아하는데 꼭 한번 시도해볼게요",
          "speaker_id": "2",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100035351.1.2",
          "form": "진짜 맛있는 과일은 그 자체로 드시구",
          "original_form": "진짜 맛있는 과일은 그 자체로 드시구",
          "speaker_id": "1",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100035351.1.3",
          "form": "살짝 아리까리한걸 이렇게 해먹음 맛있어요",
          "original_form": "살짝 아리까리한걸 이렇게 해먹음 맛있어요",
          "speaker_id": "1",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100035351.1.4",
          "form": "그리고 치즈 종류따라서도 좀 틀린데요",
          "original_form": "그리고 치즈 종류따라서도 좀 틀린데요",
          "speaker_id": "1",
          "time": "20210829 15:25"
        },
        {
          "id": "MDRW2100035351.1.5",
          "form": "산미가 있는 천도복숭이는 좀 산미있는 크림치즈랑 무화과는 산미적은 마스카포네랑 어울렸지만",
          "original_form": "산미가 있는 천도복숭이는 좀 산미있는 크림치즈랑 무화과는 산미적은 마스카포네랑 어울렸지만",
          "speaker_id": "1",
          "time": "20210829 15:26"
        },
        {
          "id": "MDRW2100035351.1.6",
          "form": "오 산미가 포인트네요",
          "original_form": "오 산미가 포인트네요",
          "speaker_id": "2",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100035351.1.7",
          "form": "둘다 크림치즈랑도 어울려요",
          "original_form": "둘다 크림치즈랑도 어울려요",
          "speaker_id": "1",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100035351.1.8",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100035351.1.9",
          "form": "꿀팁",
          "original_form": "꿀팁",
          "speaker_id": "2",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100035351.1.10",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "2",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100035351.1.11",
          "form": "진짜 좀 신선하지않은 과일은",
          "original_form": "진짜 좀 신선하지않은 과일은",
          "speaker_id": "2",
          "time": "20210829 15:27"
        },
        {
          "id": "MDRW2100035351.1.12",
          "form": "이렇게먹으면 되겠어요",
          "original_form": "이렇게먹으면 되겠어요",
          "speaker_id": "2",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100035351.1.13",
          "form": "네네 ㅋㅋ 과일종류따라 잘 골라서 드세요 메론 이런거랑은 안어울려요",
          "original_form": "네네 ㅋㅋ 과일종류따라 잘 골라서 드세요 메론 이런거랑은 안어울려요",
          "speaker_id": "1",
          "time": "20210829 15:28"
        },
        {
          "id": "MDRW2100035351.1.14",
          "form": "아 메론",
          "original_form": "아 메론",
          "speaker_id": "2",
          "time": "20210829 15:29"
        },
        {
          "id": "MDRW2100035351.1.15",
          "form": "메론은",
          "original_form": "메론은",
          "speaker_id": "2",
          "time": "20210829 15:29"
        },
        {
          "id": "MDRW2100035351.1.16",
          "form": "많이 먹어본적 없지만",
          "original_form": "많이 먹어본적 없지만",
          "speaker_id": "2",
          "time": "20210829 15:29"
        },
        {
          "id": "MDRW2100035351.1.17",
          "form": "항상 남지않았어요",
          "original_form": "항상 남지않았어요",
          "speaker_id": "2",
          "time": "20210829 15:29"
        },
        {
          "id": "MDRW2100035351.1.18",
          "form": "네 ㅋㅋ저도 메론은 잘 안남겨요",
          "original_form": "네 ㅋㅋ저도 메론은 잘 안남겨요",
          "speaker_id": "1",
          "time": "20210829 15:30"
        },
        {
          "id": "MDRW2100035351.1.19",
          "form": "통으로 썰어서 슥삭하면 금방먹죠 집에서 먹으니깐,,,",
          "original_form": "통으로 썰어서 슥삭하면 금방먹죠 집에서 먹으니깐,,,",
          "speaker_id": "1",
          "time": "20210829 15:30"
        },
        {
          "id": "MDRW2100035351.1.20",
          "form": "설빙 메론 빙수도 좋아해요",
          "original_form": "설빙 메론 빙수도 좋아해요",
          "speaker_id": "1",
          "time": "20210829 15:30"
        },
        {
          "id": "MDRW2100035351.1.21",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 15:30"
        },
        {
          "id": "MDRW2100035351.1.22",
          "form": "빙수랑 잘어울리죠",
          "original_form": "빙수랑 잘어울리죠",
          "speaker_id": "2",
          "time": "20210829 15:30"
        },
        {
          "id": "MDRW2100035351.1.23",
          "form": "그러고보니 인절미빙수도",
          "original_form": "그러고보니 인절미빙수도",
          "speaker_id": "2",
          "time": "20210829 15:30"
        },
        {
          "id": "MDRW2100035351.1.24",
          "form": "속을 떠낸 메론그릇이 너무 이뻐요",
          "original_form": "속을 떠낸 메론그릇이 너무 이뻐요",
          "speaker_id": "1",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100035351.1.25",
          "form": "참 우리나라 사람들 먹는것에 진심이네요",
          "original_form": "참 우리나라 사람들 먹는것에 진심이네요",
          "speaker_id": "2",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100035351.1.26",
          "form": "메론그릇 예쁘죠!",
          "original_form": "메론그릇 예쁘죠!",
          "speaker_id": "2",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100035351.1.27",
          "form": "영국인이나 독일인 빼면 다 그럴걸요!",
          "original_form": "영국인이나 독일인 빼면 다 그럴걸요!",
          "speaker_id": "1",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100035351.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100035351.1.29",
          "form": "네 ㅋㅋ 집에서 그렇게 해먹으려면 귀찮은데 설빙서 그런 그릇을 받으면 기분이 좋아요",
          "original_form": "네 ㅋㅋ 집에서 그렇게 해먹으려면 귀찮은데 설빙서 그런 그릇을 받으면 기분이 좋아요",
          "speaker_id": "1",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100035351.1.30",
          "form": "요즘 설빙 안간지 꽤 됐네요ㅜ",
          "original_form": "요즘 설빙 안간지 꽤 됐네요ㅜ",
          "speaker_id": "2",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100035351.1.31",
          "form": "코로나 이후에 설빙 포장을 하면",
          "original_form": "코로나 이후에 설빙 포장을 하면",
          "speaker_id": "1",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100035351.1.32",
          "form": "플라스틱 용기에 담겨오기때문에,,,",
          "original_form": "플라스틱 용기에 담겨오기때문에,,,",
          "speaker_id": "1",
          "time": "20210829 15:31"
        },
        {
          "id": "MDRW2100035351.1.33",
          "form": "메론에 담긴 빙수같은건 못본지 2년이 지났어요",
          "original_form": "메론에 담긴 빙수같은건 못본지 2년이 지났어요",
          "speaker_id": "1",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100035351.1.34",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100035351.1.35",
          "form": "그렇겠네요",
          "original_form": "그렇겠네요",
          "speaker_id": "2",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100035351.1.36",
          "form": "인절미 빙수도 용기안에 납작해져서 오면",
          "original_form": "인절미 빙수도 용기안에 납작해져서 오면",
          "speaker_id": "1",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100035351.1.37",
          "form": "생각해보면 메론그릇이 오진않겠어요",
          "original_form": "생각해보면 메론그릇이 오진않겠어요",
          "speaker_id": "2",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100035351.1.38",
          "form": "아ㅜ",
          "original_form": "아ㅜ",
          "speaker_id": "2",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100035351.1.39",
          "form": "흥이 좀 덜난달가요",
          "original_form": "흥이 좀 덜난달가요",
          "speaker_id": "1",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100035351.1.40",
          "form": "그 맛이 안살죠",
          "original_form": "그 맛이 안살죠",
          "speaker_id": "2",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100035351.1.41",
          "form": "맞아욬ㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100035351.1.42",
          "form": "네 오다가 안흔들리게 파츠별로 통에 담겨와요 ㅠㅠㅋㅋ",
          "original_form": "네 오다가 안흔들리게 파츠별로 통에 담겨와요 ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:32"
        },
        {
          "id": "MDRW2100035351.1.43",
          "form": "또 빙수는 여러명이서 먹는거라",
          "original_form": "또 빙수는 여러명이서 먹는거라",
          "speaker_id": "2",
          "time": "20210829 15:33"
        },
        {
          "id": "MDRW2100035351.1.44",
          "form": "코로나 타격이 크겠네요",
          "original_form": "코로나 타격이 크겠네요",
          "speaker_id": "2",
          "time": "20210829 15:33"
        },
        {
          "id": "MDRW2100035351.1.45",
          "form": "이쁘게 담겨오면 ㅋㅋ 야 사진직어라 빨리 찍어라 비쥬얼 미춋다 이러는데",
          "original_form": "이쁘게 담겨오면 ㅋㅋ 야 사진직어라 빨리 찍어라 비쥬얼 미춋다 이러는데",
          "speaker_id": "1",
          "time": "20210829 15:33"
        },
        {
          "id": "MDRW2100035351.1.46",
          "form": "근데 빙수 솔직히 일인당하나 아닌가요?",
          "original_form": "근데 빙수 솔직히 일인당하나 아닌가요?",
          "speaker_id": "2",
          "time": "20210829 15:33"
        },
        {
          "id": "MDRW2100035351.1.47",
          "form": "여럿이서 호들갑 떠는 세레모니가 있는 빙수랑",
          "original_form": "여럿이서 호들갑 떠는 세레모니가 있는 빙수랑",
          "speaker_id": "1",
          "time": "20210829 15:33"
        },
        {
          "id": "MDRW2100035351.1.48",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:33"
        },
        {
          "id": "MDRW2100035351.1.49",
          "form": "여럿이 가면 2개를 주문하지요 ㅋㅋㅋ",
          "original_form": "여럿이 가면 2개를 주문하지요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:33"
        },
        {
          "id": "MDRW2100035351.1.50",
          "form": "혼자서 포장용기를 여는 빙수,,",
          "original_form": "혼자서 포장용기를 여는 빙수,,",
          "speaker_id": "1",
          "time": "20210829 15:33"
        },
        {
          "id": "MDRW2100035351.1.51",
          "form": "흥이 안나네요ㅜ",
          "original_form": "흥이 안나네요ㅜ",
          "speaker_id": "2",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100035351.1.52",
          "form": "성냥팔이 소녀가 혼자 성냥을 그어서 음식보는 기분이에요",
          "original_form": "성냥팔이 소녀가 혼자 성냥을 그어서 음식보는 기분이에요",
          "speaker_id": "1",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100035351.1.53",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100035351.1.54",
          "form": "그게뭐에욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그게뭐에욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100035351.1.55",
          "form": "근데 뭔말인지알겠어요",
          "original_form": "근데 뭔말인지알겠어요",
          "speaker_id": "2",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100035351.1.56",
          "form": "불꽃속 환상을 보듯",
          "original_form": "불꽃속 환상을 보듯",
          "speaker_id": "1",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100035351.1.57",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100035351.1.58",
          "form": "절단난 빙수 파츠들을 보고",
          "original_form": "절단난 빙수 파츠들을 보고",
          "speaker_id": "1",
          "time": "20210829 15:34"
        },
        {
          "id": "MDRW2100035351.1.59",
          "form": "뭔가 급 쓸쓸",
          "original_form": "뭔가 급 쓸쓸",
          "speaker_id": "2",
          "time": "20210829 15:34"
        },
        {
