        },
        {
          "id": "MDRW2100029652.1.2",
          "form": "오늘 계속 비와,,,,,",
          "original_form": "오늘 계속 비와,,,,,",
          "speaker_id": "2",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100029652.1.3",
          "form": "날이흐려ㅠ",
          "original_form": "날이흐려ㅠ",
          "speaker_id": "2",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100029652.1.4",
          "form": "햇볕이 막 들어와",
          "original_form": "햇볕이 막 들어와",
          "speaker_id": "1",
          "time": "20210903 11:32"
        },
        {
          "id": "MDRW2100029652.1.5",
          "form": "요기는 더울정도로 쨍해",
          "original_form": "요기는 더울정도로 쨍해",
          "speaker_id": "1",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100029652.1.6",
          "form": "서울은 덥구나 나 오늘 출근허눈데",
          "original_form": "서울은 덥구나 나 오늘 출근허눈데",
          "speaker_id": "2",
          "time": "20210903 11:33"
        },
        {
          "id": "MDRW2100029652.1.7",
          "form": "18도여서 외투 입엇어",
          "original_form": "18도여서 외투 입엇어",
          "speaker_id": "2",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029652.1.8",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "1",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029652.1.9",
          "form": "선선하구나",
          "original_form": "선선하구나",
          "speaker_id": "1",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029652.1.10",
          "form": "근데 비와서 날이 습해가지구",
          "original_form": "근데 비와서 날이 습해가지구",
          "speaker_id": "2",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029652.1.11",
          "form": "에어컨은 계속 틀엇어",
          "original_form": "에어컨은 계속 틀엇어",
          "speaker_id": "2",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029652.1.12",
          "form": "바깥기온이 21도인데",
          "original_form": "바깥기온이 21도인데",
          "speaker_id": "2",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029652.1.13",
          "form": "맞아 습기있으면 에어컨 틀어야해",
          "original_form": "맞아 습기있으면 에어컨 틀어야해",
          "speaker_id": "1",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029652.1.14",
          "form": "에어컨 설정 25도",
          "original_form": "에어컨 설정 25도",
          "speaker_id": "2",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029652.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029652.1.16",
          "form": "서울은 그늘가면 시원한데",
          "original_form": "서울은 그늘가면 시원한데",
          "speaker_id": "1",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029652.1.17",
          "form": "해가 밝으니가",
          "original_form": "해가 밝으니가",
          "speaker_id": "1",
          "time": "20210903 11:34"
        },
        {
          "id": "MDRW2100029652.1.18",
          "form": "햇볕에 나가면 더워",
          "original_form": "햇볕에 나가면 더워",
          "speaker_id": "1",
          "time": "20210903 11:35"
        },
        {
          "id": "MDRW2100029652.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029652.1.20",
          "form": "해가 밝구나",
          "original_form": "해가 밝구나",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029652.1.21",
          "form": "좋겠다",
          "original_form": "좋겠다",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029652.1.22",
          "form": "여기는 흐려서 4시반쯤 되면 어두워져",
          "original_form": "여기는 흐려서 4시반쯤 되면 어두워져",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029652.1.23",
          "form": "그래? 구름이 다 거기로 갔나 ㅋㅋㅋㅋㅋ",
          "original_form": "그래? 구름이 다 거기로 갔나 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100029652.1.24",
          "form": "근데 그래도 정말 슬슬 겨울오는지",
          "original_form": "근데 그래도 정말 슬슬 겨울오는지",
          "speaker_id": "1",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100029652.1.25",
          "form": "저녁되면 금방 어둡고 쌀쌀해",
          "original_form": "저녁되면 금방 어둡고 쌀쌀해",
          "speaker_id": "1",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100029652.1.26",
          "form": "이때 가을옷 후다닥 입어야한다는데",
          "original_form": "이때 가을옷 후다닥 입어야한다는데",
          "speaker_id": "1",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100029652.1.27",
          "form": "마자 슬슬 가을옷 사야대",
          "original_form": "마자 슬슬 가을옷 사야대",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029652.1.28",
          "form": "근데 가을옷 사면 바로 겨울 되겟지 ㅋㅋㅋㅋ",
          "original_form": "근데 가을옷 사면 바로 겨울 되겟지 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029652.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아 가을옷 한 이틀? 입으면 갑자기 패딩꺼내야할듯 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아 가을옷 한 이틀? 입으면 갑자기 패딩꺼내야할듯 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029652.1.30",
          "form": "맞아,,,,",
          "original_form": "맞아,,,,",
          "speaker_id": "2",
          "time": "20210903 17:30"
        },
        {
          "id": "MDRW2100029652.1.31",
          "form": "딱 간절기용 가디건 사도 입을 일 별로 없어 ㄴㅋㅋ",
          "original_form": "딱 간절기용 가디건 사도 입을 일 별로 없어 ㄴㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029652.1.32",
          "form": "맞아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029652.1.33",
          "form": "이제 내년까지 쭉 패딩만 입는 것임",
          "original_form": "이제 내년까지 쭉 패딩만 입는 것임",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029652.1.34",
          "form": "사서.....언제입지? 환절기에 에어컨 트는 지하철 이런데서나 입고",
          "original_form": "사서.....언제입지? 환절기에 에어컨 트는 지하철 이런데서나 입고",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029652.1.35",
          "form": "그니까 나는 봄가을이 좋은데 점점 봄가을은 없어져ㅠ",
          "original_form": "그니까 나는 봄가을이 좋은데 점점 봄가을은 없어져ㅠ",
          "speaker_id": "1",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029652.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아 웃기다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아 웃기다",
          "speaker_id": "2",
          "time": "20210903 17:31"
        },
        {
          "id": "MDRW2100029652.1.37",
          "form": "예븐 옷 사둬도 입을 일이 없어 그래서 맨날 깨끗한 새옷 버리고 ㅋㅋㅋ",
          "original_form": "예븐 옷 사둬도 입을 일이 없어 그래서 맨날 깨끗한 새옷 버리고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:32"
        },
        {
          "id": "MDRW2100029652.1.38",
