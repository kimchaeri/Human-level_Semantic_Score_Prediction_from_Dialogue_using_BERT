      "utterance": [
        {
          "id": "MDRW2100017449.1.1",
          "form": "학교 개학했나요?",
          "original_form": "학교 개학했나요?",
          "speaker_id": "1",
          "time": "20210826 19:26"
        },
        {
          "id": "MDRW2100017449.1.2",
          "form": "아직 개학은 안했어요!",
          "original_form": "아직 개학은 안했어요!",
          "speaker_id": "2",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017449.1.3",
          "form": "9월에 하나요? 아니면 쭉 비대면 하나요?",
          "original_form": "9월에 하나요? 아니면 쭉 비대면 하나요?",
          "speaker_id": "1",
          "time": "20210826 19:27"
        },
        {
          "id": "MDRW2100017449.1.4",
          "form": "9월에 대면 비대면 병행할것 같아요..",
          "original_form": "9월에 대면 비대면 병행할것 같아요..",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017449.1.5",
          "form": "ㅠㅠ 친구들 많이 보고싶으세요?",
          "original_form": "ㅠㅠ 친구들 많이 보고싶으세요?",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017449.1.6",
          "form": "가끔 친구들이랑 통화도 하고 해서 괜찬..아요!",
          "original_form": "가끔 친구들이랑 통화도 하고 해서 괜찬..아요!",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017449.1.7",
          "form": "동아리 활동 하고있어요?",
          "original_form": "동아리 활동 하고있어요?",
          "speaker_id": "1",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017449.1.8",
          "form": "동아리는 안하고있어요!",
          "original_form": "동아리는 안하고있어요!",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017449.1.9",
          "form": "이과세요 문과세요?",
          "original_form": "이과세요 문과세요?",
          "speaker_id": "1",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017449.1.10",
          "form": "전 이과요!",
          "original_form": "전 이과요!",
          "speaker_id": "2",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017449.1.11",
          "form": "가장 재밌는 과목이 뭔가요?",
          "original_form": "가장 재밌는 과목이 뭔가요?",
          "speaker_id": "1",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100017449.1.12",
          "form": "어.. 없는것 같아요",
          "original_form": "어.. 없는것 같아요",
          "speaker_id": "2",
          "time": "20210826 19:34"
        },
        {
          "id": "MDRW2100017449.1.13",
          "form": "비대면일땐 중간고사 어떻게 보나요!??",
          "original_form": "비대면일땐 중간고사 어떻게 보나요!??",
          "speaker_id": "1",
          "time": "20210826 19:35"
        },
        {
          "id": "MDRW2100017449.1.14",
          "form": "1학기때 대면이었어서 잘 모르겠어요",
          "original_form": "1학기때 대면이었어서 잘 모르겠어요",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017449.1.15",
          "form": "대면으로 보는걸로 알아요",
          "original_form": "대면으로 보는걸로 알아요",
