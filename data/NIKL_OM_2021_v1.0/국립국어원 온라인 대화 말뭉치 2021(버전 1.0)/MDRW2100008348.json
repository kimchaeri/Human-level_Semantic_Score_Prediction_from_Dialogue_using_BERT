      "utterance": [
        {
          "id": "MDRW2100008348.1.1",
          "form": "SNS 즐겨하세요?",
          "original_form": "SNS 즐겨하세요?",
          "speaker_id": "1",
          "time": "20210822 15:19"
        },
        {
          "id": "MDRW2100008348.1.2",
          "form": "네! 즐겨하는 편이에요!",
          "original_form": "네! 즐겨하는 편이에요!",
          "speaker_id": "2",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100008348.1.3",
          "form": "주로 무슨 SNS하시나요?",
          "original_form": "주로 무슨 SNS하시나요?",
          "speaker_id": "1",
          "time": "20210822 15:21"
        },
        {
          "id": "MDRW2100008348.1.4",
          "form": "카카오톡 주로하죠!",
          "original_form": "카카오톡 주로하죠!",
          "speaker_id": "2",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008348.1.5",
          "form": "가끔 인스타그램도 하구요",
          "original_form": "가끔 인스타그램도 하구요",
          "speaker_id": "2",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008348.1.6",
          "form": "저도 카카오톡 주로 해요",
          "original_form": "저도 카카오톡 주로 해요",
          "speaker_id": "1",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008348.1.7",
          "form": "인스타랑 트위터도 하구",
          "original_form": "인스타랑 트위터도 하구",
          "speaker_id": "1",
          "time": "20210822 15:22"
        },
        {
          "id": "MDRW2100008348.1.8",
          "form": "아하 그렇군요!",
          "original_form": "아하 그렇군요!",
          "speaker_id": "2",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008348.1.9",
          "form": "페이스북도 아주가끔 하긴하죠!",
          "original_form": "페이스북도 아주가끔 하긴하죠!",
          "speaker_id": "2",
          "time": "20210822 15:23"
        },
        {
          "id": "MDRW2100008348.1.10",
          "form": "페북은 성인되고 나서 안하게 되는거 같아요",
          "original_form": "페북은 성인되고 나서 안하게 되는거 같아요",
          "speaker_id": "1",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008348.1.11",
          "form": "아하 맞아요",
          "original_form": "아하 맞아요",
          "speaker_id": "2",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008348.1.12",
          "form": "성인되고 나니 페이스북 빈도가 확줄긴하더라고요",
          "original_form": "성인되고 나니 페이스북 빈도가 확줄긴하더라고요",
          "speaker_id": "2",
          "time": "20210822 15:24"
        },
        {
          "id": "MDRW2100008348.1.13",
          "form": "딱 학생때 즐기기 좋은 SNS인거 같아요",
          "original_form": "딱 학생때 즐기기 좋은 SNS인거 같아요",
          "speaker_id": "1",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008348.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008348.1.15",
          "form": "왜 그런건지는 모르겠어요",
          "original_form": "왜 그런건지는 모르겠어요",
          "speaker_id": "1",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008348.1.16",
          "form": "그냥 학창시절때 엄청하고나서",
          "original_form": "그냥 학창시절때 엄청하고나서",
          "speaker_id": "2",
          "time": "20210822 15:27"
        },
        {
          "id": "MDRW2100008348.1.17",
          "form": "이제 성인되면 연락빈도도 줄어들고 질리니",
          "original_form": "이제 성인되면 연락빈도도 줄어들고 질리니",
          "speaker_id": "2",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008348.1.18",
          "form": "그런거같아요",
          "original_form": "그런거같아요",
          "speaker_id": "2",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008348.1.19",
          "form": "연락이 끊기게 되는건 진짜 너무 아쉬워요",
          "original_form": "연락이 끊기게 되는건 진짜 너무 아쉬워요",
          "speaker_id": "1",
          "time": "20210822 15:45"
        },
        {
          "id": "MDRW2100008348.1.20",
          "form": "맞아요 ㅠㅠ 그나마",
          "original_form": "맞아요 ㅠㅠ 그나마",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008348.1.21",
          "form": "카톡이라도 있으니 다행이지만요",
          "original_form": "카톡이라도 있으니 다행이지만요",
          "speaker_id": "2",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008348.1.22",
          "form": "맞아요 카톡은 모두 쓰죠",
          "original_form": "맞아요 카톡은 모두 쓰죠",
          "speaker_id": "1",
          "time": "20210822 15:58"
        },
        {
          "id": "MDRW2100008348.1.23",
          "form": "맞아요! 그래서 그나마",
          "original_form": "맞아요! 그래서 그나마",
          "speaker_id": "2",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008348.1.24",
          "form": "연락 하면서 안부묻고 하는거같아요",
          "original_form": "연락 하면서 안부묻고 하는거같아요",
          "speaker_id": "2",
          "time": "20210822 16:21"
        },
        {
          "id": "MDRW2100008348.1.25",
          "form": "이젠 SNS 없이는 못 살겠어요",
          "original_form": "이젠 SNS 없이는 못 살겠어요",
          "speaker_id": "1",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008348.1.26",
          "form": "코로나라서 더 그런가",
          "original_form": "코로나라서 더 그런가",
          "speaker_id": "1",
          "time": "20210822 16:27"
        },
        {
          "id": "MDRW2100008348.1.27",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008348.1.28",
          "form": "코로나때문에 SNS 빈도가 더 늘어난 느낌",
          "original_form": "코로나때문에 SNS 빈도가 더 늘어난 느낌",
          "speaker_id": "2",
          "time": "20210822 16:30"
        },
        {
          "id": "MDRW2100008348.1.29",
          "form": "만날 수가 없어서 그런가봐요",
          "original_form": "만날 수가 없어서 그런가봐요",
          "speaker_id": "1",
          "time": "20210822 16:31"
        },
        {
          "id": "MDRW2100008348.1.30",
          "form": "그런거같아요 ㅠㅠ",
          "original_form": "그런거같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100008348.1.31",
          "form": "요즘 만나기도 참 무서울때라",
          "original_form": "요즘 만나기도 참 무서울때라",
          "speaker_id": "2",
          "time": "20210822 16:45"
        },
        {
          "id": "MDRW2100008348.1.32",
          "form": "맞죠 코로나가 빨리 끝나야할텐데",
          "original_form": "맞죠 코로나가 빨리 끝나야할텐데",
          "speaker_id": "1",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008348.1.33",
          "form": "언능 종식되야 만나는데 안타깝죠",
          "original_form": "언능 종식되야 만나는데 안타깝죠",
          "speaker_id": "2",
          "time": "20210822 16:50"
        }
      ]
