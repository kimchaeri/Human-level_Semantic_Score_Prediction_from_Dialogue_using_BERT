      "utterance": [
        {
          "id": "MDRW2100026462.1.1",
          "form": "오오오ㅎㅎ",
          "original_form": "오오오ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026462.1.2",
          "form": "저도좋아요ㅎ",
          "original_form": "저도좋아요ㅎ",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026462.1.3",
          "form": "이벤트로 할인해서 사는게 가장 기뻐요ㅎ",
          "original_form": "이벤트로 할인해서 사는게 가장 기뻐요ㅎ",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026462.1.4",
          "form": "근데 그게 쌓여서 어마어마하게 결제되더라구여ㅎ",
          "original_form": "근데 그게 쌓여서 어마어마하게 결제되더라구여ㅎ",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100026462.1.5",
          "form": "노력해서 얻은거지만 괜히 꽁돈으로 구매하는느낌이라ㅋㅋㅋㄱㅋ흐스",
          "original_form": "노력해서 얻은거지만 괜히 꽁돈으로 구매하는느낌이라ㅋㅋㅋㄱㅋ흐스",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026462.1.6",
          "form": "맞아요맞아요ㅎㅎㅎ",
          "original_form": "맞아요맞아요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100026462.1.7",
          "form": "오늘 신세계 네이버 스토어 이벤트하셨어요?",
          "original_form": "오늘 신세계 네이버 스토어 이벤트하셨어요?",
          "speaker_id": "2",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100026462.1.8",
          "form": "아 네네 핫도그샀습니다ㅋㅋㄱㅋ",
          "original_form": "아 네네 핫도그샀습니다ㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100026462.1.9",
          "form": "사셧나용?",
          "original_form": "사셧나용?",
          "speaker_id": "1",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100026462.1.10",
          "form": "아아아ㅎㅎㅎ",
          "original_form": "아아아ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100026462.1.11",
          "form": "다들 핫도그ㅎ",
          "original_form": "다들 핫도그ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100026462.1.12",
          "form": "전 아내 핫도그, 제꺼 만두요ㅎ",
          "original_form": "전 아내 핫도그, 제꺼 만두요ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100026462.1.13",
          "form": "와 두분다성공하셨네요ㅋㅋㅋㅋ득템하셨네요!  사실다른거구경좀하고싶었는데\n쿠폰사라질까봐급하게샀어요ㅋㅋㄱㅜㅜ 핫도그도좋아하긴하지만..ㅎㅎㅎ",
          "original_form": "와 두분다성공하셨네요ㅋㅋㅋㅋ득템하셨네요!  사실다른거구경좀하고싶었는데\n쿠폰사라질까봐급하게샀어요ㅋㅋㄱㅜㅜ 핫도그도좋아하긴하지만..ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100026462.1.14",
          "form": "ㅎㅎ 그래도 성공하셨네요ㅎ",
          "original_form": "ㅎㅎ 그래도 성공하셨네요ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:04"
        },
        {
          "id": "MDRW2100026462.1.15",
          "form": "전 어제 알았어요ㅎㅎㅎ",
          "original_form": "전 어제 알았어요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100026462.1.16",
          "form": "언제아셨어요?",
          "original_form": "언제아셨어요?",
          "speaker_id": "2",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100026462.1.17",
          "form": "저새벽에자기전에 급 알고 들어갔어요ㅋㅋㅋㅋ",
          "original_form": "저새벽에자기전에 급 알고 들어갔어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100026462.1.18",
          "form": "손떨림이..ㅋㅋㄲ",
          "original_form": "손떨림이..ㅋㅋㄲ",
          "speaker_id": "1",
          "time": "20210902 23:17"
        },
        {
          "id": "MDRW2100026462.1.19",
          "form": "ㅎㅎ 저두저두ㅎ",
          "original_form": "ㅎㅎ 저두저두ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100026462.1.20",
          "form": "될까? 이게과연? 이랬었어요ㅎㅎ",
          "original_form": "될까? 이게과연? 이랬었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100026462.1.21",
          "form": "그런이벤트 한번씩올라오는데 최근에 몇번 취소된적있었거든요 오류였다구..\n그래서 취소각오하고 시키긴했어요 ㅋㅋㄱㅋㅜㅜ",
          "original_form": "그런이벤트 한번씩올라오는데 최근에 몇번 취소된적있었거든요 오류였다구..\n그래서 취소각오하고 시키긴했어요 ㅋㅋㄱㅋㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 23:21"
        },
        {
          "id": "MDRW2100026462.1.22",
          "form": "ㅎㅎ저두요ㅎ 이벤트는 그냥 하는거죠 ㅡㅎ",
          "original_form": "ㅎㅎ저두요ㅎ 이벤트는 그냥 하는거죠 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100026462.1.23",
          "form": "해주면 좋고 아니면 말고ㅎㅎ",
          "original_form": "해주면 좋고 아니면 말고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100026462.1.24",
          "form": "오늘도 스벅4개받았는데ㅠ 참 힘드네요ㅠ 너무 스벅가격이 내려갔아요ㅍㅍ",
          "original_form": "오늘도 스벅4개받았는데ㅠ 참 힘드네요ㅠ 너무 스벅가격이 내려갔아요ㅍㅍ",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100026462.1.25",
          "form": "아 깊콘받으신거예용?!  안드시구 그냥 파시나용?!",
          "original_form": "아 깊콘받으신거예용?!  안드시구 그냥 파시나용?!",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100026462.1.26",
          "form": "네네ㅎ 팔아야죠ㅎㅎ",
          "original_form": "네네ㅎ 팔아야죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100026462.1.27",
          "form": "만원권은 그냥 아내줘버리고",
          "original_form": "만원권은 그냥 아내줘버리고",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100026462.1.28",
          "form": "나머지는 팔았어요ㅎ 에고 근데 3천원에 올라오니ㅠㅠ",
          "original_form": "나머지는 팔았어요ㅎ 에고 근데 3천원에 올라오니ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 23:25"
        },
        {
          "id": "MDRW2100026462.1.29",
          "form": "그 깊콘매입하는업체? 거기도3천원정도밖에못받나요ㅜㅜ,..",
          "original_form": "그 깊콘매입하는업체? 거기도3천원정도밖에못받나요ㅜㅜ,..",
          "speaker_id": "1",
          "time": "20210902 23:27"
        },
        {
          "id": "MDRW2100026462.1.30",
          "form": "팔라고라고 앱에서 팔면되요 ㅡㅎ",
          "original_form": "팔라고라고 앱에서 팔면되요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100026462.1.31",
          "form": "전 3100원에 올려놓긴했어요ㅎ",
          "original_form": "전 3100원에 올려놓긴했어요ㅎ",
          "speaker_id": "2",
          "time": "20210902 23:28"
        },
        {
          "id": "MDRW2100026462.1.32",
          "form": "아하 그어플은첨들어봐오ㅡ!  거기서팔면되는군요ㅇㅁㅇ",
          "original_form": "아하 그어플은첨들어봐오ㅡ!  거기서팔면되는군요ㅇㅁㅇ",
          "speaker_id": "1",
          "time": "20210902 23:30"
        },
        {
          "id": "MDRW2100026462.1.33",
          "form": "깊콘당첨많이되시네요근듸ㅣ",
          "original_form": "깊콘당첨많이되시네요근듸ㅣ",
