      "utterance": [
        {
          "id": "MDRW2100029949.1.1",
          "form": "식장 대구?",
          "original_form": "식장 대구?",
          "speaker_id": "2",
          "time": "20210903 17:00"
        },
        {
          "id": "MDRW2100029949.1.2",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:02"
        },
        {
          "id": "MDRW2100029949.1.3",
          "form": "대구서 식올리셧나요",
          "original_form": "대구서 식올리셧나요",
          "speaker_id": "1",
          "time": "20210903 17:02"
        },
        {
          "id": "MDRW2100029949.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210903 17:03"
        },
        {
          "id": "MDRW2100029949.1.5",
          "form": "그 뭐더라",
          "original_form": "그 뭐더라",
          "speaker_id": "2",
          "time": "20210903 17:03"
        },
        {
          "id": "MDRW2100029949.1.6",
          "form": "노비아갈라",
          "original_form": "노비아갈라",
          "speaker_id": "2",
          "time": "20210903 17:03"
        },
        {
          "id": "MDRW2100029949.1.7",
          "form": "아ㅋ거기서많이하더라구요",
          "original_form": "아ㅋ거기서많이하더라구요",
          "speaker_id": "1",
          "time": "20210903 17:03"
        },
        {
          "id": "MDRW2100029949.1.8",
          "form": "어디로 예약?",
          "original_form": "어디로 예약?",
          "speaker_id": "2",
          "time": "20210903 17:03"
        },
        {
          "id": "MDRW2100029949.1.9",
          "form": "밥무러가도됩니까",
          "original_form": "밥무러가도됩니까",
          "speaker_id": "2",
          "time": "20210903 17:03"
        },
        {
          "id": "MDRW2100029949.1.10",
          "form": "신행은 유럽가서 축구볼때까지 미룰꺼에요",
          "original_form": "신행은 유럽가서 축구볼때까지 미룰꺼에요",
          "speaker_id": "1",
          "time": "20210903 17:03"
        },
        {
          "id": "MDRW2100029949.1.11",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:03"
        },
        {
          "id": "MDRW2100029949.1.12",
          "form": "놉",
          "original_form": "놉",
          "speaker_id": "1",
          "time": "20210903 17:04"
        },
        {
          "id": "MDRW2100029949.1.13",
          "form": "식대비쌈요",
          "original_form": "식대비쌈요",
          "speaker_id": "1",
          "time": "20210903 17:04"
        },
        {
          "id": "MDRW2100029949.1.14",
          "form": "어디?",
          "original_form": "어디?",
          "speaker_id": "2",
          "time": "20210903 17:05"
        },
        {
          "id": "MDRW2100029949.1.15",
          "form": "영국스페인",
          "original_form": "영국스페인",
          "speaker_id": "1",
          "time": "20210903 17:06"
        },
        {
          "id": "MDRW2100029949.1.16",
          "form": "저 캄푸누 갓다왓는데",
          "original_form": "저 캄푸누 갓다왓는데",
          "speaker_id": "2",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100029949.1.17",
          "form": "지깁니더 ㅋㅋ",
          "original_form": "지깁니더 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100029949.1.18",
          "form": "뒤에서 막 대마초피고 ㅋㅋ",
          "original_form": "뒤에서 막 대마초피고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 17:16"
        },
        {
          "id": "MDRW2100029949.1.19",
          "form": "오오오오",
          "original_form": "오오오오",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029949.1.20",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:36"
        },
        {
          "id": "MDRW2100029949.1.21",
          "form": "유럽 두번갓는데",
          "original_form": "유럽 두번갓는데",
          "speaker_id": "2",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100029949.1.22",
          "form": "스페인이 좋앗습니다",
          "original_form": "스페인이 좋앗습니다",
          "speaker_id": "2",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100029949.1.23",
          "form": "축하드립니다",
          "original_form": "축하드립니다",
          "speaker_id": "2",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100029949.1.24",
          "form": "근 한달 살앗서요",
          "original_form": "근 한달 살앗서요",
          "speaker_id": "2",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100029949.1.25",
          "form": "스페인 최고",
          "original_form": "스페인 최고",
          "speaker_id": "2",
          "time": "20210903 17:50"
        },
        {
          "id": "MDRW2100029949.1.26",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210903 18:26"
        },
        {
          "id": "MDRW2100029949.1.27",
          "form": "저는 한달동안 한번 갓는데",
          "original_form": "저는 한달동안 한번 갓는데",
          "speaker_id": "1",
          "time": "20210903 18:27"
        },
        {
          "id": "MDRW2100029949.1.28",
          "form": "너무좋앗어요",
          "original_form": "너무좋앗어요",
          "speaker_id": "1",
          "time": "20210903 18:27"
        },
        {
          "id": "MDRW2100029949.1.29",
          "form": "ㅋㅋ스페인을 못가봐서",
          "original_form": "ㅋㅋ스페인을 못가봐서",
          "speaker_id": "1",
          "time": "20210903 18:27"
        },
        {
          "id": "MDRW2100029949.1.30",
          "form": "기대중임다",
          "original_form": "기대중임다",
          "speaker_id": "1",
          "time": "20210903 18:27"
        },
        {
          "id": "MDRW2100029949.1.31",
          "form": "진짜 부럽습니다 아아아",
          "original_form": "진짜 부럽습니다 아아아",
          "speaker_id": "2",
          "time": "20210903 18:32"
        },
        {
          "id": "MDRW2100029949.1.32",
          "form": "갈 수 있을 때 많이 가야 돼요",
          "original_form": "갈 수 있을 때 많이 가야 돼요",
          "speaker_id": "2",
          "time": "20210903 18:32"
        },
        {
          "id": "MDRW2100029949.1.33",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "1",
          "time": "20210903 18:32"
        },
        {
          "id": "MDRW2100029949.1.34",
          "form": "더놀다 육아세계 갈려구요",
          "original_form": "더놀다 육아세계 갈려구요",
          "speaker_id": "1",
          "time": "20210903 18:32"
        },
        {
          "id": "MDRW2100029949.1.35",
          "form": "미루는중임다",
          "original_form": "미루는중임다",
          "speaker_id": "1",
          "time": "20210903 18:32"
        },
        {
          "id": "MDRW2100029949.1.36",
          "form": "아직 덜내려놧어유",
          "original_form": "아직 덜내려놧어유",
          "speaker_id": "1",
          "time": "20210903 18:32"
        }
      ]
