      "utterance": [
        {
          "id": "MDRW2100047729.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100047729.1.2",
          "form": "제일 최근에 구매한게 뭔가요?",
          "original_form": "제일 최근에 구매한게 뭔가요?",
          "speaker_id": "1",
          "time": "20210827 10:59"
        },
        {
          "id": "MDRW2100047729.1.3",
          "form": "안녕하세요 저는 동물의숲 타이틀을 샀어요",
          "original_form": "안녕하세요 저는 동물의숲 타이틀을 샀어요",
          "speaker_id": "2",
          "time": "20210827 11:02"
        },
        {
          "id": "MDRW2100047729.1.4",
          "form": "닌텐도 하세요??",
          "original_form": "닌텐도 하세요??",
          "speaker_id": "1",
          "time": "20210827 11:03"
        },
        {
          "id": "MDRW2100047729.1.5",
          "form": "부러워요!",
          "original_form": "부러워요!",
          "speaker_id": "1",
          "time": "20210827 11:03"
        },
        {
          "id": "MDRW2100047729.1.6",
          "form": "닌텐도가 여러 시리즈가 있던데 어떤거 쓰세요?",
          "original_form": "닌텐도가 여러 시리즈가 있던데 어떤거 쓰세요?",
          "speaker_id": "1",
          "time": "20210827 11:05"
        },
        {
          "id": "MDRW2100047729.1.7",
          "form": "닌텐도 스위치요!",
          "original_form": "닌텐도 스위치요!",
          "speaker_id": "2",
          "time": "20210827 11:08"
        },
        {
          "id": "MDRW2100047729.1.8",
          "form": "올해 초에 마련했어요",
          "original_form": "올해 초에 마련했어요",
          "speaker_id": "2",
          "time": "20210827 11:09"
        },
        {
          "id": "MDRW2100047729.1.9",
          "form": "그때라면 스위치 대란이 엄청나지않았나요??",
          "original_form": "그때라면 스위치 대란이 엄청나지않았나요??",
          "speaker_id": "1",
          "time": "20210827 11:11"
        },
        {
          "id": "MDRW2100047729.1.10",
          "form": "다행이도 대란 끝물에 사서 힘들지않았어요",
          "original_form": "다행이도 대란 끝물에 사서 힘들지않았어요",
          "speaker_id": "2",
          "time": "20210827 11:20"
        },
        {
          "id": "MDRW2100047729.1.11",
          "form": "다행이에요 ㅋㅋㅋ",
          "original_form": "다행이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:22"
        },
        {
          "id": "MDRW2100047729.1.12",
          "form": "매장에서 사신건가요?",
          "original_form": "매장에서 사신건가요?",
          "speaker_id": "1",
          "time": "20210827 11:23"
        },
        {
          "id": "MDRW2100047729.1.13",
          "form": "스위치는 한우리 매장에서 샀구요",
          "original_form": "스위치는 한우리 매장에서 샀구요",
          "speaker_id": "2",
          "time": "20210827 11:37"
        },
        {
          "id": "MDRW2100047729.1.14",
          "form": "타이틀들은 중고로샀어요!",
          "original_form": "타이틀들은 중고로샀어요!",
          "speaker_id": "2",
          "time": "20210827 11:37"
        },
        {
          "id": "MDRW2100047729.1.15",
          "form": "중고거래도 하시네요",
          "original_form": "중고거래도 하시네요",
          "speaker_id": "1",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100047729.1.16",
          "form": "좋죠 저렴하게 살 수 있고!",
          "original_form": "좋죠 저렴하게 살 수 있고!",
          "speaker_id": "1",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100047729.1.17",
          "form": "그죠! 중고거래 해보셨나요?",
          "original_form": "그죠! 중고거래 해보셨나요?",
          "speaker_id": "2",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100047729.1.18",
          "form": "꽤 해봤어요",
          "original_form": "꽤 해봤어요",
          "speaker_id": "1",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100047729.1.19",
          "form": "제가 팔아보기도 하구요!",
          "original_form": "제가 팔아보기도 하구요!",
          "speaker_id": "1",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100047729.1.20",
          "form": "뭐 팔아보셨나요?",
          "original_form": "뭐 팔아보셨나요?",
          "speaker_id": "2",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100047729.1.21",
          "form": "책, 기프티콘, 식당상품권 정도요!",
          "original_form": "책, 기프티콘, 식당상품권 정도요!",
          "speaker_id": "1",
          "time": "20210827 11:52"
        },
        {
          "id": "MDRW2100047729.1.22",
          "form": "쏠쏠해요 ㅎㅎㅎ",
          "original_form": "쏠쏠해요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 11:52"
        },
        {
          "id": "MDRW2100047729.1.23",
          "form": "은근 쏠쏠하져",
          "original_form": "은근 쏠쏠하져",
          "speaker_id": "2",
          "time": "20210827 11:53"
        },
        {
          "id": "MDRW2100047729.1.24",
          "form": "책 잘팔리나요?",
          "original_form": "책 잘팔리나요?",
          "speaker_id": "2",
          "time": "20210827 11:53"
        },
        {
          "id": "MDRW2100047729.1.25",
          "form": "개인적으로 책팔기가 제일 힘들던데...",
          "original_form": "개인적으로 책팔기가 제일 힘들던데...",
          "speaker_id": "2",
          "time": "20210827 11:53"
        }
      ]
    }
  ]
