      "utterance": [
        {
          "id": "MDRW2100006446.1.1",
          "form": "지분 투자면 주식도 들어갈까요?",
          "original_form": "지분 투자면 주식도 들어갈까요?",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100006446.1.2",
          "form": "주식 하셍ㆍ?",
          "original_form": "주식 하셍ㆍ?",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100006446.1.3",
          "form": "네~",
          "original_form": "네~",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100006446.1.4",
          "form": "전 하다 쉬고있어욪ㅋ",
          "original_form": "전 하다 쉬고있어욪ㅋ",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100006446.1.5",
          "form": "오~ 잘 빠지셨네요",
          "original_form": "오~ 잘 빠지셨네요",
          "speaker_id": "1",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100006446.1.6",
          "form": "돈을 빼신게 아니시군요",
          "original_form": "돈을 빼신게 아니시군요",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100006446.1.7",
          "form": "네ㅋㅋ 빼셨나요??",
          "original_form": "네ㅋㅋ 빼셨나요??",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100006446.1.8",
          "form": "아뇨~ 전 다양한 회사에 지분 투자중이예요 라고 말하고 실제론 물린. ㅠ",
          "original_form": "아뇨~ 전 다양한 회사에 지분 투자중이예요 라고 말하고 실제론 물린. ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100006446.1.9",
          "form": "ㅋㅋ 아 저도 다이소",
          "original_form": "ㅋㅋ 아 저도 다이소",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100006446.1.10",
          "form": "우리 다양한 회사에 투자중이라고 생각하자고요",
          "original_form": "우리 다양한 회사에 투자중이라고 생각하자고요",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100006446.1.11",
          "form": "대한민국 경제의 한 축을 담당하는거죠!",
          "original_form": "대한민국 경제의 한 축을 담당하는거죠!",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100006446.1.12",
          "form": "그렇게 좋게 생각해요 ㅎㅎㄹ",
          "original_form": "그렇게 좋게 생각해요 ㅎㅎㄹ",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100006446.1.13",
          "form": "ㅜㅜ 다들 미장은 대박난다던뎌...",
          "original_form": "ㅜㅜ 다들 미장은 대박난다던뎌...",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100006446.1.14",
          "form": "전 못하겠더라고요",
          "original_form": "전 못하겠더라고요",
          "speaker_id": "1",
          "time": "20210908 00:32"
        }
      ]
