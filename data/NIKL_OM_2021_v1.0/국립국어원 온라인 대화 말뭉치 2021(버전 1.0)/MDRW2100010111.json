      "utterance": [
        {
          "id": "MDRW2100010111.1.1",
          "form": "최근에 구매하신 거 있어요?",
          "original_form": "최근에 구매하신 거 있어요?",
          "speaker_id": "1",
          "time": "20210823 14:28"
        },
        {
          "id": "MDRW2100010111.1.2",
          "form": "저 패딩이요!",
          "original_form": "저 패딩이요!",
          "speaker_id": "2",
          "time": "20210823 14:28"
        },
        {
          "id": "MDRW2100010111.1.3",
          "form": "패딩을 벌써 구매하셨어요??",
          "original_form": "패딩을 벌써 구매하셨어요??",
          "speaker_id": "1",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010111.1.4",
          "form": "지금 사야 싸요",
          "original_form": "지금 사야 싸요",
          "speaker_id": "2",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010111.1.5",
          "form": "패딩 있긴한데 저도 가격 괜찮으면 사야겠네요ㅋㅋ",
          "original_form": "패딩 있긴한데 저도 가격 괜찮으면 사야겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:29"
        },
        {
          "id": "MDRW2100010111.1.6",
          "form": "스파브랜드에서 롱패딩 마지막세일 하는 거같으니까 얼른 사세요",
          "original_form": "스파브랜드에서 롱패딩 마지막세일 하는 거같으니까 얼른 사세요",
          "speaker_id": "2",
          "time": "20210823 14:30"
        },
        {
          "id": "MDRW2100010111.1.7",
          "form": "알겠습니다ㅋㅋ",
          "original_form": "알겠습니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:30"
        },
        {
          "id": "MDRW2100010111.1.8",
          "form": "쇼핑좋아하세요?",
          "original_form": "쇼핑좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 14:31"
        },
        {
          "id": "MDRW2100010111.1.9",
          "form": "음 혼자 쇼핑하는 거좋아해요",
          "original_form": "음 혼자 쇼핑하는 거좋아해요",
          "speaker_id": "1",
          "time": "20210823 14:31"
        },
        {
          "id": "MDRW2100010111.1.10",
          "form": "어 저두요!",
          "original_form": "어 저두요!",
          "speaker_id": "2",
          "time": "20210823 14:31"
        },
        {
          "id": "MDRW2100010111.1.11",
          "form": "혼자 지하상가가서 쇼핑하는 거 좋아해요",
          "original_form": "혼자 지하상가가서 쇼핑하는 거 좋아해요",
          "speaker_id": "2",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010111.1.12",
          "form": "저랑 비슷하시네요ㅋㅋ 지하상가는 점원들이 신경을 덜 써서 쇼핑하는 게 편해요",
          "original_form": "저랑 비슷하시네요ㅋㅋ 지하상가는 점원들이 신경을 덜 써서 쇼핑하는 게 편해요",
          "speaker_id": "1",
          "time": "20210823 14:32"
        },
        {
          "id": "MDRW2100010111.1.13",
          "form": "맞아요 점원분들 따라다니시면 쇼핑하는거 넘 불편하죠ㅜㅜ",
          "original_form": "맞아요 점원분들 따라다니시면 쇼핑하는거 넘 불편하죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 14:33"
        },
        {
          "id": "MDRW2100010111.1.14",
          "form": "그래서 쇼핑하러 안 갈때도많아요",
          "original_form": "그래서 쇼핑하러 안 갈때도많아요",
          "speaker_id": "1",
          "time": "20210823 14:33"
        },
        {
          "id": "MDRW2100010111.1.15",
          "form": "요즘은 폰으로 다 시키면되니까요ㅋㅋ",
          "original_form": "요즘은 폰으로 다 시키면되니까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 14:34"
        },
        {
          "id": "MDRW2100010111.1.16",
          "form": "자주 이용하는 쇼핑어플 있으세요?",
          "original_form": "자주 이용하는 쇼핑어플 있으세요?",
          "speaker_id": "1",
          "time": "20210823 14:34"
        },
        {
          "id": "MDRW2100010111.1.17",
          "form": "저는 지그재그요!",
          "original_form": "저는 지그재그요!",
          "speaker_id": "2",
          "time": "20210823 14:34"
        },
        {
          "id": "MDRW2100010111.1.18",
          "form": "저두 지그재그나 에이블리 이용해요",
          "original_form": "저두 지그재그나 에이블리 이용해요",
          "speaker_id": "1",
          "time": "20210823 14:34"
        },
        {
          "id": "MDRW2100010111.1.19",
          "form": "쇼핑몰이 많이 입점해서 다양하더라고요",
          "original_form": "쇼핑몰이 많이 입점해서 다양하더라고요",
          "speaker_id": "1",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010111.1.20",
          "form": "에이블리는 좋은데 요즘 옷 말고 문구잡화를 많이 파는 거같아서 잘 안쓰게 돼요",
          "original_form": "에이블리는 좋은데 요즘 옷 말고 문구잡화를 많이 파는 거같아서 잘 안쓰게 돼요",
          "speaker_id": "2",
          "time": "20210823 14:35"
        },
        {
          "id": "MDRW2100010111.1.21",
          "form": "저는 문구잡화 쇼핑하는 것도 좋아해서 좋더라구요",
          "original_form": "저는 문구잡화 쇼핑하는 것도 좋아해서 좋더라구요",
          "speaker_id": "1",
          "time": "20210823 14:37"
        },
        {
          "id": "MDRW2100010111.1.22",
          "form": "저 요즘 안쓰는 거 중고거래하고있어요",
          "original_form": "저 요즘 안쓰는 거 중고거래하고있어요",
          "speaker_id": "1",
          "time": "20210823 14:38"
        }
      ]
    }
  ]
