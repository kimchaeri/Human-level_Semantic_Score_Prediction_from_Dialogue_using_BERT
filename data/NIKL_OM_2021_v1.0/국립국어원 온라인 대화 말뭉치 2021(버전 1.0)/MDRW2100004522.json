      "utterance": [
        {
          "id": "MDRW2100004522.1.1",
          "form": "다다음주... 수업이 시작된다",
          "original_form": "다다음주... 수업이 시작된다",
          "speaker_id": "1",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004522.1.2",
          "form": "수강신청 진짜 이번 수강신청 가장 심장떨렸던거 같음",
          "original_form": "수강신청 진짜 이번 수강신청 가장 심장떨렸던거 같음",
          "speaker_id": "2",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004522.1.3",
          "form": "다다음주가 수업이라니....",
          "original_form": "다다음주가 수업이라니....",
          "speaker_id": "2",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004522.1.4",
          "form": "아 진짜 하기싫다...",
          "original_form": "아 진짜 하기싫다...",
          "speaker_id": "2",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004522.1.5",
          "form": "이나이먹고 졸업도 못하고 수업이나 더 들어야 된다니...",
          "original_form": "이나이먹고 졸업도 못하고 수업이나 더 들어야 된다니...",
          "speaker_id": "2",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004522.1.6",
          "form": "나는 무능해...",
          "original_form": "나는 무능해...",
          "speaker_id": "2",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004522.1.7",
          "form": "그래도 이번학기 끝나면 졸업이잖아",
          "original_form": "그래도 이번학기 끝나면 졸업이잖아",
          "speaker_id": "1",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004522.1.8",
          "form": "끝나도 토익점수 안나오면 졸업 못함",
          "original_form": "끝나도 토익점수 안나오면 졸업 못함",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004522.1.9",
          "form": "취직하면 가능할지도",
          "original_form": "취직하면 가능할지도",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004522.1.10",
          "form": "취직이냐 토익이냐 그것이 문제로다!",
          "original_form": "취직이냐 토익이냐 그것이 문제로다!",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004522.1.11",
          "form": "요즘 대학 왜 들어왔나 생각도 들고 좀 기분이 우울해...",
          "original_form": "요즘 대학 왜 들어왔나 생각도 들고 좀 기분이 우울해...",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004522.1.12",
          "form": "대학등록금 가지고 창업했으면 그게 더 보람찼겠다...",
          "original_form": "대학등록금 가지고 창업했으면 그게 더 보람찼겠다...",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004522.1.13",
          "form": "라는 생각도 들고",
          "original_form": "라는 생각도 들고",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004522.1.14",
          "form": "걍 이대로 대학생이다가",
          "original_form": "걍 이대로 대학생이다가",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004522.1.15",
          "form": "생활기반이 안정되면 그때 졸업하는 시스템이었으면!!",
          "original_form": "생활기반이 안정되면 그때 졸업하는 시스템이었으면!!",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004522.1.16",
          "form": "에이 뭐 그런 생각을 해",
          "original_form": "에이 뭐 그런 생각을 해",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004522.1.17",
          "form": "다 잘 될거라구",
          "original_form": "다 잘 될거라구",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004522.1.18",
          "form": "살다보면 지금의 고난은 별거 아닌게 되잖아",
          "original_form": "살다보면 지금의 고난은 별거 아닌게 되잖아",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004522.1.19",
          "form": "라는 생각도 들면서 걍 학교에서 날 고용해 줬으면 좋겠어...",
          "original_form": "라는 생각도 들면서 걍 학교에서 날 고용해 줬으면 좋겠어...",
          "speaker_id": "2",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004522.1.20",
          "form": "학교 고용 우선순위는 대학원 졸업생일걸?",
          "original_form": "학교 고용 우선순위는 대학원 졸업생일걸?",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004522.1.21",
          "form": "아 그런것도 있어?",
          "original_form": "아 그런것도 있어?",
          "speaker_id": "2",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004522.1.22",
          "form": "그래도 대학원 사절",
          "original_form": "그래도 대학원 사절",
          "speaker_id": "2",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004522.1.23",
          "form": "안가안가",
          "original_form": "안가안가",
          "speaker_id": "2",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004522.1.24",
          "form": "나느 더이상 학교라는 기관에 갇히고 싶지 않아ㅠㅠ",
          "original_form": "나느 더이상 학교라는 기관에 갇히고 싶지 않아ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004522.1.25",
          "form": "학교라는 기관은 다 별로였는데",
          "original_form": "학교라는 기관은 다 별로였는데",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004522.1.26",
          "form": "왜이리 학교를 싫어해",
          "original_form": "왜이리 학교를 싫어해",
          "speaker_id": "1",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004522.1.27",
          "form": "그나마 나은거 뽑으라면 고등학교였음",
          "original_form": "그나마 나은거 뽑으라면 고등학교였음",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004522.1.28",
          "form": "나라는 멋진 친구를 얻은거잖아",
          "original_form": "나라는 멋진 친구를 얻은거잖아",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004522.1.29",
          "form": "후후",
          "original_form": "후후",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004522.1.30",
          "form": "그땐 내가 하고싶은 공부만 했거든",
          "original_form": "그땐 내가 하고싶은 공부만 했거든",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004522.1.31",
          "form": "그건 그래 후후",
          "original_form": "그건 그래 후후",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004522.1.32",
          "form": "대학와서 멋진 인연들을 꽤나 만들었지",
          "original_form": "대학와서 멋진 인연들을 꽤나 만들었지",
          "speaker_id": "2",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004522.1.33",
          "form": "그것만 생각해도 되는거지",
          "original_form": "그것만 생각해도 되는거지",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004522.1.34",
          "form": "솔직히 대학생활... 즐기진 못했지만",
          "original_form": "솔직히 대학생활... 즐기진 못했지만",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004522.1.35",
          "form": "그래도 같이 여행 다닌거 생각하면 즐거웠어",
          "original_form": "그래도 같이 여행 다닌거 생각하면 즐거웠어",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004522.1.36",
          "form": "졸업즈음엔 다시 여행가면 좋겠다.",
          "original_form": "졸업즈음엔 다시 여행가면 좋겠다.",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004522.1.37",
          "form": "졸업기념 여행 이런거지!",
          "original_form": "졸업기념 여행 이런거지!",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004522.1.38",
          "form": "하지만 다른건 진짜 별루였어ㅠㅠ",
          "original_form": "하지만 다른건 진짜 별루였어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004522.1.39",
          "form": "말이 별루지 그냥 다시 가라하면 안갈거야ㅠ",
          "original_form": "말이 별루지 그냥 다시 가라하면 안갈거야ㅠ",
          "speaker_id": "2",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004522.1.40",
          "form": "아예 대학 자체를 안 갈건 아니잖아",
          "original_form": "아예 대학 자체를 안 갈건 아니잖아",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004522.1.41",
          "form": "그 당시에 가장 최선의 선택을 한거고",
          "original_form": "그 당시에 가장 최선의 선택을 한거고",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004522.1.42",
          "form": "그러니 후회하지 말어",
          "original_form": "그러니 후회하지 말어",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004522.1.43",
          "form": "도서관가서 흥미있는 주제 위주로 책읽을래ㅠ",
          "original_form": "도서관가서 흥미있는 주제 위주로 책읽을래ㅠ",
          "speaker_id": "2",
          "time": "20210821 17:13"
        }
      ]
