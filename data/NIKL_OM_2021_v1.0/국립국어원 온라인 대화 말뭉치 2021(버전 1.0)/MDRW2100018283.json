      "utterance": [
        {
          "id": "MDRW2100018283.1.1",
          "form": "제주도에서는 뭐하셨어여?",
          "original_form": "제주도에서는 뭐하셨어여?",
          "speaker_id": "2",
          "time": "20210826 23:35"
        },
        {
          "id": "MDRW2100018283.1.2",
          "form": "진짜 아무것도 안했어요",
          "original_form": "진짜 아무것도 안했어요",
          "speaker_id": "1",
          "time": "20210826 23:35"
        },
        {
          "id": "MDRW2100018283.1.3",
          "form": "오후 되야 나가고...",
          "original_form": "오후 되야 나가고...",
          "speaker_id": "1",
          "time": "20210826 23:35"
        },
        {
          "id": "MDRW2100018283.1.4",
          "form": "차로 드라이브만 했어요",
          "original_form": "차로 드라이브만 했어요",
          "speaker_id": "1",
          "time": "20210826 23:35"
        },
        {
          "id": "MDRW2100018283.1.5",
          "form": "봄이어서 유채꽃길 이런데ㅋ",
          "original_form": "봄이어서 유채꽃길 이런데ㅋ",
          "speaker_id": "1",
          "time": "20210826 23:35"
        },
        {
          "id": "MDRW2100018283.1.6",
          "form": "뭐 많이 하셨네요 안하셨다더닠ㅋㅋ",
          "original_form": "뭐 많이 하셨네요 안하셨다더닠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100018283.1.7",
          "form": "유채꽃도 안보고 잠만 자다온 제친구보다는 낫네욬ㅋㅋㅋ",
          "original_form": "유채꽃도 안보고 잠만 자다온 제친구보다는 낫네욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100018283.1.8",
          "form": "친구가 과음했나봐요ㅋㅋ",
          "original_form": "친구가 과음했나봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100018283.1.9",
          "form": "걘 잠자러 다녀온거 같다고 했어여",
          "original_form": "걘 잠자러 다녀온거 같다고 했어여",
          "speaker_id": "2",
          "time": "20210826 23:36"
        },
        {
          "id": "MDRW2100018283.1.10",
          "form": "힐링하고왔네요",
          "original_form": "힐링하고왔네요",
          "speaker_id": "1",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100018283.1.11",
          "form": "ㅋㅋㅋㅋ 그 전날까지 일하다가",
          "original_form": "ㅋㅋㅋㅋ 그 전날까지 일하다가",
          "speaker_id": "2",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100018283.1.12",
          "form": "아이고 듣기만해도 슬퍼라ㅋㅋ",
          "original_form": "아이고 듣기만해도 슬퍼라ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100018283.1.13",
          "form": "티켓 급하게 끊어서 다녀왔대요",
          "original_form": "티켓 급하게 끊어서 다녀왔대요",
          "speaker_id": "2",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100018283.1.14",
          "form": "그때가 코로나 심한줄 알았는데",
          "original_form": "그때가 코로나 심한줄 알았는데",
          "speaker_id": "1",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100018283.1.15",
          "form": "그때 더 열심히 놀껄 후회 되네요",
          "original_form": "그때 더 열심히 놀껄 후회 되네요",
          "speaker_id": "1",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100018283.1.16",
          "form": "곧주말인데",
          "original_form": "곧주말인데",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018283.1.17",
          "form": "어디 안가세요?",
          "original_form": "어디 안가세요?",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018283.1.18",
          "form": "ㅋㅋㅋ 생각보다 재미있네여",
          "original_form": "ㅋㅋㅋ 생각보다 재미있네여",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018283.1.19",
          "form": "저도 오늘 첨인데ㅋ 잘 호응해주셔서 감사해여",
          "original_form": "저도 오늘 첨인데ㅋ 잘 호응해주셔서 감사해여",
          "speaker_id": "1",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018283.1.20",
          "form": "이번 주말에는 아빠따라서 밭에나 다녀올거 같아여",
          "original_form": "이번 주말에는 아빠따라서 밭에나 다녀올거 같아여",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018283.1.21",
          "form": "아 이번주엔 못가겠구나 주사 맞으셔서",
          "original_form": "아 이번주엔 못가겠구나 주사 맞으셔서",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018283.1.22",
          "form": "모기 조심 하세요",
          "original_form": "모기 조심 하세요",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018283.1.23",
          "form": "와 맞아요 모기 장난 아니에요",
          "original_form": "와 맞아요 모기 장난 아니에요",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018283.1.24",
          "form": "저희 아빠도 일욜에 백신 맞아요ㅋㅋ",
          "original_form": "저희 아빠도 일욜에 백신 맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018283.1.25",
          "form": "요즘 모기 미쳤어여",
          "original_form": "요즘 모기 미쳤어여",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018283.1.26",
          "form": "진짜 조심하세요",
          "original_form": "진짜 조심하세요",
          "speaker_id": "1",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018283.1.27",
          "form": "저저번주에 밭 따라다녀왔는데요 종아리 장난아님",
          "original_form": "저저번주에 밭 따라다녀왔는데요 종아리 장난아님",
          "speaker_id": "2",
          "time": "20210826 23:40"
        },
        {
          "id": "MDRW2100018283.1.28",
          "form": "지금도 자국이 있어요",
          "original_form": "지금도 자국이 있어요",
          "speaker_id": "2",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018283.1.29",
          "form": "전 나이먹으니까 자국도 오래가요",
          "original_form": "전 나이먹으니까 자국도 오래가요",
          "speaker_id": "1",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018283.1.30",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210826 23:41"
        },
        {
          "id": "MDRW2100018283.1.31",
          "form": "ㅠㅠ 슬프다",
          "original_form": "ㅠㅠ 슬프다",
          "speaker_id": "2",
          "time": "20210826 23:41"
        }
      ]
    }
