      "utterance": [
        {
          "id": "MDRW2100023539.1.1",
          "form": "점심 드셨나요 ~!!",
          "original_form": "점심 드셨나요 ~!!",
          "speaker_id": "2",
          "time": "20210830 12:32"
        },
        {
          "id": "MDRW2100023539.1.2",
          "form": "토스트 배달시켜먹었어요~~",
          "original_form": "토스트 배달시켜먹었어요~~",
          "speaker_id": "1",
          "time": "20210830 13:53"
        },
        {
          "id": "MDRW2100023539.1.3",
          "form": "옛날식 토스트 너무 맛있어요ㅠ",
          "original_form": "옛날식 토스트 너무 맛있어요ㅠ",
          "speaker_id": "1",
          "time": "20210830 13:53"
        },
        {
          "id": "MDRW2100023539.1.4",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 13:53"
        },
        {
          "id": "MDRW2100023539.1.5",
          "form": "마라탕면 먹었습니당 ㅋㅋㅋㅋ 힣",
          "original_form": "마라탕면 먹었습니당 ㅋㅋㅋㅋ 힣",
          "speaker_id": "2",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100023539.1.6",
          "form": "마라탕 맛있어요?",
          "original_form": "마라탕 맛있어요?",
          "speaker_id": "1",
          "time": "20210830 13:57"
        },
        {
          "id": "MDRW2100023539.1.7",
          "form": "한번도 안 먹어봤어요",
          "original_form": "한번도 안 먹어봤어요",
          "speaker_id": "1",
          "time": "20210830 13:58"
        },
        {
          "id": "MDRW2100023539.1.8",
          "form": "근처도 안 가봄..",
          "original_form": "근처도 안 가봄..",
          "speaker_id": "1",
          "time": "20210830 13:58"
        },
        {
          "id": "MDRW2100023539.1.9",
          "form": "은근 중독되는 맛이있는데 ㅋㅋㅋㅋ 걍 시작하지 마세요 ㅋㅋㅋㅋ",
          "original_form": "은근 중독되는 맛이있는데 ㅋㅋㅋㅋ 걍 시작하지 마세요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:59"
        },
        {
          "id": "MDRW2100023539.1.10",
          "form": "너무 자극적인맛 ㅋㅋㅋㅋㅋ",
          "original_form": "너무 자극적인맛 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:00"
        },
        {
          "id": "MDRW2100023539.1.11",
          "form": "위에 안좋은거같아여 ㅋㅋㅋㅋ",
          "original_form": "위에 안좋은거같아여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:00"
        },
        {
          "id": "MDRW2100023539.1.12",
          "form": "ㅋㅋㅋ매워서 자극적인거죠?",
          "original_form": "ㅋㅋㅋ매워서 자극적인거죠?",
          "speaker_id": "1",
          "time": "20210830 14:01"
        },
        {
          "id": "MDRW2100023539.1.13",
          "form": "향신료 그런게 강한게 아니라",
          "original_form": "향신료 그런게 강한게 아니라",
          "speaker_id": "1",
          "time": "20210830 14:01"
        },
        {
          "id": "MDRW2100023539.1.14",
          "form": "향도 특이하긴해요 ㅋㅋㅋㅋㅋ 맵기도하고 ㅋㅋㅋ 센거먹으면 에린맛도 있어요 ㅋㅋ",
          "original_form": "향도 특이하긴해요 ㅋㅋㅋㅋㅋ 맵기도하고 ㅋㅋㅋ 센거먹으면 에린맛도 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:05"
        },
        {
          "id": "MDRW2100023539.1.15",
          "form": "전 맵찔이라",
          "original_form": "전 맵찔이라",
          "speaker_id": "1",
          "time": "20210830 14:06"
        },
        {
          "id": "MDRW2100023539.1.16",
          "form": "최애 음식 하나 꼽으라면 무엇인가여?!",
          "original_form": "최애 음식 하나 꼽으라면 무엇인가여?!",
          "speaker_id": "2",
          "time": "20210830 14:06"
        },
        {
          "id": "MDRW2100023539.1.17",
          "form": "도전할일 없을거같아요ㅋㅋ",
          "original_form": "도전할일 없을거같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:06"
        },
        {
          "id": "MDRW2100023539.1.18",
          "form": "하... 하나만 고르는건........",
          "original_form": "하... 하나만 고르는건........",
          "speaker_id": "1",
          "time": "20210830 14:06"
        },
        {
          "id": "MDRW2100023539.1.19",
          "form": "갈비찜! 못먹은지 몇년됐어요ㅜ",
          "original_form": "갈비찜! 못먹은지 몇년됐어요ㅜ",
          "speaker_id": "1",
          "time": "20210830 14:07"
        },
        {
          "id": "MDRW2100023539.1.20",
          "form": "으앙 갈비찜 ㅜㅜ 저도 엄마표 갈비찜 먹고싶네요..",
          "original_form": "으앙 갈비찜 ㅜㅜ 저도 엄마표 갈비찜 먹고싶네요..",
          "speaker_id": "2",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100023539.1.21",
          "form": "요리 해드시기도 하나여?!",
          "original_form": "요리 해드시기도 하나여?!",
          "speaker_id": "2",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100023539.1.22",
          "form": "본가살때는 자주 했는데",
          "original_form": "본가살때는 자주 했는데",
          "speaker_id": "1",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100023539.1.23",
          "form": "이사온집이 주방이 너무 좁고 도구도 없어서 거의 안해먹어요 ㅜ",
          "original_form": "이사온집이 주방이 너무 좁고 도구도 없어서 거의 안해먹어요 ㅜ",
          "speaker_id": "1",
          "time": "20210830 14:12"
        },
        {
          "id": "MDRW2100023539.1.24",
          "form": "아 그럼 진짜 ㅜㅜ 냄새빼는 것도 은근 일이고 ㅜㅜ",
          "original_form": "아 그럼 진짜 ㅜㅜ 냄새빼는 것도 은근 일이고 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023539.1.25",
          "form": "저도 엄청 간단한거 아니면 ㅋㅋㅋ",
          "original_form": "저도 엄청 간단한거 아니면 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023539.1.26",
          "form": "근데 걍 요리를 못해요 원래 ㅋㅋㅋㅋ",
          "original_form": "근데 걍 요리를 못해요 원래 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023539.1.27",
          "form": "저도요ㅋㅋ그냥 뭐 못먹을정도만 아니면",
          "original_form": "저도요ㅋㅋ그냥 뭐 못먹을정도만 아니면",
          "speaker_id": "1",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100023539.1.28",
          "form": "합리화하고 먹어요ㅋㅋㅋ",
          "original_form": "합리화하고 먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100023539.1.29",
          "form": "유튜트에 백종원레시피가 그나마 좀 쉽더라고요",
          "original_form": "유튜트에 백종원레시피가 그나마 좀 쉽더라고요",
          "speaker_id": "1",
          "time": "20210830 14:16"
        }
      ]
