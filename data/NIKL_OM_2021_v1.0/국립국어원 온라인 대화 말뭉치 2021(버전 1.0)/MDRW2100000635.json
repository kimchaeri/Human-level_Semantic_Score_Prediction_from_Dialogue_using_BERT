      "utterance": [
        {
          "id": "MDRW2100000635.1.1",
          "form": "요즘 하고 계신 게임있으신가요",
          "original_form": "요즘 하고 계신 게임있으신가요",
          "speaker_id": "1",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000635.1.2",
          "form": "게임잘안해여",
          "original_form": "게임잘안해여",
          "speaker_id": "2",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000635.1.3",
          "form": "잘못해서",
          "original_form": "잘못해서",
          "speaker_id": "2",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000635.1.4",
          "form": "ㅠㅜㅜㅜ",
          "original_form": "ㅠㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 17:19"
        },
        {
          "id": "MDRW2100000635.1.5",
          "form": "앗 그럼 보는것도 안 좋아하시나요??",
          "original_form": "앗 그럼 보는것도 안 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000635.1.6",
          "form": "보는것도잘",
          "original_form": "보는것도잘",
          "speaker_id": "2",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000635.1.7",
          "form": "다들마니하던데",
          "original_form": "다들마니하던데",
          "speaker_id": "2",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000635.1.8",
          "form": "현질도하구",
          "original_form": "현질도하구",
          "speaker_id": "2",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000635.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000635.1.10",
          "form": "전 게임할때 현질은 안해요 ㅋㅋ",
          "original_form": "전 게임할때 현질은 안해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 17:20"
        },
        {
          "id": "MDRW2100000635.1.11",
          "form": "돈이 아까워서...",
          "original_form": "돈이 아까워서...",
          "speaker_id": "1",
          "time": "20210819 17:21"
        },
        {
          "id": "MDRW2100000635.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000635.1.13",
          "form": "즐기는위주군요",
          "original_form": "즐기는위주군요",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000635.1.14",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000635.1.15",
          "form": "그게좋져",
          "original_form": "그게좋져",
          "speaker_id": "2",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000635.1.16",
          "form": "현질은 너무 앞서가서 즐기지도 못하고 ㄹ그래요..",
          "original_form": "현질은 너무 앞서가서 즐기지도 못하고 ㄹ그래요..",
          "speaker_id": "1",
          "time": "20210819 17:22"
        },
        {
          "id": "MDRW2100000635.1.17",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000635.1.18",
          "form": "돈은안쓰는게",
          "original_form": "돈은안쓰는게",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000635.1.19",
          "form": "정신에조쳐",
          "original_form": "정신에조쳐",
          "speaker_id": "2",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000635.1.20",
          "form": "맞아요 거기에 집착하게 돼요..ㅜ",
          "original_form": "맞아요 거기에 집착하게 돼요..ㅜ",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
          "id": "MDRW2100000635.1.21",
          "form": "그럼 옛날에도 해봣던 게임이 없으신건가요",
          "original_form": "그럼 옛날에도 해봣던 게임이 없으신건가요",
          "speaker_id": "1",
          "time": "20210819 17:23"
        },
        {
