      "utterance": [
        {
          "id": "MDRW2100034650.1.1",
          "form": "네 키핑도 가능하구요",
          "original_form": "네 키핑도 가능하구요",
          "speaker_id": "2",
          "time": "20210828 16:55"
        },
        {
          "id": "MDRW2100034650.1.2",
          "form": "아 그 어플 좋더라구요!",
          "original_form": "아 그 어플 좋더라구요!",
          "speaker_id": "1",
          "time": "20210828 16:56"
        },
        {
          "id": "MDRW2100034650.1.3",
          "form": "아이스클림 투플원할때 키핑해두면 좋아요",
          "original_form": "아이스클림 투플원할때 키핑해두면 좋아요",
          "speaker_id": "2",
          "time": "20210828 16:57"
        },
        {
          "id": "MDRW2100034650.1.4",
          "form": "어떨땐 마트보다 싸더라구요",
          "original_form": "어떨땐 마트보다 싸더라구요",
          "speaker_id": "2",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100034650.1.5",
          "form": "맞아요 그리고 편의점은",
          "original_form": "맞아요 그리고 편의점은",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100034650.1.6",
          "form": "다른데서 안파는걸 많이 팔아서 좋더라구요!",
          "original_form": "다른데서 안파는걸 많이 팔아서 좋더라구요!",
          "speaker_id": "1",
          "time": "20210828 16:58"
        },
        {
          "id": "MDRW2100034650.1.7",
          "form": "낱개로도 살 수 있어서 좋구요 다품 소량판매라 좋아요",
          "original_form": "낱개로도 살 수 있어서 좋구요 다품 소량판매라 좋아요",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100034650.1.8",
          "form": "맞아요 1인가구에겐 더 좋을거같아요",
          "original_form": "맞아요 1인가구에겐 더 좋을거같아요",
          "speaker_id": "1",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100034650.1.9",
          "form": "맞아요 ㅋㅋ 라면도 이젠 마트서 낱개로 안파니깐 더더욱이요,,",
          "original_form": "맞아요 ㅋㅋ 라면도 이젠 마트서 낱개로 안파니깐 더더욱이요,,",
          "speaker_id": "2",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100034650.1.10",
          "form": "맞아요 ㅜㅜ 전 항상 낱개로 구비하는데 난감해요",
          "original_form": "맞아요 ㅜㅜ 전 항상 낱개로 구비하는데 난감해요",
          "speaker_id": "1",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100034650.1.11",
          "form": "이마트몰조차 라면을 번들로 파니",
          "original_form": "이마트몰조차 라면을 번들로 파니",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100034650.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100034650.1.13",
          "form": "낱개로 팔아달란 말이에요 ㅠ",
          "original_form": "낱개로 팔아달란 말이에요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100034650.1.14",
          "form": "너무많음 부담스러워요",
          "original_form": "너무많음 부담스러워요",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100034650.1.15",
          "form": "ㅠㅠ 편의점 라면은 솔직히 비싸지만 낱개로 먹어볼땐 거기서 사야해요",
          "original_form": "ㅠㅠ 편의점 라면은 솔직히 비싸지만 낱개로 먹어볼땐 거기서 사야해요",
          "speaker_id": "2",
          "time": "20210828 17:03"
        }
      ]
