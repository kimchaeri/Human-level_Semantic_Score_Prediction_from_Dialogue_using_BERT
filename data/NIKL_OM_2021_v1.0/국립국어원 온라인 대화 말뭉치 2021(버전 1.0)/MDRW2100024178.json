      "utterance": [
        {
          "id": "MDRW2100024178.1.1",
          "form": "넌 어떤 동물이 좋은가 ?",
          "original_form": "넌 어떤 동물이 좋은가 ?",
          "speaker_id": "1",
          "time": "20210830 22:58"
        },
        {
          "id": "MDRW2100024178.1.2",
          "form": "나 호랑이 !! ㅋㅋㅋ",
          "original_form": "나 호랑이 !! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 10:40"
        },
        {
          "id": "MDRW2100024178.1.3",
          "form": "반려동물이 아니라 아쉽ㅠ",
          "original_form": "반려동물이 아니라 아쉽ㅠ",
          "speaker_id": "2",
          "time": "20210904 10:40"
        },
        {
          "id": "MDRW2100024178.1.4",
          "form": "헉 호랑이",
          "original_form": "헉 호랑이",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100024178.1.5",
          "form": "잪혀 먹겠다 ㅋㅋㅋ",
          "original_form": "잪혀 먹겠다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 10:51"
        },
        {
          "id": "MDRW2100024178.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ어흥!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ어흥!",
          "speaker_id": "2",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100024178.1.7",
          "form": "반려동물이라는게 꼭 정해진게 아니야",
          "original_form": "반려동물이라는게 꼭 정해진게 아니야",
          "speaker_id": "2",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100024178.1.8",
          "form": "애완동물에서 파생?된 말이고",
          "original_form": "애완동물에서 파생?된 말이고",
          "speaker_id": "2",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100024178.1.9",
          "form": "애완동물 뜻은 인간의 행복을 목적으로 기르는 동물",
          "original_form": "애완동물 뜻은 인간의 행복을 목적으로 기르는 동물",
          "speaker_id": "2",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100024178.1.10",
          "form": "인가 그 엇비슷한데",
          "original_form": "인가 그 엇비슷한데",
          "speaker_id": "2",
          "time": "20210904 13:31"
        },
        {
          "id": "MDRW2100024178.1.11",
          "form": "결국 어떤동물이든 목적에따라 나뉜다는거지 가축이랑",
          "original_form": "결국 어떤동물이든 목적에따라 나뉜다는거지 가축이랑",
          "speaker_id": "2",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100024178.1.12",
          "form": "그것도 아니면 야생동물이 되고",
          "original_form": "그것도 아니면 야생동물이 되고",
          "speaker_id": "2",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100024178.1.13",
          "form": "먼 소리 그래도 호랑인 키우면서도 위험해",
          "original_form": "먼 소리 그래도 호랑인 키우면서도 위험해",
          "speaker_id": "1",
          "time": "20210904 13:32"
        },
        {
          "id": "MDRW2100024178.1.14",
          "form": "호랑이도 애완동물로 키울수있다는거지",
          "original_form": "호랑이도 애완동물로 키울수있다는거지",
          "speaker_id": "2",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100024178.1.15",
          "form": "반려동물으루",
          "original_form": "반려동물으루",
          "speaker_id": "2",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100024178.1.16",
          "form": "아 그래 그래서",
          "original_form": "아 그래 그래서",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100024178.1.17",
          "form": "키우려면 독맆해서 키워",
          "original_form": "키우려면 독맆해서 키워",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100024178.1.18",
          "form": "당근ㅋㅋㅋ한국에서는 못키울걸ㅠ",
          "original_form": "당근ㅋㅋㅋ한국에서는 못키울걸ㅠ",
          "speaker_id": "2",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100024178.1.19",
          "form": "호랑이 닮은 고양이면 몰라더",
          "original_form": "호랑이 닮은 고양이면 몰라더",
          "speaker_id": "2",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100024178.1.20",
