      "utterance": [
        {
          "id": "MDRW2100036416.1.1",
          "form": "어우 오늘의 후식은 멜론입니다",
          "original_form": "어우 오늘의 후식은 멜론입니다",
          "speaker_id": "2",
          "time": "20210831 00:04"
        },
        {
          "id": "MDRW2100036416.1.2",
          "form": "멜론 좋죠 ㅠ .. 너무 맛있어요!",
          "original_form": "멜론 좋죠 ㅠ .. 너무 맛있어요!",
          "speaker_id": "1",
          "time": "20210831 12:32"
        },
        {
          "id": "MDRW2100036416.1.3",
          "form": "진짜 달달하니... 시원하니....",
          "original_form": "진짜 달달하니... 시원하니....",
          "speaker_id": "2",
          "time": "20210831 13:18"
        },
        {
          "id": "MDRW2100036416.1.4",
          "form": "맞아요... 진짜 맛있죠 ㅜㅜ",
          "original_form": "맞아요... 진짜 맛있죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 13:21"
        },
        {
          "id": "MDRW2100036416.1.5",
          "form": "전 심지어 살짝 얼려먹었거든요",
          "original_form": "전 심지어 살짝 얼려먹었거든요",
          "speaker_id": "2",
          "time": "20210831 15:37"
        },
        {
          "id": "MDRW2100036416.1.6",
          "form": "진짜 너무마쉿....",
          "original_form": "진짜 너무마쉿....",
          "speaker_id": "2",
          "time": "20210831 15:37"
        },
        {
          "id": "MDRW2100036416.1.7",
          "form": "과일 얼려먹음 진짜 맛있죠 약간 샤베트처럼 아삭달달 ..",
          "original_form": "과일 얼려먹음 진짜 맛있죠 약간 샤베트처럼 아삭달달 ..",
          "speaker_id": "1",
          "time": "20210831 22:53"
        },
        {
          "id": "MDRW2100036416.1.8",
          "form": "아아 상상해버렸어요...",
          "original_form": "아아 상상해버렸어요...",
          "speaker_id": "2",
          "time": "20210901 08:38"
        },
        {
          "id": "MDRW2100036416.1.9",
          "form": "홍시도 얼려먹음 최곤거 아시죠",
          "original_form": "홍시도 얼려먹음 최곤거 아시죠",
          "speaker_id": "2",
          "time": "20210901 08:38"
        },
        {
          "id": "MDRW2100036416.1.10",
          "form": "아 그럼요그럼요... 홍시 얼려먹으면 그냥 최고죠...ㅠ",
          "original_form": "아 그럼요그럼요... 홍시 얼려먹으면 그냥 최고죠...ㅠ",
          "speaker_id": "1",
          "time": "20210901 11:07"
        },
        {
          "id": "MDRW2100036416.1.11",
          "form": "크으으... 저 복숭아안에 요거트넣고 얼리는거 그것도 먹어보고싶더라구요",
          "original_form": "크으으... 저 복숭아안에 요거트넣고 얼리는거 그것도 먹어보고싶더라구요",
          "speaker_id": "2",
          "time": "20210901 13:23"
        },
        {
          "id": "MDRW2100036416.1.12",
          "form": "그거 맛있어요.. 맛있는거+맛있는거 조합..",
          "original_form": "그거 맛있어요.. 맛있는거+맛있는거 조합..",
          "speaker_id": "1",
          "time": "20210901 13:27"
        },
        {
          "id": "MDRW2100036416.1.13",
          "form": "아주 맛있어용 ..bb",
          "original_form": "아주 맛있어용 ..bb",
          "speaker_id": "1",
          "time": "20210901 13:27"
        },
        {
          "id": "MDRW2100036416.1.14",
          "form": "아직 안목어봤는데...",
          "original_form": "아직 안목어봤는데...",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100036416.1.15",
          "form": "크나큰 기대",
          "original_form": "크나큰 기대",
          "speaker_id": "2",
          "time": "20210901 19:32"
        },
        {
          "id": "MDRW2100036416.1.16",
          "form": "꼭 드셔보세오.. 강추입니다..",
          "original_form": "꼭 드셔보세오.. 강추입니다..",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100036416.1.17",
          "form": "직접 만들어먹어도 맛있나유?",
          "original_form": "직접 만들어먹어도 맛있나유?",
          "speaker_id": "2",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100036416.1.18",
          "form": "네네 맛있어요 복숭아가 맛이있기만 하면 맛있는 것 같아요",
          "original_form": "네네 맛있어요 복숭아가 맛이있기만 하면 맛있는 것 같아요",
          "speaker_id": "1",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100036416.1.19",
          "form": "헐 댑악",
          "original_form": "헐 댑악",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100036416.1.20",
          "form": "저는 대박 맛있는 복숭아를 가지고있습니다",
          "original_form": "저는 대박 맛있는 복숭아를 가지고있습니다",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100036416.1.21",
          "form": "도전해보세요ㅡ..! 맛있습니다",
          "original_form": "도전해보세요ㅡ..! 맛있습니다",
          "speaker_id": "1",
