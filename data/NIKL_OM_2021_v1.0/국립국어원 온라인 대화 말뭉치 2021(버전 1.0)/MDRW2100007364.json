      "utterance": [
        {
          "id": "MDRW2100007364.1.1",
          "form": "혹시 아직 방학 중이신가요?",
          "original_form": "혹시 아직 방학 중이신가요?",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007364.1.2",
          "form": "네 ㅎㅎ 30일 개강이에요 ㅠㅠ",
          "original_form": "네 ㅎㅎ 30일 개강이에요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007364.1.3",
          "form": "저도... 30일 개강이요 ㅠㅠㅠ",
          "original_form": "저도... 30일 개강이요 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007364.1.4",
          "form": "1교시.. 가야해요",
          "original_form": "1교시.. 가야해요",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007364.1.5",
          "form": "... 어째서죠",
          "original_form": "... 어째서죠",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007364.1.6",
          "form": "저는 31일 1교시요...ㅎ",
          "original_form": "저는 31일 1교시요...ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007364.1.7",
          "form": "괜찮아요 전...",
          "original_form": "괜찮아요 전...",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007364.1.8",
          "form": "31일에도 1교시가 있어서..",
          "original_form": "31일에도 1교시가 있어서..",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007364.1.9",
          "form": "그날 하루만 힘들진.. 않답니다",
          "original_form": "그날 하루만 힘들진.. 않답니다",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007364.1.10",
          "form": "혹시 공강 있으세요..??",
          "original_form": "혹시 공강 있으세요..??",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007364.1.11",
          "form": "저 금공강이요!!",
          "original_form": "저 금공강이요!!",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007364.1.12",
          "form": "학교 다니면서 공강 없으면..",
          "original_form": "학교 다니면서 공강 없으면..",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007364.1.13",
          "form": "울어요",
          "original_form": "울어요",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007364.1.14",
          "form": "저는 없긴한데",
          "original_form": "저는 없긴한데",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007364.1.15",
          "form": "ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007364.1.16",
          "form": "수강 신청은..  수강신청은 잘 되셨나요?!",
          "original_form": "수강 신청은..  수강신청은 잘 되셨나요?!",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007364.1.17",
          "form": "앗...저 월요일에 수강신청해요",
          "original_form": "앗...저 월요일에 수강신청해요",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007364.1.18",
          "form": "짱느리죠",
          "original_form": "짱느리죠",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007364.1.19",
          "form": "하하핫 ㅠㅡㅠㅜㅜㅜㅜ",
          "original_form": "하하핫 ㅠㅡㅠㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007364.1.20",
          "form": "제 기운 받아가세요",
          "original_form": "제 기운 받아가세요",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007364.1.21",
          "form": "감사합니당... 사실 시간표도",
          "original_form": "감사합니당... 사실 시간표도",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007364.1.22",
          "form": "짜여서 나와서",
          "original_form": "짜여서 나와서",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007364.1.23",
          "form": "폰으로 했는데 올클 했답니다",
          "original_form": "폰으로 했는데 올클 했답니다",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007364.1.24",
          "form": "의미가 없어요...",
          "original_form": "의미가 없어요...",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007364.1.25",
          "form": "앗...",
          "original_form": "앗...",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007364.1.26",
          "form": "교선 1개만",
          "original_form": "교선 1개만",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007364.1.27",
          "form": "짜서 나오면서 공강이 없다니",
          "original_form": "짜서 나오면서 공강이 없다니",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007364.1.28",
          "form": "잡으면 되긴해요",
          "original_form": "잡으면 되긴해요",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007364.1.29",
          "form": "시간표 담당자 나와라",
          "original_form": "시간표 담당자 나와라",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007364.1.30",
          "form": "혼내쥰다",
          "original_form": "혼내쥰다",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007364.1.31",
          "form": "후... 진짜 ",
          "original_form": "후... 진짜 ",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007364.1.32",
          "form": "금욜에 2시간만 듣는데",
          "original_form": "금욜에 2시간만 듣는데",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007364.1.33",
          "form": "좀 옮겨주지 ㅜㅠ 흑",
          "original_form": "좀 옮겨주지 ㅜㅠ 흑",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007364.1.34",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "1",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007364.1.35",
          "form": "통학으로 가셔야하는거에요?!?!",
          "original_form": "통학으로 가셔야하는거에요?!?!",
          "speaker_id": "1",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007364.1.36",
          "form": "진짜 너무해",
          "original_form": "진짜 너무해",
          "speaker_id": "1",
          "time": "20210822 01:48"
        },
        {
          "id": "MDRW2100007364.1.37",
          "form": "금요일에 듣는 과목은 비대면 수업일거같아요!!",
          "original_form": "금요일에 듣는 과목은 비대면 수업일거같아요!!",
          "speaker_id": "2",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007364.1.38",
          "form": "너무 다행스러워요...",
          "original_form": "너무 다행스러워요...",
          "speaker_id": "1",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007364.1.39",
          "form": "두시간 들으러 통학은 너무..",
          "original_form": "두시간 들으러 통학은 너무..",
          "speaker_id": "1",
          "time": "20210822 01:51"
        },
        {
          "id": "MDRW2100007364.1.40",
          "form": "그쵸... 근데 출석하고 잘것같아요... ㅋㅋㅋ",
          "original_form": "그쵸... 근데 출석하고 잘것같아요... ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007364.1.41",
          "form": "이번엔 진짜 열심히 해야하는데 후ㅜㅠ",
          "original_form": "이번엔 진짜 열심히 해야하는데 후ㅜㅠ",
          "speaker_id": "2",
          "time": "20210822 01:52"
        },
        {
          "id": "MDRW2100007364.1.42",
          "form": "이번엔 진짜 4점대 한번찍고싶어요",
          "original_form": "이번엔 진짜 4점대 한번찍고싶어요",
          "speaker_id": "1",
          "time": "20210822 01:53"
        },
        {
          "id": "MDRW2100007364.1.43",
