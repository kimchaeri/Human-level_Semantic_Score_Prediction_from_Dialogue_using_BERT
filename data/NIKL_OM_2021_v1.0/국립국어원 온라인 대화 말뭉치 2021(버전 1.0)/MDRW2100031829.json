        {
          "id": "MDRW2100031829.1.7",
          "form": "긍게/",
          "original_form": "긍게/",
          "speaker_id": "2",
          "time": "20210905 01:43"
        },
        {
          "id": "MDRW2100031829.1.8",
          "form": "name3가 든든한name2 만나서 다행이다",
