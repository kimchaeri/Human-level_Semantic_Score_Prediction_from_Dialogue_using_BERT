      "utterance": [
        {
          "id": "MDRW2100013216.1.1",
          "form": "아 정말 끔찍하죠..",
          "original_form": "아 정말 끔찍하죠..",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013216.1.2",
          "form": "바퀴약 치세요",
          "original_form": "바퀴약 치세요",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013216.1.3",
          "form": "어떤게 좋아요?",
          "original_form": "어떤게 좋아요?",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013216.1.4",
          "form": "맥스포스랑",
          "original_form": "맥스포스랑",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013216.1.5",
          "form": "페스트세븐",
          "original_form": "페스트세븐",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013216.1.6",
          "form": "저는 둘다 써요",
          "original_form": "저는 둘다 써요",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013216.1.7",
          "form": "역시",
          "original_form": "역시",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013216.1.8",
          "form": "저도 그거 쓰는데 ㅜㅜ",
          "original_form": "저도 그거 쓰는데 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013216.1.9",
          "form": "효과없어요?",
          "original_form": "효과없어요?",
          "speaker_id": "2",
          "time": "20210825 13:33"
        },
        {
          "id": "MDRW2100013216.1.10",
          "form": "큰바퀴 유입은 막은 거 같은데",
          "original_form": "큰바퀴 유입은 막은 거 같은데",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013216.1.11",
          "form": "진작에 알까놨나봐요",
          "original_form": "진작에 알까놨나봐요",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013216.1.12",
          "form": "헉 어떡해요",
          "original_form": "헉 어떡해요",
          "speaker_id": "2",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013216.1.13",
          "form": "어린 바퀴는",
          "original_form": "어린 바퀴는",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013216.1.14",
          "form": "약을 안 먹는대요",
          "original_form": "약을 안 먹는대요",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013216.1.15",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013216.1.16",
          "form": "그래서 일단 약 발랐는데",
          "original_form": "그래서 일단 약 발랐는데",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013216.1.17",
          "form": "자꾸 똥싸놓으니까",
          "original_form": "자꾸 똥싸놓으니까",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013216.1.18",
          "form": "치울때 너무 스트레스 받아요",
          "original_form": "치울때 너무 스트레스 받아요",
          "speaker_id": "1",
          "time": "20210825 13:34"
        },
        {
          "id": "MDRW2100013216.1.19",
          "form": "똥이라니...",
          "original_form": "똥이라니...",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100013216.1.20",
          "form": "너무 싫네요",
          "original_form": "너무 싫네요",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100013216.1.21",
          "form": "네 정말 스트레스..",
          "original_form": "네 정말 스트레스..",
          "speaker_id": "1",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100013216.1.22",
          "form": "어제 대충 약 바르긴 했는데",
          "original_form": "어제 대충 약 바르긴 했는데",
          "speaker_id": "1",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100013216.1.23",
          "form": "언제 한번 싹 청소해야죠..",
          "original_form": "언제 한번 싹 청소해야죠..",
          "speaker_id": "1",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100013216.1.24",
          "form": "네 그것도 일이네요.",
          "original_form": "네 그것도 일이네요.",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100013216.1.25",
          "form": "오늘도 화장실에서 바퀴 봐서 ㅠㅠ",
          "original_form": "오늘도 화장실에서 바퀴 봐서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100013216.1.26",
          "form": "바퀴약 또 뿌렸어요",
          "original_form": "바퀴약 또 뿌렸어요",
          "speaker_id": "1",
          "time": "20210827 14:52"
        }
      ]
    }
