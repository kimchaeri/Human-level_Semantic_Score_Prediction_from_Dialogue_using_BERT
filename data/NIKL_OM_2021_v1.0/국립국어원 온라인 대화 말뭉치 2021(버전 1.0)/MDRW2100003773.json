        },
        {
          "id": "MDRW2100003773.1.2",
          "form": "안녕하세요ㅎ",
          "original_form": "안녕하세요ㅎ",
          "speaker_id": "2",
          "time": "20210821 11:59"
        },
        {
          "id": "MDRW2100003773.1.3",
          "form": "책뭐좋아하세요?",
          "original_form": "책뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 11:59"
        },
        {
          "id": "MDRW2100003773.1.4",
          "form": "저는 철학책이나 만화책 좋아해요",
          "original_form": "저는 철학책이나 만화책 좋아해요",
          "speaker_id": "1",
          "time": "20210821 12:09"
        },
        {
          "id": "MDRW2100003773.1.5",
          "form": "만화책 좋아하세요?",
          "original_form": "만화책 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 12:09"
        },
        {
          "id": "MDRW2100003773.1.6",
          "form": "만화책좋아해요ㅎㅎ 근데 어릴때좋아했던것밖에ㄹ 요즘꺼는 잘몰라요 ㅡㅎ",
          "original_form": "만화책좋아해요ㅎㅎ 근데 어릴때좋아했던것밖에ㄹ 요즘꺼는 잘몰라요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210821 12:24"
        },
        {
          "id": "MDRW2100003773.1.7",
          "form": "그럼 평소에 어떤책 읽으세요?",
          "original_form": "그럼 평소에 어떤책 읽으세요?",
          "speaker_id": "1",
          "time": "20210821 12:28"
        },
        {
          "id": "MDRW2100003773.1.8",
          "form": "재테크책 많이 읽어요ㅎ",
          "original_form": "재테크책 많이 읽어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100003773.1.9",
          "form": "최근에는 돈요정 밍키언니 책읽었고",
          "original_form": "최근에는 돈요정 밍키언니 책읽었고",
          "speaker_id": "2",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100003773.1.10",
          "form": "그전에는 주식이랑 코인 책들 위주로요ㅎ",
          "original_form": "그전에는 주식이랑 코인 책들 위주로요ㅎ",
          "speaker_id": "2",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100003773.1.11",
          "form": "주식관련 책도 읽어보셨어요?",
          "original_form": "주식관련 책도 읽어보셨어요?",
          "speaker_id": "1",
          "time": "20210821 15:44"
        },
        {
          "id": "MDRW2100003773.1.12",
          "form": "주식은 조금밖에 안읽었어요ㅎㅎ 거의 코인위주로 읽고ㅎ 부동산책도 좀보규요ㅎ",
          "original_form": "주식은 조금밖에 안읽었어요ㅎㅎ 거의 코인위주로 읽고ㅎ 부동산책도 좀보규요ㅎ",
          "speaker_id": "2",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100003773.1.13",
          "form": "그거 읽고 수익에 영향있었어요?",
          "original_form": "그거 읽고 수익에 영향있었어요?",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100003773.1.14",
          "form": "음... 조금정도?",
          "original_form": "음... 조금정도?",
          "speaker_id": "2",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100003773.1.15",
          "form": "별로 없더라구요ㅎ 정말 전업투자하지않는 이상은 직장인이 시간써가면서 투자하는게 쉽지는 않아요ㅎ",
          "original_form": "별로 없더라구요ㅎ 정말 전업투자하지않는 이상은 직장인이 시간써가면서 투자하는게 쉽지는 않아요ㅎ",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100003773.1.16",
          "form": "단기투자 하세요 장기투자하세요?",
          "original_form": "단기투자 하세요 장기투자하세요?",
          "speaker_id": "1",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100003773.1.17",
          "form": "전 단기투자에요ㅎㅎ 무조건ㅎ 장투를 해야하는데 잘안되네요ㅎ 하루에 등락을 많이하면 장투하는거랑 똑같애서ㅎ 그러다 물리기도하는데ㅎㅎ 어짜피 내가 사고싶은 가격은 아니었어도 사려고 했던거니 강제 장투가 되죠 ㅡㅎ",
          "original_form": "전 단기투자에요ㅎㅎ 무조건ㅎ 장투를 해야하는데 잘안되네요ㅎ 하루에 등락을 많이하면 장투하는거랑 똑같애서ㅎ 그러다 물리기도하는데ㅎㅎ 어짜피 내가 사고싶은 가격은 아니었어도 사려고 했던거니 강제 장투가 되죠 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100003773.1.18",
          "form": "주식은 본업있으면 그냥 재미로 하는편이 좋은거같아요 ㅎㅎ",
          "original_form": "주식은 본업있으면 그냥 재미로 하는편이 좋은거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 17:05"
        }
      ]
    }
