      "utterance": [
        {
          "id": "MDRW2100033566.1.1",
          "form": "집안일 산더미인데",
          "original_form": "집안일 산더미인데",
          "speaker_id": "2",
          "time": "20210824 14:40"
        },
        {
          "id": "MDRW2100033566.1.2",
          "form": "저두요..그냥 쉬고있어요",
          "original_form": "저두요..그냥 쉬고있어요",
          "speaker_id": "1",
          "time": "20210824 14:41"
        },
        {
          "id": "MDRW2100033566.1.3",
          "form": "안하고 있어요 ㅋㅋ",
          "original_form": "안하고 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:41"
        },
        {
          "id": "MDRW2100033566.1.4",
          "form": "그나마 할만한것과 너무 싫은 것 무엇이 있나요",
          "original_form": "그나마 할만한것과 너무 싫은 것 무엇이 있나요",
          "speaker_id": "1",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100033566.1.5",
          "form": "설거지 젤 싫어요",
          "original_form": "설거지 젤 싫어요",
          "speaker_id": "2",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100033566.1.6",
          "form": "빨래는 세탁기가 해서 좋구요",
          "original_form": "빨래는 세탁기가 해서 좋구요",
          "speaker_id": "2",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100033566.1.7",
          "form": "어떤게 싫으세요?",
          "original_form": "어떤게 싫으세요?",
          "speaker_id": "2",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100033566.1.8",
          "form": "음 저는.. 청소나 정리는 그래도 할만한데..요리? 가 싫어요 ㅋ",
          "original_form": "음 저는.. 청소나 정리는 그래도 할만한데..요리? 가 싫어요 ㅋ",
          "speaker_id": "1",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100033566.1.9",
          "form": "저도 요리 싫어요",
          "original_form": "저도 요리 싫어요",
          "speaker_id": "2",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100033566.1.10",
          "form": "요새 배달 막시켰어요",
          "original_form": "요새 배달 막시켰어요",
          "speaker_id": "2",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100033566.1.11",
          "form": "레시피보고도 잘 안되고요 ㅋㅋ",
          "original_form": "레시피보고도 잘 안되고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:46"
        },
        {
          "id": "MDRW2100033566.1.12",
          "form": "그나마 만개의 레시피 어플 보면 할만 하더라구요 저도 요리 ㄸ손이예요 ㅋ",
          "original_form": "그나마 만개의 레시피 어플 보면 할만 하더라구요 저도 요리 ㄸ손이예요 ㅋ",
          "speaker_id": "1",
          "time": "20210824 15:02"
        },
        {
          "id": "MDRW2100033566.1.13",
          "form": "오 그런게 있군요 좋은 정보네요",
          "original_form": "오 그런게 있군요 좋은 정보네요",
          "speaker_id": "2",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100033566.1.14",
          "form": "저도 똥손요 아들이 엄마 실험하지 말래요 ㅋㅋ",
          "original_form": "저도 똥손요 아들이 엄마 실험하지 말래요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:03"
        },
        {
          "id": "MDRW2100033566.1.15",
          "form": "ㅋㅋ 아들이 좋아하는 반찬은 뭐 있나요",
          "original_form": "ㅋㅋ 아들이 좋아하는 반찬은 뭐 있나요",
          "speaker_id": "1",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100033566.1.16",
          "form": "고기죠 ㅋㅋ",
          "original_form": "고기죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:05"
        },
        {
          "id": "MDRW2100033566.1.17",
          "form": "맨날 고기고기 해요",
          "original_form": "맨날 고기고기 해요",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100033566.1.18",
          "form": "뭐 자주 하세요?",
          "original_form": "뭐 자주 하세요?",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100033566.1.19",
          "form": "저도 반찬보다는 면 요리를 잘 먹어서 종종 해줘요",
          "original_form": "저도 반찬보다는 면 요리를 잘 먹어서 종종 해줘요",
          "speaker_id": "1",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100033566.1.20",
          "form": "아 저도 좋아하는데 자주는 안해줬어요 .칼국수 끓여 봐야 겠어요 ㅎㅎ",
          "original_form": "아 저도 좋아하는데 자주는 안해줬어요 .칼국수 끓여 봐야 겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 15:07"
        },
        {
          "id": "MDRW2100033566.1.21",
          "form": "비오니 따뜻하고 국물 면 먹고 싶어요 ㅎㅎ",
          "original_form": "비오니 따뜻하고 국물 면 먹고 싶어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 15:08"
        },
        {
          "id": "MDRW2100033566.1.22",
          "form": "떡국도 잘 먹더라구요~ㅋ",
          "original_form": "떡국도 잘 먹더라구요~ㅋ",
          "speaker_id": "1",
          "time": "20210824 15:10"
        }
      ]
