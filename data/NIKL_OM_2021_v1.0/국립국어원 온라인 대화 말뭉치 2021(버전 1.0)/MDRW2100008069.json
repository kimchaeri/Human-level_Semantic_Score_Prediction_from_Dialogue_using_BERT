      "utterance": [
        {
          "id": "MDRW2100008069.1.1",
          "form": "뭐드셨나요",
          "original_form": "뭐드셨나요",
          "speaker_id": "1",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100008069.1.2",
          "form": "볶음밥 먹었어요~ 점심에",
          "original_form": "볶음밥 먹었어요~ 점심에",
          "speaker_id": "2",
          "time": "20210822 13:18"
        },
        {
          "id": "MDRW2100008069.1.3",
          "form": "무슨 볶음밥인가요",
          "original_form": "무슨 볶음밥인가요",
          "speaker_id": "1",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100008069.1.4",
          "form": "어제 먹던 고기 볶음에 김치 넣어서 볶았어요",
          "original_form": "어제 먹던 고기 볶음에 김치 넣어서 볶았어요",
          "speaker_id": "2",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008069.1.5",
          "form": "점심 드셨나요~?",
          "original_form": "점심 드셨나요~?",
          "speaker_id": "2",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008069.1.6",
          "form": "맛있겠군요..전 아점 소머리곰탕먹고",
          "original_form": "맛있겠군요..전 아점 소머리곰탕먹고",
          "speaker_id": "1",
          "time": "20210822 13:22"
        },
        {
          "id": "MDRW2100008069.1.7",
          "form": "간식으로 도넛 먹고 있어요...뜬금 배고파지네요",
          "original_form": "간식으로 도넛 먹고 있어요...뜬금 배고파지네요",
          "speaker_id": "1",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008069.1.8",
          "form": "마라샹궈 땡기는데 1인에겐 부담스러워요",
          "original_form": "마라샹궈 땡기는데 1인에겐 부담스러워요",
          "speaker_id": "1",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100008069.1.9",
          "form": "오 소머리곰탕 ㅋㅋㅋ사드신건가요?",
          "original_form": "오 소머리곰탕 ㅋㅋㅋ사드신건가요?",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008069.1.10",
          "form": "마라샹궈는 안먹어 봤네요",
          "original_form": "마라샹궈는 안먹어 봤네요",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100008069.1.11",
          "form": "엄마가 온라인으로 주문하신게 어제 왔는데..진하고 맛있더라고요",
          "original_form": "엄마가 온라인으로 주문하신게 어제 왔는데..진하고 맛있더라고요",
          "speaker_id": "1",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100008069.1.12",
          "form": "마라탕은 드셔보셨나요",
          "original_form": "마라탕은 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100008069.1.13",
          "form": "오옹 요즘 온라인에 파는군요",
          "original_form": "오옹 요즘 온라인에 파는군요",
          "speaker_id": "2",
          "time": "20210822 13:52"
        },
        {
          "id": "MDRW2100008069.1.14",
          "form": "넵 그건 먹어봤어요!!",
          "original_form": "넵 그건 먹어봤어요!!",
          "speaker_id": "2",
          "time": "20210822 13:52"
        },
        {
          "id": "MDRW2100008069.1.15",
          "form": "맛있긴한데 주위에서 안좋아해서 아주 가끔 먹네요",
          "original_form": "맛있긴한데 주위에서 안좋아해서 아주 가끔 먹네요",
          "speaker_id": "2",
          "time": "20210822 13:53"
        },
        {
          "id": "MDRW2100008069.1.16",
          "form": "마라탕은 혼자먹기에도 괜찮긴하죠... 주기적으로 땡겨요",
          "original_form": "마라탕은 혼자먹기에도 괜찮긴하죠... 주기적으로 땡겨요",
          "speaker_id": "1",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008069.1.17",
          "form": "혼밥 잘하시나봐요~",
          "original_form": "혼밥 잘하시나봐요~",
          "speaker_id": "2",
          "time": "20210822 14:00"
        },
        {
          "id": "MDRW2100008069.1.18",
          "form": "혼밥이야 어지간한건 쉽죠... 요즘 다들 시국도 안 좋은데 1인손님이라도 가주면 좋을테고요",
          "original_form": "혼밥이야 어지간한건 쉽죠... 요즘 다들 시국도 안 좋은데 1인손님이라도 가주면 좋을테고요",
          "speaker_id": "1",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008069.1.19",
          "form": "오 그건 그렇긴 하네요",
          "original_form": "오 그건 그렇긴 하네요",
          "speaker_id": "2",
          "time": "20210822 14:07"
        },
        {
          "id": "MDRW2100008069.1.20",
          "form": "시국이 시국이니까ㅠㅠ",
          "original_form": "시국이 시국이니까ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 14:08"
        },
        {
          "id": "MDRW2100008069.1.21",
          "form": "글고 혼자먹는거 은근 편해요",
          "original_form": "글고 혼자먹는거 은근 편해요",
          "speaker_id": "1",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100008069.1.22",
          "form": "맛집탐방 좋더라고요 2인이상 메뉴는 아쉽지만",
          "original_form": "맛집탐방 좋더라고요 2인이상 메뉴는 아쉽지만",
          "speaker_id": "1",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100008069.1.23",
          "form": "오옹 좋아요 맛집~",
          "original_form": "오옹 좋아요 맛집~",
          "speaker_id": "2",
