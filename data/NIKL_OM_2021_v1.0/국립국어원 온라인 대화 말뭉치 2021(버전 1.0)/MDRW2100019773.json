      "utterance": [
        {
          "id": "MDRW2100019773.1.1",
          "form": "디저트는 좋아하세요?",
          "original_form": "디저트는 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100019773.1.2",
          "form": "네~ 디저트 좋아해요 싫어하는 사람 별로 없을거라고 생각해요",
          "original_form": "네~ 디저트 좋아해요 싫어하는 사람 별로 없을거라고 생각해요",
          "speaker_id": "1",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100019773.1.3",
          "form": "달지않은 디저트도 있으니까요 ㅎㅇ",
          "original_form": "달지않은 디저트도 있으니까요 ㅎㅇ",
          "speaker_id": "1",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100019773.1.4",
          "form": "케이크 좋아합니다",
          "original_form": "케이크 좋아합니다",
          "speaker_id": "1",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019773.1.5",
          "form": "무슨 케이크 제일 좋아하세요?",
          "original_form": "무슨 케이크 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100019773.1.6",
          "form": "쉬폰이나 치즈케익이요",
          "original_form": "쉬폰이나 치즈케익이요",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019773.1.7",
          "form": "커피나 홍차랑 잘 어울리는 디저트를 좋아해요 ㅎㅎ",
          "original_form": "커피나 홍차랑 잘 어울리는 디저트를 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100019773.1.8",
          "form": "어떤 디저트를 좋아하시나요?",
          "original_form": "어떤 디저트를 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100019773.1.9",
          "form": "저도 치즈케이크 좋아하구요",
          "original_form": "저도 치즈케이크 좋아하구요",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100019773.1.10",
          "form": "스콘도 좋아해요",
          "original_form": "스콘도 좋아해요",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100019773.1.11",
          "form": "타르트도 엄청 좋아해요",
          "original_form": "타르트도 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100019773.1.12",
          "form": "스콘이랑 타르트도 맛있어요 타르트는 과일타르트도 맛있고",
          "original_form": "스콘이랑 타르트도 맛있어요 타르트는 과일타르트도 맛있고",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100019773.1.13",
          "form": "스콘은 부드러온걸 좋아하시나요? 아님 좀 뻑뻑한걸 좋아하시나요?",
          "original_form": "스콘은 부드러온걸 좋아하시나요? 아님 좀 뻑뻑한걸 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100019773.1.14",
          "form": "맞아요 과일타르트 맛잇어요",
          "original_form": "맞아요 과일타르트 맛잇어요",
          "speaker_id": "2",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019773.1.15",
          "form": "겉은 조금 뻑뻑한듯한데 안은 촉촉한 걸 좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "겉은 조금 뻑뻑한듯한데 안은 촉촉한 걸 좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019773.1.16",
          "form": "그렇지만 가리지않고 다 잘먹어요",
          "original_form": "그렇지만 가리지않고 다 잘먹어요",
          "speaker_id": "2",
          "time": "20210827 22:15"
        },
        {
          "id": "MDRW2100019773.1.17",
          "form": "까망베르치즈케이크도 진짜 좋아해요",
          "original_form": "까망베르치즈케이크도 진짜 좋아해요",
          "speaker_id": "2",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100019773.1.18",
          "form": "스콘은 겉은 뻑뻑하고 속은 촉촉한거 저두 좋아해요 색도 먹음직스럽구요 ㅎㅎ",
          "original_form": "스콘은 겉은 뻑뻑하고 속은 촉촉한거 저두 좋아해요 색도 먹음직스럽구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100019773.1.19",
          "form": "그렇죠 사실 디저트는 가릴게 없지요 ㅋㅋㅋㅋㅋ",
          "original_form": "그렇죠 사실 디저트는 가릴게 없지요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:20"
        },
        {
          "id": "MDRW2100019773.1.20",
          "form": "그리고 홍차랑 너무 잘 어울려서 좋아요 ㅎㅎ",
          "original_form": "그리고 홍차랑 너무 잘 어울려서 좋아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100019773.1.21",
          "form": "밀크티랑 먹으면 행복...",
          "original_form": "밀크티랑 먹으면 행복...",
          "speaker_id": "2",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100019773.1.22",
          "form": "아 전 사실 가리는 디저트 잇어요",
          "original_form": "아 전 사실 가리는 디저트 잇어요",
          "speaker_id": "2",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100019773.1.23",
          "form": "어? 어떤거요?",
          "original_form": "어? 어떤거요?",
          "speaker_id": "1",
          "time": "20210827 22:23"
        },
        {
          "id": "MDRW2100019773.1.24",
          "form": "전 마카롱을 그렇게 좋아하지는 않아요 ㅎㅎ",
          "original_form": "전 마카롱을 그렇게 좋아하지는 않아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:23"
        },
        {
          "id": "MDRW2100019773.1.25",
          "form": "홍차의 맛을 아시는 분이라니 ..",
          "original_form": "홍차의 맛을 아시는 분이라니 ..",
          "speaker_id": "1",
          "time": "20210827 22:23"
        },
        {
          "id": "MDRW2100019773.1.26",
          "form": "아 너무 달아서요?",
          "original_form": "아 너무 달아서요?",
          "speaker_id": "1",
          "time": "20210827 22:23"
        },
        {
          "id": "MDRW2100019773.1.27",
          "form": "네 제입엔 너무 달아서요 ㅎㅎ",
          "original_form": "네 제입엔 너무 달아서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100019773.1.28",
          "form": "name1님도 홍차좋아하세요?",
          "original_form": "&name1&님도 홍차좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 22:25"
        },
        {
          "id": "MDRW2100019773.1.29",
          "form": "네네 홍차 좋아해요",
          "original_form": "네네 홍차 좋아해요",
          "speaker_id": "1",
          "time": "20210827 22:25"
        },
        {
