      "utterance": [
        {
          "id": "MDRW2100025065.1.1",
          "form": "빨간머리앤 소설 읽으셨나요?!",
          "original_form": "빨간머리앤 소설 읽으셨나요?!",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025065.1.2",
          "form": "아 저 소설은 못읽어봤어요ㅠ",
          "original_form": "아 저 소설은 못읽어봤어요ㅠ",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025065.1.3",
          "form": "예전에 나온 애니를 좋아했어서",
          "original_form": "예전에 나온 애니를 좋아했어서",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025065.1.4",
          "form": "아 저도 좋아해요 그거!",
          "original_form": "아 저도 좋아해요 그거!",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025065.1.5",
          "form": "소설을 읽으실 계획 없나요?",
          "original_form": "소설을 읽으실 계획 없나요?",
          "speaker_id": "1",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025065.1.6",
          "form": "소설은 나중에 봐보려구요",
          "original_form": "소설은 나중에 봐보려구요",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025065.1.7",
          "form": "좋아하는 책 있으세요?",
          "original_form": "좋아하는 책 있으세요?",
          "speaker_id": "2",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100025065.1.8",
          "form": "빨간머리앤 좋아해요! ㅋㅋㅋ",
          "original_form": "빨간머리앤 좋아해요! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100025065.1.9",
          "form": "그거 시리즈 되게 많잖아요! 앤이랑 길버트랑 나중 얘기 까지 다나와요",
          "original_form": "그거 시리즈 되게 많잖아요! 앤이랑 길버트랑 나중 얘기 까지 다나와요",
          "speaker_id": "1",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100025065.1.10",
          "form": "오 정말요? 둘이 결혼한다는거 까진 들은거같아요",
          "original_form": "오 정말요? 둘이 결혼한다는거 까진 들은거같아요",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100025065.1.11",
          "form": "아니던가!?",
          "original_form": "아니던가!?",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100025065.1.12",
          "form": "맞아요!맞아요! 둘이연애하고 결혼하고 육아까지합니다!",
          "original_form": "맞아요!맞아요! 둘이연애하고 결혼하고 육아까지합니다!",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025065.1.13",
          "form": "대애박",
          "original_form": "대애박",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100025065.1.14",
          "form": "육아까지요?!",
          "original_form": "육아까지요?!",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025065.1.15",
          "form": "네네 애들낳아요 ㅋㅋ다산할걸요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네네 애들낳아요 ㅋㅋ다산할걸요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025065.1.16",
          "form": "와 안볼수가없네요",
          "original_form": "와 안볼수가없네요",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025065.1.17",
          "form": "담에 꼭 볼게요",
          "original_form": "담에 꼭 볼게요",
          "speaker_id": "2",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025065.1.18",
          "form": "근데 뒤로갈수록 좀 재미없어지고 앤 주변인물들 얘기가 더 비중이 커져요 ㅜㅜ 연애까지는 읽는거 추천드려요! ㅋㅋ",
          "original_form": "근데 뒤로갈수록 좀 재미없어지고 앤 주변인물들 얘기가 더 비중이 커져요 ㅜㅜ 연애까지는 읽는거 추천드려요! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100025065.1.19",
          "form": "아 그렇구나 ㅜ 알겠어용 연애까지 읽어볼게용",
          "original_form": "아 그렇구나 ㅜ 알겠어용 연애까지 읽어볼게용",
          "speaker_id": "2",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025065.1.20",
          "form": "소설 좋아하세요?",
          "original_form": "소설 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 23:04"
        },
        {
          "id": "MDRW2100025065.1.21",
          "form": "네네 좋아해요 ㅋㅋ근데 잘 안읽어요... 어릴때는 많이 봤는데 마치 아이들이 글을 안읽고 유투브에 빠진것마냥 영상이아니면 때려치고있네요...",
          "original_form": "네네 좋아해요 ㅋㅋ근데 잘 안읽어요... 어릴때는 많이 봤는데 마치 아이들이 글을 안읽고 유투브에 빠진것마냥 영상이아니면 때려치고있네요...",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100025065.1.22",
          "form": "ㄷㄷ 잠들어 버렸네요ㅜ",
          "original_form": "ㄷㄷ 잠들어 버렸네요ㅜ",
          "speaker_id": "2",
          "time": "20210901 09:21"
        },
        {
          "id": "MDRW2100025065.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ 저도 영상아니면 때려쳐서",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 저도 영상아니면 때려쳐서",
          "speaker_id": "2",
          "time": "20210901 09:21"
        },
        {
          "id": "MDRW2100025065.1.24",
          "form": "책...안 읽고있어요 ㅎ",
          "original_form": "책...안 읽고있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210901 09:21"
        },
        {
          "id": "MDRW2100025065.1.25",
          "form": "솔직히 새로 사둔 책들은 있는데 여전히 새책들이네요",
          "original_form": "솔직히 새로 사둔 책들은 있는데 여전히 새책들이네요",
          "speaker_id": "2",
          "time": "20210901 09:22"
        }
      ]
    }
