      "utterance": [
        {
          "id": "MDRW2100032617.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100032617.1.2",
          "form": "음 그럭저럭이요 딱히 선호하진 않아요",
          "original_form": "음 그럭저럭이요 딱히 선호하진 않아요",
          "speaker_id": "2",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100032617.1.3",
          "form": "아하 그러시군요",
          "original_form": "아하 그러시군요",
          "speaker_id": "1",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100032617.1.4",
          "form": "기억에남는데있으세요?",
          "original_form": "기억에남는데있으세요?",
          "speaker_id": "1",
          "time": "20210905 21:25"
        },
        {
          "id": "MDRW2100032617.1.5",
          "form": "가평이 기억이 좀 나요!",
          "original_form": "가평이 기억이 좀 나요!",
          "speaker_id": "2",
          "time": "20210905 21:26"
        },
        {
          "id": "MDRW2100032617.1.6",
          "form": "수목원?이 예뻤어요",
          "original_form": "수목원?이 예뻤어요",
          "speaker_id": "2",
          "time": "20210905 21:26"
        },
        {
          "id": "MDRW2100032617.1.7",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210905 21:28"
        },
        {
          "id": "MDRW2100032617.1.8",
          "form": "제이드가든?맞나요?",
          "original_form": "제이드가든?맞나요?",
          "speaker_id": "1",
          "time": "20210905 21:28"
        },
        {
          "id": "MDRW2100032617.1.9",
          "form": "앗 아닌것 같아요 어디더랑,,",
          "original_form": "앗 아닌것 같아요 어디더랑,,",
          "speaker_id": "2",
          "time": "20210905 21:30"
        },
        {
          "id": "MDRW2100032617.1.10",
          "form": "아침고요수목원이요!",
          "original_form": "아침고요수목원이요!",
          "speaker_id": "2",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100032617.1.11",
          "form": "아하 그렇군요",
          "original_form": "아하 그렇군요",
          "speaker_id": "1",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100032617.1.12",
          "form": "수상레포츠도하셨나요?",
          "original_form": "수상레포츠도하셨나요?",
          "speaker_id": "1",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100032617.1.13",
          "form": "겨울에 갔어욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "겨울에 갔어욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:32"
        },
        {
          "id": "MDRW2100032617.1.14",
          "form": "앗 헤헤..",
          "original_form": "앗 헤헤..",
          "speaker_id": "1",
          "time": "20210905 21:33"
        },
        {
          "id": "MDRW2100032617.1.15",
          "form": "또가고싶은데있으세요?",
          "original_form": "또가고싶은데있으세요?",
          "speaker_id": "1",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100032617.1.16",
          "form": "호랑이 보고싶긴해요",
          "original_form": "호랑이 보고싶긴해요",
          "speaker_id": "2",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100032617.1.17",
          "form": "가고 싶은데 있으세요?",
          "original_form": "가고 싶은데 있으세요?",
          "speaker_id": "2",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100032617.1.18",
          "form": "음 코로나만 아니면 터키 이스탄불 가고싶어요",
          "original_form": "음 코로나만 아니면 터키 이스탄불 가고싶어요",
          "speaker_id": "1",
          "time": "20210905 21:50"
        },
        {
          "id": "MDRW2100032617.1.19",
          "form": "오오 터키",
          "original_form": "오오 터키",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100032617.1.20",
          "form": "터키는 영어쓰나여",
          "original_form": "터키는 영어쓰나여",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100032617.1.21",
          "form": "앗 영어..쓰는지는모르겠지만",
          "original_form": "앗 영어..쓰는지는모르겠지만",
          "speaker_id": "1",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100032617.1.22",
          "form": "기댈곳은영어뿐이죠",
          "original_form": "기댈곳은영어뿐이죠",
          "speaker_id": "1",
          "time": "20210905 21:59"
        }
      ]
