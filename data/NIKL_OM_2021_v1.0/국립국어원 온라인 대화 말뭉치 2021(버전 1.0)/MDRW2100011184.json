        },
        {
          "id": "MDRW2100011184.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210824 02:38"
        },
        {
          "id": "MDRW2100011184.1.3",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210824 02:39"
        },
        {
          "id": "MDRW2100011184.1.4",
          "form": "아니요 name1님은요?",
          "original_form": "아니요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 02:40"
        },
        {
          "id": "MDRW2100011184.1.5",
          "form": "저도 안키우는데 한번 키워보고 싶긴하네요",
          "original_form": "저도 안키우는데 한번 키워보고 싶긴하네요",
          "speaker_id": "1",
          "time": "20210824 02:41"
        },
        {
          "id": "MDRW2100011184.1.6",
          "form": "저도요. 그치만 끝까지 책임지고 잘해줄 자신이 없어 주저하게되네요",
          "original_form": "저도요. 그치만 끝까지 책임지고 잘해줄 자신이 없어 주저하게되네요",
          "speaker_id": "2",
          "time": "20210824 02:42"
        },
        {
          "id": "MDRW2100011184.1.7",
          "form": "아맞아요.. 근데 고양이는 한번 키워보고 싶어요",
          "original_form": "아맞아요.. 근데 고양이는 한번 키워보고 싶어요",
          "speaker_id": "1",
          "time": "20210824 02:43"
        },
        {
          "id": "MDRW2100011184.1.8",
          "form": "맞아요 고양이 매력넘치죠",
          "original_form": "맞아요 고양이 매력넘치죠",
          "speaker_id": "2",
          "time": "20210824 02:44"
        },
        {
          "id": "MDRW2100011184.1.9",
          "form": "눈이 너무 귀여워요ㅠㅠ",
          "original_form": "눈이 너무 귀여워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 02:44"
        },
        {
          "id": "MDRW2100011184.1.10",
          "form": "맞아요 발바닥 젤리도",
          "original_form": "맞아요 발바닥 젤리도",
          "speaker_id": "2",
          "time": "20210824 02:45"
        },
        {
          "id": "MDRW2100011184.1.11",
          "form": "키워보고싶은 반려동물 있으세요?",
          "original_form": "키워보고싶은 반려동물 있으세요?",
          "speaker_id": "1",
          "time": "20210824 02:48"
        },
        {
          "id": "MDRW2100011184.1.12",
          "form": "저는 강아지요",
          "original_form": "저는 강아지요",
          "speaker_id": "2",
          "time": "20210824 02:48"
        },
        {
          "id": "MDRW2100011184.1.13",
          "form": "골든리트리버같은 대형견",
          "original_form": "골든리트리버같은 대형견",
          "speaker_id": "2",
          "time": "20210824 02:49"
        },
        {
          "id": "MDRW2100011184.1.14",
          "form": "헉 대형견은 조금 무섭지 않을까요",
          "original_form": "헉 대형견은 조금 무섭지 않을까요",
          "speaker_id": "1",
          "time": "20210824 02:52"
        },
        {
          "id": "MDRW2100011184.1.15",
          "form": "리트리버들은 워낙 순둥해서",
          "original_form": "리트리버들은 워낙 순둥해서",
          "speaker_id": "2",
          "time": "20210824 02:53"
        },
        {
          "id": "MDRW2100011184.1.16",
          "form": "다른 대형견에 비하면 괜찮을거같아요",
          "original_form": "다른 대형견에 비하면 괜찮을거같아요",
          "speaker_id": "2",
          "time": "20210824 02:54"
        },
        {
          "id": "MDRW2100011184.1.17",
          "form": "물론 어디나 예외가 있긴하지만...",
          "original_form": "물론 어디나 예외가 있긴하지만...",
