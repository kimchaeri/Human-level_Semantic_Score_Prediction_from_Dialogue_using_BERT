      "utterance": [
        {
          "id": "MDRW2100022384.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210829 13:19"
        },
        {
          "id": "MDRW2100022384.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210830 14:04"
        },
        {
          "id": "MDRW2100022384.1.3",
          "form": "네 안녕하세요 식사는 하셨나요?",
          "original_form": "네 안녕하세요 식사는 하셨나요?",
          "speaker_id": "1",
          "time": "20210830 15:17"
        },
        {
          "id": "MDRW2100022384.1.4",
          "form": "네~ 김치찌개 먹었어요 ㅎㅎ",
          "original_form": "네~ 김치찌개 먹었어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 15:21"
        },
        {
          "id": "MDRW2100022384.1.5",
          "form": "와 맛있겠어요 참치 김찌인가요 아님 돼지 김찌인가요",
          "original_form": "와 맛있겠어요 참치 김찌인가요 아님 돼지 김찌인가요",
          "speaker_id": "1",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100022384.1.6",
          "form": "묵은지인가요 생김친가요 세부옵션이 궁금합니다",
          "original_form": "묵은지인가요 생김친가요 세부옵션이 궁금합니다",
          "speaker_id": "1",
          "time": "20210830 15:38"
        },
        {
          "id": "MDRW2100022384.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ 돼지김찌에요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 돼지김찌에요!",
          "speaker_id": "2",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100022384.1.8",
          "form": "묵은지로 끓였어요",
          "original_form": "묵은지로 끓였어요",
          "speaker_id": "2",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100022384.1.9",
          "form": "새콤해서 맛나요",
          "original_form": "새콤해서 맛나요",
          "speaker_id": "2",
          "time": "20210831 15:42"
        },
        {
          "id": "MDRW2100022384.1.10",
          "form": "제가 점심때 얘기한 기억이 없어가지고 언제얘기했지 했는데",
          "original_form": "제가 점심때 얘기한 기억이 없어가지고 언제얘기했지 했는데",
          "speaker_id": "1",
          "time": "20210831 20:50"
        },
        {
          "id": "MDRW2100022384.1.11",
          "form": "아 전날 점심이었군요",
          "original_form": "아 전날 점심이었군요",
          "speaker_id": "1",
          "time": "20210831 20:50"
        },
        {
          "id": "MDRW2100022384.1.12",
          "form": "ㅋㅋㅋ 저도 돼지김치지개를 조 ㅎ아해요 사먹는건 새마을식당, 굴다리게 좋더라고요",
          "original_form": "ㅋㅋㅋ 저도 돼지김치지개를 조 ㅎ아해요 사먹는건 새마을식당, 굴다리게 좋더라고요",
          "speaker_id": "1",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100022384.1.13",
          "form": "새마을식당을 가보셨나요?",
          "original_form": "새마을식당을 가보셨나요?",
          "speaker_id": "1",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100022384.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ제가 답장이 너무 느리죠 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ제가 답장이 너무 느리죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 23:43"
        },
        {
          "id": "MDRW2100022384.1.15",
          "form": "새마을식당 안가봤어요! 거기 백종원이 새로 낸 프랜차이즈 맞죠?",
          "original_form": "새마을식당 안가봤어요! 거기 백종원이 새로 낸 프랜차이즈 맞죠?",
          "speaker_id": "2",
          "time": "20210831 23:43"
        },
        {
          "id": "MDRW2100022384.1.16",
          "form": "전 왠지 집에서 김치찌개 자주 먹으니 밖에서는 잘 안먹게 되더라고요 ㅋㅋㅋ",
          "original_form": "전 왠지 집에서 김치찌개 자주 먹으니 밖에서는 잘 안먹게 되더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:44"
        },
        {
          "id": "MDRW2100022384.1.17",
          "form": "맛있다고 하시니까 궁금하네요! 다음에 한 번 먹어봐야겟어요",
          "original_form": "맛있다고 하시니까 궁금하네요! 다음에 한 번 먹어봐야겟어요",
          "speaker_id": "2",
          "time": "20210831 23:44"
        },
        {
          "id": "MDRW2100022384.1.18",
          "form": "집에서 맛있게 드시면 좀 가서 실망하실텐데",
          "original_form": "집에서 맛있게 드시면 좀 가서 실망하실텐데",
          "speaker_id": "1",
          "time": "20210831 23:46"
        },
        {
          "id": "MDRW2100022384.1.19",
          "form": "밖에서 먹으면 또 색다른 맛이 나지 않나요?ㅋㅋ",
          "original_form": "밖에서 먹으면 또 색다른 맛이 나지 않나요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:46"
        },
        {
          "id": "MDRW2100022384.1.20",
          "form": "저렴한 가격에",
          "original_form": "저렴한 가격에",
          "speaker_id": "1",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100022384.1.21",
          "form": "저렴한 가격에",
          "original_form": "저렴한 가격에",
          "speaker_id": "1",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100022384.1.22",
          "form": "매콤새콤하게",
          "original_form": "매콤새콤하게",
          "speaker_id": "1",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100022384.1.23",
          "form": "소주랑",
          "original_form": "소주랑",
          "speaker_id": "1",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100022384.1.24",
          "form": "먹기",
          "original_form": "먹기",
          "speaker_id": "1",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100022384.1.25",
          "form": "좋은",
          "original_form": "좋은",
          "speaker_id": "1",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100022384.1.26",
          "form": "가격이었어요",
          "original_form": "가격이었어요",
          "speaker_id": "1",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100022384.1.27",
          "form": "아 소주안주로 김치찌개 최고죠",
          "original_form": "아 소주안주로 김치찌개 최고죠",
          "speaker_id": "2",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100022384.1.28",
          "form": "이렇게 단어별로 분절해서 보내야하는 ㅠㅠ",
          "original_form": "이렇게 단어별로 분절해서 보내야하는 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 23:47"
        },
        {
          "id": "MDRW2100022384.1.29",
          "form": "국물만 먹어도 맛나구 ㅠㅠ",
          "original_form": "국물만 먹어도 맛나구 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100022384.1.30",
          "form": "은근 생각지도 못한 필터링 단어가 많은 것 같아요 ㅋㅋㅋ",
          "original_form": "은근 생각지도 못한 필터링 단어가 많은 것 같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100022384.1.31",
          "form": "안주로 먹기에는 부대찌개도 좋은 것 같아요",
          "original_form": "안주로 먹기에는 부대찌개도 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210831 23:48"
        },
        {
          "id": "MDRW2100022384.1.32",
          "form": "먹다가 라면사리 넣어서 면으로도 먹고...",
          "original_form": "먹다가 라면사리 넣어서 면으로도 먹고...",
