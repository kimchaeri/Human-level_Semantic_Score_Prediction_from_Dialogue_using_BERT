      "utterance": [
        {
          "id": "MDRW2100047618.1.1",
          "form": "여행! 가고싶은 곳 있으새요?",
          "original_form": "여행! 가고싶은 곳 있으새요?",
          "speaker_id": "2",
          "time": "20210827 11:54"
        },
        {
          "id": "MDRW2100047618.1.2",
          "form": "지금은 동남아 쪽 가고싶어요!",
          "original_form": "지금은 동남아 쪽 가고싶어요!",
          "speaker_id": "1",
          "time": "20210827 11:55"
        },
        {
          "id": "MDRW2100047618.1.3",
          "form": "가고싶은곳 있으세요?",
          "original_form": "가고싶은곳 있으세요?",
          "speaker_id": "1",
          "time": "20210827 11:55"
        },
        {
          "id": "MDRW2100047618.1.4",
          "form": "저는 바다면 어디든 좋을것같아요",
          "original_form": "저는 바다면 어디든 좋을것같아요",
          "speaker_id": "2",
          "time": "20210827 11:59"
        },
        {
          "id": "MDRW2100047618.1.5",
          "form": "여행가서 뭐하고싶으새요?",
          "original_form": "여행가서 뭐하고싶으새요?",
          "speaker_id": "2",
          "time": "20210827 11:59"
        },
        {
          "id": "MDRW2100047618.1.6",
          "form": "그지역에서 맛있는거 먹구요",
          "original_form": "그지역에서 맛있는거 먹구요",
          "speaker_id": "1",
          "time": "20210827 12:00"
        },
        {
          "id": "MDRW2100047618.1.7",
          "form": "맛있는거 먹구요",
          "original_form": "맛있는거 먹구요",
          "speaker_id": "1",
          "time": "20210827 12:01"
        },
        {
          "id": "MDRW2100047618.1.8",
          "form": "맛있는게 최고죠!",
          "original_form": "맛있는게 최고죠!",
          "speaker_id": "2",
          "time": "20210827 12:02"
        },
        {
          "id": "MDRW2100047618.1.9",
          "form": "전에 가보신적 있으새요?",
          "original_form": "전에 가보신적 있으새요?",
          "speaker_id": "2",
          "time": "20210827 12:02"
        },
        {
          "id": "MDRW2100047618.1.10",
          "form": "그쪽은 대만정도만 가봤어요",
          "original_form": "그쪽은 대만정도만 가봤어요",
          "speaker_id": "1",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100047618.1.11",
          "form": "우와 어때요? 저는 못가봤거든요",
          "original_form": "우와 어때요? 저는 못가봤거든요",
          "speaker_id": "2",
          "time": "20210827 12:04"
        },
        {
          "id": "MDRW2100047618.1.12",
          "form": "대만 저는 딤섬이 너무 맛있었구요",
          "original_form": "대만 저는 딤섬이 너무 맛있었구요",
          "speaker_id": "1",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100047618.1.13",
          "form": "야시장도 재밌었고 마사지 저렴하게 받는게 너무 좋았어요",
          "original_form": "야시장도 재밌었고 마사지 저렴하게 받는게 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210827 12:08"
        },
        {
          "id": "MDRW2100047618.1.14",
          "form": "오 진짜 좋겠내요",
          "original_form": "오 진짜 좋겠내요",
          "speaker_id": "2",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100047618.1.15",
          "form": "마사지 한번에 완전 비싸잖아요 ㅠ",
          "original_form": "마사지 한번에 완전 비싸잖아요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100047618.1.16",
          "form": "그죠ㅠㅠ 그래서 놀러가면 날마다 받아줘야해요",
          "original_form": "그죠ㅠㅠ 그래서 놀러가면 날마다 받아줘야해요",
          "speaker_id": "1",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100047618.1.17",
          "form": "빨리 코로나.. 없어져라..",
          "original_form": "빨리 코로나.. 없어져라..",
          "speaker_id": "2",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100047618.1.18",
          "form": "빨리 코로나 ㅠㅠ",
          "original_form": "빨리 코로나 ㅠㅠ",
