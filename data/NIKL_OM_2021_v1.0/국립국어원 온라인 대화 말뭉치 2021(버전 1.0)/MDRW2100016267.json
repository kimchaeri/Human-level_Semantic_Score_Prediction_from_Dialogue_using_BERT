      "utterance": [
        {
          "id": "MDRW2100016267.1.1",
          "form": "동물 키우고싶은거 있음?",
          "original_form": "동물 키우고싶은거 있음?",
          "speaker_id": "2",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100016267.1.2",
          "form": "강아지 키우고 싶음",
          "original_form": "강아지 키우고 싶음",
          "speaker_id": "1",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100016267.1.3",
          "form": "강아지 품종 뭐?",
          "original_form": "강아지 품종 뭐?",
          "speaker_id": "2",
          "time": "20210826 17:17"
        },
        {
          "id": "MDRW2100016267.1.4",
          "form": "품종은 상관없구",
          "original_form": "품종은 상관없구",
          "speaker_id": "1",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100016267.1.5",
          "form": "리트리버가 귀엽긴 하던데",
          "original_form": "리트리버가 귀엽긴 하던데",
          "speaker_id": "1",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100016267.1.6",
          "form": "큰 개 귀여워...",
          "original_form": "큰 개 귀여워...",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100016267.1.7",
          "form": "맞아 기여워",
          "original_form": "맞아 기여워",
          "speaker_id": "1",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100016267.1.8",
          "form": "진돗개도 좋은데 키울수있나 모르겠네",
          "original_form": "진돗개도 좋은데 키울수있나 모르겠네",
          "speaker_id": "1",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100016267.1.9",
          "form": "키울수있지",
          "original_form": "키울수있지",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100016267.1.10",
          "form": "우리근처 아파트에서도 키우던데",
          "original_form": "우리근처 아파트에서도 키우던데",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100016267.1.11",
          "form": "그래?ㅋㅋㅋㅋ",
          "original_form": "그래?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100016267.1.12",
          "form": "대신 아침저녁으로 산책하더라",
          "original_form": "대신 아침저녁으로 산책하더라",
          "speaker_id": "2",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100016267.1.13",
          "form": "토종견에 로망이 있었는데",
          "original_form": "토종견에 로망이 있었는데",
          "speaker_id": "1",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100016267.1.14",
          "form": "삽살개라던가",
          "original_form": "삽살개라던가",
          "speaker_id": "1",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100016267.1.15",
          "form": "단모 삽사리 이쁘더라",
          "original_form": "단모 삽사리 이쁘더라",
          "speaker_id": "2",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100016267.1.16",
          "form": "기관에서 분양해주는데 까다롭게 분양하더라",
          "original_form": "기관에서 분양해주는데 까다롭게 분양하더라",
