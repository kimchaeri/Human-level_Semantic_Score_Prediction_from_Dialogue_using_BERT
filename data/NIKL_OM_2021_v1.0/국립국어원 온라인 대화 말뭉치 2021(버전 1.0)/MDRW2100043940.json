      "utterance": [
        {
          "id": "MDRW2100043940.1.1",
          "form": "맛있니",
          "original_form": "맛있니",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043940.1.2",
          "form": "옹옹 ㅋㅋ 맛이 좋군",
          "original_form": "옹옹 ㅋㅋ 맛이 좋군",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043940.1.3",
          "form": "홈런볼을 좋아하네",
          "original_form": "홈런볼을 좋아하네",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043940.1.4",
          "form": "웅 ㅋ 질리지 않는 맛",
          "original_form": "웅 ㅋ 질리지 않는 맛",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100043940.1.5",
          "form": "나는 너무 달아서 별로던데",
          "original_form": "나는 너무 달아서 별로던데",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043940.1.6",
          "form": "단 맛으로 먹어야지 과자는 ㅎㅎ",
          "original_form": "단 맛으로 먹어야지 과자는 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043940.1.7",
          "form": "이빨 썩는다",
          "original_form": "이빨 썩는다",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043940.1.8",
          "form": "ㅋㅋ 먹고 바로 양치 해야짓!",
          "original_form": "ㅋㅋ 먹고 바로 양치 해야짓!",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043940.1.9",
          "form": "그래 빨리 먹고 양치 해라",
          "original_form": "그래 빨리 먹고 양치 해라",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043940.1.10",
          "form": "홈런볼 천천히 먹어야지",
          "original_form": "홈런볼 천천히 먹어야지",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043940.1.11",
          "form": "비싼데",
          "original_form": "비싼데",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043940.1.12",
          "form": "그래 비싸다",
          "original_form": "그래 비싸다",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100043940.1.13",
          "form": "웅 ㅜㅋㅋ 어쩌다 한번 먹는거니까.. 칼로리 생각안해야지 ㅋ",
          "original_form": "웅 ㅜㅋㅋ 어쩌다 한번 먹는거니까.. 칼로리 생각안해야지 ㅋ",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043940.1.14",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043940.1.15",
          "form": "엄마는 무슨 과자 좋아해?",
          "original_form": "엄마는 무슨 과자 좋아해?",
