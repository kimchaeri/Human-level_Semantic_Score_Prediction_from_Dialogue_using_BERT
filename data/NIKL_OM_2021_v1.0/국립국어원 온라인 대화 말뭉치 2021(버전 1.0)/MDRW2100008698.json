      "utterance": [
        {
          "id": "MDRW2100008698.1.1",
          "form": "예쁘더라고요 하나 갖고싶긴한데 고가라서",
          "original_form": "예쁘더라고요 하나 갖고싶긴한데 고가라서",
          "speaker_id": "2",
          "time": "20210822 17:38"
        },
        {
          "id": "MDRW2100008698.1.2",
          "form": "고장내면 어쩌나 덜컥 겁도나요",
          "original_form": "고장내면 어쩌나 덜컥 겁도나요",
          "speaker_id": "2",
          "time": "20210822 17:38"
        },
        {
          "id": "MDRW2100008698.1.3",
          "form": "아 맞아요 ㅠㅠ",
          "original_form": "아 맞아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 17:39"
        },
        {
          "id": "MDRW2100008698.1.4",
          "form": "근데 진짜 이뻐서 폰바꿀때 함 살까.. 고민중이에요",
          "original_form": "근데 진짜 이뻐서 폰바꿀때 함 살까.. 고민중이에요",
          "speaker_id": "1",
          "time": "20210822 17:39"
        },
        {
          "id": "MDRW2100008698.1.5",
          "form": "체지방량 잴 수 있는 것도 너무 혹하고..",
          "original_form": "체지방량 잴 수 있는 것도 너무 혹하고..",
          "speaker_id": "1",
          "time": "20210822 17:40"
        },
        {
          "id": "MDRW2100008698.1.6",
          "form": "저는 갑자기 이름이 기억 안나는데",
          "original_form": "저는 갑자기 이름이 기억 안나는데",
          "speaker_id": "2",
          "time": "20210822 17:46"
        },
        {
          "id": "MDRW2100008698.1.7",
          "form": "무선이어폰",
          "original_form": "무선이어폰",
          "speaker_id": "2",
          "time": "20210822 17:46"
        },
        {
          "id": "MDRW2100008698.1.8",
          "form": "갤럭시꺼도 사고싶더라고요",
          "original_form": "갤럭시꺼도 사고싶더라고요",
          "speaker_id": "2",
          "time": "20210822 17:46"
        },
        {
          "id": "MDRW2100008698.1.9",
          "form": "콩나물말고 강낭콩",
          "original_form": "콩나물말고 강낭콩",
          "speaker_id": "2",
          "time": "20210822 17:46"
        },
        {
          "id": "MDRW2100008698.1.10",
          "form": "버즈요?",
          "original_form": "버즈요?",
          "speaker_id": "1",
          "time": "20210822 18:03"
        },
        {
          "id": "MDRW2100008698.1.11",
          "form": "저는 버즈 플러스쓰는데",
          "original_form": "저는 버즈 플러스쓰는데",
          "speaker_id": "1",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008698.1.12",
          "form": "좋더라고요",
          "original_form": "좋더라고요",
          "speaker_id": "1",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008698.1.13",
          "form": "근데 버즈 프로는 가끔 귀에염증 생긴다는 말있어서 그건 피하는개 좋겠더라고요",
          "original_form": "근데 버즈 프로는 가끔 귀에염증 생긴다는 말있어서 그건 피하는개 좋겠더라고요",
          "speaker_id": "1",
          "time": "20210822 18:04"
        },
        {
          "id": "MDRW2100008698.1.14",
          "form": "아 정말요? 전 싼거 삼만원대 무선이어폰  쓰는데",
          "original_form": "아 정말요? 전 싼거 삼만원대 무선이어폰  쓰는데",
          "speaker_id": "2",
          "time": "20210822 18:12"
        },
        {
          "id": "MDRW2100008698.1.15",
          "form": "비싼거 써보고싶더라고요",
          "original_form": "비싼거 써보고싶더라고요",
          "speaker_id": "2",
          "time": "20210822 18:12"
        },
        {
          "id": "MDRW2100008698.1.16",
          "form": "뭔가 귀가 덜아프다해서요",
          "original_form": "뭔가 귀가 덜아프다해서요",
          "speaker_id": "2",
          "time": "20210822 18:14"
        },
        {
          "id": "MDRW2100008698.1.17",
          "form": "확실히 좋긴해요",
          "original_form": "확실히 좋긴해요",
          "speaker_id": "1",
          "time": "20210822 18:40"
        },
        {
          "id": "MDRW2100008698.1.18",
          "form": "콩나물이 잘맞는사람도 있고 강낭콩이 잘맞는 사람도있고",
          "original_form": "콩나물이 잘맞는사람도 있고 강낭콩이 잘맞는 사람도있고",
          "speaker_id": "1",
          "time": "20210822 18:40"
        },
        {
          "id": "MDRW2100008698.1.19",
          "form": "저는 콩나물 너무 귀가 아팠어요 ㅠㅠ",
          "original_form": "저는 콩나물 너무 귀가 아팠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 18:41"
        },
        {
          "id": "MDRW2100008698.1.20",
          "form": "근데 잃어버릴거 겁도 나요",
          "original_form": "근데 잃어버릴거 겁도 나요",
          "speaker_id": "2",
          "time": "20210822 18:44"
        },
        {
          "id": "MDRW2100008698.1.21",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:44"
        },
        {
          "id": "MDRW2100008698.1.22",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:45"
        },
        {
          "id": "MDRW2100008698.1.23",
          "form": "맞이들 잃어버리죠",
          "original_form": "맞이들 잃어버리죠",
          "speaker_id": "1",
          "time": "20210822 18:45"
        },
        {
          "id": "MDRW2100008698.1.24",
          "form": "특히 한쪽만 잃어버린다면 재앙",
          "original_form": "특히 한쪽만 잃어버린다면 재앙",
          "speaker_id": "2",
          "time": "20210822 18:45"
        },
        {
          "id": "MDRW2100008698.1.25",
          "form": "저도 간당간당히 찾아서 쓰고있어요",
          "original_form": "저도 간당간당히 찾아서 쓰고있어요",
          "speaker_id": "1",
          "time": "20210822 18:45"
        },
        {
          "id": "MDRW2100008698.1.26",
          "form": "가끔 귀에서 캡만 남기고 사라지고",
          "original_form": "가끔 귀에서 캡만 남기고 사라지고",
          "speaker_id": "1",
          "time": "20210822 18:45"
        },
        {
          "id": "MDRW2100008698.1.27",
          "form": "전 엄마 선물로 사드리고 싶기도해요",
          "original_form": "전 엄마 선물로 사드리고 싶기도해요",
          "speaker_id": "2",
          "time": "20210822 18:45"
        },
        {
          "id": "MDRW2100008698.1.28",
          "form": "오 완전 효자시네요",
          "original_form": "오 완전 효자시네요",
          "speaker_id": "1",
          "time": "20210822 18:45"
        },
        {
          "id": "MDRW2100008698.1.29",
          "form": "당근에서는 되게 싸게올라오던데!",
          "original_form": "당근에서는 되게 싸게올라오던데!",
          "speaker_id": "1",
          "time": "20210822 18:46"
        },
        {
          "id": "MDRW2100008698.1.30",
          "form": "신상 나올때마다 싸게 올라오더라고요",
          "original_form": "신상 나올때마다 싸게 올라오더라고요",
          "speaker_id": "1",
          "time": "20210822 18:46"
        },
        {
          "id": "MDRW2100008698.1.31",
          "form": "요새 그립톡사드리기 제로페이 깔아드리기 백신 예약해두니기 뭐 이런거 해드리는게 효도래요",
          "original_form": "요새 그립톡사드리기 제로페이 깔아드리기 백신 예약해두니기 뭐 이런거 해드리는게 효도래요",
          "speaker_id": "2",
          "time": "20210822 18:46"
        },
        {
          "id": "MDRW2100008698.1.32",
          "form": "완전 효도죠",
          "original_form": "완전 효도죠",
          "speaker_id": "1",
          "time": "20210822 18:46"
        },
        {
          "id": "MDRW2100008698.1.33",
          "form": "저는.. 다 알아서 하셔서 제가 해드릴게 없네요 ㅋㅋ ㅠㅠ",
          "original_form": "저는.. 다 알아서 하셔서 제가 해드릴게 없네요 ㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 18:47"
        },
        {
          "id": "MDRW2100008698.1.34",
          "form": "저도 알아서하시는건 하시는데 60대 되시면서 글읽는게 귀찮다고 ㅋㅋ",
          "original_form": "저도 알아서하시는건 하시는데 60대 되시면서 글읽는게 귀찮다고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:53"
        }
      ]
