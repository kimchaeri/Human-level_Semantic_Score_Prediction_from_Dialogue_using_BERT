      "utterance": [
        {
          "id": "MDRW2100043589.1.1",
          "form": "김치찌개니!!!!!",
          "original_form": "김치찌개니!!!!!",
          "speaker_id": "2",
          "time": "20210907 19:37"
        },
        {
          "id": "MDRW2100043589.1.2",
          "form": "어!!!! 맞아 김치찌개 먹었어!",
          "original_form": "어!!!! 맞아 김치찌개 먹었어!",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043589.1.3",
          "form": "끝내주는 저녁식사였지",
          "original_form": "끝내주는 저녁식사였지",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043589.1.4",
          "form": "넌 뭐 먹었니",
          "original_form": "넌 뭐 먹었니",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043589.1.5",
          "form": "크으 나천재야",
          "original_form": "크으 나천재야",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043589.1.6",
          "form": "난 어제 먹었던거",
          "original_form": "난 어제 먹었던거",
          "speaker_id": "2",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043589.1.7",
          "form": "맞춰볼게 기억이 안 나는데",
          "original_form": "맞춰볼게 기억이 안 나는데",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043589.1.8",
          "form": "야채가 들어가나요?",
          "original_form": "야채가 들어가나요?",
          "speaker_id": "1",
          "time": "20210907 19:38"
        },
        {
          "id": "MDRW2100043589.1.9",
          "form": "응 들어가긴해",
          "original_form": "응 들어가긴해",
          "speaker_id": "2",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043589.1.10",
          "form": "으음 중식인가요?",
          "original_form": "으음 중식인가요?",
          "speaker_id": "1",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043589.1.11",
          "form": "아니 한식이야",
          "original_form": "아니 한식이야",
          "speaker_id": "2",
          "time": "20210907 19:39"
        },
        {
          "id": "MDRW2100043589.1.12",
          "form": "양식이야?",
          "original_form": "양식이야?",
          "speaker_id": "1",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043589.1.13",
          "form": "아니 한식이야",
          "original_form": "아니 한식이야",
          "speaker_id": "2",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043589.1.14",
          "form": "일식이니?",
          "original_form": "일식이니?",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043589.1.15",
          "form": "ㅋㅋㅋㅋㄱ한식이야",
          "original_form": "ㅋㅋㅋㅋㄱ한식이야",
          "speaker_id": "2",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043589.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그렇구나!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그렇구나!",
          "speaker_id": "1",
          "time": "20210907 19:43"
        },
        {
          "id": "MDRW2100043589.1.17",
          "form": "주변에서 흔하게 볼 수 있는 요리야?",
          "original_form": "주변에서 흔하게 볼 수 있는 요리야?",
