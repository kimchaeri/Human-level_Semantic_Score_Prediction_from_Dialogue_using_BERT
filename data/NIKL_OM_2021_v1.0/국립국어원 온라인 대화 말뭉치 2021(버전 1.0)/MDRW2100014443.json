      "utterance": [
        {
          "id": "MDRW2100014443.1.1",
          "form": "저녁으로는 미역국을 먹었습니다.",
          "original_form": "저녁으로는 미역국을 먹었습니다.",
          "speaker_id": "1",
          "time": "20210825 19:27"
        },
        {
          "id": "MDRW2100014443.1.2",
          "form": "주메뉴가 미역국이요?ㅎ",
          "original_form": "주메뉴가 미역국이요?ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014443.1.3",
          "form": "전 고갈비랑 된장찌개",
          "original_form": "전 고갈비랑 된장찌개",
          "speaker_id": "2",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014443.1.4",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014443.1.5",
          "form": "고갈비가 갈비인줄 아는 사람이 있더라고요ㅋㅋ",
          "original_form": "고갈비가 갈비인줄 아는 사람이 있더라고요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014443.1.6",
          "form": "이름이 그래서 ㅎ",
          "original_form": "이름이 그래서 ㅎ",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014443.1.7",
          "form": "집밥드신건가요?",
          "original_form": "집밥드신건가요?",
          "speaker_id": "2",
          "time": "20210825 19:50"
        },
        {
          "id": "MDRW2100014443.1.8",
          "form": "근데 닉넴 name1는 무슨뜻이예요?ㅋㅋ",
          "original_form": "근데 닉넴 &name1&는 무슨뜻이예요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014443.1.9",
          "form": "others others할때 그name1인가",
          "original_form": "&others& &others&할때 그&name1&인가",
          "speaker_id": "2",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014443.1.10",
          "form": "ㅋㅋ others에 관심이 많아서요ㅋㅋ",
          "original_form": "ㅋㅋ &others&에 관심이 많아서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:35"
        },
        {
          "id": "MDRW2100014443.1.11",
          "form": "others도 시원하게 끓여 먹고 싶네요ㅋㅋ",
          "original_form": "&others&도 시원하게 끓여 먹고 싶네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 08:35"
        },
        {
          "id": "MDRW2100014443.1.12",
          "form": "아 그others군요ㅎ",
          "original_form": "아 그&others&군요ㅎ",
          "speaker_id": "2",
          "time": "20210826 10:42"
        },
        {
          "id": "MDRW2100014443.1.13",
          "form": "생태탕 맛있죠",
          "original_form": "생태탕 맛있죠",
          "speaker_id": "2",
          "time": "20210826 10:42"
        },
        {
          "id": "MDRW2100014443.1.14",
          "form": "요즘 생태도 있긴하는지 몰겠지만",
          "original_form": "요즘 생태도 있긴하는지 몰겠지만",
          "speaker_id": "2",
          "time": "20210826 10:42"
        },
        {
          "id": "MDRW2100014443.1.15",
          "form": "국내산은 멸종이에요ㅠ",
          "original_form": "국내산은 멸종이에요ㅠ",
          "speaker_id": "1",
          "time": "20210826 11:15"
        },
        {
          "id": "MDRW2100014443.1.16",
          "form": "그래서 연구해서 키운담에 뿌려주던데",
          "original_form": "그래서 연구해서 키운담에 뿌려주던데",
          "speaker_id": "2",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100014443.1.17",
          "form": "전 순대국 먹고 커피한잔 사러왔어요",
          "original_form": "전 순대국 먹고 커피한잔 사러왔어요",
          "speaker_id": "2",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100014443.1.18",
          "form": "으아 좋은 코스이네요ㅋㅋ",
          "original_form": "으아 좋은 코스이네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:23"
        },
        {
          "id": "MDRW2100014443.1.19",
          "form": "순대국은 술과함께ㅋㅋ",
          "original_form": "순대국은 술과함께ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100014443.1.20",
          "form": "ㅎㅎ 전 술을 못해서",
          "original_form": "ㅎㅎ 전 술을 못해서",
          "speaker_id": "2",
          "time": "20210826 12:24"
        },
        {
          "id": "MDRW2100014443.1.21",
          "form": "맞아. 페어링을 못하셔서 어째ㅠ",
          "original_form": "맞아. 페어링을 못하셔서 어째ㅠ",
          "speaker_id": "1",
          "time": "20210826 12:25"
        },
        {
          "id": "MDRW2100014443.1.22",
          "form": "페어링은 몬가요ㅎㅎ",
          "original_form": "페어링은 몬가요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100014443.1.23",
          "form": "둘이 짝맞추는건가ㅋ",
          "original_form": "둘이 짝맞추는건가ㅋ",
          "speaker_id": "2",
          "time": "20210826 12:34"
        },
        {
          "id": "MDRW2100014443.1.24",
