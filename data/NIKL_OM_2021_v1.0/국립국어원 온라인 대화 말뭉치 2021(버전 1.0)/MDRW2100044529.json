      "utterance": [
        {
          "id": "MDRW2100044529.1.1",
          "form": "또 진행자 바뀌려나?",
          "original_form": "또 진행자 바뀌려나?",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100044529.1.2",
          "form": "바꿔서 하고 있지 않음?",
          "original_form": "바꿔서 하고 있지 않음?",
          "speaker_id": "1",
          "time": "20210907 23:05"
        },
        {
          "id": "MDRW2100044529.1.3",
          "form": "아 이미 한다는 거구낰ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 이미 한다는 거구낰ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044529.1.4",
          "form": "누가 진행하니?",
          "original_form": "누가 진행하니?",
          "speaker_id": "2",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044529.1.5",
          "form": "딘딘이랑 슬리피",
          "original_form": "딘딘이랑 슬리피",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044529.1.6",
          "form": "베라 네고하는거 봤음",
          "original_form": "베라 네고하는거 봤음",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044529.1.7",
          "form": "아 이번 배라 네고 네고왕3였구나",
          "original_form": "아 이번 배라 네고 네고왕3였구나",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044529.1.8",
          "form": "그거 가맹점 부담 있대서 별로였는대",
          "original_form": "그거 가맹점 부담 있대서 별로였는대",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044529.1.9",
          "form": "그렇다더라ㅋㅋ 인기도 많이 떨어진거 같구",
          "original_form": "그렇다더라ㅋㅋ 인기도 많이 떨어진거 같구",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044529.1.10",
          "form": "광희가 잘했음ㅋㅋㅋ",
          "original_form": "광희가 잘했음ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044529.1.11",
          "form": "그치 광희가 잘 했는데...",
          "original_form": "그치 광희가 잘 했는데...",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044529.1.12",
          "form": "2도 글케 챙겨보진 않앜ㅅ지만 나쁘지 않던데",
          "original_form": "2도 글케 챙겨보진 않앜ㅅ지만 나쁘지 않던데",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100044529.1.13",
          "form": "맞아ㅋㅋㅋ 2도 평 좋았던거 같음",
          "original_form": "맞아ㅋㅋㅋ 2도 평 좋았던거 같음",
          "speaker_id": "1",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044529.1.14",
          "form": "광희가 네고왕 덕은 많이 본 것 같음ㅋㅋㅋㅋㅋ",
          "original_form": "광희가 네고왕 덕은 많이 본 것 같음ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044529.1.15",
          "form": "광고도 찌꼬",
          "original_form": "광고도 찌꼬",
          "speaker_id": "2",
          "time": "20210907 23:26"
        },
        {
          "id": "MDRW2100044529.1.16",
          "form": "맞아. 저번에 공부왕? 그거 나온거 봤는데",
          "original_form": "맞아. 저번에 공부왕? 그거 나온거 봤는데",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044529.1.17",
          "form": "완전 유튜브 특화 캐릭터임ㅋㅋㅋㅋ",
          "original_form": "완전 유튜브 특화 캐릭터임ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044529.1.18",
          "form": "공부왕 그거 홍진경이 하는 거 아님?",
          "original_form": "공부왕 그거 홍진경이 하는 거 아님?",
          "speaker_id": "2",
          "time": "20210907 23:30"
        },
        {
          "id": "MDRW2100044529.1.19",
          "form": "ㅇㅇ 한번 광희 출현했었음",
          "original_form": "ㅇㅇ 한번 광희 출현했었음",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044529.1.20",
          "form": "공부.....광희....?",
          "original_form": "공부.....광희....?",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044529.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044529.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그냥 다 노답이던데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그냥 다 노답이던데",
          "speaker_id": "1",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044529.1.23",
          "form": "홍진경 공부 준비하다가 결국 공부 안 한 건 아는뎈ㅋㅋㅋㅋㅋㅋㅋ",
