      "utterance": [
        {
          "id": "MDRW2100023575.1.1",
          "form": "요즘 마스크쓰고 피부가 안좋아졌어요",
          "original_form": "요즘 마스크쓰고 피부가 안좋아졌어요",
          "speaker_id": "2",
          "time": "20210830 11:46"
        },
        {
          "id": "MDRW2100023575.1.2",
          "form": "다들 마스크 쓰고 트러블 호소하시더라구요ㅠ",
          "original_form": "다들 마스크 쓰고 트러블 호소하시더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210830 11:47"
        },
        {
          "id": "MDRW2100023575.1.3",
          "form": "피부도 숨을쉬어야 하는데ㅠ",
          "original_form": "피부도 숨을쉬어야 하는데ㅠ",
          "speaker_id": "1",
          "time": "20210830 11:48"
        },
        {
          "id": "MDRW2100023575.1.4",
          "form": "그쵸 요즘 피부과 다녀서 좀 덜한데 마스크를 벗지 않는한 계속 이럴거같아요",
          "original_form": "그쵸 요즘 피부과 다녀서 좀 덜한데 마스크를 벗지 않는한 계속 이럴거같아요",
          "speaker_id": "2",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023575.1.5",
          "form": "마스크 트러블로 병원 많이들 다니시더라구요~",
          "original_form": "마스크 트러블로 병원 많이들 다니시더라구요~",
          "speaker_id": "1",
          "time": "20210830 12:28"
        },
        {
          "id": "MDRW2100023575.1.6",
          "form": "저두 오돌토돌하게 올라오긴 하던데ㅠ",
          "original_form": "저두 오돌토돌하게 올라오긴 하던데ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:28"
        },
        {
          "id": "MDRW2100023575.1.7",
          "form": "그니까요 피부과를 안갈 수가 없어요",
          "original_form": "그니까요 피부과를 안갈 수가 없어요",
          "speaker_id": "2",
          "time": "20210830 12:43"
        },
        {
          "id": "MDRW2100023575.1.8",
          "form": "어른들은 마스크 트러블때문에ㅠㅠ",
          "original_form": "어른들은 마스크 트러블때문에ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 14:51"
        },
        {
          "id": "MDRW2100023575.1.9",
          "form": "저희 애들은 마스크 쓰고 감기함번 안했어요;;ㅋㅋㅋㅋ",
          "original_form": "저희 애들은 마스크 쓰고 감기함번 안했어요;;ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:52"
        },
        {
          "id": "MDRW2100023575.1.10",
          "form": "장 단점이 확실하쥬 ㅋㅋㅋㅋ",
          "original_form": "장 단점이 확실하쥬 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:52"
        },
        {
          "id": "MDRW2100023575.1.11",
          "form": "그러게요 마스크 트러블은\n 어떻게 할 수 없는것같아요",
          "original_form": "그러게요 마스크 트러블은\n 어떻게 할 수 없는것같아요",
          "speaker_id": "2",
          "time": "20210901 15:13"
        },
        {
          "id": "MDRW2100023575.1.12",
          "form": "맞아요 얼른 코로나가 없어져야 되겠죠ㅜ",
          "original_form": "맞아요 얼른 코로나가 없어져야 되겠죠ㅜ",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100023575.1.13",
          "form": "그니까요 코로나가 많은 걸 앗아갔어요",
          "original_form": "그니까요 코로나가 많은 걸 앗아갔어요",
          "speaker_id": "2",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100023575.1.14",
          "form": "맞아요 코로나 이후 알게 된 사람들은 얼굴도 제대로 몰라요ㅠ",
          "original_form": "맞아요 코로나 이후 알게 된 사람들은 얼굴도 제대로 몰라요ㅠ",
          "speaker_id": "1",
          "time": "20210901 20:29"
        },
        {
          "id": "MDRW2100023575.1.15",
          "form": "예전으로 돌아갈 수 있겠죠?",
          "original_form": "예전으로 돌아갈 수 있겠죠?",
          "speaker_id": "1",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100023575.1.16",
          "form": "맞아요 건강도 일상생활도 다 어렵네요",
          "original_form": "맞아요 건강도 일상생활도 다 어렵네요",
          "speaker_id": "2",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100023575.1.17",
          "form": "돌아갈거예요 좀 걸리긴하겠지만요",
          "original_form": "돌아갈거예요 좀 걸리긴하겠지만요",
          "speaker_id": "2",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100023575.1.18",
          "form": "맞아요 운동도 마음대로 할 수 없고",
          "original_form": "맞아요 운동도 마음대로 할 수 없고",
          "speaker_id": "1",
          "time": "20210901 20:30"
        },
        {
          "id": "MDRW2100023575.1.19",
          "form": "몸은 점점 더 건강해서 멀어져가요ㅠ",
          "original_form": "몸은 점점 더 건강해서 멀어져가요ㅠ",
          "speaker_id": "1",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100023575.1.20",
          "form": "그니까요 운동을 하려하면 코로나 심해져서 센터들 가기 꺼려지고",
          "original_form": "그니까요 운동을 하려하면 코로나 심해져서 센터들 가기 꺼려지고",
          "speaker_id": "2",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100023575.1.21",
          "form": "전 정말 대중탕한 번 가보고 싶어요ㅋㅌ",
          "original_form": "전 정말 대중탕한 번 가보고 싶어요ㅋㅌ",
          "speaker_id": "1",
          "time": "20210901 20:31"
        }
      ]
    }
