      "utterance": [
        {
          "id": "MDRW2100030093.1.1",
          "form": "이번 추석때는 무슨 계획 있나요?",
          "original_form": "이번 추석때는 무슨 계획 있나요?",
          "speaker_id": "1",
          "time": "20210903 21:20"
        },
        {
          "id": "MDRW2100030093.1.2",
          "form": "앗.. name1님 60대셨어요?! 전 30대이신줄알았어요 ㅎㅎ",
          "original_form": "앗.. &name1&님 60대셨어요?! 전 30대이신줄알았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100030093.1.3",
          "form": "저는 계획이없습니다. ㅎㅎ",
          "original_form": "저는 계획이없습니다. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100030093.1.4",
          "form": "음식 조금 만들어서 먹는 정도?",
          "original_form": "음식 조금 만들어서 먹는 정도?",
          "speaker_id": "2",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100030093.1.5",
          "form": "놀러가볼까했지만.. 코로나가 심해서 ㅠ",
          "original_form": "놀러가볼까했지만.. 코로나가 심해서 ㅠ",
          "speaker_id": "2",
          "time": "20210903 21:21"
        },
        {
          "id": "MDRW2100030093.1.6",
          "form": "젊게 봐줘서 고맙네요",
          "original_form": "젊게 봐줘서 고맙네요",
          "speaker_id": "1",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100030093.1.7",
          "form": "하하 ㅎㅎ",
          "original_form": "하하 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:23"
        },
        {
          "id": "MDRW2100030093.1.8",
          "form": "저희도 가족끼리 조촐히 모여 집에서만 있을 계획입니다",
          "original_form": "저희도 가족끼리 조촐히 모여 집에서만 있을 계획입니다",
          "speaker_id": "1",
          "time": "20210903 21:24"
        },
        {
          "id": "MDRW2100030093.1.9",
          "form": "친척들 못본지 오래라 그립네요",
          "original_form": "친척들 못본지 오래라 그립네요",
          "speaker_id": "1",
          "time": "20210903 21:24"
        },
        {
          "id": "MDRW2100030093.1.10",
          "form": "가족이 모일 수 있는게 행복한 게 아닐까요?",
          "original_form": "가족이 모일 수 있는게 행복한 게 아닐까요?",
          "speaker_id": "2",
          "time": "20210903 21:24"
        },
        {
          "id": "MDRW2100030093.1.11",
          "form": "친척은 내년에 봐요!",
          "original_form": "친척은 내년에 봐요!",
          "speaker_id": "2",
          "time": "20210903 21:24"
        },
        {
          "id": "MDRW2100030093.1.12",
          "form": "그래요 내년엔 가능하리라 기대해봅니다",
          "original_form": "그래요 내년엔 가능하리라 기대해봅니다",
          "speaker_id": "1",
          "time": "20210903 21:25"
        },
        {
          "id": "MDRW2100030093.1.13",
          "form": "name2님은 명절음식 만들줄 아나요?",
          "original_form": "&name2&님은 명절음식 만들줄 아나요?",
          "speaker_id": "1",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100030093.1.14",
          "form": "아뇨 ㅋㅋㅋ",
          "original_form": "아뇨 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100030093.1.15",
          "form": "어머니께서 어쩌피 시집가면 다 한다고 하셔서",
          "original_form": "어머니께서 어쩌피 시집가면 다 한다고 하셔서",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100030093.1.16",
          "form": "음식같은거 안시키셨어요",
          "original_form": "음식같은거 안시키셨어요",
          "speaker_id": "2",
          "time": "20210903 21:26"
        },
        {
          "id": "MDRW2100030093.1.17",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100030093.1.18",
          "form": "우리 아내도 딸들 안시키더군요",
          "original_form": "우리 아내도 딸들 안시키더군요",
          "speaker_id": "1",
          "time": "20210903 21:27"
        },
        {
          "id": "MDRW2100030093.1.19",
          "form": "ㅋㅋㅋ 그쵸 어쩌피 해야한다면 그 때가서 해도 된다고 생각하시더라구요",
          "original_form": "ㅋㅋㅋ 그쵸 어쩌피 해야한다면 그 때가서 해도 된다고 생각하시더라구요",
          "speaker_id": "2",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100030093.1.20",
          "form": "고생",
          "original_form": "고생",
          "speaker_id": "2",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100030093.1.21",
          "form": "덜 하라구 ㅎㅎ",
          "original_form": "덜 하라구 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100030093.1.22",
          "form": "엄머의 마음이 그런가 봅니다",
          "original_form": "엄머의 마음이 그런가 봅니다",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100030093.1.23",
          "form": "네 ㅎㅎ 그런가봐요",
          "original_form": "네 ㅎㅎ 그런가봐요",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100030093.1.24",
