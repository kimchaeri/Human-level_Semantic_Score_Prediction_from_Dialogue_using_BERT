      "utterance": [
        {
          "id": "MDRW2100011803.1.1",
          "form": "name2님은 파르페 좋아하시나요?",
          "original_form": "&name2&님은 파르페 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 16:12"
        },
        {
          "id": "MDRW2100011803.1.2",
          "form": "사실 한번도 안 먹어 봤어요ㅋㅋㅋ",
          "original_form": "사실 한번도 안 먹어 봤어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:14"
        },
        {
          "id": "MDRW2100011803.1.3",
          "form": "맛집 찾아가시는 편인가요?",
          "original_form": "맛집 찾아가시는 편인가요?",
          "speaker_id": "1",
          "time": "20210824 17:55"
        },
        {
          "id": "MDRW2100011803.1.4",
          "form": "아뇨 그냥 가던 곳 자주 가는 편이에요",
          "original_form": "아뇨 그냥 가던 곳 자주 가는 편이에요",
          "speaker_id": "2",
          "time": "20210824 17:57"
        },
        {
          "id": "MDRW2100011803.1.5",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210824 17:57"
        },
        {
          "id": "MDRW2100011803.1.6",
          "form": "저두요ㅋㅋ줄서서 잘 안먹어요",
          "original_form": "저두요ㅋㅋ줄서서 잘 안먹어요",
          "speaker_id": "1",
          "time": "20210824 19:32"
        },
        {
          "id": "MDRW2100011803.1.7",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210824 19:32"
        },
        {
          "id": "MDRW2100011803.1.8",
          "form": "30분 이상은 안 기다리는 거 같아요",
          "original_form": "30분 이상은 안 기다리는 거 같아요",
          "speaker_id": "2",
          "time": "20210824 19:33"
        },
        {
          "id": "MDRW2100011803.1.9",
          "form": "약간 막입이라 좀 맛없어도 그냥 잘 먹어요ㅎㅎ",
          "original_form": "약간 막입이라 좀 맛없어도 그냥 잘 먹어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 19:33"
        },
        {
          "id": "MDRW2100011803.1.10",
          "form": "저두요ㅋㅋ기다리는게 더 힘들어요",
          "original_form": "저두요ㅋㅋ기다리는게 더 힘들어요",
          "speaker_id": "1",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100011803.1.11",
          "form": "집근처에 맛집 많나요",
          "original_form": "집근처에 맛집 많나요",
          "speaker_id": "1",
          "time": "20210824 21:40"
        },
        {
          "id": "MDRW2100011803.1.12",
          "form": "닭갈비 맛있는 집있어요!",
          "original_form": "닭갈비 맛있는 집있어요!",
          "speaker_id": "2",
          "time": "20210824 21:41"
        },
        {
          "id": "MDRW2100011803.1.13",
          "form": "그리고 할맥 있어요! 친구들이랑 먹기 좋아요ㅎㅎ",
          "original_form": "그리고 할맥 있어요! 친구들이랑 먹기 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 21:41"
        },
        {
          "id": "MDRW2100011803.1.14",
          "form": "오 요새 유명하다는 할맥",
          "original_form": "오 요새 유명하다는 할맥",
          "speaker_id": "1",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100011803.1.15",
          "form": "한번도 안가봤어요 아직ㅜㅜ",
          "original_form": "한번도 안가봤어요 아직ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100011803.1.16",
          "form": "할맥 생맥주 맛있어요!",
          "original_form": "할맥 생맥주 맛있어요!",
          "speaker_id": "2",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100011803.1.17",
          "form": "생맥 할맥에서 처음 마셔봤는데 진짜 맛있었어요",
          "original_form": "생맥 할맥에서 처음 마셔봤는데 진짜 맛있었어요",
          "speaker_id": "2",
          "time": "20210824 21:46"
        },
        {
          "id": "MDRW2100011803.1.18",
          "form": "우와 가보고 싶네요",
          "original_form": "우와 가보고 싶네요",
          "speaker_id": "1",
          "time": "20210824 21:49"
        },
        {
          "id": "MDRW2100011803.1.19",
          "form": "왜 저희 동네엔 없는지...ㅜ",
          "original_form": "왜 저희 동네엔 없는지...ㅜ",
          "speaker_id": "1",
          "time": "20210824 21:49"
        },
        {
          "id": "MDRW2100011803.1.20",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 21:50"
        },
        {
          "id": "MDRW2100011803.1.21",
          "form": "술 즐겨 드시나요?",
          "original_form": "술 즐겨 드시나요?",
          "speaker_id": "2",
          "time": "20210824 21:50"
        },
        {
          "id": "MDRW2100011803.1.22",
          "form": "잘먹진않는데 분위기는 좋아해요",
          "original_form": "잘먹진않는데 분위기는 좋아해요",
          "speaker_id": "1",
          "time": "20210824 21:51"
        },
        {
          "id": "MDRW2100011803.1.23",
          "form": "뭔지 알 거 같아요! 술 마시면서 친구들이랑 얘기하고 게임하는 거 너무 재밌어요",
          "original_form": "뭔지 알 거 같아요! 술 마시면서 친구들이랑 얘기하고 게임하는 거 너무 재밌어요",
          "speaker_id": "2",
          "time": "20210824 21:52"
        },
        {
          "id": "MDRW2100011803.1.24",
          "form": "네ㅋㅋ전엔 한창 그랬는데...이젠 못하네요ㅜㅜ",
          "original_form": "네ㅋㅋ전엔 한창 그랬는데...이젠 못하네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 21:53"
        },
        {
          "id": "MDRW2100011803.1.25",
          "form": "코로나도 그렇고 다들 안 만나게 되더라고요",
          "original_form": "코로나도 그렇고 다들 안 만나게 되더라고요",
          "speaker_id": "1",
          "time": "20210824 21:54"
        },
        {
          "id": "MDRW2100011803.1.26",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 21:54"
        }
      ]
    }
