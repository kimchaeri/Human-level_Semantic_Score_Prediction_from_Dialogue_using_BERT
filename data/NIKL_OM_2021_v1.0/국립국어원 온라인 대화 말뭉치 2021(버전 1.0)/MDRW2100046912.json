      "utterance": [
        {
          "id": "MDRW2100046912.1.1",
          "form": "파이어족 가즈아!",
          "original_form": "파이어족 가즈아!",
          "speaker_id": "1",
          "time": "20211006 00:17"
        },
        {
          "id": "MDRW2100046912.1.2",
          "form": "퐈이아~",
          "original_form": "퐈이아~",
          "speaker_id": "2",
          "time": "20211006 00:17"
        },
        {
          "id": "MDRW2100046912.1.3",
          "form": "박명수 퐈이아~",
          "original_form": "박명수 퐈이아~",
          "speaker_id": "2",
          "time": "20211006 00:18"
        },
        {
          "id": "MDRW2100046912.1.4",
          "form": "그 파이어 아니예요~",
          "original_form": "그 파이어 아니예요~",
          "speaker_id": "1",
          "time": "20211006 00:18"
        },
        {
          "id": "MDRW2100046912.1.5",
          "form": "Financial Independence, Retire Early",
          "original_form": "Financial Independence, Retire Early",
          "speaker_id": "1",
          "time": "20211006 00:18"
        },
        {
          "id": "MDRW2100046912.1.6",
          "form": "명수옹도 일찍 은퇴하고 이민가고 싶어하던데",
          "original_form": "명수옹도 일찍 은퇴하고 이민가고 싶어하던데",
          "speaker_id": "2",
          "time": "20211006 00:18"
        },
        {
          "id": "MDRW2100046912.1.7",
          "form": "래요",
          "original_form": "래요",
          "speaker_id": "1",
          "time": "20211006 00:18"
        },
        {
          "id": "MDRW2100046912.1.8",
          "form": "아하 저도 그러고싶네됴",
          "original_form": "아하 저도 그러고싶네됴",
          "speaker_id": "1",
          "time": "20211006 00:19"
        },
        {
          "id": "MDRW2100046912.1.9",
          "form": "엘세갈은?",
          "original_form": "엘세갈은?",
          "speaker_id": "1",
          "time": "20211006 00:19"
        },
        {
          "id": "MDRW2100046912.1.10",
          "form": "저도 그러고 싶지요",
          "original_form": "저도 그러고 싶지요",
          "speaker_id": "2",
          "time": "20211006 00:20"
        },
        {
          "id": "MDRW2100046912.1.11",
          "form": "그러면 업헬스야 힘을내",
          "original_form": "그러면 업헬스야 힘을내",
          "speaker_id": "1",
          "time": "20211006 00:20"
        },
        {
          "id": "MDRW2100046912.1.12",
          "form": "목표 은퇴 나이는?",
          "original_form": "목표 은퇴 나이는?",
          "speaker_id": "2",
          "time": "20211006 00:21"
        },
        {
          "id": "MDRW2100046912.1.13",
          "form": "45",
          "original_form": "45",
          "speaker_id": "1",
          "time": "20211006 00:21"
        },
        {
          "id": "MDRW2100046912.1.14",
          "form": "U?",
          "original_form": "U?",
          "speaker_id": "1",
          "time": "20211006 00:22"
        },
        {
          "id": "MDRW2100046912.1.15",
          "form": "특별한 이유라도 있나요?",
          "original_form": "특별한 이유라도 있나요?",
          "speaker_id": "2",
          "time": "20211006 00:22"
        },
        {
          "id": "MDRW2100046912.1.16",
          "form": "전 asap...",
          "original_form": "전 asap...",
          "speaker_id": "2",
          "time": "20211006 00:22"
        },
        {
          "id": "MDRW2100046912.1.17",
          "form": "aeap....",
          "original_form": "aeap....",
          "speaker_id": "2",
          "time": "20211006 00:22"
        },
        {
          "id": "MDRW2100046912.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211006 00:22"
        },
        {
          "id": "MDRW2100046912.1.19",
          "form": "아삽이 젤 조치",
          "original_form": "아삽이 젤 조치",
          "speaker_id": "1",
          "time": "20211006 00:22"
        },
        {
          "id": "MDRW2100046912.1.20",
          "form": "유퀴즈에 나왔는데 봤어?",
          "original_form": "유퀴즈에 나왔는데 봤어?",
          "speaker_id": "1",
          "time": "20211006 00:23"
        },
        {
          "id": "MDRW2100046912.1.21",
          "form": "파이어족 관련해서?",
          "original_form": "파이어족 관련해서?",
          "speaker_id": "2",
          "time": "20211006 00:23"
        },
        {
          "id": "MDRW2100046912.1.22",
          "form": "응 실제 은퇴하신분",
          "original_form": "응 실제 은퇴하신분",
          "speaker_id": "1",
          "time": "20211006 00:24"
        },
        {
          "id": "MDRW2100046912.1.23",
          "form": "안본거 같아 기억이 안나",
          "original_form": "안본거 같아 기억이 안나",
          "speaker_id": "2",
          "time": "20211006 00:25"
        },
        {
          "id": "MDRW2100046912.1.24",
          "form": "부부가 같이 했나본데 아내분만 나왔어",
          "original_form": "부부가 같이 했나본데 아내분만 나왔어",
          "speaker_id": "1",
          "time": "20211006 00:25"
        },
        {
          "id": "MDRW2100046912.1.25",
          "form": "그그",
          "original_form": "그그",
          "speaker_id": "1",
          "time": "20211006 00:26"
        },
        {
          "id": "MDRW2100046912.1.26",
          "form": "어떤 준비를 했대 그분들은?",
          "original_form": "어떤 준비를 했대 그분들은?",
          "speaker_id": "2",
          "time": "20211006 00:26"
        },
        {
          "id": "MDRW2100046912.1.27",
          "form": "5억모으구 은퇴",
          "original_form": "5억모으구 은퇴",
          "speaker_id": "1",
          "time": "20211006 00:27"
        },
        {
          "id": "MDRW2100046912.1.28",
          "form": "5억으로?",
          "original_form": "5억으로?",
          "speaker_id": "2",
          "time": "20211006 00:27"
        },
        {
          "id": "MDRW2100046912.1.29",
          "form": "응 ㅋㅋㅋㅋㅋㅋ 아주 빠듯하게",
          "original_form": "응 ㅋㅋㅋㅋㅋㅋ 아주 빠듯하게",
          "speaker_id": "1",
          "time": "20211006 00:27"
        },
        {
          "id": "MDRW2100046912.1.30",
          "form": "왜 5억이냐면",
          "original_form": "왜 5억이냐면",
          "speaker_id": "1",
          "time": "20211006 00:27"
        },
        {
          "id": "MDRW2100046912.1.31",
          "form": "개인연금 받기 전까지 생활비 월 250 × n년 = 4억",
          "original_form": "개인연금 받기 전까지 생활비 월 250 × n년 = 4억",
          "speaker_id": "1",
          "time": "20211006 00:28"
        },
        {
          "id": "MDRW2100046912.1.32",
          "form": "여행자금 1억",
          "original_form": "여행자금 1억",
          "speaker_id": "1",
          "time": "20211006 00:28"
        },
        {
          "id": "MDRW2100046912.1.33",
          "form": "해서 5억이래",
          "original_form": "해서 5억이래",
          "speaker_id": "1",
          "time": "20211006 00:28"
        },
        {
          "id": "MDRW2100046912.1.34",
          "form": "단순한 계산법이네",
          "original_form": "단순한 계산법이네",
          "speaker_id": "2",
          "time": "20211006 00:28"
        },
        {
          "id": "MDRW2100046912.1.35",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "1",
          "time": "20211006 00:28"
        },
        {
          "id": "MDRW2100046912.1.36",
          "form": "경제적 자립이라는거 상대적이긴 하지만",
          "original_form": "경제적 자립이라는거 상대적이긴 하지만",
          "speaker_id": "2",
          "time": "20211006 00:29"
        },
        {
          "id": "MDRW2100046912.1.37",
          "form": "그리고 개인연금 받다가",
          "original_form": "그리고 개인연금 받다가",
          "speaker_id": "1",
          "time": "20211006 00:29"
        },
        {
          "id": "MDRW2100046912.1.38",
          "form": "그후에는 국민연금",
          "original_form": "그후에는 국민연금",
          "speaker_id": "1",
          "time": "20211006 00:29"
        },
        {
          "id": "MDRW2100046912.1.39",
          "form": "경제적 자립보다 근심걱정으로부터의 자립이 더 어울리는 느낌쓰",
          "original_form": "경제적 자립보다 근심걱정으로부터의 자립이 더 어울리는 느낌쓰",
          "speaker_id": "2",
          "time": "20211006 00:30"
        },
        {
          "id": "MDRW2100046912.1.40",
          "form": "완전한 자유는 아닌거같지",
          "original_form": "완전한 자유는 아닌거같지",
          "speaker_id": "1",
          "time": "20211006 00:31"
        },
        {
          "id": "MDRW2100046912.1.41",
          "form": "",
          "original_form": "{share:url}",
          "speaker_id": "1",
          "time": "20211006 00:31"
        },
        {
          "id": "MDRW2100046912.1.42",
          "form": "복사도 안돼요 여기",
          "original_form": "복사도 안돼요 여기",
          "speaker_id": "2",
          "time": "20211006 00:31"
        },
        {
          "id": "MDRW2100046912.1.43",
          "form": "아... 나중에 보내드릴게요",
          "original_form": "아... 나중에 보내드릴게요",
          "speaker_id": "1",
          "time": "20211006 00:31"
        },
        {
          "id": "MDRW2100046912.1.44",
          "form": "얼마정도면",
          "original_form": "얼마정도면",
          "speaker_id": "2",
          "time": "20211006 00:32"
        },
        {
          "id": "MDRW2100046912.1.45",
          "form": "경제적 자립이라고 생각하나요",
          "original_form": "경제적 자립이라고 생각하나요",
          "speaker_id": "2",
          "time": "20211006 00:32"
        },
        {
          "id": "MDRW2100046912.1.46",
          "form": "글쎄.. 집있고 암것도 안해도 꼬박꼬박 월 400이상 나오면요",
          "original_form": "글쎄.. 집있고 암것도 안해도 꼬박꼬박 월 400이상 나오면요",
          "speaker_id": "1",
          "time": "20211006 00:33"
        },
        {
          "id": "MDRW2100046912.1.47",
          "form": "세갈이는?",
          "original_form": "세갈이는?",
          "speaker_id": "1",
          "time": "20211006 00:33"
        },
        {
          "id": "MDRW2100046912.1.48",
          "form": "20억정도...?",
          "original_form": "20억정도...?",
          "speaker_id": "2",
          "time": "20211006 00:34"
        },
        {
          "id": "MDRW2100046912.1.49",
          "form": "20억이면 배당이 얼마나나와?",
          "original_form": "20억이면 배당이 얼마나나와?",
          "speaker_id": "1",
          "time": "20211006 00:34"
        },
        {
          "id": "MDRW2100046912.1.50",
          "form": "7-8천정도",
          "original_form": "7-8천정도",
          "speaker_id": "2",
          "time": "20211006 00:35"
        },
        {
          "id": "MDRW2100046912.1.51",
          "form": "오홍",
          "original_form": "오홍",
          "speaker_id": "1",
          "time": "20211006 00:35"
        },
        {
          "id": "MDRW2100046912.1.52",
          "form": "그럼 500이야?",
          "original_form": "그럼 500이야?",
          "speaker_id": "1",
          "time": "20211006 00:35"
        },
        {
          "id": "MDRW2100046912.1.53",
          "form": "4천은 쓰고 남은건 재투자 통한 복리의 마법 실현",
          "original_form": "4천은 쓰고 남은건 재투자 통한 복리의 마법 실현",
          "speaker_id": "2",
          "time": "20211006 00:35"
        },
        {
          "id": "MDRW2100046912.1.54",
          "form": "월에 7천이나왕? 20억으로?",
          "original_form": "월에 7천이나왕? 20억으로?",
          "speaker_id": "1",
          "time": "20211006 00:36"
        },
        {
          "id": "MDRW2100046912.1.55",
          "form": "년이 아니라? 오홍",
          "original_form": "년이 아니라? 오홍",
          "speaker_id": "1",
          "time": "20211006 00:36"
        },
        {
          "id": "MDRW2100046912.1.56",
          "form": "per year 로!",
          "original_form": "per year 로!",
          "speaker_id": "2",
          "time": "20211006 00:37"
        }
      ]
    }
