        {
          "id": "MDRW2100026646.1.1",
          "form": "ㅋㅋ나는한방샴푸삼ㅋㅋ",
          "original_form": "ㅋㅋ나는한방샴푸삼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026646.1.2",
          "form": "한방?어디꺼?",
          "original_form": "한방?어디꺼?",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026646.1.3",
          "form": "댕기머리?",
          "original_form": "댕기머리?",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026646.1.4",
          "form": "아니ㅋ아모레꺼",
          "original_form": "아니ㅋ아모레꺼",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026646.1.5",
          "form": "려씀",
          "original_form": "려씀",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026646.1.6",
          "form": "오ㅋㅋ좋은거쓰네",
          "original_form": "오ㅋㅋ좋은거쓰네",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026646.1.7",
          "form": "나는한방쓰면머리가부들거려",
          "original_form": "나는한방쓰면머리가부들거려",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026646.1.8",
          "form": "나는최근실리콘프리샴푸사봤어",
          "original_form": "나는최근실리콘프리샴푸사봤어",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026646.1.9",
          "form": "나도그거써보고싶은데",
          "original_form": "나도그거써보고싶은데",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026646.1.10",
          "form": "어때?",
          "original_form": "어때?",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026646.1.11",
          "form": "잘은모르겠는데",
          "original_form": "잘은모르겠는데",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026646.1.12",
          "form": "지루성두피염에좋다길래샀음",
          "original_form": "지루성두피염에좋다길래샀음",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026646.1.13",
          "form": "그런거은근머리빡빡할까봐",
          "original_form": "그런거은근머리빡빡할까봐",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026646.1.14",
          "form": "괜히피하게됨",
          "original_form": "괜히피하게됨",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026646.1.15",
          "form": "응ㅋㅋ근데이번껀나쁘지않아ㅋ",
          "original_form": "응ㅋㅋ근데이번껀나쁘지않아ㅋ",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026646.1.16",
          "form": "어디서샀어?",
          "original_form": "어디서샀어?",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026646.1.17",
          "form": "올리브영에서샀어",
          "original_form": "올리브영에서샀어",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100026646.1.18",
