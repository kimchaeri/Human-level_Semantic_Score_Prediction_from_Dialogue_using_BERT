      "utterance": [
        {
          "id": "MDRW2100037855.1.1",
          "form": "근데 이게 name1님한테는 너무 건조하려나....",
          "original_form": "근데 이게 &name1&님한테는 너무 건조하려나....",
          "speaker_id": "2",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100037855.1.2",
          "form": "하루에 아침1 저녁1이런식인걸까여",
          "original_form": "하루에 아침1 저녁1이런식인걸까여",
          "speaker_id": "1",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100037855.1.3",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100037855.1.4",
          "form": "네 민감성이라 근데 또 지루성이니까",
          "original_form": "네 민감성이라 근데 또 지루성이니까",
          "speaker_id": "1",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100037855.1.5",
          "form": "꼭하루에한번씩은 감아야돼요",
          "original_form": "꼭하루에한번씩은 감아야돼요",
          "speaker_id": "1",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100037855.1.6",
          "form": "하 진짜 답답하네요",
          "original_form": "하 진짜 답답하네요",
          "speaker_id": "2",
          "time": "20210901 19:48"
        },
        {
          "id": "MDRW2100037855.1.7",
          "form": "네네 맞아요 저도 머리 매일 감아요ㅠ",
          "original_form": "네네 맞아요 저도 머리 매일 감아요ㅠ",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100037855.1.8",
          "form": "안그러며뉴",
          "original_form": "안그러며뉴",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100037855.1.9",
          "form": "안그러면 너무 찝찝해서",
          "original_form": "안그러면 너무 찝찝해서",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100037855.1.10",
          "form": "ㅜㅜ아토히도 힘드시잖아여",
          "original_form": "ㅜㅜ아토히도 힘드시잖아여",
          "speaker_id": "1",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100037855.1.11",
          "form": "아 그러고보니 저도 하루에 두 번 감네요",
          "original_form": "아 그러고보니 저도 하루에 두 번 감네요",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100037855.1.12",
          "form": "그것도 답도없던데",
          "original_form": "그것도 답도없던데",
          "speaker_id": "1",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100037855.1.13",
          "form": "아침에 한 번 저녁에 한 번",
          "original_form": "아침에 한 번 저녁에 한 번",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100037855.1.14",
          "form": "네 어렸을 때 너무 심해서",
          "original_form": "네 어렸을 때 너무 심해서",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100037855.1.15",
          "form": "붕대를 감고 다녔어요,,",
          "original_form": "붕대를 감고 다녔어요,,",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100037855.1.16",
          "form": "ㅜㅜ제동생도 그랬어요",
          "original_form": "ㅜㅜ제동생도 그랬어요",
          "speaker_id": "1",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100037855.1.17",
          "form": "중학생 되니까 많이 나아져서",
          "original_form": "중학생 되니까 많이 나아져서",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100037855.1.18",
          "form": "지금은 아토피는 없고 비염이 되긴 했는데",
          "original_form": "지금은 아토피는 없고 비염이 되긴 했는데",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100037855.1.19",
          "form": "지금도 컨디션안좋거나 하면 접히는부위에 올라오더라구여",
          "original_form": "지금도 컨디션안좋거나 하면 접히는부위에 올라오더라구여",
          "speaker_id": "1",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100037855.1.20",
          "form": "아이고 동생분도 아토피가 심하군요ㅠ",
          "original_form": "아이고 동생분도 아토피가 심하군요ㅠ",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100037855.1.21",
          "form": "네네 접히는 부위에요!!!!!",
          "original_form": "네네 접히는 부위에요!!!!!",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100037855.1.22",
          "form": "네 평소에는 지금은괜찮은데 확나올때 한번씩있대여",
          "original_form": "네 평소에는 지금은괜찮은데 확나올때 한번씩있대여",
