        {
          "id": "MDRW2100016764.1.1",
          "form": "맛집 좋아하시나요",
          "original_form": "맛집 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 15:14"
        },
        {
          "id": "MDRW2100016764.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210826 15:15"
        },
        {
          "id": "MDRW2100016764.1.3",
          "form": "대기가 긴 집은 잘 안가긴 해요",
          "original_form": "대기가 긴 집은 잘 안가긴 해요",
          "speaker_id": "2",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016764.1.4",
          "form": "어떤메뉴 좋아하세요",
          "original_form": "어떤메뉴 좋아하세요",
          "speaker_id": "1",
          "time": "20210826 15:16"
        },
        {
          "id": "MDRW2100016764.1.5",
          "form": "돈까스 좋아해요",
          "original_form": "돈까스 좋아해요",
          "speaker_id": "2",
          "time": "20210826 15:17"
        },
        {
          "id": "MDRW2100016764.1.6",
          "form": "오 크레이지카츠 가보세요!",
          "original_form": "오 크레이지카츠 가보세요!",
          "speaker_id": "1",
          "time": "20210826 15:18"
        },
        {
          "id": "MDRW2100016764.1.7",
          "form": "맛집인가봐요?",
          "original_form": "맛집인가봐요?",
          "speaker_id": "2",
          "time": "20210826 15:21"
        },
        {
          "id": "MDRW2100016764.1.8",
          "form": "서울3대 돈까스입니다!",
          "original_form": "서울3대 돈까스입니다!",
          "speaker_id": "1",
          "time": "20210826 16:26"
        },
        {
          "id": "MDRW2100016764.1.9",
          "form": "오 한번 가보고 싶네요",
          "original_form": "오 한번 가보고 싶네요",
          "speaker_id": "2",
          "time": "20210826 16:27"
        },
        {
          "id": "MDRW2100016764.1.10",
          "form": "요샌 프랜차이즈만 갔거든요ㅋㅋ",
          "original_form": "요샌 프랜차이즈만 갔거든요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:27"
        },
        {
          "id": "MDRW2100016764.1.11",
          "form": "특로스카츠 추천합니다",
          "original_form": "특로스카츠 추천합니다",
          "speaker_id": "1",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016764.1.12",
          "form": "ㅋㅋ가면 꼭 먹어볼게요",
          "original_form": "ㅋㅋ가면 꼭 먹어볼게요",
          "speaker_id": "2",
          "time": "20210826 16:36"
        },
        {
          "id": "MDRW2100016764.1.13",
          "form": "저녁 뭐드시나요?",
          "original_form": "저녁 뭐드시나요?",
          "speaker_id": "1",
          "time": "20210826 16:38"
        },
        {
          "id": "MDRW2100016764.1.14",
          "form": "아직 못 정했어요. 정하셨나요?",
          "original_form": "아직 못 정했어요. 정하셨나요?",
          "speaker_id": "2",
          "time": "20210826 16:39"
        },
        {
          "id": "MDRW2100016764.1.15",
