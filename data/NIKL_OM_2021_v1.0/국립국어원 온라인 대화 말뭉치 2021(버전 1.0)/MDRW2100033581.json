      "utterance": [
        {
          "id": "MDRW2100033581.1.1",
          "form": "반려동물 길러보셨나요",
          "original_form": "반려동물 길러보셨나요",
          "speaker_id": "2",
          "time": "20210824 15:28"
        },
        {
          "id": "MDRW2100033581.1.2",
          "form": "전 두마리 길러요",
          "original_form": "전 두마리 길러요",
          "speaker_id": "1",
          "time": "20210824 15:29"
        },
        {
          "id": "MDRW2100033581.1.3",
          "form": "도마뱀요 ㅎㅎ",
          "original_form": "도마뱀요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 15:29"
        },
        {
          "id": "MDRW2100033581.1.4",
          "form": "반려동물 안기르시나요?",
          "original_form": "반려동물 안기르시나요?",
          "speaker_id": "1",
          "time": "20210824 15:30"
        },
        {
          "id": "MDRW2100033581.1.5",
          "form": "저 예전에 구피도 길렀는데",
          "original_form": "저 예전에 구피도 길렀는데",
          "speaker_id": "1",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100033581.1.6",
          "form": "다시 기르고 싶어요 ㅎㅎ",
          "original_form": "다시 기르고 싶어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 15:38"
        },
        {
          "id": "MDRW2100033581.1.7",
          "form": "지금은 비어디 드래곤 두마리 키워요",
          "original_form": "지금은 비어디 드래곤 두마리 키워요",
          "speaker_id": "1",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100033581.1.8",
          "form": "와 다양하게 기르시네요! 도마뱀이라니!^^",
          "original_form": "와 다양하게 기르시네요! 도마뱀이라니!^^",
          "speaker_id": "2",
          "time": "20210824 18:19"
        },
        {
          "id": "MDRW2100033581.1.9",
          "form": "ㅎㅎ지금은 도마뱀만요",
          "original_form": "ㅎㅎ지금은 도마뱀만요",
          "speaker_id": "1",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100033581.1.10",
          "form": "배우 이상이? 취미가 물고기 키우기더라구요^^",
          "original_form": "배우 이상이? 취미가 물고기 키우기더라구요^^",
          "speaker_id": "2",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100033581.1.11",
          "form": "사실 밀웜을 먹어서 밀웜도 길러요",
          "original_form": "사실 밀웜을 먹어서 밀웜도 길러요",
          "speaker_id": "1",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100033581.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100033581.1.13",
          "form": "물멍 ㅎㅎ",
          "original_form": "물멍 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100033581.1.14",
          "form": "아 다시길러야 겠어요 ㅎㅎ",
          "original_form": "아 다시길러야 겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100033581.1.15",
          "form": "오 밀웜.. 들어본것 같아요 물고기 먹이인거죠?",
          "original_form": "오 밀웜.. 들어본것 같아요 물고기 먹이인거죠?",
          "speaker_id": "2",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100033581.1.16",
          "form": "지렁이 같이 생긴건데",
          "original_form": "지렁이 같이 생긴건데",
          "speaker_id": "1",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100033581.1.17",
          "form": "도마뱀줘요",
          "original_form": "도마뱀줘요",
          "speaker_id": "1",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100033581.1.18",
          "form": "밀웜이 크면 까만 거저리 되고 아기 밀웜나요",
          "original_form": "밀웜이 크면 까만 거저리 되고 아기 밀웜나요",
          "speaker_id": "1",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100033581.1.19",
          "form": "낳아요",
          "original_form": "낳아요",
          "speaker_id": "1",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100033581.1.20",
          "form": "넘작아서 ㅋㅋ",
          "original_form": "넘작아서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100033581.1.21",
          "form": "기르시는데 비용이 많이 들진 않나요? 관리하는..",
          "original_form": "기르시는데 비용이 많이 들진 않나요? 관리하는..",
          "speaker_id": "2",
          "time": "20210824 18:51"
        },
        {
          "id": "MDRW2100033581.1.22",
          "form": "별로요 ㅎㅎ먹이도 직접 기르고 밀웜은 뭐 돈들게 없어요 ㅎㅎ",
          "original_form": "별로요 ㅎㅎ먹이도 직접 기르고 밀웜은 뭐 돈들게 없어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:52"
        },
        {
          "id": "MDRW2100033581.1.23",
          "form": "오 그렇군요 아이도 좋아하나요?",
          "original_form": "오 그렇군요 아이도 좋아하나요?",
          "speaker_id": "2",
          "time": "20210824 18:53"
        },
        {
          "id": "MDRW2100033581.1.24",
          "form": "네 ㅡㅡ근데 제가 다 키워요",
          "original_form": "네 ㅡㅡ근데 제가 다 키워요",
          "speaker_id": "1",
          "time": "20210824 18:56"
        },
        {
          "id": "MDRW2100033581.1.25",
          "form": "그죠 다 엄마일이죠 ㅠㅠ",
          "original_form": "그죠 다 엄마일이죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 18:57"
        }
      ]
    }
