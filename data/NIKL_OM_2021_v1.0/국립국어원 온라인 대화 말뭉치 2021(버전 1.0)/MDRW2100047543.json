      "utterance": [
        {
          "id": "MDRW2100047543.1.1",
          "form": "요즘 무슨노래 들으세요?",
          "original_form": "요즘 무슨노래 들으세요?",
          "speaker_id": "1",
          "time": "20210826 19:47"
        },
        {
          "id": "MDRW2100047543.1.2",
          "form": "오늘 이무진 신호등 들었어요",
          "original_form": "오늘 이무진 신호등 들었어요",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047543.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100047543.1.4",
          "form": "전 아이즈원 피에스타 들었어요 아이즈원 아시나요?",
          "original_form": "전 아이즈원 피에스타 들었어요 아이즈원 아시나요?",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100047543.1.5",
          "form": "네 알아요 엠넷방송할때 열심히봤어요",
          "original_form": "네 알아요 엠넷방송할때 열심히봤어요",
          "speaker_id": "2",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100047543.1.6",
          "form": "귀여운 워뇨...",
          "original_form": "귀여운 워뇨...",
          "speaker_id": "2",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100047543.1.7",
          "form": "워뇨ㅠㅠ 투표도 하셨나요?",
          "original_form": "워뇨ㅠㅠ 투표도 하셨나요?",
          "speaker_id": "1",
          "time": "20210826 23:57"
        },
        {
          "id": "MDRW2100047543.1.8",
          "form": "아뇨 그때 예능보듯이 봤던거라 하하..",
          "original_form": "아뇨 그때 예능보듯이 봤던거라 하하..",
          "speaker_id": "2",
          "time": "20210827 00:04"
        },
        {
          "id": "MDRW2100047543.1.9",
          "form": "뭔가 인터넷으로 한번했던것같은데 기억이안나네요",
          "original_form": "뭔가 인터넷으로 한번했던것같은데 기억이안나네요",
          "speaker_id": "2",
          "time": "20210827 00:04"
        },
        {
          "id": "MDRW2100047543.1.10",
          "form": "좋아하는 걸그룹 있으세요?",
          "original_form": "좋아하는 걸그룹 있으세요?",
          "speaker_id": "1",
          "time": "20210827 10:36"
        },
        {
          "id": "MDRW2100047543.1.11",
          "form": "블랙핑크 트와이스 노래 자주듣는것같아요",
          "original_form": "블랙핑크 트와이스 노래 자주듣는것같아요",
          "speaker_id": "2",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100047543.1.12",
          "form": "영화 어떤 장르즐겨보세요",
          "original_form": "영화 어떤 장르즐겨보세요",
          "speaker_id": "2",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100047543.1.13",
          "form": "전 로맨틱코미디요! 좋아하세요?",
          "original_form": "전 로맨틱코미디요! 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 13:43"
        },
        {
          "id": "MDRW2100047543.1.14",
          "form": "네 자주는 아니지만 티비틀면 한번씩봐요",
          "original_form": "네 자주는 아니지만 티비틀면 한번씩봐요",
          "speaker_id": "2",
          "time": "20210827 13:53"
        },
        {
          "id": "MDRW2100047543.1.15",
          "form": "로코영화 어떤게 가장재밌으셨어요",
          "original_form": "로코영화 어떤게 가장재밌으셨어요",
          "speaker_id": "2",
          "time": "20210827 13:53"
        },
        {
          "id": "MDRW2100047543.1.16",
          "form": "저 브리짓존스의 일기요! 아시나요?",
          "original_form": "저 브리짓존스의 일기요! 아시나요?",
          "speaker_id": "1",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100047543.1.17",
          "form": "아 네 볼때마다 대리수치느끼면서 이불꼭쥐고봐요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 네 볼때마다 대리수치느끼면서 이불꼭쥐고봐요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:43"
        },
        {
          "id": "MDRW2100047543.1.18",
          "form": "그게 매력이죠ㅋㅋㅋ 3까지 다 보셨나요?",
          "original_form": "그게 매력이죠ㅋㅋㅋ 3까지 다 보셨나요?",
          "speaker_id": "1",
          "time": "20210827 17:23"
        },
        {
          "id": "MDRW2100047543.1.19",
          "form": "네 티비틀면 자주나와서 다본것같아요",
          "original_form": "네 티비틀면 자주나와서 다본것같아요",
