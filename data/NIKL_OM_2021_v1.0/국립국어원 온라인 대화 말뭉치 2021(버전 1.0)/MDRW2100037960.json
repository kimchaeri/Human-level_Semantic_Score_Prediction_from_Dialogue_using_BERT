      "utterance": [
        {
          "id": "MDRW2100037960.1.1",
          "form": "중고거래도 하세요?",
          "original_form": "중고거래도 하세요?",
          "speaker_id": "2",
          "time": "20210901 21:43"
        },
        {
          "id": "MDRW2100037960.1.2",
          "form": "중고거래는 아직 안 해봤어요",
          "original_form": "중고거래는 아직 안 해봤어요",
          "speaker_id": "1",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100037960.1.3",
          "form": "중고로는 보통 어떤 거 구입하세요?",
          "original_form": "중고로는 보통 어떤 거 구입하세요?",
          "speaker_id": "1",
          "time": "20210901 22:48"
        },
        {
          "id": "MDRW2100037960.1.4",
          "form": "저는 사 본건 베개 사봤어요 ㅎ",
          "original_form": "저는 사 본건 베개 사봤어요 ㅎ",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100037960.1.5",
          "form": "대부분 팔았죠",
          "original_form": "대부분 팔았죠",
          "speaker_id": "2",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100037960.1.6",
          "form": "기능성 베개 같은 거였나봐요",
          "original_form": "기능성 베개 같은 거였나봐요",
          "speaker_id": "1",
          "time": "20210901 23:13"
        },
        {
          "id": "MDRW2100037960.1.7",
          "form": "팔 때는 진상고객?이 문제라고 들었어요",
          "original_form": "팔 때는 진상고객?이 문제라고 들었어요",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100037960.1.8",
          "form": "네 맞아요!  기능성이에요 좀 비싸서",
          "original_form": "네 맞아요!  기능성이에요 좀 비싸서",
          "speaker_id": "2",
          "time": "20210901 23:18"
        },
        {
          "id": "MDRW2100037960.1.9",
          "form": "중고로만 ㅠ 사요",
          "original_form": "중고로만 ㅠ 사요",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100037960.1.10",
          "form": "아직까진 다행히 진상은 없었어요",
          "original_form": "아직까진 다행히 진상은 없었어요",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100037960.1.11",
          "form": "막 깎아달라고 하거나 답장이 없거나 그런 경우도 있다고 하더라구요",
          "original_form": "막 깎아달라고 하거나 답장이 없거나 그런 경우도 있다고 하더라구요",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100037960.1.12",
          "form": "아!  저 한번 있었어요 인사만 하고 답이없던",
          "original_form": "아!  저 한번 있었어요 인사만 하고 답이없던",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100037960.1.13",
          "form": "뭐지..했어요",
          "original_form": "뭐지..했어요",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100037960.1.14",
          "form": "인사만 하고 사라지다니ㅋㅋ 그새 마음이 변한걸까요",
          "original_form": "인사만 하고 사라지다니ㅋㅋ 그새 마음이 변한걸까요",
          "speaker_id": "1",
          "time": "20210901 23:36"
        },
        {
          "id": "MDRW2100037960.1.15",
          "form": "아뇨 그러다가 며칠뒤에 또 물어봐요 그러고 답없는",
          "original_form": "아뇨 그러다가 며칠뒤에 또 물어봐요 그러고 답없는",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100037960.1.16",
          "form": "이상한...",
          "original_form": "이상한...",
          "speaker_id": "2",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100037960.1.17",
          "form": "아.. 진짜 이상하네요",
          "original_form": "아.. 진짜 이상하네요",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100037960.1.18",
          "form": "네 그래서 그사람껀 대답안하고 씹었던거같아요",
          "original_form": "네 그래서 그사람껀 대답안하고 씹었던거같아요",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100037960.1.19",
          "form": "나머진 쿨거래라 좋았어요",
          "original_form": "나머진 쿨거래라 좋았어요",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100037960.1.20",
          "form": "쿨거래가 깔끔하고 좋죠ㅋㅋ",
          "original_form": "쿨거래가 깔끔하고 좋죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:47"
        },
        {
          "id": "MDRW2100037960.1.21",
          "form": "네네 쿨거래가 좋아요 ㅎ",
          "original_form": "네네 쿨거래가 좋아요 ㅎ",
          "speaker_id": "2",
