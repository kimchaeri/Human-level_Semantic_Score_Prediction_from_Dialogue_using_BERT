      "utterance": [
        {
          "id": "MDRW2100002792.1.1",
          "form": "막국수 맛있었겠어요",
          "original_form": "막국수 맛있었겠어요",
          "speaker_id": "1",
          "time": "20210820 15:36"
        },
        {
          "id": "MDRW2100002792.1.2",
          "form": "네 ㅎ 맛있게 먹었어요 맛집이라고 하더라구요",
          "original_form": "네 ㅎ 맛있게 먹었어요 맛집이라고 하더라구요",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002792.1.3",
          "form": "오~맛집가셨구나",
          "original_form": "오~맛집가셨구나",
          "speaker_id": "1",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002792.1.4",
          "form": "이런 얘기하니까 배가 고프네요   ㅋ",
          "original_form": "이런 얘기하니까 배가 고프네요   ㅋ",
          "speaker_id": "2",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002792.1.5",
          "form": "메밀전도 드셧나요?",
          "original_form": "메밀전도 드셧나요?",
          "speaker_id": "1",
          "time": "20210820 15:39"
        },
        {
          "id": "MDRW2100002792.1.6",
          "form": "아 ㅎ 메밀왕만두 같이 먹었어요",
          "original_form": "아 ㅎ 메밀왕만두 같이 먹었어요",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002792.1.7",
          "form": "오~~맛집만의 메뉴인가봐요",
          "original_form": "오~~맛집만의 메뉴인가봐요",
          "speaker_id": "1",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002792.1.8",
          "form": "여름엔 메밀이 최고같아요 ㅎ",
          "original_form": "여름엔 메밀이 최고같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:40"
        },
        {
          "id": "MDRW2100002792.1.9",
          "form": "ㅋㅋ 맞아욘 너무 맛있죠 ㅎ",
          "original_form": "ㅋㅋ 맞아욘 너무 맛있죠 ㅎ",
          "speaker_id": "1",
          "time": "20210820 15:41"
        },
        {
          "id": "MDRW2100002792.1.10",
          "form": "저녁은 뭘 먹어야할지 모르겠어요",
          "original_form": "저녁은 뭘 먹어야할지 모르겠어요",
          "speaker_id": "2",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002792.1.11",
          "form": "저는 등갈비 먹으려고요",
          "original_form": "저는 등갈비 먹으려고요",
          "speaker_id": "1",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002792.1.12",
          "form": "꺄~ 맛있겠어요 등갈비",
          "original_form": "꺄~ 맛있겠어요 등갈비",
          "speaker_id": "2",
          "time": "20210820 15:43"
        },
        {
          "id": "MDRW2100002792.1.13",
          "form": "고기 좋아하세요?",
          "original_form": "고기 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 15:44"
        },
        {
          "id": "MDRW2100002792.1.14",
          "form": "조금 좋아해요 ㅋ 고기 자주 드세요~?",
          "original_form": "조금 좋아해요 ㅋ 고기 자주 드세요~?",
          "speaker_id": "2",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002792.1.15",
          "form": "아 네 아주 자주 ㅎㅎ",
          "original_form": "아 네 아주 자주 ㅎㅎ",
