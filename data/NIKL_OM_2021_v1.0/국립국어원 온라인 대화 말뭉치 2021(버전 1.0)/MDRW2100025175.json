          "speaker_id": "1",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100025175.1.20",
          "form": "전 한치물회?랑",
          "original_form": "전 한치물회?랑",
          "speaker_id": "2",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100025175.1.21",
          "form": "마라탕 먹었어요 ㅋㅋㅋ",
          "original_form": "마라탕 먹었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100025175.1.22",
          "form": "ㅋㅋㅋㅋㅋㄱㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100025175.1.23",
          "form": "한치물회맛있겠다",
          "original_form": "한치물회맛있겠다",
