        },
        {
          "id": "MDRW2100025175.1.2",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 00:10"
        },
        {
          "id": "MDRW2100025175.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100025175.1.4",
          "form": "네 근데 최근엔 코로나때문에 못가고있어요",
          "original_form": "네 근데 최근엔 코로나때문에 못가고있어요",
          "speaker_id": "2",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100025175.1.5",
          "form": "ㅠㅠ맞아요",
          "original_form": "ㅠㅠ맞아요",
          "speaker_id": "1",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100025175.1.6",
          "form": "마지막으로 간곳이 어디에요?",
          "original_form": "마지막으로 간곳이 어디에요?",
          "speaker_id": "1",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100025175.1.7",
          "form": "전 부산이었어요",
          "original_form": "전 부산이었어요",
          "speaker_id": "1",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100025175.1.8",
          "form": "재작년을끝으로ㅋㅋㅠ",
          "original_form": "재작년을끝으로ㅋㅋㅠ",
          "speaker_id": "1",
          "time": "20210901 00:11"
        },
        {
          "id": "MDRW2100025175.1.9",
          "form": "저도 부산이요 ㅋㅋㅋ",
          "original_form": "저도 부산이요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100025175.1.10",
          "form": "맛집 가셨나요?",
          "original_form": "맛집 가셨나요?",
          "speaker_id": "2",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100025175.1.11",
          "form": "네ㅋㅋㅋㅋㄱ",
          "original_form": "네ㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100025175.1.12",
          "form": "근데 역시 인터넷맛집이 좋은것같아요",
          "original_form": "근데 역시 인터넷맛집이 좋은것같아요",
          "speaker_id": "1",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100025175.1.13",
          "form": "부산사는 친구랑갓더니",
          "original_form": "부산사는 친구랑갓더니",
          "speaker_id": "1",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100025175.1.14",
          "form": "자꾸 국밥먹자고해서...",
          "original_form": "자꾸 국밥먹자고해서...",
          "speaker_id": "1",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100025175.1.15",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 00:12"
        },
        {
          "id": "MDRW2100025175.1.16",
          "form": "첫날 점심 저녁 둘다 돼지국밥먹었던기억이ㅜ",
          "original_form": "첫날 점심 저녁 둘다 돼지국밥먹었던기억이ㅜ",
          "speaker_id": "1",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100025175.1.17",
          "form": "현지인 맛집이네요",
          "original_form": "현지인 맛집이네요",
          "speaker_id": "2",
          "time": "20210901 00:13"
        },
        {
          "id": "MDRW2100025175.1.18",
          "form": "name2님이 뭐드셨나요",
          "original_form": "&name2&님이 뭐드셨나요",
