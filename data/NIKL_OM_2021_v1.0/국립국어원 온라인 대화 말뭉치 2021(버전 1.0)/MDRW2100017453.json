      "utterance": [
        {
          "id": "MDRW2100017453.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017453.1.2",
          "form": "일단은 라면을 생각중이거든",
          "original_form": "일단은 라면을 생각중이거든",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017453.1.3",
          "form": "사먹는거야 해먹는거야",
          "original_form": "사먹는거야 해먹는거야",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017453.1.4",
          "form": "사먹는거지",
          "original_form": "사먹는거지",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017453.1.5",
          "form": "내가 지금 먹고싶은거",
          "original_form": "내가 지금 먹고싶은거",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017453.1.6",
          "form": "골라도돼?",
          "original_form": "골라도돼?",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017453.1.7",
          "form": "라면이라고 해도 굉장히 종류가 많다고",
          "original_form": "라면이라고 해도 굉장히 종류가 많다고",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017453.1.8",
          "form": "일단 말해봐",
          "original_form": "일단 말해봐",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017453.1.9",
          "form": "라면이어야해?",
          "original_form": "라면이어야해?",
          "speaker_id": "2",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017453.1.10",
          "form": "수용이 될지는 모르겠네^^",
          "original_form": "수용이 될지는 모르겠네^^",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100017453.1.11",
          "form": "아니 라면 아니어도 돼",
          "original_form": "아니 라면 아니어도 돼",
          "speaker_id": "1",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017453.1.12",
          "form": "그냥 의견조사야",
          "original_form": "그냥 의견조사야",
          "speaker_id": "1",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017453.1.13",
          "form": "돈부리가 생각나는 저녁이에요",
          "original_form": "돈부리가 생각나는 저녁이에요",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017453.1.14",
          "form": "돈부리는 해먹을수가없네요",
          "original_form": "돈부리는 해먹을수가없네요",
          "speaker_id": "1",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017453.1.15",
          "form": "기각",
          "original_form": "기각",
          "speaker_id": "1",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017453.1.16",
          "form": "사먹는거라며!",
          "original_form": "사먹는거라며!",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017453.1.17",
          "form": "헐 사먹는거라고 대답했네???",
          "original_form": "헐 사먹는거라고 대답했네???",
          "speaker_id": "1",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017453.1.18",
          "form": "뭐야 왜 저랫지",
          "original_form": "뭐야 왜 저랫지",
          "speaker_id": "1",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100017453.1.19",
          "form": "해먹는거야 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "해먹는거야 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017453.1.20",
          "form": "잠깐 사먹다와 해먹다가 바뀌었어 머리속에서",
          "original_form": "잠깐 사먹다와 해먹다가 바뀌었어 머리속에서",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017453.1.21",
          "form": "해먹는걸루다가 하나만 더 말해봐",
          "original_form": "해먹는걸루다가 하나만 더 말해봐",
          "speaker_id": "1",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017453.1.22",
          "form": "해먹는거라..",
          "original_form": "해먹는거라..",
          "speaker_id": "2",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017453.1.23",
          "form": "파송송 계란탁 스팸 라면",
          "original_form": "파송송 계란탁 스팸 라면",
          "speaker_id": "2",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100017453.1.24",
          "form": "을 먹으면 맛있을거 같아요",
          "original_form": "을 먹으면 맛있을거 같아요",
          "speaker_id": "2",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100017453.1.25",
          "form": "파송송 계란탁 스팸라면은 너무",
          "original_form": "파송송 계란탁 스팸라면은 너무",
          "speaker_id": "1",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100017453.1.26",
          "form": "호화로운것같아",
          "original_form": "호화로운것같아",
          "speaker_id": "1",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100017453.1.27",
          "form": "왜냐면 난 라면은 라면 그대로 끓였을때 제일 맛있다파거든",
          "original_form": "왜냐면 난 라면은 라면 그대로 끓였을때 제일 맛있다파거든",
          "speaker_id": "1",
          "time": "20210826 19:34"
        }
      ]
    }
