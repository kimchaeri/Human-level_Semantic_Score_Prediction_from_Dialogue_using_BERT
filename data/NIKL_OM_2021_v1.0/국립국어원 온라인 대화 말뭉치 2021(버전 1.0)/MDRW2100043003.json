      "utterance": [
        {
          "id": "MDRW2100043003.1.1",
          "form": "좋아하는 음식 또 뭐있나요",
          "original_form": "좋아하는 음식 또 뭐있나요",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100043003.1.2",
          "form": "죽어도 좋아",
          "original_form": "죽어도 좋아",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100043003.1.3",
          "form": "민초.",
          "original_form": "민초.",
          "speaker_id": "1",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100043003.1.4",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100043003.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100043003.1.6",
          "form": "저 슈팅스타도 좋아하고",
          "original_form": "저 슈팅스타도 좋아하고",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100043003.1.7",
          "form": "하와이안 피자도 좋아해요",
          "original_form": "하와이안 피자도 좋아해요",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100043003.1.8",
          "form": "적폐입맛",
          "original_form": "적폐입맛",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100043003.1.9",
          "form": "아침이슬 좋아하고",
          "original_form": "아침이슬 좋아하고",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100043003.1.10",
          "form": "데자와도 먹어요",
          "original_form": "데자와도 먹어요",
          "speaker_id": "2",
          "time": "20210907 12:53"
        },
        {
          "id": "MDRW2100043003.1.11",
          "form": "저 그거다먹음",
          "original_form": "저 그거다먹음",
          "speaker_id": "1",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100043003.1.12",
          "form": "헉 님",
          "original_form": "헉 님",
          "speaker_id": "2",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100043003.1.13",
          "form": "ㅇㄴ 푸드메이트 만났다",
          "original_form": "ㅇㄴ 푸드메이트 만났다",
          "speaker_id": "2",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100043003.1.14",
          "form": "어쩔수없다..우리는 소울메이트가 맞음..",
          "original_form": "어쩔수없다..우리는 소울메이트가 맞음..",
          "speaker_id": "1",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100043003.1.15",
          "form": "이거이거 같은 배에서 태어난 혀가 아닐지요",
          "original_form": "이거이거 같은 배에서 태어난 혀가 아닐지요",
          "speaker_id": "2",
          "time": "20210907 12:54"
        },
        {
          "id": "MDRW2100043003.1.16",
          "form": "아웃기다",
          "original_form": "아웃기다",
          "speaker_id": "1",
          "time": "20210907 12:55"
        },
        {
          "id": "MDRW2100043003.1.17",
          "form": "혀만생각하기",
          "original_form": "혀만생각하기",
          "speaker_id": "1",
          "time": "20210907 12:55"
        },
        {
          "id": "MDRW2100043003.1.18",
          "form": "하와이안피자 너무 먹고싶어요",
          "original_form": "하와이안피자 너무 먹고싶어요",
          "speaker_id": "2",
          "time": "20210907 12:55"
        },
        {
          "id": "MDRW2100043003.1.19",
          "form": "공차 민트초코뭐시기",
          "original_form": "공차 민트초코뭐시기",
          "speaker_id": "1",
          "time": "20210907 12:55"
        },
        {
          "id": "MDRW2100043003.1.20",
          "form": "드셔봣나요",
          "original_form": "드셔봣나요",
          "speaker_id": "1",
          "time": "20210907 12:55"
        },
        {
