      "utterance": [
        {
          "id": "MDRW2100018682.1.1",
          "form": "출근하셧나요?",
          "original_form": "출근하셧나요?",
          "speaker_id": "1",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100018682.1.2",
          "form": "직장 스트레스 중 제일은 뭐라고 생각하세요?",
          "original_form": "직장 스트레스 중 제일은 뭐라고 생각하세요?",
          "speaker_id": "2",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100018682.1.3",
          "form": "저는 취준생입니다",
          "original_form": "저는 취준생입니다",
          "speaker_id": "2",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100018682.1.4",
          "form": "저는 근무환경이 좋은편이라서 스트레스 없어요 ㅎㅎ",
          "original_form": "저는 근무환경이 좋은편이라서 스트레스 없어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 10:40"
        },
        {
          "id": "MDRW2100018682.1.5",
          "form": "성격도 스트레스잘안받기도해요",
          "original_form": "성격도 스트레스잘안받기도해요",
          "speaker_id": "1",
          "time": "20210827 10:40"
        },
        {
          "id": "MDRW2100018682.1.6",
          "form": "너무 부러워요",
          "original_form": "너무 부러워요",
          "speaker_id": "2",
          "time": "20210827 10:41"
        },
        {
          "id": "MDRW2100018682.1.7",
          "form": "제일 좋은 성격이신걸요!",
          "original_form": "제일 좋은 성격이신걸요!",
          "speaker_id": "2",
          "time": "20210827 10:41"
        },
        {
          "id": "MDRW2100018682.1.8",
          "form": "ㅋㅋ그냥 일한지 오래되서 기계처럼 영혼없이 해요",
          "original_form": "ㅋㅋ그냥 일한지 오래되서 기계처럼 영혼없이 해요",
          "speaker_id": "1",
          "time": "20210827 10:42"
        },
        {
          "id": "MDRW2100018682.1.9",
          "form": "대단하십니다! 꾸준히가 힘들어요ㅜ",
          "original_form": "대단하십니다! 꾸준히가 힘들어요ㅜ",
          "speaker_id": "2",
          "time": "20210827 10:42"
        },
        {
          "id": "MDRW2100018682.1.10",
          "form": "요즘 일자리도 자꾸 줄고 취업시장 넘 추운거같아요",
          "original_form": "요즘 일자리도 자꾸 줄고 취업시장 넘 추운거같아요",
          "speaker_id": "1",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100018682.1.11",
          "form": "힘내세욧!!!!",
          "original_form": "힘내세욧!!!!",
          "speaker_id": "1",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100018682.1.12",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100018682.1.13",
          "form": "감사합니다!!",
          "original_form": "감사합니다!!",
          "speaker_id": "2",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100018682.1.14",
          "form": "일하시는 중이신가요?",
          "original_form": "일하시는 중이신가요?",
          "speaker_id": "2",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100018682.1.15",
          "form": "네 월급루팡이에요 ㅎㅎ",
          "original_form": "네 월급루팡이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 10:44"
        },
        {
          "id": "MDRW2100018682.1.16",
          "form": "오늘 좀 여유롭네여",
          "original_form": "오늘 좀 여유롭네여",
          "speaker_id": "1",
          "time": "20210827 10:44"
        },
        {
          "id": "MDRW2100018682.1.17",
          "form": "크으 금요일에 루팡 최고죠!",
          "original_form": "크으 금요일에 루팡 최고죠!",
          "speaker_id": "2",
          "time": "20210827 10:45"
        },
        {
          "id": "MDRW2100018682.1.18",
          "form": "계속 여유롭기를..! 응원합니닷",
          "original_form": "계속 여유롭기를..! 응원합니닷",
          "speaker_id": "2",
          "time": "20210827 10:45"
        },
        {
          "id": "MDRW2100018682.1.19",
          "form": "취준시절 생각하면 많이 조급하고 스트레스 받앗던거같아요ㅜ",
          "original_form": "취준시절 생각하면 많이 조급하고 스트레스 받앗던거같아요ㅜ",
          "speaker_id": "1",
          "time": "20210827 10:47"
        },
        {
          "id": "MDRW2100018682.1.20",
          "form": "아ㅠ 다 그때는 어쩔 수 없는가보네요ㅠ",
          "original_form": "아ㅠ 다 그때는 어쩔 수 없는가보네요ㅠ",
          "speaker_id": "2",
          "time": "20210827 10:47"
        },
        {
          "id": "MDRW2100018682.1.21",
          "form": "그럼요ㅜㅜ",
          "original_form": "그럼요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 10:47"
        },
        {
          "id": "MDRW2100018682.1.22",
          "form": "돈은없고 사고싶은건많고 놀고싶고ㅜㅜ",
          "original_form": "돈은없고 사고싶은건많고 놀고싶고ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 10:48"
        },
        {
          "id": "MDRW2100018682.1.23",
          "form": "그랫었어요",
          "original_form": "그랫었어요",
          "speaker_id": "1",
          "time": "20210827 10:48"
        },
        {
          "id": "MDRW2100018682.1.24",
          "form": "아ㅠㅠ 딱 저네요ㅠ",
          "original_form": "아ㅠㅠ 딱 저네요ㅠ",
          "speaker_id": "2",
          "time": "20210827 10:48"
        },
        {
          "id": "MDRW2100018682.1.25",
          "form": "빨리 탈출해야겠어요",
          "original_form": "빨리 탈출해야겠어요",
          "speaker_id": "2",
          "time": "20210827 10:48"
        },
        {
          "id": "MDRW2100018682.1.26",
          "form": "name1처럼 무던하게 꾸준히 일하는게 제 목표에요",
          "original_form": "&name1&처럼 무던하게 꾸준히 일하는게 제 목표에요",
          "speaker_id": "2",
          "time": "20210827 10:49"
        },
        {
          "id": "MDRW2100018682.1.27",
          "form": "저는 그래서 최대한 바로 빨리 일할수잇는곳 선택하고",
          "original_form": "저는 그래서 최대한 바로 빨리 일할수잇는곳 선택하고",
          "speaker_id": "1",
          "time": "20210827 10:49"
        },
        {
          "id": "MDRW2100018682.1.28",
