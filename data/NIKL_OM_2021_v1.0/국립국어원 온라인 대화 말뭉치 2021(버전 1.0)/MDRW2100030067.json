      "utterance": [
        {
          "id": "MDRW2100030067.1.1",
          "form": "폴더블폰 쓰세요?",
          "original_form": "폴더블폰 쓰세요?",
          "speaker_id": "1",
          "time": "20210903 20:34"
        },
        {
          "id": "MDRW2100030067.1.2",
          "form": "폴드 같은건가요?",
          "original_form": "폴드 같은건가요?",
          "speaker_id": "2",
          "time": "20210903 20:43"
        },
        {
          "id": "MDRW2100030067.1.3",
          "form": "그런거 맞을거예요",
          "original_form": "그런거 맞을거예요",
          "speaker_id": "1",
          "time": "20210903 20:45"
        },
        {
          "id": "MDRW2100030067.1.4",
          "form": "플립3 사고싶더라구요",
          "original_form": "플립3 사고싶더라구요",
          "speaker_id": "2",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100030067.1.5",
          "form": "다들 예쁘다고 하더라고요",
          "original_form": "다들 예쁘다고 하더라고요",
          "speaker_id": "1",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100030067.1.6",
          "form": "전 플립 쓰거든요",
          "original_form": "전 플립 쓰거든요",
          "speaker_id": "1",
          "time": "20210903 20:48"
        },
        {
          "id": "MDRW2100030067.1.7",
          "form": "오 몇탄이에요",
          "original_form": "오 몇탄이에요",
          "speaker_id": "2",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100030067.1.8",
          "form": "처음꺼요",
          "original_form": "처음꺼요",
          "speaker_id": "1",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100030067.1.9",
          "form": "한번도 액정이 안나가셨나요?",
          "original_form": "한번도 액정이 안나가셨나요?",
          "speaker_id": "2",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100030067.1.10",
          "form": "한번 찍힘이 있어서 갈았어요",
          "original_form": "한번 찍힘이 있어서 갈았어요",
          "speaker_id": "1",
          "time": "20210903 20:51"
        },
        {
          "id": "MDRW2100030067.1.11",
          "form": "아직 약하대요 ㅡ.ㅡ",
          "original_form": "아직 약하대요 ㅡ.ㅡ",
          "speaker_id": "1",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100030067.1.12",
          "form": "너무비싸던데 공짜로요?",
          "original_form": "너무비싸던데 공짜로요?",
          "speaker_id": "2",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100030067.1.13",
          "form": "공짜로 갈아주긴 했어요",
          "original_form": "공짜로 갈아주긴 했어요",
          "speaker_id": "1",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100030067.1.14",
          "form": "무슨 사십얼마라고 써놧던데",
          "original_form": "무슨 사십얼마라고 써놧던데",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100030067.1.15",
          "form": "갈아주며 기사분이 아직 약하다고 먼지에도 찍히고 머리카락에도 찍히고 그렇대요",
