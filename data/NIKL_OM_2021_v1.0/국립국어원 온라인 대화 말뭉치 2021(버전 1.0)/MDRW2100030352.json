      "utterance": [
        {
          "id": "MDRW2100030352.1.1",
          "form": "name2님은 좋아하는 음식 있으세요",
          "original_form": "&name2&님은 좋아하는 음식 있으세요",
          "speaker_id": "1",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030352.1.2",
          "form": "최근에 먹은 육개장 좋아해요!",
          "original_form": "최근에 먹은 육개장 좋아해요!",
          "speaker_id": "2",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100030352.1.3",
          "form": "오 육개장 맛있죠!!!",
          "original_form": "오 육개장 맛있죠!!!",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030352.1.4",
          "form": "파 많이 들어가면 맛나요",
          "original_form": "파 많이 들어가면 맛나요",
          "speaker_id": "1",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100030352.1.5",
          "form": "고추기름!!",
          "original_form": "고추기름!!",
          "speaker_id": "2",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030352.1.6",
          "form": "이 많이들어가야 맛나요!!",
          "original_form": "이 많이들어가야 맛나요!!",
          "speaker_id": "2",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030352.1.7",
          "form": "맞아요 국물 위에 둥둥 뜨잖아요 ㅋㅋ",
          "original_form": "맞아요 국물 위에 둥둥 뜨잖아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100030352.1.8",
          "form": "저는 요새 감자탕이 맛있더라고요",
          "original_form": "저는 요새 감자탕이 맛있더라고요",
          "speaker_id": "1",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030352.1.9",
          "form": "크 또 먹고싶네요",
          "original_form": "크 또 먹고싶네요",
          "speaker_id": "2",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030352.1.10",
          "form": "name1님은 뭐 좋아하세요?",
          "original_form": "&name1&님은 뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 00:05"
        },
        {
          "id": "MDRW2100030352.1.11",
          "form": "감자탕 순대국 이런거 좋아해요 ㅋㅋ",
          "original_form": "감자탕 순대국 이런거 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030352.1.12",
          "form": "오홍 거기에 소주? ㅎㅎ",
          "original_form": "오홍 거기에 소주? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030352.1.13",
          "form": "딱 안주인데요?",
          "original_form": "딱 안주인데요?",
          "speaker_id": "2",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100030352.1.14",
          "form": "아 소주는 많이 못 먹어서 그냥 식사로만요 ㅋㅋㅋ",
          "original_form": "아 소주는 많이 못 먹어서 그냥 식사로만요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100030352.1.15",
          "form": "반주 자주 하시나요? ㅋㅋ",
          "original_form": "반주 자주 하시나요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:07"
        },
        {
          "id": "MDRW2100030352.1.16",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100030352.1.17",
          "form": "하지만 같이먹을 사람은 없네요",
          "original_form": "하지만 같이먹을 사람은 없네요",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100030352.1.18",
          "form": "아항 ㅎㅎ 저는 맥주 가끔 생각날때 한잔 하는 정도네요 ㅎㅎ",
          "original_form": "아항 ㅎㅎ 저는 맥주 가끔 생각날때 한잔 하는 정도네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100030352.1.19",
          "form": "좋죠 맥주도 ㅎㅎ",
          "original_form": "좋죠 맥주도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100030352.1.20",
          "form": "배달음식도 시켜드시나요?",
          "original_form": "배달음식도 시켜드시나요?",
          "speaker_id": "1",
          "time": "20210904 01:10"
        },
        {
          "id": "MDRW2100030352.1.21",
          "form": "가끔 치킨정도요?",
          "original_form": "가끔 치킨정도요?",
          "speaker_id": "2",
          "time": "20210904 01:11"
        },
        {
          "id": "MDRW2100030352.1.22",
          "form": "외엔 ...보쌈 족발 정도?",
          "original_form": "외엔 ...보쌈 족발 정도?",
          "speaker_id": "2",
          "time": "20210904 01:11"
        },
        {
          "id": "MDRW2100030352.1.23",
          "form": "아하 저랑 비슷하시네요 ㅋㅋ 저도 치킨을 젤 많이 시키는거 같아요",
          "original_form": "아하 저랑 비슷하시네요 ㅋㅋ 저도 치킨을 젤 많이 시키는거 같아요",
          "speaker_id": "1",
          "time": "20210904 01:14"
        }
      ]
    }
