      "utterance": [
        {
          "id": "MDRW2100035143.1.1",
          "form": "심즈말고는 하는게임 없어요?",
          "original_form": "심즈말고는 하는게임 없어요?",
          "speaker_id": "1",
          "time": "20210829 06:56"
        },
        {
          "id": "MDRW2100035143.1.2",
          "form": "네네ㅠㅠ 요즘 게임을 잘 안하네용...ㅠㅠ",
          "original_form": "네네ㅠㅠ 요즘 게임을 잘 안하네용...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 06:56"
        },
        {
          "id": "MDRW2100035143.1.3",
          "form": "롤 오버워치 이런거 해보셨어요?",
          "original_form": "롤 오버워치 이런거 해보셨어요?",
          "speaker_id": "1",
          "time": "20210829 06:57"
        },
        {
          "id": "MDRW2100035143.1.4",
          "form": "아 이거 하려고 시도는 해봤는데",
          "original_form": "아 이거 하려고 시도는 해봤는데",
          "speaker_id": "2",
          "time": "20210829 06:57"
        },
        {
          "id": "MDRW2100035143.1.5",
          "form": "게임 방식이 따로 공부해야힐정도로",
          "original_form": "게임 방식이 따로 공부해야힐정도로",
          "speaker_id": "2",
          "time": "20210829 06:57"
        },
        {
          "id": "MDRW2100035143.1.6",
          "form": "좀 어렵더라구요ㅋㅋㅋㅋ",
          "original_form": "좀 어렵더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 06:57"
        },
        {
          "id": "MDRW2100035143.1.7",
          "form": "그래서 핸드촌으로 하는 게임중에",
          "original_form": "그래서 핸드촌으로 하는 게임중에",
          "speaker_id": "2",
          "time": "20210829 06:57"
        },
        {
          "id": "MDRW2100035143.1.8",
          "form": "방탈출 이런거해용",
          "original_form": "방탈출 이런거해용",
          "speaker_id": "2",
          "time": "20210829 06:58"
        },
        {
          "id": "MDRW2100035143.1.9",
          "form": "맞아요!!! 그리고 이제와서 시작하려니",
          "original_form": "맞아요!!! 그리고 이제와서 시작하려니",
          "speaker_id": "1",
          "time": "20210829 06:58"
        },
        {
          "id": "MDRW2100035143.1.10",
          "form": "사람들 레벨이 넘사고ㅋㅋㅋㅋㅋ",
          "original_form": "사람들 레벨이 넘사고ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 06:58"
        },
        {
          "id": "MDRW2100035143.1.11",
          "form": "초보는 껴주지도 않고....",
          "original_form": "초보는 껴주지도 않고....",
          "speaker_id": "1",
          "time": "20210829 06:58"
        },
        {
          "id": "MDRW2100035143.1.12",
          "form": "네네ㅋㅋㅋ 진짜 플레이보면",
          "original_form": "네네ㅋㅋㅋ 진짜 플레이보면",
          "speaker_id": "2",
          "time": "20210829 06:59"
        },
        {
          "id": "MDRW2100035143.1.13",
          "form": "눈이 화려해요ㅋㅋㅋ",
          "original_form": "눈이 화려해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 06:59"
        },
        {
          "id": "MDRW2100035143.1.14",
          "form": "순식간에 파바바박",
          "original_form": "순식간에 파바바박",
          "speaker_id": "1",
          "time": "20210829 06:59"
        },
        {
          "id": "MDRW2100035143.1.15",
          "form": "그거 대신 총게임 뭐였죵",
          "original_form": "그거 대신 총게임 뭐였죵",
          "speaker_id": "2",
          "time": "20210829 06:59"
        },
        {
          "id": "MDRW2100035143.1.16",
          "form": "엄청 유명한",
          "original_form": "엄청 유명한",
          "speaker_id": "2",
          "time": "20210829 06:59"
        },
        {
          "id": "MDRW2100035143.1.17",
          "form": "그래픽이 화려해서 그런가 좀 멀미나더라고요...",
          "original_form": "그래픽이 화려해서 그런가 좀 멀미나더라고요...",
          "speaker_id": "1",
          "time": "20210829 06:59"
        },
        {
          "id": "MDRW2100035143.1.18",
          "form": "서든어택?!",
          "original_form": "서든어택?!",
          "speaker_id": "1",
          "time": "20210829 06:59"
        },
        {
          "id": "MDRW2100035143.1.19",
          "form": "아 맞아요ㅋㅋㅋㅋ",
          "original_form": "아 맞아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 07:00"
        },
        {
          "id": "MDRW2100035143.1.20",
          "form": "서든어택 다음으로 나온거였는데",
          "original_form": "서든어택 다음으로 나온거였는데",
          "speaker_id": "2",
          "time": "20210829 07:00"
        },
        {
          "id": "MDRW2100035143.1.21",
          "form": "그것도 잠깐 하다가 접었어욬ㅋㅋ",
          "original_form": "그것도 잠깐 하다가 접었어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 07:00"
        },
        {
          "id": "MDRW2100035143.1.22",
          "form": "중학생땐가 pc방에서 서든어택 한번했는데",
          "original_form": "중학생땐가 pc방에서 서든어택 한번했는데",
          "speaker_id": "1",
          "time": "20210829 07:00"
        },
        {
          "id": "MDRW2100035143.1.23",
          "form": "그때는 미성년자라 피 대신 우유같은겈ㅋㅋㅋㅋ",
          "original_form": "그때는 미성년자라 피 대신 우유같은겈ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 07:01"
        },
        {
          "id": "MDRW2100035143.1.24",
          "form": "나오고 그랬는데 멀미나서 그후로 한번도 안했어요",
          "original_form": "나오고 그랬는데 멀미나서 그후로 한번도 안했어요",
          "speaker_id": "1",
          "time": "20210829 07:01"
        },
        {
          "id": "MDRW2100035143.1.25",
          "form": "아아ㅋㅋㅋ 전 자꾸 뒤에서 누가",
          "original_form": "아아ㅋㅋㅋ 전 자꾸 뒤에서 누가",
          "speaker_id": "2",
          "time": "20210829 07:01"
        },
        {
          "id": "MDRW2100035143.1.26",
          "form": "쏴서 ㅠㅠ",
          "original_form": "쏴서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 07:01"
        },
        {
          "id": "MDRW2100035143.1.27",
          "form": "게임을 잘 못한다는걸 깨달았어요",
          "original_form": "게임을 잘 못한다는걸 깨달았어요",
          "speaker_id": "2",
          "time": "20210829 07:01"
        },
        {
          "id": "MDRW2100035143.1.28",
          "form": "방탈출게임은 재밌어요?",
          "original_form": "방탈출게임은 재밌어요?",
          "speaker_id": "1",
          "time": "20210829 07:03"
        },
        {
          "id": "MDRW2100035143.1.29",
          "form": "한번도 안 해봣어요",
          "original_form": "한번도 안 해봣어요",
          "speaker_id": "1",
          "time": "20210829 07:04"
        },
        {
          "id": "MDRW2100035143.1.30",
          "form": "네네! 이건 혼자하는 게임이고 시간제한도 없어서",
          "original_form": "네네! 이건 혼자하는 게임이고 시간제한도 없어서",
          "speaker_id": "2",
          "time": "20210829 07:04"
        },
        {
          "id": "MDRW2100035143.1.31",
          "form": "잘 맞는것같아용ㅎㅎ",
          "original_form": "잘 맞는것같아용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 07:04"
        },
        {
          "id": "MDRW2100035143.1.32",
          "form": "제가 지금 아침을 준비하러 가야해서ㅠㅠ",
          "original_form": "제가 지금 아침을 준비하러 가야해서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 07:04"
        },
        {
          "id": "MDRW2100035143.1.33",
          "form": "죄송해용ㅠㅠ",
          "original_form": "죄송해용ㅠㅠ",
          "speaker_id": "2",
