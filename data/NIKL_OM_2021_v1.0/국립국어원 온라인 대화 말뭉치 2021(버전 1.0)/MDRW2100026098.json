      "utterance": [
        {
          "id": "MDRW2100026098.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026098.1.2",
          "form": "책 좋아하세요",
          "original_form": "책 좋아하세요",
          "speaker_id": "1",
          "time": "20210901 19:42"
        },
        {
          "id": "MDRW2100026098.1.3",
          "form": "안녕하세요! 책 자주 안읽어요... ㅋㅋㅋㅋ",
          "original_form": "안녕하세요! 책 자주 안읽어요... ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:53"
        },
        {
          "id": "MDRW2100026098.1.4",
          "form": "책 좋아하세요?",
          "original_form": "책 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 19:53"
        },
        {
          "id": "MDRW2100026098.1.5",
          "form": "좋아하는데 요즘 통 못 읽고 있어요",
          "original_form": "좋아하는데 요즘 통 못 읽고 있어요",
          "speaker_id": "1",
          "time": "20210901 19:56"
        },
        {
          "id": "MDRW2100026098.1.6",
          "form": "어느 장르 좋아하세요?",
          "original_form": "어느 장르 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100026098.1.7",
          "form": "저 가리지않고 다요",
          "original_form": "저 가리지않고 다요",
          "speaker_id": "1",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100026098.1.8",
          "form": "추리소설도 좋고",
          "original_form": "추리소설도 좋고",
          "speaker_id": "1",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100026098.1.9",
          "form": "그냥 역사서나 사회과학 책도",
          "original_form": "그냥 역사서나 사회과학 책도",
          "speaker_id": "1",
          "time": "20210901 20:12"
        },
        {
          "id": "MDRW2100026098.1.10",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "1",
          "time": "20210901 20:13"
        },
        {
          "id": "MDRW2100026098.1.11",
          "form": "책을 좋아하시나봐요!",
          "original_form": "책을 좋아하시나봐요!",
          "speaker_id": "2",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100026098.1.12",
          "form": "매번 구매해서 독서하시나요?",
          "original_form": "매번 구매해서 독서하시나요?",
          "speaker_id": "2",
          "time": "20210901 20:31"
        },
        {
          "id": "MDRW2100026098.1.13",
          "form": "아니요 이북대여해서 읽기도 해요",
          "original_form": "아니요 이북대여해서 읽기도 해요",
          "speaker_id": "1",
          "time": "20210901 20:32"
        },
        {
          "id": "MDRW2100026098.1.14",
          "form": "그렇군요 최근에 무슨책 읽으셧어요?",
          "original_form": "그렇군요 최근에 무슨책 읽으셧어요?",
          "speaker_id": "2",
          "time": "20210901 21:35"
        },
        {
          "id": "MDRW2100026098.1.15",
          "form": "저 백번째여왕",
          "original_form": "저 백번째여왕",
          "speaker_id": "1",
          "time": "20210902 09:51"
        },
        {
          "id": "MDRW2100026098.1.16",
          "form": "이라는 판타지소설이요",
          "original_form": "이라는 판타지소설이요",
          "speaker_id": "1",
          "time": "20210902 09:51"
        },
        {
          "id": "MDRW2100026098.1.17",
          "form": "오 그렇군여",
          "original_form": "오 그렇군여",
          "speaker_id": "2",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100026098.1.18",
          "form": "판타지소설중에 젤 재밌었던 책이 몬가요?",
          "original_form": "판타지소설중에 젤 재밌었던 책이 몬가요?",
          "speaker_id": "2",
          "time": "20210902 14:00"
        },
        {
          "id": "MDRW2100026098.1.19",
          "form": "어슐라 르 권 바람의 일곱방향이요",
          "original_form": "어슐라 르 권 바람의 일곱방향이요",
          "speaker_id": "1",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100026098.1.20",
          "form": "오멜라스를 떠나는 사람도",
          "original_form": "오멜라스를 떠나는 사람도",
          "speaker_id": "1",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100026098.1.21",
          "form": "좋았어요",
          "original_form": "좋았어요",
          "speaker_id": "1",
          "time": "20210902 14:42"
        },
        {
          "id": "MDRW2100026098.1.22",
          "form": "오 그렇군요 저도 한번 찾아봐볼게요",
          "original_form": "오 그렇군요 저도 한번 찾아봐볼게요",
          "speaker_id": "2",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100026098.1.23",
          "form": "일반 소설 중에서는 뭐가 재밋엇나요?",
          "original_form": "일반 소설 중에서는 뭐가 재밋엇나요?",
          "speaker_id": "2",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100026098.1.24",
          "form": "한국 작가는 김애란 좋아해요",
          "original_form": "한국 작가는 김애란 좋아해요",
          "speaker_id": "1",
          "time": "20210902 18:50"
        }
      ]
