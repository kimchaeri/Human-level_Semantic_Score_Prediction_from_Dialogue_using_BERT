        {
          "id": "MDRW2100041454.1.1",
          "form": "나 오늘 소개팅 했어",
          "original_form": "나 오늘 소개팅 했어",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100041454.1.2",
          "form": "어땠어",
          "original_form": "어땠어",
          "speaker_id": "2",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100041454.1.3",
          "form": "너무 오랜만에 해서 낯설었어",
          "original_form": "너무 오랜만에 해서 낯설었어",
          "speaker_id": "1",
          "time": "20210906 22:59"
        },
        {
          "id": "MDRW2100041454.1.4",
          "form": "그러게 너 소개팅 별로 안 좋아하잖아",
          "original_form": "그러게 너 소개팅 별로 안 좋아하잖아",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100041454.1.5",
          "form": "그래도 요즘 외로웠단 말이야",
          "original_form": "그래도 요즘 외로웠단 말이야",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100041454.1.6",
          "form": "상대방은 어땠어",
          "original_form": "상대방은 어땠어",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100041454.1.7",
          "form": "일단 분위기가 좋아서",
          "original_form": "일단 분위기가 좋아서",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100041454.1.8",
          "form": "에 기분이 들떴겠네",
          "original_form": "에 기분이 들떴겠네",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100041454.1.9",
          "form": "남자 분도 마음에 들었어",
          "original_form": "남자 분도 마음에 들었어",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100041454.1.10",
          "form": "우와 좋은 소식이네",
          "original_form": "우와 좋은 소식이네",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100041454.1.11",
          "form": "한번 더 만나 볼 거 같아",
          "original_form": "한번 더 만나 볼 거 같아",
          "speaker_id": "1",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100041454.1.12",
          "form": "맞아 한번 더 만나보고 결정해봐",
          "original_form": "맞아 한번 더 만나보고 결정해봐",
          "speaker_id": "2",
          "time": "20210906 23:00"
        },
        {
          "id": "MDRW2100041454.1.13",
          "form": "잘 됐으면 좋겠다",
          "original_form": "잘 됐으면 좋겠다",
          "speaker_id": "1",
          "time": "20210906 23:01"
        },
        {
          "id": "MDRW2100041454.1.14",
          "form": "괜찮을 거 같아",
          "original_form": "괜찮을 거 같아",
          "speaker_id": "2",
          "time": "20210906 23:01"
        }
      ]
