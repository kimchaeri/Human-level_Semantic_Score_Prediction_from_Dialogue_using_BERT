      "utterance": [
        {
          "id": "MDRW2100004876.1.1",
          "form": "유산소만 하루에 1~2시간 정도 하고 있어요",
          "original_form": "유산소만 하루에 1~2시간 정도 하고 있어요",
          "speaker_id": "2",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004876.1.2",
          "form": "name1 님은요?",
          "original_form": "&name1& 님은요?",
          "speaker_id": "2",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004876.1.3",
          "form": "홈트요",
          "original_form": "홈트요",
          "speaker_id": "1",
          "time": "20210821 19:16"
        },
        {
          "id": "MDRW2100004876.1.4",
          "form": "운동 말고 건강을 위해서 챙기는 거 있으세요?",
          "original_form": "운동 말고 건강을 위해서 챙기는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004876.1.5",
          "form": "저는 비타민이랑 유산균 챙겨 먹어요",
          "original_form": "저는 비타민이랑 유산균 챙겨 먹어요",
          "speaker_id": "2",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004876.1.6",
          "form": "저는 락토핏",
          "original_form": "저는 락토핏",
          "speaker_id": "1",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004876.1.7",
          "form": "유산균이죠? 유산균도 챙겨 먹어야 겠더라고요",
          "original_form": "유산균이죠? 유산균도 챙겨 먹어야 겠더라고요",
          "speaker_id": "2",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004876.1.8",
          "form": "금주나 금연은 하시나요?",
          "original_form": "금주나 금연은 하시나요?",
          "speaker_id": "2",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004876.1.9",
          "form": "둘다 안해요",
          "original_form": "둘다 안해요",
          "speaker_id": "1",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004876.1.10",
          "form": "안 하시는 거면 술도 마시고 담배도 피시는 건가요?",
          "original_form": "안 하시는 거면 술도 마시고 담배도 피시는 건가요?",
          "speaker_id": "2",
          "time": "20210821 19:29"
        },
        {
          "id": "MDRW2100004876.1.11",
          "form": "아 반대네요",
          "original_form": "아 반대네요",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100004876.1.12",
          "form": "아 이해했어요. 안 하는게 제일 좋죠.",
          "original_form": "아 이해했어요. 안 하는게 제일 좋죠.",
          "speaker_id": "2",
          "time": "20210821 21:18"
        },
        {
          "id": "MDRW2100004876.1.13",
          "form": "술이랑 담배는 처음부터 안 하셨나요?",
          "original_form": "술이랑 담배는 처음부터 안 하셨나요?",
          "speaker_id": "2",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100004876.1.14",
          "form": "담배는 첨부터 술은 코로나 이후로요",
          "original_form": "담배는 첨부터 술은 코로나 이후로요",
          "speaker_id": "1",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100004876.1.15",
          "form": "코로나 이후로 자연스럽게 약속에 안 나가니까 술은 안 마시게 되는 거 같아요",
          "original_form": "코로나 이후로 자연스럽게 약속에 안 나가니까 술은 안 마시게 되는 거 같아요",
          "speaker_id": "2",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100004876.1.16",
          "form": "네 줄더라구요",
          "original_form": "네 줄더라구요",
          "speaker_id": "1",
          "time": "20210821 21:23"
        },
        {
          "id": "MDRW2100004876.1.17",
          "form": "건강에는 좋은데 그래도 가끔 친구들 못 만나는 건 아쉬워요",
