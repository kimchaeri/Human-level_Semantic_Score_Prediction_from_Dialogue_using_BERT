      "utterance": [
        {
          "id": "MDRW2100043353.1.1",
          "form": "오늘 저녁 뭐먹을거얌",
          "original_form": "오늘 저녁 뭐먹을거얌",
          "speaker_id": "1",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100043353.1.2",
          "form": "치킨~",
          "original_form": "치킨~",
          "speaker_id": "2",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100043353.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100043353.1.4",
          "form": "난 고민중이여",
          "original_form": "난 고민중이여",
          "speaker_id": "1",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100043353.1.5",
          "form": "잔치국수 해먹늘랬믄데 너누 힘들다",
          "original_form": "잔치국수 해먹늘랬믄데 너누 힘들다",
          "speaker_id": "1",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100043353.1.6",
          "form": "잔치국수 손 엄청 가잖아",
          "original_form": "잔치국수 손 엄청 가잖아",
          "speaker_id": "2",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100043353.1.7",
          "form": "난 편한게 좋아 배달음식 짱",
          "original_form": "난 편한게 좋아 배달음식 짱",
          "speaker_id": "2",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100043353.1.8",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210907 18:14"
        },
        {
          "id": "MDRW2100043353.1.9",
          "form": "배달도 근데 아제 질려ㅠㅠ",
          "original_form": "배달도 근데 아제 질려ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 18:14"
        },
        {
          "id": "MDRW2100043353.1.10",
          "form": "그럴 수도 있지만...",
          "original_form": "그럴 수도 있지만...",
          "speaker_id": "2",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100043353.1.11",
          "form": "혹시 요리 잘해?",
          "original_form": "혹시 요리 잘해?",
          "speaker_id": "2",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100043353.1.12",
          "form": "난 요리 못해서 배달음식 질려두 계속 먹게되더라구",
          "original_form": "난 요리 못해서 배달음식 질려두 계속 먹게되더라구",
          "speaker_id": "2",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100043353.1.13",
          "form": "응응 요리 조아햐",
          "original_form": "응응 요리 조아햐",
          "speaker_id": "1",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100043353.1.14",
          "form": "카레 잔치국수 미역굳 김치찌개 정도 ㅋㅋ",
          "original_form": "카레 잔치국수 미역굳 김치찌개 정도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:25"
        },
        {
          "id": "MDRW2100043353.1.15",
          "form": "와 나는 냉동음식 뎁히는게 최대야 ㅋㅋㅋㅋ",
          "original_form": "와 나는 냉동음식 뎁히는게 최대야 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043353.1.16",
          "form": "아 정말 ㅋㅋㅋ",
          "original_form": "아 정말 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043353.1.17",
          "form": "하다보면 늘어 ㅋㅋ",
          "original_form": "하다보면 늘어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043353.1.18",
          "form": "ㅋㅋㅋㅋ라면 하나는 기깔나게 잘끓이는데...",
          "original_form": "ㅋㅋㅋㅋ라면 하나는 기깔나게 잘끓이는데...",
          "speaker_id": "2",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100043353.1.19",
          "form": "그거면 된거 아닐까..ㅎ?",
          "original_form": "그거면 된거 아닐까..ㅎ?",
          "speaker_id": "2",
          "time": "20210907 18:28"
        },
        {
          "id": "MDRW2100043353.1.20",
          "form": "그러게말이야 그럼 다 됐네 ㅋㅋㅋ",
          "original_form": "그러게말이야 그럼 다 됐네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100043353.1.21",
          "form": "ㅋㅋㅋ 다른 음식은 내후년의 나한테 맡겨놓으려구",
          "original_form": "ㅋㅋㅋ 다른 음식은 내후년의 나한테 맡겨놓으려구",
          "speaker_id": "2",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100043353.1.22",
          "form": "맞아 ㅋㅋㅋ 때되면 다 해",
          "original_form": "맞아 ㅋㅋㅋ 때되면 다 해",
          "speaker_id": "1",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100043353.1.23",
          "form": "그 연륜이 느껴지는 대답 뭐야ㅋㅋㅋ!!!",
          "original_form": "그 연륜이 느껴지는 대답 뭐야ㅋㅋㅋ!!!",
          "speaker_id": "2",
          "time": "20210907 18:31"
        },
        {
          "id": "MDRW2100043353.1.24",
          "form": "나 서른살이거든 ㅠㅠㅋㅋㅋㅋ",
          "original_form": "나 서른살이거든 ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:31"
        }
      ]
