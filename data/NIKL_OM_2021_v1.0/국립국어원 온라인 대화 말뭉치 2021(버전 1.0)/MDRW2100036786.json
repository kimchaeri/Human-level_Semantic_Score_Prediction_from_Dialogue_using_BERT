      "utterance": [
        {
          "id": "MDRW2100036786.1.1",
          "form": "그래도 맛있게 드셔요!",
          "original_form": "그래도 맛있게 드셔요!",
          "speaker_id": "1",
          "time": "20210831 12:08"
        },
        {
          "id": "MDRW2100036786.1.2",
          "form": "넹넹! 다시 도전중이에용ㅋㅋㅋ",
          "original_form": "넹넹! 다시 도전중이에용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:08"
        },
        {
          "id": "MDRW2100036786.1.3",
          "form": "name1님은 어떤거 드세요??",
          "original_form": "&name1&님은 어떤거 드세요??",
          "speaker_id": "2",
          "time": "20210831 12:08"
        },
        {
          "id": "MDRW2100036786.1.4",
          "form": "저는 콩불해먹을거 같아요!",
          "original_form": "저는 콩불해먹을거 같아요!",
          "speaker_id": "1",
          "time": "20210831 12:11"
        },
        {
          "id": "MDRW2100036786.1.5",
          "form": "자취인의 든든한 식재료 대패삼겹이있으니까요",
          "original_form": "자취인의 든든한 식재료 대패삼겹이있으니까요",
          "speaker_id": "1",
          "time": "20210831 12:11"
        },
        {
          "id": "MDRW2100036786.1.6",
          "form": "우와.. 대박",
          "original_form": "우와.. 대박",
          "speaker_id": "2",
          "time": "20210831 12:13"
        },
        {
          "id": "MDRW2100036786.1.7",
          "form": "집에서 드시다니 요리 고수시네오ㅡ",
          "original_form": "집에서 드시다니 요리 고수시네오ㅡ",
          "speaker_id": "2",
          "time": "20210831 12:13"
        },
        {
          "id": "MDRW2100036786.1.8",
          "form": "시판소스 사용해서 간단하게 해요..ㅎ...",
          "original_form": "시판소스 사용해서 간단하게 해요..ㅎ...",
          "speaker_id": "1",
          "time": "20210831 12:13"
        },
        {
          "id": "MDRW2100036786.1.9",
          "form": "그걸 쓰는 사람이 요리사인걸용",
          "original_form": "그걸 쓰는 사람이 요리사인걸용",
          "speaker_id": "2",
          "time": "20210831 12:14"
        },
        {
          "id": "MDRW2100036786.1.10",
          "form": "소스사랑으로서..! 전 당당히 자칭 고수라합니닷ㅋㅋ",
          "original_form": "소스사랑으로서..! 전 당당히 자칭 고수라합니닷ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:14"
        },
        {
          "id": "MDRW2100036786.1.11",
          "form": "앜ㅋㅋㅋㅋㄲ저는 소스만들기가 너무 힘들더라구요ㅋㅋㅋㄱ",
          "original_form": "앜ㅋㅋㅋㅋㄲ저는 소스만들기가 너무 힘들더라구요ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210831 12:16"
        },
        {
          "id": "MDRW2100036786.1.12",
          "form": "냉장고에 소스가 가득해요",
          "original_form": "냉장고에 소스가 가득해요",
          "speaker_id": "1",
          "time": "20210831 12:16"
        },
        {
          "id": "MDRW2100036786.1.13",
          "form": "저두요ㅋㅋㅋ 이번에 이거 받아서 전부 소스사려구요ㅋㅋㅋㅋ",
          "original_form": "저두요ㅋㅋㅋ 이번에 이거 받아서 전부 소스사려구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:16"
        },
        {
          "id": "MDRW2100036786.1.14",
          "form": "역시 냉장고 문쪽칸은 소스칸이죠?ㅋㅋㅋㅋㄲ",
          "original_form": "역시 냉장고 문쪽칸은 소스칸이죠?ㅋㅋㅋㅋㄲ",
          "speaker_id": "1",
          "time": "20210831 12:18"
        },
        {
          "id": "MDRW2100036786.1.15",
          "form": "그럼요!! 혹시 모를 예비까지가 요리의 기본이잖아요?!",
          "original_form": "그럼요!! 혹시 모를 예비까지가 요리의 기본이잖아요?!",
          "speaker_id": "2",
          "time": "20210831 12:19"
        },
        {
          "id": "MDRW2100036786.1.16",
          "form": "소스만 잘써도 왠만한거 다먹을만하단말이예요ㅋㅋㄲㅋ",
          "original_form": "소스만 잘써도 왠만한거 다먹을만하단말이예요ㅋㅋㄲㅋ",
          "speaker_id": "1",
          "time": "20210831 12:20"
        },
        {
          "id": "MDRW2100036786.1.17",
          "form": "케챱 ,마요, 허니머스타드, 칠리 이정도는 기본이 아닌가하는 마음...",
          "original_form": "케챱 ,마요, 허니머스타드, 칠리 이정도는 기본이 아닌가하는 마음...",
          "speaker_id": "1",
          "time": "20210831 12:20"
        },
        {
          "id": "MDRW2100036786.1.18",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "2",
          "time": "20210831 12:22"
        },
        {
          "id": "MDRW2100036786.1.19",
          "form": "그건 진짜..",
          "original_form": "그건 진짜..",
          "speaker_id": "2",
          "time": "20210831 12:22"
        },
        {
          "id": "MDRW2100036786.1.20",
          "form": "소스와 함께면 초대도 두렵지 않다구용!",
          "original_form": "소스와 함께면 초대도 두렵지 않다구용!",
          "speaker_id": "2",
          "time": "20210831 12:22"
        },
        {
          "id": "MDRW2100036786.1.21",
          "form": "대충 월남쌈에 소스만 여러개 내놓으면",
          "original_form": "대충 월남쌈에 소스만 여러개 내놓으면",
          "speaker_id": "1",
          "time": "20210831 12:24"
        },
        {
          "id": "MDRW2100036786.1.22",
          "form": "그럴듯하다구요?",
          "original_form": "그럴듯하다구요?",
          "speaker_id": "1",
          "time": "20210831 12:24"
        },
        {
          "id": "MDRW2100036786.1.23",
          "form": "ㅋㄱㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㄱㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:24"
        },
        {
          "id": "MDRW2100036786.1.24",
          "form": "크으 역시 짬바 있으시네여!",
          "original_form": "크으 역시 짬바 있으시네여!",
          "speaker_id": "2",
          "time": "20210831 12:26"
        },
        {
          "id": "MDRW2100036786.1.25",
          "form": "야채도 대충 썰어줘도 근사하다구용",
          "original_form": "야채도 대충 썰어줘도 근사하다구용",
          "speaker_id": "2",
          "time": "20210831 12:26"
        },
        {
          "id": "MDRW2100036786.1.26",
          "form": "뭘좀아시는군요",
          "original_form": "뭘좀아시는군요",
          "speaker_id": "1",
          "time": "20210831 12:27"
        }
      ]
    }
