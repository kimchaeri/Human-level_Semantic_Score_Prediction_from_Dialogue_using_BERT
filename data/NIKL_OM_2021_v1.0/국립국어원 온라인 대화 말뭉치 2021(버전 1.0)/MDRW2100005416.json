      "utterance": [
        {
          "id": "MDRW2100005416.1.1",
          "form": "고양이들은 요즘 잘 지내니",
          "original_form": "고양이들은 요즘 잘 지내니",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005416.1.2",
          "form": "우리 고영희들이 지금 밥달라고 난리다",
          "original_form": "우리 고영희들이 지금 밥달라고 난리다",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005416.1.3",
          "form": "아주 잘지내서 문제다",
          "original_form": "아주 잘지내서 문제다",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005416.1.4",
          "form": "여전하구만 하하",
          "original_form": "여전하구만 하하",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005416.1.5",
          "form": "고양이 다이어트는",
          "original_form": "고양이 다이어트는",
          "speaker_id": "1",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005416.1.6",
          "form": "살이 너무 쪘어",
          "original_form": "살이 너무 쪘어",
          "speaker_id": "2",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005416.1.7",
          "form": "또할거니.",
          "original_form": "또할거니.",
          "speaker_id": "1",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005416.1.8",
          "form": "이번주부타 다시 시작했어",
          "original_form": "이번주부타 다시 시작했어",
          "speaker_id": "2",
          "time": "20210821 22:18"
        },
        {
          "id": "MDRW2100005416.1.9",
          "form": "오오 다시하는구너",
          "original_form": "오오 다시하는구너",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005416.1.10",
          "form": "너가 힘들겧어",
          "original_form": "너가 힘들겧어",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005416.1.11",
          "form": "어럅지",
          "original_form": "어럅지",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005416.1.12",
          "form": "나쁜말이라니",
          "original_form": "나쁜말이라니",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005416.1.13",
          "form": "3시3끼를",
          "original_form": "3시3끼를",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005416.1.14",
          "form": "한국말로 다쳣더니",
          "original_form": "한국말로 다쳣더니",
          "speaker_id": "1",
          "time": "20210821 22:19"
        },
        {
          "id": "MDRW2100005416.1.15",
          "form": "나쁜말은 보냘수 없다고 허내",
          "original_form": "나쁜말은 보냘수 없다고 허내",
          "speaker_id": "1",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005416.1.16",
          "form": "맞아 애들이 점점 밥달라고 보채는 스킬이 좋아져서 너무 괴롭다 ㅠ",
          "original_form": "맞아 애들이 점점 밥달라고 보채는 스킬이 좋아져서 너무 괴롭다 ㅠ",
          "speaker_id": "2",
          "time": "20210821 22:20"
        },
        {
          "id": "MDRW2100005416.1.17",
          "form": "반려동물이 3마리라",
          "original_form": "반려동물이 3마리라",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005416.1.18",
          "form": "더 함들겨",
          "original_form": "더 함들겨",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005416.1.19",
          "form": "애들이 막 울고 부르짖지",
          "original_form": "애들이 막 울고 부르짖지",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005416.1.20",
          "form": "지난 일년동안 힘들게 뺀거 도로 돌아왔어 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "지난 일년동안 힘들게 뺀거 도로 돌아왔어 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005416.1.21",
          "form": "아니 몇개월만애",
          "original_form": "아니 몇개월만애",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005416.1.22",
          "form": "돌아오다닠ㅋㅋㅋㅋ",
          "original_form": "돌아오다닠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005416.1.23",
          "form": "대단한 녀석들",
          "original_form": "대단한 녀석들",
          "speaker_id": "1",
          "time": "20210821 22:22"
        },
        {
          "id": "MDRW2100005416.1.24",
          "form": "얘네들이 너무 안 움직이니까 밥 양 조절이 진짜 중요해",
          "original_form": "얘네들이 너무 안 움직이니까 밥 양 조절이 진짜 중요해",
          "speaker_id": "2",
          "time": "20210821 22:23"
        },
        {
          "id": "MDRW2100005416.1.25",
          "form": "강아지마냥",
          "original_form": "강아지마냥",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005416.1.26",
          "form": "산책이라도 시틸수 있으먼 젛으련만",
          "original_form": "산책이라도 시틸수 있으먼 젛으련만",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005416.1.27",
          "form": "홈트시켜러",
          "original_form": "홈트시켜러",
          "speaker_id": "1",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005416.1.28",
          "form": "시간되는 대로 많이 놀아줘야 겠어",
          "original_form": "시간되는 대로 많이 놀아줘야 겠어",
          "speaker_id": "2",
          "time": "20210821 22:25"
        }
      ]
    }
  ]
