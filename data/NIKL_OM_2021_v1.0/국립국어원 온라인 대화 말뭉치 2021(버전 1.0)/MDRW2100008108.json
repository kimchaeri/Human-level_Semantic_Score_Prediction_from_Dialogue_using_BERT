        {
          "id": "MDRW2100008108.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210822 13:21"
        },
        {
          "id": "MDRW2100008108.1.2",
          "form": "넹 하죠",
          "original_form": "넹 하죠",
          "speaker_id": "2",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008108.1.3",
          "form": "하기싫지만ㅋㅋ",
          "original_form": "하기싫지만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:28"
        },
        {
          "id": "MDRW2100008108.1.4",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100008108.1.5",
          "form": "전 빨래담당이에요",
          "original_form": "전 빨래담당이에요",
          "speaker_id": "2",
          "time": "20210822 13:31"
        },
        {
          "id": "MDRW2100008108.1.6",
          "form": "화장실 청소가 젤 싫어요",
          "original_form": "화장실 청소가 젤 싫어요",
          "speaker_id": "1",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100008108.1.7",
          "form": "귀찮죠.. 물때도 자주끼고",
          "original_form": "귀찮죠.. 물때도 자주끼고",
          "speaker_id": "2",
          "time": "20210822 13:32"
        },
        {
          "id": "MDRW2100008108.1.8",
          "form": "네 곰팡이 생길까보ㅓ",
          "original_form": "네 곰팡이 생길까보ㅓ",
          "speaker_id": "1",
          "time": "20210822 13:33"
        },
        {
          "id": "MDRW2100008108.1.9",
          "form": "얼마나 자주하세요 화장실청소?",
          "original_form": "얼마나 자주하세요 화장실청소?",
          "speaker_id": "2",
          "time": "20210822 13:34"
        },
        {
          "id": "MDRW2100008108.1.10",
          "form": "일주일에1번요",
          "original_form": "일주일에1번요",
          "speaker_id": "1",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008108.1.11",
          "form": "자주 하시네요",
          "original_form": "자주 하시네요",
          "speaker_id": "2",
          "time": "20210822 13:35"
        },
        {
          "id": "MDRW2100008108.1.12",
          "form": "그른가요?",
          "original_form": "그른가요?",
