      "utterance": [
        {
          "id": "MDRW2100017930.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017930.1.2",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017930.1.3",
          "form": "네 여행 다니는거 되게 좋아해요!",
          "original_form": "네 여행 다니는거 되게 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017930.1.4",
          "form": "오 어디를 주로 다니셔요?ㅎㅎ",
          "original_form": "오 어디를 주로 다니셔요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017930.1.5",
          "form": "저는 부산으로 여행 자주가요!",
          "original_form": "저는 부산으로 여행 자주가요!",
          "speaker_id": "1",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017930.1.6",
          "form": "오호 부산 좋죠!!",
          "original_form": "오호 부산 좋죠!!",
          "speaker_id": "2",
          "time": "20210826 21:51"
        },
        {
          "id": "MDRW2100017930.1.7",
          "form": "저도 부산 좋아해요",
          "original_form": "저도 부산 좋아해요",
          "speaker_id": "2",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100017930.1.8",
          "form": "부산에 맛집도 많고 좋은것같아요",
          "original_form": "부산에 맛집도 많고 좋은것같아요",
          "speaker_id": "1",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100017930.1.9",
          "form": "네 바다도 있고",
          "original_form": "네 바다도 있고",
          "speaker_id": "2",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017930.1.10",
          "form": "도시도 있고",
          "original_form": "도시도 있고",
          "speaker_id": "2",
          "time": "20210826 21:54"
        },
        {
          "id": "MDRW2100017930.1.11",
          "form": "맞아요 저도 그 느낌이 좋아서 자주 여행가는거 같아요",
          "original_form": "맞아요 저도 그 느낌이 좋아서 자주 여행가는거 같아요",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017930.1.12",
          "form": "맛집도 많고요",
          "original_form": "맛집도 많고요",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017930.1.13",
          "form": "부산에서 먹는 밀면은 너무 맛있는것 같아요",
          "original_form": "부산에서 먹는 밀면은 너무 맛있는것 같아요",
          "speaker_id": "1",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100017930.1.14",
          "form": "부산밀면은 워낙 유명하죠",
          "original_form": "부산밀면은 워낙 유명하죠",
          "speaker_id": "2",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100017930.1.15",
          "form": "저는 부산 갔을때 양곱창구이랑 생선까스 먹었던 기억이",
          "original_form": "저는 부산 갔을때 양곱창구이랑 생선까스 먹었던 기억이",
          "speaker_id": "2",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017930.1.16",
          "form": "저는 돼지 국밥도 되게 맛있게 먹고왔어요 ㅋㅋ",
          "original_form": "저는 돼지 국밥도 되게 맛있게 먹고왔어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017930.1.17",
          "form": "맞아요 돼지국밥도 맛있죠",
          "original_form": "맞아요 돼지국밥도 맛있죠",
          "speaker_id": "2",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017930.1.18",
          "form": "저는 한겨울에 부산 갔을때 먹었는데 계속 생각나더라고요",
          "original_form": "저는 한겨울에 부산 갔을때 먹었는데 계속 생각나더라고요",
          "speaker_id": "2",
