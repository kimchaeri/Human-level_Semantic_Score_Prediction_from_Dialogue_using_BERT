      "utterance": [
        {
          "id": "MDRW2100038243.1.1",
          "form": "보조개 이쁨",
          "original_form": "보조개 이쁨",
          "speaker_id": "2",
          "time": "20210902 01:40"
        },
        {
          "id": "MDRW2100038243.1.2",
          "form": "나도 보조개 잇었으면 좋겠는데",
          "original_form": "나도 보조개 잇었으면 좋겠는데",
          "speaker_id": "1",
          "time": "20210902 01:40"
        },
        {
          "id": "MDRW2100038243.1.3",
          "form": "요즘 수술로 가능",
          "original_form": "요즘 수술로 가능",
          "speaker_id": "2",
          "time": "20210902 01:40"
        },
        {
          "id": "MDRW2100038243.1.4",
          "form": "인위적인건 싫다",
          "original_form": "인위적인건 싫다",
          "speaker_id": "1",
          "time": "20210902 01:40"
        },
        {
          "id": "MDRW2100038243.1.5",
          "form": "현명하단다 손님들이",
          "original_form": "현명하단다 손님들이",
          "speaker_id": "2",
          "time": "20210902 01:41"
        },
        {
          "id": "MDRW2100038243.1.6",
          "form": "그러게 이서진이 일머리는 있음",
          "original_form": "그러게 이서진이 일머리는 있음",
          "speaker_id": "1",
          "time": "20210902 01:41"
        },
        {
          "id": "MDRW2100038243.1.7",
          "form": "그래 머리가 좋다 미대형이라도",
          "original_form": "그래 머리가 좋다 미대형이라도",
          "speaker_id": "2",
          "time": "20210902 01:42"
        },
        {
          "id": "MDRW2100038243.1.8",
          "form": "1박2일 게스트로 만나서 저렇게 많은 프로를 하고",
          "original_form": "1박2일 게스트로 만나서 저렇게 많은 프로를 하고",
          "speaker_id": "1",
          "time": "20210902 01:42"
        },
        {
          "id": "MDRW2100038243.1.9",
          "form": "그래 인연이다",
          "original_form": "그래 인연이다",
          "speaker_id": "2",
          "time": "20210902 01:42"
        },
        {
          "id": "MDRW2100038243.1.10",
          "form": "윤식당 꽃보다할배 삼 시 세 끼 3개나 했네",
          "original_form": "윤식당 꽃보다할배 삼 시 세 끼 3개나 했네",
          "speaker_id": "1",
          "time": "20210902 01:43"
        },
        {
          "id": "MDRW2100038243.1.11",
          "form": "왜 뉴욕 가서 한것도 있잖아",
          "original_form": "왜 뉴욕 가서 한것도 있잖아",
          "speaker_id": "2",
          "time": "20210902 01:43"
        },
        {
          "id": "MDRW2100038243.1.12",
          "form": "많다",
          "original_form": "많다",
          "speaker_id": "2",
          "time": "20210902 01:43"
        },
        {
          "id": "MDRW2100038243.1.13",
          "form": "그럼 나피디랑 한개 4 개 네",
          "original_form": "그럼 나피디랑 한개 4 개 네",
          "speaker_id": "1",
          "time": "20210902 01:44"
        },
        {
          "id": "MDRW2100038243.1.14",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210902 01:44"
        },
        {
          "id": "MDRW2100038243.1.15",
          "form": "이서진이 성격이 괜찮으니 나피디랑 계속 하는거겠지",
          "original_form": "이서진이 성격이 괜찮으니 나피디랑 계속 하는거겠지",
