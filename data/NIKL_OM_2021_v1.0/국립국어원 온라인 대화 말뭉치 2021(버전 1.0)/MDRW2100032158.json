      "utterance": [
        {
          "id": "MDRW2100032158.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210905 13:29"
        },
        {
          "id": "MDRW2100032158.1.2",
          "form": "안녕하세요 ㅎㄹ",
          "original_form": "안녕하세요 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100032158.1.3",
          "form": "혹시 연애하고 계세요?",
          "original_form": "혹시 연애하고 계세요?",
          "speaker_id": "1",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100032158.1.4",
          "form": "아뇨ㅋㅋㅋ 연애중이신가요??",
          "original_form": "아뇨ㅋㅋㅋ 연애중이신가요??",
          "speaker_id": "2",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100032158.1.5",
          "form": "저도 아니요ㅋㅋㅋ",
          "original_form": "저도 아니요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:31"
        },
        {
          "id": "MDRW2100032158.1.6",
          "form": "ㅋㅋㅋ 오 그럼 소개팅은 해보셨나여",
          "original_form": "ㅋㅋㅋ 오 그럼 소개팅은 해보셨나여",
          "speaker_id": "2",
          "time": "20210905 13:31"
        },
        {
          "id": "MDRW2100032158.1.7",
          "form": "아 네 그건 몇 번 해봤는데 으 별로더라구요ㅋㅋㅋ",
          "original_form": "아 네 그건 몇 번 해봤는데 으 별로더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:31"
        },
        {
          "id": "MDRW2100032158.1.8",
          "form": "해보셨어요?",
          "original_form": "해보셨어요?",
          "speaker_id": "1",
          "time": "20210905 13:31"
        },
        {
          "id": "MDRW2100032158.1.9",
          "form": "네 저두별로더라구옄ㅋㅋㅋ",
          "original_form": "네 저두별로더라구옄ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100032158.1.10",
          "form": "차라리 모임에서만나는게낫지..",
          "original_form": "차라리 모임에서만나는게낫지..",
          "speaker_id": "2",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100032158.1.11",
          "form": "하나하나 알아가려니 귀찮고 ㅋㅋㅋ 안맞는게 많은거같고ㅎㅎ",
          "original_form": "하나하나 알아가려니 귀찮고 ㅋㅋㅋ 안맞는게 많은거같고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100032158.1.12",
          "form": "저도요ㅋㅋㅋㅋ그 숨막히는 어색함",
          "original_form": "저도요ㅋㅋㅋㅋ그 숨막히는 어색함",
          "speaker_id": "1",
          "time": "20210905 13:33"
        },
        {
          "id": "MDRW2100032158.1.13",
          "form": "맞아여 ㅋㅋㅋㅋ 그래서 소개팅은 앞으로는 생각이없어요ㅠㅠ",
          "original_form": "맞아여 ㅋㅋㅋㅋ 그래서 소개팅은 앞으로는 생각이없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100032158.1.14",
          "form": "저도요ㅋㅋㅋㅋㅋ점점 그냥 연애에 대한 생각 자체가 없어지는 것 같아요 사는 것도 너무 바쁘고",
          "original_form": "저도요ㅋㅋㅋㅋㅋ점점 그냥 연애에 대한 생각 자체가 없어지는 것 같아요 사는 것도 너무 바쁘고",
          "speaker_id": "1",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100032158.1.15",
          "form": "맞아요 제 앞가림하기바빠여 ㅋㅋㅋ",
          "original_form": "맞아요 제 앞가림하기바빠여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100032158.1.16",
          "form": "ㅋㅋㅋ점점 더 그렇게 되어가는 것 같아요 제 주위도 비슷하더라구요",
          "original_form": "ㅋㅋㅋ점점 더 그렇게 되어가는 것 같아요 제 주위도 비슷하더라구요",
          "speaker_id": "1",
          "time": "20210905 13:35"
        },
        {
          "id": "MDRW2100032158.1.17",
          "form": "맞아요 저는 제주위 갈애들은 일찍가서..ㅎㅎ..ㅠㅠㅋㅋ",
          "original_form": "맞아요 저는 제주위 갈애들은 일찍가서..ㅎㅎ..ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100032158.1.18",
          "form": "와 정말요? 저는 아직 주위에 결혼한 친구는 아예 없어서 좀 낯설어요",
          "original_form": "와 정말요? 저는 아직 주위에 결혼한 친구는 아예 없어서 좀 낯설어요",
          "speaker_id": "1",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100032158.1.19",
          "form": "저도 한 이삼년전에는 그랬는데 갑자기 다들 가더라구욬ㅋㅋㅋ 근데 그렇다고 조급하진 않고.. 별생각이없어서 ㅋㅋㅋ",
          "original_form": "저도 한 이삼년전에는 그랬는데 갑자기 다들 가더라구욬ㅋㅋㅋ 근데 그렇다고 조급하진 않고.. 별생각이없어서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100032158.1.20",
          "form": "ㅋㅋㅋㅋㅋ신기하네요 이야기 들어보면 다들 비슷한 시기에 가는 것 같아요",
          "original_form": "ㅋㅋㅋㅋㅋ신기하네요 이야기 들어보면 다들 비슷한 시기에 가는 것 같아요",
          "speaker_id": "1",
          "time": "20210905 13:38"
        }
      ]
    }
