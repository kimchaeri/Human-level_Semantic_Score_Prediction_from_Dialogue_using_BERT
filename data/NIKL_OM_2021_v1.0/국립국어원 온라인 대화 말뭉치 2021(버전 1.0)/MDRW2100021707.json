      "utterance": [
        {
          "id": "MDRW2100021707.1.1",
          "form": "음악 좋아하세요?",
          "original_form": "음악 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 22:02"
        },
        {
          "id": "MDRW2100021707.1.2",
          "form": "ㅠㅠ음악별로안들어서 잘몰라욥",
          "original_form": "ㅠㅠ음악별로안들어서 잘몰라욥",
          "speaker_id": "2",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021707.1.3",
          "form": "영상은 보시는 편이세요?",
          "original_form": "영상은 보시는 편이세요?",
          "speaker_id": "1",
          "time": "20210828 23:23"
        },
        {
          "id": "MDRW2100021707.1.4",
          "form": "네 저 신서유기 좋아해요",
          "original_form": "네 저 신서유기 좋아해요",
          "speaker_id": "2",
          "time": "20210828 23:24"
        },
        {
          "id": "MDRW2100021707.1.5",
          "form": "아 신서유기 재밌죠!!",
          "original_form": "아 신서유기 재밌죠!!",
          "speaker_id": "1",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100021707.1.6",
          "form": "저는 요즘 대탈출이 참 재밌더라구요",
          "original_form": "저는 요즘 대탈출이 참 재밌더라구요",
          "speaker_id": "1",
          "time": "20210828 23:26"
        },
        {
          "id": "MDRW2100021707.1.7",
          "form": "앗 저도봐요 대탈출 내일하는데 기대되요",
          "original_form": "앗 저도봐요 대탈출 내일하는데 기대되요",
          "speaker_id": "2",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100021707.1.8",
          "form": "오오 맞아요ㅋㅋ",
          "original_form": "오오 맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100021707.1.9",
          "form": "전 시즌들은 본방 본적 없는데 이번엔 보고있어요!",
          "original_form": "전 시즌들은 본방 본적 없는데 이번엔 보고있어요!",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100021707.1.10",
          "form": "저랑 똑같네요 저도 재방보고 이번 시즌은 본방봐요",
          "original_form": "저랑 똑같네요 저도 재방보고 이번 시즌은 본방봐요",
          "speaker_id": "2",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100021707.1.11",
          "form": "오옹~  이번에 갈수록 재밌어지고 있죠!!",
          "original_form": "오옹~  이번에 갈수록 재밌어지고 있죠!!",
          "speaker_id": "1",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021707.1.12",
          "form": "1ㅡ2화는 진짜..참담했어요",
          "original_form": "1ㅡ2화는 진짜..참담했어요",
          "speaker_id": "1",
          "time": "20210828 23:53"
        },
        {
          "id": "MDRW2100021707.1.13",
          "form": "저도 1,2화는 그냥그랫어요 좀비편도 재밋긴했는데",
          "original_form": "저도 1,2화는 그냥그랫어요 좀비편도 재밋긴했는데",
          "speaker_id": "2",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100021707.1.14",
          "form": "이번 주제가 너무 신박해서 좋은거같아요",
          "original_form": "이번 주제가 너무 신박해서 좋은거같아요",
          "speaker_id": "2",
          "time": "20210828 23:54"
        },
        {
          "id": "MDRW2100021707.1.15",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210828 23:56"
        },
        {
          "id": "MDRW2100021707.1.16",
          "form": "움직일때 같이 두근두근했어요ㅋㅋ",
          "original_form": "움직일때 같이 두근두근했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:57"
        },
        {
          "id": "MDRW2100021707.1.17",
          "form": "내일본방송은 못보구 티빙으로 재방보려구요 낼기대되요",
          "original_form": "내일본방송은 못보구 티빙으로 재방보려구요 낼기대되요",
          "speaker_id": "2",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100021707.1.18",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "1",
          "time": "20210829 00:14"
        }
      ]
