        {
          "id": "MDRW2100005155.1.2",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005155.1.3",
          "form": "마라탕은 리얼 마약이야",
          "original_form": "마라탕은 리얼 마약이야",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005155.1.4",
          "form": "나도 그렇게 생각해",
          "original_form": "나도 그렇게 생각해",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005155.1.5",
          "form": "또 먹고 싶네",
          "original_form": "또 먹고 싶네",
          "speaker_id": "2",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005155.1.6",
          "form": "다음 달에도 한 4번은 시킬 듯",
          "original_form": "다음 달에도 한 4번은 시킬 듯",
          "speaker_id": "1",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005155.1.7",
          "form": "4번이면 많이 참는다 생각해 솔직히",
          "original_form": "4번이면 많이 참는다 생각해 솔직히",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005155.1.8",
          "form": "물론 4×4해서 최소 16끼는 먹어",
          "original_form": "물론 4×4해서 최소 16끼는 먹어",
          "speaker_id": "1",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005155.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005155.1.10",
          "form": "ㅋㅋㅋㅋㅋ갓성비 처ㅣ고다",
          "original_form": "ㅋㅋㅋㅋㅋ갓성비 처ㅣ고다",
          "speaker_id": "2",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100005155.1.11",
          "form": "중독이야 중독",
          "original_form": "중독이야 중독",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005155.1.12",
          "form": "마라탕 먹으니까 다른 거 안 먹게 됨",
          "original_form": "마라탕 먹으니까 다른 거 안 먹게 됨",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005155.1.13",
          "form": "마라탕이 너무 자극적이라",
          "original_form": "마라탕이 너무 자극적이라",
          "speaker_id": "1",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100005155.1.14",
          "form": "인정",
          "original_form": "인정",
          "speaker_id": "2",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005155.1.15",
          "form": "다른 건 간에 기별도 안 가",
          "original_form": "다른 건 간에 기별도 안 가",
          "speaker_id": "2",
          "time": "20210821 21:03"
        },
        {
          "id": "MDRW2100005155.1.16",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005155.1.17",
          "form": "그리고 마라탕 정도면 완전 식품이라고 생각해",
          "original_form": "그리고 마라탕 정도면 완전 식품이라고 생각해",
          "speaker_id": "1",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005155.1.18",
          "form": "ㅋㅋㅋㅋ맞지",
          "original_form": "ㅋㅋㅋㅋ맞지",
          "speaker_id": "2",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005155.1.19",
          "form": "할머니가 곰탕 끓이면 난 마라탕을 직접 만들어 먹을 거야",
          "original_form": "할머니가 곰탕 끓이면 난 마라탕을 직접 만들어 먹을 거야",
          "speaker_id": "1",
          "time": "20210821 21:10"
        },
        {
          "id": "MDRW2100005155.1.20",
          "form": "만드는 게 훨씬 저렴해지면 만들어 먹을 거야..ㅎㅎ",
          "original_form": "만드는 게 훨씬 저렴해지면 만들어 먹을 거야..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005155.1.21",
          "form": "나도 언젠간 마라탕 직접 만들어 먹ㄱ어야지",
