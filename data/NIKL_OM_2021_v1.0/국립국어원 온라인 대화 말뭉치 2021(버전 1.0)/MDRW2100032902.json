      "utterance": [
        {
          "id": "MDRW2100032902.1.1",
          "form": "그래도 가보자..",
          "original_form": "그래도 가보자..",
          "speaker_id": "2",
          "time": "20210906 04:32"
        },
        {
          "id": "MDRW2100032902.1.2",
          "form": "비행기 값만 많이 드나..",
          "original_form": "비행기 값만 많이 드나..",
          "speaker_id": "1",
          "time": "20210906 04:32"
        },
        {
          "id": "MDRW2100032902.1.3",
          "form": "5명이면 방도 두개 잡아야해..",
          "original_form": "5명이면 방도 두개 잡아야해..",
          "speaker_id": "1",
          "time": "20210906 04:32"
        },
        {
          "id": "MDRW2100032902.1.4",
          "form": ".......그렇네???",
          "original_form": ".......그렇네???",
          "speaker_id": "2",
          "time": "20210906 04:32"
        },
        {
          "id": "MDRW2100032902.1.5",
          "form": "어... 못가겠지???",
          "original_form": "어... 못가겠지???",
          "speaker_id": "1",
          "time": "20210906 04:32"
        },
        {
          "id": "MDRW2100032902.1.6",
          "form": "그럴지도..",
          "original_form": "그럴지도..",
          "speaker_id": "2",
          "time": "20210906 04:32"
        },
        {
          "id": "MDRW2100032902.1.7",
          "form": "흑 우울해 졌어..ㅠ",
          "original_form": "흑 우울해 졌어..ㅠ",
          "speaker_id": "1",
          "time": "20210906 04:33"
        },
        {
          "id": "MDRW2100032902.1.8",
          "form": "그래도 코로나 끝나면 가까운 괌이라도 다녀오자..",
          "original_form": "그래도 코로나 끝나면 가까운 괌이라도 다녀오자..",
          "speaker_id": "2",
          "time": "20210906 04:33"
        },
        {
          "id": "MDRW2100032902.1.9",
          "form": "리조트로 묵으면 하나만 빌려도 돼지 않나??",
          "original_form": "리조트로 묵으면 하나만 빌려도 돼지 않나??",
          "speaker_id": "2",
          "time": "20210906 04:33"
        },
        {
          "id": "MDRW2100032902.1.10",
          "form": "몰라..",
          "original_form": "몰라..",
          "speaker_id": "1",
          "time": "20210906 04:33"
        },
        {
          "id": "MDRW2100032902.1.11",
          "form": "여보가 알아볼꺼??",
          "original_form": "여보가 알아볼꺼??",
          "speaker_id": "1",
          "time": "20210906 04:34"
        },
        {
          "id": "MDRW2100032902.1.12",
          "form": "그러지 뭐",
          "original_form": "그러지 뭐",
          "speaker_id": "2",
          "time": "20210906 04:34"
        },
        {
          "id": "MDRW2100032902.1.13",
          "form": "그래.. 그럼 코로나... 종식..... 되면???",
          "original_form": "그래.. 그럼 코로나... 종식..... 되면???",
          "speaker_id": "1",
          "time": "20210906 04:34"
        },
        {
          "id": "MDRW2100032902.1.14",
          "form": "종식이 오긴 오냐???;;ㅠㅠ",
          "original_form": "종식이 오긴 오냐???;;ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 04:34"
        },
        {
          "id": "MDRW2100032902.1.15",
          "form": "종식 안돼도 치료제 나옴 여행은 다니지 않을까??",
          "original_form": "종식 안돼도 치료제 나옴 여행은 다니지 않을까??",
          "speaker_id": "2",
          "time": "20210906 04:34"
        },
        {
          "id": "MDRW2100032902.1.16",
          "form": "신종플루처럼???",
          "original_form": "신종플루처럼???",
          "speaker_id": "2",
          "time": "20210906 04:34"
        },
        {
          "id": "MDRW2100032902.1.17",
          "form": "후 그래 알았어..",
          "original_form": "후 그래 알았어..",
          "speaker_id": "1",
          "time": "20210906 04:35"
        },
        {
          "id": "MDRW2100032902.1.18",
          "form": "그래 너무 우울해 하지마..",
          "original_form": "그래 너무 우울해 하지마..",
          "speaker_id": "2",
          "time": "20210906 04:35"
        },
        {
          "id": "MDRW2100032902.1.19",
          "form": "해외여행 못가면 캠핑카 빌려서 국내 가면 되지.",
          "original_form": "해외여행 못가면 캠핑카 빌려서 국내 가면 되지.",
          "speaker_id": "2",
          "time": "20210906 04:35"
        },
        {
          "id": "MDRW2100032902.1.20",
          "form": "그것도 방법이긴하네.",
          "original_form": "그것도 방법이긴하네.",
          "speaker_id": "1",
          "time": "20210906 04:35"
        }
      ]
