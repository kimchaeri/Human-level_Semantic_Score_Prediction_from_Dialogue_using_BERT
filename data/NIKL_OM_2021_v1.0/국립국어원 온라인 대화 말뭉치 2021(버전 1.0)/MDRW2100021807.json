      "utterance": [
        {
          "id": "MDRW2100021807.1.1",
          "form": "뭥요",
          "original_form": "뭥요",
          "speaker_id": "2",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021807.1.2",
          "form": "그 피크닉 갈 때 딱인데!",
          "original_form": "그 피크닉 갈 때 딱인데!",
          "speaker_id": "1",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021807.1.3",
          "form": "일회용 도시락 용기라고 해야될까요?",
          "original_form": "일회용 도시락 용기라고 해야될까요?",
          "speaker_id": "1",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021807.1.4",
          "form": "샌드위치나 김밥 같은거 담기 좋아요",
          "original_form": "샌드위치나 김밥 같은거 담기 좋아요",
          "speaker_id": "1",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021807.1.5",
          "form": "아아! 피크닉 자주 가세여?",
          "original_form": "아아! 피크닉 자주 가세여?",
          "speaker_id": "2",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021807.1.6",
          "form": "되게 예쁘게 잘 나오더라구요 일회용 도시락통",
          "original_form": "되게 예쁘게 잘 나오더라구요 일회용 도시락통",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021807.1.7",
          "form": "ㅋㅋㅋ피크닉 자주 가진 않구",
          "original_form": "ㅋㅋㅋ피크닉 자주 가진 않구",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021807.1.8",
          "form": "회사 도시락으로 싸가요 *^^*",
          "original_form": "회사 도시락으로 싸가요 *^^*",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021807.1.9",
          "form": "코로나 금방 끝날 줄 알고 도시락통 사기는 싫고 비닐에 담긴 싫고 해서 샀어요",
          "original_form": "코로나 금방 끝날 줄 알고 도시락통 사기는 싫고 비닐에 담긴 싫고 해서 샀어요",
          "speaker_id": "1",
          "time": "20210828 22:42"
        },
        {
          "id": "MDRW2100021807.1.10",
          "form": "아 회사도시락 ㅋㅋ 부지런하셔라",
          "original_form": "아 회사도시락 ㅋㅋ 부지런하셔라",
          "speaker_id": "2",
          "time": "20210828 22:42"
        },
        {
          "id": "MDRW2100021807.1.11",
          "form": "그때 제가 한창 다이어트 하던 때여서 *^^*",
          "original_form": "그때 제가 한창 다이어트 하던 때여서 *^^*",
          "speaker_id": "1",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021807.1.12",
          "form": "누가 차려주는 밥 먹고 싶지 않아요? 식단 관리하시나",
          "original_form": "누가 차려주는 밥 먹고 싶지 않아요? 식단 관리하시나",
          "speaker_id": "2",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021807.1.13",
          "form": "겸사겸사~ 도시락 싸갔네용 ㅎㅎ",
          "original_form": "겸사겸사~ 도시락 싸갔네용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021807.1.14",
          "form": "아아 식단 관리 역시 ㅋㅋㅋㅋㅋ",
          "original_form": "아아 식단 관리 역시 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021807.1.15",
          "form": "그때 코로나도 터지고 해서 겸사겸사~",
          "original_form": "그때 코로나도 터지고 해서 겸사겸사~",
          "speaker_id": "1",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021807.1.16",
          "form": "그때 다들 ??하던 눈으로 쳐다보더라구요",
          "original_form": "그때 다들 ??하던 눈으로 쳐다보더라구요",
          "speaker_id": "1",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021807.1.17",
          "form": "전 그냥 맨날 계란 두알씩 싸가고있는데",
          "original_form": "전 그냥 맨날 계란 두알씩 싸가고있는데",
          "speaker_id": "2",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021807.1.18",
          "form": "도시락통 왓다갓다해요 ㅋㅋㅋ",
          "original_form": "도시락통 왓다갓다해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 22:44"
        },
        {
          "id": "MDRW2100021807.1.19",
          "form": "계란 두 알만 먹어요? ㄷㄷ",
          "original_form": "계란 두 알만 먹어요? ㄷㄷ",
          "speaker_id": "1",
          "time": "20210828 22:44"
        },
        {
          "id": "MDRW2100021807.1.20",
          "form": "아침이요",
          "original_form": "아침이요",
          "speaker_id": "2",
          "time": "20210828 22:44"
        },
        {
          "id": "MDRW2100021807.1.21",
          "form": "점심은 푸짐하게",
          "original_form": "점심은 푸짐하게",
          "speaker_id": "2",
          "time": "20210828 22:44"
        },
        {
          "id": "MDRW2100021807.1.22",
          "form": "오모 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "오모 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:44"
        },
        {
          "id": "MDRW2100021807.1.23",
          "form": "저는 아침 안먹고 점심으로 계란 두알 먹은 적 있어요..",
          "original_form": "저는 아침 안먹고 점심으로 계란 두알 먹은 적 있어요..",
          "speaker_id": "1",
          "time": "20210828 22:44"
        },
        {
          "id": "MDRW2100021807.1.24",
          "form": "와쓰러져요",
          "original_form": "와쓰러져요",
          "speaker_id": "2",
          "time": "20210828 22:45"
        },
        {
          "id": "MDRW2100021807.1.25",
          "form": "그래서 중간중간에 견과류를 간식으로 먹기도 했어요",
          "original_form": "그래서 중간중간에 견과류를 간식으로 먹기도 했어요",
          "speaker_id": "1",
          "time": "20210828 22:45"
        },
        {
          "id": "MDRW2100021807.1.26",
          "form": "와 이런 사람이 다이어트 성공하는군요",
          "original_form": "와 이런 사람이 다이어트 성공하는군요",
          "speaker_id": "2",
