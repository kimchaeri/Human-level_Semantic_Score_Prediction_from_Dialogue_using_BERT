      "utterance": [
        {
          "id": "MDRW2100010342.1.1",
          "form": "안녕하세요 수강신청 하셨나요?",
          "original_form": "안녕하세요 수강신청 하셨나요?",
          "speaker_id": "1",
          "time": "20210823 17:03"
        },
        {
          "id": "MDRW2100010342.1.2",
          "form": "ㅋㅋㅋ저는 대딩이",
          "original_form": "ㅋㅋㅋ저는 대딩이",
          "speaker_id": "2",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010342.1.3",
          "form": "아니랍니다",
          "original_form": "아니랍니다",
          "speaker_id": "2",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010342.1.4",
          "form": "대학생인가요?",
          "original_form": "대학생인가요?",
          "speaker_id": "2",
          "time": "20210823 17:11"
        },
        {
          "id": "MDRW2100010342.1.5",
          "form": "헉 그렇군요! 맞아요ㅋㅋㅋ",
          "original_form": "헉 그렇군요! 맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 17:12"
        },
        {
          "id": "MDRW2100010342.1.6",
          "form": "요즘 학교가나요?",
          "original_form": "요즘 학교가나요?",
          "speaker_id": "2",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010342.1.7",
          "form": "코로나때문에",
          "original_form": "코로나때문에",
          "speaker_id": "2",
          "time": "20210823 17:23"
        },
        {
          "id": "MDRW2100010342.1.8",
          "form": "못가고 온라인 수업만 해요ㅠ",
          "original_form": "못가고 온라인 수업만 해요ㅠ",
          "speaker_id": "1",
          "time": "20210823 17:24"
        },
        {
          "id": "MDRW2100010342.1.9",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100010342.1.10",
          "form": "하루도 안가나요?",
          "original_form": "하루도 안가나요?",
          "speaker_id": "2",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100010342.1.11",
          "form": "새내기들은 슬프겠어요",
          "original_form": "새내기들은 슬프겠어요",
          "speaker_id": "2",
          "time": "20210823 17:39"
        },
        {
          "id": "MDRW2100010342.1.12",
          "form": "맞아요ㅠ 저번학기는 격주로 다녔어요",
          "original_form": "맞아요ㅠ 저번학기는 격주로 다녔어요",
          "speaker_id": "1",
          "time": "20210823 17:45"
        },
        {
          "id": "MDRW2100010342.1.13",
          "form": "새터도",
          "original_form": "새터도",
          "speaker_id": "2",
          "time": "20210823 17:48"
        },
        {
          "id": "MDRW2100010342.1.14",
          "form": "없겠네요?",
          "original_form": "없겠네요?",
          "speaker_id": "2",
          "time": "20210823 17:48"
        },
        {
          "id": "MDRW2100010342.1.15",
          "form": "단톡에서 간략하게 한거말고 없어요ㅠ",
          "original_form": "단톡에서 간략하게 한거말고 없어요ㅠ",
          "speaker_id": "1",
          "time": "20210823 17:51"
        },
        {
          "id": "MDRW2100010342.1.16",
          "form": "선후배도 거의 모르고..",
          "original_form": "선후배도 거의 모르고..",
          "speaker_id": "1",
          "time": "20210823 17:51"
        },
        {
          "id": "MDRW2100010342.1.17",
          "form": "어유",
          "original_form": "어유",
          "speaker_id": "2",
          "time": "20210824 10:36"
        },
        {
          "id": "MDRW2100010342.1.18",
          "form": "아쉽겠어요ㅠㅠ",
          "original_form": "아쉽겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 10:36"
        },
        {
          "id": "MDRW2100010342.1.19",
          "form": "맞아요ㅠ 학점따기쉬워서 그것만 좋아용",
          "original_form": "맞아요ㅠ 학점따기쉬워서 그것만 좋아용",
          "speaker_id": "1",
          "time": "20210825 17:31"
        },
        {
          "id": "MDRW2100010342.1.20",
          "form": "ㅠ 학점이라",
          "original_form": "ㅠ 학점이라",
          "speaker_id": "2",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100010342.1.21",
          "form": "완전예전얘기군요",
          "original_form": "완전예전얘기군요",
          "speaker_id": "2",
          "time": "20210825 17:40"
        },
        {
          "id": "MDRW2100010342.1.22",
          "form": "부러워요..!",
          "original_form": "부러워요..!",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100010342.1.23",
          "form": "지금은 졸업해도 취업이 될지도 모르겠고ㅠ",
          "original_form": "지금은 졸업해도 취업이 될지도 모르겠고ㅠ",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100010342.1.24",
          "form": "ㅋㅋㅋ 저는 님이더부럽네요",
          "original_form": "ㅋㅋㅋ 저는 님이더부럽네요",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100010342.1.25",
          "form": "젊음이 최고죠",
          "original_form": "젊음이 최고죠",
          "speaker_id": "2",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100010342.1.26",
          "form": "돈과 여유가 최고아닐까요!ㅋㅋㅋㅋ",
          "original_form": "돈과 여유가 최고아닐까요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:54"
        }
      ]
    }
