      "utterance": [
        {
          "id": "MDRW2100032595.1.1",
          "form": "여행좋아하세요?",
          "original_form": "여행좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 21:05"
        },
        {
          "id": "MDRW2100032595.1.2",
          "form": "아뇨 집순이에요ㅜㅡㅠㅋㅋㅋ",
          "original_form": "아뇨 집순이에요ㅜㅡㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:05"
        },
        {
          "id": "MDRW2100032595.1.3",
          "form": "다큐만 봐도 대충 만족합니다ㅎ",
          "original_form": "다큐만 봐도 대충 만족합니다ㅎ",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100032595.1.4",
          "form": "ㅎㅎㅎ앗 다큐요?",
          "original_form": "ㅎㅎㅎ앗 다큐요?",
          "speaker_id": "1",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100032595.1.5",
          "form": "호캉스는어떠세요",
          "original_form": "호캉스는어떠세요",
          "speaker_id": "1",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100032595.1.6",
          "form": "다큐라고 할까 세계기행 뭐 이런거요..",
          "original_form": "다큐라고 할까 세계기행 뭐 이런거요..",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100032595.1.7",
          "form": "호캉스 가면 의외로 더 힘들더라구요ㅠㅠ",
          "original_form": "호캉스 가면 의외로 더 힘들더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100032595.1.8",
          "form": "제 집이 아니면 잠을 잘 못자요ㅠ",
          "original_form": "제 집이 아니면 잠을 잘 못자요ㅠ",
          "speaker_id": "2",
          "time": "20210905 21:07"
        },
        {
          "id": "MDRW2100032595.1.9",
          "form": "앗규ㅠ그럴수있겠어요",
          "original_form": "앗규ㅠ그럴수있겠어요",
          "speaker_id": "1",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100032595.1.10",
          "form": "세계기행 오..",
          "original_form": "세계기행 오..",
          "speaker_id": "1",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100032595.1.11",
          "form": "여행 자주 다니셨나요? 요즘은 코로나라 어렵죠ㅜ",
          "original_form": "여행 자주 다니셨나요? 요즘은 코로나라 어렵죠ㅜ",
          "speaker_id": "2",
          "time": "20210905 21:09"
        },
        {
          "id": "MDRW2100032595.1.12",
          "form": "저는좋아해요ㅎㅎ",
          "original_form": "저는좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100032595.1.13",
          "form": "요새는많이못다니지만",
          "original_form": "요새는많이못다니지만",
          "speaker_id": "1",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100032595.1.14",
          "form": "끝나면가고싶죠ㅎㅎ",
          "original_form": "끝나면가고싶죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100032595.1.15",
          "form": "어디여행이 제일 기억에 남으세요?",
          "original_form": "어디여행이 제일 기억에 남으세요?",
          "speaker_id": "2",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100032595.1.16",
          "form": "음 다좋았지만",
          "original_form": "음 다좋았지만",
          "speaker_id": "1",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100032595.1.17",
          "form": "런던이랑 라오스요",
          "original_form": "런던이랑 라오스요",
          "speaker_id": "1",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100032595.1.18",
          "form": "가보고싶으신데있으신가요?",
          "original_form": "가보고싶으신데있으신가요?",
          "speaker_id": "1",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100032595.1.19",
          "form": "크리스마스에 뉴욕이랑요 몽골이요!",
          "original_form": "크리스마스에 뉴욕이랑요 몽골이요!",
          "speaker_id": "2",
          "time": "20210905 21:19"
        },
        {
          "id": "MDRW2100032595.1.20",
          "form": "오로라가 보이는 추운나라도 ㅎㅎ",
          "original_form": "오로라가 보이는 추운나라도 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:19"
        },
        {
          "id": "MDRW2100032595.1.21",
          "form": "런던이랑 라오스가 좋았던 이유가 어떤건가요?",
          "original_form": "런던이랑 라오스가 좋았던 이유가 어떤건가요?",
          "speaker_id": "2",
          "time": "20210905 21:20"
        },
        {
          "id": "MDRW2100032595.1.22",
          "form": "크으 다 낭만적이에요",
          "original_form": "크으 다 낭만적이에요",
          "speaker_id": "1",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100032595.1.23",
          "form": "런던은 처음간유럽이라 좋았고 라오스는 첫사랑이랑 가서 인상깊어요ㅎㅎ",
          "original_form": "런던은 처음간유럽이라 좋았고 라오스는 첫사랑이랑 가서 인상깊어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:28"
        },
        {
          "id": "MDRW2100032595.1.24",
          "form": "우와 첫사랑이랑 여행..!! 두근두근하네요 제 일도 아닌뎈ㅋㅋㅋㅋㅋ",
          "original_form": "우와 첫사랑이랑 여행..!! 두근두근하네요 제 일도 아닌뎈ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:39"
        },
        {
          "id": "MDRW2100032595.1.25",
          "form": "국내여행은요? 자주다니시나요?",
          "original_form": "국내여행은요? 자주다니시나요?",
          "speaker_id": "2",
          "time": "20210905 21:40"
        },
        {
          "id": "MDRW2100032595.1.26",
          "form": "ㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:40"
        },
        {
          "id": "MDRW2100032595.1.27",
          "form": "국내는 코로나이후로 좀 가는것 같아요",
          "original_form": "국내는 코로나이후로 좀 가는것 같아요",
          "speaker_id": "1",
          "time": "20210905 21:41"
        },
        {
          "id": "MDRW2100032595.1.28",
          "form": "최근에담양다녀왔어요!",
          "original_form": "최근에담양다녀왔어요!",
          "speaker_id": "1",
          "time": "20210905 21:41"
        },
        {
          "id": "MDRW2100032595.1.29",
          "form": "오 멀리 다녀오셨네요? 전라도죠?",
          "original_form": "오 멀리 다녀오셨네요? 전라도죠?",
          "speaker_id": "2",
          "time": "20210905 21:42"
        },
        {
          "id": "MDRW2100032595.1.30",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100032595.1.31",
          "form": "한적하니좋더라고요",
          "original_form": "한적하니좋더라고요",
          "speaker_id": "1",
          "time": "20210905 21:47"
        }
      ]
    }
