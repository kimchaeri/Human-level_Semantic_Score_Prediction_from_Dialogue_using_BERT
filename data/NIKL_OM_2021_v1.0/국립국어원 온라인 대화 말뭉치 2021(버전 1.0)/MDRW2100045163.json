      "utterance": [
        {
          "id": "MDRW2100045163.1.1",
          "form": "언니 오늘 연어 먹었다며?",
          "original_form": "언니 오늘 연어 먹었다며?",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045163.1.2",
          "form": "맞아 진짜 극락이였어",
          "original_form": "맞아 진짜 극락이였어",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045163.1.3",
          "form": "나는 연어를 진짜 좋아하는거같아..",
          "original_form": "나는 연어를 진짜 좋아하는거같아..",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045163.1.4",
          "form": "너는 뭐먹었어?",
          "original_form": "너는 뭐먹었어?",
          "speaker_id": "2",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045163.1.5",
          "form": "진짜 맛있겠다.. 나도 알바비 들어오면 시켜먹어야지",
          "original_form": "진짜 맛있겠다.. 나도 알바비 들어오면 시켜먹어야지",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045163.1.6",
          "form": "난 햄버거 먹을까 고민하다가 그냥 고기 시켜서 밥먹었어",
          "original_form": "난 햄버거 먹을까 고민하다가 그냥 고기 시켜서 밥먹었어",
          "speaker_id": "1",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045163.1.7",
          "form": "잘했어ㅠㅠ",
          "original_form": "잘했어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045163.1.8",
          "form": "내가 건강 챙기면서 끊은게",
          "original_form": "내가 건강 챙기면서 끊은게",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045163.1.9",
          "form": "라면이랑 햄버거야..",
          "original_form": "라면이랑 햄버거야..",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045163.1.10",
          "form": "헐.. 나 햄버거 귀신인데...",
          "original_form": "헐.. 나 햄버거 귀신인데...",
          "speaker_id": "1",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045163.1.11",
          "form": "사실 지금도 배고파서 뭐 먹을까 고민중이었어",
          "original_form": "사실 지금도 배고파서 뭐 먹을까 고민중이었어",
          "speaker_id": "1",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045163.1.12",
          "form": "나도 1955버거 엄청좋아해서 자주시켜먹었어",
          "original_form": "나도 1955버거 엄청좋아해서 자주시켜먹었어",
          "speaker_id": "2",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045163.1.13",
          "form": "지금? 저녁언제먹었는데?",
          "original_form": "지금? 저녁언제먹었는데?",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045163.1.14",
          "form": "근데 나도 시원한거먹고싶다",
          "original_form": "근데 나도 시원한거먹고싶다",
          "speaker_id": "2",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045163.1.15",
          "form": "아까 알바 출근 전에 먹었어.. ㅜㅜ 이시간쯤 되니까 배고프더라고",
          "original_form": "아까 알바 출근 전에 먹었어.. ㅜㅜ 이시간쯤 되니까 배고프더라고",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045163.1.16",
          "form": "나는 베토디 제일 좋아해!! 맛있엉",
          "original_form": "나는 베토디 제일 좋아해!! 맛있엉",
          "speaker_id": "1",
          "time": "20210908 01:09"
        },
        {
          "id": "MDRW2100045163.1.17",
          "form": "베토디도 먹어봐야하는데",
          "original_form": "베토디도 먹어봐야하는데",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045163.1.18",
          "form": "근데 배달음식만 너무많이시켜먹으니까",
          "original_form": "근데 배달음식만 너무많이시켜먹으니까",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045163.1.19",
          "form": "돈이 너무 금방나가 ㅠ",
          "original_form": "돈이 너무 금방나가 ㅠ",
          "speaker_id": "2",
          "time": "20210908 01:10"
        },
        {
          "id": "MDRW2100045163.1.20",
          "form": "맞아맞아ㅠㅜ 이럴때는 요리가 진짜 중노동이구나 싶어",
          "original_form": "맞아맞아ㅠㅜ 이럴때는 요리가 진짜 중노동이구나 싶어",
          "speaker_id": "1",
          "time": "20210908 01:13"
        },
        {
          "id": "MDRW2100045163.1.21",
          "form": "요리할수있는 환경만 갖춰지면 나는 요리하고싶은데",
          "original_form": "요리할수있는 환경만 갖춰지면 나는 요리하고싶은데",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045163.1.22",
          "form": "그게안되니까 좀 아쉽다 ㅠㅠ",
          "original_form": "그게안되니까 좀 아쉽다 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045163.1.23",
          "form": "그래도 요리를해야 돈이 절약되긴해서!",
          "original_form": "그래도 요리를해야 돈이 절약되긴해서!",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045163.1.24",
          "form": "나지금 카페음료가 너무먹고싶어",
          "original_form": "나지금 카페음료가 너무먹고싶어",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045163.1.25",
          "form": "요즘엔 카페도 배달되니까 시켜먹으면 되긴하는데",
          "original_form": "요즘엔 카페도 배달되니까 시켜먹으면 되긴하는데",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045163.1.26",
          "form": "역시 배달비가 좀 아깝지..",
          "original_form": "역시 배달비가 좀 아깝지..",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045163.1.27",
          "form": "시간때문에 못시켜먹어 너무아쉽다",
          "original_form": "시간때문에 못시켜먹어 너무아쉽다",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045163.1.28",
          "form": "나 기숙사라 통금있어서ㅠㅠ",
          "original_form": "나 기숙사라 통금있어서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045163.1.29",
          "form": "내일은 카페음료 먹는다..",
          "original_form": "내일은 카페음료 먹는다..",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045163.1.30",
          "form": "메뉴 추천해줘",
          "original_form": "메뉴 추천해줘",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045163.1.31",
          "form": "음... 나는 아이스티에 샷추가 조아하는데",
          "original_form": "음... 나는 아이스티에 샷추가 조아하는데",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045163.1.32",
          "form": "친구들은 괴식이래ㅠㅠㅋㅋㅋㅋ",
          "original_form": "친구들은 괴식이래ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:21"
        }
      ]
