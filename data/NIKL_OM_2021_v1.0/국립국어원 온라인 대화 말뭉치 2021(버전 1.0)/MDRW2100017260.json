        {
          "id": "MDRW2100017260.1.2",
          "form": "동물 한번도 키워본적없으신가요",
          "original_form": "동물 한번도 키워본적없으신가요",
          "speaker_id": "1",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017260.1.3",
          "form": "요런 이름은 대형견",
          "original_form": "요런 이름은 대형견",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017260.1.4",
          "form": "예전에 룸메가 고양이 키워서",
          "original_form": "예전에 룸메가 고양이 키워서",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100017260.1.5",
          "form": "같이 도와주면서 살짝 키웠어요",
          "original_form": "같이 도와주면서 살짝 키웠어요",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017260.1.6",
          "form": "고양이 귀여웠겠다",
          "original_form": "고양이 귀여웠겠다",
          "speaker_id": "1",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017260.1.7",
          "form": "근데 제가 주인은 아니니까",
          "original_form": "근데 제가 주인은 아니니까",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017260.1.8",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017260.1.9",
          "form": "고양이는 뭐 외모올킬",
          "original_form": "고양이는 뭐 외모올킬",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017260.1.10",
          "form": "그 고양이는 무슨색이었나요",
          "original_form": "그 고양이는 무슨색이었나요",
          "speaker_id": "1",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017260.1.11",
          "form": "ㅋㅋㅋㅋ턱시도라고 해서",
          "original_form": "ㅋㅋㅋㅋ턱시도라고 해서",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017260.1.12",
          "form": "검흰",
          "original_form": "검흰",
          "speaker_id": "1",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017260.1.13",
          "form": "맞죠",
          "original_form": "맞죠",
          "speaker_id": "1",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017260.1.14",
          "form": "새까만데 목주변만 흰색",
          "original_form": "새까만데 목주변만 흰색",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017260.1.15",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017260.1.16",
          "form": "예뻤어요 착하고",
          "original_form": "예뻤어요 착하고",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017260.1.17",
          "form": "고양이가",
          "original_form": "고양이가",
          "speaker_id": "1",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017260.1.18",
          "form": "착할수도 있구나",
          "original_form": "착할수도 있구나",
          "speaker_id": "1",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017260.1.19",
          "form": "앗 제가 방금",
          "original_form": "앗 제가 방금",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017260.1.20",
          "form": "착하다고 했나요",
          "original_form": "착하다고 했나요",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017260.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017260.1.22",
          "form": "실수입니다",
          "original_form": "실수입니다",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017260.1.23",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017260.1.24",
          "form": "안 착했어요",
          "original_form": "안 착했어요",
          "speaker_id": "2",
          "time": "20210826 18:21"
        },
