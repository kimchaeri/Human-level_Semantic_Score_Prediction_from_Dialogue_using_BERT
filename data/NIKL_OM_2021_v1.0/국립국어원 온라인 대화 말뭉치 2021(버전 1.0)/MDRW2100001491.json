        {
          "id": "MDRW2100001491.1.1",
          "form": "안녕하세요,",
          "original_form": "안녕하세요,",
          "speaker_id": "1",
          "time": "20211015 16:04"
        },
        {
          "id": "MDRW2100001491.1.2",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 16:14"
        },
        {
          "id": "MDRW2100001491.1.3",
          "form": "혹시 결혼하셨나요~?",
          "original_form": "혹시 결혼하셨나요~?",
          "speaker_id": "2",
          "time": "20211015 16:14"
        },
        {
          "id": "MDRW2100001491.1.4",
          "form": "아니오 미혼입니다",
          "original_form": "아니오 미혼입니다",
          "speaker_id": "1",
          "time": "20211015 16:42"
        },
        {
          "id": "MDRW2100001491.1.5",
          "form": "아하 저두요 ㅎㅎ",
          "original_form": "아하 저두요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 16:44"
        },
        {
          "id": "MDRW2100001491.1.6",
          "form": "요즘은 남녀 상관없이",
          "original_form": "요즘은 남녀 상관없이",
          "speaker_id": "2",
          "time": "20211015 16:44"
        },
        {
          "id": "MDRW2100001491.1.7",
          "form": "비혼 다짐하시는 분들이 꽤 많던데",
          "original_form": "비혼 다짐하시는 분들이 꽤 많던데",
          "speaker_id": "2",
          "time": "20211015 16:44"
        },
        {
          "id": "MDRW2100001491.1.8",
          "form": "이게 다 세상 살기가 팍팍해서..그런게 아닐까 싶어서 씁쓸하네요 ㅠ",
          "original_form": "이게 다 세상 살기가 팍팍해서..그런게 아닐까 싶어서 씁쓸하네요 ㅠ",
          "speaker_id": "2",
          "time": "20211015 16:44"
        },
        {
          "id": "MDRW2100001491.1.9",
          "form": "비혼주의는 아니지만, 결혼생각하면 경제적으로 숨이 턱 막혀서 막막하네요 ㅠ",
          "original_form": "비혼주의는 아니지만, 결혼생각하면 경제적으로 숨이 턱 막혀서 막막하네요 ㅠ",
          "speaker_id": "1",
          "time": "20211015 16:47"
        },
        {
          "id": "MDRW2100001491.1.10",
          "form": "하 정말 ㅠㅠ 저도 좋은 사람 있으면 결혼은 하고 싶은데",
          "original_form": "하 정말 ㅠㅠ 저도 좋은 사람 있으면 결혼은 하고 싶은데",
          "speaker_id": "2",
          "time": "20211015 16:47"
        },
        {
          "id": "MDRW2100001491.1.11",
          "form": "ㅋㅋㅋㅋㅋ정말 당장 어디서 살거냐며....",
          "original_form": "ㅋㅋㅋㅋㅋ정말 당장 어디서 살거냐며....",
          "speaker_id": "2",
          "time": "20211015 16:47"
        },
        {
          "id": "MDRW2100001491.1.12",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20211015 16:47"
        },
        {
          "id": "MDRW2100001491.1.13",
          "form": "진짜 요즘 너무 힘든 세상이죠..",
          "original_form": "진짜 요즘 너무 힘든 세상이죠..",
          "speaker_id": "1",
          "time": "20211015 16:48"
        },
        {
          "id": "MDRW2100001491.1.14",
          "form": "ㅠㅠ 그러게요 집값도 정말",
          "original_form": "ㅠㅠ 그러게요 집값도 정말",
          "speaker_id": "2",
          "time": "20211015 16:48"
        },
        {
          "id": "MDRW2100001491.1.15",
          "form": "미쳤어요 허허",
          "original_form": "미쳤어요 허허",
          "speaker_id": "2",
          "time": "20211015 16:48"
        },
        {
          "id": "MDRW2100001491.1.16",
          "form": "이젠 지방에도 내 집은 없을 것 같은 느낌..",
          "original_form": "이젠 지방에도 내 집은 없을 것 같은 느낌..",
          "speaker_id": "2",
          "time": "20211015 16:48"
        },
        {
          "id": "MDRW2100001491.1.17",
          "form": "^^",
          "original_form": "^^",
          "speaker_id": "2",
          "time": "20211015 16:48"
        },
        {
          "id": "MDRW2100001491.1.18",
          "form": "집값이 빨리 떨어져야 할텐데말이죠..",
          "original_form": "집값이 빨리 떨어져야 할텐데말이죠..",
          "speaker_id": "1",
          "time": "20211015 16:49"
        },
        {
          "id": "MDRW2100001491.1.19",
          "form": "결혼할즈음엔떨어져있기를..ㅎㅎ",
          "original_form": "결혼할즈음엔떨어져있기를..ㅎㅎ",
          "speaker_id": "1",
          "time": "20211015 16:49"
        },
        {
          "id": "MDRW2100001491.1.20",
          "form": "정말 그랬으면 좋겠네요..ㅎㅎ",
          "original_form": "정말 그랬으면 좋겠네요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 16:49"
        },
        {
          "id": "MDRW2100001491.1.21",
          "form": "주변에 혹시 결혼한 친구는 있으세용?",
          "original_form": "주변에 혹시 결혼한 친구는 있으세용?",
          "speaker_id": "2",
          "time": "20211015 16:49"
        },
        {
          "id": "MDRW2100001491.1.22",
          "form": "아직은 없네요 ㅎㅎ",
          "original_form": "아직은 없네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211015 16:51"
        },
        {
          "id": "MDRW2100001491.1.23",
          "form": "주변에 있으세요?",
          "original_form": "주변에 있으세요?",
          "speaker_id": "1",
          "time": "20211015 16:51"
        },
        {
          "id": "MDRW2100001491.1.24",
          "form": "저두 아직은..!",
          "original_form": "저두 아직은..!",
          "speaker_id": "2",
          "time": "20211015 16:51"
        },
        {
          "id": "MDRW2100001491.1.25",
          "form": "제일 먼저 가는 친구 결혼식 때",
          "original_form": "제일 먼저 가는 친구 결혼식 때",
          "speaker_id": "2",
          "time": "20211015 16:51"
        },
        {
          "id": "MDRW2100001491.1.26",
          "form": "눈물 흘릴듯요..",
          "original_form": "눈물 흘릴듯요..",
          "speaker_id": "2",
          "time": "20211015 16:51"
        },
        {
          "id": "MDRW2100001491.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 16:51"
        },
        {
          "id": "MDRW2100001491.1.28",
          "form": "할수만있다면 빨리하고싶지만..ㅎㅎ",
          "original_form": "할수만있다면 빨리하고싶지만..ㅎㅎ",
          "speaker_id": "1",
          "time": "20211015 16:52"
        },
        {
          "id": "MDRW2100001491.1.29",
          "form": "오 빨리하고싶으시구나!",
          "original_form": "오 빨리하고싶으시구나!",
          "speaker_id": "2",
          "time": "20211015 16:52"
        },
        {
          "id": "MDRW2100001491.1.30",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211015 16:52"
        },
        {
          "id": "MDRW2100001491.1.31",
          "form": "저는 뭔가 즐길거 다 즐겨보고",
          "original_form": "저는 뭔가 즐길거 다 즐겨보고",
          "speaker_id": "2",
          "time": "20211015 16:52"
        },
        {
          "id": "MDRW2100001491.1.32",
          "form": "늦게가고싶어요 ㅎㅎㅋㅋㅋㅋㅋ",
          "original_form": "늦게가고싶어요 ㅎㅎㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 16:52"
        },
        {
          "id": "MDRW2100001491.1.33",
          "form": "ㅋㅋㅋㅋ 그렇군요",
          "original_form": "ㅋㅋㅋㅋ 그렇군요",
          "speaker_id": "1",
          "time": "20211015 16:52"
        },
        {
          "id": "MDRW2100001491.1.34",
          "form": "네에 ㅋㅋㅋㅋ 사람마다 결혼시기에 대한 생각은 조금씩 다른 거 같아요 ㅎㅎ",
