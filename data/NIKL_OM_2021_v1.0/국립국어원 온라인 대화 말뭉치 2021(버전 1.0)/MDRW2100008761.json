      "utterance": [
        {
          "id": "MDRW2100008761.1.1",
          "form": "요즘 날씨가 변덕이 심하죠?",
          "original_form": "요즘 날씨가 변덕이 심하죠?",
          "speaker_id": "1",
          "time": "20210822 18:13"
        },
        {
          "id": "MDRW2100008761.1.2",
          "form": "비오고 덥고",
          "original_form": "비오고 덥고",
          "speaker_id": "2",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008761.1.3",
          "form": "습하고 힘드네요ㅎ",
          "original_form": "습하고 힘드네요ㅎ",
          "speaker_id": "2",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100008761.1.4",
          "form": "태풍 올라오고 있다고 하더라구요",
          "original_form": "태풍 올라오고 있다고 하더라구요",
          "speaker_id": "1",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008761.1.5",
          "form": "대비 확실하게 해야될듯요",
          "original_form": "대비 확실하게 해야될듯요",
          "speaker_id": "1",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008761.1.6",
          "form": "네ㅜ 여름 끝나려니까 태풍이 오네요ㅜ",
          "original_form": "네ㅜ 여름 끝나려니까 태풍이 오네요ㅜ",
          "speaker_id": "2",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008761.1.7",
          "form": "태풍 지나면 바로 가을이겠죠",
          "original_form": "태풍 지나면 바로 가을이겠죠",
          "speaker_id": "1",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008761.1.8",
          "form": "긴옷들 꺼내야겠어요",
          "original_form": "긴옷들 꺼내야겠어요",
          "speaker_id": "1",
          "time": "20210822 18:24"
        },
        {
          "id": "MDRW2100008761.1.9",
          "form": "습할거 같아서 걱정이네여ㅎㅎㅎ",
          "original_form": "습할거 같아서 걱정이네여ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 18:25"
        },
        {
          "id": "MDRW2100008761.1.10",
          "form": "습한거 너무 싫어요",
          "original_form": "습한거 너무 싫어요",
          "speaker_id": "1",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008761.1.11",
          "form": "불쾌지수가 올라간다고 하나",
          "original_form": "불쾌지수가 올라간다고 하나",
          "speaker_id": "1",
          "time": "20210822 18:26"
        },
        {
          "id": "MDRW2100008761.1.12",
          "form": "저두요ㅜㅜ 그래서 비오는게 싫어요ㅎㅎ",
          "original_form": "저두요ㅜㅜ 그래서 비오는게 싫어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 18:32"
        },
        {
          "id": "MDRW2100008761.1.13",
          "form": "저는 비오기 딱 일보직전이 싫어요",
          "original_form": "저는 비오기 딱 일보직전이 싫어요",
          "speaker_id": "1",
          "time": "20210822 18:33"
        },
        {
          "id": "MDRW2100008761.1.14",
          "form": "차라리 비가 내리면 시원하기라도 한데",
          "original_form": "차라리 비가 내리면 시원하기라도 한데",
          "speaker_id": "1",
          "time": "20210822 18:33"
        },
        {
          "id": "MDRW2100008761.1.15",
          "form": "습도 최강이죠",
          "original_form": "습도 최강이죠",
          "speaker_id": "2",
          "time": "20210822 18:35"
        },
        {
          "id": "MDRW2100008761.1.16",
          "form": "비오기전은ㅎㅎ",
          "original_form": "비오기전은ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 18:35"
        },
        {
          "id": "MDRW2100008761.1.17",
          "form": "맞아요ㅠ 그 불쾌한 습기",
          "original_form": "맞아요ㅠ 그 불쾌한 습기",
          "speaker_id": "1",
          "time": "20210822 18:36"
        },
        {
          "id": "MDRW2100008761.1.18",
          "form": "그땐 제습기가 필요합니다ㅋㅋㅋ",
          "original_form": "그땐 제습기가 필요합니다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:37"
        },
        {
          "id": "MDRW2100008761.1.19",
          "form": "제습기가 필수가 될줄은 몰랐어요",
          "original_form": "제습기가 필수가 될줄은 몰랐어요",
          "speaker_id": "1",
          "time": "20210822 19:23"
        },
        {
          "id": "MDRW2100008761.1.20",
          "form": "장마철엔 제습기 건조기 필수인듯 합니다ㅎㅎ",
          "original_form": "장마철엔 제습기 건조기 필수인듯 합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:43"
        }
      ]
