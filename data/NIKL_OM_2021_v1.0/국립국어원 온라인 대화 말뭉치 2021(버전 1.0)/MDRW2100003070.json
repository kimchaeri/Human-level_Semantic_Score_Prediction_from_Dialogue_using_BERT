      "utterance": [
        {
          "id": "MDRW2100003070.1.1",
          "form": "요즘 유퀴즈 재밌더라고요",
          "original_form": "요즘 유퀴즈 재밌더라고요",
          "speaker_id": "2",
          "time": "20210820 18:20"
        },
        {
          "id": "MDRW2100003070.1.2",
          "form": "저도 그거좋아해요 본방은 못챙겨보고 재방으로 봐요",
          "original_form": "저도 그거좋아해요 본방은 못챙겨보고 재방으로 봐요",
          "speaker_id": "1",
          "time": "20210820 18:21"
        },
        {
          "id": "MDRW2100003070.1.3",
          "form": "네 요즘엔 짤로도 재밌더라고요ㅋㅋ",
          "original_form": "네 요즘엔 짤로도 재밌더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:43"
        },
        {
          "id": "MDRW2100003070.1.4",
          "form": "예고편에 올림픽선수들 나오던데 이미 방송했나요?",
          "original_form": "예고편에 올림픽선수들 나오던데 이미 방송했나요?",
          "speaker_id": "1",
          "time": "20210820 18:44"
        },
        {
          "id": "MDRW2100003070.1.5",
          "form": "아직 최근엔 못봤어요",
          "original_form": "아직 최근엔 못봤어요",
          "speaker_id": "2",
          "time": "20210820 18:44"
        },
        {
          "id": "MDRW2100003070.1.6",
          "form": "유재석은 진짜 멋있는거같아요",
          "original_form": "유재석은 진짜 멋있는거같아요",
          "speaker_id": "2",
          "time": "20210820 18:44"
        },
        {
          "id": "MDRW2100003070.1.7",
          "form": "저도 유재석 팬이예요",
          "original_form": "저도 유재석 팬이예요",
          "speaker_id": "1",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100003070.1.8",
          "form": "실제로 보면 진짜 말랐을 거 같아요",
          "original_form": "실제로 보면 진짜 말랐을 거 같아요",
          "speaker_id": "2",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100003070.1.9",
          "form": "엄청  말랐대요ㅎ 저희엄마가 봤는데 다리 부러질거같다고",
          "original_form": "엄청  말랐대요ㅎ 저희엄마가 봤는데 다리 부러질거같다고",
          "speaker_id": "1",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100003070.1.10",
          "form": "운동해서 몸 좋은줄 알았는데 아닌가봐요",
          "original_form": "운동해서 몸 좋은줄 알았는데 아닌가봐요",
          "speaker_id": "1",
          "time": "20210820 18:47"
        },
        {
          "id": "MDRW2100003070.1.11",
          "form": "와 어머니 성덕이시네요ㅎㅎ",
          "original_form": "와 어머니 성덕이시네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 18:48"
        },
        {
          "id": "MDRW2100003070.1.12",
          "form": "근처에 촬영왔었대요",
          "original_form": "근처에 촬영왔었대요",
          "speaker_id": "1",
          "time": "20210820 18:49"
        },
        {
          "id": "MDRW2100003070.1.13",
          "form": "요즘엔 또 뭐가 재밌나요?",
          "original_form": "요즘엔 또 뭐가 재밌나요?",
          "speaker_id": "2",
          "time": "20210820 19:00"
        },
        {
          "id": "MDRW2100003070.1.14",
          "form": "전 전지적참견시점 그알 요런거봐요",
          "original_form": "전 전지적참견시점 그알 요런거봐요",
          "speaker_id": "1",
          "time": "20210820 19:01"
        },
        {
          "id": "MDRW2100003070.1.15",
          "form": "전참시 유세윤",
          "original_form": "전참시 유세윤",
          "speaker_id": "2",
          "time": "20210820 19:02"
        },
        {
          "id": "MDRW2100003070.1.16",
          "form": "빵터졌어요ㅋㅋ",
          "original_form": "빵터졌어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 19:02"
        },
        {
          "id": "MDRW2100003070.1.17",
          "form": "유세윤은 못봤는데 잼났나보네요ㅎ",
          "original_form": "유세윤은 못봤는데 잼났나보네요ㅎ",
          "speaker_id": "1",
          "time": "20210820 19:02"
        },
        {
          "id": "MDRW2100003070.1.18",
          "form": "네 최근 건데 꼭 보세요ㅋㅋ",
          "original_form": "네 최근 건데 꼭 보세요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 19:04"
        },
        {
          "id": "MDRW2100003070.1.19",
          "form": "그알은 무서워요ㅠ",
          "original_form": "그알은 무서워요ㅠ",
          "speaker_id": "2",
          "time": "20210820 19:04"
