      "utterance": [
        {
          "id": "MDRW2100004217.1.1",
          "form": "최근에 다녀온 여행이 있나요??ㅎㅎ",
          "original_form": "최근에 다녀온 여행이 있나요??ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004217.1.2",
          "form": "음 아니요",
          "original_form": "음 아니요",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004217.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004217.1.4",
          "form": "저도 없어요ㅠㅠ",
          "original_form": "저도 없어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004217.1.5",
          "form": "가봣던 여행지 중 어디가 젤 좋았나요?",
          "original_form": "가봣던 여행지 중 어디가 젤 좋았나요?",
          "speaker_id": "1",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004217.1.6",
          "form": "저는 전주요",
          "original_form": "저는 전주요",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004217.1.7",
          "form": "해외는 안가봐서..",
          "original_form": "해외는 안가봐서..",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004217.1.8",
          "form": "name1님은 어디가 좋았나요",
          "original_form": "&name1&님은 어디가 좋았나요",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004217.1.9",
          "form": "저는 국내는 부산이 젤 좋았던 것 같아요!",
          "original_form": "저는 국내는 부산이 젤 좋았던 것 같아요!",
          "speaker_id": "1",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004217.1.10",
          "form": "바다 보니까 좋더라구요 ㅎㅎ",
          "original_form": "바다 보니까 좋더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004217.1.11",
          "form": "부산 광안리!",
          "original_form": "부산 광안리!",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004217.1.12",
          "form": "바다되게 좋은 거 같아요",
          "original_form": "바다되게 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004217.1.13",
          "form": "바다가면 주로 뭐 하시나요?",
          "original_form": "바다가면 주로 뭐 하시나요?",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004217.1.14",
          "form": "저는 수영은 잘 안 하고 보통 그냥 걸으면서 구경해요!!",
          "original_form": "저는 수영은 잘 안 하고 보통 그냥 걸으면서 구경해요!!",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004217.1.15",
          "form": "name2님은 바다 가면 뭐하시나요?",
          "original_form": "&name2&님은 바다 가면 뭐하시나요?",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004217.1.16",
          "form": "저도 비슷한거같아요",
          "original_form": "저도 비슷한거같아요",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004217.1.17",
          "form": "바다는 좋은데 물은 무서워서ㅎㅎ",
          "original_form": "바다는 좋은데 물은 무서워서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004217.1.18",
          "form": "특히 바다는 그 파도가 은근 무서워서",
          "original_form": "특히 바다는 그 파도가 은근 무서워서",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004217.1.19",
          "form": "수영하기는 힘든 것 같아요ㅠㅠ",
          "original_form": "수영하기는 힘든 것 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004217.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004217.1.21",
          "form": "코로나 끝나면 가고싶은 곳 있으세요??",
          "original_form": "코로나 끝나면 가고싶은 곳 있으세요??",
          "speaker_id": "1",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004217.1.22",
          "form": "스페인이요",
          "original_form": "스페인이요",
          "speaker_id": "2",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004217.1.23",
          "form": "가우디 보러가고싶어요",
          "original_form": "가우디 보러가고싶어요",
          "speaker_id": "2",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004217.1.24",
          "form": "여름사탕님은요?",
          "original_form": "여름사탕님은요?",
          "speaker_id": "2",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004217.1.25",
          "form": "저는 발리나 하와이 같은 휴양지요!",
          "original_form": "저는 발리나 하와이 같은 휴양지요!",
          "speaker_id": "1",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004217.1.26",
          "form": "탁 트인 자연에서 쉬고싶은 마음이에요..",
          "original_form": "탁 트인 자연에서 쉬고싶은 마음이에요..",
          "speaker_id": "1",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004217.1.27",
          "form": "휴양지 너무 좋죠 여유롭고",
          "original_form": "휴양지 너무 좋죠 여유롭고",
          "speaker_id": "2",
