      "utterance": [
        {
          "id": "MDRW2100000672.1.1",
          "form": "이상형 어떻게되시나요?",
          "original_form": "이상형 어떻게되시나요?",
          "speaker_id": "1",
          "time": "20210819 17:33"
        },
        {
          "id": "MDRW2100000672.1.2",
          "form": "지금 와이프가 제 이상형 입니다",
          "original_form": "지금 와이프가 제 이상형 입니다",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000672.1.3",
          "form": "아 그렇군요....",
          "original_form": "아 그렇군요....",
          "speaker_id": "1",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000672.1.4",
          "form": "name1씨는 이상형이 어떻게 되시나요",
          "original_form": "&name1&씨는 이상형이 어떻게 되시나요",
          "speaker_id": "2",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000672.1.5",
          "form": "제 이상형은 키 크고 이목구비 뚜렷하고 좀 착했으면 좋겠어요",
          "original_form": "제 이상형은 키 크고 이목구비 뚜렷하고 좀 착했으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210819 17:34"
        },
        {
          "id": "MDRW2100000672.1.6",
          "form": "이목구비 뚜렷하고 착한건 만족하는데 키가... 하하하",
          "original_form": "이목구비 뚜렷하고 착한건 만족하는데 키가... 하하하",
          "speaker_id": "2",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000672.1.7",
          "form": "누가요?",
          "original_form": "누가요?",
          "speaker_id": "1",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000672.1.8",
          "form": "누가 착하던가요?",
          "original_form": "누가 착하던가요?",
          "speaker_id": "1",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000672.1.9",
          "form": "저 말이죠 저",
          "original_form": "저 말이죠 저",
          "speaker_id": "2",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000672.1.10",
          "form": "아닌거같은데...",
          "original_form": "아닌거같은데...",
          "speaker_id": "1",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000672.1.11",
          "form": "잘 생각해보시고 대답하시는건가요?",
          "original_form": "잘 생각해보시고 대답하시는건가요?",
          "speaker_id": "1",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000672.1.12",
          "form": "겪어보셔서 아시겠지만 제가 한 착함 한답니다",
          "original_form": "겪어보셔서 아시겠지만 제가 한 착함 한답니다",
          "speaker_id": "2",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000672.1.13",
          "form": "뭔가 크게 착각하시는것같은데",
          "original_form": "뭔가 크게 착각하시는것같은데",
          "speaker_id": "1",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000672.1.14",
          "form": "지금 굉장히 진지하답니다",
          "original_form": "지금 굉장히 진지하답니다",
          "speaker_id": "2",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000672.1.15",
          "form": "이상하네 거",
          "original_form": "이상하네 거",
          "speaker_id": "1",
          "time": "20210819 17:36"
        },
        {
          "id": "MDRW2100000672.1.16",
          "form": "요즘 주변 친구분들은 결혼 많이들 하셨나요?",
          "original_form": "요즘 주변 친구분들은 결혼 많이들 하셨나요?",
          "speaker_id": "2",
          "time": "20210819 17:37"
        },
        {
          "id": "MDRW2100000672.1.17",
          "form": "한명빼고 다 했어요",
          "original_form": "한명빼고 다 했어요",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000672.1.18",
          "form": "name2님 친구들은 결혼하셨나요?",
          "original_form": "&name2&님 친구들은 결혼하셨나요?",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000672.1.19",
          "form": "제 친구들도 거의 다 했네요 이젠",
          "original_form": "제 친구들도 거의 다 했네요 이젠",
          "speaker_id": "2",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000672.1.20",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000672.1.21",
          "form": "친한 동생 한명이랑 친구 한명 빼곤 다 했네요",
          "original_form": "친한 동생 한명이랑 친구 한명 빼곤 다 했네요",
          "speaker_id": "2",
          "time": "20210819 17:38"
        },
        {
          "id": "MDRW2100000672.1.22",
          "form": "요즘 젊은이들은 시국이 이래서 연애도 결혼도 힘들어하던데ㅠ",
          "original_form": "요즘 젊은이들은 시국이 이래서 연애도 결혼도 힘들어하던데ㅠ",
          "speaker_id": "1",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000672.1.23",
          "form": "그래도 연애 또는 결혼 할 사람들은 다 하겠죠?",
          "original_form": "그래도 연애 또는 결혼 할 사람들은 다 하겠죠?",
          "speaker_id": "1",
          "time": "20210819 17:39"
        },
        {
          "id": "MDRW2100000672.1.24",
          "form": "집값이 폭등하고 전세 매물이 거의 없어서 결혼하기 힘들다고 하더라구요",
