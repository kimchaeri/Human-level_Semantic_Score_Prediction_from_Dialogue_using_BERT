      "utterance": [
        {
          "id": "MDRW2100009231.1.1",
          "form": "애들은 이제 자나?",
          "original_form": "애들은 이제 자나?",
          "speaker_id": "2",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009231.1.2",
          "form": "ㅇㅇ 방금 잠들었어",
          "original_form": "ㅇㅇ 방금 잠들었어",
          "speaker_id": "1",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009231.1.3",
          "form": "스스로 자면 참 좋을텐데 ㅋ",
          "original_form": "스스로 자면 참 좋을텐데 ㅋ",
          "speaker_id": "2",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009231.1.4",
          "form": "때되면 스스로 자겠지.. 그냥 편하게 해주려고",
          "original_form": "때되면 스스로 자겠지.. 그냥 편하게 해주려고",
          "speaker_id": "1",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009231.1.5",
          "form": "작은 아들은 아직도 불끄면 무서워해?",
          "original_form": "작은 아들은 아직도 불끄면 무서워해?",
          "speaker_id": "2",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009231.1.6",
          "form": "ㅇㅇ 겁이 많아서 무서워하지 ㅎㅎ",
          "original_form": "ㅇㅇ 겁이 많아서 무서워하지 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:16"
        },
        {
          "id": "MDRW2100009231.1.7",
          "form": "잠 독립하려면 아직 먼 것 같은데",
          "original_form": "잠 독립하려면 아직 먼 것 같은데",
          "speaker_id": "2",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009231.1.8",
          "form": "어두운 곳은 아예 혼자 못가 ㅎㅎ",
          "original_form": "어두운 곳은 아예 혼자 못가 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009231.1.9",
          "form": "무슨 겁이 그리 많은지 ㅋ",
          "original_form": "무슨 겁이 그리 많은지 ㅋ",
          "speaker_id": "2",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009231.1.10",
          "form": "그냥 혼자 있는게 싫대 ㅎㅎ",
          "original_form": "그냥 혼자 있는게 싫대 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009231.1.11",
          "form": "아이구",
          "original_form": "아이구",
          "speaker_id": "2",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009231.1.12",
          "form": "아까도 창고에 게임 찾으러 가다가 도저히 못가겠다고 몇번 돌아옴 ㅎㅎ",
          "original_form": "아까도 창고에 게임 찾으러 가다가 도저히 못가겠다고 몇번 돌아옴 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 22:18"
        },
        {
          "id": "MDRW2100009231.1.13",
          "form": "그래서 가긴 갔어?",
          "original_form": "그래서 가긴 갔어?",
