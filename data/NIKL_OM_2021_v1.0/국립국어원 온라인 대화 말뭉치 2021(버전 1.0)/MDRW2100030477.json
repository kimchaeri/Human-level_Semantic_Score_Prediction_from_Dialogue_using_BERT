      "utterance": [
        {
          "id": "MDRW2100030477.1.1",
          "form": "아니 아까  펜트하우스 보다가 말았는데",
          "original_form": "아니 아까  펜트하우스 보다가 말았는데",
          "speaker_id": "2",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100030477.1.2",
          "form": "하윤철도 죽었어?!!",
          "original_form": "하윤철도 죽었어?!!",
          "speaker_id": "2",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100030477.1.3",
          "form": "응 천서진이 죽여버렀어",
          "original_form": "응 천서진이 죽여버렀어",
          "speaker_id": "1",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100030477.1.4",
          "form": "천서진 대단하네",
          "original_form": "천서진 대단하네",
          "speaker_id": "2",
          "time": "20210904 01:51"
        },
        {
          "id": "MDRW2100030477.1.5",
          "form": "몇명째야",
          "original_form": "몇명째야",
          "speaker_id": "2",
          "time": "20210904 01:51"
        },
        {
          "id": "MDRW2100030477.1.6",
          "form": "근데 하윤철이 복수했어",
          "original_form": "근데 하윤철이 복수했어",
          "speaker_id": "1",
          "time": "20210904 01:52"
        },
        {
          "id": "MDRW2100030477.1.7",
          "form": "어떻게 복수했어!!!",
          "original_form": "어떻게 복수했어!!!",
          "speaker_id": "2",
          "time": "20210904 01:53"
        },
        {
          "id": "MDRW2100030477.1.8",
          "form": "죽으면서 윤희한테 사랑한다고 말하그 죽었어",
          "original_form": "죽으면서 윤희한테 사랑한다고 말하그 죽었어",
          "speaker_id": "1",
          "time": "20210904 01:54"
        },
        {
          "id": "MDRW2100030477.1.9",
          "form": "이야 역시 순옥킴...",
          "original_form": "이야 역시 순옥킴...",
          "speaker_id": "2",
          "time": "20210904 01:56"
        },
        {
          "id": "MDRW2100030477.1.10",
          "form": "아내의 유혹부터 대단해",
          "original_form": "아내의 유혹부터 대단해",
          "speaker_id": "2",
          "time": "20210904 01:56"
        },
        {
          "id": "MDRW2100030477.1.11",
          "form": "근데 이제 로나한테는 아무도 안남았네ㅠ",
          "original_form": "근데 이제 로나한테는 아무도 안남았네ㅠ",
          "speaker_id": "2",
          "time": "20210904 01:56"
        },
        {
          "id": "MDRW2100030477.1.12",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210904 01:58"
        },
        {
          "id": "MDRW2100030477.1.13",
          "form": "심수련이 데리고살려나",
          "original_form": "심수련이 데리고살려나",
          "speaker_id": "1",
          "time": "20210904 01:59"
        },
        {
          "id": "MDRW2100030477.1.14",
          "form": "아니면 제니엄.마가 데리고 살지도?!",
          "original_form": "아니면 제니엄.마가 데리고 살지도?!",
          "speaker_id": "2",
          "time": "20210904 02:02"
        },
        {
          "id": "MDRW2100030477.1.15",
          "form": "그럴지도 지금도 같이사니까",
          "original_form": "그럴지도 지금도 같이사니까",
          "speaker_id": "1",
          "time": "20210904 02:32"
        },
        {
          "id": "MDRW2100030477.1.16",
          "form": "다음주에는 천서진한터ㅣ복수하고 끝나겠네",
          "original_form": "다음주에는 천서진한터ㅣ복수하고 끝나겠네",
          "speaker_id": "2",
          "time": "20210904 02:32"
        },
        {
          "id": "MDRW2100030477.1.17",
          "form": "너무 막장이 되버렸어",
          "original_form": "너무 막장이 되버렸어",
          "speaker_id": "1",
          "time": "20210904 02:33"
        },
        {
          "id": "MDRW2100030477.1.18",
          "form": "맞아 재밌게 끝낼수 있엏을텐데",
          "original_form": "맞아 재밌게 끝낼수 있엏을텐데",
