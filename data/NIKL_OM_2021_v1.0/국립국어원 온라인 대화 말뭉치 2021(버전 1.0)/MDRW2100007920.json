      "utterance": [
        {
          "id": "MDRW2100007920.1.1",
          "form": "다이어트 책 도서관에어 빌렸었어요",
          "original_form": "다이어트 책 도서관에어 빌렸었어요",
          "speaker_id": "2",
          "time": "20210822 11:53"
        },
        {
          "id": "MDRW2100007920.1.2",
          "form": "저도 도서관가는거 좋아하는데ㅎㅎ",
          "original_form": "저도 도서관가는거 좋아하는데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 11:54"
        },
        {
          "id": "MDRW2100007920.1.3",
          "form": "아 수면 관련책도 빌렸어요",
          "original_form": "아 수면 관련책도 빌렸어요",
          "speaker_id": "2",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007920.1.4",
          "form": "실용서를 많이보네요",
          "original_form": "실용서를 많이보네요",
          "speaker_id": "2",
          "time": "20210822 11:56"
        },
        {
          "id": "MDRW2100007920.1.5",
          "form": "그런책들은 도서관에서 빌리는게 편한거같아요ㅎㅎ",
          "original_form": "그런책들은 도서관에서 빌리는게 편한거같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100007920.1.6",
          "form": "이북도 보세요?",
          "original_form": "이북도 보세요?",
          "speaker_id": "2",
          "time": "20210822 11:57"
        },
        {
          "id": "MDRW2100007920.1.7",
          "form": "이북리더기는 있는데ㅎㅎ 귀찮아서 안보게 되네요",
          "original_form": "이북리더기는 있는데ㅎㅎ 귀찮아서 안보게 되네요",
          "speaker_id": "1",
          "time": "20210822 11:58"
        },
        {
          "id": "MDRW2100007920.1.8",
          "form": "올 리더기 어디꺼세요",
          "original_form": "올 리더기 어디꺼세요",
          "speaker_id": "2",
          "time": "20210822 11:59"
        },
        {
          "id": "MDRW2100007920.1.9",
          "form": "리디페이퍼로 샀는데...후회되기도 하궁..",
          "original_form": "리디페이퍼로 샀는데...후회되기도 하궁..",
          "speaker_id": "1",
          "time": "20210822 11:59"
        },
        {
          "id": "MDRW2100007920.1.10",
          "form": "저는 yes24 리더기 보기만하다가 결국 안샀어요",
          "original_form": "저는 yes24 리더기 보기만하다가 결국 안샀어요",
          "speaker_id": "2",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007920.1.11",
          "form": "별로인가요",
          "original_form": "별로인가요",
          "speaker_id": "2",
          "time": "20210822 12:02"
        },
        {
          "id": "MDRW2100007920.1.12",
          "form": "네 그냥 패드로 보는게 편하고..좋아요..ㅎㅎ",
          "original_form": "네 그냥 패드로 보는게 편하고..좋아요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 12:03"
        },
        {
          "id": "MDRW2100007920.1.13",
          "form": "저는 대신 예스24에서 태블릿 샀어요 ㅋㅋㅋ",
          "original_form": "저는 대신 예스24에서 태블릿 샀어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:05"
        },
        {
          "id": "MDRW2100007920.1.14",
          "form": "우와 그건 이북보기 편한가요?",
          "original_form": "우와 그건 이북보기 편한가요?",
          "speaker_id": "1",
          "time": "20210822 12:08"
        },
        {
          "id": "MDRW2100007920.1.15",
          "form": "아뇨 그냥저냥인데 이북 볼수는있어요 그냥 싼 태블릿?",
          "original_form": "아뇨 그냥저냥인데 이북 볼수는있어요 그냥 싼 태블릿?",
          "speaker_id": "2",
          "time": "20210822 12:11"
        },
        {
          "id": "MDRW2100007920.1.16",
          "form": "비싼 태블릿샀다가 고장날까봐ㅜ",
          "original_form": "비싼 태블릿샀다가 고장날까봐ㅜ",
