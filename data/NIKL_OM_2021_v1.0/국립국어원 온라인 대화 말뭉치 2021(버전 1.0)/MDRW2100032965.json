      "utterance": [
        {
          "id": "MDRW2100032965.1.1",
          "form": "음... 그냥 사람이스트레스네요 ㅠㅠ",
          "original_form": "음... 그냥 사람이스트레스네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 10:58"
        },
        {
          "id": "MDRW2100032965.1.2",
          "form": "내부요? 외부요?ㅎㅎ",
          "original_form": "내부요? 외부요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 11:43"
        },
        {
          "id": "MDRW2100032965.1.3",
          "form": "내부요...ㅎ",
          "original_form": "내부요...ㅎ",
          "speaker_id": "2",
          "time": "20210906 11:43"
        },
        {
          "id": "MDRW2100032965.1.4",
          "form": "ㅎㅎ저도 내부때문에",
          "original_form": "ㅎㅎ저도 내부때문에",
          "speaker_id": "1",
          "time": "20210906 11:44"
        },
        {
          "id": "MDRW2100032965.1.5",
          "form": "이직해봐서 이해합니다 ㅎㅎ",
          "original_form": "이직해봐서 이해합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 11:44"
        },
        {
          "id": "MDRW2100032965.1.6",
          "form": "진짜 이런스트레스가없네요..",
          "original_form": "진짜 이런스트레스가없네요..",
          "speaker_id": "2",
          "time": "20210906 11:44"
        },
        {
          "id": "MDRW2100032965.1.7",
          "form": "일 안하나요?ㅋㅋ",
          "original_form": "일 안하나요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 11:46"
        },
        {
          "id": "MDRW2100032965.1.8",
          "form": "안하면 다행이지",
          "original_form": "안하면 다행이지",
          "speaker_id": "2",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100032965.1.9",
          "form": "하는데 못해서 ㅠ",
          "original_form": "하는데 못해서 ㅠ",
          "speaker_id": "2",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100032965.1.10",
          "form": "ㅋㅋ 안하는 것도 열받습니다",
          "original_form": "ㅋㅋ 안하는 것도 열받습니다",
          "speaker_id": "1",
          "time": "20210906 12:44"
        },
        {
          "id": "MDRW2100032965.1.11",
          "form": "차라리 가만히있는게 ㅠㅠㅠ",
          "original_form": "차라리 가만히있는게 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 13:02"
        },
        {
          "id": "MDRW2100032965.1.12",
          "form": "전 위에가 일 개 안해서 스트레스 받아서",
          "original_form": "전 위에가 일 개 안해서 스트레스 받아서",
          "speaker_id": "1",
          "time": "20210906 14:18"
        },
        {
          "id": "MDRW2100032965.1.13",
          "form": "생색만 내고 ㅋㅋ",
          "original_form": "생색만 내고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:18"
        },
        {
          "id": "MDRW2100032965.1.14",
          "form": "홀랭",
          "original_form": "홀랭",
          "speaker_id": "2",
          "time": "20210906 15:05"
        },
        {
          "id": "MDRW2100032965.1.15",
          "form": "그건또 그것대로 빡치죠",
          "original_form": "그건또 그것대로 빡치죠",
          "speaker_id": "2",
          "time": "20210906 15:05"
        },
        {
          "id": "MDRW2100032965.1.16",
          "form": "정치질 오지게 해서 ㅋㅋ",
          "original_form": "정치질 오지게 해서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:20"
        },
        {
          "id": "MDRW2100032965.1.17",
          "form": "수출팀 차장으로 와놓고 일안합니다.....",
          "original_form": "수출팀 차장으로 와놓고 일안합니다.....",
          "speaker_id": "1",
          "time": "20210906 16:21"
        },
        {
          "id": "MDRW2100032965.1.18",
          "form": "아니왜그런데요??",
          "original_form": "아니왜그런데요??",
          "speaker_id": "2",
          "time": "20210906 16:25"
        },
        {
          "id": "MDRW2100032965.1.19",
          "form": "한대꿀밤줘야겠아요",
          "original_form": "한대꿀밤줘야겠아요",
          "speaker_id": "2",
          "time": "20210906 16:25"
        },
        {
          "id": "MDRW2100032965.1.20",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:25"
        },
        {
          "id": "MDRW2100032965.1.21",
          "form": "원래 직무랑 겸직한다고 그래서 팀장님이 일 많아서 안될꺼라고 했는데",
          "original_form": "원래 직무랑 겸직한다고 그래서 팀장님이 일 많아서 안될꺼라고 했는데",
          "speaker_id": "1",
          "time": "20210906 16:35"
        },
        {
          "id": "MDRW2100032965.1.22",
          "form": "자기기 해보겠다고 하고서 일 안합니다",
          "original_form": "자기기 해보겠다고 하고서 일 안합니다",
          "speaker_id": "1",
          "time": "20210906 16:35"
        },
        {
          "id": "MDRW2100032965.1.23",
          "form": "자기가 알아서 할 수 있지? 하고 진짜 안합니다ㅋㅋㅋ",
          "original_form": "자기가 알아서 할 수 있지? 하고 진짜 안합니다ㅋㅋㅋ",
          "speaker_id": "1",
