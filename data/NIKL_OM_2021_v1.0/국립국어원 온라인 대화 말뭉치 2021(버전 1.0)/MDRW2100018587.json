      "utterance": [
        {
          "id": "MDRW2100018587.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 08:59"
        },
        {
          "id": "MDRW2100018587.1.2",
          "form": "안녕하세요 게임좋아하세요?",
          "original_form": "안녕하세요 게임좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 11:39"
        },
        {
          "id": "MDRW2100018587.1.3",
          "form": "모바일 게임 하나 하고있습니다 ㅎ\n게임 좋아하세요?",
          "original_form": "모바일 게임 하나 하고있습니다 ㅎ\n게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 11:41"
        },
        {
          "id": "MDRW2100018587.1.4",
          "form": "전 퍼즐게임같은거 좋아합니다 ㅎ",
          "original_form": "전 퍼즐게임같은거 좋아합니다 ㅎ",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018587.1.5",
          "form": "요새는즐겨하는건없네요ㅠ",
          "original_form": "요새는즐겨하는건없네요ㅠ",
          "speaker_id": "2",
          "time": "20210827 13:32"
        },
        {
          "id": "MDRW2100018587.1.6",
          "form": "퍼즐게임이 시간 보내기 좋은 것 같아요 ㅎㅎ\n모바일 퍼즐게임으로 하시나요?",
          "original_form": "퍼즐게임이 시간 보내기 좋은 것 같아요 ㅎㅎ\n모바일 퍼즐게임으로 하시나요?",
          "speaker_id": "1",
          "time": "20210827 13:35"
        },
        {
          "id": "MDRW2100018587.1.7",
          "form": "맞아요~ 전 위베어베어스 한참햇어요",
          "original_form": "맞아요~ 전 위베어베어스 한참햇어요",
          "speaker_id": "2",
          "time": "20210827 13:40"
        },
        {
          "id": "MDRW2100018587.1.8",
          "form": "캐릭터 보는 재미도 있었겠어요 ㅎㅎ\n저도 그런 평화로운 게임이 좋은 것 같아요!\n배그같은 게임하는 친구들 보면 화만 내고 뭐가 재미있는지 모르겠어요 ㅎㅎ",
          "original_form": "캐릭터 보는 재미도 있었겠어요 ㅎㅎ\n저도 그런 평화로운 게임이 좋은 것 같아요!\n배그같은 게임하는 친구들 보면 화만 내고 뭐가 재미있는지 모르겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 13:42"
        },
        {
          "id": "MDRW2100018587.1.9",
          "form": "맞아요ㅠㅠ 저도 피나고 막 죽이고이런게임은 별로ㅜㅠ",
          "original_form": "맞아요ㅠㅠ 저도 피나고 막 죽이고이런게임은 별로ㅜㅠ",
          "speaker_id": "2",
          "time": "20210827 13:44"
        },
        {
          "id": "MDRW2100018587.1.10",
          "form": "저두요 ㅎㅎ 하면서 욕 나오는 게임을 왜 하는지 모르겠어요 ㅎㅎ",
          "original_form": "저두요 ㅎㅎ 하면서 욕 나오는 게임을 왜 하는지 모르겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 13:45"
        },
        {
          "id": "MDRW2100018587.1.11",
          "form": "뭔가 대리감정표출일까요;; 청소년기에 하는건 안좋은거가같아여ㅠㅠ",
          "original_form": "뭔가 대리감정표출일까요;; 청소년기에 하는건 안좋은거가같아여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 13:50"
        },
        {
          "id": "MDRW2100018587.1.12",
          "form": "맞아요ㅠ 애들 정서에 진짜 안좋은 것 같은데 이번에 셧다운제마저 없어진다고 하더라고요ㅠㅠ",
          "original_form": "맞아요ㅠ 애들 정서에 진짜 안좋은 것 같은데 이번에 셧다운제마저 없어진다고 하더라고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100018587.1.13",
          "form": "그렇군요ㅠ 몰랐어요!!",
          "original_form": "그렇군요ㅠ 몰랐어요!!",
          "speaker_id": "2",
          "time": "20210827 13:55"
        },
        {
          "id": "MDRW2100018587.1.14",
          "form": "저도 어제 뉴스 보고 알았어요ㅠ",
          "original_form": "저도 어제 뉴스 보고 알았어요ㅠ",
          "speaker_id": "1",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100018587.1.15",
          "form": "자율에 맡기겠대요ㅠ",
          "original_form": "자율에 맡기겠대요ㅠ",
