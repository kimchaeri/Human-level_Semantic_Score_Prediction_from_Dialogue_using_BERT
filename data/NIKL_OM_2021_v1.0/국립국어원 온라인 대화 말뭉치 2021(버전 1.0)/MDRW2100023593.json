      "utterance": [
        {
          "id": "MDRW2100023593.1.1",
          "form": "요즘 나는 건강에 관심이 많은데 들디는 어때?",
          "original_form": "요즘 나는 건강에 관심이 많은데 들디는 어때?",
          "speaker_id": "1",
          "time": "20210830 12:07"
        },
        {
          "id": "MDRW2100023593.1.2",
          "form": "나도 완전 그래!",
          "original_form": "나도 완전 그래!",
          "speaker_id": "2",
          "time": "20210830 12:13"
        },
        {
          "id": "MDRW2100023593.1.3",
          "form": "나도 그래서 필라테스 신청했잖아~",
          "original_form": "나도 그래서 필라테스 신청했잖아~",
          "speaker_id": "2",
          "time": "20210830 12:13"
        },
        {
          "id": "MDRW2100023593.1.4",
          "form": "근데 아직 한 번도 안 간 게 함정임.",
          "original_form": "근데 아직 한 번도 안 간 게 함정임.",
          "speaker_id": "2",
          "time": "20210830 12:13"
        },
        {
          "id": "MDRW2100023593.1.5",
          "form": "그룹수업이야?",
          "original_form": "그룹수업이야?",
          "speaker_id": "1",
          "time": "20210830 12:46"
        },
        {
          "id": "MDRW2100023593.1.6",
          "form": "우와 진짜?",
          "original_form": "우와 진짜?",
          "speaker_id": "1",
          "time": "20210830 12:46"
        },
        {
          "id": "MDRW2100023593.1.7",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210830 13:03"
        },
        {
          "id": "MDRW2100023593.1.8",
          "form": "원래 처음에는",
          "original_form": "원래 처음에는",
          "speaker_id": "2",
          "time": "20210830 13:03"
        },
        {
          "id": "MDRW2100023593.1.9",
          "form": "1대 1로 시작한다는데",
          "original_form": "1대 1로 시작한다는데",
          "speaker_id": "2",
          "time": "20210830 13:03"
        },
        {
          "id": "MDRW2100023593.1.10",
          "form": "이건 진짜 너~~~무 비싼거야...",
          "original_form": "이건 진짜 너~~~무 비싼거야...",
          "speaker_id": "2",
          "time": "20210830 13:03"
        },
        {
          "id": "MDRW2100023593.1.11",
          "form": "그래서 내 통장과 타협해서",
          "original_form": "그래서 내 통장과 타협해서",
          "speaker_id": "2",
          "time": "20210830 13:03"
        },
        {
          "id": "MDRW2100023593.1.12",
          "form": "6대 1로,,,,",
          "original_form": "6대 1로,,,,",
          "speaker_id": "2",
          "time": "20210830 13:03"
        },
        {
          "id": "MDRW2100023593.1.13",
          "form": "맞아.. 필테 너무 비싸지..ㅠ",
          "original_form": "맞아.. 필테 너무 비싸지..ㅠ",
          "speaker_id": "1",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100023593.1.14",
          "form": "나도 요즘 운동 너무 안가게 돼서 고민이야",
          "original_form": "나도 요즘 운동 너무 안가게 돼서 고민이야",
          "speaker_id": "1",
          "time": "20210830 13:22"
        },
        {
          "id": "MDRW2100023593.1.15",
          "form": "오오 밍정이도 신청해놓은 피티가 있구나!!!",
          "original_form": "오오 밍정이도 신청해놓은 피티가 있구나!!!",
          "speaker_id": "2",
          "time": "20210830 13:52"
        },
        {
          "id": "MDRW2100023593.1.16",
          "form": "막 옷 갈아입어야하고 그런 것도 귀찮은거같아,,,",
          "original_form": "막 옷 갈아입어야하고 그런 것도 귀찮은거같아,,,",
          "speaker_id": "2",
          "time": "20210830 13:52"
        },
        {
          "id": "MDRW2100023593.1.17",
          "form": "ㅋㅋㅋ원래 헬스장까지 기어가는게 젤 귀찮은 법이라더라구",
          "original_form": "ㅋㅋㅋ원래 헬스장까지 기어가는게 젤 귀찮은 법이라더라구",
          "speaker_id": "1",
          "time": "20210830 13:53"
        },
        {
          "id": "MDRW2100023593.1.18",
          "form": "웃기지만..넘나 트루..",
          "original_form": "웃기지만..넘나 트루..",
          "speaker_id": "1",
          "time": "20210830 13:53"
        },
        {
          "id": "MDRW2100023593.1.19",
          "form": "맞아 진짜",
          "original_form": "맞아 진짜",
          "speaker_id": "2",
          "time": "20210830 14:09"
        },
        {
          "id": "MDRW2100023593.1.20",
          "form": "나도 돈까지 냈는데",
          "original_form": "나도 돈까지 냈는데",
          "speaker_id": "2",
          "time": "20210830 14:09"
        },
        {
          "id": "MDRW2100023593.1.21",
          "form": "어떻게 이렇게 안가려고 할 수 있지?",
          "original_form": "어떻게 이렇게 안가려고 할 수 있지?",
          "speaker_id": "2",
          "time": "20210830 14:09"
        },
        {
          "id": "MDRW2100023593.1.22",
          "form": "내가 너무 대단해...",
          "original_form": "내가 너무 대단해...",
          "speaker_id": "2",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100023593.1.23",
          "form": "운동해야하는데....",
          "original_form": "운동해야하는데....",
          "speaker_id": "2",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100023593.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100023593.1.25",
          "form": "일주일에 몇번이야?",
          "original_form": "일주일에 몇번이야?",
          "speaker_id": "1",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100023593.1.26",
          "form": "정해진 건 없는데",
          "original_form": "정해진 건 없는데",
          "speaker_id": "2",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100023593.1.27",
          "form": "자기가 그 날 그 날 예약해서",
          "original_form": "자기가 그 날 그 날 예약해서",
          "speaker_id": "2",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100023593.1.28",
          "form": "갈 수 있는 날 가는거야!",
          "original_form": "갈 수 있는 날 가는거야!",
          "speaker_id": "2",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100023593.1.29",
          "form": "무제한으로 수업을 듣는거야??",
          "original_form": "무제한으로 수업을 듣는거야??",
          "speaker_id": "1",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100023593.1.30",
          "form": "난 일단 수요일 금요일 밤 9시로 해놨어",
          "original_form": "난 일단 수요일 금요일 밤 9시로 해놨어",
          "speaker_id": "2",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100023593.1.31",
          "form": "아아 25회 정해져있어!",
          "original_form": "아아 25회 정해져있어!",
          "speaker_id": "2",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100023593.1.32",
          "form": "3개월 동안 25회인데",
          "original_form": "3개월 동안 25회인데",
          "speaker_id": "2",
          "time": "20210830 14:10"
        },
        {
          "id": "MDRW2100023593.1.33",
          "form": "아직 한 번도 안감,,,",
          "original_form": "아직 한 번도 안감,,,",
          "speaker_id": "2",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100023593.1.34",
          "form": "아아 그렇구나",
          "original_form": "아아 그렇구나",
          "speaker_id": "1",
          "time": "20210830 14:12"
        },
        {
          "id": "MDRW2100023593.1.35",
          "form": "운동 귀찮지만 주 1회만이라도 가보쟈아",
          "original_form": "운동 귀찮지만 주 1회만이라도 가보쟈아",
          "speaker_id": "1",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100023593.1.36",
          "form": "응응 운동은 정말 시작이 반인 것 같아!!!",
          "original_form": "응응 운동은 정말 시작이 반인 것 같아!!!",
          "speaker_id": "2",
          "time": "20210830 14:16"
        },
        {
          "id": "MDRW2100023593.1.37",
          "form": "화이팅하자아아",
          "original_form": "화이팅하자아아",
          "speaker_id": "1",
          "time": "20210830 14:22"
        },
        {
          "id": "MDRW2100023593.1.38",
          "form": "응응!!!!",
          "original_form": "응응!!!!",
          "speaker_id": "2",
          "time": "20210830 14:29"
        },
        {
          "id": "MDRW2100023593.1.39",
          "form": "운동을 좋아해버릇 해야지",
          "original_form": "운동을 좋아해버릇 해야지",
          "speaker_id": "2",
          "time": "20210830 14:29"
        },
        {
          "id": "MDRW2100023593.1.40",
          "form": "뭐든지 습관이 중요한거같아 ㅎㅎㅎ",
          "original_form": "뭐든지 습관이 중요한거같아 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:29"
        },
        {
          "id": "MDRW2100023593.1.41",
          "form": "맞습니당",
          "original_form": "맞습니당",
          "speaker_id": "1",
          "time": "20210830 14:47"
        },
        {
          "id": "MDRW2100023593.1.42",
