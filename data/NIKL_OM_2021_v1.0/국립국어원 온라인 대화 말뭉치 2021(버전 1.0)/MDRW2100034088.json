        },
        {
          "id": "MDRW2100034088.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100034088.1.3",
          "form": "점심 드셨나용",
          "original_form": "점심 드셨나용",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100034088.1.4",
          "form": "네 점심드셨어요?",
          "original_form": "네 점심드셨어요?",
          "speaker_id": "1",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100034088.1.5",
          "form": "네 저도 먹었어요",
          "original_form": "네 저도 먹었어요",
          "speaker_id": "2",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100034088.1.6",
          "form": "어떤거 드셨나요?",
          "original_form": "어떤거 드셨나요?",
          "speaker_id": "2",
          "time": "20210827 15:35"
        },
        {
          "id": "MDRW2100034088.1.7",
          "form": "전 백반먹엇어요 name2님은요?",
          "original_form": "전 백반먹엇어요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210827 15:37"
        },
        {
          "id": "MDRW2100034088.1.8",
          "form": "백반 맛있죠! 저는 볶음쌀국수 먹었어용",
          "original_form": "백반 맛있죠! 저는 볶음쌀국수 먹었어용",
          "speaker_id": "2",
          "time": "20210827 15:39"
        },
        {
          "id": "MDRW2100034088.1.9",
          "form": "볶음쌀국수!! 아 오랜만에 먹고싶네요",
          "original_form": "볶음쌀국수!! 아 오랜만에 먹고싶네요",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100034088.1.10",
          "form": "좋아하시나요? 맛있는데 쌀국수는 배가 금방 꺼지네요",
          "original_form": "좋아하시나요? 맛있는데 쌀국수는 배가 금방 꺼지네요",
          "speaker_id": "2",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100034088.1.11",
          "form": "조금 배고픈 것 같아요..ㅎㅎ",
          "original_form": "조금 배고픈 것 같아요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100034088.1.12",
          "form": "맞아요 면 너무맛잇는데 배가 빨리 꺼짐 ㅠㅠ",
          "original_form": "맞아요 면 너무맛잇는데 배가 빨리 꺼짐 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100034088.1.13",
          "form": "그쵸 ..저녁은 뭐드실 예정이신가요?",
          "original_form": "그쵸 ..저녁은 뭐드실 예정이신가요?",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100034088.1.14",
          "form": "아직 메뉴 고민중이에요 name2님은요?",
          "original_form": "아직 메뉴 고민중이에요 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100034088.1.15",
          "form": "저는 오늘 회 먹기로했어요!",
          "original_form": "저는 오늘 회 먹기로했어요!",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100034088.1.16",
          "form": "회 좋아하시나요?",
          "original_form": "회 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100034088.1.17",
          "form": "회 너무 좋아해요",
          "original_form": "회 너무 좋아해요",
          "speaker_id": "1",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100034088.1.18",
          "form": "무슨 회 드시나요?",
          "original_form": "무슨 회 드시나요?",
          "speaker_id": "1",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100034088.1.19",
          "form": "노량진에서 배달도 되더라구요! 모둠회 먹을 것 같아요ㅎㅎ",
          "original_form": "노량진에서 배달도 되더라구요! 모둠회 먹을 것 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:55"
        },
        {
          "id": "MDRW2100034088.1.20",
          "form": "아 저도 회먹은지 좀 됏는데 군침도네요 ㅎ",
          "original_form": "아 저도 회먹은지 좀 됏는데 군침도네요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 15:56"
        }
