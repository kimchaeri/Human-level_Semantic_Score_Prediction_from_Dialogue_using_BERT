      "utterance": [
        {
          "id": "MDRW2100040548.1.1",
          "form": "김치도 혹시 담그실줄 아시나요..?",
          "original_form": "김치도 혹시 담그실줄 아시나요..?",
          "speaker_id": "1",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100040548.1.2",
          "form": "다른건 미역국정도?",
          "original_form": "다른건 미역국정도?",
          "speaker_id": "2",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100040548.1.3",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100040548.1.4",
          "form": "걍 얻어먹거나 사서 씁니다",
          "original_form": "걍 얻어먹거나 사서 씁니다",
          "speaker_id": "2",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100040548.1.5",
          "form": "역시 그게 최고인거같아요 ㅋㅋㅋㅋ",
          "original_form": "역시 그게 최고인거같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100040548.1.6",
          "form": "우리 어머니도 김치는 걍 사먹으라고 하시더라구요 ㅋㅋ ㅋ",
          "original_form": "우리 어머니도 김치는 걍 사먹으라고 하시더라구요 ㅋㅋ ㅋ",
          "speaker_id": "2",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100040548.1.7",
          "form": "사드신것중에 맛있는 김치있었나요?!",
          "original_form": "사드신것중에 맛있는 김치있었나요?!",
          "speaker_id": "1",
          "time": "20210904 22:30"
        },
        {
          "id": "MDRW2100040548.1.8",
          "form": "비비고 괜찮았던것 같아요",
          "original_form": "비비고 괜찮았던것 같아요",
          "speaker_id": "2",
          "time": "20210904 22:31"
        },
        {
          "id": "MDRW2100040548.1.9",
          "form": "조선호텔김치 드셔보셨어요?",
          "original_form": "조선호텔김치 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210904 22:32"
        },
        {
          "id": "MDRW2100040548.1.10",
          "form": "음...제 돈주고 사본적은 없는데",
          "original_form": "음...제 돈주고 사본적은 없는데",
          "speaker_id": "2",
          "time": "20210904 22:32"
        },
        {
          "id": "MDRW2100040548.1.11",
          "form": "다들 맛있다하던데 아직 안먹어봤어요 ㅎㅎ",
          "original_form": "다들 맛있다하던데 아직 안먹어봤어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 22:32"
        },
        {
          "id": "MDRW2100040548.1.12",
          "form": "어디서 얻어 먹었을수는 있지만 아마 없을걸요",
          "original_form": "어디서 얻어 먹었을수는 있지만 아마 없을걸요",
          "speaker_id": "2",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100040548.1.13",
          "form": "맛있고 비싼걸로 유명하죠",
          "original_form": "맛있고 비싼걸로 유명하죠",
          "speaker_id": "2",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100040548.1.14",
          "form": "달다는 평이있더라구요",
          "original_form": "달다는 평이있더라구요",
          "speaker_id": "1",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100040548.1.15",
          "form": "저도 궁금하긴한데 비싸서 손이 안갔어요",
          "original_form": "저도 궁금하긴한데 비싸서 손이 안갔어요",
          "speaker_id": "2",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100040548.1.16",
          "form": "그래서 선뜻 못시키겠어요 ㅜ",
          "original_form": "그래서 선뜻 못시키겠어요 ㅜ",
          "speaker_id": "1",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100040548.1.17",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "2",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100040548.1.18",
          "form": "저도 단 김치는 별론데...",
          "original_form": "저도 단 김치는 별론데...",
          "speaker_id": "2",
          "time": "20210904 22:33"
        },
        {
          "id": "MDRW2100040548.1.19",
          "form": "네 좀 단편이라고 하더라구요",
          "original_form": "네 좀 단편이라고 하더라구요",
          "speaker_id": "1",
          "time": "20210904 22:34"
        }
