      "utterance": [
        {
          "id": "MDRW2100041763.1.1",
          "form": "양말 다 빵꾸났어요",
          "original_form": "양말 다 빵꾸났어요",
          "speaker_id": "1",
          "time": "20210906 15:36"
        },
        {
          "id": "MDRW2100041763.1.2",
          "form": "꼬매신어",
          "original_form": "꼬매신어",
          "speaker_id": "2",
          "time": "20210906 15:36"
        },
        {
          "id": "MDRW2100041763.1.3",
          "form": "새걸로 사야죠 맨날 기워입구 ㅠ",
          "original_form": "새걸로 사야죠 맨날 기워입구 ㅠ",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041763.1.4",
          "form": "돈을 물처럼 쓰냐",
          "original_form": "돈을 물처럼 쓰냐",
          "speaker_id": "2",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041763.1.5",
          "form": "양말정도는 별로 비싸지도 않은걸요",
          "original_form": "양말정도는 별로 비싸지도 않은걸요",
          "speaker_id": "1",
          "time": "20210906 15:37"
        },
        {
          "id": "MDRW2100041763.1.6",
          "form": "그런것부터 아껴야 해",
          "original_form": "그런것부터 아껴야 해",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041763.1.7",
          "form": "흥",
          "original_form": "흥",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041763.1.8",
          "form": "아무튼엄만 맨날 캐릭터양말만 사오고",
          "original_form": "아무튼엄만 맨날 캐릭터양말만 사오고",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041763.1.9",
          "form": "귀엽잖아",
          "original_form": "귀엽잖아",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041763.1.10",
          "form": "그게왜",
          "original_form": "그게왜",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041763.1.11",
          "form": "촌시럽다구요",
          "original_form": "촌시럽다구요",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041763.1.12",
          "form": "별개 다 촌스럽대",
          "original_form": "별개 다 촌스럽대",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041763.1.13",
          "form": "요즘 시대에 먹히는 촌스러움은 꽃무늬 정도죠 ㅋ",
          "original_form": "요즘 시대에 먹히는 촌스러움은 꽃무늬 정도죠 ㅋ",
          "speaker_id": "1",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041763.1.14",
          "form": "지 취향이 더 촌스러우면서",
          "original_form": "지 취향이 더 촌스러우면서",
          "speaker_id": "2",
          "time": "20210906 15:40"
        },
        {
          "id": "MDRW2100041763.1.15",
          "form": "꽃무늬 예쁘거든요!!",
          "original_form": "꽃무늬 예쁘거든요!!",
          "speaker_id": "1",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100041763.1.16",
          "form": "너 지갑도 꽃이잖아",
          "original_form": "너 지갑도 꽃이잖아",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100041763.1.17",
          "form": "고등학교때 산거 오래 쓰고 있다구요",
          "original_form": "고등학교때 산거 오래 쓰고 있다구요",
          "speaker_id": "1",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100041763.1.18",
          "form": "잘 쓰면 됐다",
          "original_form": "잘 쓰면 됐다",
          "speaker_id": "2",
          "time": "20210906 16:17"
        },
        {
          "id": "MDRW2100041763.1.19",
          "form": "ㅎㅎ 아무튼 꽃무늬는 좋은데... 그건 좀 그래요",
          "original_form": "ㅎㅎ 아무튼 꽃무늬는 좋은데... 그건 좀 그래요",
          "speaker_id": "1",
          "time": "20210906 16:17"
        },
        {
          "id": "MDRW2100041763.1.20",
          "form": "뭐가 그래 취향 존중해",
          "original_form": "뭐가 그래 취향 존중해",
          "speaker_id": "2",
          "time": "20210906 16:17"
        },
        {
          "id": "MDRW2100041763.1.21",
          "form": "그런말두 아시냐구요 ㅋㅋㅋ",
          "original_form": "그런말두 아시냐구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:17"
        }
      ]
