      "utterance": [
        {
          "id": "MDRW2100002232.1.1",
          "form": "최근에 양궁 경기를 보고 양궁에 빠졌어",
          "original_form": "최근에 양궁 경기를 보고 양궁에 빠졌어",
          "speaker_id": "1",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002232.1.2",
          "form": "사실 영상은 잘 찾아보지 않아. 올림픽 때 본 게 다야.",
          "original_form": "사실 영상은 잘 찾아보지 않아. 올림픽 때 본 게 다야.",
          "speaker_id": "1",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002232.1.3",
          "form": "양궁카페 가보자!",
          "original_form": "양궁카페 가보자!",
          "speaker_id": "2",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002232.1.4",
          "form": "하지만 그 여운은 여전하단다",
          "original_form": "하지만 그 여운은 여전하단다",
          "speaker_id": "1",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002232.1.5",
          "form": "오 그거 정말 좋은 일이야.",
          "original_form": "오 그거 정말 좋은 일이야.",
          "speaker_id": "1",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002232.1.6",
          "form": "안산선수 너무 멋있어",
          "original_form": "안산선수 너무 멋있어",
          "speaker_id": "2",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002232.1.7",
          "form": "유퀴즈 나온거 봣니",
          "original_form": "유퀴즈 나온거 봣니",
          "speaker_id": "2",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002232.1.8",
          "form": "맞아.",
          "original_form": "맞아.",
          "speaker_id": "1",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002232.1.9",
          "form": "난 아직 못봤어",
          "original_form": "난 아직 못봤어",
          "speaker_id": "2",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002232.1.10",
          "form": "아직 못봤는데 보려고",
          "original_form": "아직 못봤는데 보려고",
          "speaker_id": "1",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002232.1.11",
          "form": "아 너도?",
          "original_form": "아 너도?",
          "speaker_id": "1",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002232.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ너무 바쁘다야",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ너무 바쁘다야",
          "speaker_id": "2",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002232.1.13",
          "form": "그럼 오늘 대화가 끝난 후 각자 보도록 하자.",
          "original_form": "그럼 오늘 대화가 끝난 후 각자 보도록 하자.",
          "speaker_id": "1",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002232.1.14",
          "form": "할 게 많네.",
          "original_form": "할 게 많네.",
          "speaker_id": "1",
          "time": "20210820 00:57"
        },
        {
          "id": "MDRW2100002232.1.15",
          "form": "그래그래 좋아 그리고 다시 대화를 나누자",
          "original_form": "그래그래 좋아 그리고 다시 대화를 나누자",
          "speaker_id": "2",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002232.1.16",
          "form": "양궁카페는 시내에 하나 잇더라",
          "original_form": "양궁카페는 시내에 하나 잇더라",
          "speaker_id": "2",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002232.1.17",
          "form": "그래. 감상후기에 대해 이야기 나누자.",
          "original_form": "그래. 감상후기에 대해 이야기 나누자.",
          "speaker_id": "1",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002232.1.18",
          "form": "시내라면 괜찬ㅎ은 위치네.",
          "original_form": "시내라면 괜찬ㅎ은 위치네.",
          "speaker_id": "1",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002232.1.19",
          "form": "우리의 스포츠 호기심을 충족시킬 좋은 기회야.",
          "original_form": "우리의 스포츠 호기심을 충족시킬 좋은 기회야.",
          "speaker_id": "1",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002232.1.20",
          "form": "교보 근처거든 나중에 같이 가보자",
          "original_form": "교보 근처거든 나중에 같이 가보자",
          "speaker_id": "2",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002232.1.21",
          "form": "정말 가깝네. 좋아.",
          "original_form": "정말 가깝네. 좋아.",
          "speaker_id": "1",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002232.1.22",
          "form": "그러게 이렇게 직접 해보는거 좋은거같아",
          "original_form": "그러게 이렇게 직접 해보는거 좋은거같아",
          "speaker_id": "2",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002232.1.23",
          "form": "빨리 코로나가 끝나서 스포츠 관람을 하고싶어.",
          "original_form": "빨리 코로나가 끝나서 스포츠 관람을 하고싶어.",
          "speaker_id": "1",
          "time": "20210820 00:58"
        },
        {
          "id": "MDRW2100002232.1.24",
          "form": "양궁은 관람할 수 없겠지만",
          "original_form": "양궁은 관람할 수 없겠지만",
          "speaker_id": "1",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002232.1.25",
          "form": "다른 좋은 기회들이 있겠지",
          "original_form": "다른 좋은 기회들이 있겠지",
          "speaker_id": "1",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002232.1.26",
          "form": "그러게 너는 야구 직관하는거 좋아하니?",
          "original_form": "그러게 너는 야구 직관하는거 좋아하니?",
          "speaker_id": "2",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002232.1.27",
          "form": "나는 치킨을 먹다가 온 기억밖에 없긴 해.",
          "original_form": "나는 치킨을 먹다가 온 기억밖에 없긴 해.",
          "speaker_id": "1",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002232.1.28",
          "form": "그게다지뭐",
          "original_form": "그게다지뭐",
          "speaker_id": "2",
          "time": "20210820 00:59"
        },
        {
          "id": "MDRW2100002232.1.29",
          "form": "치맥이 참 맛있었어.",
          "original_form": "치맥이 참 맛있었어.",
          "speaker_id": "1",
          "time": "20210820 00:59"
        }
      ]
    }
