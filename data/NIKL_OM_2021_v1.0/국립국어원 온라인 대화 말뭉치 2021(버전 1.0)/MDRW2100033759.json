          "speaker_id": "2",
          "time": "20210825 00:57"
        },
        {
          "id": "MDRW2100033759.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:57"
        },
        {
          "id": "MDRW2100033759.1.18",
          "form": "잠만 오늘 한끼냐구요",
