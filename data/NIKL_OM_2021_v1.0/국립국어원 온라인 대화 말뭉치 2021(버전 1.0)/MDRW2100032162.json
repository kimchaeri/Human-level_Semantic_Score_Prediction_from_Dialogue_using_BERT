      "utterance": [
        {
          "id": "MDRW2100032162.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210905 13:34"
        },
        {
          "id": "MDRW2100032162.1.2",
          "form": "저는 최애과자가 화이트하임이에요",
          "original_form": "저는 최애과자가 화이트하임이에요",
          "speaker_id": "1",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100032162.1.3",
          "form": "어머머, 참미각이시네요",
          "original_form": "어머머, 참미각이시네요",
          "speaker_id": "2",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100032162.1.4",
          "form": "화이트하임 맛있죠",
          "original_form": "화이트하임 맛있죠",
          "speaker_id": "2",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100032162.1.5",
          "form": "최애 과자 이신가요?ㅋㅋㅋㅋㅋ",
          "original_form": "최애 과자 이신가요?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100032162.1.6",
          "form": "반전으로 최애과자는 포카칩입니다",
          "original_form": "반전으로 최애과자는 포카칩입니다",
          "speaker_id": "2",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100032162.1.7",
          "form": "닉네임 만들 적에 먹던게 화이트하임이었어요",
          "original_form": "닉네임 만들 적에 먹던게 화이트하임이었어요",
          "speaker_id": "2",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100032162.1.8",
          "form": "아 그렇군욬ㅋㅋㅋㅋㅋ",
          "original_form": "아 그렇군욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100032162.1.9",
          "form": "저도 쿠크다스는 그렇게 잘 먹진않아욬ㅋㅋㅋㅋㅋ",
          "original_form": "저도 쿠크다스는 그렇게 잘 먹진않아욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032162.1.10",
          "form": "ㅋㅋㅋㅋㅋ 과자 말고 밥메뉴는 어떤거 좋아하세요? 디저트나 음료도 상관없어요",
          "original_form": "ㅋㅋㅋㅋㅋ 과자 말고 밥메뉴는 어떤거 좋아하세요? 디저트나 음료도 상관없어요",
          "speaker_id": "2",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100032162.1.11",
          "form": "밥메뉴는 음...저는 돼지고기두루치기 좋아해욬ㅋㅋㅋㅋ",
          "original_form": "밥메뉴는 음...저는 돼지고기두루치기 좋아해욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100032162.1.12",
          "form": "밥메뉴 뭐 좋아하세요?",
          "original_form": "밥메뉴 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 13:43"
        },
        {
          "id": "MDRW2100032162.1.13",
          "form": "저는 김파사에 파는 것들 좋아해요",
          "original_form": "저는 김파사에 파는 것들 좋아해요",
          "speaker_id": "2",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032162.1.14",
          "form": "떡볶이 김밥 오므라이스 이런것들요ㅎㅎ",
          "original_form": "떡볶이 김밥 오므라이스 이런것들요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032162.1.15",
          "form": "오 다양하게 좋아하시나봐요",
          "original_form": "오 다양하게 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032162.1.16",
          "form": "저도 김파사메뉴 다 좋아하는거같네욬ㅋㅋㅋㅋ",
          "original_form": "저도 김파사메뉴 다 좋아하는거같네욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:44"
        },
        {
          "id": "MDRW2100032162.1.17",
          "form": "네넹ㅎㅎ 글로벌하게 나가면 인도 멕시코 베트남도 좋아합니다",
          "original_form": "네넹ㅎㅎ 글로벌하게 나가면 인도 멕시코 베트남도 좋아합니다",
          "speaker_id": "2",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100032162.1.18",
          "form": "태국 스페인도ㅎㅎ",
          "original_form": "태국 스페인도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 13:47"
        },
        {
          "id": "MDRW2100032162.1.19",
          "form": "오 음식가리지않고 드시나봐요",
          "original_form": "오 음식가리지않고 드시나봐요",
          "speaker_id": "1",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100032162.1.20",
          "form": "향신료쎈음식도 잘 드셔요?",
          "original_form": "향신료쎈음식도 잘 드셔요?",
          "speaker_id": "1",
          "time": "20210905 13:48"
        },
        {
          "id": "MDRW2100032162.1.21",
          "form": "가리지 않는 것 같아요ㅋㅋㅋ 대신 고수 같은건 무리",
          "original_form": "가리지 않는 것 같아요ㅋㅋㅋ 대신 고수 같은건 무리",
          "speaker_id": "2",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100032162.1.22",
          "form": "진짜 비누를 먹으면 그런 느낌 아닐지",
          "original_form": "진짜 비누를 먹으면 그런 느낌 아닐지",
          "speaker_id": "2",
          "time": "20210905 13:50"
        },
        {
          "id": "MDRW2100032162.1.23",
          "form": "ㅋㅋㅋㅋㅋ저두 고수는....",
          "original_form": "ㅋㅋㅋㅋㅋ저두 고수는....",
          "speaker_id": "1",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100032162.1.24",
          "form": "쌀국수 국물도 잘 못마셔욬ㅋㅋㅋ",
          "original_form": "쌀국수 국물도 잘 못마셔욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:51"
        },
        {
          "id": "MDRW2100032162.1.25",
          "form": "ㅋㅋㅋㅋㅋ 디저트는 좋아하시는 거 있으세요?",
          "original_form": "ㅋㅋㅋㅋㅋ 디저트는 좋아하시는 거 있으세요?",
          "speaker_id": "2",
          "time": "20210905 13:52"
        },
        {
          "id": "MDRW2100032162.1.26",
          "form": "저는 빵 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "저는 빵 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:54"
        },
        {
          "id": "MDRW2100032162.1.27",
          "form": "오, 종류 안 가리시나요? 특히 좋아하시는 빵은요?",
          "original_form": "오, 종류 안 가리시나요? 특히 좋아하시는 빵은요?",
          "speaker_id": "2",
          "time": "20210905 13:56"
        },
        {
          "id": "MDRW2100032162.1.28",
          "form": "음 팥빵빼고는 다 잘먹어요ㅋㅋㅋㅋ",
          "original_form": "음 팥빵빼고는 다 잘먹어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032162.1.29",
          "form": "크로와상이나 버터프레첼좋아해요!",
          "original_form": "크로와상이나 버터프레첼좋아해요!",
          "speaker_id": "1",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032162.1.30",
          "form": "오, 팥빵은 취향 아니시구나",
          "original_form": "오, 팥빵은 취향 아니시구나",
          "speaker_id": "2",
          "time": "20210905 14:00"
        },
        {
          "id": "MDRW2100032162.1.31",
          "form": "저는 타르트류 좋아해요 위는 촉촉 아래는 바삭",
          "original_form": "저는 타르트류 좋아해요 위는 촉촉 아래는 바삭",
          "speaker_id": "2",
          "time": "20210905 14:00"
        },
        {
          "id": "MDRW2100032162.1.32",
          "form": "아 저도 타르트 좋아해요 ㅠㅠ 종류도 다양하고 먹은재미잇어요",
