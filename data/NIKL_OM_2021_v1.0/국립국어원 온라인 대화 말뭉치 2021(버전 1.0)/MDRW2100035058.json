      "utterance": [
        {
          "id": "MDRW2100035058.1.1",
          "form": "나 내일 소개팅해",
          "original_form": "나 내일 소개팅해",
          "speaker_id": "2",
          "time": "20210829 01:23"
        },
        {
          "id": "MDRW2100035058.1.2",
          "form": "무척 오랜만이네",
          "original_form": "무척 오랜만이네",
          "speaker_id": "1",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100035058.1.3",
          "form": "그래서 기대를 많이 하고 있어",
          "original_form": "그래서 기대를 많이 하고 있어",
          "speaker_id": "2",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100035058.1.4",
          "form": "너 이상형은 어떻게 되니",
          "original_form": "너 이상형은 어떻게 되니",
          "speaker_id": "1",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100035058.1.5",
          "form": "사실 이상형이 까다로워서 내일 걱정이야",
          "original_form": "사실 이상형이 까다로워서 내일 걱정이야",
          "speaker_id": "2",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100035058.1.6",
          "form": "도대체 어떻길래 그래",
          "original_form": "도대체 어떻길래 그래",
          "speaker_id": "1",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100035058.1.7",
          "form": "일단 키가 백 78 이 넘어야 돼",
          "original_form": "일단 키가 백 78 이 넘어야 돼",
          "speaker_id": "2",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100035058.1.8",
          "form": "키 큰 사람 좋아하는구나",
          "original_form": "키 큰 사람 좋아하는구나",
          "speaker_id": "1",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100035058.1.9",
          "form": "얼굴은 귀여웠으면 좋겠어",
          "original_form": "얼굴은 귀여웠으면 좋겠어",
          "speaker_id": "2",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100035058.1.10",
          "form": "귀염 상 좋아하는구나",
          "original_form": "귀염 상 좋아하는구나",
          "speaker_id": "1",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100035058.1.11",
          "form": "성격은 재밌는 사람이 좋아",
          "original_form": "성격은 재밌는 사람이 좋아",
          "speaker_id": "2",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100035058.1.12",
          "form": "나는 재밌는 사람 별로야",
          "original_form": "나는 재밌는 사람 별로야",
          "speaker_id": "1",
          "time": "20210829 01:24"
        },
        {
          "id": "MDRW2100035058.1.13",
          "form": "재밌으면서 다정한 사람이 최고야",
          "original_form": "재밌으면서 다정한 사람이 최고야",
          "speaker_id": "2",
          "time": "20210829 01:25"
        },
        {
          "id": "MDRW2100035058.1.14",
          "form": "너 말대로 까다롭기는 하네",
          "original_form": "너 말대로 까다롭기는 하네",
          "speaker_id": "1",
