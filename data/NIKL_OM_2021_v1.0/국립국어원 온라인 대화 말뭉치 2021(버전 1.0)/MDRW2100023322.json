      "utterance": [
        {
          "id": "MDRW2100023322.1.1",
          "form": "재택근무 해본적 있으세요?",
          "original_form": "재택근무 해본적 있으세요?",
          "speaker_id": "1",
          "time": "20210830 00:03"
        },
        {
          "id": "MDRW2100023322.1.2",
          "form": "네 잠깐요 ㅋㅋ",
          "original_form": "네 잠깐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:07"
        },
        {
          "id": "MDRW2100023322.1.3",
          "form": "잠깐요를 잠깐만요로 봣네요 그래서 왜 말씀안하시나 했습니다",
          "original_form": "잠깐요를 잠깐만요로 봣네요 그래서 왜 말씀안하시나 했습니다",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100023322.1.4",
          "form": "저 재택중인데 좋습니다",
          "original_form": "저 재택중인데 좋습니다",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100023322.1.5",
          "form": "유튜브 엄청했네요",
          "original_form": "유튜브 엄청했네요",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100023322.1.6",
          "form": "아 재택 좋죠 ㅠㅠ 근데 일과 분리가 아니라 진짜 너무 집만 있던 기억이 있어요 ㅋㅋㅋ",
          "original_form": "아 재택 좋죠 ㅠㅠ 근데 일과 분리가 아니라 진짜 너무 집만 있던 기억이 있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100023322.1.7",
          "form": "유투브 재미나자나요 ㅋㅋ",
          "original_form": "유투브 재미나자나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:04"
        },
        {
          "id": "MDRW2100023322.1.8",
          "form": "네 ㅎㅎㅎ프리미엄 했더니 재택근무가 풍족해지고 있습니다",
          "original_form": "네 ㅎㅎㅎ프리미엄 했더니 재택근무가 풍족해지고 있습니다",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100023322.1.9",
          "form": "재택해도 지겹지 않아요",
          "original_form": "재택해도 지겹지 않아요",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100023322.1.10",
          "form": "재택해도 되는 직종이신가보네요",
          "original_form": "재택해도 되는 직종이신가보네요",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100023322.1.11",
          "form": "아 그렇진 않아용ㅋㅋㅋㅋ 프리미엄이라시면 혹시 인도인이신가요?",
          "original_form": "아 그렇진 않아용ㅋㅋㅋㅋ 프리미엄이라시면 혹시 인도인이신가요?",
          "speaker_id": "2",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100023322.1.12",
          "form": "아 저 아르헨티나요",
          "original_form": "아 저 아르헨티나요",
          "speaker_id": "1",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100023322.1.13",
          "form": "인도시군요?",
          "original_form": "인도시군요?",
          "speaker_id": "1",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100023322.1.14",
          "form": "아르헨티나도 있어요?ㅋㅋㅋㅋㅋ",
          "original_form": "아르헨티나도 있어요?ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100023322.1.15",
          "form": "아 전 아직 낫인도인입니다 ㅋㅋ",
          "original_form": "아 전 아직 낫인도인입니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:21"
        },
        {
          "id": "MDRW2100023322.1.16",
          "form": "네 아르헨티나 싸요",
          "original_form": "네 아르헨티나 싸요",
          "speaker_id": "1",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100023322.1.17",
          "form": "아르헨티나에서 재택도 가능합니다",
          "original_form": "아르헨티나에서 재택도 가능합니다",
          "speaker_id": "1",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100023322.1.18",
          "form": "우회해서요",
          "original_form": "우회해서요",
          "speaker_id": "1",
          "time": "20210830 22:26"
        },
        {
          "id": "MDRW2100023322.1.19",
          "form": "vpn쓰신거죠?ㅋ",
          "original_form": "vpn쓰신거죠?ㅋ",
          "speaker_id": "2",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100023322.1.20",
          "form": "네 튠베어인가",
          "original_form": "네 튠베어인가",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100023322.1.21",
          "form": "네이버 횽아들이 시키는데로 하니까 됐어요 ㅎㅎ",
          "original_form": "네이버 횽아들이 시키는데로 하니까 됐어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100023322.1.22",
          "form": "대단한형ㄷㅇㄹ",
          "original_form": "대단한형ㄷㅇㄹ",
          "speaker_id": "1",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100023322.1.23",
          "form": "ㅋㅋㅋㅋ 우리나라 사람들 너무 대단해요 진짜 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 우리나라 사람들 너무 대단해요 진짜 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100023322.1.24",
          "form": "재택하면서 이런저런 정보찾고 하니까",
          "original_form": "재택하면서 이런저런 정보찾고 하니까",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100023322.1.25",
          "form": "좋은것 같습니다",
          "original_form": "좋은것 같습니다",
          "speaker_id": "1",
          "time": "20210830 22:45"
        },
        {
          "id": "MDRW2100023322.1.26",
          "form": "ㅋㅋㅋㅋ저도 재택 시켜주세여 ㅋㅋㅋ 아 부러워요",
          "original_form": "ㅋㅋㅋㅋ저도 재택 시켜주세여 ㅋㅋㅋ 아 부러워요",
          "speaker_id": "2",
          "time": "20210830 22:50"
        },
        {
          "id": "MDRW2100023322.1.27",
          "form": "내일부터 재택하세요",
          "original_form": "내일부터 재택하세요",
          "speaker_id": "1",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100023322.1.28",
          "form": "명령입니다",
          "original_form": "명령입니다",
          "speaker_id": "1",
          "time": "20210830 22:55"
        },
        {
          "id": "MDRW2100023322.1.29",
          "form": "이렇게 백수의 길로",
          "original_form": "이렇게 백수의 길로",
          "speaker_id": "2",
          "time": "20210830 22:57"
        },
        {
          "id": "MDRW2100023322.1.30",
          "form": "아 업종이 무엇인가요?",
          "original_form": "아 업종이 무엇인가요?",
          "speaker_id": "1",
          "time": "20210830 22:59"
        },
        {
          "id": "MDRW2100023322.1.31",
          "form": "저요? 서비스직요 ㅠㅠ",
          "original_form": "저요? 서비스직요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100023322.1.32",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 23:03"
        },
        {
          "id": "MDRW2100023322.1.33",
          "form": "저는 음",
          "original_form": "저는 음",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100023322.1.34",
          "form": "복합멀티입니닼",
          "original_form": "복합멀티입니닼",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100023322.1.35",
          "form": "베이스는 기술직인데",
          "original_form": "베이스는 기술직인데",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100023322.1.36",
          "form": "행정업무도 하고",
          "original_form": "행정업무도 하고",
          "speaker_id": "1",
          "time": "20210830 23:04"
        },
        {
          "id": "MDRW2100023322.1.37",
          "form": "서비스도 하고",
          "original_form": "서비스도 하고",
          "speaker_id": "1",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100023322.1.38",
          "form": "이상하죠?",
          "original_form": "이상하죠?",
          "speaker_id": "1",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100023322.1.39",
          "form": "부러워 능력자자나여",
          "original_form": "부러워 능력자자나여",
          "speaker_id": "2",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100023322.1.40",
          "form": "나중에 굶어죽지 않으시겠어 역시 기술이 최고야..",
          "original_form": "나중에 굶어죽지 않으시겠어 역시 기술이 최고야..",
          "speaker_id": "2",
          "time": "20210830 23:05"
        },
        {
          "id": "MDRW2100023322.1.41",
          "form": "네 기사 자격증만 5개입니다",
          "original_form": "네 기사 자격증만 5개입니다",
          "speaker_id": "1",
          "time": "20210830 23:06"
        },
        {
          "id": "MDRW2100023322.1.42",
          "form": "멋지시네요 태오님",
          "original_form": "멋지시네요 태오님",
          "speaker_id": "2",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100023322.1.43",
          "form": "서비스 고생많으시겠어요",
          "original_form": "서비스 고생많으시겠어요",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100023322.1.44",
          "form": "사람 상대하는것이",
          "original_form": "사람 상대하는것이",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100023322.1.45",
          "form": "쉽지 않잖아요 맘고생 많으시구",
          "original_form": "쉽지 않잖아요 맘고생 많으시구",
          "speaker_id": "1",
          "time": "20210830 23:07"
        },
        {
          "id": "MDRW2100023322.1.46",
          "form": "그쵸 맞아요 하지만 박봉 ㅋㅋㅋ",
          "original_form": "그쵸 맞아요 하지만 박봉 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:21"
        },
        {
          "id": "MDRW2100023322.1.47",
          "form": "실례지만 어떤일 하시나요?",
          "original_form": "실례지만 어떤일 하시나요?",
          "speaker_id": "1",
          "time": "20210830 23:24"
        },
        {
          "id": "MDRW2100023322.1.48",
          "form": "저 고객센터 쪽요",
          "original_form": "저 고객센터 쪽요",
          "speaker_id": "2",
          "time": "20210830 23:38"
        },
        {
          "id": "MDRW2100023322.1.49",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100023322.1.50",
          "form": "압니다 제  사촌동생이 U+고객센터에서 일합니다",
          "original_form": "압니다 제  사촌동생이 U+고객센터에서 일합니다",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100023322.1.51",
          "form": "진상들이 많아서 조심하세요",
          "original_form": "진상들이 많아서 조심하세요",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100023322.1.52",
          "form": "원래 이쪽 일하던 사람은 아닌데요 지인이 한번 해보는게 어때해서 해봤어요 ㅋㅋㅋㅋ",
          "original_form": "원래 이쪽 일하던 사람은 아닌데요 지인이 한번 해보는게 어때해서 해봤어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100023322.1.53",
          "form": "진상진짜 많져 마자여 그래도 역시 뭐든 일에는 배울게 있어요~",
          "original_form": "진상진짜 많져 마자여 그래도 역시 뭐든 일에는 배울게 있어요~",
          "speaker_id": "2",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100023322.1.54",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "1",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100023322.1.55",
          "form": "재택은 어렵겠네요",
          "original_form": "재택은 어렵겠네요",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100023322.1.56",
          "form": "특성상",
          "original_form": "특성상",
          "speaker_id": "1",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100023322.1.57",
          "form": "재택 지금은 안될 거 같아요 ㅋㅋㅋ 재택 집이라 편한데 심심한 감이 있는데 또 회사나가니 즐겁기도 한거 같아요 ㅋ",
          "original_form": "재택 지금은 안될 거 같아요 ㅋㅋㅋ 재택 집이라 편한데 심심한 감이 있는데 또 회사나가니 즐겁기도 한거 같아요 ㅋ",
          "speaker_id": "2",
          "time": "20210831 00:08"
        },
        {
          "id": "MDRW2100023322.1.58",
          "form": "맞습니다 ㅋㅋㅋ동료랑 이야기하면서 즐거움이 배가 되요",
