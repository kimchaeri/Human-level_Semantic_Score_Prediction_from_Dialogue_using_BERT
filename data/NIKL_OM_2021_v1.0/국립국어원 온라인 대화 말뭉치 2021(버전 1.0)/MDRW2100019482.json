      "utterance": [
        {
          "id": "MDRW2100019482.1.1",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100019482.1.2",
          "form": "네!ㅎㅎㅎ 이번 도쿄올림픽 보셨어요?",
          "original_form": "네!ㅎㅎㅎ 이번 도쿄올림픽 보셨어요?",
          "speaker_id": "1",
          "time": "20210827 19:42"
        },
        {
          "id": "MDRW2100019482.1.3",
          "form": "네네 아주 하루종일 봤어요ㅋㅋㅋㅋㅋ 평소에 좋아하는편은 아닌데 올림픽 월드컵때만 되면 갑자기 애국자가 되가지구 ㅋㅋㅋ",
          "original_form": "네네 아주 하루종일 봤어요ㅋㅋㅋㅋㅋ 평소에 좋아하는편은 아닌데 올림픽 월드컵때만 되면 갑자기 애국자가 되가지구 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:43"
        },
        {
          "id": "MDRW2100019482.1.4",
          "form": "ㅋㅋㅋㅋ 엇 저도 그래요!",
          "original_form": "ㅋㅋㅋㅋ 엇 저도 그래요!",
          "speaker_id": "1",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019482.1.5",
          "form": "어떤 종목을 젤 재밌게 보셨나요?",
          "original_form": "어떤 종목을 젤 재밌게 보셨나요?",
          "speaker_id": "1",
          "time": "20210827 19:44"
        },
        {
          "id": "MDRW2100019482.1.6",
          "form": "다 재미있었는데 양궁이랑 여자배구요!",
          "original_form": "다 재미있었는데 양궁이랑 여자배구요!",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019482.1.7",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 19:45"
        },
        {
          "id": "MDRW2100019482.1.8",
          "form": "저도 동감이요!",
          "original_form": "저도 동감이요!",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019482.1.9",
          "form": "그리고 클라이밍이랑 조깅 같은 비인기 종목도 구경했어요~",
          "original_form": "그리고 클라이밍이랑 조깅 같은 비인기 종목도 구경했어요~",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019482.1.10",
          "form": "저도 보기는 다 봤어요ㅋㅋㅋㅋ 근대5종은 아예 처음 들어봤는데 되게 어렵고 힘든 종목이더라구요",
          "original_form": "저도 보기는 다 봤어요ㅋㅋㅋㅋ 근대5종은 아예 처음 들어봤는데 되게 어렵고 힘든 종목이더라구요",
          "speaker_id": "2",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100019482.1.11",
          "form": "근대5종 하는 선수들 진짜 대단하지 않아요?",
          "original_form": "근대5종 하는 선수들 진짜 대단하지 않아요?",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019482.1.12",
          "form": "하나만 잘하기도 힘든데 여러가지를 동시에 잘해야 한다니..",
          "original_form": "하나만 잘하기도 힘든데 여러가지를 동시에 잘해야 한다니..",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100019482.1.13",
          "form": "그니까요 약간 사람이 아닌 느낌...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그니까요 약간 사람이 아닌 느낌...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019482.1.14",
          "form": "깜짝놀랐어요",
          "original_form": "깜짝놀랐어요",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019482.1.15",
          "form": "name2님은 올림픽 대회 나간다고 하면 어떤 종목 나가보고 싶어요?",
          "original_form": "&name2&님은 올림픽 대회 나간다고 하면 어떤 종목 나가보고 싶어요?",
          "speaker_id": "1",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100019482.1.16",
          "form": "왜 저를 갑자기 민폐덩어리로...ㅠㅠㅠㅠㅠ",
          "original_form": "왜 저를 갑자기 민폐덩어리로...ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100019482.1.17",
          "form": "그냥 집에 있으면 안될까요",
          "original_form": "그냥 집에 있으면 안될까요",
          "speaker_id": "2",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019482.1.18",
          "form": "ㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019482.1.19",
          "form": "ㅋㅋㅋㅋ 특별히 잘하는 운동은 없으신가봐요...?",
          "original_form": "ㅋㅋㅋㅋ 특별히 잘하는 운동은 없으신가봐요...?",
          "speaker_id": "1",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019482.1.20",
          "form": "죄소앻요....ㅋㅋㅋ",
          "original_form": "죄소앻요....ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019482.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋ name1님은 뭐 어떤거 출전하실거에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ &name1&님은 뭐 어떤거 출전하실거에요",
          "speaker_id": "2",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019482.1.22",
          "form": "출전할 수 있는 실력은 아니지만...",
          "original_form": "출전할 수 있는 실력은 아니지만...",
          "speaker_id": "1",
          "time": "20210827 19:54"
        },
        {
          "id": "MDRW2100019482.1.23",
          "form": "사격 같은거 해보고 싶어요 ㅋㅋㅋㅋ",
          "original_form": "사격 같은거 해보고 싶어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:54"
        },
        {
          "id": "MDRW2100019482.1.24",
