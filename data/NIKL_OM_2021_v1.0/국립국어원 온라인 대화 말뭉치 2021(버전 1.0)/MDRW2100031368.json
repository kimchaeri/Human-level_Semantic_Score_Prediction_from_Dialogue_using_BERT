      "utterance": [
        {
          "id": "MDRW2100031368.1.1",
          "form": "음 주로 흰살생선류요",
          "original_form": "음 주로 흰살생선류요",
          "speaker_id": "2",
          "time": "20210904 20:50"
        },
        {
          "id": "MDRW2100031368.1.2",
          "form": "흰살요? 음 광어같은?",
          "original_form": "흰살요? 음 광어같은?",
          "speaker_id": "1",
          "time": "20210904 20:50"
        },
        {
          "id": "MDRW2100031368.1.3",
          "form": "네 광어도 좋아하고..",
          "original_form": "네 광어도 좋아하고..",
          "speaker_id": "2",
          "time": "20210904 20:50"
        },
        {
          "id": "MDRW2100031368.1.4",
          "form": "광어 지느러미 좋아하세요?",
          "original_form": "광어 지느러미 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 20:51"
        },
        {
          "id": "MDRW2100031368.1.5",
          "form": "엄청 좋아하죠 쫄깃해서",
          "original_form": "엄청 좋아하죠 쫄깃해서",
          "speaker_id": "2",
          "time": "20210904 20:51"
        },
        {
          "id": "MDRW2100031368.1.6",
          "form": "저도 좋아해요 ㅎㅎ",
          "original_form": "저도 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 20:51"
        },
        {
          "id": "MDRW2100031368.1.7",
          "form": "연어도 좋아해요",
          "original_form": "연어도 좋아해요",
          "speaker_id": "2",
          "time": "20210904 20:51"
        },
        {
          "id": "MDRW2100031368.1.8",
          "form": "와 저도요 저는 훈제연어도 좋아하고 생연어도 좋아해요",
          "original_form": "와 저도요 저는 훈제연어도 좋아하고 생연어도 좋아해요",
          "speaker_id": "1",
          "time": "20210904 20:51"
        },
        {
          "id": "MDRW2100031368.1.9",
          "form": "연어는 어떻게 먹어도 맛있더라고요",
          "original_form": "연어는 어떻게 먹어도 맛있더라고요",
          "speaker_id": "2",
          "time": "20210904 20:51"
        },
        {
          "id": "MDRW2100031368.1.10",
          "form": "맞아요ㅠㅠ 저는 주로 덮밥해서 먹어요 연어는",
          "original_form": "맞아요ㅠㅠ 저는 주로 덮밥해서 먹어요 연어는",
          "speaker_id": "1",
          "time": "20210904 20:51"
        },
        {
          "id": "MDRW2100031368.1.11",
          "form": "덮밥은 어떻게 하는건가요?",
          "original_form": "덮밥은 어떻게 하는건가요?",
          "speaker_id": "2",
          "time": "20210904 20:51"
        },
        {
          "id": "MDRW2100031368.1.12",
          "form": "생연어 썰어서 간장소스 뿌려서 먹는거요!",
          "original_form": "생연어 썰어서 간장소스 뿌려서 먹는거요!",
          "speaker_id": "1",
          "time": "20210904 20:52"
        },
        {
          "id": "MDRW2100031368.1.13",
          "form": "간장소스는 사는건가요?",
          "original_form": "간장소스는 사는건가요?",
          "speaker_id": "2",
          "time": "20210904 20:53"
        },
        {
          "id": "MDRW2100031368.1.14",
          "form": "간장에 물이랑 설탕 넣고 끓이면 되는거요!",
          "original_form": "간장에 물이랑 설탕 넣고 끓이면 되는거요!",
          "speaker_id": "1",
          "time": "20210904 20:53"
        },
        {
          "id": "MDRW2100031368.1.15",
          "form": "달착지근 짭잘해서 맛있겠어요",
          "original_form": "달착지근 짭잘해서 맛있겠어요",
          "speaker_id": "2",
          "time": "20210904 20:53"
        }
      ]
