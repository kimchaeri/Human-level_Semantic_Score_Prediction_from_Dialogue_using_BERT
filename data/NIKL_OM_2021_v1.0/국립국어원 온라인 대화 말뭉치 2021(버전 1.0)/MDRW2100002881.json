      "utterance": [
        {
          "id": "MDRW2100002881.1.1",
          "form": "저는 집에 돌아가는길인데 벌써 땀나네요...",
          "original_form": "저는 집에 돌아가는길인데 벌써 땀나네요...",
          "speaker_id": "1",
          "time": "20210820 16:14"
        },
        {
          "id": "MDRW2100002881.1.2",
          "form": "너무 덥네요오늘은요",
          "original_form": "너무 덥네요오늘은요",
          "speaker_id": "2",
          "time": "20210820 16:15"
        },
        {
          "id": "MDRW2100002881.1.3",
          "form": "그러게요 오늘같이 더울땐 집에만 있는게 최곤데..",
          "original_form": "그러게요 오늘같이 더울땐 집에만 있는게 최곤데..",
          "speaker_id": "1",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002881.1.4",
          "form": "아님 계곡가고 싶어요 못가지만 ㅠㅠ",
          "original_form": "아님 계곡가고 싶어요 못가지만 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002881.1.5",
          "form": "집에만 있으니 살찌고ㅜ",
          "original_form": "집에만 있으니 살찌고ㅜ",
          "speaker_id": "2",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002881.1.6",
          "form": "운동하러가기도 힘드네요ㅜ",
          "original_form": "운동하러가기도 힘드네요ㅜ",
          "speaker_id": "2",
          "time": "20210820 16:16"
        },
        {
          "id": "MDRW2100002881.1.7",
          "form": "악..마쟈요 헬스장두 샤워가 안되니까",
          "original_form": "악..마쟈요 헬스장두 샤워가 안되니까",
          "speaker_id": "1",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002881.1.8",
          "form": "실내운동도 여의치않고",
          "original_form": "실내운동도 여의치않고",
          "speaker_id": "2",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002881.1.9",
          "form": "밖은 덥구요ㅋㅋ",
          "original_form": "밖은 덥구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002881.1.10",
          "form": "한 2주정도 런닝했는데..",
          "original_form": "한 2주정도 런닝했는데..",
          "speaker_id": "1",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002881.1.11",
          "form": "해지니까 그나마 할만하더라구요!",
          "original_form": "해지니까 그나마 할만하더라구요!",
          "speaker_id": "1",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002881.1.12",
          "form": "밤되서 나가야겠네요ㅋ",
          "original_form": "밤되서 나가야겠네요ㅋ",
          "speaker_id": "2",
          "time": "20210820 16:17"
        },
        {
          "id": "MDRW2100002881.1.13",
          "form": "ㅋㅋㅋㅋ해질때쯤 나가시면 일몰도 예뻐서 돌아다닐맛 나요",
          "original_form": "ㅋㅋㅋㅋ해질때쯤 나가시면 일몰도 예뻐서 돌아다닐맛 나요",
          "speaker_id": "1",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002881.1.14",
          "form": "물론 돌아오는길에 두손 무겁지 않게 조심...",
          "original_form": "물론 돌아오는길에 두손 무겁지 않게 조심...",
          "speaker_id": "1",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002881.1.15",
          "form": "그게 큰문제같아요ㅋㅋㅋ",
          "original_form": "그게 큰문제같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002881.1.16",
          "form": "그냥와야하는데",
          "original_form": "그냥와야하는데",
          "speaker_id": "2",
          "time": "20210820 16:21"
        },
        {
          "id": "MDRW2100002881.1.17",
          "form": "전에 런닝 한창하고있는데 삼겹살 트럭아져씨를 만난거예요...",
          "original_form": "전에 런닝 한창하고있는데 삼겹살 트럭아져씨를 만난거예요...",
          "speaker_id": "1",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002881.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ집에와서 삼겹살 먹었어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ집에와서 삼겹살 먹었어요",
          "speaker_id": "1",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002881.1.19",
          "form": "런닝은 그냥 입맛을 돋구기 위한..",
          "original_form": "런닝은 그냥 입맛을 돋구기 위한..",
          "speaker_id": "1",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002881.1.20",
          "form": "너무 큰 산인데요ㅋㅋㅋ",
          "original_form": "너무 큰 산인데요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002881.1.21",
          "form": "이건 못넘죠ㅋㅋㅋ당연히",
          "original_form": "이건 못넘죠ㅋㅋㅋ당연히",
          "speaker_id": "2",
          "time": "20210820 16:22"
        },
        {
          "id": "MDRW2100002881.1.22",
          "form": "그래서 양심상 제로콜라 샀어요...",
          "original_form": "그래서 양심상 제로콜라 샀어요...",
          "speaker_id": "1",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002881.1.23",
          "form": "맥주는 차마..ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맥주는 차마..ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 16:23"
        },
        {
          "id": "MDRW2100002881.1.24",
          "form": "맥주가 단짝인데요~~한고비는 넘기셨네요ㅋㅋ",
          "original_form": "맥주가 단짝인데요~~한고비는 넘기셨네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 16:24"
        },
        {
          "id": "MDRW2100002881.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ넵 다행이예요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ넵 다행이예요",
