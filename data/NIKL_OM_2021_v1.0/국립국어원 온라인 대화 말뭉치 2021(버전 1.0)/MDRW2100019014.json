      "utterance": [
        {
          "id": "MDRW2100019014.1.1",
          "form": "누구요?",
          "original_form": "누구요?",
          "speaker_id": "2",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019014.1.2",
          "form": "전에말한 돌비요!",
          "original_form": "전에말한 돌비요!",
          "speaker_id": "1",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019014.1.3",
          "form": "저도 한 번 들어볼까요 ㅎㅎ",
          "original_form": "저도 한 번 들어볼까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019014.1.4",
          "form": "ㅋㅋ 할 거 없으시면?",
          "original_form": "ㅋㅋ 할 거 없으시면?",
          "speaker_id": "1",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019014.1.5",
          "form": "추천 에피소드 있나요?",
          "original_form": "추천 에피소드 있나요?",
          "speaker_id": "2",
          "time": "20210827 14:40"
        },
        {
          "id": "MDRW2100019014.1.6",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019014.1.7",
          "form": "그 법률사무직 사람꺼 재밌어요",
          "original_form": "그 법률사무직 사람꺼 재밌어요",
          "speaker_id": "1",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019014.1.8",
          "form": "기억해둘게요 ㅎ",
          "original_form": "기억해둘게요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019014.1.9",
          "form": "지금은 조현병 얘기 듣는중~",
          "original_form": "지금은 조현병 얘기 듣는중~",
          "speaker_id": "1",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019014.1.10",
          "form": "그런 것도 올라오는 군요",
          "original_form": "그런 것도 올라오는 군요",
          "speaker_id": "2",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019014.1.11",
          "form": "네 신병이랑 조현병??",
          "original_form": "네 신병이랑 조현병??",
          "speaker_id": "1",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019014.1.12",
          "form": "그렇게 엮여있군요",
          "original_form": "그렇게 엮여있군요",
          "speaker_id": "2",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019014.1.13",
          "form": "네 그냥 킬링타임용으로 틀어놨어요 ㅋㅋ",
          "original_form": "네 그냥 킬링타임용으로 틀어놨어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019014.1.14",
          "form": "라디오처럼 들으시는 군요 ㅎㅎ",
          "original_form": "라디오처럼 들으시는 군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:42"
        },
        {
          "id": "MDRW2100019014.1.15",
          "form": "이제 무법변호사 다보고",
          "original_form": "이제 무법변호사 다보고",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019014.1.16",
          "form": "모두의 거짓말 봐요 ㅎㅎ",
          "original_form": "모두의 거짓말 봐요 ㅎㅎ",
