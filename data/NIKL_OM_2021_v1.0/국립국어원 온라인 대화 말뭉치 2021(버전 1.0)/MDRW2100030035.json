      "utterance": [
        {
          "id": "MDRW2100030035.1.1",
          "form": "거기에서는 4일 정도 있었구요",
          "original_form": "거기에서는 4일 정도 있었구요",
          "speaker_id": "2",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100030035.1.2",
          "form": "국립공원 안에 캠핑도 했었습니다",
          "original_form": "국립공원 안에 캠핑도 했었습니다",
          "speaker_id": "2",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100030035.1.3",
          "form": "알버타주 국립공원 공식 홈페이지에서 예약할 수가 있어요",
          "original_form": "알버타주 국립공원 공식 홈페이지에서 예약할 수가 있어요",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100030035.1.4",
          "form": "운 좋게 예약해서 해서",
          "original_form": "운 좋게 예약해서 해서",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100030035.1.5",
          "form": "캠핑장에서 물소도 봤어요",
          "original_form": "캠핑장에서 물소도 봤어요",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100030035.1.6",
          "form": "정말 자연친화적인 나라네요 캐나다..",
          "original_form": "정말 자연친화적인 나라네요 캐나다..",
          "speaker_id": "1",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100030035.1.7",
          "form": "스페인은 어떠셨어요?",
          "original_form": "스페인은 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210903 19:41"
        },
        {
          "id": "MDRW2100030035.1.8",
          "form": "스페인은 도시마다 느낌이 다르더라구요",
          "original_form": "스페인은 도시마다 느낌이 다르더라구요",
          "speaker_id": "2",
          "time": "20210903 19:49"
        },
        {
          "id": "MDRW2100030035.1.9",
          "form": "남부스페인은",
          "original_form": "남부스페인은",
          "speaker_id": "2",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100030035.1.10",
          "form": "이슬람 흔적이 많이 남아있어요",
          "original_form": "이슬람 흔적이 많이 남아있어요",
          "speaker_id": "2",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100030035.1.11",
          "form": "이슬람 양식에 건축물이 많아요",
          "original_form": "이슬람 양식에 건축물이 많아요",
          "speaker_id": "2",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100030035.1.12",
          "form": "되게 멋져 보였습니다",
          "original_form": "되게 멋져 보였습니다",
          "speaker_id": "2",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100030035.1.13",
          "form": "아 그리고 그 그라나다",
          "original_form": "아 그리고 그 그라나다",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100030035.1.14",
          "form": "가시면",
          "original_form": "가시면",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100030035.1.15",
          "form": "술한잔 드시면 안주가 하나 꽁짜",
          "original_form": "술한잔 드시면 안주가 하나 꽁짜",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100030035.1.16",
          "form": "또한 잔 시키면 또 꽁짜",
          "original_form": "또한 잔 시키면 또 꽁짜",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100030035.1.17",
          "form": "오 ㅋㅋ 좋네요 인심",
          "original_form": "오 ㅋㅋ 좋네요 인심",
          "speaker_id": "1",
          "time": "20210903 20:11"
        },
        {
          "id": "MDRW2100030035.1.18",
          "form": "스페인에 이슬람 양식이 있을줄은 몰랐네요",
          "original_form": "스페인에 이슬람 양식이 있을줄은 몰랐네요",
          "speaker_id": "1",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100030035.1.19",
          "form": "음식은 입에 맞으셨어요?",
          "original_form": "음식은 입에 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210903 20:12"
        },
        {
          "id": "MDRW2100030035.1.20",
          "form": "완전 맞아요",
          "original_form": "완전 맞아요",
          "speaker_id": "2",
          "time": "20210903 21:42"
        },
        {
          "id": "MDRW2100030035.1.21",
          "form": "잘 맞으실겁니다",
          "original_form": "잘 맞으실겁니다",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100030035.1.22",
          "form": "단짠임 ㅋㅋ",
          "original_form": "단짠임 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100030035.1.23",
          "form": "오 ㅋㅋ",
          "original_form": "오 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030035.1.24",
          "form": "캐나다랑 비교했을 때 어디가 더 맛있나요",
          "original_form": "캐나다랑 비교했을 때 어디가 더 맛있나요",
          "speaker_id": "1",
          "time": "20210904 11:05"
        },
        {
          "id": "MDRW2100030035.1.25",
          "form": "단연코 스페인 입니다 단연코 단연코 어",
          "original_form": "단연코 스페인 입니다 단연코 단연코 어",
          "speaker_id": "2",
          "time": "20210904 12:30"
        },
        {
          "id": "MDRW2100030035.1.26",
          "form": "우리랑 비슷한거 같아욬",
          "original_form": "우리랑 비슷한거 같아욬",
          "speaker_id": "2",
          "time": "20210904 12:31"
        },
        {
          "id": "MDRW2100030035.1.27",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "1",
          "time": "20210904 12:42"
        },
        {
          "id": "MDRW2100030035.1.28",
          "form": "캐나다 음식은 그냥 심심했나요?",
          "original_form": "캐나다 음식은 그냥 심심했나요?",
          "speaker_id": "1",
          "time": "20210904 12:43"
        },
        {
          "id": "MDRW2100030035.1.29",
          "form": "몇개 안무봐서 ㅠ",
          "original_form": "몇개 안무봐서 ㅠ",
          "speaker_id": "2",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030035.1.30",
          "form": "짠듯",
          "original_form": "짠듯",
          "speaker_id": "2",
          "time": "20210904 13:19"
        },
        {
          "id": "MDRW2100030035.1.31",
          "form": "아 그래여? 캐나다는 더 자연의 맛일줄 알았는데 ㅋㅋ",
          "original_form": "아 그래여? 캐나다는 더 자연의 맛일줄 알았는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:46"
        },
        {
          "id": "MDRW2100030035.1.32",
          "form": "스페인에서 뭐가 제일 좋으셨어요",
          "original_form": "스페인에서 뭐가 제일 좋으셨어요",
          "speaker_id": "1",
          "time": "20210904 13:47"
        },
        {
          "id": "MDRW2100030035.1.33",
          "form": "음",
          "original_form": "음",
          "speaker_id": "2",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100030035.1.34",
          "form": "남부 말라가 쪽가면",
          "original_form": "남부 말라가 쪽가면",
          "speaker_id": "2",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100030035.1.35",
          "form": "발코니 데 유로파",
          "original_form": "발코니 데 유로파",
          "speaker_id": "2",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100030035.1.36",
          "form": "라고 있는데",
          "original_form": "라고 있는데",
          "speaker_id": "2",
          "time": "20210904 13:55"
        },
        {
          "id": "MDRW2100030035.1.37",
          "form": "진짜 지중해 뷰가 지립니다",
          "original_form": "진짜 지중해 뷰가 지립니다",
          "speaker_id": "2",
          "time": "20210904 13:56"
        },
        {
          "id": "MDRW2100030035.1.38",
          "form": "아..,지중해",
          "original_form": "아..,지중해",
          "speaker_id": "1",
          "time": "20210904 14:16"
        },
        {
          "id": "MDRW2100030035.1.39",
          "form": "보고만 있어도 가슴이 탁 트이는 기분이겠네요",
          "original_form": "보고만 있어도 가슴이 탁 트이는 기분이겠네요",
          "speaker_id": "1",
          "time": "20210904 14:17"
        },
        {
          "id": "MDRW2100030035.1.40",
          "form": "거기가면 하프 연주자가 있는데",
          "original_form": "거기가면 하프 연주자가 있는데",
          "speaker_id": "2",
          "time": "20210905 06:46"
        },
        {
          "id": "MDRW2100030035.1.41",
          "form": "그 선율들으면서 사색에 잠기면 되세요",
          "original_form": "그 선율들으면서 사색에 잠기면 되세요",
          "speaker_id": "2",
          "time": "20210905 06:46"
        },
        {
          "id": "MDRW2100030035.1.42",
          "form": "유럽의 발코니라 불리는 이유를 찾으실겁니다",
          "original_form": "유럽의 발코니라 불리는 이유를 찾으실겁니다",
          "speaker_id": "2",
          "time": "20210905 06:47"
        }
      ]
