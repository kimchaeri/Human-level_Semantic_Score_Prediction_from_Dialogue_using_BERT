      "utterance": [
        {
          "id": "MDRW2100013995.1.1",
          "form": "동물 좋아하나요",
          "original_form": "동물 좋아하나요",
          "speaker_id": "1",
          "time": "20210825 16:44"
        },
        {
          "id": "MDRW2100013995.1.2",
          "form": "아주 좋아합니다",
          "original_form": "아주 좋아합니다",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013995.1.3",
          "form": "근데 지금은 키우지는 않고 보기만해요",
          "original_form": "근데 지금은 키우지는 않고 보기만해요",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013995.1.4",
          "form": "어떤동물 좋아하나요",
          "original_form": "어떤동물 좋아하나요",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013995.1.5",
          "form": "뷰니멀족이에요",
          "original_form": "뷰니멀족이에요",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013995.1.6",
          "form": "뷰니멀족 새로운단어를 알아갑니다",
          "original_form": "뷰니멀족 새로운단어를 알아갑니다",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013995.1.7",
          "form": "저 고양이 강아지 좋아하고 특히 조류 보는 거 좋아해요",
          "original_form": "저 고양이 강아지 좋아하고 특히 조류 보는 거 좋아해요",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013995.1.8",
          "form": "오목눈이 이런거",
          "original_form": "오목눈이 이런거",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013995.1.9",
          "form": "똑똑하시네요",
          "original_form": "똑똑하시네요",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013995.1.10",
          "form": "오목눈?",
          "original_form": "오목눈?",
          "speaker_id": "1",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013995.1.11",
          "form": "저도 이거 하면서 주워들었답니다",
          "original_form": "저도 이거 하면서 주워들었답니다",
          "speaker_id": "2",
          "time": "20210825 16:45"
        },
        {
          "id": "MDRW2100013995.1.12",
          "form": "조류?  왜 조류지",
          "original_form": "조류?  왜 조류지",
          "speaker_id": "1",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013995.1.13",
          "form": "뷰+애니멀 족이라고 동물보면서 힐링하는 사람들이래요",
          "original_form": "뷰+애니멀 족이라고 동물보면서 힐링하는 사람들이래요",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013995.1.14",
          "form": "아 제가 예전에 닭을 키웠어서",
          "original_form": "아 제가 예전에 닭을 키웠어서",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013995.1.15",
          "form": "조류의 매력이 뭔가요",
          "original_form": "조류의 매력이 뭔가요",
          "speaker_id": "1",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013995.1.16",
          "form": "조류를 좋아해요",
          "original_form": "조류를 좋아해요",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013995.1.17",
          "form": "아 닭을..?",
          "original_form": "아 닭을..?",
          "speaker_id": "1",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013995.1.18",
          "form": "쥐똥만한 눈과",
          "original_form": "쥐똥만한 눈과",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013995.1.19",
          "form": "어쩌다가요",
          "original_form": "어쩌다가요",
          "speaker_id": "1",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013995.1.20",
          "form": "부리",
          "original_form": "부리",
          "speaker_id": "2",
          "time": "20210825 16:46"
        },
        {
          "id": "MDRW2100013995.1.21",
          "form": "아 쓰읍..썰 풀어도 되나요",
          "original_form": "아 쓰읍..썰 풀어도 되나요",
          "speaker_id": "2",
          "time": "20210825 16:46"
        }
