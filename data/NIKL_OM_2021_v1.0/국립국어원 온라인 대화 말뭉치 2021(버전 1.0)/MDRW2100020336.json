      "utterance": [
        {
          "id": "MDRW2100020336.1.1",
          "form": "친구들이랑 식사하나요?",
          "original_form": "친구들이랑 식사하나요?",
          "speaker_id": "1",
          "time": "20210828 01:36"
        },
        {
          "id": "MDRW2100020336.1.2",
          "form": "아뇨 카페가기로했어요",
          "original_form": "아뇨 카페가기로했어요",
          "speaker_id": "2",
          "time": "20210828 01:36"
        },
        {
          "id": "MDRW2100020336.1.3",
          "form": "산본에서 오는 친구가",
          "original_form": "산본에서 오는 친구가",
          "speaker_id": "2",
          "time": "20210828 01:36"
        },
        {
          "id": "MDRW2100020336.1.4",
          "form": "어디카페요",
          "original_form": "어디카페요",
          "speaker_id": "1",
          "time": "20210828 01:37"
        },
        {
          "id": "MDRW2100020336.1.5",
          "form": "강아지데려오기로 했거든요",
          "original_form": "강아지데려오기로 했거든요",
          "speaker_id": "2",
          "time": "20210828 01:37"
        },
        {
          "id": "MDRW2100020336.1.6",
          "form": "강아지동반가능 카페 찾아봤는데",
          "original_form": "강아지동반가능 카페 찾아봤는데",
          "speaker_id": "2",
          "time": "20210828 01:37"
        },
        {
          "id": "MDRW2100020336.1.7",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210828 01:37"
        },
        {
          "id": "MDRW2100020336.1.8",
          "form": "올덴브라운이나 펄치커피 갈듯",
          "original_form": "올덴브라운이나 펄치커피 갈듯",
          "speaker_id": "2",
          "time": "20210828 01:37"
        },
        {
          "id": "MDRW2100020336.1.9",
          "form": "판교에 있는거에요?",
          "original_form": "판교에 있는거에요?",
          "speaker_id": "1",
          "time": "20210828 01:37"
        },
        {
          "id": "MDRW2100020336.1.10",
          "form": "넹 두개다 판교 카페거리에있음",
          "original_form": "넹 두개다 판교 카페거리에있음",
          "speaker_id": "2",
          "time": "20210828 01:37"
        },
        {
          "id": "MDRW2100020336.1.11",
          "form": "가봤어요?",
          "original_form": "가봤어요?",
          "speaker_id": "1",
          "time": "20210828 01:37"
        },
        {
          "id": "MDRW2100020336.1.12",
          "form": "올덴은 가봤고",
          "original_form": "올덴은 가봤고",
          "speaker_id": "2",
          "time": "20210828 01:37"
        },
        {
          "id": "MDRW2100020336.1.13",
          "form": "펄치는 처음가봐요",
          "original_form": "펄치는 처음가봐요",
          "speaker_id": "2",
          "time": "20210828 01:37"
        },
        {
          "id": "MDRW2100020336.1.14",
          "form": "올덴은 무슨 음료가 유명해요",
          "original_form": "올덴은 무슨 음료가 유명해요",
          "speaker_id": "1",
          "time": "20210828 01:38"
        },
        {
          "id": "MDRW2100020336.1.15",
          "form": "펄치옆집은 또 가봄",
          "original_form": "펄치옆집은 또 가봄",
          "speaker_id": "2",
          "time": "20210828 01:38"
        },
        {
          "id": "MDRW2100020336.1.16",
          "form": "글쎄요?",
          "original_form": "글쎄요?",
          "speaker_id": "2",
          "time": "20210828 01:38"
        },
        {
          "id": "MDRW2100020336.1.17",
          "form": "ㅋㅋㅋ옆집뭔데요",
          "original_form": "ㅋㅋㅋ옆집뭔데요",
          "speaker_id": "1",
          "time": "20210828 01:38"
        },
        {
          "id": "MDRW2100020336.1.18",
          "form": "유명한건 모르겠고",
          "original_form": "유명한건 모르겠고",
          "speaker_id": "2",
          "time": "20210828 01:38"
        },
        {
          "id": "MDRW2100020336.1.19",
          "form": "천장이 뚫려있어요",
          "original_form": "천장이 뚫려있어요",
          "speaker_id": "2",
          "time": "20210828 01:38"
        },
        {
          "id": "MDRW2100020336.1.20",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210828 01:38"
        },
        {
          "id": "MDRW2100020336.1.21",
          "form": "옆집은 아이홈이라고",
          "original_form": "옆집은 아이홈이라고",
          "speaker_id": "2",
          "time": "20210828 01:38"
        },
        {
          "id": "MDRW2100020336.1.22",
          "form": "빙수맞집이있어요",
          "original_form": "빙수맞집이있어요",
          "speaker_id": "2",
          "time": "20210828 01:38"
        },
        {
          "id": "MDRW2100020336.1.23",
          "form": "맛집",
          "original_form": "맛집",
          "speaker_id": "2",
          "time": "20210828 01:38"
        },
        {
          "id": "MDRW2100020336.1.24",
          "form": "빙수",
          "original_form": "빙수",
          "speaker_id": "1",
          "time": "20210828 01:38"
        },
        {
          "id": "MDRW2100020336.1.25",
          "form": "거기서 에스프레소 빙수인가",
          "original_form": "거기서 에스프레소 빙수인가",
          "speaker_id": "2",
          "time": "20210828 01:38"
        },
        {
          "id": "MDRW2100020336.1.26",
          "form": "올 여름은 빙수를 못 먹었네요",
          "original_form": "올 여름은 빙수를 못 먹었네요",
          "speaker_id": "1",
          "time": "20210828 01:38"
        },
        {
          "id": "MDRW2100020336.1.27",
          "form": "드셔보세요",
          "original_form": "드셔보세요",
          "speaker_id": "2",
          "time": "20210828 01:38"
        },
        {
          "id": "MDRW2100020336.1.28",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210828 01:38"
        },
        {
          "id": "MDRW2100020336.1.29",
          "form": "양이많아요",
          "original_form": "양이많아요",
          "speaker_id": "2",
          "time": "20210828 01:38"
        },
        {
          "id": "MDRW2100020336.1.30",
          "form": "여름다갔는데",
          "original_form": "여름다갔는데",
          "speaker_id": "1",
          "time": "20210828 01:39"
        },
        {
          "id": "MDRW2100020336.1.31",
          "form": "이제와서요?",
          "original_form": "이제와서요?",
          "speaker_id": "1",
          "time": "20210828 01:39"
        },
        {
          "id": "MDRW2100020336.1.32",
          "form": "입시림",
          "original_form": "입시림",
          "speaker_id": "1",
          "time": "20210828 01:39"
        },
        {
          "id": "MDRW2100020336.1.33",
          "form": "빙수에 계절이어딧어요",
          "original_form": "빙수에 계절이어딧어요",
          "speaker_id": "2",
          "time": "20210828 01:39"
        },
        {
          "id": "MDRW2100020336.1.34",
          "form": "아 박씨 나이가 좀 있지",
          "original_form": "아 박씨 나이가 좀 있지",
          "speaker_id": "2",
          "time": "20210828 01:39"
        },
        {
          "id": "MDRW2100020336.1.35",
          "form": "제가 고려하지 못했네요",
          "original_form": "제가 고려하지 못했네요",
          "speaker_id": "2",
          "time": "20210828 01:39"
        }
      ]
    }
