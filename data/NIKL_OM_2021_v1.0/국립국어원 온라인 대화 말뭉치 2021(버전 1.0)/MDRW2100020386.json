      "utterance": [
        {
          "id": "MDRW2100020386.1.1",
          "form": "예쁘더라고요",
          "original_form": "예쁘더라고요",
          "speaker_id": "2",
          "time": "20210828 02:34"
        },
        {
          "id": "MDRW2100020386.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020386.1.3",
          "form": "예쁘긴하죠 ㅋㅋ",
          "original_form": "예쁘긴하죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020386.1.4",
          "form": "네 뭔가 말간 이미지",
          "original_form": "네 뭔가 말간 이미지",
          "speaker_id": "2",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020386.1.5",
          "form": "네 ㅋㅋ 근데 연기는 뭔가",
          "original_form": "네 ㅋㅋ 근데 연기는 뭔가",
          "speaker_id": "1",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020386.1.6",
          "form": "잘 모르겠어요",
          "original_form": "잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020386.1.7",
          "form": "발성이 사근사근 하더군요",
          "original_form": "발성이 사근사근 하더군요",
          "speaker_id": "2",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020386.1.8",
          "form": "네 ㅋㅋ 영화 디바에서는 잘했는데",
          "original_form": "네 ㅋㅋ 영화 디바에서는 잘했는데",
          "speaker_id": "1",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020386.1.9",
          "form": "신민아 나온거요?",
          "original_form": "신민아 나온거요?",
          "speaker_id": "2",
          "time": "20210828 02:35"
        },
        {
          "id": "MDRW2100020386.1.10",
          "form": "네 거기서 둘다 잘하더라고요",
          "original_form": "네 거기서 둘다 잘하더라고요",
          "speaker_id": "1",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100020386.1.11",
          "form": "재밌어요?",
          "original_form": "재밌어요?",
          "speaker_id": "2",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100020386.1.12",
          "form": "그건 재밌었어요!!",
          "original_form": "그건 재밌었어요!!",
          "speaker_id": "1",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100020386.1.13",
          "form": "추천!",
          "original_form": "추천!",
          "speaker_id": "1",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100020386.1.14",
          "form": "오 봐볼게요",
          "original_form": "오 봐볼게요",
          "speaker_id": "2",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100020386.1.15",
          "form": "네 80분 정도밖에 안되서 가볍게 보가도 좋아요",
          "original_form": "네 80분 정도밖에 안되서 가볍게 보가도 좋아요",
          "speaker_id": "1",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100020386.1.16",
          "form": "내용은 좀 다크하지만.",
          "original_form": "내용은 좀 다크하지만.",
          "speaker_id": "1",
          "time": "20210828 02:36"
        },
        {
          "id": "MDRW2100020386.1.17",
          "form": "짧은 편이네여",
          "original_form": "짧은 편이네여",
          "speaker_id": "2",
          "time": "20210828 02:36"
        }
