      "utterance": [
        {
          "id": "MDRW2100025245.1.1",
          "form": "집안일에 자주 참여하는 편이야?",
          "original_form": "집안일에 자주 참여하는 편이야?",
          "speaker_id": "1",
          "time": "20210901 02:35"
        },
        {
          "id": "MDRW2100025245.1.2",
          "form": "평일에는 어렵고 주말에 하는편ㅜ",
          "original_form": "평일에는 어렵고 주말에 하는편ㅜ",
          "speaker_id": "2",
          "time": "20210901 02:37"
        },
        {
          "id": "MDRW2100025245.1.3",
          "form": "name1는?",
          "original_form": "&name1&는?",
          "speaker_id": "2",
          "time": "20210901 02:37"
        },
        {
          "id": "MDRW2100025245.1.4",
          "form": "나는 자취해서 일주일 내내 늘 집안일을 해ㅎㅎ",
          "original_form": "나는 자취해서 일주일 내내 늘 집안일을 해ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 02:42"
        },
        {
          "id": "MDRW2100025245.1.5",
          "form": "집안일도 참 많잖아!",
          "original_form": "집안일도 참 많잖아!",
          "speaker_id": "1",
          "time": "20210901 02:43"
        },
        {
          "id": "MDRW2100025245.1.6",
          "form": "주말에 몰아서 집안일을 하는 편인거야?",
          "original_form": "주말에 몰아서 집안일을 하는 편인거야?",
          "speaker_id": "1",
          "time": "20210901 02:43"
        },
        {
          "id": "MDRW2100025245.1.7",
          "form": "아님 집안일을 가족구성원과 나눠서 하는 편인거야?",
          "original_form": "아님 집안일을 가족구성원과 나눠서 하는 편인거야?",
          "speaker_id": "1",
          "time": "20210901 02:43"
        },
        {
          "id": "MDRW2100025245.1.8",
          "form": "가족과 같이 살고",
          "original_form": "가족과 같이 살고",
          "speaker_id": "2",
          "time": "20210901 02:44"
        },
        {
          "id": "MDRW2100025245.1.9",
          "form": "주말에 몰아 하는편",
          "original_form": "주말에 몰아 하는편",
          "speaker_id": "2",
          "time": "20210901 02:44"
        },
        {
          "id": "MDRW2100025245.1.10",
          "form": "가족구성원과 나누기도 하고",
          "original_form": "가족구성원과 나누기도 하고",
          "speaker_id": "2",
          "time": "20210901 02:44"
        },
        {
          "id": "MDRW2100025245.1.11",
          "form": "청소는 로봇청소기 써?",
          "original_form": "청소는 로봇청소기 써?",
          "speaker_id": "2",
          "time": "20210901 02:45"
        },
        {
          "id": "MDRW2100025245.1.12",
          "form": "아니!",
          "original_form": "아니!",
          "speaker_id": "1",
          "time": "20210901 02:45"
        },
        {
          "id": "MDRW2100025245.1.13",
          "form": "원룸이라 부직포 밀대로 밀고",
          "original_form": "원룸이라 부직포 밀대로 밀고",
          "speaker_id": "1",
          "time": "20210901 02:45"
        },
        {
          "id": "MDRW2100025245.1.14",
          "form": "오오 제대로 하네",
          "original_form": "오오 제대로 하네",
          "speaker_id": "2",
          "time": "20210901 02:48"
        },
        {
          "id": "MDRW2100025245.1.15",
          "form": "매일 해?",
          "original_form": "매일 해?",
          "speaker_id": "2",
          "time": "20210901 02:48"
        },
        {
          "id": "MDRW2100025245.1.16",
          "form": "매일은 아니고 이틀에 한 번?",
          "original_form": "매일은 아니고 이틀에 한 번?",
          "speaker_id": "1",
          "time": "20210901 02:48"
        },
        {
          "id": "MDRW2100025245.1.17",
          "form": "근데 바쁘면 까먹기도 하고ㅋㅋㅋㅋ",
          "original_form": "근데 바쁘면 까먹기도 하고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 02:48"
        },
        {
          "id": "MDRW2100025245.1.18",
          "form": "오오 부지런하네",
          "original_form": "오오 부지런하네",
          "speaker_id": "2",
          "time": "20210901 02:49"
        },
        {
          "id": "MDRW2100025245.1.19",
          "form": "자취 오래 했어?",
          "original_form": "자취 오래 했어?",
          "speaker_id": "2",
          "time": "20210901 02:50"
        },
        {
          "id": "MDRW2100025245.1.20",
          "form": "올해부터 자취하기 시작했어~",
          "original_form": "올해부터 자취하기 시작했어~",
          "speaker_id": "1",
          "time": "20210901 02:51"
        },
        {
          "id": "MDRW2100025245.1.21",
          "form": "한 2~3개월 정도는 집안일이 이렇게도 많았던가... 실감하다가",
          "original_form": "한 2~3개월 정도는 집안일이 이렇게도 많았던가... 실감하다가",
          "speaker_id": "1",
          "time": "20210901 02:52"
        },
        {
          "id": "MDRW2100025245.1.22",
          "form": "이제는 조금 루틴처럼 정해서 집안일 하는 편이야!",
          "original_form": "이제는 조금 루틴처럼 정해서 집안일 하는 편이야!",
          "speaker_id": "1",
          "time": "20210901 02:52"
        },
        {
          "id": "MDRW2100025245.1.23",
          "form": "요리도 직접 해먹어?",
          "original_form": "요리도 직접 해먹어?",
          "speaker_id": "2",
          "time": "20210901 02:53"
        },
        {
          "id": "MDRW2100025245.1.24",
          "form": "응응!",
          "original_form": "응응!",
          "speaker_id": "1",
          "time": "20210901 02:54"
        },
        {
          "id": "MDRW2100025245.1.25",
          "form": "막상 레시피 보고하는데도 아직은...문제가 많아ㅋㅋㅋ",
          "original_form": "막상 레시피 보고하는데도 아직은...문제가 많아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 02:54"
        },
        {
          "id": "MDRW2100025245.1.26",
          "form": "그래도 실패한 음식도 꿋꿋하게 먹는 편이라서 다행인거같아!",
          "original_form": "그래도 실패한 음식도 꿋꿋하게 먹는 편이라서 다행인거같아!",
          "speaker_id": "1",
          "time": "20210901 02:55"
        },
        {
          "id": "MDRW2100025245.1.27",
          "form": "ㅎㅎ 장기요리는 뭐야?",
          "original_form": "ㅎㅎ 장기요리는 뭐야?",
          "speaker_id": "2",
          "time": "20210901 02:55"
        },
        {
          "id": "MDRW2100025245.1.28",
          "form": "엄마 집에서 가져온 김치로 김치찌개끓이기?",
          "original_form": "엄마 집에서 가져온 김치로 김치찌개끓이기?",
          "speaker_id": "1",
          "time": "20210901 02:57"
        },
        {
          "id": "MDRW2100025245.1.29",
          "form": "사실 김치가 다하는 요리라 실패할 수 없기는한뎈ㅋㅋㅋ",
          "original_form": "사실 김치가 다하는 요리라 실패할 수 없기는한뎈ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 02:57"
        },
        {
          "id": "MDRW2100025245.1.30",
          "form": "실패가 되더라..",
          "original_form": "실패가 되더라..",
          "speaker_id": "1",
          "time": "20210901 02:57"
        },
        {
          "id": "MDRW2100025245.1.31",
          "form": "그리고 파스타도 시판 소스사서 후딱해먹기 좋은 편이라 자주해먹어!",
          "original_form": "그리고 파스타도 시판 소스사서 후딱해먹기 좋은 편이라 자주해먹어!",
          "speaker_id": "1",
          "time": "20210901 02:57"
        },
        {
          "id": "MDRW2100025245.1.32",
          "form": "오오 파스타는 어려울것 같은데 의외네",
          "original_form": "오오 파스타는 어려울것 같은데 의외네",
          "speaker_id": "2",
          "time": "20210901 02:58"
        },
        {
          "id": "MDRW2100025245.1.33",
          "form": "파스타가 진짜 편한게, 소스만 있으면 집에 있는 웬만한 재료랑 어울리더라구!",
          "original_form": "파스타가 진짜 편한게, 소스만 있으면 집에 있는 웬만한 재료랑 어울리더라구!",
          "speaker_id": "1",
          "time": "20210901 03:01"
        }
      ]
    }
  ]
