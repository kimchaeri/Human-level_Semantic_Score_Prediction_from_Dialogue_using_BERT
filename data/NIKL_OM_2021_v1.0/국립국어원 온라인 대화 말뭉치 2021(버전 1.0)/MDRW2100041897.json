      "utterance": [
        {
          "id": "MDRW2100041897.1.1",
          "form": "너무 좋을거같아요 흑",
          "original_form": "너무 좋을거같아요 흑",
          "speaker_id": "2",
          "time": "20210906 17:16"
        },
        {
          "id": "MDRW2100041897.1.2",
          "form": "그런 기계 있으면 다같이 공구라도 했으면 좋겠네욬ㅋㅋ",
          "original_form": "그런 기계 있으면 다같이 공구라도 했으면 좋겠네욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:16"
        },
        {
          "id": "MDRW2100041897.1.3",
          "form": "그러게여 \n그럼 배달도 덜먹을거겉아여ㅋㅋ",
          "original_form": "그러게여 \n그럼 배달도 덜먹을거겉아여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:17"
        },
        {
          "id": "MDRW2100041897.1.4",
          "form": "ㅋㅋㅋ 저 그래도 먹을거 같은데여",
          "original_form": "ㅋㅋㅋ 저 그래도 먹을거 같은데여",
          "speaker_id": "1",
          "time": "20210906 17:17"
        },
        {
          "id": "MDRW2100041897.1.5",
          "form": "그것도 귀찮아서",
          "original_form": "그것도 귀찮아서",
          "speaker_id": "1",
          "time": "20210906 17:17"
        },
        {
          "id": "MDRW2100041897.1.6",
          "form": "귀찮긴 하지만 지금보다는 더 요리하겠져ㅋㅋ",
          "original_form": "귀찮긴 하지만 지금보다는 더 요리하겠져ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:18"
        },
        {
          "id": "MDRW2100041897.1.7",
          "form": "ㅋㅋㅋ 그렇긴 하네여",
          "original_form": "ㅋㅋㅋ 그렇긴 하네여",
          "speaker_id": "1",
          "time": "20210906 17:18"
        },
        {
          "id": "MDRW2100041897.1.8",
          "form": "이제 또 저녁 시간이",
          "original_form": "이제 또 저녁 시간이",
          "speaker_id": "1",
          "time": "20210906 17:19"
        },
        {
          "id": "MDRW2100041897.1.9",
          "form": "저 지금 뽀또 먹으면서 당 올리고 있어요 ㅋㅋㅋㅋ",
          "original_form": "저 지금 뽀또 먹으면서 당 올리고 있어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:19"
        },
        {
          "id": "MDRW2100041897.1.10",
          "form": "간식 좋아하세여?",
          "original_form": "간식 좋아하세여?",
          "speaker_id": "2",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100041897.1.11",
          "form": "과자나",
          "original_form": "과자나",
          "speaker_id": "2",
          "time": "20210906 17:20"
        },
        {
          "id": "MDRW2100041897.1.12",
          "form": "저 과자 진짜 좋아해요 ㅋㅋㅋ",
          "original_form": "저 과자 진짜 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100041897.1.13",
          "form": "빵도...",
          "original_form": "빵도...",
          "speaker_id": "1",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100041897.1.14",
          "form": "밥보다 사실 덬ㅋㅋ",
          "original_form": "밥보다 사실 덬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:21"
        },
        {
          "id": "MDRW2100041897.1.15",
          "form": "저도 좋아하는데 과자는 조그만 먹어도 \n입천장이 까져여ㅠ",
          "original_form": "저도 좋아하는데 과자는 조그만 먹어도 \n입천장이 까져여ㅠ",
          "speaker_id": "2",
          "time": "20210906 17:22"
        },
        {
          "id": "MDRW2100041897.1.16",
          "form": "입천장이 까지시다니 ㅜㅜ",
          "original_form": "입천장이 까지시다니 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100041897.1.17",
          "form": "파이류는 좀 괜찮죠??",
          "original_form": "파이류는 좀 괜찮죠??",
          "speaker_id": "1",
          "time": "20210906 17:23"
        },
        {
          "id": "MDRW2100041897.1.18",
          "form": "몽셸좋아해요ㅋㅋ",
          "original_form": "몽셸좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100041897.1.19",
          "form": "까져도 참고 먹지만욬ㅋ",
          "original_form": "까져도 참고 먹지만욬ㅋ",
          "speaker_id": "2",
          "time": "20210906 17:25"
        },
        {
          "id": "MDRW2100041897.1.20",
          "form": "앜ㅋㅋㅋㅋ 참고 드실정도로 좋아하시네여",
          "original_form": "앜ㅋㅋㅋㅋ 참고 드실정도로 좋아하시네여",
          "speaker_id": "1",
          "time": "20210906 17:26"
        },
        {
          "id": "MDRW2100041897.1.21",
          "form": "전에는 박스로 사서 먹엇는데\n아예 안사려고 자제중이요",
          "original_form": "전에는 박스로 사서 먹엇는데\n아예 안사려고 자제중이요",
          "speaker_id": "2",
          "time": "20210906 17:29"
        },
        {
          "id": "MDRW2100041897.1.22",
          "form": "박스로 사는게 싸니까 그렇게 되더라고요",
          "original_form": "박스로 사는게 싸니까 그렇게 되더라고요",
          "speaker_id": "1",
          "time": "20210906 17:31"
        }
      ]
