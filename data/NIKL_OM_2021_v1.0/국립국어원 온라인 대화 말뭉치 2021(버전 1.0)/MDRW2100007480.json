      "utterance": [
        {
          "id": "MDRW2100007480.1.1",
          "form": "나 카드지갑 사고싶어",
          "original_form": "나 카드지갑 사고싶어",
          "speaker_id": "1",
          "time": "20210822 02:27"
        },
        {
          "id": "MDRW2100007480.1.2",
          "form": "카드를 주머니에 그냥 넣고 다니니까 더러워지더라고ㅠ",
          "original_form": "카드를 주머니에 그냥 넣고 다니니까 더러워지더라고ㅠ",
          "speaker_id": "1",
          "time": "20210822 02:27"
        },
        {
          "id": "MDRW2100007480.1.3",
          "form": "ic칩?에 때낌;;",
          "original_form": "ic칩?에 때낌;;",
          "speaker_id": "1",
          "time": "20210822 02:27"
        },
        {
          "id": "MDRW2100007480.1.4",
          "form": "악 더러웡",
          "original_form": "악 더러웡",
          "speaker_id": "2",
          "time": "20210822 02:27"
        },
        {
          "id": "MDRW2100007480.1.5",
          "form": "근데 엄청 비싸더라",
          "original_form": "근데 엄청 비싸더라",
          "speaker_id": "1",
          "time": "20210822 02:27"
        },
        {
          "id": "MDRW2100007480.1.6",
          "form": "사달라고?",
          "original_form": "사달라고?",
          "speaker_id": "2",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007480.1.7",
          "form": "내 통장은 열리지 않아",
          "original_form": "내 통장은 열리지 않아",
          "speaker_id": "2",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007480.1.8",
          "form": "가죽같은거는 금방 더러워지자너",
          "original_form": "가죽같은거는 금방 더러워지자너",
          "speaker_id": "2",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007480.1.9",
          "form": "질 스튜어트도 엄청비싸",
          "original_form": "질 스튜어트도 엄청비싸",
          "speaker_id": "1",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007480.1.10",
          "form": "검정색 사려고",
          "original_form": "검정색 사려고",
          "speaker_id": "1",
          "time": "20210822 02:28"
        },
        {
          "id": "MDRW2100007480.1.11",
          "form": "그냥 지갑을 사",
          "original_form": "그냥 지갑을 사",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007480.1.12",
          "form": "동전도 들어가는거면 좋은데",
          "original_form": "동전도 들어가는거면 좋은데",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007480.1.13",
          "form": "그게 제일 나을 거 같은대",
          "original_form": "그게 제일 나을 거 같은대",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007480.1.14",
          "form": "그겅 5만원",
          "original_form": "그겅 5만원",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007480.1.15",
          "form": "데",
          "original_form": "데",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007480.1.16",
          "form": "그건 싫어",
          "original_form": "그건 싫어",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007480.1.17",
          "form": "비싸다",
          "original_form": "비싸다",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007480.1.18",
          "form": "카드지갑!!",
          "original_form": "카드지갑!!",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007480.1.19",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007480.1.20",
          "form": "나름 브랜드라고",
          "original_form": "나름 브랜드라고",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007480.1.21",
          "form": "나는 돈이 없는 그지에요",
          "original_form": "나는 돈이 없는 그지에요",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007480.1.22",
          "form": "너도에요",
          "original_form": "너도에요",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007480.1.23",
          "form": "생로랑은 13만원 하더라",
          "original_form": "생로랑은 13만원 하더라",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007480.1.24",
          "form": "으아악",
          "original_form": "으아악",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007480.1.25",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007480.1.26",
          "form": "비싸",
          "original_form": "비싸",
          "speaker_id": "2",
          "time": "20210822 02:29"
        },
        {
          "id": "MDRW2100007480.1.27",
          "form": "눈에서 땀나네",
          "original_form": "눈에서 땀나네",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007480.1.28",
          "form": "ㅠㅜㅠㅜ",
          "original_form": "ㅠㅜㅠㅜ",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007480.1.29",
          "form": "나는 차라리 옷을 산다",
          "original_form": "나는 차라리 옷을 산다",
          "speaker_id": "2",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007480.1.30",
          "form": "지갑은 내게 무쓸모",
          "original_form": "지갑은 내게 무쓸모",
          "speaker_id": "2",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007480.1.31",
          "form": "그런가?",
          "original_form": "그런가?",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007480.1.32",
          "form": "근데 요즘 옷이 다 별로",
          "original_form": "근데 요즘 옷이 다 별로",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007480.1.33",
          "form": "맘에 딱 드는 옷이 없어",
          "original_form": "맘에 딱 드는 옷이 없어",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007480.1.34",
          "form": "근데 개학하면 옷 필요 없어",
          "original_form": "근데 개학하면 옷 필요 없어",
          "speaker_id": "2",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007480.1.35",
          "form": "저번에도 옷사려다가 그냥 나옴",
          "original_form": "저번에도 옷사려다가 그냥 나옴",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007480.1.36",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007480.1.37",
          "form": "근데 개학해도 근근히 필요해",
          "original_form": "근데 개학해도 근근히 필요해",
          "speaker_id": "1",
          "time": "20210822 02:30"
        },
        {
          "id": "MDRW2100007480.1.38",
          "form": "그래 맞아",
          "original_form": "그래 맞아",
          "speaker_id": "2",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007480.1.39",
          "form": "주말에 벗고다닐수는 없잖아",
          "original_form": "주말에 벗고다닐수는 없잖아",
          "speaker_id": "1",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007480.1.40",
          "form": "근데 좀 있으면 롱패딩 입을거잖어",
          "original_form": "근데 좀 있으면 롱패딩 입을거잖어",
          "speaker_id": "2",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007480.1.41",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007480.1.42",
          "form": "빤스만 입어",
          "original_form": "빤스만 입어",
          "speaker_id": "2",
          "time": "20210822 02:31"
        },
        {
          "id": "MDRW2100007480.1.43",
          "form": "롱패딩이면 나체도 가늘",
          "original_form": "롱패딩이면 나체도 가늘",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007480.1.44",
          "form": "아니면 잠옷만 입고 롱패딩 걸쳐",
          "original_form": "아니면 잠옷만 입고 롱패딩 걸쳐",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007480.1.45",
          "form": "가능",
          "original_form": "가능",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007480.1.46",
          "form": "싫어",
          "original_form": "싫어",
          "speaker_id": "1",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007480.1.47",
          "form": "크록스",
          "original_form": "크록스",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007480.1.48",
          "form": "뙇",
          "original_form": "뙇",
          "speaker_id": "2",
          "time": "20210822 02:32"
        },
        {
          "id": "MDRW2100007480.1.49",
          "form": "시원하게 안입어야지ㅋㅋ",
          "original_form": "시원하게 안입어야지ㅋㅋ",
