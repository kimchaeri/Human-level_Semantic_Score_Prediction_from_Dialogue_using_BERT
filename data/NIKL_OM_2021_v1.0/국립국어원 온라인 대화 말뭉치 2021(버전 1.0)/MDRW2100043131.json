      "utterance": [
        {
          "id": "MDRW2100043131.1.1",
          "form": "오 동유럽! 저도 유럽가고 싶어요 가본적이 없어서요 ㅠㅠ",
          "original_form": "오 동유럽! 저도 유럽가고 싶어요 가본적이 없어서요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 14:45"
        },
        {
          "id": "MDRW2100043131.1.2",
          "form": "저는남부랑서부만가봤는데",
          "original_form": "저는남부랑서부만가봤는데",
          "speaker_id": "2",
          "time": "20210907 14:46"
        },
        {
          "id": "MDRW2100043131.1.3",
          "form": "동부궁금해요",
          "original_form": "동부궁금해요",
          "speaker_id": "2",
          "time": "20210907 14:47"
        },
        {
          "id": "MDRW2100043131.1.4",
          "form": "동부에서 어느나라 특히 가보고 싶으신가요?",
          "original_form": "동부에서 어느나라 특히 가보고 싶으신가요?",
          "speaker_id": "1",
          "time": "20210907 14:51"
        },
        {
          "id": "MDRW2100043131.1.5",
          "form": "저는크로아티아요",
          "original_form": "저는크로아티아요",
          "speaker_id": "2",
          "time": "20210907 14:54"
        },
        {
          "id": "MDRW2100043131.1.6",
          "form": "와 크로아티아 좋을거 같아요",
          "original_form": "와 크로아티아 좋을거 같아요",
          "speaker_id": "1",
          "time": "20210907 14:55"
        },
        {
          "id": "MDRW2100043131.1.7",
          "form": "제버킷리스트에ㅇㆍ",
          "original_form": "제버킷리스트에ㅇㆍ",
          "speaker_id": "2",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100043131.1.8",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210907 14:58"
        },
        {
          "id": "MDRW2100043131.1.9",
          "form": "버킷리스트셨구나! 이제 코로나 괜찮아지면",
          "original_form": "버킷리스트셨구나! 이제 코로나 괜찮아지면",
          "speaker_id": "1",
          "time": "20210907 15:00"
        },
        {
          "id": "MDRW2100043131.1.10",
          "form": "가볼수 있겠네요!!",
          "original_form": "가볼수 있겠네요!!",
          "speaker_id": "1",
          "time": "20210907 15:00"
        },
        {
          "id": "MDRW2100043131.1.11",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210907 15:02"
        },
        {
          "id": "MDRW2100043131.1.12",
          "form": "한달살기해보고싶어요",
          "original_form": "한달살기해보고싶어요",
          "speaker_id": "2",
          "time": "20210907 15:02"
        },
        {
          "id": "MDRW2100043131.1.13",
          "form": "오 크로아티아에서 한달살기요? 진짜 좋을거 같아요",
          "original_form": "오 크로아티아에서 한달살기요? 진짜 좋을거 같아요",
          "speaker_id": "1",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100043131.1.14",
          "form": "어디든지 한달살기 좋겠지만요",
          "original_form": "어디든지 한달살기 좋겠지만요",
          "speaker_id": "1",
          "time": "20210907 15:03"
        },
        {
          "id": "MDRW2100043131.1.15",
          "form": "맞아요ㅎㅎ가고싶네요",
          "original_form": "맞아요ㅎㅎ가고싶네요",
          "speaker_id": "2",
          "time": "20210907 15:05"
        },
        {
          "id": "MDRW2100043131.1.16",
          "form": "저는 스페인 가고 싶은데 가보셨어요?",
          "original_form": "저는 스페인 가고 싶은데 가보셨어요?",
          "speaker_id": "1",
          "time": "20210907 15:05"
        },
        {
          "id": "MDRW2100043131.1.17",
          "form": "느낌에 가보셨을거 같아서",
          "original_form": "느낌에 가보셨을거 같아서",
          "speaker_id": "1",
          "time": "20210907 15:05"
        },
        {
          "id": "MDRW2100043131.1.18",
          "form": "ㅎㅎ앗저가봤어욬ㅋㅋㅋ",
          "original_form": "ㅎㅎ앗저가봤어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:07"
        },
        {
          "id": "MDRW2100043131.1.19",
          "form": "마드리드 그라나다 바르셀로나 일케만요",
          "original_form": "마드리드 그라나다 바르셀로나 일케만요",
          "speaker_id": "2",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100043131.1.20",
          "form": "역시 ㅋㅋㅋㅋ 가보셨을거 같았어요",
          "original_form": "역시 ㅋㅋㅋㅋ 가보셨을거 같았어요",
          "speaker_id": "1",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100043131.1.21",
          "form": "추천좀 해주세요",
          "original_form": "추천좀 해주세요",
          "speaker_id": "1",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100043131.1.22",
          "form": "저 죽기전에 스페인은 가볼거 같아서 ㅎㅎ",
          "original_form": "저 죽기전에 스페인은 가볼거 같아서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 15:08"
        },
        {
          "id": "MDRW2100043131.1.23",
          "form": "ㅎㅎㅎ어뜨케아셨죠 신기해요",
          "original_form": "ㅎㅎㅎ어뜨케아셨죠 신기해요",
          "speaker_id": "2",
          "time": "20210907 15:10"
        },
        {
          "id": "MDRW2100043131.1.24",
          "form": "저는 그라나다요~펍이랑 분위기좋고",
          "original_form": "저는 그라나다요~펍이랑 분위기좋고",
          "speaker_id": "2",
          "time": "20210907 15:10"
        },
        {
          "id": "MDRW2100043131.1.25",
          "form": "사실스페인보다옆에포르투갈도좋았어요",
          "original_form": "사실스페인보다옆에포르투갈도좋았어요",
