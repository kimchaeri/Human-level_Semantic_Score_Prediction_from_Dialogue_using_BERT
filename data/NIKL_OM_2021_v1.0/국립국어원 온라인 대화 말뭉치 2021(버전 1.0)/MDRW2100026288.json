      "utterance": [
        {
          "id": "MDRW2100026288.1.1",
          "form": "제주는 배송료가 추가로 붙는다던데요ㅜ",
          "original_form": "제주는 배송료가 추가로 붙는다던데요ㅜ",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026288.1.2",
          "form": "네 멀 사려면 배송료가 짜증나서리",
          "original_form": "네 멀 사려면 배송료가 짜증나서리",
          "speaker_id": "1",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026288.1.3",
          "form": "넵ㅋㅋ 제주도도 당근하죠? 오래 계실거예요?",
          "original_form": "넵ㅋㅋ 제주도도 당근하죠? 오래 계실거예요?",
          "speaker_id": "2",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100026288.1.4",
          "form": "아 지금 당근으로 물건 엄청 샀죠",
          "original_form": "아 지금 당근으로 물건 엄청 샀죠",
          "speaker_id": "1",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026288.1.5",
          "form": "내년 3월까지 있을거라",
          "original_form": "내년 3월까지 있을거라",
          "speaker_id": "1",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026288.1.6",
          "form": "아 오래 계시네요! 이주전에 오셨으니 엄청 쇼핑하셨겠네요",
          "original_form": "아 오래 계시네요! 이주전에 오셨으니 엄청 쇼핑하셨겠네요",
          "speaker_id": "2",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026288.1.7",
          "form": "그리고 쭉 하셔야하고ㅋㅋ",
          "original_form": "그리고 쭉 하셔야하고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026288.1.8",
          "form": "서울에서는 무료나눔천사고 여기선 과소비녀예요",
          "original_form": "서울에서는 무료나눔천사고 여기선 과소비녀예요",
          "speaker_id": "1",
          "time": "20210901 21:23"
        },
        {
          "id": "MDRW2100026288.1.9",
          "form": "ㅋㅋ 또 내년 3월에는 반대로 제주기부천사ㅜㅋㅋ",
          "original_form": "ㅋㅋ 또 내년 3월에는 반대로 제주기부천사ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:24"
        },
        {
          "id": "MDRW2100026288.1.10",
          "form": "저도 캐나다잠깐 살다왔을때 무나 되게 많이 했어요ㅋㅋ 갈길이 머니 물욕이 사라지고... ㅋㅋ",
          "original_form": "저도 캐나다잠깐 살다왔을때 무나 되게 많이 했어요ㅋㅋ 갈길이 머니 물욕이 사라지고... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026288.1.11",
          "form": "근데 인간이 살다보면 필요한 물건이 많더라구요",
          "original_form": "근데 인간이 살다보면 필요한 물건이 많더라구요",
          "speaker_id": "1",
          "time": "20210901 22:10"
        },
        {
          "id": "MDRW2100026288.1.12",
          "form": "금방 또 세탁기 청소를 위해 세탁조 클리너를 구입했다는 ㅠ",
          "original_form": "금방 또 세탁기 청소를 위해 세탁조 클리너를 구입했다는 ㅠ",
          "speaker_id": "1",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026288.1.13",
          "form": "ㅋㅋ 어쩔수가 없어요 그거시 인간의 삶ㅋㅋ",
          "original_form": "ㅋㅋ 어쩔수가 없어요 그거시 인간의 삶ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026288.1.14",
          "form": "전 믹서기샀어요ㅋㅋ",
          "original_form": "전 믹서기샀어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:11"
        },
        {
          "id": "MDRW2100026288.1.15",
          "form": "오 저도 아침에.토마토 갈아먹었어요",
          "original_form": "오 저도 아침에.토마토 갈아먹었어요",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100026288.1.16",
          "form": "새거 사면 이제 과일 갈아야죠! 그전에는 마늘 고추... ㅋㅋ",
          "original_form": "새거 사면 이제 과일 갈아야죠! 그전에는 마늘 고추... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100026288.1.17",
          "form": "바질페스토도 만들수있죠",
          "original_form": "바질페스토도 만들수있죠",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100026288.1.18",
          "form": "ㅋㅋ 바질 키워보셨나요? 저 다이소 바질 사서 키워봤어요!",
          "original_form": "ㅋㅋ 바질 키워보셨나요? 저 다이소 바질 사서 키워봤어요!",
          "speaker_id": "2",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026288.1.19",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100026288.1.20",
          "form": "바질 잘 자라죠",
          "original_form": "바질 잘 자라죠",
          "speaker_id": "1",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026288.1.21",
          "form": "다이소에 그런것도 파나요?",
          "original_form": "다이소에 그런것도 파나요?",
          "speaker_id": "1",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026288.1.22",
          "form": "네 한번 자라니까 엄청 잘자라는데 저 혼자 먹다보니ㅜㅜㅜ",
          "original_form": "네 한번 자라니까 엄청 잘자라는데 저 혼자 먹다보니ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026288.1.23",
          "form": "씨앗을 팔아요",
          "original_form": "씨앗을 팔아요",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100026288.1.24",
          "form": "아 토마토 기르는 키트 본 것 같아요",
          "original_form": "아 토마토 기르는 키트 본 것 같아요",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026288.1.25",
          "form": "네 아마 그 옆에 바질있을거예요ㅋㅋ",
          "original_form": "네 아마 그 옆에 바질있을거예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100026288.1.26",
          "form": "다이소는 진짜 없는 게 없어요",
          "original_form": "다이소는 진짜 없는 게 없어요",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026288.1.27",
          "form": "그러니까요 와인잔 너무 웃기고요 너무 본격적이에요ㅋㅋ",
          "original_form": "그러니까요 와인잔 너무 웃기고요 너무 본격적이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026288.1.28",
          "form": "시간가는줄 모르고 구경하는 곳..",
          "original_form": "시간가는줄 모르고 구경하는 곳..",
          "speaker_id": "2",
          "time": "20210901 22:23"
        }
      ]
    }
