      "utterance": [
        {
          "id": "MDRW2100029742.1.1",
          "form": "이제 밥먹고 일하네요",
          "original_form": "이제 밥먹고 일하네요",
          "speaker_id": "1",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100029742.1.2",
          "form": "저도 막 밥먹고 은행왔다가 점심시간이 끝났네요ㅠ",
          "original_form": "저도 막 밥먹고 은행왔다가 점심시간이 끝났네요ㅠ",
          "speaker_id": "2",
          "time": "20210903 13:00"
        },
        {
          "id": "MDRW2100029742.1.3",
          "form": "아근데 삼성 대신증권사들 제한기간이어도 방문하면 다 개설 되나요?",
          "original_form": "아근데 삼성 대신증권사들 제한기간이어도 방문하면 다 개설 되나요?",
          "speaker_id": "2",
          "time": "20210903 13:01"
        },
        {
          "id": "MDRW2100029742.1.4",
          "form": "공모주단체방도 커뮤니티겠죠?ㅋ",
          "original_form": "공모주단체방도 커뮤니티겠죠?ㅋ",
          "speaker_id": "2",
          "time": "20210903 13:02"
        },
        {
          "id": "MDRW2100029742.1.5",
          "form": "삼성은 제한없을거에요",
          "original_form": "삼성은 제한없을거에요",
          "speaker_id": "1",
          "time": "20210903 13:03"
        },
        {
          "id": "MDRW2100029742.1.6",
          "form": "그럼요ㅋ",
          "original_form": "그럼요ㅋ",
          "speaker_id": "1",
          "time": "20210903 13:03"
        },
        {
          "id": "MDRW2100029742.1.7",
          "form": "저는대신이 없는건데",
          "original_form": "저는대신이 없는건데",
          "speaker_id": "2",
          "time": "20210903 13:03"
        },
        {
          "id": "MDRW2100029742.1.8",
          "form": "애들꺼는 삼성 대신 미래 신한? 다없어서ㅋㅋㅋㄱ휴가내고 갔다올까해서요ㅜ",
          "original_form": "애들꺼는 삼성 대신 미래 신한? 다없어서ㅋㅋㅋㄱ휴가내고 갔다올까해서요ㅜ",
          "speaker_id": "2",
          "time": "20210903 13:05"
        },
        {
          "id": "MDRW2100029742.1.9",
          "form": "은행연계로 만들수있으니까 지인중에 은행원있나 찾아보셔요",
          "original_form": "은행연계로 만들수있으니까 지인중에 은행원있나 찾아보셔요",
          "speaker_id": "1",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100029742.1.10",
          "form": "대신은 직접가야하고",
          "original_form": "대신은 직접가야하고",
          "speaker_id": "1",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100029742.1.11",
          "form": "삼성은 비대면 제한 없을걸요",
          "original_form": "삼성은 비대면 제한 없을걸요",
          "speaker_id": "1",
          "time": "20210903 13:06"
        },
        {
          "id": "MDRW2100029742.1.12",
          "form": "어차피 증권사 방문할꺼라서요ㅋ",
          "original_form": "어차피 증권사 방문할꺼라서요ㅋ",
          "speaker_id": "2",
          "time": "20210903 13:07"
        },
        {
          "id": "MDRW2100029742.1.13",
          "form": "근데 삼성은 비대면으로 만들어야",
          "original_form": "근데 삼성은 비대면으로 만들어야",
          "speaker_id": "1",
          "time": "20210903 13:41"
        },
        {
          "id": "MDRW2100029742.1.14",
          "form": "해외주식 수수료 혜택받고 이벤트 할수있어쇼",
          "original_form": "해외주식 수수료 혜택받고 이벤트 할수있어쇼",
          "speaker_id": "1",
          "time": "20210903 13:41"
        },
        {
          "id": "MDRW2100029742.1.15",
          "form": "그쵸?ㅠ 근데 애들껀 별수없어서ㅋ",
          "original_form": "그쵸?ㅠ 근데 애들껀 별수없어서ㅋ",
          "speaker_id": "2",
          "time": "20210903 13:58"
        },
        {
          "id": "MDRW2100029742.1.16",
          "form": "아 애들건 안되는구나",
          "original_form": "아 애들건 안되는구나",
          "speaker_id": "1",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100029742.1.17",
          "form": "비대면 혜택을 참 많이주더라구요ㅜㅜ",
          "original_form": "비대면 혜택을 참 많이주더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100029742.1.18",
          "form": "ㅋㅋㄱ속상하네요ㅋ",
          "original_form": "ㅋㅋㄱ속상하네요ㅋ",
          "speaker_id": "2",
          "time": "20210903 15:02"
        },
        {
          "id": "MDRW2100029742.1.19",
          "form": "그나저나 단체카톡방 너무 좋은것같애요ㅋㅋ",
          "original_form": "그나저나 단체카톡방 너무 좋은것같애요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:03"
        },
        {
          "id": "MDRW2100029742.1.20",
          "form": "조ㅡㅎ은정보가 진짜 많습니다",
          "original_form": "조ㅡㅎ은정보가 진짜 많습니다",
          "speaker_id": "1",
          "time": "20210903 15:06"
        },
        {
          "id": "MDRW2100029742.1.21",
          "form": "재테크하기 딱좋아여",
          "original_form": "재테크하기 딱좋아여",
          "speaker_id": "1",
          "time": "20210903 15:06"
        },
        {
          "id": "MDRW2100029742.1.22",
          "form": "일하면서 이런 좋은부업을 이제알다니ㅠ",
          "original_form": "일하면서 이런 좋은부업을 이제알다니ㅠ",
          "speaker_id": "2",
          "time": "20210903 15:15"
        },
        {
          "id": "MDRW2100029742.1.23",
          "form": "중복청약될때 열심히 했어야했는데ㅠㅋ",
          "original_form": "중복청약될때 열심히 했어야했는데ㅠㅋ",
          "speaker_id": "2",
          "time": "20210903 15:16"
        },
        {
          "id": "MDRW2100029742.1.24",
          "form": "전 요즘드는생각이 진작에 재테크해볼걸 이러네요",
          "original_form": "전 요즘드는생각이 진작에 재테크해볼걸 이러네요",
          "speaker_id": "1",
          "time": "20210903 15:30"
        },
        {
          "id": "MDRW2100029742.1.25",
          "form": "집값도 엄청뜨고",
          "original_form": "집값도 엄청뜨고",
          "speaker_id": "1",
          "time": "20210903 15:30"
        }
      ]
    }
