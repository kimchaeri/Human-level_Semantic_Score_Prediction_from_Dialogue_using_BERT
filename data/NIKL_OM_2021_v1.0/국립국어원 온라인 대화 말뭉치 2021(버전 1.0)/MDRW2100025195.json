      "utterance": [
        {
          "id": "MDRW2100025195.1.1",
          "form": "일본여행 기억나니",
          "original_form": "일본여행 기억나니",
          "speaker_id": "1",
          "time": "20210901 00:37"
        },
        {
          "id": "MDRW2100025195.1.2",
          "form": "아 재밌었지",
          "original_form": "아 재밌었지",
          "speaker_id": "2",
          "time": "20210901 00:37"
        },
        {
          "id": "MDRW2100025195.1.3",
          "form": "온천 또가고싶어",
          "original_form": "온천 또가고싶어",
          "speaker_id": "1",
          "time": "20210901 00:38"
        },
        {
          "id": "MDRW2100025195.1.4",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210901 00:38"
        },
        {
          "id": "MDRW2100025195.1.5",
          "form": "다음에 가면 개인노천탕 있는 곳으로 가자",
          "original_form": "다음에 가면 개인노천탕 있는 곳으로 가자",
          "speaker_id": "1",
          "time": "20210901 00:38"
        },
        {
          "id": "MDRW2100025195.1.6",
          "form": "노천탕 좋았어",
          "original_form": "노천탕 좋았어",
          "speaker_id": "2",
          "time": "20210901 00:38"
        },
        {
          "id": "MDRW2100025195.1.7",
          "form": "다 혼용이니까 불편하더라",
          "original_form": "다 혼용이니까 불편하더라",
          "speaker_id": "1",
          "time": "20210901 00:38"
        },
        {
          "id": "MDRW2100025195.1.8",
          "form": "그리고 오사카 후쿠오카 당일치기는 하지말자",
          "original_form": "그리고 오사카 후쿠오카 당일치기는 하지말자",
          "speaker_id": "1",
          "time": "20210901 00:38"
        },
        {
          "id": "MDRW2100025195.1.9",
          "form": "맞아 사람있는지 확인도 잘 안되고",
          "original_form": "맞아 사람있는지 확인도 잘 안되고",
          "speaker_id": "2",
          "time": "20210901 00:39"
        },
        {
          "id": "MDRW2100025195.1.10",
          "form": "그땐 우리가 계획을 너무 무리하게 잡았지",
          "original_form": "그땐 우리가 계획을 너무 무리하게 잡았지",
          "speaker_id": "2",
          "time": "20210901 00:39"
        },
        {
          "id": "MDRW2100025195.1.11",
          "form": "패기가 넘쳤어",
          "original_form": "패기가 넘쳤어",
          "speaker_id": "1",
          "time": "20210901 00:39"
        },
        {
          "id": "MDRW2100025195.1.12",
          "form": "근데 그래서 기억에 오래남아",
          "original_form": "근데 그래서 기억에 오래남아",
          "speaker_id": "1",
          "time": "20210901 00:39"
        },
        {
          "id": "MDRW2100025195.1.13",
          "form": "어리니까 가능했었다 진짜",
          "original_form": "어리니까 가능했었다 진짜",
          "speaker_id": "2",
          "time": "20210901 00:39"
        },
        {
          "id": "MDRW2100025195.1.14",
          "form": "다음엔 홋카이도에섣",
          "original_form": "다음엔 홋카이도에섣",
          "speaker_id": "1",
          "time": "20210901 00:39"
        },
        {
          "id": "MDRW2100025195.1.15",
          "form": "도쿄로 가볼까",
          "original_form": "도쿄로 가볼까",
          "speaker_id": "1",
          "time": "20210901 00:39"
        },
        {
          "id": "MDRW2100025195.1.16",
          "form": "너무 멀지 않아??",
          "original_form": "너무 멀지 않아??",
          "speaker_id": "2",
          "time": "20210901 00:39"
        },
        {
          "id": "MDRW2100025195.1.17",
          "form": "비행기 타고 움직여야 할 거 같은데",
          "original_form": "비행기 타고 움직여야 할 거 같은데",
          "speaker_id": "2",
          "time": "20210901 00:40"
        },
        {
          "id": "MDRW2100025195.1.18",
          "form": "기억에는 잘 남을거같은데?",
          "original_form": "기억에는 잘 남을거같은데?",
          "speaker_id": "1",
          "time": "20210901 00:40"
        },
        {
          "id": "MDRW2100025195.1.19",
          "form": "비행기 한번타보자",
          "original_form": "비행기 한번타보자",
          "speaker_id": "1",
          "time": "20210901 00:40"
        },
        {
          "id": "MDRW2100025195.1.20",
          "form": "기억에 남자고 몸을 고생시키자는 거냐고",
          "original_form": "기억에 남자고 몸을 고생시키자는 거냐고",
          "speaker_id": "2",
          "time": "20210901 00:40"
        },
        {
          "id": "MDRW2100025195.1.21",
          "form": "비행기는 갈때 올때 탈거잖니",
          "original_form": "비행기는 갈때 올때 탈거잖니",
          "speaker_id": "2",
          "time": "20210901 00:40"
        },
        {
          "id": "MDRW2100025195.1.22",
          "form": "우린 이제 그렇게 건강하지 않아",
          "original_form": "우린 이제 그렇게 건강하지 않아",
          "speaker_id": "2",
          "time": "20210901 00:40"
        },
        {
          "id": "MDRW2100025195.1.23",
          "form": "일본 국내선은 특별한 경험이 될거야",
          "original_form": "일본 국내선은 특별한 경험이 될거야",
          "speaker_id": "1",
          "time": "20210901 00:41"
        },
        {
          "id": "MDRW2100025195.1.24",
          "form": "전날부터 링겔 맞고오자",
          "original_form": "전날부터 링겔 맞고오자",
          "speaker_id": "1",
          "time": "20210901 00:41"
        },
        {
          "id": "MDRW2100025195.1.25",
          "form": "왜이래 진짜",
          "original_form": "왜이래 진짜",
          "speaker_id": "2",
          "time": "20210901 00:41"
        },
        {
          "id": "MDRW2100025195.1.26",
          "form": "난 힐링보다 아웃도어파야",
          "original_form": "난 힐링보다 아웃도어파야",
          "speaker_id": "1",
          "time": "20210901 00:41"
        },
        {
          "id": "MDRW2100025195.1.27",
          "form": "격하게 여행해보자",
          "original_form": "격하게 여행해보자",
          "speaker_id": "1",
          "time": "20210901 00:41"
        },
        {
          "id": "MDRW2100025195.1.28",
          "form": "우린 안 맞는 것 같다",
          "original_form": "우린 안 맞는 것 같다",
          "speaker_id": "2",
          "time": "20210901 00:41"
        },
        {
          "id": "MDRW2100025195.1.29",
          "form": "여행은 없던 얘기로 하자",
          "original_form": "여행은 없던 얘기로 하자",
          "speaker_id": "2",
          "time": "20210901 00:41"
        }
      ]
    }
