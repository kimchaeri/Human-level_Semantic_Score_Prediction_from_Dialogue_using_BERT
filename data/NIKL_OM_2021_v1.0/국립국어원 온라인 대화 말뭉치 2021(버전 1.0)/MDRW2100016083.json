      "utterance": [
        {
          "id": "MDRW2100016083.1.1",
          "form": "오늘 닭고기 주문 좀 부탁해요",
          "original_form": "오늘 닭고기 주문 좀 부탁해요",
          "speaker_id": "2",
          "time": "20210826 09:19"
        },
        {
          "id": "MDRW2100016083.1.2",
          "form": "아 맞다.. 오늘 냉장고 보니 다 먹었더라",
          "original_form": "아 맞다.. 오늘 냉장고 보니 다 먹었더라",
          "speaker_id": "1",
          "time": "20210826 09:20"
        },
        {
          "id": "MDRW2100016083.1.3",
          "form": "ㅇㅇ 트레이더스 닭고기로",
          "original_form": "ㅇㅇ 트레이더스 닭고기로",
          "speaker_id": "2",
          "time": "20210826 09:20"
        },
        {
          "id": "MDRW2100016083.1.4",
          "form": "이름이 뭐였더라?",
          "original_form": "이름이 뭐였더라?",
          "speaker_id": "1",
          "time": "20210826 09:20"
        },
        {
          "id": "MDRW2100016083.1.5",
          "form": "그릴닭고기",
          "original_form": "그릴닭고기",
          "speaker_id": "2",
          "time": "20210826 09:20"
        },
        {
          "id": "MDRW2100016083.1.6",
          "form": "ㅇㅇ ㅎㅎ 몇 봉다리 주문해 놓을께",
          "original_form": "ㅇㅇ ㅎㅎ 몇 봉다리 주문해 놓을께",
          "speaker_id": "1",
          "time": "20210826 09:21"
        },
        {
          "id": "MDRW2100016083.1.7",
          "form": "요즘 운동 시작해서 다시 닭고기 꾸준히 먹어야 됑",
          "original_form": "요즘 운동 시작해서 다시 닭고기 꾸준히 먹어야 됑",
          "speaker_id": "2",
          "time": "20210826 09:21"
        },
        {
          "id": "MDRW2100016083.1.8",
          "form": "사 놓으니 활용도도 좋더라고",
          "original_form": "사 놓으니 활용도도 좋더라고",
          "speaker_id": "1",
          "time": "20210826 09:21"
        },
        {
          "id": "MDRW2100016083.1.9",
          "form": "글치? 맛도 좋고 건강에도 좋고",
          "original_form": "글치? 맛도 좋고 건강에도 좋고",
          "speaker_id": "2",
          "time": "20210826 09:21"
        },
        {
          "id": "MDRW2100016083.1.10",
          "form": "볶음밥, 카레, 샐러드 다 넣을 수 있으니까",
          "original_form": "볶음밥, 카레, 샐러드 다 넣을 수 있으니까",
          "speaker_id": "1",
          "time": "20210826 09:22"
        },
        {
          "id": "MDRW2100016083.1.11",
          "form": "간편하게 애들 간식으로도 좋아",
          "original_form": "간편하게 애들 간식으로도 좋아",
          "speaker_id": "2",
          "time": "20210826 09:22"
        },
        {
          "id": "MDRW2100016083.1.12",
          "form": "오늘 주문하면 내일 정도 올거야",
          "original_form": "오늘 주문하면 내일 정도 올거야",
          "speaker_id": "1",
          "time": "20210826 09:22"
        },
        {
          "id": "MDRW2100016083.1.13",
          "form": "ㅇㅇ 알았어~ 4봉지 정도 주문 부탁요",
          "original_form": "ㅇㅇ 알았어~ 4봉지 정도 주문 부탁요",
