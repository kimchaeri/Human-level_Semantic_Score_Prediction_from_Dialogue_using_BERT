        },
        {
          "id": "MDRW2100005770.1.19",
          "form": "손 잡으면 코로나에 좋지 않아요",
          "original_form": "손 잡으면 코로나에 좋지 않아요",
          "speaker_id": "2",
          "time": "20210907 11:29"
        },
        {
          "id": "MDRW2100005770.1.20",
          "form": "팔꿈치로 악수 부탁드려요",
          "original_form": "팔꿈치로 악수 부탁드려요",
          "speaker_id": "2",
          "time": "20210907 11:29"
        },
        {
          "id": "MDRW2100005770.1.21",
          "form": "팔꿈치라도 닿게해주세요",
          "original_form": "팔꿈치라도 닿게해주세요",
