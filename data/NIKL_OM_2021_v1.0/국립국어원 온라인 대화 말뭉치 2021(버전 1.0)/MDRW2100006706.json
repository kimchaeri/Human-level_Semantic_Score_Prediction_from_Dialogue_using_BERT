      "utterance": [
        {
          "id": "MDRW2100006706.1.1",
          "form": "최근에 뭐 맛있게 드셨어요?",
          "original_form": "최근에 뭐 맛있게 드셨어요?",
          "speaker_id": "1",
          "time": "20210908 07:15"
        },
        {
          "id": "MDRW2100006706.1.2",
          "form": "떡볶이요!",
          "original_form": "떡볶이요!",
          "speaker_id": "2",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100006706.1.3",
          "form": "저도 떡볶이 홀릭이에요ㅎㅎ 로제도",
          "original_form": "저도 떡볶이 홀릭이에요ㅎㅎ 로제도",
          "speaker_id": "1",
          "time": "20210908 07:17"
        },
        {
          "id": "MDRW2100006706.1.4",
          "form": "로제 완전 맛있죠~",
          "original_form": "로제 완전 맛있죠~",
          "speaker_id": "2",
          "time": "20210908 07:19"
        },
        {
          "id": "MDRW2100006706.1.5",
          "form": "로제가 토마토랑 크림인가요?",
          "original_form": "로제가 토마토랑 크림인가요?",
          "speaker_id": "1",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100006706.1.6",
          "form": "네네! 맞아요ㅎㅎ",
          "original_form": "네네! 맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 07:29"
        },
        {
          "id": "MDRW2100006706.1.7",
          "form": "근데 기름이 엄청난듯 하더라고요",
          "original_form": "근데 기름이 엄청난듯 하더라고요",
          "speaker_id": "1",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100006706.1.8",
          "form": "그건 그래요ㅠㅠ 조금 부대끼긴 하죠",
          "original_form": "그건 그래요ㅠㅠ 조금 부대끼긴 하죠",
          "speaker_id": "2",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100006706.1.9",
          "form": "하지만 맵기 잘 조절하면 찰떡궁합!",
          "original_form": "하지만 맵기 잘 조절하면 찰떡궁합!",
          "speaker_id": "1",
          "time": "20210908 07:31"
        },
        {
          "id": "MDRW2100006706.1.10",
          "form": "맞아요ㅋㅋㅋ찰떡궁합!",
          "original_form": "맞아요ㅋㅋㅋ찰떡궁합!",
          "speaker_id": "2",
          "time": "20210908 07:32"
        },
        {
          "id": "MDRW2100006706.1.11",
          "form": "치킨은 어떤 거 좋아하세요?",
          "original_form": "치킨은 어떤 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 07:32"
        },
        {
          "id": "MDRW2100006706.1.12",
          "form": "치킨은 후라이드요",
          "original_form": "치킨은 후라이드요",
          "speaker_id": "1",
          "time": "20210908 07:32"
        },
        {
          "id": "MDRW2100006706.1.13",
          "form": "추천할 것이 있으세요?",
          "original_form": "추천할 것이 있으세요?",
          "speaker_id": "1",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100006706.1.14",
          "form": "전 지코바 좋아해요!",
          "original_form": "전 지코바 좋아해요!",
          "speaker_id": "2",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100006706.1.15",
          "form": "와 그거 치밥이던가요",
          "original_form": "와 그거 치밥이던가요",
          "speaker_id": "1",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100006706.1.16",
          "form": "네 치밥하면 환상적이에요ㅋㅋㅋ",
          "original_form": "네 치밥하면 환상적이에요ㅋㅋㅋ",
