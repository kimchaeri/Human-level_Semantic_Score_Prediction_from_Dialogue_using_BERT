      "utterance": [
        {
          "id": "MDRW2100039362.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100039362.1.2",
          "form": "네 좋아하지요 ㅎㅎ name1님은요?",
          "original_form": "네 좋아하지요 ㅎㅎ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210903 17:57"
        },
        {
          "id": "MDRW2100039362.1.3",
          "form": "네 저도 좋아해요 좋으셨던 여행지 있으셨나요?",
          "original_form": "네 저도 좋아해요 좋으셨던 여행지 있으셨나요?",
          "speaker_id": "1",
          "time": "20210903 18:08"
        },
        {
          "id": "MDRW2100039362.1.4",
          "form": "저는 최근에 속초 다녀왓는데 좋앗어요 ㅎㅎ",
          "original_form": "저는 최근에 속초 다녀왓는데 좋앗어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039362.1.5",
          "form": "속초어떤점이 좋으셨나요?",
          "original_form": "속초어떤점이 좋으셨나요?",
          "speaker_id": "1",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100039362.1.6",
          "form": "바다색이 엄청 청량한게 좋앗어요",
          "original_form": "바다색이 엄청 청량한게 좋앗어요",
          "speaker_id": "2",
          "time": "20210903 18:23"
        },
        {
          "id": "MDRW2100039362.1.7",
          "form": "바다색이 참 파랗죠~",
          "original_form": "바다색이 참 파랗죠~",
          "speaker_id": "1",
          "time": "20210903 18:39"
        },
        {
          "id": "MDRW2100039362.1.8",
          "form": "맛집 있나요?",
          "original_form": "맛집 있나요?",
          "speaker_id": "1",
          "time": "20210903 18:39"
        },
        {
          "id": "MDRW2100039362.1.9",
          "form": "칼제비? 파는 곳이랑 물회 파는 곳 갔었는데 둘다 맛있엇어요",
          "original_form": "칼제비? 파는 곳이랑 물회 파는 곳 갔었는데 둘다 맛있엇어요",
          "speaker_id": "2",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100039362.1.10",
          "form": "칼제비 파는 곳은 줄이 엄청 길엇어요",
          "original_form": "칼제비 파는 곳은 줄이 엄청 길엇어요",
          "speaker_id": "2",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100039362.1.11",
          "form": "상호가 뭔가요?",
          "original_form": "상호가 뭔가요?",
          "speaker_id": "1",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100039362.1.12",
          "form": "금이야 옥이야 에요!ㅎㅎ",
          "original_form": "금이야 옥이야 에요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100039362.1.13",
          "form": "아 감사합니다~다음에 저도 가봐야겠네요",
          "original_form": "아 감사합니다~다음에 저도 가봐야겠네요",
          "speaker_id": "1",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100039362.1.14",
          "form": "네 칼국수랑 수제비 합쳐진거라는데 맛잇더라구요 ㅎㅎ",
          "original_form": "네 칼국수랑 수제비 합쳐진거라는데 맛잇더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 20:43"
        },
        {
          "id": "MDRW2100039362.1.15",
          "form": "비오는 날 먹으면 더 맛나겠어요~",
          "original_form": "비오는 날 먹으면 더 맛나겠어요~",
