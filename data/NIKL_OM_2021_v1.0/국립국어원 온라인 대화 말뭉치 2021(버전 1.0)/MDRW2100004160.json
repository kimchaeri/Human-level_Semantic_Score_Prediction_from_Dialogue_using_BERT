        },
        {
          "id": "MDRW2100004160.1.6",
          "form": "수학여행",
          "original_form": "수학여행",
          "speaker_id": "2",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004160.1.7",
          "form": "뭐 사람들이 무술?같은거하면서 연극하는 그런거였다",
          "original_form": "뭐 사람들이 무술?같은거하면서 연극하는 그런거였다",
          "speaker_id": "1",
          "time": "20210821 15:17"
        },
        {
          "id": "MDRW2100004160.1.8",
          "form": "독특했다",
          "original_form": "독특했다",
          "speaker_id": "1",
          "time": "20210821 15:17"
        },
        {
          "id": "MDRW2100004160.1.9",
          "form": "어 난타같은건가",
          "original_form": "어 난타같은건가",
          "speaker_id": "2",
          "time": "20210821 15:17"
        },
        {
          "id": "MDRW2100004160.1.10",
          "form": "난 그거 본적있음",
          "original_form": "난 그거 본적있음",
          "speaker_id": "2",
          "time": "20210821 15:17"
        },
        {
          "id": "MDRW2100004160.1.11",
          "form": "몰라 막 밟고 높이뛰어오르고 그렇던데",
          "original_form": "몰라 막 밟고 높이뛰어오르고 그렇던데",
          "speaker_id": "1",
          "time": "20210821 15:17"
        },
        {
          "id": "MDRW2100004160.1.12",
          "form": "중국에서 뭔가 엄청난",
          "original_form": "중국에서 뭔가 엄청난",
          "speaker_id": "2",
          "time": "20210821 15:17"
        },
        {
          "id": "MDRW2100004160.1.13",
          "form": "무대장치가 엄청난게 있었는데",
          "original_form": "무대장치가 엄청난게 있었는데",
          "speaker_id": "2",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004160.1.14",
          "form": "금면왕조?",
          "original_form": "금면왕조?",
          "speaker_id": "2",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004160.1.15",
          "form": "금면왕조?",
          "original_form": "금면왕조?",
          "speaker_id": "1",
          "time": "20210821 15:18"
        },
        {
          "id": "MDRW2100004160.1.16",
          "form": "모르는거다",
          "original_form": "모르는거다",
          "speaker_id": "1",
          "time": "20210821 15:18"
        },
        {
