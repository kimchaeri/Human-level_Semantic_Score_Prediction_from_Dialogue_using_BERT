      "utterance": [
        {
          "id": "MDRW2100020870.1.1",
          "form": "앗 그래도 전 사람이었어요 (배신)",
          "original_form": "앗 그래도 전 사람이었어요 (배신)",
          "speaker_id": "2",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020870.1.2",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020870.1.3",
          "form": "나쁜말이라고 차단되시는 거 아닌가요 name1님",
          "original_form": "나쁜말이라고 차단되시는 거 아닌가요 &name1&님",
          "speaker_id": "2",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020870.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020870.1.5",
          "form": "ㅋㅋㅋㅋㄱㄱㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㄱㄱㅋㄱ",
          "speaker_id": "1",
          "time": "20210828 13:42"
        },
        {
          "id": "MDRW2100020870.1.6",
          "form": "안그래도 저 자주 글이 차단되더라고요 ㅋㅋㄱ",
          "original_form": "안그래도 저 자주 글이 차단되더라고요 ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100020870.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100020870.1.8",
          "form": "지금 신입님이 7살이 어리신데",
          "original_form": "지금 신입님이 7살이 어리신데",
          "speaker_id": "1",
          "time": "20210828 13:43"
        },
        {
          "id": "MDRW2100020870.1.9",
          "form": "제가 뭔말만 하면 웃으시는데 진짜 웃겨서 웃으시는걸까...? 의구심이",
          "original_form": "제가 뭔말만 하면 웃으시는데 진짜 웃겨서 웃으시는걸까...? 의구심이",
          "speaker_id": "1",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100020870.1.10",
          "form": "아 근데 스물두살때는 정말 인생을 책임질만한 일은 전혀 생각지도 못했어요",
          "original_form": "아 근데 스물두살때는 정말 인생을 책임질만한 일은 전혀 생각지도 못했어요",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100020870.1.11",
          "form": "아 안됩니다 믿으면 안돼요",
          "original_form": "아 안됩니다 믿으면 안돼요",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100020870.1.12",
          "form": "상사의 말이니까요",
          "original_form": "상사의 말이니까요",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100020870.1.13",
          "form": "스물둘이면 그냥 놀죸ㅋㅋ",
          "original_form": "스물둘이면 그냥 놀죸ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100020870.1.14",
          "form": "그래서 되도록 말을 안겁니다",
          "original_form": "그래서 되도록 말을 안겁니다",
          "speaker_id": "1",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100020870.1.15",
          "form": "젊은이들이 웃어준다고 내가 재밌지 생각하면 망조의 지름길입니다",
          "original_form": "젊은이들이 웃어준다고 내가 재밌지 생각하면 망조의 지름길입니다",
          "speaker_id": "2",
          "time": "20210828 13:44"
        },
        {
          "id": "MDRW2100020870.1.16",
          "form": "그냥 돈만쓰고.. 접촉은 최소.. 조언은 그들이 물어볼때만..",
          "original_form": "그냥 돈만쓰고.. 접촉은 최소.. 조언은 그들이 물어볼때만..",
          "speaker_id": "2",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100020870.1.17",
          "form": "ㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210828 13:45"
        },
        {
          "id": "MDRW2100020870.1.18",
          "form": "저는 어르신(40대)가 망발하면 표정이 썩어서 어린분들이 표정관리 정말 잘하시는구나 매번 감탄합니다",
          "original_form": "저는 어르신(40대)가 망발하면 표정이 썩어서 어린분들이 표정관리 정말 잘하시는구나 매번 감탄합니다",
          "speaker_id": "1",
          "time": "20210828 13:46"
        },
        {
          "id": "MDRW2100020870.1.19",
          "form": "나중에 망발시리즈 책도 낼수있을거같아요 ㅋㅋ",
          "original_form": "나중에 망발시리즈 책도 낼수있을거같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:53"
        },
        {
          "id": "MDRW2100020870.1.20",
          "form": "ㅋㅋㅋㅋㅋ밥먹구 왔어요!",
          "original_form": "ㅋㅋㅋㅋㅋ밥먹구 왔어요!",
          "speaker_id": "2",
          "time": "20210828 14:10"
        },
        {
          "id": "MDRW2100020870.1.21",
          "form": "오 맛점하셨길 바래요",
          "original_form": "오 맛점하셨길 바래요",
          "speaker_id": "1",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020870.1.22",
          "form": "저는 이제 회사에선 헛소리 듣고 안웃기 연습하고 있어요 웃어주니까 재밌는줄 알아서...",
          "original_form": "저는 이제 회사에선 헛소리 듣고 안웃기 연습하고 있어요 웃어주니까 재밌는줄 알아서...",
          "speaker_id": "2",
          "time": "20210828 14:11"
        },
        {
          "id": "MDRW2100020870.1.23",
          "form": "ㅋㅋㅋㄱㅋ맞아요 저도 듣고 가만히 있으면 끝이 없어서 도망칩니다",
          "original_form": "ㅋㅋㅋㄱㅋ맞아요 저도 듣고 가만히 있으면 끝이 없어서 도망칩니다",
          "speaker_id": "1",
          "time": "20210828 14:12"
        },
        {
          "id": "MDRW2100020870.1.24",
          "form": "요즘 마스크있어서 넘조쿠요...남몰래 싸늘한 표정 가능",
          "original_form": "요즘 마스크있어서 넘조쿠요...남몰래 싸늘한 표정 가능",
          "speaker_id": "1",
          "time": "20210828 14:13"
        },
        {
          "id": "MDRW2100020870.1.25",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100020870.1.26",
          "form": "마스크는 직장내에서 쭉 쓰게해주면 좋겠어요",
          "original_form": "마스크는 직장내에서 쭉 쓰게해주면 좋겠어요",
          "speaker_id": "2",
          "time": "20210828 14:21"
        }
      ]
