      "utterance": [
        {
          "id": "MDRW2100044610.1.1",
          "form": "근데 그러면",
          "original_form": "근데 그러면",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044610.1.2",
          "form": "아니 나는 지금도 1층 플로어 잡을수있는데",
          "original_form": "아니 나는 지금도 1층 플로어 잡을수있는데",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044610.1.3",
          "form": "콘 하기 전에",
          "original_form": "콘 하기 전에",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044610.1.4",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044610.1.5",
          "form": "앨범 나오나?",
          "original_form": "앨범 나오나?",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044610.1.6",
          "form": "기대해도 되나",
          "original_form": "기대해도 되나",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044610.1.7",
          "form": "무리인가",
          "original_form": "무리인가",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044610.1.8",
          "form": "아니 그건 아닐거같기도한데....",
          "original_form": "아니 그건 아닐거같기도한데....",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044610.1.9",
          "form": "머르겠어",
          "original_form": "머르겠어",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044610.1.10",
          "form": "그러고보면",
          "original_form": "그러고보면",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044610.1.11",
          "form": "2컴백 안할생각인가",
          "original_form": "2컴백 안할생각인가",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044610.1.12",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044610.1.13",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044610.1.14",
          "form": "9 10월쯤 되면",
          "original_form": "9 10월쯤 되면",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044610.1.15",
          "form": "마지막 컴백",
          "original_form": "마지막 컴백",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044610.1.16",
          "form": "언제였지?",
          "original_form": "언제였지?",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044610.1.17",
          "form": "애들 좀 덜바빠질거같은데",
          "original_form": "애들 좀 덜바빠질거같은데",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044610.1.18",
          "form": "4월",
          "original_form": "4월",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044610.1.19",
          "form": "에 마지막이었죵",
          "original_form": "에 마지막이었죵",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044610.1.20",
          "form": "인사이드아웃",
          "original_form": "인사이드아웃",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100044610.1.21",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100044610.1.22",
          "form": "1컴백 더 해라",
          "original_form": "1컴백 더 해라",
