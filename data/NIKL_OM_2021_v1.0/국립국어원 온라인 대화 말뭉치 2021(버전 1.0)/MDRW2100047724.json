      "utterance": [
        {
          "id": "MDRW2100047724.1.1",
          "form": "운동화는 어디꺼 신으세요?",
          "original_form": "운동화는 어디꺼 신으세요?",
          "speaker_id": "1",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100047724.1.2",
          "form": "뭐 전 선호메이커 없어요",
          "original_form": "뭐 전 선호메이커 없어요",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100047724.1.3",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "2",
          "time": "20210827 10:29"
        },
        {
          "id": "MDRW2100047724.1.4",
          "form": "예전에는 리복이 좋았는데 실상 나이키를 많이 신어요ㅋㅋ",
          "original_form": "예전에는 리복이 좋았는데 실상 나이키를 많이 신어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:31"
        },
        {
          "id": "MDRW2100047724.1.5",
          "form": "아 그러시구나",
          "original_form": "아 그러시구나",
          "speaker_id": "2",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100047724.1.6",
          "form": "전 그냥 아무거나 특사 뜨면사는데",
          "original_form": "전 그냥 아무거나 특사 뜨면사는데",
          "speaker_id": "2",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100047724.1.7",
          "form": "사실 운동화가 불편해요 ㅡㅡ",
          "original_form": "사실 운동화가 불편해요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100047724.1.8",
          "form": "나이키는 좀 나은가요?",
          "original_form": "나이키는 좀 나은가요?",
          "speaker_id": "2",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100047724.1.9",
          "form": "잘못사면 쪼여서 걷기도 힘들고요",
          "original_form": "잘못사면 쪼여서 걷기도 힘들고요",
          "speaker_id": "2",
          "time": "20210827 10:32"
        },
        {
          "id": "MDRW2100047724.1.10",
          "form": "맞아요ㅠ 살때는 괜찮은데 신을때보면 그런것들이 있더라구요",
          "original_form": "맞아요ㅠ 살때는 괜찮은데 신을때보면 그런것들이 있더라구요",
          "speaker_id": "1",
          "time": "20210827 10:33"
        },
        {
          "id": "MDRW2100047724.1.11",
          "form": "나이키는 무난하달까.. 운동화로는 편해서 자주 신게돼요",
          "original_form": "나이키는 무난하달까.. 운동화로는 편해서 자주 신게돼요",
          "speaker_id": "1",
          "time": "20210827 10:34"
        },
        {
          "id": "MDRW2100047724.1.12",
          "form": "오 저 하나 사볼께요",
          "original_form": "오 저 하나 사볼께요",
          "speaker_id": "2",
          "time": "20210827 10:34"
        },
        {
          "id": "MDRW2100047724.1.13",
          "form": "안그래도 발언쪽이 쓸려서 아파요 물집이 ㅜㅜ",
          "original_form": "안그래도 발언쪽이 쓸려서 아파요 물집이 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 10:34"
        },
        {
          "id": "MDRW2100047724.1.14",
          "form": "오옷 괜찮으시면 좋겠어요!",
          "original_form": "오옷 괜찮으시면 좋겠어요!",
          "speaker_id": "1",
          "time": "20210827 10:35"
        },
        {
          "id": "MDRW2100047724.1.15",
          "form": "네 굳은살 되겠어요",
          "original_form": "네 굳은살 되겠어요",
          "speaker_id": "2",
          "time": "20210827 10:35"
        },
        {
          "id": "MDRW2100047724.1.16",
          "form": "운동시 심발 중요해요",
          "original_form": "운동시 심발 중요해요",
          "speaker_id": "2",
          "time": "20210827 10:35"
        },
        {
          "id": "MDRW2100047724.1.17",
          "form": "맞아요ㅠ 저도 비와서 오랜만에 다른신발 신었는데 물집이..",
          "original_form": "맞아요ㅠ 저도 비와서 오랜만에 다른신발 신었는데 물집이..",
          "speaker_id": "1",
          "time": "20210827 10:35"
        },
        {
          "id": "MDRW2100047724.1.18",
          "form": "신발",
          "original_form": "신발",
          "speaker_id": "2",
          "time": "20210827 10:35"
        },
        {
          "id": "MDRW2100047724.1.19",
          "form": "으윽ㅠㅠ 맞아요",
          "original_form": "으윽ㅠㅠ 맞아요",
          "speaker_id": "1",
          "time": "20210827 10:35"
        },
        {
          "id": "MDRW2100047724.1.20",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210827 10:35"
        },
        {
          "id": "MDRW2100047724.1.21",
          "form": "그게 진짜 저만 그런게 아니네요",
          "original_form": "그게 진짜 저만 그런게 아니네요",
          "speaker_id": "2",
          "time": "20210827 10:36"
        },
        {
          "id": "MDRW2100047724.1.22",
          "form": "발 편하게 해주기 힘드네요",
          "original_form": "발 편하게 해주기 힘드네요",
          "speaker_id": "1",
          "time": "20210827 10:36"
        },
        {
          "id": "MDRW2100047724.1.23",
          "form": "정말요! 제 걷는 방법이 잘못됐나싶기도 했는데",
          "original_form": "정말요! 제 걷는 방법이 잘못됐나싶기도 했는데",
          "speaker_id": "1",
          "time": "20210827 10:36"
        },
        {
          "id": "MDRW2100047724.1.24",
          "form": "신발 문제라고 믿고싶어요 ㅋㅋ",
          "original_form": "신발 문제라고 믿고싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:37"
        },
        {
          "id": "MDRW2100047724.1.25",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:38"
        },
        {
          "id": "MDRW2100047724.1.26",
          "form": "비오는데 미끄러지지 않게 조심하셔요",
          "original_form": "비오는데 미끄러지지 않게 조심하셔요",
          "speaker_id": "2",
          "time": "20210827 10:38"
        },
        {
          "id": "MDRW2100047724.1.27",
          "form": "넵! name2님도요!",
          "original_form": "넵! &name2&님도요!",
          "speaker_id": "1",
          "time": "20210827 10:38"
        },
        {
          "id": "MDRW2100047724.1.28",
          "form": "오늘은 실내에서 운동해야겠러요",
          "original_form": "오늘은 실내에서 운동해야겠러요",
          "speaker_id": "2",
          "time": "20210827 10:38"
        },
        {
          "id": "MDRW2100047724.1.29",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 10:38"
        },
        {
          "id": "MDRW2100047724.1.30",
          "form": "우산은요?",
          "original_form": "우산은요?",
          "speaker_id": "2",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100047724.1.31",
          "form": "비가 많이와요",
          "original_form": "비가 많이와요",
          "speaker_id": "2",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100047724.1.32",
          "form": "사셨어여?",
          "original_form": "사셨어여?",
          "speaker_id": "2",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100047724.1.33",
          "form": "이럴땐 비와서 운동도 땡이네요 ㅋㅋ",
          "original_form": "이럴땐 비와서 운동도 땡이네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:39"
        },
        {
          "id": "MDRW2100047724.1.34",
          "form": "네! name2님 덕분에 안전하게 무사히 도착했어요ㅋㅋ",
          "original_form": "네! &name2&님 덕분에 안전하게 무사히 도착했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 10:41"
        },
        {
          "id": "MDRW2100047724.1.35",
          "form": "맞아요 홈트는 가끔만 해야 할만한데 말이죠",
          "original_form": "맞아요 홈트는 가끔만 해야 할만한데 말이죠",
          "speaker_id": "1",
          "time": "20210827 10:41"
        },
        {
          "id": "MDRW2100047724.1.36",
          "form": "그러니까요 홈트 윽 잼없어요 ㅋㅋ",
          "original_form": "그러니까요 홈트 윽 잼없어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:43"
        }
      ]
