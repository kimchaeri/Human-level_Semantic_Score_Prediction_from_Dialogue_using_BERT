      "utterance": [
        {
          "id": "MDRW2100024000.1.1",
          "form": "시리우스도 솔직히 죽을 줄 몰랐구요 ㅠ.ㅠ",
          "original_form": "시리우스도 솔직히 죽을 줄 몰랐구요 ㅠ.ㅠ",
          "speaker_id": "1",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100024000.1.2",
          "form": "특히 쌍둥이 형제..... 중 한 명",
          "original_form": "특히 쌍둥이 형제..... 중 한 명",
          "speaker_id": "1",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100024000.1.3",
          "form": "저 볼 때마다 울어요",
          "original_form": "저 볼 때마다 울어요",
          "speaker_id": "1",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100024000.1.4",
          "form": "아 ㅠㅠㅠ",
          "original_form": "아 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100024000.1.5",
          "form": "맞아요 ..",
          "original_form": "맞아요 ..",
          "speaker_id": "2",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100024000.1.6",
          "form": "저는 진짜..",
          "original_form": "저는 진짜..",
          "speaker_id": "2",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100024000.1.7",
          "form": "블랙이랑 해리랑 행복하게 살길 바랐는데 ㅠㅠ",
          "original_form": "블랙이랑 해리랑 행복하게 살길 바랐는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100024000.1.8",
          "form": "그니까요........ ㅠ.ㅠ 전 의외로 헤르미온느 론 중에 한 명이 죽을 줄 알았는데",
          "original_form": "그니까요........ ㅠ.ㅠ 전 의외로 헤르미온느 론 중에 한 명이 죽을 줄 알았는데",
          "speaker_id": "1",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100024000.1.9",
          "form": "그 쪽은 안 죽더라고요...?",
          "original_form": "그 쪽은 안 죽더라고요...?",
          "speaker_id": "1",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100024000.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210830 21:12"
        },
        {
          "id": "MDRW2100024000.1.11",
          "form": "약간 조연....???",
          "original_form": "약간 조연....???",
          "speaker_id": "2",
          "time": "20210830 21:12"
        },
        {
          "id": "MDRW2100024000.1.12",
          "form": "느낌이라서 .. ㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "느낌이라서 .. ㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:12"
        },
        {
          "id": "MDRW2100024000.1.13",
          "form": "그래도",
          "original_form": "그래도",
          "speaker_id": "2",
          "time": "20210830 21:12"
        },
        {
          "id": "MDRW2100024000.1.14",
          "form": "셋 다 살아서 다행이에요..ㅋㅋㅋ",
          "original_form": "셋 다 살아서 다행이에요..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:12"
        },
        {
          "id": "MDRW2100024000.1.15",
          "form": "그르니까요 그 중에 한 명 죽었으면 저 정말",
          "original_form": "그르니까요 그 중에 한 명 죽었으면 저 정말",
          "speaker_id": "1",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100024000.1.16",
          "form": "해리포터 다시는 못 볼 어 같어요.....",
          "original_form": "해리포터 다시는 못 볼 어 같어요.....",
          "speaker_id": "1",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100024000.1.17",
          "form": "거",
          "original_form": "거",
          "speaker_id": "1",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100024000.1.18",
          "form": "맞아요 ..",
          "original_form": "맞아요 ..",
          "speaker_id": "2",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100024000.1.19",
          "form": "비중 큰 친구들 못 잃어.....",
          "original_form": "비중 큰 친구들 못 잃어.....",
          "speaker_id": "2",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100024000.1.20",
          "form": "정이 많이 들었는데",
          "original_form": "정이 많이 들었는데",
          "speaker_id": "2",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100024000.1.21",
          "form": "죽으면 정말.. ㅠㅠ",
          "original_form": "죽으면 정말.. ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:14"
        },
        {
          "id": "MDRW2100024000.1.22",
          "form": "그래서 쌍둥이 죽을 때!!!",
          "original_form": "그래서 쌍둥이 죽을 때!!!",
          "speaker_id": "1",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100024000.1.23",
          "form": "진짜 충격받았어요 ㅠㅠ",
          "original_form": "진짜 충격받았어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100024000.1.24",
          "form": "ㅠㅠ 맞아요 ㅠ",
          "original_form": "ㅠㅠ 맞아요 ㅠ",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024000.1.25",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024000.1.26",
          "form": "그 매드아이 죽었다 했을때도 ㅠ",
          "original_form": "그 매드아이 죽었다 했을때도 ㅠ",
          "speaker_id": "2",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024000.1.27",
          "form": "아 그니까요 ㅠ.^ 매드아이도 완전 허무하게?",
          "original_form": "아 그니까요 ㅠ.^ 매드아이도 완전 허무하게?",
          "speaker_id": "1",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024000.1.28",
          "form": "끝난 느낌이었어요",
          "original_form": "끝난 느낌이었어요",
          "speaker_id": "1",
          "time": "20210830 21:22"
        },
        {
          "id": "MDRW2100024000.1.29",
          "form": "그 해리 거처 옮길 때 죽은 거 맞죠",
          "original_form": "그 해리 거처 옮길 때 죽은 거 맞죠",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024000.1.30",
          "form": "맞아요!!ㅠㅠㅠ",
          "original_form": "맞아요!!ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024000.1.31",
          "form": "증말...",
          "original_form": "증말...",
          "speaker_id": "2",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024000.1.32",
          "form": "목적지에 도착했을 때",
          "original_form": "목적지에 도착했을 때",
          "speaker_id": "2",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024000.1.33",
          "form": "얼마나 조마조마했는지 몰라요 ㅠㅠ",
          "original_form": "얼마나 조마조마했는지 몰라요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024000.1.34",
          "form": "그런데 죽었다니... ㅠㅠ",
          "original_form": "그런데 죽었다니... ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100024000.1.35",
          "form": "그러니까요.... 솔직히 약간 죽는 장면 보여준 것도 아니고",
          "original_form": "그러니까요.... 솔직히 약간 죽는 장면 보여준 것도 아니고",
          "speaker_id": "1",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100024000.1.36",
          "form": "넘 생략된 거라.... ㅠ.ㅠ 그래도 매력있는 캐릭어였는데",
          "original_form": "넘 생략된 거라.... ㅠ.ㅠ 그래도 매력있는 캐릭어였는데",
          "speaker_id": "1",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100024000.1.37",
          "form": "너무 아쉬웠어요",
          "original_form": "너무 아쉬웠어요",
          "speaker_id": "1",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024000.1.38",
          "form": "그니까요 ㅠ",
          "original_form": "그니까요 ㅠ",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024000.1.39",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100024000.1.40",
          "form": "암튼 해리포터 넘 재밌었어요 ..",
          "original_form": "암튼 해리포터 넘 재밌었어요 ..",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024000.1.41",
          "form": "유니버셜 스튜디오도 가보고 싶어요..",
          "original_form": "유니버셜 스튜디오도 가보고 싶어요..",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024000.1.42",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100024000.1.43",
          "form": "헐 저도요!!!! 일본에 있는 곳 가고 싶은데",
          "original_form": "헐 저도요!!!! 일본에 있는 곳 가고 싶은데",
          "speaker_id": "1",
          "time": "20210830 21:31"
        },
        {
          "id": "MDRW2100024000.1.44",
