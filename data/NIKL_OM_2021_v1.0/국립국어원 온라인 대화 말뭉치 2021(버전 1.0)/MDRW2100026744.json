      "utterance": [
        {
          "id": "MDRW2100026744.1.1",
          "form": "오 봉사하시나요?",
          "original_form": "오 봉사하시나요?",
          "speaker_id": "2",
          "time": "20210902 09:31"
        },
        {
          "id": "MDRW2100026744.1.2",
          "form": "봉사를...안해요 ㅋㅋㅋ",
          "original_form": "봉사를...안해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:33"
        },
        {
          "id": "MDRW2100026744.1.3",
          "form": "name2 님은 봉사 하시나요???",
          "original_form": "&name2& 님은 봉사 하시나요???",
          "speaker_id": "1",
          "time": "20210902 13:34"
        },
        {
          "id": "MDRW2100026744.1.4",
          "form": "와이프한테 봉사합니다 ㅋㅋㅋ",
          "original_form": "와이프한테 봉사합니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100026744.1.5",
          "form": "사실 좋은 활동해야되는데 제가 게을러서 못하고있네요 ㅠ",
          "original_form": "사실 좋은 활동해야되는데 제가 게을러서 못하고있네요 ㅠ",
          "speaker_id": "2",
          "time": "20210902 15:04"
        },
        {
          "id": "MDRW2100026744.1.6",
          "form": "가족에게 봉사 중요하죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "가족에게 봉사 중요하죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100026744.1.7",
          "form": "전 기회되면 유기견 봉사 가보고싶긴 한데요",
          "original_form": "전 기회되면 유기견 봉사 가보고싶긴 한데요",
          "speaker_id": "1",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100026744.1.8",
          "form": "엄청 힘들다고 들었어요ㅠㅠ",
          "original_form": "엄청 힘들다고 들었어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 15:19"
        },
        {
          "id": "MDRW2100026744.1.9",
          "form": "맘 먹으시는것만으로도 시작하신겁니다",
          "original_form": "맘 먹으시는것만으로도 시작하신겁니다",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100026744.1.10",
          "form": "힘들어도 보람찰것같아요",
          "original_form": "힘들어도 보람찰것같아요",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100026744.1.11",
          "form": "강아지 밥주고 씻기고 하는건가요?",
          "original_form": "강아지 밥주고 씻기고 하는건가요?",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100026744.1.12",
          "form": "네 그런 것 같아요!",
          "original_form": "네 그런 것 같아요!",
          "speaker_id": "1",
          "time": "20210902 16:24"
        },
        {
          "id": "MDRW2100026744.1.13",
          "form": "아는 친구가 주기적으로 다니는데 체력이 많이 필요하대요",
          "original_form": "아는 친구가 주기적으로 다니는데 체력이 많이 필요하대요",
          "speaker_id": "1",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100026744.1.14",
          "form": "name2님은 어떤 봉사에 관심이 있으신가요?",
          "original_form": "&name2&님은 어떤 봉사에 관심이 있으신가요?",
          "speaker_id": "1",
          "time": "20210902 16:26"
        },
        {
          "id": "MDRW2100026744.1.15",
          "form": "사실 관심을 안가졌습니다",
          "original_form": "사실 관심을 안가졌습니다",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100026744.1.16",
          "form": "공부도해야되고 일도 해야되고 가족과 시간도 보내야되는데",
          "original_form": "공부도해야되고 일도 해야되고 가족과 시간도 보내야되는데",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100026744.1.17",
          "form": "게을러서 다 못하겟더라구요",
          "original_form": "게을러서 다 못하겟더라구요",
          "speaker_id": "2",
          "time": "20210902 17:08"
        },
        {
          "id": "MDRW2100026744.1.18",
          "form": "아기 좀 더 크면",
          "original_form": "아기 좀 더 크면",
          "speaker_id": "2",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100026744.1.19",
          "form": "같오 해보려구요",
          "original_form": "같오 해보려구요",
          "speaker_id": "2",
          "time": "20210902 17:09"
        },
        {
          "id": "MDRW2100026744.1.20",
          "form": "아 좋네요",
          "original_form": "아 좋네요",
          "speaker_id": "1",
          "time": "20210902 17:36"
        },
        {
          "id": "MDRW2100026744.1.21",
          "form": "아이들은 학교 들어가면 봉사점수 필수 같더라구요 ㅋㅋㅋ",
          "original_form": "아이들은 학교 들어가면 봉사점수 필수 같더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:37"
        },
        {
          "id": "MDRW2100026744.1.22",
          "form": "아 그런가요?오",
          "original_form": "아 그런가요?오",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100026744.1.23",
          "form": "좋은정보 !",
          "original_form": "좋은정보 !",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100026744.1.24",
          "form": "그쵸ㅠㅠㅠ",
          "original_form": "그쵸ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 17:39"
        },
        {
          "id": "MDRW2100026744.1.25",
          "form": "봉사활동도 찾아보면 좀 다양하더라구요",
          "original_form": "봉사활동도 찾아보면 좀 다양하더라구요",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100026744.1.26",
          "form": "선생님이신가요?",
          "original_form": "선생님이신가요?",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100026744.1.27",
          "form": "어찌 잘아심?",
          "original_form": "어찌 잘아심?",
          "speaker_id": "2",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100026744.1.28",
          "form": "아뇨아뇨 ㅋㅋㅋ",
          "original_form": "아뇨아뇨 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100026744.1.29",
          "form": "그냥 정규교육과정을 밟아서...ㅋㅋㅋ",
          "original_form": "그냥 정규교육과정을 밟아서...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100026744.1.30",
          "form": "그런데 요즘애들도 그런지는 모르겠네요ㅠㅠ",
          "original_form": "그런데 요즘애들도 그런지는 모르겠네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 17:40"
        },
        {
          "id": "MDRW2100026744.1.31",
          "form": "요즘도 할것 같아요",
          "original_form": "요즘도 할것 같아요",
          "speaker_id": "2",
          "time": "20210902 17:40"
        }
