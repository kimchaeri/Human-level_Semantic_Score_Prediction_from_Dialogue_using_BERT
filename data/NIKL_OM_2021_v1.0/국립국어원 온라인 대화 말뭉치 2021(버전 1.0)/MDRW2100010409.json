      "utterance": [
        {
          "id": "MDRW2100010409.1.1",
          "form": "안녕하셍노",
          "original_form": "안녕하셍노",
          "speaker_id": "1",
          "time": "20210823 17:44"
        },
        {
          "id": "MDRW2100010409.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210823 17:48"
        },
        {
          "id": "MDRW2100010409.1.3",
          "form": "군대가보셨나요?",
          "original_form": "군대가보셨나요?",
          "speaker_id": "1",
          "time": "20210823 17:50"
        },
        {
          "id": "MDRW2100010409.1.4",
          "form": "저는 여자라서 군대를 못 가네요ㅋㅋㅋ 군대 가보셨나요?",
          "original_form": "저는 여자라서 군대를 못 가네요ㅋㅋㅋ 군대 가보셨나요?",
          "speaker_id": "2",
          "time": "20210823 17:50"
        },
        {
          "id": "MDRW2100010409.1.5",
          "form": "아 저도요ㅋㅋㅋ 같은이유로요",
          "original_form": "아 저도요ㅋㅋㅋ 같은이유로요",
          "speaker_id": "1",
          "time": "20210823 17:51"
        },
        {
          "id": "MDRW2100010409.1.6",
          "form": "요즘은 군대에서 핸드폰이",
          "original_form": "요즘은 군대에서 핸드폰이",
          "speaker_id": "1",
          "time": "20210823 17:51"
        },
        {
          "id": "MDRW2100010409.1.7",
          "form": "자유롭다고하더라고요",
          "original_form": "자유롭다고하더라고요",
          "speaker_id": "1",
          "time": "20210823 17:51"
        },
        {
          "id": "MDRW2100010409.1.8",
          "form": "아 그래요? 군대에 있을 지인이 없어서 몰랐네요ㅋㅋㅋ 얼마나 자유롭다고 하던가요?",
          "original_form": "아 그래요? 군대에 있을 지인이 없어서 몰랐네요ㅋㅋㅋ 얼마나 자유롭다고 하던가요?",
          "speaker_id": "2",
          "time": "20210823 18:00"
        },
        {
          "id": "MDRW2100010409.1.9",
          "form": "저도그냥",
          "original_form": "저도그냥",
          "speaker_id": "1",
          "time": "20210824 10:40"
        },
        {
          "id": "MDRW2100010409.1.10",
          "form": "뉴스로 본거라서요ㅎㅎ",
          "original_form": "뉴스로 본거라서요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 10:40"
        },
        {
          "id": "MDRW2100010409.1.11",
          "form": "아ㅋㅋㅋ 이거 여자 둘은 대화할 거리가 없네요...ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ 이거 여자 둘은 대화할 거리가 없네요...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 10:40"
        },
        {
          "id": "MDRW2100010409.1.12",
          "form": "군대가 요즘",
          "original_form": "군대가 요즘",
          "speaker_id": "1",
          "time": "20210824 10:57"
        },
        {
          "id": "MDRW2100010409.1.13",
          "form": "2년이던가요?!",
          "original_form": "2년이던가요?!",
          "speaker_id": "1",
          "time": "20210824 10:57"
        },
        {
          "id": "MDRW2100010409.1.14",
          "form": "엇... 잘 모르는데 2년보다 적은 걸로 알고 있어요",
          "original_form": "엇... 잘 모르는데 2년보다 적은 걸로 알고 있어요",
          "speaker_id": "2",
          "time": "20210824 11:00"
        },
        {
          "id": "MDRW2100010409.1.15",
          "form": "그렇군요ㅋㅋㅋ 군인들월급도",
          "original_form": "그렇군요ㅋㅋㅋ 군인들월급도",
          "speaker_id": "1",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100010409.1.16",
          "form": "올랐다고하더라구요",
          "original_form": "올랐다고하더라구요",
          "speaker_id": "1",
          "time": "20210824 11:06"
        },
        {
          "id": "MDRW2100010409.1.17",
          "form": "오 저는 최근에 군인 배식 받은 걸 뉴스에서 봤는데 엄청 적게 주더라고요",
          "original_form": "오 저는 최근에 군인 배식 받은 걸 뉴스에서 봤는데 엄청 적게 주더라고요",
          "speaker_id": "2",
          "time": "20210824 11:08"
        },
        {
