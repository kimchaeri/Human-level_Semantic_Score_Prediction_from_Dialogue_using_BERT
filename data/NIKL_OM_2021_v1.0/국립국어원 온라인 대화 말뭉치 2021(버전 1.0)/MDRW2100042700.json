      "utterance": [
        {
          "id": "MDRW2100042700.1.1",
          "form": "아 그게 쇼케인가?",
          "original_form": "아 그게 쇼케인가?",
          "speaker_id": "2",
          "time": "20210907 01:52"
        },
        {
          "id": "MDRW2100042700.1.2",
          "form": "아 아니다",
          "original_form": "아 아니다",
          "speaker_id": "2",
          "time": "20210907 01:52"
        },
        {
          "id": "MDRW2100042700.1.3",
          "form": "따로 찍은 영상ㅇㅇㅇ",
          "original_form": "따로 찍은 영상ㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 01:52"
        },
        {
          "id": "MDRW2100042700.1.4",
          "form": "스튜디오라이브라니",
          "original_form": "스튜디오라이브라니",
          "speaker_id": "1",
          "time": "20210907 01:52"
        },
        {
          "id": "MDRW2100042700.1.5",
          "form": "스제가",
          "original_form": "스제가",
          "speaker_id": "1",
          "time": "20210907 01:52"
        },
        {
          "id": "MDRW2100042700.1.6",
          "form": "그거 원래도 주긴 했었는데",
          "original_form": "그거 원래도 주긴 했었는데",
          "speaker_id": "2",
          "time": "20210907 01:53"
        },
        {
          "id": "MDRW2100042700.1.7",
          "form": "이번 이오데때부터 힘 더 쓴 느낌..",
          "original_form": "이번 이오데때부터 힘 더 쓴 느낌..",
          "speaker_id": "2",
          "time": "20210907 01:53"
        },
        {
          "id": "MDRW2100042700.1.8",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210907 01:53"
        },
        {
          "id": "MDRW2100042700.1.9",
          "form": "아번 영현이 솔로는 아예 오늘 바로 올려준거 보니까",
          "original_form": "아번 영현이 솔로는 아예 오늘 바로 올려준거 보니까",
          "speaker_id": "2",
          "time": "20210907 01:53"
        },
        {
          "id": "MDRW2100042700.1.10",
          "form": "찍어놓은 거 더 있나봐",
          "original_form": "찍어놓은 거 더 있나봐",
          "speaker_id": "2",
          "time": "20210907 01:54"
        },
        {
          "id": "MDRW2100042700.1.11",
          "form": "오홍",
          "original_form": "오홍",
          "speaker_id": "1",
          "time": "20210907 01:56"
        },
        {
          "id": "MDRW2100042700.1.12",
          "form": "베스트송",
          "original_form": "베스트송",
          "speaker_id": "1",
          "time": "20210907 01:56"
        },
        {
          "id": "MDRW2100042700.1.13",
          "form": "이거제취향이군여",
          "original_form": "이거제취향이군여",
          "speaker_id": "1",
          "time": "20210907 01:56"
        },
        {
          "id": "MDRW2100042700.1.14",
          "form": "그거 뭔가 편하게? 듣기에 좋더라고여 사운드가 차분한 건 아니지만",
          "original_form": "그거 뭔가 편하게? 듣기에 좋더라고여 사운드가 차분한 건 아니지만",
          "speaker_id": "2",
          "time": "20210907 01:57"
        },
        {
          "id": "MDRW2100042700.1.15",
          "form": "라디오에서",
          "original_form": "라디오에서",
          "speaker_id": "2",
          "time": "20210907 01:58"
        },
        {
          "id": "MDRW2100042700.1.16",
          "form": "신청곡으로",
          "original_form": "신청곡으로",
          "speaker_id": "2",
          "time": "20210907 01:58"
        },
        {
          "id": "MDRW2100042700.1.17",
          "form": "나올듯한",
          "original_form": "나올듯한",
          "speaker_id": "2",
          "time": "20210907 01:58"
        },
        {
          "id": "MDRW2100042700.1.18",
          "form": "느낌으로",
          "original_form": "느낌으로",
          "speaker_id": "2",
          "time": "20210907 01:58"
        },
        {
          "id": "MDRW2100042700.1.19",
          "form": "ㅋㅋㅋㄱㄱ전",
          "original_form": "ㅋㅋㅋㄱㄱ전",
          "speaker_id": "1",
          "time": "20210907 01:58"
        },
        {
          "id": "MDRW2100042700.1.20",
          "form": "차분한건 안듣는사람이라",
          "original_form": "차분한건 안듣는사람이라",
          "speaker_id": "1",
          "time": "20210907 01:58"
        },
        {
          "id": "MDRW2100042700.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 01:58"
        },
        {
          "id": "MDRW2100042700.1.22",
          "form": "저도 신나는 게 좋아...",
          "original_form": "저도 신나는 게 좋아...",
          "speaker_id": "2",
          "time": "20210907 01:58"
        },
        {
          "id": "MDRW2100042700.1.23",
          "form": "차분한 것도 싫진 않지만",
          "original_form": "차분한 것도 싫진 않지만",
          "speaker_id": "2",
          "time": "20210907 01:59"
        },
        {
          "id": "MDRW2100042700.1.24",
          "form": "둘중에 하나 고르라면 신나는거 쪽이라",
          "original_form": "둘중에 하나 고르라면 신나는거 쪽이라",
          "speaker_id": "2",
          "time": "20210907 01:59"
        },
        {
          "id": "MDRW2100042700.1.25",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210907 01:59"
        },
        {
          "id": "MDRW2100042700.1.26",
          "form": "아저는 ㅊ7분하지만",
          "original_form": "아저는 ㅊ7분하지만",
          "speaker_id": "1",
          "time": "20210907 01:59"
        },
        {
          "id": "MDRW2100042700.1.27",
          "form": "아마 맨 마지막 트랙은 좀 심심하실수도 있겠어여",
          "original_form": "아마 맨 마지막 트랙은 좀 심심하실수도 있겠어여",
          "speaker_id": "2",
          "time": "20210907 01:59"
        },
        {
          "id": "MDRW2100042700.1.28",
          "form": "밝은노랜 ㅇㅋ",
          "original_form": "밝은노랜 ㅇㅋ",
          "speaker_id": "1",
          "time": "20210907 01:59"
        },
        {
          "id": "MDRW2100042700.1.29",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210907 01:59"
        },
        {
          "id": "MDRW2100042700.1.30",
          "form": "차분하면서 몹시우울한노래는",
          "original_form": "차분하면서 몹시우울한노래는",
          "speaker_id": "1",
          "time": "20210907 01:59"
        },
        {
          "id": "MDRW2100042700.1.31",
          "form": "처지는 슬픈노래",
          "original_form": "처지는 슬픈노래",
          "speaker_id": "2",
          "time": "20210907 01:59"
        },
        {
          "id": "MDRW2100042700.1.32",
          "form": "안듣는편",
          "original_form": "안듣는편",
          "speaker_id": "1",
          "time": "20210907 01:59"
        },
        {
          "id": "MDRW2100042700.1.33",
          "form": "기빨려...",
          "original_form": "기빨려...",
          "speaker_id": "2",
          "time": "20210907 02:00"
        },
        {
          "id": "MDRW2100042700.1.34",
          "form": "사랑은얼어죽을ㅋㅋㅋㅋㅋ",
          "original_form": "사랑은얼어죽을ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 02:00"
        },
        {
          "id": "MDRW2100042700.1.35",
          "form": "아니 방금까지 네 마이크로폰이될게",
          "original_form": "아니 방금까지 네 마이크로폰이될게",
          "speaker_id": "1",
          "time": "20210907 02:00"
        },
        {
          "id": "MDRW2100042700.1.36",
          "form": "하다가",
          "original_form": "하다가",
          "speaker_id": "1",
          "time": "20210907 02:00"
        }
      ]
