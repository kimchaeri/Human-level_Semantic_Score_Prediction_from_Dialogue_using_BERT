      "utterance": [
        {
          "id": "MDRW2100008660.1.1",
          "form": "최근에 사신 물건은 어떤거세요?",
          "original_form": "최근에 사신 물건은 어떤거세요?",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100008660.1.2",
          "form": "저는 귤모자를구매했어요",
          "original_form": "저는 귤모자를구매했어요",
          "speaker_id": "2",
          "time": "20210822 21:54"
        },
        {
          "id": "MDRW2100008660.1.3",
          "form": "엇. 어떤건지 궁금하네요",
          "original_form": "엇. 어떤건지 궁금하네요",
          "speaker_id": "1",
          "time": "20210822 21:55"
        },
        {
          "id": "MDRW2100008660.1.4",
          "form": "제주귤모자인데 귀여워요",
          "original_form": "제주귤모자인데 귀여워요",
          "speaker_id": "2",
          "time": "20210822 21:56"
        },
        {
          "id": "MDRW2100008660.1.5",
          "form": "여행갈때쓰려고샀는데 못가고있어요",
          "original_form": "여행갈때쓰려고샀는데 못가고있어요",
          "speaker_id": "2",
          "time": "20210822 21:57"
        },
        {
          "id": "MDRW2100008660.1.6",
          "form": "앗본것같아요ㅋㅋ",
          "original_form": "앗본것같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 21:57"
        },
        {
          "id": "MDRW2100008660.1.7",
          "form": "네 평소엔쓰고다닐수없어서ㅋㅋ",
          "original_form": "네 평소엔쓰고다닐수없어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 21:57"
        },
        {
          "id": "MDRW2100008660.1.8",
          "form": "못가면사촌동생들 선물주려구요",
          "original_form": "못가면사촌동생들 선물주려구요",
          "speaker_id": "2",
          "time": "20210822 21:57"
        },
        {
          "id": "MDRW2100008660.1.9",
          "form": "ㅋㅋㅋ 애들이 쓰면 귀엽겠네요",
          "original_form": "ㅋㅋㅋ 애들이 쓰면 귀엽겠네요",
          "speaker_id": "1",
          "time": "20210822 22:57"
        },
        {
          "id": "MDRW2100008660.1.10",
          "form": "맞아요ㅎㅎ초등학생이라",
          "original_form": "맞아요ㅎㅎ초등학생이라",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100008660.1.11",
          "form": "인싸템될듯",
          "original_form": "인싸템될듯",
          "speaker_id": "2",
          "time": "20210822 23:06"
        },
        {
          "id": "MDRW2100008660.1.12",
          "form": "와 그러게요~이거머야 이거머야 난리",
          "original_form": "와 그러게요~이거머야 이거머야 난리",
          "speaker_id": "1",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100008660.1.13",
          "form": "요즘 특이한모자많더라구요",
          "original_form": "요즘 특이한모자많더라구요",
          "speaker_id": "2",
          "time": "20210823 02:10"
        },
        {
          "id": "MDRW2100008660.1.14",
          "form": "귀엽더라구요~ 저는 모자가 그렇게 안어울리더라구요ㅋㅋ특히 캡ㅋㅋ",
          "original_form": "귀엽더라구요~ 저는 모자가 그렇게 안어울리더라구요ㅋㅋ특히 캡ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 10:47"
        },
        {
          "id": "MDRW2100008660.1.15",
          "form": "저도캡모자는별로인데",
          "original_form": "저도캡모자는별로인데",
          "speaker_id": "2",
          "time": "20210823 21:03"
        },
        {
          "id": "MDRW2100008660.1.16",
          "form": "요즘벙거지모자유행이라 사볼려구요ㅋㅋ",
          "original_form": "요즘벙거지모자유행이라 사볼려구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:03"
        },
        {
          "id": "MDRW2100008660.1.17",
          "form": "벙거지~ 사서하나 써봤는데 음..ㅋㅋ  애매?ㅋㅋ했어요ㅋㅋㅋㅋㅋ",
