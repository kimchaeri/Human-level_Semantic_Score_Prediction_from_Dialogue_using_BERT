        },
        {
          "id": "MDRW2100046592.1.2",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "2",
          "time": "20211101 13:32"
        },
        {
          "id": "MDRW2100046592.1.3",
          "form": "점심 드셨나용",
          "original_form": "점심 드셨나용",
          "speaker_id": "2",
          "time": "20211101 13:32"
        },
        {
          "id": "MDRW2100046592.1.4",
          "form": "아항.. 저도 점심 먹고 왔어요",
          "original_form": "아항.. 저도 점심 먹고 왔어요",
          "speaker_id": "1",
          "time": "20211101 13:45"
        },
        {
          "id": "MDRW2100046592.1.5",
          "form": "뭐드셨어요!?",
          "original_form": "뭐드셨어요!?",
          "speaker_id": "2",
          "time": "20211101 13:58"
        },
        {
          "id": "MDRW2100046592.1.6",
          "form": "학생이라 급식 먹었어요!",
          "original_form": "학생이라 급식 먹었어요!",
          "speaker_id": "1",
          "time": "20211101 15:52"
        },
        {
          "id": "MDRW2100046592.1.7",
          "form": "짚신매운돼지갈비찜 나왔어요",
          "original_form": "짚신매운돼지갈비찜 나왔어요",
          "speaker_id": "1",
          "time": "20211101 15:52"
        },
        {
          "id": "MDRW2100046592.1.8",
          "form": "아하 ㅎㅎㅎㅎ",
          "original_form": "아하 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211101 15:52"
        },
        {
          "id": "MDRW2100046592.1.9",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20211101 15:52"
        },
        {
          "id": "MDRW2100046592.1.10",
          "form": "급식인데 저런 고퀄리티 메뉴가 나오는군요 ㅎㅎ",
          "original_form": "급식인데 저런 고퀄리티 메뉴가 나오는군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211101 15:52"
        },
        {
          "id": "MDRW2100046592.1.11",
          "form": "이름만 거창하고",
          "original_form": "이름만 거창하고",
          "speaker_id": "1",
          "time": "20211101 15:52"
        },
        {
          "id": "MDRW2100046592.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211101 15:52"
        },
        {
          "id": "MDRW2100046592.1.13",
          "form": "실제로 보면 고퀄리틴 아니에요 ㅋㅋㅋ",
          "original_form": "실제로 보면 고퀄리틴 아니에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20211101 15:52"
        },
        {
          "id": "MDRW2100046592.1.14",
          "form": "그래도 맛있게 드셨죠?ㅋㅋㅋㅋㅋㅎㅎ",
          "original_form": "그래도 맛있게 드셨죠?ㅋㅋㅋㅋㅋㅎㅎ",
          "speaker_id": "2",
          "time": "20211101 15:52"
        },
        {
          "id": "MDRW2100046592.1.15",
          "form": "갈비찜은..못참죠",
          "original_form": "갈비찜은..못참죠",
          "speaker_id": "2",
          "time": "20211101 15:52"
        },
        {
          "id": "MDRW2100046592.1.16",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211101 15:52"
        },
        {
          "id": "MDRW2100046592.1.17",
          "form": "넼ㅋㅋㅋㅋ 맛은 있었네요",
          "original_form": "넼ㅋㅋㅋㅋ 맛은 있었네요",
          "speaker_id": "1",
          "time": "20211101 15:52"
        },
        {
          "id": "MDRW2100046592.1.18",
          "form": "석식도 드세요?",
          "original_form": "석식도 드세요?",
          "speaker_id": "2",
          "time": "20211101 15:53"
        },
        {
          "id": "MDRW2100046592.1.19",
          "form": "아녀... 학교에서 석식은 안해줘요",
          "original_form": "아녀... 학교에서 석식은 안해줘요",
          "speaker_id": "1",
          "time": "20211101 15:53"
        },
        {
          "id": "MDRW2100046592.1.20",
          "form": "엇",
          "original_form": "엇",
          "speaker_id": "2",
          "time": "20211101 15:53"
        },
        {
          "id": "MDRW2100046592.1.21",
          "form": "왜죱",
          "original_form": "왜죱",
          "speaker_id": "2",
          "time": "20211101 15:53"
        },
        {
          "id": "MDRW2100046592.1.22",
          "form": "코로나 때문인가용..",
          "original_form": "코로나 때문인가용..",
          "speaker_id": "2",
          "time": "20211101 15:54"
        },
        {
          "id": "MDRW2100046592.1.23",
          "form": "그렇죠.. 야자를 안하니 석식할 필요도 없어여",
          "original_form": "그렇죠.. 야자를 안하니 석식할 필요도 없어여",
          "speaker_id": "1",
          "time": "20211101 15:54"
        },
        {
          "id": "MDRW2100046592.1.24",
          "form": "아 그러네요 ㅠㅠ",
          "original_form": "아 그러네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211101 15:56"
        },
        {
          "id": "MDRW2100046592.1.25",
          "form": "오늘부터 위드코로나인데",
          "original_form": "오늘부터 위드코로나인데",
          "speaker_id": "2",
          "time": "20211101 15:56"
        },
        {
          "id": "MDRW2100046592.1.26",
          "form": "야자 다시 안하나요?",
          "original_form": "야자 다시 안하나요?",
