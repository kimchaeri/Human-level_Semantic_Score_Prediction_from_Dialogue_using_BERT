      "utterance": [
        {
          "id": "MDRW2100002581.1.1",
          "form": "좋아하시는 음식있으신가요?",
          "original_form": "좋아하시는 음식있으신가요?",
          "speaker_id": "1",
          "time": "20210820 14:03"
        },
        {
          "id": "MDRW2100002581.1.2",
          "form": "전 매운거좋아해요",
          "original_form": "전 매운거좋아해요",
          "speaker_id": "2",
          "time": "20210820 14:46"
        },
        {
          "id": "MDRW2100002581.1.3",
          "form": "오 엽떡",
          "original_form": "오 엽떡",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002581.1.4",
          "form": "매운맛드시나요?",
          "original_form": "매운맛드시나요?",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002581.1.5",
          "form": "당연하죠",
          "original_form": "당연하죠",
          "speaker_id": "2",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002581.1.6",
          "form": "전 매운건못먹겠더라고요ㅠ",
          "original_form": "전 매운건못먹겠더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002581.1.7",
          "form": "초보맛먹습니다",
          "original_form": "초보맛먹습니다",
          "speaker_id": "1",
          "time": "20210820 15:21"
        },
        {
          "id": "MDRW2100002581.1.8",
          "form": "이런 아직 분발하셔야하네요!",
          "original_form": "이런 아직 분발하셔야하네요!",
          "speaker_id": "2",
          "time": "20210820 15:29"
        },
        {
          "id": "MDRW2100002581.1.9",
          "form": "불닭은 그냥",
          "original_form": "불닭은 그냥",
          "speaker_id": "1",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002581.1.10",
          "form": "껌이시겠네요?!",
          "original_form": "껌이시겠네요?!",
          "speaker_id": "1",
          "time": "20210820 15:30"
        },
        {
          "id": "MDRW2100002581.1.11",
          "form": "네 좋아해요~!!",
          "original_form": "네 좋아해요~!!",
          "speaker_id": "2",
          "time": "20210820 15:31"
        },
        {
          "id": "MDRW2100002581.1.12",
          "form": "로제는 어떤가요?",
          "original_form": "로제는 어떤가요?",
          "speaker_id": "1",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002581.1.13",
          "form": "전 그냥 매운맛파인데",
          "original_form": "전 그냥 매운맛파인데",
          "speaker_id": "2",
          "time": "20210820 15:37"
        },
        {
          "id": "MDRW2100002581.1.14",
          "form": "요즘은 로제도 괜찮은ㄱ,ㅓ같아요",
          "original_form": "요즘은 로제도 괜찮은ㄱ,ㅓ같아요",
          "speaker_id": "2",
          "time": "20210820 15:38"
        },
        {
          "id": "MDRW2100002581.1.15",
          "form": "전 로제요즘너무좋더라구요",
          "original_form": "전 로제요즘너무좋더라구요",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002581.1.16",
          "form": "스파게티면 추가 강추요!",
          "original_form": "스파게티면 추가 강추요!",
          "speaker_id": "1",
          "time": "20210820 15:42"
        },
        {
          "id": "MDRW2100002581.1.17",
          "form": "오 추가 기억할게요!!",
          "original_form": "오 추가 기억할게요!!",
          "speaker_id": "2",
          "time": "20210820 20:07"
        },
        {
          "id": "MDRW2100002581.1.18",
          "form": "네네 배떡보다는",
          "original_form": "네네 배떡보다는",
          "speaker_id": "1",
          "time": "20210822 18:11"
        },
        {
          "id": "MDRW2100002581.1.19",
          "form": "떡군이네가 더 맛있는것같아요ㅋㅋㅋ",
          "original_form": "떡군이네가 더 맛있는것같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 18:12"
        },
        {
          "id": "MDRW2100002581.1.20",
          "form": "ㅋㅋ오늘은 로제찜잙입니다!!",
          "original_form": "ㅋㅋ오늘은 로제찜잙입니다!!",
          "speaker_id": "2",
          "time": "20210822 18:15"
        },
        {
          "id": "MDRW2100002581.1.21",
          "form": "좋네요ㅋㅋㅋ",
          "original_form": "좋네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:53"
        }
      ]
