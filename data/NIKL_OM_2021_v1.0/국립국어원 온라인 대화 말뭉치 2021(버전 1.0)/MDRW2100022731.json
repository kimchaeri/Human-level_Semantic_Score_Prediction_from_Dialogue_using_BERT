      "utterance": [
        {
          "id": "MDRW2100022731.1.1",
          "form": "연애하세여?",
          "original_form": "연애하세여?",
          "speaker_id": "1",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022731.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210829 17:28"
        },
        {
          "id": "MDRW2100022731.1.3",
          "form": "언제쯤?",
          "original_form": "언제쯤?",
          "speaker_id": "1",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022731.1.4",
          "form": "오래됐어요",
          "original_form": "오래됐어요",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022731.1.5",
          "form": "한국인 맞으세요? 주어가 없네",
          "original_form": "한국인 맞으세요? 주어가 없네",
          "speaker_id": "2",
          "time": "20210829 17:30"
        },
        {
          "id": "MDRW2100022731.1.6",
          "form": "비혼주의자는 아니시죠?",
          "original_form": "비혼주의자는 아니시죠?",
          "speaker_id": "1",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022731.1.7",
          "form": "맞춰보세요",
          "original_form": "맞춰보세요",
          "speaker_id": "2",
          "time": "20210829 17:32"
        },
        {
          "id": "MDRW2100022731.1.8",
          "form": "음 연애세포는 있으신듯",
          "original_form": "음 연애세포는 있으신듯",
          "speaker_id": "1",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022731.1.9",
          "form": "오 어떤 부분에서 그렇게 생각했어요",
          "original_form": "오 어떤 부분에서 그렇게 생각했어요",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022731.1.10",
          "form": "오래됐다는 점에서 생각없다는 뜻은 아니잖아요",
          "original_form": "오래됐다는 점에서 생각없다는 뜻은 아니잖아요",
          "speaker_id": "1",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022731.1.11",
          "form": "오 예리하다",
          "original_form": "오 예리하다",
          "speaker_id": "2",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100022731.1.12",
          "form": "유미의 세포들 본적 있어요?",
          "original_form": "유미의 세포들 본적 있어요?",
          "speaker_id": "2",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100022731.1.13",
          "form": "웹툰이죠?",
          "original_form": "웹툰이죠?",
          "speaker_id": "1",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100022731.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100022731.1.15",
          "form": "연애세포 이야기하길래",
          "original_form": "연애세포 이야기하길래",
          "speaker_id": "2",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022731.1.16",
          "form": "물어봤어요 그냥",
          "original_form": "물어봤어요 그냥",
          "speaker_id": "2",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022731.1.17",
          "form": "저도 들어만 봤어요",
          "original_form": "저도 들어만 봤어요",
          "speaker_id": "1",
          "time": "20210829 17:38"
        }
