      "utterance": [
        {
          "id": "MDRW2100006456.1.1",
          "form": "다회용컵쓰지??",
          "original_form": "다회용컵쓰지??",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100006456.1.2",
          "form": "텀블러 있지",
          "original_form": "텀블러 있지",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100006456.1.3",
          "form": "나는 스타벅스에서 저번에 하나 샀어 붉은색",
          "original_form": "나는 스타벅스에서 저번에 하나 샀어 붉은색",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100006456.1.4",
          "form": "들고 다닐 수 있을 땐 들고다니고",
          "original_form": "들고 다닐 수 있을 땐 들고다니고",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100006456.1.5",
          "form": "잘했네",
          "original_form": "잘했네",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100006456.1.6",
          "form": "아 꽤 됐다 ㅋㅋㅋ",
          "original_form": "아 꽤 됐다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100006456.1.7",
          "form": "나중에 자랑해줘",
          "original_form": "나중에 자랑해줘",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100006456.1.8",
          "form": "스타벅스 텀블러 예쁘잖아",
          "original_form": "스타벅스 텀블러 예쁘잖아",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100006456.1.9",
          "form": "오 좋지 !! 이쁘다구",
          "original_form": "오 좋지 !! 이쁘다구",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100006456.1.10",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100006456.1.11",
          "form": "스타벅스 기프티콘 많아서 산건데 요즘 다회용컵 쓰면 막 할인해줘서",
          "original_form": "스타벅스 기프티콘 많아서 산건데 요즘 다회용컵 쓰면 막 할인해줘서",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100006456.1.12",
          "form": "난 비싸서 안 사긴했는데 ㅋㅋ예쁘지",
          "original_form": "난 비싸서 안 사긴했는데 ㅋㅋ예쁘지",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100006456.1.13",
          "form": "지나다니면서 쏠쏠히 쓰는중",
          "original_form": "지나다니면서 쏠쏠히 쓰는중",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100006456.1.14",
          "form": "맞아 500원씩 할인해주나?",
          "original_form": "맞아 500원씩 할인해주나?",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100006456.1.15",
          "form": "그정도는 아닐껄",
          "original_form": "그정도는 아닐껄",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100006456.1.16",
          "form": "한 100원일텐데?",
          "original_form": "한 100원일텐데?",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100006456.1.17",
          "form": "그래? 할인 꽤 해줬던 것 같은데",
          "original_form": "그래? 할인 꽤 해줬던 것 같은데",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100006456.1.18",
          "form": "ㅋㅋㅋㅋ아닌가? 잘 모르겠당",
          "original_form": "ㅋㅋㅋㅋ아닌가? 잘 모르겠당",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100006456.1.19",
          "form": "스벅 안간지 오래라 잘 모르겠다",
          "original_form": "스벅 안간지 오래라 잘 모르겠다",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100006456.1.20",
          "form": "나도 ㅋㅋㅋㅋㅋ",
          "original_form": "나도 ㅋㅋㅋㅋㅋ",
