      "utterance": [
        {
          "id": "MDRW2100009523.1.1",
          "form": "싸이월드 비지엠 뭐였나요",
          "original_form": "싸이월드 비지엠 뭐였나요",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009523.1.2",
          "form": "저는 그 달콤한 커피 어쩌구",
          "original_form": "저는 그 달콤한 커피 어쩌구",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009523.1.3",
          "form": "근데 싸이월드 첨에 부활할 때",
          "original_form": "근데 싸이월드 첨에 부활할 때",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009523.1.4",
          "form": "개인정보 우려있었는데 그건 어케됐는지 모르겠어요",
          "original_form": "개인정보 우려있었는데 그건 어케됐는지 모르겠어요",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009523.1.5",
          "form": "내..내과거사진이..안지워져 있다고..?",
          "original_form": "내..내과거사진이..안지워져 있다고..?",
          "speaker_id": "2",
          "time": "20210823 00:07"
        },
        {
          "id": "MDRW2100009523.1.6",
          "form": "저 빅뱅...노래...메들리였습니다....",
          "original_form": "저 빅뱅...노래...메들리였습니다....",
          "speaker_id": "1",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009523.1.7",
          "form": "..",
          "original_form": "..",
          "speaker_id": "1",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009523.1.8",
          "form": "쓰읍..",
          "original_form": "쓰읍..",
          "speaker_id": "2",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009523.1.9",
          "form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009523.1.10",
          "form": "아아..",
          "original_form": "아아..",
          "speaker_id": "2",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009523.1.11",
          "form": "미안합니다-거미",
          "original_form": "미안합니다-거미",
          "speaker_id": "2",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009523.1.12",
          "form": "그거 마지막으로 기사봤을땐 가오픈할때 미리 지울 수 있다고 했던거 같은데",
          "original_form": "그거 마지막으로 기사봤을땐 가오픈할때 미리 지울 수 있다고 했던거 같은데",
          "speaker_id": "1",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009523.1.13",
          "form": "그거 아이디 찾기도 못하는게 번호 바뀌었으면 안된다고 들어서",
          "original_form": "그거 아이디 찾기도 못하는게 번호 바뀌었으면 안된다고 들어서",
          "speaker_id": "2",
          "time": "20210823 00:08"
        },
        {
          "id": "MDRW2100009523.1.14",
          "form": "아니 하긴..커뮤니티에 사진올렸다가 지워도 구글링하면 정보가 남아있다던데 싸이월드도 똑같은거겠죠?",
          "original_form": "아니 하긴..커뮤니티에 사진올렸다가 지워도 구글링하면 정보가 남아있다던데 싸이월드도 똑같은거겠죠?",
          "speaker_id": "1",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009523.1.15",
          "form": "아?",
          "original_form": "아?",
          "speaker_id": "1",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009523.1.16",
          "form": "부활을 왜 멋대로 하는건가 싶었거든요",
          "original_form": "부활을 왜 멋대로 하는건가 싶었거든요",
          "speaker_id": "2",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009523.1.17",
          "form": "아놔 전 도토리가 증발했네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㅜㅜ",
          "original_form": "아놔 전 도토리가 증발했네요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009523.1.18",
          "form": "근데 막상 부활한다니 잘해보라고 하고싶곸ㅋㅋㅋ",
          "original_form": "근데 막상 부활한다니 잘해보라고 하고싶곸ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009523.1.19",
          "form": "아나",
          "original_form": "아나",
          "speaker_id": "2",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009523.1.20",
          "form": "번호 3번이나 바꿧는데?",
          "original_form": "번호 3번이나 바꿧는데?",
          "speaker_id": "1",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009523.1.21",
          "form": "얘들아...",
          "original_form": "얘들아...",
          "speaker_id": "1",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009523.1.22",
          "form": "도토리 그거 다 돈인데",
          "original_form": "도토리 그거 다 돈인데",
          "speaker_id": "2",
          "time": "20210823 00:09"
        },
        {
          "id": "MDRW2100009523.1.23",
          "form": "이건 .. 환불해죠라",
          "original_form": "이건 .. 환불해죠라",
          "speaker_id": "2",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100009523.1.24",
          "form": "환불도 못해주겠네 아이디도 못찾으니까..",
          "original_form": "환불도 못해주겠네 아이디도 못찾으니까..",
          "speaker_id": "1",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100009523.1.25",
          "form": "삼성 샀으면 올랐을텐데 도토리여서 삭았다",
          "original_form": "삼성 샀으면 올랐을텐데 도토리여서 삭았다",
          "speaker_id": "2",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100009523.1.26",
          "form": "심지어 이메일도 새로 만들었다고요ㅠ",
          "original_form": "심지어 이메일도 새로 만들었다고요ㅠ",
          "speaker_id": "1",
          "time": "20210823 00:10"
        },
        {
          "id": "MDRW2100009523.1.27",
          "form": "!!!!",
          "original_form": "!!!!",
