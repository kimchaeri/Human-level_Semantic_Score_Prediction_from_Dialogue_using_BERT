      "utterance": [
        {
          "id": "MDRW2100043025.1.1",
          "form": "아 ㅋㅋㅋㅋㅋ넘 웃겨요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋ넘 웃겨요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100043025.1.2",
          "form": "진짜 맛있을 걱 ㄱ같아요",
          "original_form": "진짜 맛있을 걱 ㄱ같아요",
          "speaker_id": "2",
          "time": "20210907 13:16"
        },
        {
          "id": "MDRW2100043025.1.3",
          "form": "중간에 한번굴려주세요",
          "original_form": "중간에 한번굴려주세요",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043025.1.4",
          "form": "타면..ㅠ많이 쓰더라구요",
          "original_form": "타면..ㅠ많이 쓰더라구요",
          "speaker_id": "1",
          "time": "20210907 13:18"
        },
        {
          "id": "MDRW2100043025.1.5",
          "form": "앗 그렇군요,, 와 완전 알쓸신잡이에요 넘 유용한",
          "original_form": "앗 그렇군요,, 와 완전 알쓸신잡이에요 넘 유용한",
          "speaker_id": "2",
          "time": "20210907 13:19"
        },
        {
          "id": "MDRW2100043025.1.6",
          "form": "ㅋㅋㅋㅋ다른과자도 돌려보심 ㄱㅊ은게 좀 나와요",
          "original_form": "ㅋㅋㅋㅋ다른과자도 돌려보심 ㄱㅊ은게 좀 나와요",
          "speaker_id": "1",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100043025.1.7",
          "form": "칙촉도 ㄱㅊ습니다",
          "original_form": "칙촉도 ㄱㅊ습니다",
          "speaker_id": "1",
          "time": "20210907 13:20"
        },
        {
          "id": "MDRW2100043025.1.8",
          "form": "헉 지금 칙촉 있는데 도전해봐야겠어요ㅋㅋ",
          "original_form": "헉 지금 칙촉 있는데 도전해봐야겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:21"
        },
        {
          "id": "MDRW2100043025.1.9",
          "form": "많이 돌림딱딱해져요ㅠㅠ!!",
          "original_form": "많이 돌림딱딱해져요ㅠㅠ!!",
          "speaker_id": "1",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100043025.1.10",
          "form": "살짝만돌려야됨!!!",
          "original_form": "살짝만돌려야됨!!!",
          "speaker_id": "1",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100043025.1.11",
          "form": "헉 몇 분 정도 돌려야될까요",
          "original_form": "헉 몇 분 정도 돌려야될까요",
          "speaker_id": "2",
          "time": "20210907 13:22"
        },
        {
          "id": "MDRW2100043025.1.12",
          "form": "아 웃ㄱㅕㅠ 지령내려드립니다",
          "original_form": "아 웃ㄱㅕㅠ 지령내려드립니다",
          "speaker_id": "1",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100043025.1.13",
          "form": "170도 2분?좀안되게 하심될꺼에요",
          "original_form": "170도 2분?좀안되게 하심될꺼에요",
          "speaker_id": "1",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100043025.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ감사합니당 완전 좋아요ㅠㅠㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ감사합니당 완전 좋아요ㅠㅠㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100043025.1.15",
          "form": "에어프라이어 제 손으로 돌려본 적이 없어서..",
          "original_form": "에어프라이어 제 손으로 돌려본 적이 없어서..",
          "speaker_id": "2",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100043025.1.16",
          "form": "ㅋㅋㄱ저도 과자아니었응 안돌렸다",
          "original_form": "ㅋㅋㄱ저도 과자아니었응 안돌렸다",
