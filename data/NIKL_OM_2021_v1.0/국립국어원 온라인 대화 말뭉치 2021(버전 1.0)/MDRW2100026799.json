        },
        {
          "id": "MDRW2100026799.1.2",
          "form": "안녕!",
          "original_form": "안녕!",
          "speaker_id": "2",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100026799.1.3",
          "form": "너는 어디에 살아?",
          "original_form": "너는 어디에 살아?",
          "speaker_id": "1",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100026799.1.4",
          "form": "나는 전남쪽! 너는?",
          "original_form": "나는 전남쪽! 너는?",
          "speaker_id": "2",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026799.1.5",
          "form": "나는 경남! 완전 반대편에 살고 있당ㅋㅋㅋㅋㅋ",
          "original_form": "나는 경남! 완전 반대편에 살고 있당ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026799.1.6",
          "form": "헐 진짜 ㅋㅋㅋㅋㅋㅋ 양옆으로 갈라져있네 ㅋㅋㅋㅋ",
          "original_form": "헐 진짜 ㅋㅋㅋㅋㅋㅋ 양옆으로 갈라져있네 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:15"
        },
        {
          "id": "MDRW2100026799.1.7",
          "form": "여행.. 여행가고싶다",
          "original_form": "여행.. 여행가고싶다",
          "speaker_id": "2",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026799.1.8",
          "form": "최근에 여행 간거 어디야?",
          "original_form": "최근에 여행 간거 어디야?",
          "speaker_id": "2",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026799.1.9",
          "form": "오 여행 좋아하는구나",
          "original_form": "오 여행 좋아하는구나",
          "speaker_id": "1",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026799.1.10",
          "form": "나는 한 달 전에 여수 놀러가려고 했었는데ㅠㅠ",
          "original_form": "나는 한 달 전에 여수 놀러가려고 했었는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026799.1.11",
          "form": "코로나 때문에 취소했어..ㅎㅎ 여행 너무 가고싶다",
          "original_form": "코로나 때문에 취소했어..ㅎㅎ 여행 너무 가고싶다",
          "speaker_id": "1",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026799.1.12",
          "form": "나도 코로나땜에 요즘 어디 못가서ㅜㅜ",
          "original_form": "나도 코로나땜에 요즘 어디 못가서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026799.1.13",
          "form": "그냥 차타고 드라이브만 해 ..",
          "original_form": "그냥 차타고 드라이브만 해 ..",
          "speaker_id": "2",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026799.1.14",
          "form": "와! 차 있어?? 대박 부럽당",
          "original_form": "와! 차 있어?? 대박 부럽당",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026799.1.15",
          "form": "그치ㅜㅜ 코로나 땜에 어딜 다닐 수가 없어ㅠㅠㅠ",
          "original_form": "그치ㅜㅜ 코로나 땜에 어딜 다닐 수가 없어ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 00:20"
        },
        {
          "id": "MDRW2100026799.1.16",
          "form": "진짜 코로나땜에 넘ㅠㅜㅜ",
          "original_form": "진짜 코로나땜에 넘ㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 00:21"
        },
        {
          "id": "MDRW2100026799.1.17",
          "form": "서울사는친구가 놀러오라고 예전부터ㅜ하는데",
          "original_form": "서울사는친구가 놀러오라고 예전부터ㅜ하는데",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026799.1.18",
          "form": "진짜 서울안간지도 1년넘은거같애...",
          "original_form": "진짜 서울안간지도 1년넘은거같애...",
          "speaker_id": "2",
          "time": "20210902 00:22"
        },
        {
          "id": "MDRW2100026799.1.19",
          "form": "ㅋㅋㅋ와ㅠㅠㅠ진짜 서울 안간지 엄청 오래됐다",
          "original_form": "ㅋㅋㅋ와ㅠㅠㅠ진짜 서울 안간지 엄청 오래됐다",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100026799.1.20",
          "form": "해외 여행은 꿈도 못 꾸니 국내 여행이라도 가고 싶은데ㅠ",
          "original_form": "해외 여행은 꿈도 못 꾸니 국내 여행이라도 가고 싶은데ㅠ",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100026799.1.21",
          "form": "그것도 너무 힘든 것 같아...",
          "original_form": "그것도 너무 힘든 것 같아...",
          "speaker_id": "1",
          "time": "20210902 00:24"
        },
        {
          "id": "MDRW2100026799.1.22",
          "form": "그치....ㅜㅠ 진짜 코로나전에 갔어야하는데 ㅠㅠㅠ 고민만하다 못하고 ㅜㅜㅜㅜㅜ",
          "original_form": "그치....ㅜㅠ 진짜 코로나전에 갔어야하는데 ㅠㅠㅠ 고민만하다 못하고 ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 00:25"
        },
        {
          "id": "MDRW2100026799.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ원래 집순이라 여행 휴가 철에만 다녔는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ원래 집순이라 여행 휴가 철에만 다녔는데",
          "speaker_id": "1",
          "time": "20210902 00:29"
        },
        {
          "id": "MDRW2100026799.1.24",
          "form": "최근 1년 반동안 여행이 너무 간절해졌어...",
