      "utterance": [
        {
          "id": "MDRW2100017925.1.1",
          "form": "코로나 백신 맞으셨나요?",
          "original_form": "코로나 백신 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210826 21:42"
        },
        {
          "id": "MDRW2100017925.1.2",
          "form": "1차만 맞았어요",
          "original_form": "1차만 맞았어요",
          "speaker_id": "2",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017925.1.3",
          "form": "이제 다음주에 2차 맞아여",
          "original_form": "이제 다음주에 2차 맞아여",
          "speaker_id": "2",
          "time": "20210826 21:43"
        },
        {
          "id": "MDRW2100017925.1.4",
          "form": "오 저도 다음주에 2차 맞는데",
          "original_form": "오 저도 다음주에 2차 맞는데",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017925.1.5",
          "form": "1차 때는 건강 이상 반응 없으셨어요?",
          "original_form": "1차 때는 건강 이상 반응 없으셨어요?",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017925.1.6",
          "form": "네네 그냥 근육통 정도?",
          "original_form": "네네 그냥 근육통 정도?",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017925.1.7",
          "form": "2차가 찐이래서 무섭네요",
          "original_form": "2차가 찐이래서 무섭네요",
          "speaker_id": "2",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017925.1.8",
          "form": "저도 팔만 이틀 아팡ㅎ는데",
          "original_form": "저도 팔만 이틀 아팡ㅎ는데",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017925.1.9",
          "form": "아팠는데",
          "original_form": "아팠는데",
          "speaker_id": "1",
          "time": "20210826 21:44"
        },
        {
          "id": "MDRW2100017925.1.10",
          "form": "타이레놀 사놔야해요",
          "original_form": "타이레놀 사놔야해요",
          "speaker_id": "2",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017925.1.11",
          "form": "제 주변에 2차 맞은 사람들 중에 크게 앓은 분들도 있어서",
          "original_form": "제 주변에 2차 맞은 사람들 중에 크게 앓은 분들도 있어서",
          "speaker_id": "1",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017925.1.12",
          "form": "1차때 근육통 정도였던 사람도요",
          "original_form": "1차때 근육통 정도였던 사람도요",
          "speaker_id": "1",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017925.1.13",
          "form": "걱정되기는 해요ㅜ",
          "original_form": "걱정되기는 해요ㅜ",
          "speaker_id": "1",
          "time": "20210826 21:45"
        },
        {
          "id": "MDRW2100017925.1.14",
          "form": "ㅠㅠ토요일에 맞는데 주말내내 집에서 요양해야해요",
          "original_form": "ㅠㅠ토요일에 맞는데 주말내내 집에서 요양해야해요",
          "speaker_id": "2",
          "time": "20210826 21:46"
        },
        {
          "id": "MDRW2100017925.1.15",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017925.1.16",
          "form": "저도 주말엔 운동도 외출도 다 자제하고 쉬려고요",
          "original_form": "저도 주말엔 운동도 외출도 다 자제하고 쉬려고요",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100017925.1.17",
          "form": "부디 무탈하게 지나가길 바라요!",
          "original_form": "부디 무탈하게 지나가길 바라요!",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017925.1.18",
          "form": "그냥 1차땐 유서 쓰고 가겠다 유난이었는데",
          "original_form": "그냥 1차땐 유서 쓰고 가겠다 유난이었는데",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100017925.1.19",
          "form": "근데 또 2차때 무사히 지나간 사람들도 많으니",
          "original_form": "근데 또 2차때 무사히 지나간 사람들도 많으니",
          "speaker_id": "1",
          "time": "20210826 21:49"
        },
        {
          "id": "MDRW2100017925.1.20",
          "form": "저희도 무탈하게 지나갈 거라 믿어요!",
          "original_form": "저희도 무탈하게 지나갈 거라 믿어요!",
          "speaker_id": "1",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017925.1.21",
          "form": "넵 화이자 맞으시죠?",
          "original_form": "넵 화이자 맞으시죠?",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100017925.1.22",
          "form": "네네 화이자요",
          "original_form": "네네 화이자요",
          "speaker_id": "1",
          "time": "20210826 21:51"
        }
      ]
