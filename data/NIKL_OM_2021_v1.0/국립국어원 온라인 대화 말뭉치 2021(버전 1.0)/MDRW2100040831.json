      "utterance": [
        {
          "id": "MDRW2100040831.1.1",
          "form": "그리고 암이 유전성 클 거 같아요",
          "original_form": "그리고 암이 유전성 클 거 같아요",
          "speaker_id": "1",
          "time": "20210905 09:16"
        },
        {
          "id": "MDRW2100040831.1.2",
          "form": "네 가족력이 영향이 있다더라고요",
          "original_form": "네 가족력이 영향이 있다더라고요",
          "speaker_id": "2",
          "time": "20210905 09:16"
        },
        {
          "id": "MDRW2100040831.1.3",
          "form": "저희 엄마 동생의 시아버지",
          "original_form": "저희 엄마 동생의 시아버지",
          "speaker_id": "1",
          "time": "20210905 09:17"
        },
        {
          "id": "MDRW2100040831.1.4",
          "form": "엄청 근육맨이셨는데 암으로 돌아가셨어요",
          "original_form": "엄청 근육맨이셨는데 암으로 돌아가셨어요",
          "speaker_id": "1",
          "time": "20210905 09:17"
        },
        {
          "id": "MDRW2100040831.1.5",
          "form": "에휴",
          "original_form": "에휴",
          "speaker_id": "2",
          "time": "20210905 09:17"
        },
        {
          "id": "MDRW2100040831.1.6",
          "form": "그럼 이모부의 아버지신 거절",
          "original_form": "그럼 이모부의 아버지신 거절",
          "speaker_id": "2",
          "time": "20210905 09:18"
        },
        {
          "id": "MDRW2100040831.1.7",
          "form": "거죠?",
          "original_form": "거죠?",
          "speaker_id": "2",
          "time": "20210905 09:18"
        },
        {
          "id": "MDRW2100040831.1.8",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210905 09:18"
        },
        {
          "id": "MDRW2100040831.1.9",
          "form": "나중에 이모 부도 똑같은 암 걸렸어요",
          "original_form": "나중에 이모 부도 똑같은 암 걸렸어요",
          "speaker_id": "1",
          "time": "20210905 09:19"
        },
        {
          "id": "MDRW2100040831.1.10",
          "form": "이모부께서도 건강관리 잘하셔야겠어요",
          "original_form": "이모부께서도 건강관리 잘하셔야겠어요",
          "speaker_id": "2",
          "time": "20210905 09:19"
        },
        {
          "id": "MDRW2100040831.1.11",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210905 09:19"
        },
        {
          "id": "MDRW2100040831.1.12",
          "form": "가족력이 ㄷㄷㄷ",
          "original_form": "가족력이 ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210905 09:19"
        },
        {
          "id": "MDRW2100040831.1.13",
          "form": "벌써 걸리셨구나",
          "original_form": "벌써 걸리셨구나",
          "speaker_id": "2",
          "time": "20210905 09:19"
        },
        {
          "id": "MDRW2100040831.1.14",
          "form": "그나마 빨리 수술해서 잘 살아 있어요",
          "original_form": "그나마 빨리 수술해서 잘 살아 있어요",
          "speaker_id": "1",
          "time": "20210905 09:19"
        },
        {
          "id": "MDRW2100040831.1.15",
          "form": "이모부는 괜찮으세요 지금?",
          "original_form": "이모부는 괜찮으세요 지금?",
          "speaker_id": "2",
          "time": "20210905 09:19"
        },
        {
          "id": "MDRW2100040831.1.16",
          "form": "아 다행이네요",
          "original_form": "아 다행이네요",
          "speaker_id": "2",
          "time": "20210905 09:19"
        },
        {
          "id": "MDRW2100040831.1.17",
          "form": "네 근데 막 산 거 아니고 평소 때 생식도 하고 건강해 신경 많이 썼거든요",
          "original_form": "네 근데 막 산 거 아니고 평소 때 생식도 하고 건강해 신경 많이 썼거든요",
          "speaker_id": "1",
          "time": "20210905 09:19"
        },
        {
          "id": "MDRW2100040831.1.18",
          "form": "건강검진 정기적으로 받아야하는 것 같아요",
          "original_form": "건강검진 정기적으로 받아야하는 것 같아요",
          "speaker_id": "2",
          "time": "20210905 09:19"
        },
        {
          "id": "MDRW2100040831.1.19",
          "form": "근데 그렇게 걸리니까 좀 어이없더라고요",
          "original_form": "근데 그렇게 걸리니까 좀 어이없더라고요",
          "speaker_id": "1",
          "time": "20210905 09:19"
        }
      ]
