      "utterance": [
        {
          "id": "MDRW2100006523.1.1",
          "form": "병장월급 많아졌더라구?",
          "original_form": "병장월급 많아졌더라구?",
          "speaker_id": "1",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100006523.1.2",
          "form": "많아져야지",
          "original_form": "많아져야지",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100006523.1.3",
          "form": "더 받아도 된다고 생각해 사실",
          "original_form": "더 받아도 된다고 생각해 사실",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100006523.1.4",
          "form": "오 얼마가 적당하다구 생각행?",
          "original_form": "오 얼마가 적당하다구 생각행?",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100006523.1.5",
          "form": "최저가 보장되었으면 하지만 일단 저정도라도 만족하지",
          "original_form": "최저가 보장되었으면 하지만 일단 저정도라도 만족하지",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100006523.1.6",
          "form": "하루에 8시간 이상 일하니까 사실",
          "original_form": "하루에 8시간 이상 일하니까 사실",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100006523.1.7",
          "form": "음 최저면 한 300정도 나올라나?",
          "original_form": "음 최저면 한 300정도 나올라나?",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100006523.1.8",
          "form": "아 아니다 ㅋㅋㅋ 너무 최저를 높게 본거 같다",
          "original_form": "아 아니다 ㅋㅋㅋ 너무 최저를 높게 본거 같다",
          "speaker_id": "1",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100006523.1.9",
          "form": "200좀 안되겠다",
          "original_form": "200좀 안되겠다",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100006523.1.10",
          "form": "이것저것 다 하면 250 정도 잡히지 않으려나",
          "original_form": "이것저것 다 하면 250 정도 잡히지 않으려나",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100006523.1.11",
          "form": "새벽 근무도 이틀에 한번 씩은 있으니까",
          "original_form": "새벽 근무도 이틀에 한번 씩은 있으니까",
          "speaker_id": "2",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100006523.1.12",
          "form": "아 맞네 새벽은 더 주니까",
          "original_form": "아 맞네 새벽은 더 주니까",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100006523.1.13",
          "form": "고렿겠다",
          "original_form": "고렿겠다",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100006523.1.14",
          "form": "응응 그냥 고생한만큼 모으고 나오게 해줬으면 해",
          "original_form": "응응 그냥 고생한만큼 모으고 나오게 해줬으면 해",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100006523.1.15",
          "form": "나 같은 경우는 진짜 안 쓰고 최대한 긁어 모았는데 2년동안 400정도더라 허무했어 사실",
          "original_form": "나 같은 경우는 진짜 안 쓰고 최대한 긁어 모았는데 2년동안 400정도더라 허무했어 사실",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100006523.1.16",
          "form": "얼마 모으면 좋을까",
          "original_form": "얼마 모으면 좋을까",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100006523.1.17",
          "form": "몇백 정도??",
          "original_form": "몇백 정도??",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100006523.1.18",
          "form": "근데 100씩 받으면 몇천 받을 수 있겠다",
          "original_form": "근데 100씩 받으면 몇천 받을 수 있겠다",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100006523.1.19",
          "form": "아니 천...",
          "original_form": "아니 천...",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100006523.1.20",
          "form": "근데 병장 때만 100이니까",
          "original_form": "근데 병장 때만 100이니까",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100006523.1.21",
          "form": "병장 3개월 하거든",
          "original_form": "병장 3개월 하거든",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100006523.1.22",
          "form": "아 맞네",
          "original_form": "아 맞네",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100006523.1.23",
          "form": "똑똑하구망",
          "original_form": "똑똑하구망",
          "speaker_id": "1",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100006523.1.24",
          "form": "응응 사실 난 천에 근접하게 모을 수 있었으면 해",
          "original_form": "응응 사실 난 천에 근접하게 모을 수 있었으면 해",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100006523.1.25",
          "form": "2년 동안 악착같이 모아서 400 500 인데 사실 그정도는 방학동안 열심히 알바하면 버는 돈이니까",
          "original_form": "2년 동안 악착같이 모아서 400 500 인데 사실 그정도는 방학동안 열심히 알바하면 버는 돈이니까",
          "speaker_id": "2",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100006523.1.26",
          "form": "나오고나서 기분이 애매하더라고",
          "original_form": "나오고나서 기분이 애매하더라고",
          "speaker_id": "2",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100006523.1.27",
          "form": "글쿠망 아무래도 ㅠ 천 모으면 참 좋은데",
          "original_form": "글쿠망 아무래도 ㅠ 천 모으면 참 좋은데",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100006523.1.28",
          "form": "그래도 좋은 방향으로 나아가고 있어서 다행이다",
          "original_form": "그래도 좋은 방향으로 나아가고 있어서 다행이다",
          "speaker_id": "1",
          "time": "20210908 01:23"
        },
        {
          "id": "MDRW2100006523.1.29",
          "form": "맞아맞아 뭐 난 이미 지난 일이지만",
          "original_form": "맞아맞아 뭐 난 이미 지난 일이지만",
