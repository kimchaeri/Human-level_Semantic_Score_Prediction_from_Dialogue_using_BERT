      "utterance": [
        {
          "id": "MDRW2100004259.1.1",
          "form": "혹시 학교 다녀?",
          "original_form": "혹시 학교 다녀?",
          "speaker_id": "2",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004259.1.2",
          "form": "응응 대학교",
          "original_form": "응응 대학교",
          "speaker_id": "1",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004259.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210821 15:47"
        },
        {
          "id": "MDRW2100004259.1.4",
          "form": "나도! 이제 곧 개강이네ㅜㅜ",
          "original_form": "나도! 이제 곧 개강이네ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 15:49"
        },
        {
          "id": "MDRW2100004259.1.5",
          "form": "그러게.. 수강신청은 했어? 나는 23일이야",
          "original_form": "그러게.. 수강신청은 했어? 나는 23일이야",
          "speaker_id": "1",
          "time": "20210821 15:50"
        },
        {
          "id": "MDRW2100004259.1.6",
          "form": "바로 다음주네? 응 수강신청은 지난주쯤? 한 거 같애",
          "original_form": "바로 다음주네? 응 수강신청은 지난주쯤? 한 거 같애",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004259.1.7",
          "form": "원하는 수업 잘 잡았어?",
          "original_form": "원하는 수업 잘 잡았어?",
          "speaker_id": "1",
          "time": "20210821 15:56"
        },
        {
          "id": "MDRW2100004259.1.8",
          "form": "응 나는 들을 거 별로 없어서 수강신청 수월하게 했어",
          "original_form": "응 나는 들을 거 별로 없어서 수강신청 수월하게 했어",
          "speaker_id": "2",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004259.1.9",
          "form": "수강신청 잘 됐어?",
          "original_form": "수강신청 잘 됐어?",
          "speaker_id": "2",
          "time": "20210821 16:00"
        },
        {
          "id": "MDRW2100004259.1.10",
          "form": "노노 다음주 월욜이 수강신청이야",
          "original_form": "노노 다음주 월욜이 수강신청이야",
          "speaker_id": "1",
          "time": "20210821 16:17"
        },
        {
          "id": "MDRW2100004259.1.11",
          "form": "아 수강신청이 23일이라는거구나",
          "original_form": "아 수강신청이 23일이라는거구나",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004259.1.12",
          "form": "헉 되게 늦네 신청할 거 많아?",
          "original_form": "헉 되게 늦네 신청할 거 많아?",
          "speaker_id": "2",
          "time": "20210821 16:26"
        },
        {
          "id": "MDRW2100004259.1.13",
          "form": "미리 장바구니 담아놔서 한번에 누르기만 하면 되는데",
          "original_form": "미리 장바구니 담아놔서 한번에 누르기만 하면 되는데",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004259.1.14",
          "form": "튕기지만 않았음 좋겠어",
          "original_form": "튕기지만 않았음 좋겠어",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004259.1.15",
          "form": "응응 개강은 9월 1일",
          "original_form": "응응 개강은 9월 1일",
          "speaker_id": "1",
          "time": "20210821 16:28"
        },
        {
          "id": "MDRW2100004259.1.16",
          "form": "잘 됐으면 좋겠다",
          "original_form": "잘 됐으면 좋겠다",
          "speaker_id": "2",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004259.1.17",
          "form": "나는 이번에 거의 인원초과 안 돼서 그냥 일괄로 처리돼서",
          "original_form": "나는 이번에 거의 인원초과 안 돼서 그냥 일괄로 처리돼서",
          "speaker_id": "2",
          "time": "20210821 16:43"
        },
        {
          "id": "MDRW2100004259.1.18",
          "form": "수업은 비대면으로 해?",
          "original_form": "수업은 비대면으로 해?",
          "speaker_id": "2",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004259.1.19",
          "form": "오 다행이네",
          "original_form": "오 다행이네",
          "speaker_id": "1",
          "time": "20210821 16:45"
        },
        {
          "id": "MDRW2100004259.1.20",
          "form": "우리는 비대면 예정이야 너도 그래??",
          "original_form": "우리는 비대면 예정이야 너도 그래??",
          "speaker_id": "1",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004259.1.21",
          "form": "응 나도 10월정도까진 비대면이더라고",
          "original_form": "응 나도 10월정도까진 비대면이더라고",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004259.1.22",
          "form": "근데 아직 제대로 된 공지는 없고 기숙사입사할 학생들한테만 얘기했더라",
          "original_form": "근데 아직 제대로 된 공지는 없고 기숙사입사할 학생들한테만 얘기했더라",
          "speaker_id": "2",
          "time": "20210821 17:02"
        }
      ]
