        },
        {
          "id": "MDRW2100042559.1.2",
          "form": "60후반인줄 ㄷㄷ",
          "original_form": "60후반인줄 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042559.1.3",
          "form": "체중계올라간거 직접보여줬음ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "체중계올라간거 직접보여줬음ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042559.1.4",
          "form": "와 ㄷㄷㄷㄷ",
          "original_form": "와 ㄷㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042559.1.5",
          "form": "나도 안믿겨서",
          "original_form": "나도 안믿겨서",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042559.1.6",
          "form": "계속 구.라하지말라햇거든",
          "original_form": "계속 구.라하지말라햇거든",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042559.1.7",
          "form": "그럼 아이돌들이나 배우들은",
          "original_form": "그럼 아이돌들이나 배우들은",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042559.1.8",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "2",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042559.1.9",
          "form": "그러니까 직접 보여주더라ㅋㅋㅋ",
          "original_form": "그러니까 직접 보여주더라ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:52"
        },
        {
          "id": "MDRW2100042559.1.10",
          "form": "진짜 이쁜데",
          "original_form": "진짜 이쁜데",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042559.1.11",
          "form": "건강 다 망칠것 같아",
          "original_form": "건강 다 망칠것 같아",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042559.1.12",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042559.1.13",
          "form": "솔직히 tv로보면",
          "original_form": "솔직히 tv로보면",
          "speaker_id": "1",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042559.1.14",
          "form": "음영 다 사라져서 더 부하게 보여서 예쁘게보이는거지",
          "original_form": "음영 다 사라져서 더 부하게 보여서 예쁘게보이는거지",
          "speaker_id": "1",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042559.1.15",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042559.1.16",
          "form": "실제로보면 예쁘게 보이는거보다 너무 앙상해보일거같음 ㅠㅠ",
          "original_form": "실제로보면 예쁘게 보이는거보다 너무 앙상해보일거같음 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042559.1.17",
          "form": "맞아 ㅠㅠㅠㅠ",
          "original_form": "맞아 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042559.1.18",
          "form": "그리고 카메라에",
          "original_form": "그리고 카메라에",
          "speaker_id": "2",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100042559.1.19",
          "form": "원래 뚱뚱하게 잡힌다며",
          "original_form": "원래 뚱뚱하게 잡힌다며",
          "speaker_id": "2",
