      "utterance": [
        {
          "id": "MDRW2100026781.1.1",
          "form": "쇼핑중독에서 날씨로 넘어왔네요ㅋ",
          "original_form": "쇼핑중독에서 날씨로 넘어왔네요ㅋ",
          "speaker_id": "1",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026781.1.2",
          "form": "아침저녁으로 쌀쌀해졌어요",
          "original_form": "아침저녁으로 쌀쌀해졌어요",
          "speaker_id": "2",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026781.1.3",
          "form": "요즘 날마다 비가와서...죽겠어요",
          "original_form": "요즘 날마다 비가와서...죽겠어요",
          "speaker_id": "1",
          "time": "20210902 00:02"
        },
        {
          "id": "MDRW2100026781.1.4",
          "form": "이젠 가을인가봐요",
          "original_form": "이젠 가을인가봐요",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026781.1.5",
          "form": "덥다덥다 더워죽겠다 그랬는더",
          "original_form": "덥다덥다 더워죽겠다 그랬는더",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026781.1.6",
          "form": "곧 명절이라....",
          "original_form": "곧 명절이라....",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026781.1.7",
          "form": "태풍도 자주 올 것같아서",
          "original_form": "태풍도 자주 올 것같아서",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026781.1.8",
          "form": "무섭네요 ㅋㅋ",
          "original_form": "무섭네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026781.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026781.1.10",
          "form": "지금 밖에서 비와 번개가 난리입니다.",
          "original_form": "지금 밖에서 비와 번개가 난리입니다.",
          "speaker_id": "1",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100026781.1.11",
          "form": "미세먼지는 요즘에 관심이 밀렸어요",
          "original_form": "미세먼지는 요즘에 관심이 밀렸어요",
          "speaker_id": "2",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026781.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026781.1.13",
          "form": "코로나 이후로..",
          "original_form": "코로나 이후로..",
          "speaker_id": "2",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026781.1.14",
          "form": "한창 미세먼지로 걱정이였는데",
          "original_form": "한창 미세먼지로 걱정이였는데",
          "speaker_id": "1",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026781.1.15",
          "form": "코로나로 황사니 미세먼지는 그냥 지나가네요",
          "original_form": "코로나로 황사니 미세먼지는 그냥 지나가네요",
          "speaker_id": "1",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026781.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026781.1.17",
          "form": "걱정도 안됨요",
          "original_form": "걱정도 안됨요",
          "speaker_id": "2",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026781.1.18",
          "form": "매일 미세먼지 농도 챙겨봤는데",
          "original_form": "매일 미세먼지 농도 챙겨봤는데",
          "speaker_id": "2",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026781.1.19",
          "form": "요즘은 매일 비라..",
          "original_form": "요즘은 매일 비라..",
          "speaker_id": "1",
          "time": "20210902 00:05"
        },
        {
          "id": "MDRW2100026781.1.20",
          "form": "언제 비 안오나..\n일기예보만 봅니다!",
          "original_form": "언제 비 안오나..\n일기예보만 봅니다!",
          "speaker_id": "1",
          "time": "20210902 00:05"
