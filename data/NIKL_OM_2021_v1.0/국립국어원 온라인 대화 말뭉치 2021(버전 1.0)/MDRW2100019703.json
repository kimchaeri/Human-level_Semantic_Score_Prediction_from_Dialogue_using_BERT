      "utterance": [
        {
          "id": "MDRW2100019703.1.1",
          "form": "네 늘하죠 ㅜ",
          "original_form": "네 늘하죠 ㅜ",
          "speaker_id": "2",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100019703.1.2",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100019703.1.3",
          "form": "전 주부라 전부요 ㅋ",
          "original_form": "전 주부라 전부요 ㅋ",
          "speaker_id": "2",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100019703.1.4",
          "form": "분담해야죠",
          "original_form": "분담해야죠",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019703.1.5",
          "form": "신랑은 출근시간도빠르고 야근하는날이 많아요",
          "original_form": "신랑은 출근시간도빠르고 야근하는날이 많아요",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100019703.1.6",
          "form": "아 그럼 어쩔수 없나오?",
          "original_form": "아 그럼 어쩔수 없나오?",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019703.1.7",
          "form": "집에서 밥은 주말에만 먹어요",
          "original_form": "집에서 밥은 주말에만 먹어요",
          "speaker_id": "2",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100019703.1.8",
          "form": "식사 준비는 없네오",
          "original_form": "식사 준비는 없네오",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019703.1.9",
          "form": "남편도 회사일이 워낙많으니..퇴근하면 10시40분쯤이라",
          "original_form": "남편도 회사일이 워낙많으니..퇴근하면 10시40분쯤이라",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100019703.1.10",
          "form": "헉 많이 늦네요",
          "original_form": "헉 많이 늦네요",
          "speaker_id": "1",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019703.1.11",
          "form": "평일엔 아이와 제가먹을거 정도요",
          "original_form": "평일엔 아이와 제가먹을거 정도요",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100019703.1.12",
          "form": "아 그래도 힘들죠?",
          "original_form": "아 그래도 힘들죠?",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100019703.1.13",
          "form": "조금요 ㅋ 그래서 저도 회사 그만뒀어요",
          "original_form": "조금요 ㅋ 그래서 저도 회사 그만뒀어요",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100019703.1.14",
          "form": "주부신가요?",
          "original_form": "주부신가요?",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
