      "utterance": [
        {
          "id": "MDRW2100046005.1.1",
          "form": "파라솔이 인기네.",
          "original_form": "파라솔이 인기네.",
          "speaker_id": "1",
          "time": "20210908 09:30"
        },
        {
          "id": "MDRW2100046005.1.2",
          "form": "그러게 하나 더사고싶네",
          "original_form": "그러게 하나 더사고싶네",
          "speaker_id": "2",
          "time": "20210908 09:31"
        },
        {
          "id": "MDRW2100046005.1.3",
          "form": "돈이있어도 살수가없는ㅠㅠ",
          "original_form": "돈이있어도 살수가없는ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 09:32"
        },
        {
          "id": "MDRW2100046005.1.4",
          "form": "그렇네 사고싶어도살수없으니 인기가 있는듯ㅎ",
          "original_form": "그렇네 사고싶어도살수없으니 인기가 있는듯ㅎ",
          "speaker_id": "2",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100046005.1.5",
          "form": "그러이ㅋㅋ 안팔고 소장해야될각이다",
          "original_form": "그러이ㅋㅋ 안팔고 소장해야될각이다",
          "speaker_id": "1",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100046005.1.6",
          "form": "탐나지ㅋㅋ",
          "original_form": "탐나지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:33"
        },
        {
          "id": "MDRW2100046005.1.7",
          "form": "뭐근래 산거있나?",
          "original_form": "뭐근래 산거있나?",
          "speaker_id": "2",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100046005.1.8",
          "form": "어제 소고기 산거와서 오늘 먹으려고",
          "original_form": "어제 소고기 산거와서 오늘 먹으려고",
          "speaker_id": "1",
          "time": "20210908 09:34"
        },
        {
          "id": "MDRW2100046005.1.9",
          "form": "소고기 좋네",
          "original_form": "소고기 좋네",
          "speaker_id": "2",
          "time": "20210908 09:35"
        },
        {
          "id": "MDRW2100046005.1.10",
          "form": "핫딜떠서 사서 싸게 샀음ㅋㅋ",
          "original_form": "핫딜떠서 사서 싸게 샀음ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 09:35"
        },
        {
          "id": "MDRW2100046005.1.11",
          "form": "그런거있음 같이사자",
          "original_form": "그런거있음 같이사자",
          "speaker_id": "2",
          "time": "20210908 09:36"
        },
        {
          "id": "MDRW2100046005.1.12",
          "form": "아ㅡ다음엔 알려주께ㅋㅋ쏘리",
          "original_form": "아ㅡ다음엔 알려주께ㅋㅋ쏘리",
          "speaker_id": "1",
          "time": "20210908 09:36"
        },
        {
          "id": "MDRW2100046005.1.13",
          "form": "빅세일 끝나는데 뭐사?",
          "original_form": "빅세일 끝나는데 뭐사?",
          "speaker_id": "2",
          "time": "20210908 09:36"
        },
        {
          "id": "MDRW2100046005.1.14",
          "form": "기저귀도 쟁이고 물티슈도 쟁이고ㅋㅋ",
          "original_form": "기저귀도 쟁이고 물티슈도 쟁이고ㅋㅋ",
