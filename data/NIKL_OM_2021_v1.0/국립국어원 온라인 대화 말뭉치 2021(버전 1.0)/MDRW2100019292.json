      "utterance": [
        {
          "id": "MDRW2100019292.1.1",
          "form": "치킨을 자주 시켜드시나요?",
          "original_form": "치킨을 자주 시켜드시나요?",
          "speaker_id": "2",
          "time": "20210827 17:59"
        },
        {
          "id": "MDRW2100019292.1.2",
          "form": "치킨은가끔",
          "original_form": "치킨은가끔",
          "speaker_id": "1",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019292.1.3",
          "form": "피자파에요",
          "original_form": "피자파에요",
          "speaker_id": "1",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019292.1.4",
          "form": "아하 전 치킨파라 교촌을 자주 먹어요",
          "original_form": "아하 전 치킨파라 교촌을 자주 먹어요",
          "speaker_id": "2",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100019292.1.5",
          "form": "피자는 어떤브랜드를?",
          "original_form": "피자는 어떤브랜드를?",
          "speaker_id": "2",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100019292.1.6",
          "form": "사실 다맛잇져 교촌은 레드",
          "original_form": "사실 다맛잇져 교촌은 레드",
          "speaker_id": "1",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100019292.1.7",
          "form": "피자는 도미노임다",
          "original_form": "피자는 도미노임다",
          "speaker_id": "1",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100019292.1.8",
          "form": "그러시구나 저도 교촌은 레드 파!",
          "original_form": "그러시구나 저도 교촌은 레드 파!",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019292.1.9",
          "form": "피자는 피자헛 테이크아웃이요",
          "original_form": "피자는 피자헛 테이크아웃이요",
          "speaker_id": "2",
          "time": "20210827 18:35"
        },
        {
          "id": "MDRW2100019292.1.10",
          "form": "식사하셧나요?",
          "original_form": "식사하셧나요?",
          "speaker_id": "1",
          "time": "20210827 19:17"
        },
        {
          "id": "MDRW2100019292.1.11",
          "form": "네 name1님은요?",
          "original_form": "네 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 20:00"
        },
        {
          "id": "MDRW2100019292.1.12",
          "form": "미역국먹고",
          "original_form": "미역국먹고",
          "speaker_id": "1",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019292.1.13",
          "form": "옥수수도ㅋㅋㅋ",
          "original_form": "옥수수도ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100019292.1.14",
          "form": "든든하게 드셨네요",
          "original_form": "든든하게 드셨네요",
          "speaker_id": "2",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100019292.1.15",
          "form": "전 호박죽이라 좀 배고파요 ㅋㅋ",
          "original_form": "전 호박죽이라 좀 배고파요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019292.1.16",
          "form": "헉ㅋㅋ",
          "original_form": "헉ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019292.1.17",
          "form": "요리해드시나요?",
          "original_form": "요리해드시나요?",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019292.1.18",
          "form": "엄마찬스 호박죽이에요 ㅋㅋ",
          "original_form": "엄마찬스 호박죽이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019292.1.19",
          "form": "ㅎㅎ저는 자취하는데",
          "original_form": "ㅎㅎ저는 자취하는데",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019292.1.20",
          "form": "남자친구가 장금이라서",
          "original_form": "남자친구가 장금이라서",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019292.1.21",
          "form": "ㅎㅅㅎ남친찬스!!",
          "original_form": "ㅎㅅㅎ남친찬스!!",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019292.1.22",
          "form": "아 님친이 요리해주는군요 어떤 요리를 자주해주나요?",
          "original_form": "아 님친이 요리해주는군요 어떤 요리를 자주해주나요?",
          "speaker_id": "2",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100019292.1.23",
          "form": "제가 한식 좋아해서 주로 한식?",
          "original_form": "제가 한식 좋아해서 주로 한식?",
          "speaker_id": "1",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100019292.1.24",
          "form": "말만하면 뚝딱 나와요 ㅎㅅㅎ",
          "original_form": "말만하면 뚝딱 나와요 ㅎㅅㅎ",
          "speaker_id": "1",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100019292.1.25",
          "form": "예를 들면 어떤 요리인가요?",
          "original_form": "예를 들면 어떤 요리인가요?",
          "speaker_id": "2",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100019292.1.26",
          "form": "내일은 수육 해준다그랫어요",
          "original_form": "내일은 수육 해준다그랫어요",
