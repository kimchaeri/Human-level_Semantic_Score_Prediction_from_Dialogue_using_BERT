      "utterance": [
        {
          "id": "MDRW2100019647.1.1",
          "form": "뮤지컬이나 연극",
          "original_form": "뮤지컬이나 연극",
          "speaker_id": "2",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100019647.1.2",
          "form": "어떤작품?",
          "original_form": "어떤작품?",
          "speaker_id": "1",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100019647.1.3",
          "form": "뮤지컬은 킹키부츠",
          "original_form": "뮤지컬은 킹키부츠",
          "speaker_id": "2",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100019647.1.4",
          "form": "난 요즘 전시회 보러가고 싶어",
          "original_form": "난 요즘 전시회 보러가고 싶어",
          "speaker_id": "1",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100019647.1.5",
          "form": "무슨 전시?",
          "original_form": "무슨 전시?",
          "speaker_id": "2",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100019647.1.6",
          "form": "미술쪽?",
          "original_form": "미술쪽?",
          "speaker_id": "1",
          "time": "20210827 20:59"
        },
        {
          "id": "MDRW2100019647.1.7",
          "form": "어떤 류가 좋아? 회화나 조소?",
          "original_form": "어떤 류가 좋아? 회화나 조소?",
          "speaker_id": "2",
          "time": "20210827 20:59"
        },
        {
          "id": "MDRW2100019647.1.8",
          "form": "미술쪽?",
          "original_form": "미술쪽?",
          "speaker_id": "1",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100019647.1.9",
          "form": "응 그림이 좋아 조각이 좋아?",
          "original_form": "응 그림이 좋아 조각이 좋아?",
          "speaker_id": "2",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100019647.1.10",
          "form": "난 둘 다 좋아",
          "original_form": "난 둘 다 좋아",
          "speaker_id": "1",
          "time": "20210827 21:01"
        },
        {
          "id": "MDRW2100019647.1.11",
          "form": "둘다 있는 경우도 많지",
          "original_form": "둘다 있는 경우도 많지",
          "speaker_id": "2",
          "time": "20210827 21:01"
        },
        {
          "id": "MDRW2100019647.1.12",
          "form": "전시회 안간지 넘 오래되서",
          "original_form": "전시회 안간지 넘 오래되서",
          "speaker_id": "1",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100019647.1.13",
          "form": "맞아 진짜 오래됐어",
          "original_form": "맞아 진짜 오래됐어",
          "speaker_id": "2",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100019647.1.14",
          "form": "코로나 땜에",
          "original_form": "코로나 땜에",
          "speaker_id": "1",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100019647.1.15",
          "form": "그놈의 코로나좀 사라졌으면",
          "original_form": "그놈의 코로나좀 사라졌으면",
          "speaker_id": "2",
          "time": "20210827 21:03"
        },
        {
          "id": "MDRW2100019647.1.16",
          "form": "언제 없어질까",
          "original_form": "언제 없어질까",
          "speaker_id": "1",
          "time": "20210827 21:03"
        },
        {
          "id": "MDRW2100019647.1.17",
          "form": "그러게 말야 나도 전시회 공연 보고싶다",
          "original_form": "그러게 말야 나도 전시회 공연 보고싶다",
          "speaker_id": "2",
          "time": "20210827 21:03"
        },
        {
          "id": "MDRW2100019647.1.18",
          "form": "아까 봤던 안나 카레니나 영화나 발레로 본 적 있었어",
          "original_form": "아까 봤던 안나 카레니나 영화나 발레로 본 적 있었어",
