      "utterance": [
        {
          "id": "MDRW2100011042.1.1",
          "form": "저는 백수입니다",
          "original_form": "저는 백수입니다",
          "speaker_id": "2",
          "time": "20210824 00:05"
        },
        {
          "id": "MDRW2100011042.1.2",
          "form": "저는 프리랜서에요.",
          "original_form": "저는 프리랜서에요.",
          "speaker_id": "1",
          "time": "20210824 00:05"
        },
        {
          "id": "MDRW2100011042.1.3",
          "form": "재택근무에 대해서 어떻게 생각하시나요?",
          "original_form": "재택근무에 대해서 어떻게 생각하시나요?",
          "speaker_id": "1",
          "time": "20210824 00:05"
        },
        {
          "id": "MDRW2100011042.1.4",
          "form": "전 좋은 것 같아요",
          "original_form": "전 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210824 00:05"
        },
        {
          "id": "MDRW2100011042.1.5",
          "form": "좋다고 생각해요",
          "original_form": "좋다고 생각해요",
          "speaker_id": "2",
          "time": "20210824 00:05"
        },
        {
          "id": "MDRW2100011042.1.6",
          "form": "앞으로 일상생활이 재택근무가 자유로운 분위기로 가면 좋겠어용",
          "original_form": "앞으로 일상생활이 재택근무가 자유로운 분위기로 가면 좋겠어용",
          "speaker_id": "2",
          "time": "20210824 00:05"
        },
        {
          "id": "MDRW2100011042.1.7",
          "form": "재택하기 편한 일은 뭐가있을까요",
          "original_form": "재택하기 편한 일은 뭐가있을까요",
          "speaker_id": "2",
          "time": "20210824 00:06"
        },
        {
          "id": "MDRW2100011042.1.8",
          "form": "음 영업직이나 제조직 아니면 다 가능 할 것 같아요  사무직은",
          "original_form": "음 영업직이나 제조직 아니면 다 가능 할 것 같아요  사무직은",
          "speaker_id": "1",
          "time": "20210824 00:06"
        },
        {
          "id": "MDRW2100011042.1.9",
          "form": "친구들 요즘 재택해서 너무 부러워요 ㅠ",
          "original_form": "친구들 요즘 재택해서 너무 부러워요 ㅠ",
          "speaker_id": "1",
          "time": "20210824 00:06"
        },
        {
          "id": "MDRW2100011042.1.10",
          "form": "아아 영업직이나 제조직은 어렵겠네요ㅜㅜ",
          "original_form": "아아 영업직이나 제조직은 어렵겠네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011042.1.11",
          "form": "저는 이전에 일할때 잠깐 재택했었는데 초반에망 좀 불편하고 너무 좋은거같아요",
          "original_form": "저는 이전에 일할때 잠깐 재택했었는데 초반에망 좀 불편하고 너무 좋은거같아요",
          "speaker_id": "2",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011042.1.12",
          "form": "저도 초반엔 너무 불편했는데 나중엔 출근 하기 싫더라구여 ㅠㅠ",
          "original_form": "저도 초반엔 너무 불편했는데 나중엔 출근 하기 싫더라구여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 00:07"
        },
        {
          "id": "MDRW2100011042.1.13",
          "form": "프리랜서로 강사일을 하고있는데 저는 요즘에 출근해요 ㅠㅠ",
          "original_form": "프리랜서로 강사일을 하고있는데 저는 요즘에 출근해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 00:08"
        },
        {
          "id": "MDRW2100011042.1.14",
          "form": "코로나 긑나도 재택 하는 분위기가 올까요 ㅜ",
          "original_form": "코로나 긑나도 재택 하는 분위기가 올까요 ㅜ",
          "speaker_id": "2",
          "time": "20210824 00:08"
        },
        {
          "id": "MDRW2100011042.1.15",
          "form": "우리나라 정서상 없을 것 같아요 ㅠㅠ",
          "original_form": "우리나라 정서상 없을 것 같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100011042.1.16",
          "form": "얼른 문화가 바뀌었으면 좋겠어용",
          "original_form": "얼른 문화가 바뀌었으면 좋겠어용",
          "speaker_id": "2",
          "time": "20210824 00:10"
        },
        {
          "id": "MDRW2100011042.1.17",
          "form": "재택 알바같은것도 해보셨나요",
          "original_form": "재택 알바같은것도 해보셨나요",
          "speaker_id": "2",
          "time": "20210824 00:10"
        },
        {
          "id": "MDRW2100011042.1.18",
          "form": "네 가끔 했어요!! 디자인쪽으로 ㅎㅎ",
          "original_form": "네 가끔 했어요!! 디자인쪽으로 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011042.1.19",
          "form": "페이 잘주는건 진짜 편하더라구여",
          "original_form": "페이 잘주는건 진짜 편하더라구여",
          "speaker_id": "1",
          "time": "20210824 00:11"
        },
        {
          "id": "MDRW2100011042.1.20",
          "form": "오호 ㅜㅜ 역시",
          "original_form": "오호 ㅜㅜ 역시",
          "speaker_id": "2",
          "time": "20210824 00:12"
        },
        {
          "id": "MDRW2100011042.1.21",
          "form": "사람은 기술이있어야 하나봐용",
          "original_form": "사람은 기술이있어야 하나봐용",
          "speaker_id": "2",
          "time": "20210824 00:12"
        },
        {
