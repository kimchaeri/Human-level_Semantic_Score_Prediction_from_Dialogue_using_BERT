      "utterance": [
        {
          "id": "MDRW2100035951.1.1",
          "form": "저는 오늘 서브웨이 갔답니다 ㅎㅎ",
          "original_form": "저는 오늘 서브웨이 갔답니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 18:54"
        },
        {
          "id": "MDRW2100035951.1.2",
          "form": "오 어떤 메뉴 드셨나요!",
          "original_form": "오 어떤 메뉴 드셨나요!",
          "speaker_id": "1",
          "time": "20210830 19:11"
        },
        {
          "id": "MDRW2100035951.1.3",
          "form": "로티세리 치킨이요!",
          "original_form": "로티세리 치킨이요!",
          "speaker_id": "2",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100035951.1.4",
          "form": "핫칠리소스랑 머스타드 소스넣었는대",
          "original_form": "핫칠리소스랑 머스타드 소스넣었는대",
          "speaker_id": "2",
          "time": "20210830 22:33"
        },
        {
          "id": "MDRW2100035951.1.5",
          "form": "와 너무 맛있어서 감탄했어요",
          "original_form": "와 너무 맛있어서 감탄했어요",
          "speaker_id": "2",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100035951.1.6",
          "form": "name1님은 좋아하는 메뉴 있으세요?",
          "original_form": "&name1&님은 좋아하는 메뉴 있으세요?",
          "speaker_id": "2",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100035951.1.7",
          "form": "저는 예전에는 에그마요에 베이컨추가 좋아했는데 요즘에는 터키요 !",
          "original_form": "저는 예전에는 에그마요에 베이컨추가 좋아했는데 요즘에는 터키요 !",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100035951.1.8",
          "form": "로티세리 먹어봐야겠네요 ㅎㅎ",
          "original_form": "로티세리 먹어봐야겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100035951.1.9",
          "form": "아아 에그마요 맛있다 들었어요 ㅋㅋㅋ",
          "original_form": "아아 에그마요 맛있다 들었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:36"
        },
        {
          "id": "MDRW2100035951.1.10",
          "form": "터키도 맛있나요!!",
          "original_form": "터키도 맛있나요!!",
          "speaker_id": "2",
          "time": "20210830 23:37"
        },
        {
          "id": "MDRW2100035951.1.11",
          "form": "소스 맛 레시피는 뭐죠 !!",
          "original_form": "소스 맛 레시피는 뭐죠 !!",
          "speaker_id": "2",
          "time": "20210830 23:37"
        },
        {
          "id": "MDRW2100035951.1.12",
          "form": "터키는 담백해요ㅋㅋ!",
          "original_form": "터키는 담백해요ㅋㅋ!",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100035951.1.13",
          "form": "소스는 렌치 스윗어니언 핫칠리조금 입니다..",
          "original_form": "소스는 렌치 스윗어니언 핫칠리조금 입니다..",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100035951.1.14",
          "form": "조금 소스가 많아도 밸런스가 굿이에용",
          "original_form": "조금 소스가 많아도 밸런스가 굿이에용",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100035951.1.15",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210831 11:33"
        },
        {
          "id": "MDRW2100035951.1.16",
          "form": "기억해두겠숩니다 ㅋㅋㅋㅋㅋ",
          "original_form": "기억해두겠숩니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:33"
        },
        {
          "id": "MDRW2100035951.1.17",
          "form": "좋아요ㅋㅋ 저도 로티세리 그 조합으로 먹어보겠어요..",
          "original_form": "좋아요ㅋㅋ 저도 로티세리 그 조합으로 먹어보겠어요..",
          "speaker_id": "1",
          "time": "20210831 12:33"
        },
        {
          "id": "MDRW2100035951.1.18",
          "form": "쿠키는 뭐 좋아하새요?",
          "original_form": "쿠키는 뭐 좋아하새요?",
          "speaker_id": "2",
          "time": "20210831 16:59"
        },
        {
          "id": "MDRW2100035951.1.19",
          "form": "제 최애는 화이트마카다미아요 !!",
          "original_form": "제 최애는 화이트마카다미아요 !!",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100035951.1.20",
          "form": "쿠키뭐 좋아하세요?",
          "original_form": "쿠키뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 22:52"
        },
        {
          "id": "MDRW2100035951.1.21",
          "form": "오 그것도 맛나나요?!",
          "original_form": "오 그것도 맛나나요?!",
          "speaker_id": "2",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100035951.1.22",
          "form": "잔 다크초코 그것만 먹어봐서 ㅋㅋㅋㅋㅋ",
          "original_form": "잔 다크초코 그것만 먹어봐서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100035951.1.23",
          "form": "네네 젤 맛있어요 ㅋㅋㅋㅋ!!",
          "original_form": "네네 젤 맛있어요 ㅋㅋㅋㅋ!!",
          "speaker_id": "1",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100035951.1.24",
          "form": "사실 서브웨이쿠키는 다 맛있는 것 같아요ㅜㅜ",
          "original_form": "사실 서브웨이쿠키는 다 맛있는 것 같아요ㅜㅜ",
          "speaker_id": "1",
