      "utterance": [
        {
          "id": "MDRW2100036810.1.1",
          "form": "그곳 날씨는 어떤가요?",
          "original_form": "그곳 날씨는 어떤가요?",
          "speaker_id": "1",
          "time": "20210831 12:45"
        },
        {
          "id": "MDRW2100036810.1.2",
          "form": "흐렸는데 지금은 맑은거같아요",
          "original_form": "흐렸는데 지금은 맑은거같아요",
          "speaker_id": "2",
          "time": "20210831 12:46"
        },
        {
          "id": "MDRW2100036810.1.3",
          "form": "오 다행이예요!",
          "original_form": "오 다행이예요!",
          "speaker_id": "1",
          "time": "20210831 12:49"
        },
        {
          "id": "MDRW2100036810.1.4",
          "form": "여기도 아침에 빗방울이 떨어지더니 맑아졌어요",
          "original_form": "여기도 아침에 빗방울이 떨어지더니 맑아졌어요",
          "speaker_id": "1",
          "time": "20210831 12:49"
        },
        {
          "id": "MDRW2100036810.1.5",
          "form": "요며칠 천둥번개치고 무서웠어요",
          "original_form": "요며칠 천둥번개치고 무서웠어요",
          "speaker_id": "2",
          "time": "20210831 12:51"
        },
        {
          "id": "MDRW2100036810.1.6",
          "form": "헉 거긴 장난아녔네요",
          "original_form": "헉 거긴 장난아녔네요",
          "speaker_id": "1",
          "time": "20210831 12:54"
        },
        {
          "id": "MDRW2100036810.1.7",
          "form": "비도 많이 왔어요?",
          "original_form": "비도 많이 왔어요?",
          "speaker_id": "1",
          "time": "20210831 12:54"
        },
        {
          "id": "MDRW2100036810.1.8",
          "form": "그랬던거같아요 집에만 있어서 잘은 모르겠어요",
          "original_form": "그랬던거같아요 집에만 있어서 잘은 모르겠어요",
          "speaker_id": "2",
          "time": "20210831 12:54"
        },
        {
          "id": "MDRW2100036810.1.9",
          "form": "어휴 그럴때 집밖은 위험하죠",
          "original_form": "어휴 그럴때 집밖은 위험하죠",
          "speaker_id": "1",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100036810.1.10",
          "form": "미세먼지 많다잖아요",
          "original_form": "미세먼지 많다잖아요",
          "speaker_id": "2",
          "time": "20210831 12:56"
        },
        {
          "id": "MDRW2100036810.1.11",
          "form": "근데 이거 직접 느껴지세요? 많은거요",
          "original_form": "근데 이거 직접 느껴지세요? 많은거요",
          "speaker_id": "2",
          "time": "20210831 12:56"
        },
        {
          "id": "MDRW2100036810.1.12",
          "form": "아뇨 전 느껴보지 못한거같아요",
          "original_form": "아뇨 전 느껴보지 못한거같아요",
          "speaker_id": "1",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100036810.1.13",
          "form": "저도 그래서요",
          "original_form": "저도 그래서요",
          "speaker_id": "2",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100036810.1.14",
          "form": "하늘보면 탁한건 느껴지는데 직접적인건 그닥ㅇㅣ랄까요",
          "original_form": "하늘보면 탁한건 느껴지는데 직접적인건 그닥ㅇㅣ랄까요",
          "speaker_id": "1",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100036810.1.15",
          "form": "많다하니 많은가 하는 정도요;;;",
          "original_form": "많다하니 많은가 하는 정도요;;;",
          "speaker_id": "2",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100036810.1.16",
          "form": "맞아요ㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100036810.1.17",
          "form": "진짜 딱 그래요",
          "original_form": "진짜 딱 그래요",
          "speaker_id": "1",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100036810.1.18",
          "form": "네네 그죠... 호흡기로는 잘 모르겠어서",
          "original_form": "네네 그죠... 호흡기로는 잘 모르겠어서",
          "speaker_id": "2",
          "time": "20210831 12:57"
        },
        {
          "id": "MDRW2100036810.1.19",
          "form": "호들갑떠는 사람들 보면 생경해요",
          "original_form": "호들갑떠는 사람들 보면 생경해요",
          "speaker_id": "2",
          "time": "20210831 12:57"
        }
