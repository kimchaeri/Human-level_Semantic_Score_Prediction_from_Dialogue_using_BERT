      "utterance": [
        {
          "id": "MDRW2100005467.1.1",
          "form": "연예인 누구 좋아해?",
          "original_form": "연예인 누구 좋아해?",
          "speaker_id": "1",
          "time": "20210821 22:31"
        },
        {
          "id": "MDRW2100005467.1.2",
          "form": "전에는 아이돌 좋아했었는데",
          "original_form": "전에는 아이돌 좋아했었는데",
          "speaker_id": "2",
          "time": "20210821 22:33"
        },
        {
          "id": "MDRW2100005467.1.3",
          "form": "오 누구??",
          "original_form": "오 누구??",
          "speaker_id": "1",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005467.1.4",
          "form": "요새는 마음이 식어서 아무도 안 좋아하는 중이에요",
          "original_form": "요새는 마음이 식어서 아무도 안 좋아하는 중이에요",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005467.1.5",
          "form": "몇 년 전에 엑소 좋아했었어요",
          "original_form": "몇 년 전에 엑소 좋아했었어요",
          "speaker_id": "2",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005467.1.6",
          "form": "그렇구나 나는 배우 김동욱 좋아해",
          "original_form": "그렇구나 나는 배우 김동욱 좋아해",
          "speaker_id": "1",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005467.1.7",
          "form": "래퍼 우원재랑~",
          "original_form": "래퍼 우원재랑~",
          "speaker_id": "1",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005467.1.8",
          "form": "엑소 좋아했으면 깊게 좋아했을 것 같은데 ㅋㅋㅋ 맞아?",
          "original_form": "엑소 좋아했으면 깊게 좋아했을 것 같은데 ㅋㅋㅋ 맞아?",
          "speaker_id": "1",
          "time": "20210821 22:34"
        },
        {
          "id": "MDRW2100005467.1.9",
          "form": "오 김동욱 신과함께 나왔던 그 배우 맞죠",
          "original_form": "오 김동욱 신과함께 나왔던 그 배우 맞죠",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005467.1.10",
          "form": "도경수 나와서 봤던 기억이 나요",
          "original_form": "도경수 나와서 봤던 기억이 나요",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005467.1.11",
          "form": "엑소..  꽤 깊게 좋아했죠 ㅋㅋㅋㅋㅋ",
          "original_form": "엑소..  꽤 깊게 좋아했죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:35"
        },
        {
          "id": "MDRW2100005467.1.12",
          "form": "앜ㅋㅋㅋㅋㅋㅋ 최애 누구였어?",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ 최애 누구였어?",
          "speaker_id": "1",
          "time": "20210821 22:36"
        },
        {
          "id": "MDRW2100005467.1.13",
          "form": "도경수요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "도경수요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100005467.1.14",
          "form": "아ㅋㅋㅋㅋㅋ 제 친구랑 같네요",
          "original_form": "아ㅋㅋㅋㅋㅋ 제 친구랑 같네요",
          "speaker_id": "1",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005467.1.15",
          "form": "저는 백현 요새 좋아하는 것 같아요",
          "original_form": "저는 백현 요새 좋아하는 것 같아요",
          "speaker_id": "1",
          "time": "20210821 22:39"
        },
        {
          "id": "MDRW2100005467.1.16",
          "form": "오 백현도 좋죠",
          "original_form": "오 백현도 좋죠",
          "speaker_id": "2",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005467.1.17",
          "form": "진짜 천상 연예인 같지 않나요",
          "original_form": "진짜 천상 연예인 같지 않나요",
          "speaker_id": "2",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005467.1.18",
          "form": "그 유엔빌리지인가 그거 보고",
          "original_form": "그 유엔빌리지인가 그거 보고",
          "speaker_id": "1",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005467.1.19",
          "form": "깜짝놀랐어요 실력파구나 싶어서",
          "original_form": "깜짝놀랐어요 실력파구나 싶어서",
          "speaker_id": "1",
          "time": "20210821 22:41"
        },
        {
          "id": "MDRW2100005467.1.20",
          "form": "유엔빌리지 노래방에서 절대 못 불러요",
          "original_form": "유엔빌리지 노래방에서 절대 못 불러요",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005467.1.21",
          "form": "호흡 모자라서 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "호흡 모자라서 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005467.1.22",
          "form": "그런 댓글 많더라구요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그런 댓글 많더라구요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005467.1.23",
          "form": "대단하신 분이였구나 싶더라고요..",
          "original_form": "대단하신 분이였구나 싶더라고요..",
          "speaker_id": "1",
          "time": "20210821 22:48"
        }
      ]
    }
