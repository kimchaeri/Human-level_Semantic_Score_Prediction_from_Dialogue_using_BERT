      "utterance": [
        {
          "id": "MDRW2100030730.1.1",
          "form": "점심 시간이네요",
          "original_form": "점심 시간이네요",
          "speaker_id": "1",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100030730.1.2",
          "form": "무슨 메뉴 드실건가요?",
          "original_form": "무슨 메뉴 드실건가요?",
          "speaker_id": "1",
          "time": "20210904 12:00"
        },
        {
          "id": "MDRW2100030730.1.3",
          "form": "유부초밥 재료 사놓은게 있어서 유부초밥 만들어 먹으려구요",
          "original_form": "유부초밥 재료 사놓은게 있어서 유부초밥 만들어 먹으려구요",
          "speaker_id": "2",
          "time": "20210904 12:01"
        },
        {
          "id": "MDRW2100030730.1.4",
          "form": "유부초밥 맛나죠 ㅎㅎ 전 목살구이 먹으려구요",
          "original_form": "유부초밥 맛나죠 ㅎㅎ 전 목살구이 먹으려구요",
          "speaker_id": "1",
          "time": "20210904 12:02"
        },
        {
          "id": "MDRW2100030730.1.5",
          "form": "맛있게 점심 드세요 전 먹으러 갑니다",
          "original_form": "맛있게 점심 드세요 전 먹으러 갑니다",
          "speaker_id": "1",
          "time": "20210904 12:03"
        },
        {
          "id": "MDRW2100030730.1.6",
          "form": "네 맛있게 드세요~^^",
          "original_form": "네 맛있게 드세요~^^",
          "speaker_id": "2",
          "time": "20210904 12:04"
        },
        {
          "id": "MDRW2100030730.1.7",
          "form": "점심 먹고 왔어요",
          "original_form": "점심 먹고 왔어요",
          "speaker_id": "1",
          "time": "20210904 13:30"
        },
        {
          "id": "MDRW2100030730.1.8",
          "form": "ㅎ저는 저녁까지 먹고 왔네요",
          "original_form": "ㅎ저는 저녁까지 먹고 왔네요",
          "speaker_id": "2",
          "time": "20210904 20:12"
        },
        {
          "id": "MDRW2100030730.1.9",
          "form": "저녁 뭐 드셨나요? 전 분식이요",
          "original_form": "저녁 뭐 드셨나요? 전 분식이요",
          "speaker_id": "1",
          "time": "20210904 20:14"
        },
        {
          "id": "MDRW2100030730.1.10",
          "form": "전 된장찌개요",
          "original_form": "전 된장찌개요",
          "speaker_id": "2",
          "time": "20210904 20:15"
        },
        {
          "id": "MDRW2100030730.1.11",
          "form": "된장찌개 맛있죠 ㅎㅎ 전 많이 먹었더니 살짝 질리더라구요",
          "original_form": "된장찌개 맛있죠 ㅎㅎ 전 많이 먹었더니 살짝 질리더라구요",
          "speaker_id": "1",
          "time": "20210904 20:17"
        },
        {
          "id": "MDRW2100030730.1.12",
          "form": "전 된장찌개는 별로 안질리는거 같아요~\r\n먹고 나면 속이 편안해지는거 같아서 좋아해요",
          "original_form": "전 된장찌개는 별로 안질리는거 같아요~\r\n먹고 나면 속이 편안해지는거 같아서 좋아해요",
          "speaker_id": "2",
          "time": "20210904 20:19"
        },
        {
          "id": "MDRW2100030730.1.13",
          "form": "된장이 소화 잘 된다고 하더라구요 아 청국장도요 청국장 먹고 싶네요",
          "original_form": "된장이 소화 잘 된다고 하더라구요 아 청국장도요 청국장 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210904 20:21"
        },
        {
          "id": "MDRW2100030730.1.14",
          "form": "요즘은 청국장도 냄새 안나긴 하던데 그래도 전 된장파임다~ㅎ",
          "original_form": "요즘은 청국장도 냄새 안나긴 하던데 그래도 전 된장파임다~ㅎ",
          "speaker_id": "2",
          "time": "20210904 20:23"
        },
        {
          "id": "MDRW2100030730.1.15",
          "form": "청국장이 냄새가 좀 꾸리꾸리하긴 하죠 ㅋㅋ 요샌 달라졌어도요",
          "original_form": "청국장이 냄새가 좀 꾸리꾸리하긴 하죠 ㅋㅋ 요샌 달라졌어도요",
          "speaker_id": "1",
          "time": "20210904 20:35"
        },
        {
          "id": "MDRW2100030730.1.16",
          "form": "청국장 가루로도 팔더라구요~\r\n그건 냄새 안난다고 하던데 아직 안사봤어요",
          "original_form": "청국장 가루로도 팔더라구요~\r\n그건 냄새 안난다고 하던데 아직 안사봤어요",
          "speaker_id": "2",
          "time": "20210904 20:39"
        }
      ]
