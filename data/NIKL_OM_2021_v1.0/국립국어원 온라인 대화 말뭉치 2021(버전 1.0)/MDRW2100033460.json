      "utterance": [
        {
          "id": "MDRW2100033460.1.1",
          "form": "지금 오상욱나오네요",
          "original_form": "지금 오상욱나오네요",
          "speaker_id": "1",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100033460.1.2",
          "form": "오 그래요 전 그편은 못보고 기안봤어요",
          "original_form": "오 그래요 전 그편은 못보고 기안봤어요",
          "speaker_id": "2",
          "time": "20210823 23:18"
        },
        {
          "id": "MDRW2100033460.1.3",
          "form": "엠비씨드라마에서 나오고있어요",
          "original_form": "엠비씨드라마에서 나오고있어요",
          "speaker_id": "1",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100033460.1.4",
          "form": "오상욱 넘 귀엽네요",
          "original_form": "오상욱 넘 귀엽네요",
          "speaker_id": "1",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100033460.1.5",
          "form": "오 그렇군요 귀요미 좋네요",
          "original_form": "오 그렇군요 귀요미 좋네요",
          "speaker_id": "2",
          "time": "20210823 23:20"
        },
        {
          "id": "MDRW2100033460.1.6",
          "form": "얼굴은 베이비같고 체격은 워너비네요",
          "original_form": "얼굴은 베이비같고 체격은 워너비네요",
          "speaker_id": "1",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100033460.1.7",
          "form": "전 기안 큰 다라 ㅋㅋ타고 노는거 봤어요",
          "original_form": "전 기안 큰 다라 ㅋㅋ타고 노는거 봤어요",
          "speaker_id": "2",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100033460.1.8",
          "form": "만찢남 같아요",
          "original_form": "만찢남 같아요",
          "speaker_id": "1",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100033460.1.9",
          "form": "다라 ㅋㅋ 저도 그부분 본거같아요 ㅎㅎ",
          "original_form": "다라 ㅋㅋ 저도 그부분 본거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100033460.1.10",
          "form": "앙 베이비",
          "original_form": "앙 베이비",
          "speaker_id": "2",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100033460.1.11",
          "form": "만찢남 좋아요 요새 왜이리 다들 잘생겼나요",
          "original_form": "만찢남 좋아요 요새 왜이리 다들 잘생겼나요",
          "speaker_id": "2",
          "time": "20210823 23:21"
        },
        {
          "id": "MDRW2100033460.1.12",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100033460.1.13",
          "form": "이번 펜싱선수들 아이돌같더라구요",
          "original_form": "이번 펜싱선수들 아이돌같더라구요",
          "speaker_id": "1",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100033460.1.14",
          "form": "그 차은우는 진짜 딱만화예요",
          "original_form": "그 차은우는 진짜 딱만화예요",
          "speaker_id": "2",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100033460.1.15",
          "form": "좀 느끼한 ?ㅋㅋ",
          "original_form": "좀 느끼한 ?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100033460.1.16",
          "form": "맞아요 요새 스타일이 너무 좋아요",
          "original_form": "맞아요 요새 스타일이 너무 좋아요",
          "speaker_id": "2",
          "time": "20210823 23:23"
        },
        {
          "id": "MDRW2100033460.1.17",
          "form": "어찌 저리 예쁘게 생기셨는지...유전자가 남다른건지 ㅎ",
          "original_form": "어찌 저리 예쁘게 생기셨는지...유전자가 남다른건지 ㅎ",
          "speaker_id": "1",
          "time": "20210823 23:24"
        },
        {
          "id": "MDRW2100033460.1.18",
          "form": "아 제가 어이들에게 못할짓 했네요 ㅜㅜ",
          "original_form": "아 제가 어이들에게 못할짓 했네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 23:24"
        },
        {
          "id": "MDRW2100033460.1.19",
          "form": "그렇게 나줘야 하는데 ㅋㅋ",
          "original_form": "그렇게 나줘야 하는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:24"
        },
        {
          "id": "MDRW2100033460.1.20",
          "form": "유전자가 틀리죠",
          "original_form": "유전자가 틀리죠",
          "speaker_id": "2",
          "time": "20210823 23:24"
        },
        {
          "id": "MDRW2100033460.1.21",
          "form": "ㅋㅋㅋ 저희 아들은 의학의 힘을 빌려줘야할듯요",
          "original_form": "ㅋㅋㅋ 저희 아들은 의학의 힘을 빌려줘야할듯요",
          "speaker_id": "1",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100033460.1.22",
          "form": "저희 딸도요",
          "original_form": "저희 딸도요",
          "speaker_id": "2",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100033460.1.23",
          "form": "코눈은 해주려고요",
          "original_form": "코눈은 해주려고요",
          "speaker_id": "2",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100033460.1.24",
          "form": "근데 ㅋㅋ아들은 잘생겼어요",
          "original_form": "근데 ㅋㅋ아들은 잘생겼어요",
          "speaker_id": "2",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100033460.1.25",
          "form": "베이비 페이스",
          "original_form": "베이비 페이스",
          "speaker_id": "2",
          "time": "20210823 23:25"
        },
        {
          "id": "MDRW2100033460.1.26",
          "form": "딸한테 미안하네요 ㅜㅜ",
          "original_form": "딸한테 미안하네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100033460.1.27",
          "form": "오모나",
          "original_form": "오모나",
          "speaker_id": "1",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100033460.1.28",
          "form": "요새 배우가 너무 잘생겨서 눈버렸어요",
          "original_form": "요새 배우가 너무 잘생겨서 눈버렸어요",
          "speaker_id": "2",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100033460.1.29",
          "form": "좀 생겨서 잘생겼단 소리 들어요 ㅋㅋ",
          "original_form": "좀 생겨서 잘생겼단 소리 들어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100033460.1.30",
          "form": "저 안닮았어요",
          "original_form": "저 안닮았어요",
          "speaker_id": "2",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100033460.1.31",
          "form": "그러게요 티비가 다들 예쁜 남자나 여자들만 나와서 ㅋ",
          "original_form": "그러게요 티비가 다들 예쁜 남자나 여자들만 나와서 ㅋ",
          "speaker_id": "1",
          "time": "20210823 23:26"
        },
        {
          "id": "MDRW2100033460.1.32",
          "form": "날씬하고요",
          "original_form": "날씬하고요",
          "speaker_id": "2",
          "time": "20210823 23:27"
        },
        {
          "id": "MDRW2100033460.1.33",
          "form": "심하긴해요",
          "original_form": "심하긴해요",
          "speaker_id": "2",
          "time": "20210823 23:27"
        },
        {
          "id": "MDRW2100033460.1.34",
          "form": "심각하게 마른건데 기준이 되니 애들이 연예인 하게돼요",
          "original_form": "심각하게 마른건데 기준이 되니 애들이 연예인 하게돼요",
          "speaker_id": "2",
          "time": "20210823 23:27"
        }
      ]
