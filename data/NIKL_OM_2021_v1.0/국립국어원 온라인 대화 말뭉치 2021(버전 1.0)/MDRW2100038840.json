        {
          "id": "MDRW2100038840.1.1",
          "form": "넷플릭스 보세용?",
          "original_form": "넷플릭스 보세용?",
          "speaker_id": "1",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100038840.1.2",
          "form": "한 달 정도 봤어요",
          "original_form": "한 달 정도 봤어요",
          "speaker_id": "2",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038840.1.3",
          "form": "넷플릭스 자주 보시나요?",
          "original_form": "넷플릭스 자주 보시나요?",
          "speaker_id": "2",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038840.1.4",
          "form": "저 요즘 디피라는 드라마 보고있어요!!",
          "original_form": "저 요즘 디피라는 드라마 보고있어요!!",
          "speaker_id": "1",
          "time": "20210902 22:03"
        },
        {
          "id": "MDRW2100038840.1.5",
          "form": "오 어떤 장르인가요?",
          "original_form": "오 어떤 장르인가요?",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100038840.1.6",
          "form": "군대 장르인데 되게 심오해여 ㅠㅠ",
          "original_form": "군대 장르인데 되게 심오해여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100038840.1.7",
          "form": "군대 에서 벌어지는..? 장르가 뭐라해야할지 ㅋㅋ",
          "original_form": "군대 에서 벌어지는..? 장르가 뭐라해야할지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100038840.1.8",
          "form": "아하ㅋㅋㅋ",
          "original_form": "아하ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100038840.1.9",
          "form": "또 재밌게 본 작품 있으세요?",
          "original_form": "또 재밌게 본 작품 있으세요?",
          "speaker_id": "2",
          "time": "20210902 22:05"
        },
        {
          "id": "MDRW2100038840.1.10",
          "form": "인간수업 아세요??",
          "original_form": "인간수업 아세요??",
          "speaker_id": "1",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038840.1.11",
          "form": "광고하는 건 봤어요",
          "original_form": "광고하는 건 봤어요",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038840.1.12",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038840.1.13",
          "form": "조금 우울한..? 내용 같아요 ㅋㅋㅋ",
          "original_form": "조금 우울한..? 내용 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:08"
        },
        {
          "id": "MDRW2100038840.1.14",
          "form": "킹덤도 재밌게봣어요!!",
          "original_form": "킹덤도 재밌게봣어요!!",
          "speaker_id": "1",
          "time": "20210902 22:09"
        },
        {
          "id": "MDRW2100038840.1.15",
          "form": "그 좀비 나오는 거 맞나요?",
          "original_form": "그 좀비 나오는 거 맞나요?",
          "speaker_id": "2",
