      "utterance": [
        {
          "id": "MDRW2100045712.1.1",
          "form": "ㅋㅋㅌㅋㅋㅋ강추여요",
          "original_form": "ㅋㅋㅌㅋㅋㅋ강추여요",
          "speaker_id": "2",
