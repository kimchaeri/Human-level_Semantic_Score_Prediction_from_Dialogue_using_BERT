        },
        {
          "id": "MDRW2100045712.1.2",
          "form": "지금 생각나시는 먹거리 있으세요?",
          "original_form": "지금 생각나시는 먹거리 있으세요?",
          "speaker_id": "2",
          "time": "20210908 05:49"
        },
        {
          "id": "MDRW2100045712.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ진짜 좋은 정보를 알아갑니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ진짜 좋은 정보를 알아갑니다",
          "speaker_id": "1",
          "time": "20210908 05:49"
        },
        {
          "id": "MDRW2100045712.1.4",
          "form": "저는 민초요!",
          "original_form": "저는 민초요!",
          "speaker_id": "1",
          "time": "20210908 05:49"
        },
        {
          "id": "MDRW2100045712.1.5",
          "form": "민초얘기를 딴데서 하고 왔더닠ㅋㅋㅋㅋ",
          "original_form": "민초얘기를 딴데서 하고 왔더닠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:49"
        },
        {
          "id": "MDRW2100045712.1.6",
          "form": "민초 되게 중독적이더라구요",
          "original_form": "민초 되게 중독적이더라구요",
          "speaker_id": "2",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045712.1.7",
          "form": "예전에는 치약을 왜 먹노 싶었는데",
          "original_form": "예전에는 치약을 왜 먹노 싶었는데",
          "speaker_id": "2",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045712.1.8",
          "form": "ㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045712.1.9",
          "form": "크고 나서 먹어보니 깔끔하고 좋더라구요",
          "original_form": "크고 나서 먹어보니 깔끔하고 좋더라구요",
          "speaker_id": "2",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045712.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 또 다양하게 이것저것 나오니끼",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요 또 다양하게 이것저것 나오니끼",
          "speaker_id": "1",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045712.1.11",
          "form": "시도해보고 새롭게 접할 기회도 느는 것 같아요",
          "original_form": "시도해보고 새롭게 접할 기회도 느는 것 같아요",
          "speaker_id": "1",
          "time": "20210908 05:50"
        },
        {
          "id": "MDRW2100045712.1.12",
          "form": "민초파 반민초파 얘기도 했는데",
          "original_form": "민초파 반민초파 얘기도 했는데",
          "speaker_id": "1",
          "time": "20210908 05:51"
        },
        {
          "id": "MDRW2100045712.1.13",
          "form": "처음에는 과한 반민초파 얄밉지만 덕분에 바이럴 된 것 같다 아주 조금 고맙다!!! 는 얘기까지 했어욬ㅋㅋㅋㅋ",
          "original_form": "처음에는 과한 반민초파 얄밉지만 덕분에 바이럴 된 것 같다 아주 조금 고맙다!!! 는 얘기까지 했어욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:51"
        },
        {
          "id": "MDRW2100045712.1.14",
          "form": "ㅌㅋㅌㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅌㅋㅌㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210908 05:51"
        },
        {
          "id": "MDRW2100045712.1.15",
          "form": "민초단 반민초단 (장난이겠지만)싸워서",
          "original_form": "민초단 반민초단 (장난이겠지만)싸워서",
          "speaker_id": "2",
          "time": "20210908 05:52"
        },
        {
          "id": "MDRW2100045712.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ반민초파가 있기에 민초파가 빛나는 법",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ반민초파가 있기에 민초파가 빛나는 법",
          "speaker_id": "1",
          "time": "20210908 05:52"
        },
        {
          "id": "MDRW2100045712.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210908 05:52"
        },
        {
          "id": "MDRW2100045712.1.18",
          "form": "사실 장난이라곤 하지만 너무 진심으로까서",
          "original_form": "사실 장난이라곤 하지만 너무 진심으로까서",
          "speaker_id": "1",
          "time": "20210908 05:52"
        },
        {
          "id": "MDRW2100045712.1.19",
          "form": "덕분에 모르는 사람이 없게 됐어요",
          "original_form": "덕분에 모르는 사람이 없게 됐어요",
          "speaker_id": "2",
          "time": "20210908 05:52"
        },
        {
          "id": "MDRW2100045712.1.20",
          "form": "아진짜요? 어떻게 까길래요",
          "original_form": "아진짜요? 어떻게 까길래요",
          "speaker_id": "2",
          "time": "20210908 05:52"
        },
        {
          "id": "MDRW2100045712.1.21",
          "form": "장난이겠지만 토하는 이모티콘(기본 임티라 그렇게 쎄보이진 않아요ㅋㅋㅋㅋㅋ) 도배하고",
          "original_form": "장난이겠지만 토하는 이모티콘(기본 임티라 그렇게 쎄보이진 않아요ㅋㅋㅋㅋㅋ) 도배하고",
          "speaker_id": "1",
          "time": "20210908 05:53"
        },
        {
          "id": "MDRW2100045712.1.22",
          "form": "걍 치약맛이라고 하는건 괜찮아요 사실이니까!!",
          "original_form": "걍 치약맛이라고 하는건 괜찮아요 사실이니까!!",
          "speaker_id": "1",
          "time": "20210908 05:53"
        },
        {
          "id": "MDRW2100045712.1.23",
          "form": "근데 음식앞에 대고 그러는건 아무리 온라인이라도 좀!!!",
          "original_form": "근데 음식앞에 대고 그러는건 아무리 온라인이라도 좀!!!",
          "speaker_id": "1",
          "time": "20210908 05:53"
        },
        {
          "id": "MDRW2100045712.1.24",
          "form": "ㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
