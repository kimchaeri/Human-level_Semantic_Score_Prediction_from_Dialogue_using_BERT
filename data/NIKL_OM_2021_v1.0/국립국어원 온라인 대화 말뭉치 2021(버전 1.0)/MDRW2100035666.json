      "utterance": [
        {
          "id": "MDRW2100035666.1.1",
          "form": "요즘 생선요리가 당기네요",
          "original_form": "요즘 생선요리가 당기네요",
          "speaker_id": "2",
          "time": "20210829 22:10"
        },
        {
          "id": "MDRW2100035666.1.2",
          "form": "생선 어떤 거 좋아하시나요?",
          "original_form": "생선 어떤 거 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035666.1.3",
          "form": "대부분 안 가리지만 요즘은 고등어랑 갈치요!",
          "original_form": "대부분 안 가리지만 요즘은 고등어랑 갈치요!",
          "speaker_id": "2",
          "time": "20210829 22:11"
        },
        {
          "id": "MDRW2100035666.1.4",
          "form": "어 저도 갈치요",
          "original_form": "어 저도 갈치요",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035666.1.5",
          "form": "고등어는 그냥...",
          "original_form": "고등어는 그냥...",
          "speaker_id": "1",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035666.1.6",
          "form": "조림도 생각나고 구이도 먹고싶어요ㅋㅋ",
          "original_form": "조림도 생각나고 구이도 먹고싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:12"
        },
        {
          "id": "MDRW2100035666.1.7",
          "form": "갈치구이 좋아해요 ㅠㅠ",
          "original_form": "갈치구이 좋아해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035666.1.8",
          "form": "가자미 먹어보셨나요?",
          "original_form": "가자미 먹어보셨나요?",
          "speaker_id": "1",
          "time": "20210829 22:13"
        },
        {
          "id": "MDRW2100035666.1.9",
          "form": "네네! 가자미구이 맛나죠",
          "original_form": "네네! 가자미구이 맛나죠",
          "speaker_id": "2",
          "time": "20210829 22:19"
        },
        {
          "id": "MDRW2100035666.1.10",
          "form": "전 안먹어봤는데",
          "original_form": "전 안먹어봤는데",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035666.1.11",
          "form": "어떤 맛인가요...",
          "original_form": "어떤 맛인가요...",
          "speaker_id": "1",
          "time": "20210829 22:25"
        },
        {
          "id": "MDRW2100035666.1.12",
          "form": "다른 구이류와 비슷한데 살이 조금 더 찰졌어요",
          "original_form": "다른 구이류와 비슷한데 살이 조금 더 찰졌어요",
          "speaker_id": "2",
          "time": "20210829 22:26"
        },
        {
          "id": "MDRW2100035666.1.13",
          "form": "ㅋㅋ아하 맛있겠네요ㅋㅋ",
          "original_form": "ㅋㅋ아하 맛있겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:40"
        },
        {
          "id": "MDRW2100035666.1.14",
          "form": "전 갈치만 먹어서 궁금했어요",
          "original_form": "전 갈치만 먹어서 궁금했어요",
          "speaker_id": "1",
          "time": "20210829 22:40"
        },
        {
          "id": "MDRW2100035666.1.15",
          "form": "그래두 갈치가 좀 더 접근성 있어서 더 먹게 되더라구요ㅋㅋ",
          "original_form": "그래두 갈치가 좀 더 접근성 있어서 더 먹게 되더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:21"
        },
        {
          "id": "MDRW2100035666.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100035666.1.17",
          "form": "그리고 비리지 않아요",
          "original_form": "그리고 비리지 않아요",
          "speaker_id": "1",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100035666.1.18",
          "form": "그쵸! 발라먹기가 귀찮지만요ㅋㅋㅋㅋ",
          "original_form": "그쵸! 발라먹기가 귀찮지만요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 01:21"
        }
      ]
