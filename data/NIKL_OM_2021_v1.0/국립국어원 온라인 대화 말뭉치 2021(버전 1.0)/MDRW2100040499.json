      "utterance": [
        {
          "id": "MDRW2100040499.1.1",
          "form": "아 글고 데리",
          "original_form": "아 글고 데리",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100040499.1.2",
          "form": "진짜 캐리어 들고올거면",
          "original_form": "진짜 캐리어 들고올거면",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100040499.1.3",
          "form": "응?",
          "original_form": "응?",
          "speaker_id": "2",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100040499.1.4",
          "form": "움직일 때 멜 에코백?같은거 하나 넣어서 와",
          "original_form": "움직일 때 멜 에코백?같은거 하나 넣어서 와",
          "speaker_id": "1",
          "time": "20210904 22:05"
        },
        {
          "id": "MDRW2100040499.1.5",
          "form": "해외여행 용으로 들고 가야해? ㅏ 그 얘기가 아니구나 ㅋㅋㅋㅋㅋ",
          "original_form": "해외여행 용으로 들고 가야해? ㅏ 그 얘기가 아니구나 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:06"
        },
        {
          "id": "MDRW2100040499.1.6",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:06"
        },
        {
          "id": "MDRW2100040499.1.7",
          "form": "최대한 가벼운거 들고와 ㅋㅋㅋ",
          "original_form": "최대한 가벼운거 들고와 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:06"
        },
        {
          "id": "MDRW2100040499.1.8",
          "form": "캐리어 무거우니까",
          "original_form": "캐리어 무거우니까",
          "speaker_id": "1",
          "time": "20210904 22:06"
        },
        {
          "id": "MDRW2100040499.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 국내여행용 캐리어를 가져가든가 해야지 ㅇㅇ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 국내여행용 캐리어를 가져가든가 해야지 ㅇㅇ",
          "speaker_id": "2",
          "time": "20210904 22:06"
        },
        {
          "id": "MDRW2100040499.1.10",
          "form": "숙소에 맡겨두고",
          "original_form": "숙소에 맡겨두고",
          "speaker_id": "1",
          "time": "20210904 22:07"
        },
        {
          "id": "MDRW2100040499.1.11",
          "form": "에코백만 들고 돌아다니는게 나을듯",
          "original_form": "에코백만 들고 돌아다니는게 나을듯",
          "speaker_id": "1",
          "time": "20210904 22:07"
        },
        {
          "id": "MDRW2100040499.1.12",
          "form": "뷔페 갈 때도 그렇고",
          "original_form": "뷔페 갈 때도 그렇고",
          "speaker_id": "1",
          "time": "20210904 22:07"
        },
        {
          "id": "MDRW2100040499.1.13",
          "form": "그러게 거기 맡겨두거나 잠깐 둬도 누가 안가져가겠지ㅜ머",
          "original_form": "그러게 거기 맡겨두거나 잠깐 둬도 누가 안가져가겠지ㅜ머",
          "speaker_id": "2",
          "time": "20210904 22:07"
        },
        {
          "id": "MDRW2100040499.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 보통 체크아웃해도 짐 맡아주는 정도는 해주는걸로 알거든",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 보통 체크아웃해도 짐 맡아주는 정도는 해주는걸로 알거든",
          "speaker_id": "1",
          "time": "20210904 22:08"
        },
        {
          "id": "MDRW2100040499.1.15",
          "form": "5성에 자부심 갖고잇으니까 해주겟지 머(?",
          "original_form": "5성에 자부심 갖고잇으니까 해주겟지 머(?",
          "speaker_id": "1",
          "time": "20210904 22:08"
        },
        {
          "id": "MDRW2100040499.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 머 호텔이나 그런 곳 맡아주는 것 정도는 해주겠지.. 5성 아니더라도 고객인데!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 머 호텔이나 그런 곳 맡아주는 것 정도는 해주겠지.. 5성 아니더라도 고객인데!",
          "speaker_id": "2",
          "time": "20210904 22:11"
        },
        {
          "id": "MDRW2100040499.1.17",
          "form": "ㅇㅇ!",
          "original_form": "ㅇㅇ!",
          "speaker_id": "1",
          "time": "20210904 22:12"
        },
        {
          "id": "MDRW2100040499.1.18",
          "form": "아 근데 여기",
          "original_form": "아 근데 여기",
          "speaker_id": "1",
          "time": "20210904 22:12"
        },
        {
          "id": "MDRW2100040499.1.19",
          "form": "짐 들어서 운반해주는 사람? 있대",
          "original_form": "짐 들어서 운반해주는 사람? 있대",
          "speaker_id": "1",
          "time": "20210904 22:12"
        },
        {
          "id": "MDRW2100040499.1.20",
          "form": "근데 회사사람이",
          "original_form": "근데 회사사람이",
          "speaker_id": "1",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100040499.1.21",
          "form": "신라호텔 이용했는데",
          "original_form": "신라호텔 이용했는데",
          "speaker_id": "1",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100040499.1.22",
          "form": "그 직원이 캐리어 옮겨다주고 자꾸 안나가면서",
          "original_form": "그 직원이 캐리어 옮겨다주고 자꾸 안나가면서",
          "speaker_id": "1",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100040499.1.23",
          "form": "더 필요한거 없냐고 물어보는데",
          "original_form": "더 필요한거 없냐고 물어보는데",
          "speaker_id": "1",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100040499.1.24",
          "form": "순간 팁 달라는 얘긴가...? 하는 느낌이 들엇다고 ㅋㅋㅋㅋㅋ",
          "original_form": "순간 팁 달라는 얘긴가...? 하는 느낌이 들엇다고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100040499.1.25",
          "form": "ㅋㅋㅋㅋㅋ아 그런거 있을거 같아 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ아 그런거 있을거 같아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:25"
        },
        {
          "id": "MDRW2100040499.1.26",
          "form": "캐리어는 내가 옮기겠다고 해야겠다...(팁 없는 늄",
          "original_form": "캐리어는 내가 옮기겠다고 해야겠다...(팁 없는 늄",
          "speaker_id": "2",
          "time": "20210904 22:26"
        },
        {
          "id": "MDRW2100040499.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:26"
        },
        {
          "id": "MDRW2100040499.1.28",
          "form": "걍 모른척하고",
          "original_form": "걍 모른척하고",
          "speaker_id": "1",
          "time": "20210904 22:26"
        },
        {
          "id": "MDRW2100040499.1.29",
          "form": "없어요~감사합니다~",
          "original_form": "없어요~감사합니다~",
          "speaker_id": "1",
          "time": "20210904 22:26"
        },
        {
          "id": "MDRW2100040499.1.30",
          "form": "하면 그냥 나간대 ㅋㅋㅋㅋㅋ",
          "original_form": "하면 그냥 나간대 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:26"
        },
        {
          "id": "MDRW2100040499.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 먼가 그럼 눈치 보이는건 마찬가지긴 한뎈ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 먼가 그럼 눈치 보이는건 마찬가지긴 한뎈ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:28"
        },
        {
          "id": "MDRW2100040499.1.32",
          "form": "팁문화 나랑 넘 안친해...",
          "original_form": "팁문화 나랑 넘 안친해...",
          "speaker_id": "2",
          "time": "20210904 22:28"
        },
        {
          "id": "MDRW2100040499.1.33",
          "form": "마자 여긴 한국인데",
          "original_form": "마자 여긴 한국인데",
          "speaker_id": "1",
          "time": "20210904 22:29"
        },
        {
          "id": "MDRW2100040499.1.34",
