      "utterance": [
        {
          "id": "MDRW2100021234.1.1",
          "form": "버블티?",
          "original_form": "버블티?",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021234.1.2",
          "form": "공차야?",
          "original_form": "공차야?",
          "speaker_id": "2",
          "time": "20210828 16:59"
        },
        {
          "id": "MDRW2100021234.1.3",
          "form": "응응 공차",
          "original_form": "응응 공차",
          "speaker_id": "1",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021234.1.4",
          "form": "난 공차 주문 어렵더라",
          "original_form": "난 공차 주문 어렵더라",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021234.1.5",
          "form": "아 진짜? 사실 나도 ㅋㅋㅋㅋ",
          "original_form": "아 진짜? 사실 나도 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021234.1.6",
          "form": "당도 이런거 정하는거 어려워ㅋㅋ",
          "original_form": "당도 이런거 정하는거 어려워ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021234.1.7",
          "form": "나 저번에 얼음 보통했더니 음료 적게줘서 얼음 적게 주문했거든?",
          "original_form": "나 저번에 얼음 보통했더니 음료 적게줘서 얼음 적게 주문했거든?",
          "speaker_id": "1",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100021234.1.8",
          "form": "응 그럼 아예 양이 줄지않아?",
          "original_form": "응 그럼 아예 양이 줄지않아?",
          "speaker_id": "2",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021234.1.9",
          "form": "그랫더니 아이스 음료인데...차가 많긴한데...미지근했엌ㅋㅋㅋ",
          "original_form": "그랫더니 아이스 음료인데...차가 많긴한데...미지근했엌ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021234.1.10",
          "form": "ㅋㅋㅋ아무래도 차는 뜨겁게 우려서 얼음을 타주는거라 그런가?",
          "original_form": "ㅋㅋㅋ아무래도 차는 뜨겁게 우려서 얼음을 타주는거라 그런가?",
          "speaker_id": "2",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021234.1.11",
          "form": "아니 음료는 많이줬어 꽉 채워서",
          "original_form": "아니 음료는 많이줬어 꽉 채워서",
          "speaker_id": "1",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021234.1.12",
          "form": "우리 카페에서 파는 아이스차는 뜨겁게 우린담에 얼음에 부어서 식혀주는거란말야. 그래서 얼음적게 해서 미지근한가 했지ㅋㅋ",
          "original_form": "우리 카페에서 파는 아이스차는 뜨겁게 우린담에 얼음에 부어서 식혀주는거란말야. 그래서 얼음적게 해서 미지근한가 했지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021234.1.13",
          "form": "아 정말? 거기도 그런가봐 차는 엄청 줬어 ㅋㅋㅋ",
          "original_form": "아 정말? 거기도 그런가봐 차는 엄청 줬어 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021234.1.14",
          "form": "버블티면 막 펄 이런거도 추가하는거야?",
          "original_form": "버블티면 막 펄 이런거도 추가하는거야?",
          "speaker_id": "2",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021234.1.15",
          "form": "응응 추가해도 되는데 원래 기본으로도 들어가있어",
          "original_form": "응응 추가해도 되는데 원래 기본으로도 들어가있어",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021234.1.16",
          "form": "그러고보니 나 공차 기프티콘이 있었던거같은데.. 안썻나?",
          "original_form": "그러고보니 나 공차 기프티콘이 있었던거같은데.. 안썻나?",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021234.1.17",
          "form": "자동으로 연장될걸? 나는 우롱밀크티 좋아해 ㅋㅋ",
          "original_form": "자동으로 연장될걸? 나는 우롱밀크티 좋아해 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021234.1.18",
          "form": "오오 우롱차에 우유 탄건가?",
          "original_form": "오오 우롱차에 우유 탄건가?",
          "speaker_id": "2",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100021234.1.19",
          "form": "우유에 우롱차 우린게 아닐까? 찐하던데! 맛있어 먹어봐봐",
          "original_form": "우유에 우롱차 우린게 아닐까? 찐하던데! 맛있어 먹어봐봐",
          "speaker_id": "1",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100021234.1.20",
          "form": "요즘 곱창 먹고싶다",
          "original_form": "요즘 곱창 먹고싶다",
          "speaker_id": "1",
