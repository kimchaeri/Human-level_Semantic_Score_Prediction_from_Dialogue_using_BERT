        },
        {
          "id": "MDRW2100029823.1.3",
          "form": "어느 유튜브 보고 하세요?",
          "original_form": "어느 유튜브 보고 하세요?",
          "speaker_id": "2",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100029823.1.4",
          "form": "땅크봐요",
          "original_form": "땅크봐요",
          "speaker_id": "1",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100029823.1.5",
          "form": "땅크 좋나요",
          "original_form": "땅크 좋나요",
          "speaker_id": "2",
          "time": "20210903 15:37"
        },
        {
          "id": "MDRW2100029823.1.6",
          "form": "부부케미가 좋아서 그거 보는 맛도 있습니다",
          "original_form": "부부케미가 좋아서 그거 보는 맛도 있습니다",
          "speaker_id": "1",
          "time": "20210903 15:39"
        },
        {
          "id": "MDRW2100029823.1.7",
          "form": "한번 봐야겠네요",
          "original_form": "한번 봐야겠네요",
          "speaker_id": "2",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100029823.1.8",
          "form": "난이도는 어떤가요",
          "original_form": "난이도는 어떤가요",
          "speaker_id": "2",
          "time": "20210903 15:40"
        },
        {
          "id": "MDRW2100029823.1.9",
          "form": "어렵죠",
          "original_form": "어렵죠",
          "speaker_id": "1",
          "time": "20210903 15:41"
        },
        {
          "id": "MDRW2100029823.1.10",
          "form": "힘드시겠어요",
          "original_form": "힘드시겠어요",
          "speaker_id": "2",
          "time": "20210903 15:42"
        },
        {
          "id": "MDRW2100029823.1.11",
          "form": "건강 위해서 하는거죠",
          "original_form": "건강 위해서 하는거죠",
