        },
        {
          "id": "MDRW2100045551.1.3",
          "form": "게임 하시나용",
          "original_form": "게임 하시나용",
          "speaker_id": "1",
          "time": "20210908 03:26"
        },
        {
          "id": "MDRW2100045551.1.4",
          "form": "쿠키런... ㅋㅋ",
          "original_form": "쿠키런... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:27"
        },
        {
          "id": "MDRW2100045551.1.5",
          "form": "그거 캬릭터 너무 귀여워요ㅋㅋㅋㅋ",
          "original_form": "그거 캬릭터 너무 귀여워요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100045551.1.6",
          "form": "맞아요ㅋㅋ다들 천재같아요",
          "original_form": "맞아요ㅋㅋ다들 천재같아요",
          "speaker_id": "2",
          "time": "20210908 03:29"
        },
        {
          "id": "MDRW2100045551.1.7",
          "form": "잘하시나요?",
          "original_form": "잘하시나요?",
          "speaker_id": "1",
          "time": "20210908 03:31"
        },
        {
          "id": "MDRW2100045551.1.8",
          "form": "안한지 두달?? ㅋㅋ",
          "original_form": "안한지 두달?? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:31"
        },
        {
          "id": "MDRW2100045551.1.9",
          "form": "사실 게임잘안해요ㅋㅋ",
          "original_form": "사실 게임잘안해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:32"
        },
        {
          "id": "MDRW2100045551.1.10",
          "form": "아하ㅋㅋㅋ 그러셨구나 아니면 예전에 뭐 해보신거 있으세요?",
          "original_form": "아하ㅋㅋㅋ 그러셨구나 아니면 예전에 뭐 해보신거 있으세요?",
          "speaker_id": "1",
          "time": "20210908 03:33"
        },
        {
          "id": "MDRW2100045551.1.11",
          "form": "애니팡ㅋㅋ",
          "original_form": "애니팡ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:36"
        },
        {
          "id": "MDRW2100045551.1.12",
          "form": "귀여운 캐릭터들 좋아하시는구나 맞나요?",
          "original_form": "귀여운 캐릭터들 좋아하시는구나 맞나요?",
          "speaker_id": "1",
          "time": "20210908 03:37"
        },
        {
          "id": "MDRW2100045551.1.13",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:39"
        },
        {
          "id": "MDRW2100045551.1.14",
          "form": "어떤 게임좋아하시나요?",
          "original_form": "어떤 게임좋아하시나요?",
          "speaker_id": "2",
          "time": "20210908 03:39"
        },
        {
          "id": "MDRW2100045551.1.15",
          "form": "저는 캔디크러쉬사가라고 예에전에 하던건데 ㅋㅋㅋ",
          "original_form": "저는 캔디크러쉬사가라고 예에전에 하던건데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:41"
        },
        {
          "id": "MDRW2100045551.1.16",
          "form": "요즘 다시 깔았어요",
          "original_form": "요즘 다시 깔았어요",
