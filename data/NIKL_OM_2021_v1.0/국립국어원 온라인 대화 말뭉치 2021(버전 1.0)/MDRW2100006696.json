      "utterance": [
        {
          "id": "MDRW2100006696.1.1",
          "form": "당신은 학교다닐때 어느 과목이 제일 좋았나요",
          "original_form": "당신은 학교다닐때 어느 과목이 제일 좋았나요",
          "speaker_id": "1",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100006696.1.2",
          "form": "난 수학",
          "original_form": "난 수학",
          "speaker_id": "2",
          "time": "20210908 07:12"
        },
        {
          "id": "MDRW2100006696.1.3",
          "form": "난 체육인데",
          "original_form": "난 체육인데",
          "speaker_id": "1",
          "time": "20210908 07:14"
        },
        {
          "id": "MDRW2100006696.1.4",
          "form": "당신은 운동 좋아하니까",
          "original_form": "당신은 운동 좋아하니까",
          "speaker_id": "2",
          "time": "20210908 07:20"
        },
        {
          "id": "MDRW2100006696.1.5",
          "form": "그렇죠 어렸을 때부터 운동장 뛰노는 걸 좋아했으니까요",
          "original_form": "그렇죠 어렸을 때부터 운동장 뛰노는 걸 좋아했으니까요",
          "speaker_id": "1",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100006696.1.6",
          "form": "상상이 되는군",
          "original_form": "상상이 되는군",
          "speaker_id": "2",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100006696.1.7",
          "form": "대신 공부과목은 별로 였죠",
          "original_form": "대신 공부과목은 별로 였죠",
          "speaker_id": "1",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100006696.1.8",
          "form": "그런거 같아",
          "original_form": "그런거 같아",
          "speaker_id": "2",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100006696.1.9",
          "form": "아이들이 당신닮아서 똑똑해서 다행이에요",
          "original_form": "아이들이 당신닮아서 똑똑해서 다행이에요",
          "speaker_id": "1",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100006696.1.10",
          "form": "학교성적이 다가 아니지",
          "original_form": "학교성적이 다가 아니지",
          "speaker_id": "2",
          "time": "20210908 07:22"
        },
        {
          "id": "MDRW2100006696.1.11",
          "form": "그래도요",
          "original_form": "그래도요",
          "speaker_id": "1",
          "time": "20210908 07:22"
        },
        {
          "id": "MDRW2100006696.1.12",
          "form": "선생님과 학우들과 사이좋게 지내는 성격도 중요한데 그건 당신 닮았으니까",
          "original_form": "선생님과 학우들과 사이좋게 지내는 성격도 중요한데 그건 당신 닮았으니까",
          "speaker_id": "2",
          "time": "20210908 07:22"
        },
        {
          "id": "MDRW2100006696.1.13",
          "form": "제게 사람들 대하는 건 잘하죠 ㅎㅎ",
          "original_form": "제게 사람들 대하는 건 잘하죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100006696.1.14",
          "form": "우리 딸들이 학교생활을 잘 했던 것은 우리 둘다를 닮아서지",
          "original_form": "우리 딸들이 학교생활을 잘 했던 것은 우리 둘다를 닮아서지",
          "speaker_id": "2",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100006696.1.15",
          "form": "그렇네요",
          "original_form": "그렇네요",
          "speaker_id": "1",
          "time": "20210908 07:23"
        }
