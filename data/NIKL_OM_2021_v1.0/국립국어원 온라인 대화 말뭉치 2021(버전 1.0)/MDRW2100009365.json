        {
          "id": "MDRW2100009365.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009365.1.2",
          "form": "당근마켓으로 몇 번 해봤어요",
          "original_form": "당근마켓으로 몇 번 해봤어요",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009365.1.3",
          "form": "저도 당근",
          "original_form": "저도 당근",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009365.1.4",
          "form": "거래 많이 해보셨나요?",
          "original_form": "거래 많이 해보셨나요?",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009365.1.5",
          "form": "가끔요",
          "original_form": "가끔요",
          "speaker_id": "1",
          "time": "20210822 23:08"
        },
        {
          "id": "MDRW2100009365.1.6",
          "form": "어떤 품목을 거래하셨나요?",
          "original_form": "어떤 품목을 거래하셨나요?",
          "speaker_id": "2",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009365.1.7",
          "form": "공기청정기요",
          "original_form": "공기청정기요",
          "speaker_id": "1",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009365.1.8",
          "form": "부피가 있는 걸 거래하셨네요",
          "original_form": "부피가 있는 걸 거래하셨네요",
          "speaker_id": "2",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009365.1.9",
          "form": "차량용이라소형이에요",
          "original_form": "차량용이라소형이에요",
          "speaker_id": "1",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009365.1.10",
          "form": "차량용이면 부피 때문에 거래하기 어렵진 않으셨겠네요",
          "original_form": "차량용이면 부피 때문에 거래하기 어렵진 않으셨겠네요",
          "speaker_id": "2",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009365.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
