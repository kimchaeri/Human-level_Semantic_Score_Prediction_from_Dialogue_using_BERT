      "utterance": [
        {
          "id": "MDRW2100003386.1.1",
          "form": "이번주부터 장마래",
          "original_form": "이번주부터 장마래",
          "speaker_id": "1",
          "time": "20210820 22:27"
        },
        {
          "id": "MDRW2100003386.1.2",
          "form": "주말에 계속 비온다는 말이 있더라고",
          "original_form": "주말에 계속 비온다는 말이 있더라고",
          "speaker_id": "2",
          "time": "20210820 22:27"
        },
        {
          "id": "MDRW2100003386.1.3",
          "form": "제2의 장마라는데.. 근데 원래 오늘도 비소식 있지않아써?",
          "original_form": "제2의 장마라는데.. 근데 원래 오늘도 비소식 있지않아써?",
          "speaker_id": "1",
          "time": "20210820 22:27"
        },
        {
          "id": "MDRW2100003386.1.4",
          "form": "잘은 모르겠지만 계속 먹구름 같은게 있긴 하더라",
          "original_form": "잘은 모르겠지만 계속 먹구름 같은게 있긴 하더라",
          "speaker_id": "2",
          "time": "20210820 22:27"
        },
        {
          "id": "MDRW2100003386.1.5",
          "form": "이번주 주말에 계속 비오고 월요일만 비 안오고 화수는 비온대",
          "original_form": "이번주 주말에 계속 비오고 월요일만 비 안오고 화수는 비온대",
          "speaker_id": "1",
          "time": "20210820 22:28"
        },
        {
          "id": "MDRW2100003386.1.6",
          "form": "비가 너무 안와서 힘들다고 엄마가 그러시긴 하더라",
          "original_form": "비가 너무 안와서 힘들다고 엄마가 그러시긴 하더라",
          "speaker_id": "2",
          "time": "20210820 22:28"
        },
        {
          "id": "MDRW2100003386.1.7",
          "form": "콩들이 마르고 있어.... 자기 좋아하는 청국장 못먹을수도 있어",
          "original_form": "콩들이 마르고 있어.... 자기 좋아하는 청국장 못먹을수도 있어",
          "speaker_id": "1",
          "time": "20210820 22:29"
        },
        {
          "id": "MDRW2100003386.1.8",
          "form": "비가 와야 콩들이 자라서 맛있는 청국장이 될텐데 ㅠㅠ",
          "original_form": "비가 와야 콩들이 자라서 맛있는 청국장이 될텐데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 22:29"
        },
        {
          "id": "MDRW2100003386.1.9",
          "form": "수박도 그래서 못먹자낭.. 더워서 많이 망가졌대.. 내 수박",
          "original_form": "수박도 그래서 못먹자낭.. 더워서 많이 망가졌대.. 내 수박",
          "speaker_id": "1",
          "time": "20210820 22:29"
        },
        {
          "id": "MDRW2100003386.1.10",
          "form": "그래서 가격이 비싸진거야?",
          "original_form": "그래서 가격이 비싸진거야?",
          "speaker_id": "2",
          "time": "20210820 22:30"
        },
        {
          "id": "MDRW2100003386.1.11",
          "form": "응 너무 뜨거워서 수박이 망가져서 한통에 3만원에 샀자나",
          "original_form": "응 너무 뜨거워서 수박이 망가져서 한통에 3만원에 샀자나",
          "speaker_id": "1",
          "time": "20210820 22:30"
        },
        {
          "id": "MDRW2100003386.1.12",
          "form": "진짜 비싸긴 하다 전에 2만원 내로 샀던거 같은데",
          "original_form": "진짜 비싸긴 하다 전에 2만원 내로 샀던거 같은데",
          "speaker_id": "2",
          "time": "20210820 22:30"
        },
        {
          "id": "MDRW2100003386.1.13",
          "form": "2만원도 비싸지 작년에는 만원대에 샀었어",
          "original_form": "2만원도 비싸지 작년에는 만원대에 샀었어",
          "speaker_id": "1",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003386.1.14",
          "form": "날이 한참 더웠는데 비오고 나면 추워질까?",
          "original_form": "날이 한참 더웠는데 비오고 나면 추워질까?",
          "speaker_id": "2",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003386.1.15",
          "form": "아니 아직일거같아.. 솔직히 이번 비도 와봐야 비오는 거지 구라청들..",
          "original_form": "아니 아직일거같아.. 솔직히 이번 비도 와봐야 비오는 거지 구라청들..",
          "speaker_id": "1",
          "time": "20210820 22:31"
        },
        {
          "id": "MDRW2100003386.1.16",
          "form": "날이 지금도 좋은데 좀 더 추워졌으면 좋겠어",
          "original_form": "날이 지금도 좋은데 좀 더 추워졌으면 좋겠어",
          "speaker_id": "2",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003386.1.17",
          "form": "안돼 그러면 너무 춥잖아",
          "original_form": "안돼 그러면 너무 춥잖아",
          "speaker_id": "1",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003386.1.18",
          "form": "나 전기장판 사기 전에는 안돼",
          "original_form": "나 전기장판 사기 전에는 안돼",
          "speaker_id": "1",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003386.1.19",
          "form": "두꺼운 옷을 입으면 뱃살을 감출 수 있어",
          "original_form": "두꺼운 옷을 입으면 뱃살을 감출 수 있어",
          "speaker_id": "2",
          "time": "20210820 22:33"
        },
        {
          "id": "MDRW2100003386.1.20",
          "form": "그래도 나오던데..",
          "original_form": "그래도 나오던데..",
          "speaker_id": "1",
          "time": "20210820 22:33"
        },
        {
          "id": "MDRW2100003386.1.21",
          "form": "뚫던데..",
          "original_form": "뚫던데..",
          "speaker_id": "1",
          "time": "20210820 22:33"
        },
        {
          "id": "MDRW2100003386.1.22",
          "form": "여름만큼 노골적이진 않을거야",
          "original_form": "여름만큼 노골적이진 않을거야",
          "speaker_id": "2",
          "time": "20210820 22:33"
        },
        {
          "id": "MDRW2100003386.1.23",
          "form": "겨울 별로야",
          "original_form": "겨울 별로야",
          "speaker_id": "1",
          "time": "20210820 22:33"
        },
        {
          "id": "MDRW2100003386.1.24",
          "form": "추워지는거 짱 싫어",
          "original_form": "추워지는거 짱 싫어",
          "speaker_id": "1",
          "time": "20210820 22:33"
        },
        {
          "id": "MDRW2100003386.1.25",
          "form": "나는 가을이 제일 좋아",
          "original_form": "나는 가을이 제일 좋아",
          "speaker_id": "2",
          "time": "20210820 22:34"
        },
        {
          "id": "MDRW2100003386.1.26",
          "form": "지금 날씨도 괜찮긴한데 나는",
          "original_form": "지금 날씨도 괜찮긴한데 나는",
          "speaker_id": "1",
          "time": "20210820 22:35"
        },
        {
          "id": "MDRW2100003386.1.27",
          "form": "지금은 여름의 끝 같은 느낌이지",
          "original_form": "지금은 여름의 끝 같은 느낌이지",
          "speaker_id": "2",
          "time": "20210820 22:35"
        },
        {
          "id": "MDRW2100003386.1.28",
          "form": "여름동안 너무 많이 탔어",
          "original_form": "여름동안 너무 많이 탔어",
          "speaker_id": "1",
          "time": "20210820 22:35"
        },
        {
          "id": "MDRW2100003386.1.29",
          "form": "거울 보고 깜짝 놀라써",
          "original_form": "거울 보고 깜짝 놀라써",
          "speaker_id": "1",
          "time": "20210820 22:36"
        },
        {
          "id": "MDRW2100003386.1.30",
          "form": "나도 목이랑 싹 다 탔더라",
          "original_form": "나도 목이랑 싹 다 탔더라",
          "speaker_id": "2",
          "time": "20210820 22:36"
        },
        {
          "id": "MDRW2100003386.1.31",
          "form": "자전거 타기 좋은 날이 얼른 왔으면 좋겠어",
          "original_form": "자전거 타기 좋은 날이 얼른 왔으면 좋겠어",
          "speaker_id": "1",
          "time": "20210820 22:36"
        },
        {
          "id": "MDRW2100003386.1.32",
          "form": "좀 더 추워지면 자전거타도 땀안나서 좋겠는데",
          "original_form": "좀 더 추워지면 자전거타도 땀안나서 좋겠는데",
          "speaker_id": "2",
          "time": "20210820 22:36"
        },
        {
          "id": "MDRW2100003386.1.33",
          "form": "응 너무 추워지면 자전거 타기 힘들어서 걱정이야",
          "original_form": "응 너무 추워지면 자전거 타기 힘들어서 걱정이야",
          "speaker_id": "1",
          "time": "20210820 22:37"
        },
        {
          "id": "MDRW2100003386.1.34",
          "form": "아 겨울에 자전거 타면 바람이 너무 차긴해",
          "original_form": "아 겨울에 자전거 타면 바람이 너무 차긴해",
          "speaker_id": "2",
          "time": "20210820 22:37"
        },
        {
          "id": "MDRW2100003386.1.35",
          "form": "손도 시리잖아",
          "original_form": "손도 시리잖아",
          "speaker_id": "1",
          "time": "20210820 22:38"
        },
        {
          "id": "MDRW2100003386.1.36",
          "form": "장갑도 꼭끼고 귀마개도 하고 타야해",
          "original_form": "장갑도 꼭끼고 귀마개도 하고 타야해",
          "speaker_id": "2",
          "time": "20210820 22:38"
        },
        {
          "id": "MDRW2100003386.1.37",
          "form": "얼굴도 가려야될거가튼데",
          "original_form": "얼굴도 가려야될거가튼데",
          "speaker_id": "1",
          "time": "20210820 22:38"
        },
        {
          "id": "MDRW2100003386.1.38",
          "form": "겨울은 너무 길어",
          "original_form": "겨울은 너무 길어",
          "speaker_id": "1",
          "time": "20210820 22:38"
        },
        {
          "id": "MDRW2100003386.1.39",
          "form": "나 근데 가을 타",
          "original_form": "나 근데 가을 타",
          "speaker_id": "2",
          "time": "20210820 22:39"
        },
        {
          "id": "MDRW2100003386.1.40",
          "form": "가을은 진짜 찰나인데?",
          "original_form": "가을은 진짜 찰나인데?",
          "speaker_id": "1",
          "time": "20210820 22:39"
        },
        {
          "id": "MDRW2100003386.1.41",
          "form": "근데 그 순간이 뭔가 힘들어",
          "original_form": "근데 그 순간이 뭔가 힘들어",
          "speaker_id": "2",
          "time": "20210820 22:39"
        }
      ]
    }
