      "utterance": [
        {
          "id": "MDRW2100016962.1.1",
          "form": "아 일해야되는데",
          "original_form": "아 일해야되는데",
          "speaker_id": "2",
          "time": "20210826 16:33"
        },
        {
          "id": "MDRW2100016962.1.2",
          "form": "오늘은 안바쁘신가봐요 ㅋㅋㅋㅋ",
          "original_form": "오늘은 안바쁘신가봐요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:33"
        },
        {
          "id": "MDRW2100016962.1.3",
          "form": "그때처럼 컴퓨터랑 연동하셨어요?",
          "original_form": "그때처럼 컴퓨터랑 연동하셨어요?",
          "speaker_id": "1",
          "time": "20210826 16:33"
        },
        {
          "id": "MDRW2100016962.1.4",
          "form": "적당히 바쁩니다",
          "original_form": "적당히 바쁩니다",
          "speaker_id": "2",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016962.1.5",
          "form": "그거 은근 느려서 불편하드라구요",
          "original_form": "그거 은근 느려서 불편하드라구요",
          "speaker_id": "2",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016962.1.6",
          "form": "저는완전잘사용중",
          "original_form": "저는완전잘사용중",
          "speaker_id": "1",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016962.1.7",
          "form": "와 커피먹고싶다",
          "original_form": "와 커피먹고싶다",
          "speaker_id": "1",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016962.1.8",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100016962.1.9",
          "form": "무슨 커피 좋아하세요",
          "original_form": "무슨 커피 좋아하세요",
          "speaker_id": "1",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016962.1.10",
          "form": "저는 그냥 아메리카노요!",
          "original_form": "저는 그냥 아메리카노요!",
          "speaker_id": "2",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100016962.1.11",
          "form": "아이스랑 핫 중에서 어떤거 선호하시나요",
          "original_form": "아이스랑 핫 중에서 어떤거 선호하시나요",
          "speaker_id": "1",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100016962.1.12",
          "form": "저는 .. 겨울엔 아이스 여름엔 핫이요 ㅋㅋ",
          "original_form": "저는 .. 겨울엔 아이스 여름엔 핫이요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:47"
        },
        {
          "id": "MDRW2100016962.1.13",
          "form": "ㅋㅋㅋ엥 뭔가 반대 같군요",
          "original_form": "ㅋㅋㅋ엥 뭔가 반대 같군요",
          "speaker_id": "1",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100016962.1.14",
          "form": "그런가요 이열치열이죠",
