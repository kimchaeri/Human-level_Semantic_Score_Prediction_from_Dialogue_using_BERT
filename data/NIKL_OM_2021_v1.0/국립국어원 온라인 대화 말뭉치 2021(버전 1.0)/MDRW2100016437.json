        {
          "id": "MDRW2100016437.1.1",
          "form": "게임 좋아하세용?",
          "original_form": "게임 좋아하세용?",
          "speaker_id": "2",
          "time": "20210826 12:49"
        },
        {
          "id": "MDRW2100016437.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100016437.1.3",
          "form": "하는 게임 있나요?",
          "original_form": "하는 게임 있나요?",
          "speaker_id": "2",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016437.1.4",
          "form": "피파온라인이요",
          "original_form": "피파온라인이요",
          "speaker_id": "1",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016437.1.5",
          "form": "오 피파온라인은 언제부터 하셨어요?",
          "original_form": "오 피파온라인은 언제부터 하셨어요?",
          "speaker_id": "2",
          "time": "20210826 13:44"
        },
        {
          "id": "MDRW2100016437.1.6",
          "form": "2부터 했었어요",
          "original_form": "2부터 했었어요",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016437.1.7",
          "form": "와 많이 하셨네요!",
          "original_form": "와 많이 하셨네요!",
          "speaker_id": "2",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100016437.1.8",
          "form": "어떤게임 하시나요?",
          "original_form": "어떤게임 하시나요?",
          "speaker_id": "1",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100016437.1.9",
          "form": "저는 롤이랑 쿠키런 해요!",
          "original_form": "저는 롤이랑 쿠키런 해요!",
          "speaker_id": "2",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100016437.1.10",
          "form": "오 롤 넘 어려워요",
          "original_form": "오 롤 넘 어려워요",
          "speaker_id": "1",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016437.1.11",
          "form": "롤 어렵죠... 저도 초보예요ㅋㅋㅋㅋ",
          "original_form": "롤 어렵죠... 저도 초보예요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:40"
        },
        {
          "id": "MDRW2100016437.1.12",
          "form": "티어가 어디신가요",
          "original_form": "티어가 어디신가요",
