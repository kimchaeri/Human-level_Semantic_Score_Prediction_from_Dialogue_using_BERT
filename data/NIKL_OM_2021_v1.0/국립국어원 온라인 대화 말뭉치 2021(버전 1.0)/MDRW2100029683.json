      "utterance": [
        {
          "id": "MDRW2100029683.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210903 11:41"
        },
        {
          "id": "MDRW2100029683.1.2",
          "form": "당근 콜라겐 팔아봤어요",
          "original_form": "당근 콜라겐 팔아봤어요",
          "speaker_id": "2",
          "time": "20210903 11:43"
        },
        {
          "id": "MDRW2100029683.1.3",
          "form": "비싸져?",
          "original_form": "비싸져?",
          "speaker_id": "1",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100029683.1.4",
          "form": "쫌 싸게 팔아써요 ㅎㅎ",
          "original_form": "쫌 싸게 팔아써요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 11:52"
        },
        {
          "id": "MDRW2100029683.1.5",
          "form": "몸에 흡수가 잘안된다던데요 ㅋㅋ",
          "original_form": "몸에 흡수가 잘안된다던데요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 11:52"
        },
        {
          "id": "MDRW2100029683.1.6",
          "form": "님은 머 팔거나 사봤어요",
          "original_form": "님은 머 팔거나 사봤어요",
          "speaker_id": "2",
          "time": "20210903 11:52"
        },
        {
          "id": "MDRW2100029683.1.7",
          "form": "팔기만요",
          "original_form": "팔기만요",
          "speaker_id": "1",
          "time": "20210903 11:55"
        },
        {
          "id": "MDRW2100029683.1.8",
          "form": "어떤 거 파셨어요",
          "original_form": "어떤 거 파셨어요",
          "speaker_id": "2",
          "time": "20210903 11:57"
        },
        {
          "id": "MDRW2100029683.1.9",
          "form": "공기청정기요",
          "original_form": "공기청정기요",
          "speaker_id": "1",
          "time": "20210903 12:00"
        },
        {
          "id": "MDRW2100029683.1.10",
          "form": "오 저는 펫전용 청정기 샀어요",
          "original_form": "오 저는 펫전용 청정기 샀어요",
          "speaker_id": "2",
          "time": "20210903 12:01"
        },
        {
          "id": "MDRW2100029683.1.11",
          "form": "그런것도 있어요?",
          "original_form": "그런것도 있어요?",
          "speaker_id": "1",
          "time": "20210903 12:03"
        },
        {
          "id": "MDRW2100029683.1.12",
          "form": "네 털을 걸러줘요",
          "original_form": "네 털을 걸러줘요",
          "speaker_id": "2",
          "time": "20210903 12:03"
        },
        {
          "id": "MDRW2100029683.1.13",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "1",
          "time": "20210903 14:19"
        },
        {
          "id": "MDRW2100029683.1.14",
          "form": "공기 청정기 좋은걸로 바꾸셨나봐요",
          "original_form": "공기 청정기 좋은걸로 바꾸셨나봐요",
          "speaker_id": "2",
          "time": "20210903 14:55"
        },
        {
          "id": "MDRW2100029683.1.15",
          "form": "차량용이라 소형이에요",
          "original_form": "차량용이라 소형이에요",
