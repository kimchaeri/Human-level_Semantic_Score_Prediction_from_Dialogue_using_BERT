      "utterance": [
        {
          "id": "MDRW2100020235.1.1",
          "form": "저는 요즘 뭘 사야 잘 샀다고 소문이 날까 고민 중이예요",
          "original_form": "저는 요즘 뭘 사야 잘 샀다고 소문이 날까 고민 중이예요",
          "speaker_id": "2",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100020235.1.2",
          "form": "일기를 쓰고 받은 보너스가 3만원 쯤 있는데",
          "original_form": "일기를 쓰고 받은 보너스가 3만원 쯤 있는데",
          "speaker_id": "2",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100020235.1.3",
          "form": "어디에 써야 만족할 지 궁리하느라",
          "original_form": "어디에 써야 만족할 지 궁리하느라",
          "speaker_id": "2",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100020235.1.4",
          "form": "한 2달 정도 고민 중이예요",
          "original_form": "한 2달 정도 고민 중이예요",
          "speaker_id": "2",
          "time": "20210828 18:32"
        },
        {
          "id": "MDRW2100020235.1.5",
          "form": "맨날 장바구니에 물건 담았다가 삭제하길 반복하고 있어요ㅋㅋㅋ",
          "original_form": "맨날 장바구니에 물건 담았다가 삭제하길 반복하고 있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:33"
        },
        {
          "id": "MDRW2100020235.1.6",
          "form": "name1도 혹시 저처럼 고민 중인 물건이 있나요?",
          "original_form": "&name1&도 혹시 저처럼 고민 중인 물건이 있나요?",
          "speaker_id": "2",
          "time": "20210828 18:34"
        },
        {
          "id": "MDRW2100020235.1.7",
          "form": "저는 최근에 가을 옷을 사려고",
          "original_form": "저는 최근에 가을 옷을 사려고",
          "speaker_id": "1",
          "time": "20210828 18:58"
        },
        {
          "id": "MDRW2100020235.1.8",
          "form": "보고있얻욛",
          "original_form": "보고있얻욛",
          "speaker_id": "1",
          "time": "20210828 18:58"
        },
        {
          "id": "MDRW2100020235.1.9",
          "form": "옷을 여러벌 사려다보니까",
          "original_form": "옷을 여러벌 사려다보니까",
          "speaker_id": "1",
          "time": "20210828 18:58"
        },
        {
          "id": "MDRW2100020235.1.10",
          "form": "더 고민이 깊어요",
          "original_form": "더 고민이 깊어요",
          "speaker_id": "1",
          "time": "20210828 18:59"
        },
        {
          "id": "MDRW2100020235.1.11",
          "form": "가을 옷 좋지요",
          "original_form": "가을 옷 좋지요",
          "speaker_id": "2",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100020235.1.12",
          "form": "가을겨울 패션이 볼 게 많아서 좋아요",
          "original_form": "가을겨울 패션이 볼 게 많아서 좋아요",
          "speaker_id": "2",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100020235.1.13",
          "form": "저돋 가디건 하나 넣어놓고",
          "original_form": "저돋 가디건 하나 넣어놓고",
          "speaker_id": "1",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100020235.1.14",
          "form": "두달째 고민중입니다",
          "original_form": "두달째 고민중입니다",
          "speaker_id": "1",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100020235.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:01"
        },
        {
          "id": "MDRW2100020235.1.16",
          "form": "무지막지 신중하시네요",
          "original_form": "무지막지 신중하시네요",
          "speaker_id": "2",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100020235.1.17",
          "form": "무려이번에 세일해서 4만원짜리",
          "original_form": "무려이번에 세일해서 4만원짜리",
          "speaker_id": "1",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100020235.1.18",
          "form": "2만5천원ㄷ뇄는데",
          "original_form": "2만5천원ㄷ뇄는데",
          "speaker_id": "1",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100020235.1.19",
          "form": "와 세일 많이 하네요",
          "original_form": "와 세일 많이 하네요",
          "speaker_id": "2",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100020235.1.20",
          "form": "그래도 계속 고민중입니다",
          "original_form": "그래도 계속 고민중입니다",
          "speaker_id": "1",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100020235.1.21",
          "form": "어째서죠 지금이 기회 아닌가요",
          "original_form": "어째서죠 지금이 기회 아닌가요",
          "speaker_id": "2",
          "time": "20210828 19:02"
        },
        {
          "id": "MDRW2100020235.1.22",
          "form": "사실 살려는 마음이",
          "original_form": "사실 살려는 마음이",
          "speaker_id": "1",
          "time": "20210828 19:03"
        },
        {
          "id": "MDRW2100020235.1.23",
          "form": "좀 식었어요",
          "original_form": "좀 식었어요",
          "speaker_id": "1",
          "time": "20210828 19:03"
        },
        {
          "id": "MDRW2100020235.1.24",
          "form": "아 정말요?",
          "original_form": "아 정말요?",
          "speaker_id": "2",
          "time": "20210828 19:03"
        },
        {
          "id": "MDRW2100020235.1.25",
          "form": "취향이 바뀌셨나요?",
          "original_form": "취향이 바뀌셨나요?",
          "speaker_id": "2",
          "time": "20210828 19:03"
        },
        {
          "id": "MDRW2100020235.1.26",
          "form": "네 그사이에 저랑 안어울릴것 같은",
          "original_form": "네 그사이에 저랑 안어울릴것 같은",
          "speaker_id": "1",
          "time": "20210828 19:03"
        },
        {
          "id": "MDRW2100020235.1.27",
          "form": "느낌이 들어서요",
          "original_form": "느낌이 들어서요",
          "speaker_id": "1",
          "time": "20210828 19:03"
        },
        {
          "id": "MDRW2100020235.1.28",
          "form": "오.. 그러셨구나",
          "original_form": "오.. 그러셨구나",
          "speaker_id": "2",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100020235.1.29",
          "form": "원래 이건 사야해! 하는 옷도",
          "original_form": "원래 이건 사야해! 하는 옷도",
          "speaker_id": "2",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100020235.1.30",
          "form": "나중에 왜 샀지 하는 생각이 들 수 있다는데",
          "original_form": "나중에 왜 샀지 하는 생각이 들 수 있다는데",
          "speaker_id": "2",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100020235.1.31",
          "form": "안 사고싶은 이유가 생길 정도면",
          "original_form": "안 사고싶은 이유가 생길 정도면",
          "speaker_id": "2",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100020235.1.32",
          "form": "그래서 계속 갖고싶은게 아니면",
          "original_form": "그래서 계속 갖고싶은게 아니면",
          "speaker_id": "1",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100020235.1.33",
          "form": "안 사는 게 현명한 선택일 것 같아요",
          "original_form": "안 사는 게 현명한 선택일 것 같아요",
          "speaker_id": "2",
          "time": "20210828 19:04"
        },
        {
          "id": "MDRW2100020235.1.34",
          "form": "사지않으려구요",
          "original_form": "사지않으려구요",
          "speaker_id": "1",
          "time": "20210828 19:05"
        },
        {
          "id": "MDRW2100020235.1.35",
          "form": "좋은 결정이세요",
          "original_form": "좋은 결정이세요",
          "speaker_id": "2",
          "time": "20210828 19:05"
        },
        {
          "id": "MDRW2100020235.1.36",
          "form": "돈 아껴서 정말",
          "original_form": "돈 아껴서 정말",
          "speaker_id": "2",
          "time": "20210828 19:05"
        },
        {
          "id": "MDRW2100020235.1.37",
          "form": "이것은 나의 피부이니라 싶은 옷이",
          "original_form": "이것은 나의 피부이니라 싶은 옷이",
          "speaker_id": "2",
          "time": "20210828 19:05"
        },
        {
          "id": "MDRW2100020235.1.38",
          "form": "나타난다면",
          "original_form": "나타난다면",
          "speaker_id": "2",
          "time": "20210828 19:05"
        },
        {
          "id": "MDRW2100020235.1.39",
          "form": "거기에 지르시는 것도ㅋㅋㅋ괜찮겠어요",
          "original_form": "거기에 지르시는 것도ㅋㅋㅋ괜찮겠어요",
          "speaker_id": "2",
          "time": "20210828 19:05"
        },
        {
          "id": "MDRW2100020235.1.40",
          "form": "저는 바지 허리가 커서",
          "original_form": "저는 바지 허리가 커서",
          "speaker_id": "2",
          "time": "20210828 19:06"
        },
        {
          "id": "MDRW2100020235.1.41",
          "form": "벨트를 살까 하는데",
          "original_form": "벨트를 살까 하는데",
          "speaker_id": "2",
          "time": "20210828 19:06"
        },
        {
          "id": "MDRW2100020235.1.42",
          "form": "그렇잖아도 정작 갖고싶은 옷은",
          "original_form": "그렇잖아도 정작 갖고싶은 옷은",
          "speaker_id": "1",
          "time": "20210828 19:06"
        },
        {
          "id": "MDRW2100020235.1.43",
          "form": "품절이라",
          "original_form": "품절이라",
          "speaker_id": "1",
          "time": "20210828 19:06"
        },
        {
          "id": "MDRW2100020235.1.44",
          "form": "생각보더 비싸가지구 원",
          "original_form": "생각보더 비싸가지구 원",
          "speaker_id": "2",
          "time": "20210828 19:06"
        },
        {
          "id": "MDRW2100020235.1.45",
          "form": "맘이 아프네요",
          "original_form": "맘이 아프네요",
          "speaker_id": "1",
          "time": "20210828 19:06"
        },
        {
          "id": "MDRW2100020235.1.46",
          "form": "ㅋㅋㅋㅋ아 맞아요",
          "original_form": "ㅋㅋㅋㅋ아 맞아요",
          "speaker_id": "2",
          "time": "20210828 19:06"
        },
        {
          "id": "MDRW2100020235.1.47",
          "form": "어떤벨트인가요",
          "original_form": "어떤벨트인가요",
          "speaker_id": "1",
          "time": "20210828 19:06"
        },
        {
          "id": "MDRW2100020235.1.48",
          "form": "꼭 내가 찾으면 품절임",
          "original_form": "꼭 내가 찾으면 품절임",
          "speaker_id": "2",
          "time": "20210828 19:06"
        },
        {
          "id": "MDRW2100020235.1.49",
          "form": "아이고 렉걸려서 채팅이 늦게 가네요 양해해주세요",
          "original_form": "아이고 렉걸려서 채팅이 늦게 가네요 양해해주세요",
          "speaker_id": "2",
          "time": "20210828 19:06"
        },
        {
          "id": "MDRW2100020235.1.50",
          "form": "웨빙벨트라고",
          "original_form": "웨빙벨트라고",
          "speaker_id": "2",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100020235.1.51",
          "form": "가죽이 아니라 끈을 꼬아서 만든 벨트가 있는데",
          "original_form": "가죽이 아니라 끈을 꼬아서 만든 벨트가 있는데",
          "speaker_id": "2",
          "time": "20210828 19:07"
        },
        {
          "id": "MDRW2100020235.1.52",
          "form": "유니클로에서 이번에 좀 짜임이 촘촘한 스웨이드웨빙벨트를",
          "original_form": "유니클로에서 이번에 좀 짜임이 촘촘한 스웨이드웨빙벨트를",
          "speaker_id": "2",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100020235.1.53",
          "form": "베이직한 디자인으로 내놨어요",
          "original_form": "베이직한 디자인으로 내놨어요",
          "speaker_id": "2",
          "time": "20210828 19:08"
        },
        {
          "id": "MDRW2100020235.1.54",
          "form": "벨트이야기 계속 해주세용",
          "original_form": "벨트이야기 계속 해주세용",
          "speaker_id": "1",
