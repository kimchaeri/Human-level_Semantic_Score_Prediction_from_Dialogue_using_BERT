      "utterance": [
        {
          "id": "MDRW2100025215.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 01:08"
        },
        {
          "id": "MDRW2100025215.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210901 01:08"
        },
        {
          "id": "MDRW2100025215.1.3",
          "form": "요즘 날씨가 많이 선선해진것 같아요",
          "original_form": "요즘 날씨가 많이 선선해진것 같아요",
          "speaker_id": "2",
          "time": "20210901 01:08"
        },
        {
          "id": "MDRW2100025215.1.4",
          "form": "비가 많이 와서",
          "original_form": "비가 많이 와서",
          "speaker_id": "1",
          "time": "20210901 01:09"
        },
        {
          "id": "MDRW2100025215.1.5",
          "form": "확실히 선선해졌죠 ㅎㅎ",
          "original_form": "확실히 선선해졌죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 01:09"
        },
        {
          "id": "MDRW2100025215.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210901 01:12"
        },
        {
          "id": "MDRW2100025215.1.7",
          "form": "근데 또",
          "original_form": "근데 또",
          "speaker_id": "1",
          "time": "20210901 01:13"
        },
        {
          "id": "MDRW2100025215.1.8",
          "form": "너무 많이 오니까",
          "original_form": "너무 많이 오니까",
          "speaker_id": "1",
          "time": "20210901 01:13"
        },
        {
          "id": "MDRW2100025215.1.9",
          "form": "습해서 ㅋㅋ",
          "original_form": "습해서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 01:13"
        },
        {
          "id": "MDRW2100025215.1.10",
          "form": "맞아요 남부지방은 비 피해 심한것같더라구요",
          "original_form": "맞아요 남부지방은 비 피해 심한것같더라구요",
          "speaker_id": "2",
          "time": "20210901 01:13"
        },
        {
          "id": "MDRW2100025215.1.11",
          "form": "북부지방이신군요 ㅎㅎ",
          "original_form": "북부지방이신군요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 01:14"
        },
        {
          "id": "MDRW2100025215.1.12",
          "form": "여기는 습한정도인데",
          "original_form": "여기는 습한정도인데",
          "speaker_id": "1",
          "time": "20210901 01:14"
        },
        {
          "id": "MDRW2100025215.1.13",
          "form": "남부는 진짜 물난리던데ㅠ",
          "original_form": "남부는 진짜 물난리던데ㅠ",
          "speaker_id": "1",
          "time": "20210901 01:14"
        },
        {
          "id": "MDRW2100025215.1.14",
          "form": "비가 좀 덜와야 할텐데요",
          "original_form": "비가 좀 덜와야 할텐데요",
          "speaker_id": "2",
          "time": "20210901 01:17"
        },
        {
          "id": "MDRW2100025215.1.15",
          "form": "다음주에 또 비오는것 같더라구요ㅜㅜ",
          "original_form": "다음주에 또 비오는것 같더라구요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 01:17"
        },
        {
          "id": "MDRW2100025215.1.16",
          "form": "집콕해야겠어요ㅠ",
          "original_form": "집콕해야겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210901 01:17"
        },
        {
          "id": "MDRW2100025215.1.17",
          "form": "저도 비오는날 집에 있어야겠어요",
          "original_form": "저도 비오는날 집에 있어야겠어요",
          "speaker_id": "2",
          "time": "20210901 01:19"
        },
        {
          "id": "MDRW2100025215.1.18",
          "form": "요즘 날 선선해졌는데 가을 옷은 꺼내셨나요??",
          "original_form": "요즘 날 선선해졌는데 가을 옷은 꺼내셨나요??",
          "speaker_id": "2",
          "time": "20210901 01:19"
        },
        {
          "id": "MDRW2100025215.1.19",
          "form": "행거에 쫘라락 걸어놔서 따로 안꺼내도 됩니다 ㅎㅎ",
          "original_form": "행거에 쫘라락 걸어놔서 따로 안꺼내도 됩니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 01:22"
        },
        {
          "id": "MDRW2100025215.1.20",
          "form": "우와 편하시겠네요!",
          "original_form": "우와 편하시겠네요!",
          "speaker_id": "2",
          "time": "20210901 01:25"
        },
        {
          "id": "MDRW2100025215.1.21",
          "form": "갑자기 추워져서 저는 옷장에서 가을옷 몇개 꺼내놨어요ㅜ",
          "original_form": "갑자기 추워져서 저는 옷장에서 가을옷 몇개 꺼내놨어요ㅜ",
          "speaker_id": "2",
          "time": "20210901 01:25"
        },
        {
          "id": "MDRW2100025215.1.22",
          "form": "집이 좁아서ㅋㅋ결혼하면서 독립했거든요",
          "original_form": "집이 좁아서ㅋㅋ결혼하면서 독립했거든요",
          "speaker_id": "1",
          "time": "20210901 01:29"
        },
        {
          "id": "MDRW2100025215.1.23",
          "form": "이제 슬슬 긴팔입긴 해야겠어요",
          "original_form": "이제 슬슬 긴팔입긴 해야겠어요",
          "speaker_id": "1",
          "time": "20210901 01:29"
        }
      ]
