      "utterance": [
        {
          "id": "MDRW2100011075.1.1",
          "form": "잡지는 보시는거 있으세요?",
          "original_form": "잡지는 보시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210824 22:25"
        },
        {
          "id": "MDRW2100011075.1.2",
          "form": "잡지 얼루어 보는 것 같아요",
          "original_form": "잡지 얼루어 보는 것 같아요",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100011075.1.3",
          "form": "미용실 가면 그게 있더라규요",
          "original_form": "미용실 가면 그게 있더라규요",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100011075.1.4",
          "form": "아하ㅎㅎ",
          "original_form": "아하ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100011075.1.5",
          "form": "잡지본지는 진짜 오래됐네요!",
          "original_form": "잡지본지는 진짜 오래됐네요!",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100011075.1.6",
          "form": "ㅋㅋㄱㅋㅋ가끔보면 재밌는데",
          "original_form": "ㅋㅋㄱㅋㅋ가끔보면 재밌는데",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100011075.1.7",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100011075.1.8",
          "form": "근데 잡지 안에 막",
          "original_form": "근데 잡지 안에 막",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100011075.1.9",
          "form": "머리할 때만 보는데",
          "original_form": "머리할 때만 보는데",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100011075.1.10",
          "form": "전 남친 이야기 썰로 풀어져있고",
          "original_form": "전 남친 이야기 썰로 풀어져있고",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100011075.1.11",
          "form": "괜히 흥미진진하더라고요",
          "original_form": "괜히 흥미진진하더라고요",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100011075.1.12",
          "form": "ㅋㅋㅋㄱㅋㄱㄱㅋㄱ아 맞아요 그런 페이지가 진짜 시간가는줄 모르고 읽는다니까요",
          "original_form": "ㅋㅋㅋㄱㅋㄱㄱㅋㄱ아 맞아요 그런 페이지가 진짜 시간가는줄 모르고 읽는다니까요",
          "speaker_id": "2",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100011075.1.13",
          "form": "사실 다른 패션이나 뷰티는 오~~~이쁘네~~~이게 다고 사진만 구경해요",
          "original_form": "사실 다른 패션이나 뷰티는 오~~~이쁘네~~~이게 다고 사진만 구경해요",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100011075.1.14",
          "form": "맞아요 가끔 연예인 좋아하는 연예인 나오면",
          "original_form": "맞아요 가끔 연예인 좋아하는 연예인 나오면",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100011075.1.15",
          "form": "쓰다듬고..",
          "original_form": "쓰다듬고..",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100011075.1.16",
          "form": "잡지나 모니터나 다를바가 없죠",
          "original_form": "잡지나 모니터나 다를바가 없죠",
          "speaker_id": "1",
          "time": "20210824 23:08"
        },
        {
          "id": "MDRW2100011075.1.17",
          "form": "앜ㅋㄱㅋㅋㄱㅋㅋㅋ그쵸그쵸",
          "original_form": "앜ㅋㄱㅋㅋㄱㅋㅋㅋ그쵸그쵸",
          "speaker_id": "2",
          "time": "20210824 23:19"
        },
        {
          "id": "MDRW2100011075.1.18",
          "form": "화보찍었다고하면 또 봐줘야하잖아요",
          "original_form": "화보찍었다고하면 또 봐줘야하잖아요",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100011075.1.19",
          "form": "예전엔 사기도 했는데 요즘은 덕질안해서 사본지도 오래됐어요",
          "original_form": "예전엔 사기도 했는데 요즘은 덕질안해서 사본지도 오래됐어요",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100011075.1.20",
          "form": "마지막에 사본 잡지가 어떤건가요??",
          "original_form": "마지막에 사본 잡지가 어떤건가요??",
          "speaker_id": "1",
          "time": "20210824 23:47"
        },
        {
          "id": "MDRW2100011075.1.21",
          "form": "저는 그 일본 잡지 앙앙 샀던게 기억나네요",
          "original_form": "저는 그 일본 잡지 앙앙 샀던게 기억나네요",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100011075.1.22",
          "form": "아 사실 마지막으로 산거는 컨셉진이라는 잡진데 쪼그매서 귀여운 라이프스타일?에 관한 잡지에요!",
          "original_form": "아 사실 마지막으로 산거는 컨셉진이라는 잡진데 쪼그매서 귀여운 라이프스타일?에 관한 잡지에요!",
          "speaker_id": "2",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100011075.1.23",
          "form": "오오오오오",
          "original_form": "오오오오오",
          "speaker_id": "1",
          "time": "20210825 00:52"
        },
        {
          "id": "MDRW2100011075.1.24",
          "form": "라이프스타일 잡지",
          "original_form": "라이프스타일 잡지",
          "speaker_id": "1",
          "time": "20210825 00:53"
        },
        {
          "id": "MDRW2100011075.1.25",
          "form": "인테리어 잡지가 잘 나오더라고요",
          "original_form": "인테리어 잡지가 잘 나오더라고요",
          "speaker_id": "1",
          "time": "20210825 00:53"
        },
        {
          "id": "MDRW2100011075.1.26",
          "form": "생각해보니까 패션잡지 말고 이런잡지는 종종샀네요",
          "original_form": "생각해보니까 패션잡지 말고 이런잡지는 종종샀네요",
          "speaker_id": "2",
          "time": "20210825 00:53"
        },
        {
          "id": "MDRW2100011075.1.27",
          "form": "맞아요!!",
          "original_form": "맞아요!!",
          "speaker_id": "2",
          "time": "20210825 00:53"
        },
        {
          "id": "MDRW2100011075.1.28",
          "form": "근데 잡지가 점점 구독으로 인터넷으로 가잖어요 혹시",
          "original_form": "근데 잡지가 점점 구독으로 인터넷으로 가잖어요 혹시",
          "speaker_id": "1",
          "time": "20210825 00:53"
        },
        {
          "id": "MDRW2100011075.1.29",
          "form": "구독해보신 거 있으세요?!",
          "original_form": "구독해보신 거 있으세요?!",
          "speaker_id": "1",
          "time": "20210825 00:53"
        },
        {
          "id": "MDRW2100011075.1.30",
          "form": "요즘 웹진이 대세긴 한것같은데 전 아직 없어요ㅎㅎ",
          "original_form": "요즘 웹진이 대세긴 한것같은데 전 아직 없어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:53"
        },
        {
          "id": "MDRW2100011075.1.31",
          "form": "혹시 있으세요?",
          "original_form": "혹시 있으세요?",
          "speaker_id": "2",
          "time": "20210825 00:54"
        },
        {
          "id": "MDRW2100011075.1.32",
          "form": "저는 그 구독까지는 아니고",
          "original_form": "저는 그 구독까지는 아니고",
          "speaker_id": "1",
          "time": "20210825 00:54"
        },
        {
          "id": "MDRW2100011075.1.33",
          "form": "꾸준히 샀던게 그 웹툰 리뷰 잡지가 있더라고요",
          "original_form": "꾸준히 샀던게 그 웹툰 리뷰 잡지가 있더라고요",
          "speaker_id": "1",
          "time": "20210825 00:54"
        },
        {
          "id": "MDRW2100011075.1.34",
          "form": "그거 재미잇게 봤는데 이북으로 이제 발행하도라고요",
          "original_form": "그거 재미잇게 봤는데 이북으로 이제 발행하도라고요",
          "speaker_id": "1",
          "time": "20210825 00:55"
        },
        {
          "id": "MDRW2100011075.1.35",
          "form": "오 웹툰리뷰잡지도 있나봐요!! 신기하네요",
          "original_form": "오 웹툰리뷰잡지도 있나봐요!! 신기하네요",
          "speaker_id": "2",
          "time": "20210825 00:58"
        },
        {
          "id": "MDRW2100011075.1.36",
          "form": "이북도 있어서 좋을것같아요",
          "original_form": "이북도 있어서 좋을것같아요",
          "speaker_id": "2",
          "time": "20210825 00:58"
        }
      ]
    }
