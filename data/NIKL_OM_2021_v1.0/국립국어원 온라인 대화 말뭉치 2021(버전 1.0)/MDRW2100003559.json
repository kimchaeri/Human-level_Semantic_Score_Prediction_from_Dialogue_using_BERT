      "utterance": [
        {
          "id": "MDRW2100003559.1.1",
          "form": "새벽인데 안주무시나요?",
          "original_form": "새벽인데 안주무시나요?",
          "speaker_id": "1",
          "time": "20210821 01:24"
        },
        {
          "id": "MDRW2100003559.1.2",
          "form": "네 잠이 잘 안 오네요",
          "original_form": "네 잠이 잘 안 오네요",
          "speaker_id": "2",
          "time": "20210821 01:53"
        },
        {
          "id": "MDRW2100003559.1.3",
          "form": "늦게 자면 피부에 안 좋은데",
          "original_form": "늦게 자면 피부에 안 좋은데",
          "speaker_id": "2",
          "time": "20210821 01:53"
        },
        {
          "id": "MDRW2100003559.1.4",
          "form": "불면증이신건가요?",
          "original_form": "불면증이신건가요?",
          "speaker_id": "1",
          "time": "20210821 01:54"
        },
        {
          "id": "MDRW2100003559.1.5",
          "form": "어제 백신을 맞았더니 팔이 아파서 잠이 잘 안 와요",
          "original_form": "어제 백신을 맞았더니 팔이 아파서 잠이 잘 안 와요",
          "speaker_id": "2",
          "time": "20210821 01:54"
        },
        {
          "id": "MDRW2100003559.1.6",
          "form": "10시엔 자야 피부도 좋대요^^",
          "original_form": "10시엔 자야 피부도 좋대요^^",
          "speaker_id": "1",
          "time": "20210821 01:54"
        },
        {
          "id": "MDRW2100003559.1.7",
          "form": "아 백신 맞으셨어요?",
          "original_form": "아 백신 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210821 01:54"
        },
        {
          "id": "MDRW2100003559.1.8",
          "form": "그쵸 잠이 보약인데..",
          "original_form": "그쵸 잠이 보약인데..",
          "speaker_id": "2",
          "time": "20210821 01:54"
        },
        {
          "id": "MDRW2100003559.1.9",
          "form": "네~",
          "original_form": "네~",
          "speaker_id": "2",
          "time": "20210821 01:54"
        },
        {
          "id": "MDRW2100003559.1.10",
          "form": "코로나 백신 맞으셨어요?",
          "original_form": "코로나 백신 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210821 01:54"
        },
        {
          "id": "MDRW2100003559.1.11",
          "form": "화이자맞으셨나봐요",
          "original_form": "화이자맞으셨나봐요",
          "speaker_id": "1",
          "time": "20210821 01:54"
        },
        {
          "id": "MDRW2100003559.1.12",
          "form": "네 화이자 맞았어요",
          "original_form": "네 화이자 맞았어요",
          "speaker_id": "2",
          "time": "20210821 01:55"
        },
        {
          "id": "MDRW2100003559.1.13",
          "form": "전 이번에 신청하긴했는데 무서워서 그때가서 결정할려구요",
          "original_form": "전 이번에 신청하긴했는데 무서워서 그때가서 결정할려구요",
          "speaker_id": "1",
          "time": "20210821 01:55"
        },
        {
          "id": "MDRW2100003559.1.14",
          "form": "증상이 어때요?",
          "original_form": "증상이 어때요?",
          "speaker_id": "1",
          "time": "20210821 01:55"
        },
        {
          "id": "MDRW2100003559.1.15",
          "form": "코로나가 무서워서 빨리 맞으려고 잔여백신 잡았네요",
          "original_form": "코로나가 무서워서 빨리 맞으려고 잔여백신 잡았네요",
          "speaker_id": "2",
          "time": "20210821 01:55"
        },
        {
          "id": "MDRW2100003559.1.16",
          "form": "전 크게 아프진 않고 열도 안 나는데",
          "original_form": "전 크게 아프진 않고 열도 안 나는데",
          "speaker_id": "2",
          "time": "20210821 01:55"
        },
        {
          "id": "MDRW2100003559.1.17",
          "form": "주사 맞은 쪽 팔이 많이 아파요",
          "original_form": "주사 맞은 쪽 팔이 많이 아파요",
          "speaker_id": "2",
          "time": "20210821 01:55"
        },
        {
          "id": "MDRW2100003559.1.18",
          "form": "화이자가 다른건 잘 모르겠는데  팔이 아프대요",
          "original_form": "화이자가 다른건 잘 모르겠는데  팔이 아프대요",
          "speaker_id": "1",
          "time": "20210821 01:56"
        },
        {
          "id": "MDRW2100003559.1.19",
          "form": "며칠 가기도 한다던데 심하면 저리기도 하고요",
          "original_form": "며칠 가기도 한다던데 심하면 저리기도 하고요",
          "speaker_id": "1",
          "time": "20210821 01:56"
        },
        {
          "id": "MDRW2100003559.1.20",
          "form": "네! 진짜 멍든 것처럼 계속 아파서 얼음 찜질하고 있어요",
          "original_form": "네! 진짜 멍든 것처럼 계속 아파서 얼음 찜질하고 있어요",
          "speaker_id": "2",
          "time": "20210821 01:56"
        },
        {
          "id": "MDRW2100003559.1.21",
          "form": "다른 부작용은 없어서 그나마 다행이라고 생각하고 있어요",
          "original_form": "다른 부작용은 없어서 그나마 다행이라고 생각하고 있어요",
          "speaker_id": "2",
          "time": "20210821 01:57"
        },
        {
          "id": "MDRW2100003559.1.22",
          "form": "그래도  다른증상은 없으셔서 다행이네요",
          "original_form": "그래도  다른증상은 없으셔서 다행이네요",
          "speaker_id": "1",
          "time": "20210821 01:57"
        },
        {
          "id": "MDRW2100003559.1.23",
          "form": "맞아요~ name1님은 늦은 시간에도 왜 안 자고 계세요?",
