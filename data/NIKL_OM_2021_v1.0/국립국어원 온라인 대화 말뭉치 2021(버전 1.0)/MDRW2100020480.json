      "utterance": [
        {
          "id": "MDRW2100020480.1.1",
          "form": "중국집 한번 갑시다",
          "original_form": "중국집 한번 갑시다",
          "speaker_id": "1",
          "time": "20210828 08:48"
        },
        {
          "id": "MDRW2100020480.1.2",
          "form": "중국집 너무조효",
          "original_form": "중국집 너무조효",
          "speaker_id": "2",
          "time": "20210828 08:48"
        },
        {
          "id": "MDRW2100020480.1.3",
          "form": "좋죠",
          "original_form": "좋죠",
          "speaker_id": "2",
          "time": "20210828 08:48"
        },
        {
          "id": "MDRW2100020480.1.4",
          "form": "전 중식도 사랑합니다",
          "original_form": "전 중식도 사랑합니다",
          "speaker_id": "2",
          "time": "20210828 08:48"
        },
        {
          "id": "MDRW2100020480.1.5",
          "form": "짜장면 짬뽕?",
          "original_form": "짜장면 짬뽕?",
          "speaker_id": "1",
          "time": "20210828 08:48"
        },
        {
          "id": "MDRW2100020480.1.6",
          "form": "짜장면",
          "original_form": "짜장면",
          "speaker_id": "2",
          "time": "20210828 08:48"
        },
        {
          "id": "MDRW2100020480.1.7",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210828 08:48"
        },
        {
          "id": "MDRW2100020480.1.8",
          "form": "짬뽕좋아하시는구나",
          "original_form": "짬뽕좋아하시는구나",
          "speaker_id": "2",
          "time": "20210828 08:49"
        },
        {
          "id": "MDRW2100020480.1.9",
          "form": "너무 반갑네요",
          "original_form": "너무 반갑네요",
          "speaker_id": "1",
          "time": "20210828 08:49"
        },
        {
          "id": "MDRW2100020480.1.10",
          "form": "보통 짬뽕이시던데",
          "original_form": "보통 짬뽕이시던데",
          "speaker_id": "1",
          "time": "20210828 08:49"
        },
        {
          "id": "MDRW2100020480.1.11",
          "form": "짜장면좋아하시나요",
          "original_form": "짜장면좋아하시나요",
          "speaker_id": "2",
          "time": "20210828 08:49"
        },
        {
          "id": "MDRW2100020480.1.12",
          "form": "짜장면을 좋아하는 건 아니지만",
          "original_form": "짜장면을 좋아하는 건 아니지만",
          "speaker_id": "1",
          "time": "20210828 08:49"
        },
        {
          "id": "MDRW2100020480.1.13",
          "form": "저 간짜장이 너무 좋아서",
          "original_form": "저 간짜장이 너무 좋아서",
          "speaker_id": "2",
          "time": "20210828 08:49"
        },
        {
          "id": "MDRW2100020480.1.14",
          "form": "짬뽕을 못먹는답니다",
          "original_form": "짬뽕을 못먹는답니다",
          "speaker_id": "1",
          "time": "20210828 08:49"
        },
        {
          "id": "MDRW2100020480.1.15",
          "form": "그쵸 간짜장은 좋아합니다",
          "original_form": "그쵸 간짜장은 좋아합니다",
          "speaker_id": "1",
          "time": "20210828 08:49"
        },
        {
          "id": "MDRW2100020480.1.16",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210828 08:49"
        },
        {
          "id": "MDRW2100020480.1.17",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210828 08:49"
        },
        {
          "id": "MDRW2100020480.1.18",
          "form": "천원추가의 행복",
          "original_form": "천원추가의 행복",
          "speaker_id": "1",
          "time": "20210828 08:49"
        },
        {
          "id": "MDRW2100020480.1.19",
          "form": "저는 사실.....",
          "original_form": "저는 사실.....",
          "speaker_id": "1",
          "time": "20210828 08:49"
        },
        {
          "id": "MDRW2100020480.1.20",
          "form": "알러지있으세요,",
          "original_form": "알러지있으세요,",
          "speaker_id": "2",
          "time": "20210828 08:49"
        },
        {
          "id": "MDRW2100020480.1.21",
          "form": "사실...",
          "original_form": "사실...",
          "speaker_id": "1",
          "time": "20210828 08:49"
        },
        {
          "id": "MDRW2100020480.1.22",
          "form": "해물을 안먹는답니다",
          "original_form": "해물을 안먹는답니다",
          "speaker_id": "1",
          "time": "20210828 08:50"
        },
        {
          "id": "MDRW2100020480.1.23",
          "form": "자는 해산물 알러지때문에",
          "original_form": "자는 해산물 알러지때문에",
          "speaker_id": "2",
          "time": "20210828 08:50"
        },
        {
          "id": "MDRW2100020480.1.24",
          "form": "왜냐면 맛이 없으기까요",
          "original_form": "왜냐면 맛이 없으기까요",
          "speaker_id": "1",
          "time": "20210828 08:50"
        },
        {
          "id": "MDRW2100020480.1.25",
          "form": "못먹거든요ㅋㅋㅋㅋ",
          "original_form": "못먹거든요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 08:50"
        },
        {
          "id": "MDRW2100020480.1.26",
          "form": "아 맛이없구나",
          "original_form": "아 맛이없구나",
          "speaker_id": "2",
          "time": "20210828 08:50"
        },
        {
          "id": "MDRW2100020480.1.27",
          "form": "헉..!",
          "original_form": "헉..!",
          "speaker_id": "1",
          "time": "20210828 08:50"
        },
        {
          "id": "MDRW2100020480.1.28",
          "form": "아예 못드시나요?",
          "original_form": "아예 못드시나요?",
          "speaker_id": "1",
          "time": "20210828 08:50"
        },
        {
          "id": "MDRW2100020480.1.29",
          "form": "하긴 라면이 더 나을 때  있어요",
          "original_form": "하긴 라면이 더 나을 때  있어요",
          "speaker_id": "2",
          "time": "20210828 08:50"
        },
        {
          "id": "MDRW2100020480.1.30",
          "form": "생선구운건 조금씩 먹는데",
          "original_form": "생선구운건 조금씩 먹는데",
          "speaker_id": "2",
          "time": "20210828 08:50"
        },
        {
          "id": "MDRW2100020480.1.31",
          "form": "갑각류 이런게 확 올라와서",
          "original_form": "갑각류 이런게 확 올라와서",
          "speaker_id": "2",
