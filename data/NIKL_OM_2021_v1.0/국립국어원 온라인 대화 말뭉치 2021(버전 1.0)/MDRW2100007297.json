      "utterance": [
        {
          "id": "MDRW2100007297.1.1",
          "form": "오늘 점심은 삼겹살 먹었다",
          "original_form": "오늘 점심은 삼겹살 먹었다",
          "speaker_id": "2",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100007297.1.2",
          "form": "헐 삼겹살",
          "original_form": "헐 삼겹살",
          "speaker_id": "1",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100007297.1.3",
          "form": "직접구워서요? 아님 배달?",
          "original_form": "직접구워서요? 아님 배달?",
          "speaker_id": "1",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100007297.1.4",
          "form": "버섯이랑 마늘도 꾸워서",
          "original_form": "버섯이랑 마늘도 꾸워서",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100007297.1.5",
          "form": "구웠엉",
          "original_form": "구웠엉",
          "speaker_id": "2",
          "time": "20210822 12:56"
        },
        {
          "id": "MDRW2100007297.1.6",
          "form": "헐 왜 저는 빼고",
          "original_form": "헐 왜 저는 빼고",
          "speaker_id": "1",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100007297.1.7",
          "form": "혼자드셨죠???",
          "original_form": "혼자드셨죠???",
          "speaker_id": "1",
          "time": "20210822 13:00"
        },
        {
          "id": "MDRW2100007297.1.8",
          "form": "전 오늘... 오징어 젓갈로 점심먹었어요",
          "original_form": "전 오늘... 오징어 젓갈로 점심먹었어요",
          "speaker_id": "1",
          "time": "20210822 13:00"
        },
        {
          "id": "MDRW2100007297.1.9",
          "form": "오징어젓갈 맛있징",
          "original_form": "오징어젓갈 맛있징",
          "speaker_id": "2",
          "time": "20210822 13:00"
        },
        {
          "id": "MDRW2100007297.1.10",
          "form": "너가 서울 안왔잖아 오라고 하는데도",
          "original_form": "너가 서울 안왔잖아 오라고 하는데도",
          "speaker_id": "2",
          "time": "20210822 13:00"
        },
        {
          "id": "MDRW2100007297.1.11",
          "form": "힝...ㅠㅠ",
          "original_form": "힝...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 13:01"
        },
        {
          "id": "MDRW2100007297.1.12",
          "form": "언니는 언니만의",
          "original_form": "언니는 언니만의",
          "speaker_id": "1",
          "time": "20210822 13:01"
        },
        {
          "id": "MDRW2100007297.1.13",
          "form": "고기잘굽는 꿀팁있나요",
          "original_form": "고기잘굽는 꿀팁있나요",
          "speaker_id": "1",
          "time": "20210822 13:01"
        },
        {
          "id": "MDRW2100007297.1.14",
          "form": "불판을 비우지 말라",
          "original_form": "불판을 비우지 말라",
          "speaker_id": "2",
          "time": "20210822 13:01"
        },
        {
          "id": "MDRW2100007297.1.15",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:01"
        },
        {
          "id": "MDRW2100007297.1.16",
          "form": "ㅇㄴㅋㅋㅋㅋㅋㅋ 혼자 해드실때도",
          "original_form": "ㅇㄴㅋㅋㅋㅋㅋㅋ 혼자 해드실때도",
          "speaker_id": "1",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100007297.1.17",
          "form": "불판안비우고 꽉채워 구우시나요",
          "original_form": "불판안비우고 꽉채워 구우시나요",
          "speaker_id": "1",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100007297.1.18",
          "form": "한번에 다 굽는거지",
          "original_form": "한번에 다 굽는거지",
          "speaker_id": "2",
          "time": "20210822 13:07"
        },
        {
          "id": "MDRW2100007297.1.19",
          "form": "나 명륜진사갈비 먹고싶은데 같이 갈 사람이 없엉ㅠ",
          "original_form": "나 명륜진사갈비 먹고싶은데 같이 갈 사람이 없엉ㅠ",
          "speaker_id": "2",
          "time": "20210822 13:07"
        },
        {
          "id": "MDRW2100007297.1.20",
          "form": "아 저도 거기 한번도 안가봄여",
          "original_form": "아 저도 거기 한번도 안가봄여",
          "speaker_id": "1",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100007297.1.21",
          "form": "코로나 끝나면 가보고 싶어요 ㅠ",
          "original_form": "코로나 끝나면 가보고 싶어요 ㅠ",
          "speaker_id": "1",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100007297.1.22",
          "form": "나도 무한으로 즐겨볼래...",
          "original_form": "나도 무한으로 즐겨볼래...",
          "speaker_id": "1",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100007297.1.23",
          "form": "난 가봤는데 내 입맛에 맞아",
          "original_form": "난 가봤는데 내 입맛에 맞아",
          "speaker_id": "2",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100007297.1.24",
          "form": "난 사실 저렴한것도 잘먹는 싼입맛인데 걍 달달한 갈비맛이야",
          "original_form": "난 사실 저렴한것도 잘먹는 싼입맛인데 걍 달달한 갈비맛이야",
          "speaker_id": "2",
          "time": "20210822 13:08"
        },
        {
          "id": "MDRW2100007297.1.25",
          "form": "저두 싼입맛인뎅",
          "original_form": "저두 싼입맛인뎅",
          "speaker_id": "1",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100007297.1.26",
          "form": "나중에 저랑 손잡고 가욧",
          "original_form": "나중에 저랑 손잡고 가욧",
          "speaker_id": "1",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100007297.1.27",
          "form": "ㅇㅅㅇ)/ 갈비 조아",
          "original_form": "ㅇㅅㅇ)/ 갈비 조아",
          "speaker_id": "1",
          "time": "20210822 13:10"
        },
        {
          "id": "MDRW2100007297.1.28",
          "form": "맞아 음료랑 밥도 무한인게 좋아",
          "original_form": "맞아 음료랑 밥도 무한인게 좋아",
          "speaker_id": "2",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100007297.1.29",
          "form": "음료수가 있어서 다행",
          "original_form": "음료수가 있어서 다행",
          "speaker_id": "2",
          "time": "20210822 13:11"
        },
        {
          "id": "MDRW2100007297.1.30",
          "form": "오 음료랑 밥도 무한이예요???",
          "original_form": "오 음료랑 밥도 무한이예요???",
          "speaker_id": "1",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100007297.1.31",
          "form": "근데 그런 무한리필 고기집가면 전 밥 안건들고",
          "original_form": "근데 그런 무한리필 고기집가면 전 밥 안건들고",
          "speaker_id": "1",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100007297.1.32",
          "form": "고기로만 꽉채워 먹습니당",
          "original_form": "고기로만 꽉채워 먹습니당",
          "speaker_id": "1",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100007297.1.33",
          "form": "ㅋㅋㅋ 근데 난 요즘 밥을 같이먹어",
          "original_form": "ㅋㅋㅋ 근데 난 요즘 밥을 같이먹어",
          "speaker_id": "2",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100007297.1.34",
          "form": "단점으로는 그거 있더라 달달한맛이라서 금방 물려하는 사람들이 있어",
          "original_form": "단점으로는 그거 있더라 달달한맛이라서 금방 물려하는 사람들이 있어",
          "speaker_id": "2",
          "time": "20210822 13:12"
        },
        {
          "id": "MDRW2100007297.1.35",
          "form": "난 아니지만",
          "original_form": "난 아니지만",
          "speaker_id": "2",
          "time": "20210822 13:13"
        },
        {
          "id": "MDRW2100007297.1.36",
          "form": "ㅇㅎ 물릴수가있구나",
          "original_form": "ㅇㅎ 물릴수가있구나",
          "speaker_id": "1",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100007297.1.37",
          "form": "근데 전 갈비를 원래 삼겹살목살보다 더 좋아했는데",
          "original_form": "근데 전 갈비를 원래 삼겹살목살보다 더 좋아했는데",
          "speaker_id": "1",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100007297.1.38",
          "form": "갈비 무한리필은 잘안보여서 ㅠ 못먹은지 꽤됐어요",
          "original_form": "갈비 무한리필은 잘안보여서 ㅠ 못먹은지 꽤됐어요",
          "speaker_id": "1",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100007297.1.39",
          "form": "숯불같은곳에 구워먹는거라서 나름 먹는재미도 있어",
          "original_form": "숯불같은곳에 구워먹는거라서 나름 먹는재미도 있어",
          "speaker_id": "2",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100007297.1.40",
          "form": "그럼 가야지ㅠ",
          "original_form": "그럼 가야지ㅠ",
          "speaker_id": "2",
          "time": "20210822 13:16"
        },
        {
          "id": "MDRW2100007297.1.41",
          "form": "같이 가잣",
          "original_form": "같이 가잣",
          "speaker_id": "2",
          "time": "20210822 13:17"
        },
        {
          "id": "MDRW2100007297.1.42",
          "form": "조아욧 갈비랑 막창이랑",
          "original_form": "조아욧 갈비랑 막창이랑",
          "speaker_id": "1",
          "time": "20210822 13:18"
        },
        {
          "id": "MDRW2100007297.1.43",
          "form": "다 먹는거예욧",
          "original_form": "다 먹는거예욧",
          "speaker_id": "1",
          "time": "20210822 13:18"
        },
        {
          "id": "MDRW2100007297.1.44",
          "form": "그리고 저 연어두 ㅇㅅㅇ...",
          "original_form": "그리고 저 연어두 ㅇㅅㅇ...",
          "speaker_id": "1",
          "time": "20210822 13:18"
        },
        {
          "id": "MDRW2100007297.1.45",
          "form": "ㅋㅋㅋㅋ 막창 대구에서 포장해와",
          "original_form": "ㅋㅋㅋㅋ 막창 대구에서 포장해와",
          "speaker_id": "2",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100007297.1.46",
          "form": "그 닭똥집도",
          "original_form": "그 닭똥집도",
          "speaker_id": "2",
          "time": "20210822 13:19"
        },
        {
          "id": "MDRW2100007297.1.47",
          "form": "저 이제 대구 안사는데 어케 포장해가욧",
          "original_form": "저 이제 대구 안사는데 어케 포장해가욧",
          "speaker_id": "1",
          "time": "20210822 13:23"
        },
        {
          "id": "MDRW2100007297.1.48",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그쯤되면 인터넷 주문으로 퀵배송 시키세욧",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그쯤되면 인터넷 주문으로 퀵배송 시키세욧",
          "speaker_id": "1",
          "time": "20210822 13:24"
        },
        {
          "id": "MDRW2100007297.1.49",
          "form": "거기 퀵배송해줌? 아니잖아",
          "original_form": "거기 퀵배송해줌? 아니잖아",
          "speaker_id": "2",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100007297.1.50",
          "form": "들렸다가 포장해와",
          "original_form": "들렸다가 포장해와",
          "speaker_id": "2",
          "time": "20210822 13:25"
        },
        {
          "id": "MDRW2100007297.1.51",
          "form": "ㅇㄴㅋㅋㅋㅋㅋㅋ사양하겠습니다 ㅋㅋㅋㅋ",
          "original_form": "ㅇㄴㅋㅋㅋㅋㅋㅋ사양하겠습니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100007297.1.52",
          "form": "막창은 그리고 대구가 머 특별한 맛인건 아니라서",
          "original_form": "막창은 그리고 대구가 머 특별한 맛인건 아니라서",
          "speaker_id": "1",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100007297.1.53",
          "form": "걍 먹으면 될듯여 닭똥집은 아니지만...",
          "original_form": "걍 먹으면 될듯여 닭똥집은 아니지만...",
          "speaker_id": "1",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100007297.1.54",
          "form": "서울 너무 비싸",
          "original_form": "서울 너무 비싸",
          "speaker_id": "2",
          "time": "20210822 13:26"
        },
        {
          "id": "MDRW2100007297.1.55",
          "form": "근데 진짜 먹투어 같이 하고싶다",
          "original_form": "근데 진짜 먹투어 같이 하고싶다",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100007297.1.56",
          "form": "나 갔던 부산뷔페도 좋았는데",
          "original_form": "나 갔던 부산뷔페도 좋았는데",
          "speaker_id": "2",
          "time": "20210822 13:27"
        },
        {
          "id": "MDRW2100007297.1.57",
          "form": "저도 거기 뷔페 가보고 싶어요 ㅠㅠ",
          "original_form": "저도 거기 뷔페 가보고 싶어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100007297.1.58",
          "form": "나중에 부산 다시 갈 예정있으면 저 델꼬",
          "original_form": "나중에 부산 다시 갈 예정있으면 저 델꼬",
          "speaker_id": "1",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100007297.1.59",
          "form": "거기 가줘요 ㅠㅠㅠ 나도 먹고싶다",
          "original_form": "거기 가줘요 ㅠㅠㅠ 나도 먹고싶다",
          "speaker_id": "1",
          "time": "20210822 13:29"
        },
        {
          "id": "MDRW2100007297.1.60",
          "form": "튀김을 아예 주문즉시 튀기는거 좋았고",
          "original_form": "튀김을 아예 주문즉시 튀기는거 좋았고",
          "speaker_id": "2",
          "time": "20210822 13:30"
