      "utterance": [
        {
          "id": "MDRW2100021048.1.1",
          "form": "요즘 보는 소설 있나?",
          "original_form": "요즘 보는 소설 있나?",
          "speaker_id": "1",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021048.1.2",
          "form": "닥터최태수ㅋ",
          "original_form": "닥터최태수ㅋ",
          "speaker_id": "2",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021048.1.3",
          "form": "영원히 보는중",
          "original_form": "영원히 보는중",
          "speaker_id": "2",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021048.1.4",
          "form": "ㅋㅋ 안질리나 ㅋㅋ",
          "original_form": "ㅋㅋ 안질리나 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100021048.1.5",
          "form": "난 너무 길면 중간에 하차하게 됨",
          "original_form": "난 너무 길면 중간에 하차하게 됨",
          "speaker_id": "1",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100021048.1.6",
          "form": "보다보면 기무를 못참아\r\n결제하게돼",
          "original_form": "보다보면 기무를 못참아\r\n결제하게돼",
          "speaker_id": "2",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100021048.1.7",
          "form": "결제 ㅜㅜ 난 엄청 보고싶은거 아님 참으려고함 결제하면 끝이없어서",
          "original_form": "결제 ㅜㅜ 난 엄청 보고싶은거 아님 참으려고함 결제하면 끝이없어서",
          "speaker_id": "1",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100021048.1.8",
          "form": "맞어 근데 그게 코로나 시국이라 더 결제가 잘돼\r\n안참아져",
          "original_form": "맞어 근데 그게 코로나 시국이라 더 결제가 잘돼\r\n안참아져",
          "speaker_id": "2",
          "time": "20210828 15:30"
        },
        {
          "id": "MDRW2100021048.1.9",
          "form": "책이라도 봐야지싶어서 그런가",
          "original_form": "책이라도 봐야지싶어서 그런가",
          "speaker_id": "2",
          "time": "20210828 15:30"
        },
        {
          "id": "MDRW2100021048.1.10",
          "form": "예스24에는 없어?",
          "original_form": "예스24에는 없어?",
          "speaker_id": "1",
          "time": "20210828 15:36"
        },
        {
          "id": "MDRW2100021048.1.11",
          "form": "아 예스에는 없네 아마 시리즈에도 있을걸? 최태수",
          "original_form": "아 예스에는 없네 아마 시리즈에도 있을걸? 최태수",
          "speaker_id": "1",
          "time": "20210828 15:39"
        },
        {
          "id": "MDRW2100021048.1.12",
          "form": "시리즈랑 카카페 번갈아 보면 안되나?",
          "original_form": "시리즈랑 카카페 번갈아 보면 안되나?",
          "speaker_id": "1",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100021048.1.13",
          "form": "시리즈 쿠키도 자주 줌",
          "original_form": "시리즈 쿠키도 자주 줌",
          "speaker_id": "1",
          "time": "20210828 15:40"
        },
        {
          "id": "MDRW2100021048.1.14",
          "form": "시리즈 저번에 한때 하다가 안해지더라ㅎ",
          "original_form": "시리즈 저번에 한때 하다가 안해지더라ㅎ",
          "speaker_id": "2",
          "time": "20210828 15:41"
        },
        {
          "id": "MDRW2100021048.1.15",
          "form": "그냥 어플 켜서 보던거 보면 되는거 아녀?",
          "original_form": "그냥 어플 켜서 보던거 보면 되는거 아녀?",
          "speaker_id": "1",
          "time": "20210828 15:42"
        },
        {
          "id": "MDRW2100021048.1.16",
          "form": "처음을 카카페에서 해서 그런가\r\n시리즈 뭔가 읽는거 불편한가봐\r\n손이 잘 안가",
          "original_form": "처음을 카카페에서 해서 그런가\r\n시리즈 뭔가 읽는거 불편한가봐\r\n손이 잘 안가",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100021048.1.17",
          "form": "그렇구나 그래도 아깝다 ㅜㅜ 몇만원은 절약될듯",
          "original_form": "그렇구나 그래도 아깝다 ㅜㅜ 몇만원은 절약될듯",
          "speaker_id": "1",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100021048.1.18",
          "form": "그르게 시리즈 왜 그런지 몰라",
          "original_form": "그르게 시리즈 왜 그런지 몰라",
          "speaker_id": "2",
          "time": "20210828 15:56"
        },
        {
          "id": "MDRW2100021048.1.19",
          "form": "ㅋㅋ 난 요즘 카카페 남주가 내 후원을 좋아해 본다",
          "original_form": "ㅋㅋ 난 요즘 카카페 남주가 내 후원을 좋아해 본다",
          "speaker_id": "1",
          "time": "20210828 16:10"
        },
        {
          "id": "MDRW2100021048.1.20",
          "form": "오 나 카카페 새로 보는거있다\r\n남주의 입양딸이 되었습니다ㅎ",
          "original_form": "오 나 카카페 새로 보는거있다\r\n남주의 입양딸이 되었습니다ㅎ",
          "speaker_id": "2",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100021048.1.21",
          "form": "환생 비슷한거같은데 아무 얘기도 없어",
