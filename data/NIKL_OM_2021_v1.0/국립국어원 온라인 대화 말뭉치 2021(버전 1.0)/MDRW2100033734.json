      "utterance": [
        {
          "id": "MDRW2100033734.1.1",
          "form": "나 곧 개강하자나",
          "original_form": "나 곧 개강하자나",
          "speaker_id": "1",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100033734.1.2",
          "form": "헉 정말?",
          "original_form": "헉 정말?",
          "speaker_id": "2",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100033734.1.3",
          "form": "수강신청도 다 한거야?",
          "original_form": "수강신청도 다 한거야?",
          "speaker_id": "2",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100033734.1.4",
          "form": "응 정말 실감이안나",
          "original_form": "응 정말 실감이안나",
          "speaker_id": "1",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100033734.1.5",
          "form": "몇학점 달어?",
          "original_form": "몇학점 달어?",
          "speaker_id": "2",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100033734.1.6",
          "form": "내 방학은 누가 뺏어간걸까?",
          "original_form": "내 방학은 누가 뺏어간걸까?",
          "speaker_id": "1",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100033734.1.7",
          "form": "들어?",
          "original_form": "들어?",
          "speaker_id": "2",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100033734.1.8",
          "form": "나는 13학점만 들으면 돼",
          "original_form": "나는 13학점만 들으면 돼",
          "speaker_id": "1",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100033734.1.9",
          "form": "이제 막학기였던가?",
          "original_form": "이제 막학기였던가?",
          "speaker_id": "2",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100033734.1.10",
          "form": "수강신청을 덜해서 13학점이야",
          "original_form": "수강신청을 덜해서 13학점이야",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033734.1.11",
          "form": "13점이라니 정말 할만하네",
          "original_form": "13점이라니 정말 할만하네",
          "speaker_id": "2",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033734.1.12",
          "form": "아아..실례했구나 내가.",
          "original_form": "아아..실례했구나 내가.",
          "speaker_id": "2",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033734.1.13",
          "form": "아니 아직 3년이나 남았어",
          "original_form": "아니 아직 3년이나 남았어",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033734.1.14",
          "form": "나 만학도잖아",
          "original_form": "나 만학도잖아",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033734.1.15",
          "form": "조금 미안해!",
          "original_form": "조금 미안해!",
          "speaker_id": "2",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033734.1.16",
          "form": "그래 사과를 받아줄께",
          "original_form": "그래 사과를 받아줄께",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033734.1.17",
          "form": "내가 너의 아킬레스건을 건드린걸까?",
          "original_form": "내가 너의 아킬레스건을 건드린걸까?",
          "speaker_id": "2",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033734.1.18",
          "form": "너도 다시 대학에 가는건어때?",
          "original_form": "너도 다시 대학에 가는건어때?",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033734.1.19",
          "form": "초등학교졸업은 좀 그렇잖아",
          "original_form": "초등학교졸업은 좀 그렇잖아",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033734.1.20",
          "form": "참을수없을만큼 화가난다면 대화를 꺼도 좋아.",
          "original_form": "참을수없을만큼 화가난다면 대화를 꺼도 좋아.",
          "speaker_id": "2",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033734.1.21",
          "form": "초졸인데 다시대학이라니 아귀가 안맞네",
          "original_form": "초졸인데 다시대학이라니 아귀가 안맞네",
          "speaker_id": "2",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100033734.1.22",
          "form": "나는 이미 학사를 땄는걸.",
          "original_form": "나는 이미 학사를 땄는걸.",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100033734.1.23",
          "form": "참고로 4년제 복수전공이었어.",
          "original_form": "참고로 4년제 복수전공이었어.",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100033734.1.24",
          "form": "너 그거 평생대학이잖아",
          "original_form": "너 그거 평생대학이잖아",
          "speaker_id": "1",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100033734.1.25",
          "form": "아무래도 우리 교류가 더 필요한 듯 해.",
          "original_form": "아무래도 우리 교류가 더 필요한 듯 해.",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100033734.1.26",
          "form": "나 곧 개강하잖아",
          "original_form": "나 곧 개강하잖아",
          "speaker_id": "1",
          "time": "20210831 00:14"
        }
      ]
    }
