        {
          "id": "MDRW2100003744.1.1",
          "form": "점심메뉴 정하셨나요",
          "original_form": "점심메뉴 정하셨나요",
          "speaker_id": "2",
          "time": "20210821 11:42"
        },
        {
          "id": "MDRW2100003744.1.2",
          "form": "제가 교정을 시작해서",
          "original_form": "제가 교정을 시작해서",
          "speaker_id": "1",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003744.1.3",
          "form": "아마 계란찜 먹지 않을까 싶네요",
          "original_form": "아마 계란찜 먹지 않을까 싶네요",
          "speaker_id": "1",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003744.1.4",
          "form": "헉 점심으로 계란찜이요? 다른건요?",
          "original_form": "헉 점심으로 계란찜이요? 다른건요?",
          "speaker_id": "2",
          "time": "20210821 11:52"
        },
        {
          "id": "MDRW2100003744.1.5",
          "form": "다른건 따로 안먹고 계란찜만 먹을거같아요",
          "original_form": "다른건 따로 안먹고 계란찜만 먹을거같아요",
          "speaker_id": "1",
          "time": "20210821 11:55"
        },
        {
          "id": "MDRW2100003744.1.6",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 11:55"
        },
        {
          "id": "MDRW2100003744.1.7",
          "form": "아이고 교정이 힘들구나",
          "original_form": "아이고 교정이 힘들구나",
          "speaker_id": "2",
          "time": "20210821 11:55"
        },
        {
          "id": "MDRW2100003744.1.8",
          "form": "저 가리는거 없이 다 잘먹어요",
          "original_form": "저 가리는거 없이 다 잘먹어요",
          "speaker_id": "2",
          "time": "20210821 11:55"
        },
        {
          "id": "MDRW2100003744.1.9",
          "form": "오늘은 비와서 그런지 햄버거나 배달시킬까봐요",
          "original_form": "오늘은 비와서 그런지 햄버거나 배달시킬까봐요",
          "speaker_id": "2",
          "time": "20210821 11:56"
        },
        {
          "id": "MDRW2100003744.1.10",
          "form": "어디 햄버거 제일 좋아하세요?",
          "original_form": "어디 햄버거 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210821 12:00"
        },
        {
          "id": "MDRW2100003744.1.11",
          "form": "저 버거킹이요",
          "original_form": "저 버거킹이요",
          "speaker_id": "2",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003744.1.12",
          "form": "근데 동네 배달이 안돼요ㅎㅎ",
          "original_form": "근데 동네 배달이 안돼요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003744.1.13",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 12:03"
        },
        {
          "id": "MDRW2100003744.1.14",
          "form": "저는 맥도날드가 좋더라구요",
          "original_form": "저는 맥도날드가 좋더라구요",
          "speaker_id": "1",
          "time": "20210821 12:13"
        },
        {
          "id": "MDRW2100003744.1.15",
          "form": "맥날 감튀 맛있죠",
          "original_form": "맥날 감튀 맛있죠",
