      "utterance": [
        {
          "id": "MDRW2100004325.1.1",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "1",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004325.1.2",
          "form": "아니요 무교예요",
          "original_form": "아니요 무교예요",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004325.1.3",
          "form": "아무 종교나 존중해요",
          "original_form": "아무 종교나 존중해요",
          "speaker_id": "2",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004325.1.4",
          "form": "저도요 ㅎㅎ",
          "original_form": "저도요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 16:01"
        },
        {
          "id": "MDRW2100004325.1.5",
          "form": "존중하지만 넘 절실한건 좋아하지않아요",
          "original_form": "존중하지만 넘 절실한건 좋아하지않아요",
          "speaker_id": "1",
          "time": "20210821 16:02"
        },
        {
          "id": "MDRW2100004325.1.6",
          "form": "저한테 해되지 않으면 괜찮아요",
          "original_form": "저한테 해되지 않으면 괜찮아요",
          "speaker_id": "2",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004325.1.7",
          "form": "종교활동 해본적있어요?",
          "original_form": "종교활동 해본적있어요?",
          "speaker_id": "2",
          "time": "20210821 16:03"
        },
        {
          "id": "MDRW2100004325.1.8",
          "form": "아니요 없어용 ㅎ",
          "original_form": "아니요 없어용 ㅎ",
          "speaker_id": "1",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004325.1.9",
          "form": "모임하고하는거 본적은 있네여",
          "original_form": "모임하고하는거 본적은 있네여",
          "speaker_id": "1",
          "time": "20210821 16:05"
        },
        {
          "id": "MDRW2100004325.1.10",
          "form": "성탄절되면 집에 노래부르러 오더라고요ㅎ.ㅎ",
          "original_form": "성탄절되면 집에 노래부르러 오더라고요ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004325.1.11",
          "form": "간식준비했다가 드려요",
          "original_form": "간식준비했다가 드려요",
          "speaker_id": "2",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004325.1.12",
          "form": "아진짜요? 저흰 그런건 없어용 ㅎㅎ",
          "original_form": "아진짜요? 저흰 그런건 없어용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004325.1.13",
          "form": "주택이라있는거같아요",
          "original_form": "주택이라있는거같아요",
          "speaker_id": "2",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004325.1.14",
          "form": "절은 등달고 축제하잖아요 가봤어요",
          "original_form": "절은 등달고 축제하잖아요 가봤어요",
          "speaker_id": "2",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004325.1.15",
          "form": "저도 가봤어용",
          "original_form": "저도 가봤어용",
          "speaker_id": "1",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004325.1.16",
          "form": "연꽃은 예쁜거같아요",
          "original_form": "연꽃은 예쁜거같아요",
          "speaker_id": "1",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004325.1.17",
          "form": "혹시 주변에 신천지 있어요?",
          "original_form": "혹시 주변에 신천지 있어요?",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004325.1.18",
          "form": "최근에 알게됬는데 좀 이미지가 안좋게보이더라고요",
          "original_form": "최근에 알게됬는데 좀 이미지가 안좋게보이더라고요",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004325.1.19",
          "form": "저 여주에 사는데 여기 사이비 본사?? 본교 있는거아세요?ㅋㅋㅋㅋㅋㅋ 대순진리라고",
          "original_form": "저 여주에 사는데 여기 사이비 본사?? 본교 있는거아세요?ㅋㅋㅋㅋㅋㅋ 대순진리라고",
          "speaker_id": "1",
          "time": "20210821 18:49"
        },
        {
          "id": "MDRW2100004325.1.20",
          "form": "진짜 겁나 징그러워요;",
          "original_form": "진짜 겁나 징그러워요;",
          "speaker_id": "1",
          "time": "20210821 18:49"
        },
        {
          "id": "MDRW2100004325.1.21",
          "form": "세상에 저는 대전인데 일반점포나 상가에 숨어서 살았더라고요",
          "original_form": "세상에 저는 대전인데 일반점포나 상가에 숨어서 살았더라고요",
          "speaker_id": "2",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004325.1.22",
          "form": "나중에 알게됬는데 소릉",
          "original_form": "나중에 알게됬는데 소릉",
          "speaker_id": "2",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004325.1.23",
          "form": "아닌척하고 살았다는게ㅠㅠ",
          "original_form": "아닌척하고 살았다는게ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004325.1.24",
          "form": "여기서 살인사건도 났어요..",
          "original_form": "여기서 살인사건도 났어요..",
          "speaker_id": "1",
          "time": "20210821 19:05"
        },
        {
          "id": "MDRW2100004325.1.25",
          "form": "근데 다들 숨겼다더라고 ㅋㅋ",
          "original_form": "근데 다들 숨겼다더라고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:05"
        },
        {
          "id": "MDRW2100004325.1.26",
          "form": "아이구 저는 회사에도 있었더라고요ㅋㅋ",
          "original_form": "아이구 저는 회사에도 있었더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:16"
        },
        {
          "id": "MDRW2100004325.1.27",
          "form": "안그만두고 잘 다니네요",
          "original_form": "안그만두고 잘 다니네요",
