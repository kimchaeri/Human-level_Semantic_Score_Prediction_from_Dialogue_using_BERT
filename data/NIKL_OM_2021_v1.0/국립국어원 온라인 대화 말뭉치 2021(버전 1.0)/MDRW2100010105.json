      "utterance": [
        {
          "id": "MDRW2100010105.1.1",
          "form": "네 오늘은 근데 윗분들이 휴가라 한가해요 ㅎㅎ",
          "original_form": "네 오늘은 근데 윗분들이 휴가라 한가해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 15:10"
        },
        {
          "id": "MDRW2100010105.1.2",
          "form": "상사 없으면 출근도 좋죠",
          "original_form": "상사 없으면 출근도 좋죠",
          "speaker_id": "1",
          "time": "20210823 17:08"
        },
        {
          "id": "MDRW2100010105.1.3",
          "form": "그러게요 ㅎㅎ",
          "original_form": "그러게요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 19:03"
        },
        {
          "id": "MDRW2100010105.1.4",
          "form": "맨날 이랬으면 좋겠어요 ㅎ",
          "original_form": "맨날 이랬으면 좋겠어요 ㅎ",
          "speaker_id": "2",
          "time": "20210823 19:03"
        },
        {
          "id": "MDRW2100010105.1.5",
          "form": "꿀이져",
          "original_form": "꿀이져",
          "speaker_id": "1",
          "time": "20210823 23:05"
        },
        {
          "id": "MDRW2100010105.1.6",
          "form": "부럽네네요",
          "original_form": "부럽네네요",
          "speaker_id": "1",
          "time": "20210827 07:08"
        },
        {
          "id": "MDRW2100010105.1.7",
          "form": "오늘은 금요일이라 좋네요",
          "original_form": "오늘은 금요일이라 좋네요",
          "speaker_id": "2",
          "time": "20210827 07:09"
        },
        {
          "id": "MDRW2100010105.1.8",
          "form": "금요일에는 칼퇴하시나요?",
          "original_form": "금요일에는 칼퇴하시나요?",
          "speaker_id": "2",
          "time": "20210827 07:09"
        },
        {
          "id": "MDRW2100010105.1.9",
          "form": "아마도요",
          "original_form": "아마도요",
          "speaker_id": "1",
          "time": "20210827 07:09"
        },
        {
          "id": "MDRW2100010105.1.10",
          "form": "저희회사는 금요일은 야근 못하게 해서 강제칼퇴에요 ㅎ",
          "original_form": "저희회사는 금요일은 야근 못하게 해서 강제칼퇴에요 ㅎ",
          "speaker_id": "2",
          "time": "20210827 07:10"
        },
        {
          "id": "MDRW2100010105.1.11",
          "form": "좋네요",
          "original_form": "좋네요",
          "speaker_id": "1",
          "time": "20210827 07:11"
        },
        {
          "id": "MDRW2100010105.1.12",
          "form": "같이 일하는 동료들은 어때요? 좋은 분들이 많나요?",
          "original_form": "같이 일하는 동료들은 어때요? 좋은 분들이 많나요?",
          "speaker_id": "2",
          "time": "20210827 07:15"
        },
        {
          "id": "MDRW2100010105.1.13",
          "form": "나쁜사람도요",
          "original_form": "나쁜사람도요",
          "speaker_id": "1",
          "time": "20210827 07:56"
        },
        {
          "id": "MDRW2100010105.1.14",
          "form": "ㅎㅎ 좋은사람만 있는 곳은 없네요 역시 ㅎ",
          "original_form": "ㅎㅎ 좋은사람만 있는 곳은 없네요 역시 ㅎ",
          "speaker_id": "2",
          "time": "20210827 08:13"
        },
        {
          "id": "MDRW2100010105.1.15",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210827 08:14"
        },
        {
          "id": "MDRW2100010105.1.16",
          "form": "그래도 좋은사람들 보면거 버텨야죠 ㅎㅎ",
          "original_form": "그래도 좋은사람들 보면거 버텨야죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 11:14"
        },
