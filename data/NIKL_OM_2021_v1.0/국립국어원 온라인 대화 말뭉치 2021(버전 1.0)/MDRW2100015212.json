      "utterance": [
        {
          "id": "MDRW2100015212.1.1",
          "form": "책 많이 보시나요?",
          "original_form": "책 많이 보시나요?",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015212.1.2",
          "form": "음 가끔봐요 ㅎㅎ",
          "original_form": "음 가끔봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015212.1.3",
          "form": "요즘 책보는 사람 진짜 없는것 같아요.. 물론 저도 포함.. ㅋㅋ",
          "original_form": "요즘 책보는 사람 진짜 없는것 같아요.. 물론 저도 포함.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:17"
        },
        {
          "id": "MDRW2100015212.1.4",
          "form": "도서관가는거 좋아하는데 코로나 ㅋㅋㅋ 슬픔요",
          "original_form": "도서관가는거 좋아하는데 코로나 ㅋㅋㅋ 슬픔요",
          "speaker_id": "2",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015212.1.5",
          "form": "저희쪽은 대여는 열려있어요.. \r\n완전 폐쇄에요?",
          "original_form": "저희쪽은 대여는 열려있어요.. \r\n완전 폐쇄에요?",
          "speaker_id": "1",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015212.1.6",
          "form": "아 저희는 경기쪽이라 대여도 대고\r\n도서관안에서  볼수있어요",
          "original_form": "아 저희는 경기쪽이라 대여도 대고\r\n도서관안에서  볼수있어요",
          "speaker_id": "2",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100015212.1.7",
          "form": "아ㅡ.저도 경기도..",
          "original_form": "아ㅡ.저도 경기도..",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015212.1.8",
          "form": "최근에 뭐 읽으셧어여?",
          "original_form": "최근에 뭐 읽으셧어여?",
          "speaker_id": "1",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100015212.1.9",
          "form": "최근에 도서관 읽은건 동화책 봤어요",
          "original_form": "최근에 도서관 읽은건 동화책 봤어요",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015212.1.10",
          "form": "백희나 작가님 책",
          "original_form": "백희나 작가님 책",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015212.1.11",
          "form": "아이들위해 쓴책인데  보고있으니 제가 힐링 되더라구요",
          "original_form": "아이들위해 쓴책인데  보고있으니 제가 힐링 되더라구요",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015212.1.12",
          "form": "동화책 은근 감동이죠...",
          "original_form": "동화책 은근 감동이죠...",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015212.1.13",
          "form": "성인도 가끔씩 볼만한거 같아요",
          "original_form": "성인도 가끔씩 볼만한거 같아요",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015212.1.14",
          "form": "한동안은  어린이도서관가서 살았던거 같아요 ㅋ",
          "original_form": "한동안은  어린이도서관가서 살았던거 같아요 ㅋ",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015212.1.15",
          "form": "ㅋㅋㅋ 저는 예전에 일본어 공부 초창기에 일본어 동화책 마니 봤죠..",
          "original_form": "ㅋㅋㅋ 저는 예전에 일본어 공부 초창기에 일본어 동화책 마니 봤죠..",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015212.1.16",
          "form": "아항 공부하는데 도움 될거 같아요 ㅎㅎ",
          "original_form": "아항 공부하는데 도움 될거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015212.1.17",
          "form": "어린이 도서관에 어른도 대여가능해요?",
          "original_form": "어린이 도서관에 어른도 대여가능해요?",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015212.1.18",
          "form": "그럼여 ㅎㅎㅎ",
          "original_form": "그럼여 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:48"
        }
      ]
