      "utterance": [
        {
          "id": "MDRW2100016078.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 09:15"
        },
        {
          "id": "MDRW2100016078.1.2",
          "form": "안녕하세요 수업중이세요?",
          "original_form": "안녕하세요 수업중이세요?",
          "speaker_id": "2",
          "time": "20210826 09:47"
        },
        {
          "id": "MDRW2100016078.1.3",
          "form": "저는 직장인이에요^^ 학생이신가요?",
          "original_form": "저는 직장인이에요^^ 학생이신가요?",
          "speaker_id": "1",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016078.1.4",
          "form": "아 저도요 학교생활이 그립네요",
          "original_form": "아 저도요 학교생활이 그립네요",
          "speaker_id": "2",
          "time": "20210826 11:36"
        },
        {
          "id": "MDRW2100016078.1.5",
          "form": "수강신청 앞두고 긴장하던 그 때로 돌아가고파요",
          "original_form": "수강신청 앞두고 긴장하던 그 때로 돌아가고파요",
          "speaker_id": "2",
          "time": "20210826 18:33"
        },
        {
          "id": "MDRW2100016078.1.6",
          "form": "기억나는 수업 있으세요?",
          "original_form": "기억나는 수업 있으세요?",
          "speaker_id": "2",
          "time": "20210827 07:36"
        },
        {
          "id": "MDRW2100016078.1.7",
          "form": "학창시절을 떠올릴때 가장 기억에 남는?",
          "original_form": "학창시절을 떠올릴때 가장 기억에 남는?",
          "speaker_id": "2",
          "time": "20210827 07:55"
        },
        {
          "id": "MDRW2100016078.1.8",
          "form": "동아리 했어요?",
          "original_form": "동아리 했어요?",
          "speaker_id": "2",
          "time": "20210827 09:55"
        },
        {
          "id": "MDRW2100016078.1.9",
          "form": "음.. 저는 학교 외부동아리해봤는데, 종교동아리였어요^^ 근데지금은 종교가없어서 ㅎㅎㅎ",
          "original_form": "음.. 저는 학교 외부동아리해봤는데, 종교동아리였어요^^ 근데지금은 종교가없어서 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 16:05"
        },
        {
          "id": "MDRW2100016078.1.10",
          "form": "그러시군용",
          "original_form": "그러시군용",
          "speaker_id": "2",
          "time": "20210827 16:09"
        },
        {
          "id": "MDRW2100016078.1.11",
          "form": "학교 때 친구들과 자주 만나는 편이세요?",
          "original_form": "학교 때 친구들과 자주 만나는 편이세요?",
          "speaker_id": "2",
          "time": "20210830 20:03"
        },
        {
          "id": "MDRW2100016078.1.12",
          "form": "요즘은 전혀요ㅜㅜ 연락만 하고있어요ㅜㅜ",
          "original_form": "요즘은 전혀요ㅜㅜ 연락만 하고있어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100016078.1.13",
          "form": "맞아요 요즘은 모이기가 참 힘들죠ㅜㅜ",
          "original_form": "맞아요 요즘은 모이기가 참 힘들죠ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 20:06"
        },
        {
          "id": "MDRW2100016078.1.14",
          "form": "네 만나려면 저녁에 봐야되는데 2인밖에 못모이니.... 친구들보고싶네요ㅎㅎ",
          "original_form": "네 만나려면 저녁에 봐야되는데 2인밖에 못모이니.... 친구들보고싶네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100016078.1.15",
          "form": "저도요  학교때 친구들이 참 그리워요",
          "original_form": "저도요  학교때 친구들이 참 그리워요",
          "speaker_id": "2",
          "time": "20210830 20:07"
        },
        {
          "id": "MDRW2100016078.1.16",
          "form": "동아리하셨어요?ㅎㅎ",
          "original_form": "동아리하셨어요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100016078.1.17",
          "form": "네 기타동아리랑 국악동아리 했는데",
          "original_form": "네 기타동아리랑 국악동아리 했는데",
          "speaker_id": "2",
          "time": "20210830 20:09"
        },
        {
          "id": "MDRW2100016078.1.18",
          "form": "이름 올려놓고 가서 놀기만하고",
          "original_form": "이름 올려놓고 가서 놀기만하고",
          "speaker_id": "2",
          "time": "20210830 20:09"
        },
        {
          "id": "MDRW2100016078.1.19",
          "form": "연주발표회 준비하는 연습은 제대로 안 나갔어요ㅋㅋ",
          "original_form": "연주발표회 준비하는 연습은 제대로 안 나갔어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:09"
        },
        {
          "id": "MDRW2100016078.1.20",
          "form": "국악신기한데요? 가야금같은 악기를 다루는건가요?",
          "original_form": "국악신기한데요? 가야금같은 악기를 다루는건가요?",
          "speaker_id": "1",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100016078.1.21",
          "form": "네 막 가야금 거문고 퉁소 해금 등등등",
          "original_form": "네 막 가야금 거문고 퉁소 해금 등등등",
          "speaker_id": "2",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100016078.1.22",
          "form": "국악과 전공 친구들이",
          "original_form": "국악과 전공 친구들이",
          "speaker_id": "2",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100016078.1.23",
          "form": "가면 악기 알려주고 해셔",
          "original_form": "가면 악기 알려주고 해셔",
          "speaker_id": "2",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100016078.1.24",
          "form": "신기한 경험이라 인기많은 동아리였어요",
          "original_form": "신기한 경험이라 인기많은 동아리였어요",
          "speaker_id": "2",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100016078.1.25",
          "form": "신기해요^^ 저도고등학교때 가야금배웠었어요ㅎㅎ",
          "original_form": "신기해요^^ 저도고등학교때 가야금배웠었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 20:12"
        },
        {
          "id": "MDRW2100016078.1.26",
          "form": "우왕 가야금 줄많아서 어려워보이던데 멋져요",
          "original_form": "우왕 가야금 줄많아서 어려워보이던데 멋져요",
          "speaker_id": "2",
          "time": "20210830 20:12"
        },
        {
          "id": "MDRW2100016078.1.27",
          "form": "대단대단",
          "original_form": "대단대단",
          "speaker_id": "2",
          "time": "20210830 20:12"
        },
        {
          "id": "MDRW2100016078.1.28",
          "form": "재밌긴했는데 줄튕긴다고해야하나",
          "original_form": "재밌긴했는데 줄튕긴다고해야하나",
          "speaker_id": "1",
          "time": "20210830 20:12"
        },
        {
          "id": "MDRW2100016078.1.29",
          "form": "음악시간에 배우셨어요?",
          "original_form": "음악시간에 배우셨어요?",
          "speaker_id": "2",
          "time": "20210830 20:12"
        },
        {
          "id": "MDRW2100016078.1.30",
          "form": "물집이 너무생겨서ㅜㅜ",
          "original_form": "물집이 너무생겨서ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 20:12"
        },
        {
          "id": "MDRW2100016078.1.31",
          "form": "아ㅜㅜ 그런 고충이ㅠ",
          "original_form": "아ㅜㅜ 그런 고충이ㅠ",
          "speaker_id": "2",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100016078.1.32",
          "form": "네네 음악쌤이 가야금전공하셨던분이라 전교생이 다 배웠어요ㅋㅋ",
          "original_form": "네네 음악쌤이 가야금전공하셨던분이라 전교생이 다 배웠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:13"
        }
      ]
