      "utterance": [
        {
          "id": "MDRW2100019500.1.1",
          "form": "혹시 회사 다니시나요?",
          "original_form": "혹시 회사 다니시나요?",
          "speaker_id": "1",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019500.1.2",
          "form": "아직 대학생이에요ㅠ",
          "original_form": "아직 대학생이에요ㅠ",
          "speaker_id": "2",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019500.1.3",
          "form": "name1님은 직장 있으신가요?",
          "original_form": "&name1&님은 직장 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019500.1.4",
          "form": "네 오늘 금요일이라 너무 좋아요 ㅎㅎ",
          "original_form": "네 오늘 금요일이라 너무 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019500.1.5",
          "form": "그럼 취준중이세요?",
          "original_form": "그럼 취준중이세요?",
          "speaker_id": "1",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019500.1.6",
          "form": "취준은 꾸준히 하고 있어요",
          "original_form": "취준은 꾸준히 하고 있어요",
          "speaker_id": "2",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019500.1.7",
          "form": "포트폴리오 준비하고 있습니다",
          "original_form": "포트폴리오 준비하고 있습니다",
          "speaker_id": "2",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019500.1.8",
          "form": "포트폴리오면 예술쪽 준비하시나요?",
          "original_form": "포트폴리오면 예술쪽 준비하시나요?",
          "speaker_id": "1",
          "time": "20210827 19:53"
        },
        {
          "id": "MDRW2100019500.1.9",
          "form": "디자인 전공하고 있어요",
          "original_form": "디자인 전공하고 있어요",
          "speaker_id": "2",
          "time": "20210827 19:55"
        },
        {
          "id": "MDRW2100019500.1.10",
          "form": "와 멋있어요",
          "original_form": "와 멋있어요",
          "speaker_id": "1",
          "time": "20210827 19:57"
        },
        {
          "id": "MDRW2100019500.1.11",
          "form": "저는 그냥 되는대로 취업했는데 좀 후회돼요ㅠ",
          "original_form": "저는 그냥 되는대로 취업했는데 좀 후회돼요ㅠ",
          "speaker_id": "1",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100019500.1.12",
          "form": "어떤 점에서 후회되시던가요?ㅠ 힘내세요",
          "original_form": "어떤 점에서 후회되시던가요?ㅠ 힘내세요",
          "speaker_id": "2",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100019500.1.13",
          "form": "제 전공도 딱히 살리지 않는 업무고",
          "original_form": "제 전공도 딱히 살리지 않는 업무고",
          "speaker_id": "1",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100019500.1.14",
          "form": "문과가 대부분 그렇지만 단순 사무직이라",
          "original_form": "문과가 대부분 그렇지만 단순 사무직이라",
          "speaker_id": "1",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100019500.1.15",
          "form": "연차 쌓일수록 일에 대한 회의감?이 생기는거같아요",
          "original_form": "연차 쌓일수록 일에 대한 회의감?이 생기는거같아요",
          "speaker_id": "1",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019500.1.16",
          "form": "ㅠ 슬프네요 열심히 배워서 써먹지 못하면 허무할 수 있죠",
          "original_form": "ㅠ 슬프네요 열심히 배워서 써먹지 못하면 허무할 수 있죠",
          "speaker_id": "2",
          "time": "20210827 20:00"
        },
        {
          "id": "MDRW2100019500.1.17",
          "form": "네네 ㅠ",
          "original_form": "네네 ㅠ",
          "speaker_id": "1",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019500.1.18",
          "form": "인턴도 했었는데",
          "original_form": "인턴도 했었는데",
          "speaker_id": "1",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019500.1.19",
          "form": "학교 다닐때 대외활동 활발하게 안한게 좀 아쉽더라구요",
          "original_form": "학교 다닐때 대외활동 활발하게 안한게 좀 아쉽더라구요",
          "speaker_id": "1",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019500.1.20",
          "form": "너무 겁먹고 안전한 선택을 했던거같아요..",
          "original_form": "너무 겁먹고 안전한 선택을 했던거같아요..",
          "speaker_id": "1",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019500.1.21",
          "form": "대외활동이 중요하군요... 저는 대외활동을 할 기회조차 없어요",
          "original_form": "대외활동이 중요하군요... 저는 대외활동을 할 기회조차 없어요",
          "speaker_id": "2",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019500.1.22",
          "form": "시간 내기 어려우세요?",
          "original_form": "시간 내기 어려우세요?",
          "speaker_id": "1",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019500.1.23",
          "form": "코로나 때문에 많이 막혔어요",
          "original_form": "코로나 때문에 많이 막혔어요",
          "speaker_id": "2",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019500.1.24",
          "form": "헉 그러네요",
          "original_form": "헉 그러네요",
          "speaker_id": "1",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019500.1.25",
          "form": "코로나를 생각 못했어요...",
          "original_form": "코로나를 생각 못했어요...",
          "speaker_id": "1",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019500.1.26",
          "form": "코로나 때문에 올해는 그른거 같아요",
          "original_form": "코로나 때문에 올해는 그른거 같아요",
          "speaker_id": "2",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100019500.1.27",
          "form": "그건 그래요",
          "original_form": "그건 그래요",
          "speaker_id": "1",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019500.1.28",
          "form": "주변에도 올해까지는 코로나 영향이 있는거같아요",
          "original_form": "주변에도 올해까지는 코로나 영향이 있는거같아요",
          "speaker_id": "1",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019500.1.29",
          "form": "여행 항공쪽은 아직도 돌아가면서 휴직하더라구요",
          "original_form": "여행 항공쪽은 아직도 돌아가면서 휴직하더라구요",
