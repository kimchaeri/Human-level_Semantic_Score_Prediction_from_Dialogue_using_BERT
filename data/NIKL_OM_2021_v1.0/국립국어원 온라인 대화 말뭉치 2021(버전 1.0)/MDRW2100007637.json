      "utterance": [
        {
          "id": "MDRW2100007637.1.1",
          "form": "네일 해보셨어요? ㅋㅋ",
          "original_form": "네일 해보셨어요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 08:53"
        },
        {
          "id": "MDRW2100007637.1.2",
          "form": "아뇨 ㅋㅋ",
          "original_form": "아뇨 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 08:53"
        },
        {
          "id": "MDRW2100007637.1.3",
          "form": "예전에 한번했는게",
          "original_form": "예전에 한번했는게",
          "speaker_id": "2",
          "time": "20210822 08:53"
        },
        {
          "id": "MDRW2100007637.1.4",
          "form": "손에 뭐가 붙어있는 느낌에 불편에서 안했어요",
          "original_form": "손에 뭐가 붙어있는 느낌에 불편에서 안했어요",
          "speaker_id": "2",
          "time": "20210822 08:53"
        },
        {
          "id": "MDRW2100007637.1.5",
          "form": "하세요?",
          "original_form": "하세요?",
          "speaker_id": "2",
          "time": "20210822 08:53"
        },
        {
          "id": "MDRW2100007637.1.6",
          "form": "아뇨ㅋㅋㅋ저도 약간 꽉 막혀서 숨막힌 느낌?",
          "original_form": "아뇨ㅋㅋㅋ저도 약간 꽉 막혀서 숨막힌 느낌?",
          "speaker_id": "1",
          "time": "20210822 08:54"
        },
        {
          "id": "MDRW2100007637.1.7",
          "form": "요새 디자인도 넘이뿌고 오래가서 해보고 싶긴해요",
          "original_form": "요새 디자인도 넘이뿌고 오래가서 해보고 싶긴해요",
          "speaker_id": "2",
          "time": "20210822 08:54"
        },
        {
          "id": "MDRW2100007637.1.8",
          "form": "이라서 안해요ㅋㅋㅋ 한번 쯤 해봤나 안해봤나 그래요",
          "original_form": "이라서 안해요ㅋㅋㅋ 한번 쯤 해봤나 안해봤나 그래요",
          "speaker_id": "1",
          "time": "20210822 08:54"
        },
        {
          "id": "MDRW2100007637.1.9",
          "form": "오 맞아요",
          "original_form": "오 맞아요",
          "speaker_id": "2",
          "time": "20210822 08:54"
        },
        {
          "id": "MDRW2100007637.1.10",
          "form": "저랑 똑같네요",
          "original_form": "저랑 똑같네요",
          "speaker_id": "2",
          "time": "20210822 08:54"
        },
        {
          "id": "MDRW2100007637.1.11",
          "form": "그래도 화사나ㅋㅋㅋ연옌들이 하고 있는거 보면 우와아 하기는 합니다ㅋㅋㅋ",
          "original_form": "그래도 화사나ㅋㅋㅋ연옌들이 하고 있는거 보면 우와아 하기는 합니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 08:54"
        },
        {
          "id": "MDRW2100007637.1.12",
          "form": "맞아요 화사 ㅋㅋ진짜 어찌 하고 다니는지",
          "original_form": "맞아요 화사 ㅋㅋ진짜 어찌 하고 다니는지",
          "speaker_id": "2",
          "time": "20210822 08:55"
        },
        {
          "id": "MDRW2100007637.1.13",
          "form": "넘 불편해 보여요",
          "original_form": "넘 불편해 보여요",
          "speaker_id": "2",
          "time": "20210822 08:55"
        },
        {
          "id": "MDRW2100007637.1.14",
          "form": "근데ㅋㅋㅋ패디도 안하시죠ㅋㅋㅋ",
          "original_form": "근데ㅋㅋㅋ패디도 안하시죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 08:55"
        },
        {
          "id": "MDRW2100007637.1.15",
          "form": "네 ㅋㅋ",
          "original_form": "네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 08:55"
        },
        {
          "id": "MDRW2100007637.1.16",
          "form": "저 여름철에 슬리퍼를 진짜 많이 신거든요",
          "original_form": "저 여름철에 슬리퍼를 진짜 많이 신거든요",
          "speaker_id": "1",
          "time": "20210822 08:55"
        },
        {
          "id": "MDRW2100007637.1.17",
          "form": "전 역시 자연인",
          "original_form": "전 역시 자연인",
          "speaker_id": "2",
          "time": "20210822 08:56"
        },
        {
          "id": "MDRW2100007637.1.18",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 08:56"
        },
        {
          "id": "MDRW2100007637.1.19",
          "form": "회사에서도 앞 뚫린 실내용 슬리퍼 신고ㅋㅋㅋ",
          "original_form": "회사에서도 앞 뚫린 실내용 슬리퍼 신고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 08:56"
        },
        {
          "id": "MDRW2100007637.1.20",
          "form": "저도 샌들만 신어요",
          "original_form": "저도 샌들만 신어요",
          "speaker_id": "2",
          "time": "20210822 08:56"
        },
        {
          "id": "MDRW2100007637.1.21",
          "form": "그게 편하죠",
          "original_form": "그게 편하죠",
          "speaker_id": "2",
          "time": "20210822 08:56"
        },
        {
          "id": "MDRW2100007637.1.22",
          "form": "그랬더니 회사에 언니들이 득달같이 달려들어서 이거는 죄악이라고ㅋㅋㅋㅋ",
          "original_form": "그랬더니 회사에 언니들이 득달같이 달려들어서 이거는 죄악이라고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 08:56"
        },
        {
          "id": "MDRW2100007637.1.23",
          "form": "여름엔 답답한거 싫어요",
          "original_form": "여름엔 답답한거 싫어요",
          "speaker_id": "2",
          "time": "20210822 08:56"
        },
        {
          "id": "MDRW2100007637.1.24",
          "form": "헉 세상에 죄악이라니",
          "original_form": "헉 세상에 죄악이라니",
          "speaker_id": "2",
          "time": "20210822 08:56"
        },
        {
          "id": "MDRW2100007637.1.25",
          "form": "점심시간에 저를 끌고가서 패디 구매해 주더라구요ㅋㅋㅋㅋㅋㅋㅋ스티커로 된거ㅋㅋㅋ",
          "original_form": "점심시간에 저를 끌고가서 패디 구매해 주더라구요ㅋㅋㅋㅋㅋㅋㅋ스티커로 된거ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 08:56"
        },
        {
          "id": "MDRW2100007637.1.26",
          "form": "(제 돈으로)",
          "original_form": "(제 돈으로)",
          "speaker_id": "1",
          "time": "20210822 08:56"
        },
        {
          "id": "MDRW2100007637.1.27",
          "form": "요새 스티커 엄청 잘나와요",
          "original_form": "요새 스티커 엄청 잘나와요",
          "speaker_id": "2",
          "time": "20210822 08:57"
        },
        {
          "id": "MDRW2100007637.1.28",
          "form": "헉 사주지도 않으면서 강매를 ㅋㅋ",
          "original_form": "헉 사주지도 않으면서 강매를 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 08:57"
        },
        {
          "id": "MDRW2100007637.1.29",
          "form": "ㅋㅋㅋㅋ근데 3년째 서랍에 있어요ㅋㅋㅋ써보고는 싶었는데 이쁘게 써보려고 나중에 쓰는법 영상보고 따라해야지",
          "original_form": "ㅋㅋㅋㅋ근데 3년째 서랍에 있어요ㅋㅋㅋ써보고는 싶었는데 이쁘게 써보려고 나중에 쓰는법 영상보고 따라해야지",
          "speaker_id": "1",
          "time": "20210822 08:57"
        },
        {
          "id": "MDRW2100007637.1.30",
          "form": "했던게 3년 전ㅋㅋㅋㅋ",
          "original_form": "했던게 3년 전ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 08:57"
        },
        {
          "id": "MDRW2100007637.1.31",
          "form": "오 저랑같네요",
          "original_form": "오 저랑같네요",
          "speaker_id": "2",
          "time": "20210822 08:57"
        },
        {
          "id": "MDRW2100007637.1.32",
          "form": "언젠깐 쓰겠죠ㅋㅋㅋ?",
          "original_form": "언젠깐 쓰겠죠ㅋㅋㅋ?",
          "speaker_id": "1",
          "time": "20210822 08:57"
        },
        {
          "id": "MDRW2100007637.1.33",
          "form": "저도 서랍에 2년째 있어요 ㅋㅋ",
          "original_form": "저도 서랍에 2년째 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 08:57"
        },
        {
          "id": "MDRW2100007637.1.34",
          "form": "아뇨 ㅋㅋ안써요",
          "original_form": "아뇨 ㅋㅋ안써요",
          "speaker_id": "2",
          "time": "20210822 08:57"
        },
        {
          "id": "MDRW2100007637.1.35",
          "form": "저도 넘 아까워요",
          "original_form": "저도 넘 아까워요",
          "speaker_id": "2",
          "time": "20210822 08:57"
        },
        {
          "id": "MDRW2100007637.1.36",
          "form": "근데 신기했던게ㅋㅋ저는 제가 안하니까 남 발도 본적이 없는데ㅋㅋ그분들은ㅋㅋㅋ눈에 그게 보이나봐요",
          "original_form": "근데 신기했던게ㅋㅋ저는 제가 안하니까 남 발도 본적이 없는데ㅋㅋ그분들은ㅋㅋㅋ눈에 그게 보이나봐요",
          "speaker_id": "1",
          "time": "20210822 08:57"
        },
        {
          "id": "MDRW2100007637.1.37",
          "form": "건기식은 많이 챙겨두시는 편이에요?",
          "original_form": "건기식은 많이 챙겨두시는 편이에요?",
