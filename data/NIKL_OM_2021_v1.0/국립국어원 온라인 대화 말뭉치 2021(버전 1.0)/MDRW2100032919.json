      "utterance": [
        {
          "id": "MDRW2100032919.1.1",
          "form": "오늘 학원어땠어",
          "original_form": "오늘 학원어땠어",
          "speaker_id": "2",
          "time": "20210906 19:40"
        },
        {
          "id": "MDRW2100032919.1.2",
          "form": "예전 선생님이 더 잘가르치는 거같아",
          "original_form": "예전 선생님이 더 잘가르치는 거같아",
          "speaker_id": "1",
          "time": "20210906 19:41"
        },
        {
          "id": "MDRW2100032919.1.3",
          "form": "왜 선생님이 별로야???",
          "original_form": "왜 선생님이 별로야???",
          "speaker_id": "2",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100032919.1.4",
          "form": "아직 첫날이라 더 다녀봐야겠지만",
          "original_form": "아직 첫날이라 더 다녀봐야겠지만",
          "speaker_id": "1",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100032919.1.5",
          "form": "너무 쓸때없는걸 많이해",
          "original_form": "너무 쓸때없는걸 많이해",
          "speaker_id": "1",
          "time": "20210906 19:42"
        },
        {
          "id": "MDRW2100032919.1.6",
          "form": "예전 선생님은 어디갔어??",
          "original_form": "예전 선생님은 어디갔어??",
          "speaker_id": "2",
          "time": "20210906 19:43"
        },
        {
          "id": "MDRW2100032919.1.7",
          "form": "그 선생님은 다른 학원으로갔어",
          "original_form": "그 선생님은 다른 학원으로갔어",
          "speaker_id": "1",
          "time": "20210906 19:44"
        },
        {
          "id": "MDRW2100032919.1.8",
          "form": "그럼 너도 그 학원으로 가지",
          "original_form": "그럼 너도 그 학원으로 가지",
          "speaker_id": "2",
          "time": "20210906 19:44"
        },
        {
          "id": "MDRW2100032919.1.9",
          "form": "내가 보는 시험은 강의가 안열려서ㅠ",
          "original_form": "내가 보는 시험은 강의가 안열려서ㅠ",
          "speaker_id": "1",
          "time": "20210906 19:45"
        },
        {
          "id": "MDRW2100032919.1.10",
          "form": "공부는 원래 혼자하는거지뭐...",
          "original_form": "공부는 원래 혼자하는거지뭐...",
          "speaker_id": "1",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100032919.1.11",
          "form": "그래 선생님이 별로여도 이미 다니는거 어쩔수없지 뭐",
          "original_form": "그래 선생님이 별로여도 이미 다니는거 어쩔수없지 뭐",
          "speaker_id": "2",
          "time": "20210906 19:46"
        },
        {
          "id": "MDRW2100032919.1.12",
          "form": "첫날인데 할 게 왜이렇게 많은지ㅠ",
          "original_form": "첫날인데 할 게 왜이렇게 많은지ㅠ",
          "speaker_id": "1",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100032919.1.13",
          "form": "열심히 해",
          "original_form": "열심히 해",
          "speaker_id": "2",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100032919.1.14",
          "form": "넹 열심히 할게요",
          "original_form": "넹 열심히 할게요",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100032919.1.15",
          "form": "돈 필요하면 얘기해",
          "original_form": "돈 필요하면 얘기해",
          "speaker_id": "2",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100032919.1.16",
          "form": "필요하지 한 10억만 있으면 좋겠어",
          "original_form": "필요하지 한 10억만 있으면 좋겠어",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100032919.1.17",
          "form": "아니 오늘 별로 한것도 없는데 자야돼",
          "original_form": "아니 오늘 별로 한것도 없는데 자야돼",
          "speaker_id": "1",
          "time": "20210907 00:19"
        },
        {
          "id": "MDRW2100032919.1.18",
