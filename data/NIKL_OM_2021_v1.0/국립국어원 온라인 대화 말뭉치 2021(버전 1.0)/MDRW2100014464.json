      "utterance": [
        {
          "id": "MDRW2100014464.1.1",
          "form": "구직중이신가요?",
          "original_form": "구직중이신가요?",
          "speaker_id": "1",
          "time": "20210825 19:13"
        },
        {
          "id": "MDRW2100014464.1.2",
          "form": "아예",
          "original_form": "아예",
          "speaker_id": "2",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014464.1.3",
          "form": "제가 삐딱하게 나가도 이해해주세요",
          "original_form": "제가 삐딱하게 나가도 이해해주세요",
          "speaker_id": "2",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014464.1.4",
          "form": "많이 힘드신가봐요.",
          "original_form": "많이 힘드신가봐요.",
          "speaker_id": "1",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014464.1.5",
          "form": "요새 취업률이 형편없다잖아요 ㅠㅠ",
          "original_form": "요새 취업률이 형편없다잖아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014464.1.6",
          "form": "청년 실업률이 장난아니래요",
          "original_form": "청년 실업률이 장난아니래요",
          "speaker_id": "1",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014464.1.7",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 19:17"
        },
        {
          "id": "MDRW2100014464.1.8",
          "form": "빨리 직장 들어가서",
          "original_form": "빨리 직장 들어가서",
          "speaker_id": "2",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014464.1.9",
          "form": "잔소리 좀 그만 듣고싶어요",
          "original_form": "잔소리 좀 그만 듣고싶어요",
          "speaker_id": "2",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014464.1.10",
          "form": "하루에 두번 매일 들으니깐 노이로제 걸릴거같아요",
          "original_form": "하루에 두번 매일 들으니깐 노이로제 걸릴거같아요",
          "speaker_id": "2",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014464.1.11",
          "form": "맞아요 취준생인것도 힘든데 주위에서 더  난리죠",
          "original_form": "맞아요 취준생인것도 힘든데 주위에서 더  난리죠",
          "speaker_id": "1",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014464.1.12",
          "form": "저도 그럴때가 있어서 공감가네요 ㅠㅠ",
          "original_form": "저도 그럴때가 있어서 공감가네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 19:18"
        },
        {
          "id": "MDRW2100014464.1.13",
          "form": "무슨 직업을 희망하나요?",
          "original_form": "무슨 직업을 희망하나요?",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014464.1.14",
          "form": "제일 힘든건 본인인데 말이죠 후..",
          "original_form": "제일 힘든건 본인인데 말이죠 후..",
          "speaker_id": "2",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014464.1.15",
          "form": "저는 사무보조로 갈려구요",
          "original_form": "저는 사무보조로 갈려구요",
          "speaker_id": "2",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014464.1.16",
          "form": "갈수있나??",
          "original_form": "갈수있나??",
          "speaker_id": "2",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014464.1.17",
          "form": "경리나 회계일은 몰라서",
          "original_form": "경리나 회계일은 몰라서",
          "speaker_id": "2",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014464.1.18",
          "form": "그쪽으로는 못 갈꺼같아요",
          "original_form": "그쪽으로는 못 갈꺼같아요",
          "speaker_id": "2",
          "time": "20210825 19:20"
        },
        {
          "id": "MDRW2100014464.1.19",
          "form": "경리나 회계는 경력쌓아서 본인 커리어 쌓는게 아니면 대게 박봉이예요",
          "original_form": "경리나 회계는 경력쌓아서 본인 커리어 쌓는게 아니면 대게 박봉이예요",
          "speaker_id": "1",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014464.1.20",
          "form": "저는 절대 추천하지 않아요",
          "original_form": "저는 절대 추천하지 않아요",
          "speaker_id": "1",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014464.1.21",
          "form": "어릴때 장래희망은 뭐였나요?",
          "original_form": "어릴때 장래희망은 뭐였나요?",
          "speaker_id": "1",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014464.1.22",
          "form": "간호사요",
          "original_form": "간호사요",
          "speaker_id": "2",
          "time": "20210825 19:24"
        },
        {
          "id": "MDRW2100014464.1.23",
          "form": "소꿉놀이도 그쪽으로 많이 했었죠",
          "original_form": "소꿉놀이도 그쪽으로 많이 했었죠",
          "speaker_id": "2",
          "time": "20210825 19:24"
        },
        {
          "id": "MDRW2100014464.1.24",
          "form": "그래서 고등학교때",
          "original_form": "그래서 고등학교때",
          "speaker_id": "2",
          "time": "20210825 19:25"
        },
        {
          "id": "MDRW2100014464.1.25",
          "form": "간호학과로",
          "original_form": "간호학과로",
          "speaker_id": "2",
          "time": "20210825 19:25"
        },
        {
          "id": "MDRW2100014464.1.26",
          "form": "3군데 시험쳤다가",
          "original_form": "3군데 시험쳤다가",
          "speaker_id": "2",
          "time": "20210825 19:25"
        },
        {
          "id": "MDRW2100014464.1.27",
          "form": "광탈해서 식영과로 갔어요",
          "original_form": "광탈해서 식영과로 갔어요",
          "speaker_id": "2",
          "time": "20210825 19:25"
        },
        {
          "id": "MDRW2100014464.1.28",
          "form": "앗.. 힘내세요",
          "original_form": "앗.. 힘내세요",
          "speaker_id": "1",
          "time": "20210825 19:27"
        },
        {
          "id": "MDRW2100014464.1.29",
          "form": "취업얘기는 스트레스 많이 받으시겠어요",
          "original_form": "취업얘기는 스트레스 많이 받으시겠어요",
          "speaker_id": "1",
          "time": "20210825 19:27"
        },
        {
          "id": "MDRW2100014464.1.30",
          "form": "그럼 원하는 급여액을 물어봐도 될까요?",
          "original_form": "그럼 원하는 급여액을 물어봐도 될까요?",
          "speaker_id": "1",
          "time": "20210825 19:27"
        },
        {
          "id": "MDRW2100014464.1.31",
          "form": "진짜 면접관이야!!!!",
          "original_form": "진짜 면접관이야!!!!",
          "speaker_id": "2",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014464.1.32",
          "form": "저는 신입이니깐",
          "original_form": "저는 신입이니깐",
          "speaker_id": "2",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014464.1.33",
          "form": "큰거 안바랍니다",
          "original_form": "큰거 안바랍니다",
          "speaker_id": "2",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014464.1.34",
          "form": "최저시급은 받아야죠",
          "original_form": "최저시급은 받아야죠",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014464.1.35",
          "form": "오.. 최저시급받고 야근해도 괜찮으신가요?",
          "original_form": "오.. 최저시급받고 야근해도 괜찮으신가요?",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014464.1.36",
          "form": "야근수당을 준다면",
          "original_form": "야근수당을 준다면",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014464.1.37",
          "form": "하죠",
          "original_form": "하죠",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014464.1.38",
          "form": "집에 못들어가게해도요?",
          "original_form": "집에 못들어가게해도요?",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014464.1.39",
          "form": "예?",
          "original_form": "예?",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014464.1.40",
          "form": "집은 가게해줘야할꺼같습니다",
          "original_form": "집은 가게해줘야할꺼같습니다",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014464.1.41",
          "form": "집에서 저를 기다리는",
          "original_form": "집에서 저를 기다리는",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014464.1.42",
          "form": "아이가 없지만",
          "original_form": "아이가 없지만",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014464.1.43",
          "form": "가고싶어요",
          "original_form": "가고싶어요",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014464.1.44",
          "form": "name1님은 지금 하시는일에",
          "original_form": "&name1&님은 지금 하시는일에",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014464.1.45",
          "form": "ㅋㅋㅋㅋ 그렇군요",
          "original_form": "ㅋㅋㅋㅋ 그렇군요",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014464.1.46",
          "form": "만족스러우신가요?",
          "original_form": "만족스러우신가요?",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014464.1.47",
          "form": "만족스러운 사람이 있을까요?",
          "original_form": "만족스러운 사람이 있을까요?",
          "speaker_id": "1",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014464.1.48",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014464.1.49",
          "form": "악으로깡으로 다니는거죠",
          "original_form": "악으로깡으로 다니는거죠",
          "speaker_id": "2",
          "time": "20210825 19:58"
        },
        {
          "id": "MDRW2100014464.1.50",
          "form": "저는 다음생에 재벌4세로 태어나서 받아먹고 싶어요",
          "original_form": "저는 다음생에 재벌4세로 태어나서 받아먹고 싶어요",
          "speaker_id": "1",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014464.1.51",
          "form": "저는 name1님의 애완동물로",
          "original_form": "저는 &name1&님의 애완동물로",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014464.1.52",
          "form": "태어나면될거같네요",
          "original_form": "태어나면될거같네요",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014464.1.53",
          "form": "서로 윈윈이죠",
          "original_form": "서로 윈윈이죠",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014464.1.54",
          "form": "어우 당신은 면접에서 탈락이예요",
          "original_form": "어우 당신은 면접에서 탈락이예요",
          "speaker_id": "1",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014464.1.55",
          "form": "에",
          "original_form": "에",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014464.1.56",
          "form": "회사사람들하고는 어떤가요?",
          "original_form": "회사사람들하고는 어떤가요?",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014464.1.57",
          "form": "어우 스트레스받아",
          "original_form": "어우 스트레스받아",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014464.1.58",
          "form": "회사얘기는 그만하죠?",
          "original_form": "회사얘기는 그만하죠?",
          "speaker_id": "1",
          "time": "20210825 20:00"
        },
        {
          "id": "MDRW2100014464.1.59",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014464.1.60",
          "form": "당신 회사이야기는 얼마 하지도 않았는ㄷ니ㅣ",
          "original_form": "당신 회사이야기는 얼마 하지도 않았는ㄷ니ㅣ",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014464.1.61",
          "form": "마지막으로 해본 아르바이트가 뭔가요?",
          "original_form": "마지막으로 해본 아르바이트가 뭔가요?",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014464.1.62",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014464.1.63",
          "form": "회사는 생각만해도 스트레스 받아요",
          "original_form": "회사는 생각만해도 스트레스 받아요",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014464.1.64",
          "form": "저 방금 퇴근하고 집왔다구요",
          "original_form": "저 방금 퇴근하고 집왔다구요",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014464.1.65",
          "form": "너무 옛날이라 까먹었어요",
          "original_form": "너무 옛날이라 까먹었어요",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014464.1.66",
          "form": "대충 서빙이라하죠",
          "original_form": "대충 서빙이라하죠",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014464.1.67",
          "form": "요식업 알바인가요?",
          "original_form": "요식업 알바인가요?",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014464.1.68",
