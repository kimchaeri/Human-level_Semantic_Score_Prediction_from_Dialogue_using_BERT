      "utterance": [
        {
          "id": "MDRW2100042049.1.1",
          "form": "이제 먹을려구용!",
          "original_form": "이제 먹을려구용!",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100042049.1.2",
          "form": "하셧어요?",
          "original_form": "하셧어요?",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100042049.1.3",
          "form": "네 저는 좀전에 먹었어요!",
          "original_form": "네 저는 좀전에 먹었어요!",
          "speaker_id": "1",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100042049.1.4",
          "form": "어떤메뉴",
          "original_form": "어떤메뉴",
          "speaker_id": "2",
          "time": "20210906 19:08"
        },
        {
          "id": "MDRW2100042049.1.5",
          "form": "드셧나요",
          "original_form": "드셧나요",
          "speaker_id": "2",
          "time": "20210906 19:08"
        },
        {
          "id": "MDRW2100042049.1.6",
          "form": "저는 마라탕이랑 꿔바로우 먹었어요!",
          "original_form": "저는 마라탕이랑 꿔바로우 먹었어요!",
          "speaker_id": "1",
          "time": "20210906 19:09"
        },
        {
          "id": "MDRW2100042049.1.7",
          "form": "뭐드시려구요?",
          "original_form": "뭐드시려구요?",
          "speaker_id": "1",
          "time": "20210906 19:09"
        },
        {
          "id": "MDRW2100042049.1.8",
          "form": "우아 마라탕!!",
          "original_form": "우아 마라탕!!",
          "speaker_id": "2",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100042049.1.9",
          "form": "저는 간단하게 볶음밥 먹을려구요",
          "original_form": "저는 간단하게 볶음밥 먹을려구요",
          "speaker_id": "2",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100042049.1.10",
          "form": "마라탕 자주 드세여?",
          "original_form": "마라탕 자주 드세여?",
          "speaker_id": "2",
          "time": "20210906 19:10"
        },
        {
          "id": "MDRW2100042049.1.11",
          "form": "오 볶음밥도 맛있겠네요",
          "original_form": "오 볶음밥도 맛있겠네요",
          "speaker_id": "1",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100042049.1.12",
          "form": "자주는 아니고 가끔  먹어요",
          "original_form": "자주는 아니고 가끔  먹어요",
          "speaker_id": "1",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100042049.1.13",
          "form": "로제떡볶이  드셔보셨나요?",
          "original_form": "로제떡볶이  드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210906 19:12"
        },
        {
          "id": "MDRW2100042049.1.14",
          "form": "네네!!! 너무 좋아해요!!",
          "original_form": "네네!!! 너무 좋아해요!!",
          "speaker_id": "2",
          "time": "20210906 19:13"
        },
        {
          "id": "MDRW2100042049.1.15",
          "form": "로제는 다 맛있드라구요",
          "original_form": "로제는 다 맛있드라구요",
          "speaker_id": "2",
          "time": "20210906 19:13"
        },
        {
          "id": "MDRW2100042049.1.16",
          "form": "마자요 \n신전꺼 드셔보셨나요?",
          "original_form": "마자요 \n신전꺼 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210906 19:13"
        },
        {
          "id": "MDRW2100042049.1.17",
          "form": "신전은 아니고 배떡...? 먹어본거 같아요!",
          "original_form": "신전은 아니고 배떡...? 먹어본거 같아요!",
          "speaker_id": "2",
          "time": "20210906 19:14"
        },
        {
          "id": "MDRW2100042049.1.18",
          "form": "매운거 잘드세요?",
          "original_form": "매운거 잘드세요?",
          "speaker_id": "2",
          "time": "20210906 19:14"
        },
        {
          "id": "MDRW2100042049.1.19",
          "form": "어! 저도 배떡만 먹어봤는데",
          "original_form": "어! 저도 배떡만 먹어봤는데",
          "speaker_id": "1",
          "time": "20210906 20:05"
        },
        {
          "id": "MDRW2100042049.1.20",
          "form": "신전도 맛있다그러더라구요",
          "original_form": "신전도 맛있다그러더라구요",
          "speaker_id": "1",
          "time": "20210906 20:05"
        },
        {
          "id": "MDRW2100042049.1.21",
          "form": "매운거 잘먹진 못해요ㅠ",
          "original_form": "매운거 잘먹진 못해요ㅠ",
          "speaker_id": "1",
          "time": "20210906 20:05"
        },
        {
          "id": "MDRW2100042049.1.22",
          "form": "오오 로제가 유행인가바요 ㅋㅋ",
          "original_form": "오오 로제가 유행인가바요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:21"
        },
        {
          "id": "MDRW2100042049.1.23",
          "form": "마자요 떡볶이 체인점은 거의 로제 있는거같더라구요",
          "original_form": "마자요 떡볶이 체인점은 거의 로제 있는거같더라구요",
          "speaker_id": "1",
          "time": "20210906 21:53"
        },
        {
          "id": "MDRW2100042049.1.24",
          "form": "로제 맞아요 !!",
          "original_form": "로제 맞아요 !!",
          "speaker_id": "2",
          "time": "20210906 21:54"
        }
      ]
