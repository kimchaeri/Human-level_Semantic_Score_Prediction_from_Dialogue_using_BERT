        },
        {
          "id": "MDRW2100036572.1.2",
          "form": "맞다 강아지 키우기가 싶지 않다",
          "original_form": "맞다 강아지 키우기가 싶지 않다",
          "speaker_id": "1",
          "time": "20210831 00:52"
        },
        {
          "id": "MDRW2100036572.1.3",
          "form": "엉 노견되면은 질병도 많아지고",
          "original_form": "엉 노견되면은 질병도 많아지고",
          "speaker_id": "2",
          "time": "20210831 00:53"
        },
        {
          "id": "MDRW2100036572.1.4",
          "form": "응 병원비가 너무 드는것 같다",
          "original_form": "응 병원비가 너무 드는것 같다",
          "speaker_id": "1",
          "time": "20210831 00:53"
        },
        {
          "id": "MDRW2100036572.1.5",
          "form": "사료에 간식비는",
          "original_form": "사료에 간식비는",
          "speaker_id": "2",
          "time": "20210831 00:53"
        },
        {
          "id": "MDRW2100036572.1.6",
          "form": "맞다 그것도 한두푼이 아니지",
          "original_form": "맞다 그것도 한두푼이 아니지",
          "speaker_id": "1",
          "time": "20210831 00:53"
        },
        {
          "id": "MDRW2100036572.1.7",
          "form": "거기다 미용해봐라",
          "original_form": "거기다 미용해봐라",
          "speaker_id": "2",
          "time": "20210831 00:53"
        },
        {
          "id": "MDRW2100036572.1.8",
          "form": "엉 난 산책을 하루에 2번씩은 못시켜 줄듯",
          "original_form": "엉 난 산책을 하루에 2번씩은 못시켜 줄듯",
          "speaker_id": "1",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100036572.1.9",
          "form": "반려동물을 키운다는게 싶지않다",
          "original_form": "반려동물을 키운다는게 싶지않다",
          "speaker_id": "2",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100036572.1.10",
          "form": "그러게 애들은 안키우고 싶어하나",
          "original_form": "그러게 애들은 안키우고 싶어하나",
          "speaker_id": "1",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100036572.1.11",
          "form": "키우고 싶다하지",
          "original_form": "키우고 싶다하지",
          "speaker_id": "2",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100036572.1.12",
          "form": "그래 어떻게 안된다고 햇는데",
          "original_form": "그래 어떻게 안된다고 햇는데",
          "speaker_id": "1",
          "time": "20210831 00:55"
        },
        {
          "id": "MDRW2100036572.1.13",
          "form": "독립하면 키우라고 했다",
          "original_form": "독립하면 키우라고 했다",
