      "utterance": [
        {
          "id": "MDRW2100027672.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210902 14:09"
        },
        {
          "id": "MDRW2100027672.1.2",
          "form": "아, 네.",
          "original_form": "아, 네.",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027672.1.3",
          "form": "기억나는 여행지가 있나요?",
          "original_form": "기억나는 여행지가 있나요?",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100027672.1.4",
          "form": "저는 뉴질랜드!!",
          "original_form": "저는 뉴질랜드!!",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100027672.1.5",
          "form": "1년 워킹홀리데이 다녀왔어요!",
          "original_form": "1년 워킹홀리데이 다녀왔어요!",
          "speaker_id": "1",
          "time": "20210902 14:29"
        },
        {
          "id": "MDRW2100027672.1.6",
          "form": "우와, 기억이 많이 나겠어요",
          "original_form": "우와, 기억이 많이 나겠어요",
          "speaker_id": "2",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100027672.1.7",
          "form": "네네ㅠㅠ",
          "original_form": "네네ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100027672.1.8",
          "form": "뉴질랜드에선 한국에 오고싶어 향수병걸렸는데",
          "original_form": "뉴질랜드에선 한국에 오고싶어 향수병걸렸는데",
          "speaker_id": "1",
          "time": "20210902 14:43"
        },
        {
          "id": "MDRW2100027672.1.9",
          "form": "막상 오니 뉴질랜드 가고싶어 향수병걸렸네여ㅠㅠ",
          "original_form": "막상 오니 뉴질랜드 가고싶어 향수병걸렸네여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 14:44"
        },
        {
          "id": "MDRW2100027672.1.10",
          "form": "워홀은 아니고 제동생이 1달정도 뉴질랜드로 배낭여행을 간적이 있어요",
          "original_form": "워홀은 아니고 제동생이 1달정도 뉴질랜드로 배낭여행을 간적이 있어요",
          "speaker_id": "2",
          "time": "20210902 14:58"
        },
        {
          "id": "MDRW2100027672.1.11",
          "form": "지금도 심심찮게 그 얘기를 하곤해요 ㅋㅋ",
          "original_form": "지금도 심심찮게 그 얘기를 하곤해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 14:58"
        },
        {
          "id": "MDRW2100027672.1.12",
          "form": "아아ㅋㅋㅋㅋㅋ",
          "original_form": "아아ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027672.1.13",
          "form": "근데 진짜 여행같은것은 평생기억에남는거같아요!",
          "original_form": "근데 진짜 여행같은것은 평생기억에남는거같아요!",
          "speaker_id": "1",
          "time": "20210902 15:03"
        },
        {
          "id": "MDRW2100027672.1.14",
          "form": "맞아요, 뭔가 영혼의 배터리를 채워주는 느낌",
          "original_form": "맞아요, 뭔가 영혼의 배터리를 채워주는 느낌",
          "speaker_id": "2",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027672.1.15",
          "form": "전 집순인데도 여행은 가게되더라구요",
          "original_form": "전 집순인데도 여행은 가게되더라구요",
          "speaker_id": "2",
          "time": "20210902 15:13"
        },
        {
          "id": "MDRW2100027672.1.16",
          "form": "아아 맞아요!",
          "original_form": "아아 맞아요!",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027672.1.17",
          "form": "여행은 진짜 힐링이져ㅠㅠ",
          "original_form": "여행은 진짜 힐링이져ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027672.1.18",
          "form": "코시국이여도 어쩔수없네여",
          "original_form": "코시국이여도 어쩔수없네여",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027672.1.19",
          "form": "지금은 코시국이라 어쩔수 없이 호캉스 정도로 타협하고 있어요",
          "original_form": "지금은 코시국이라 어쩔수 없이 호캉스 정도로 타협하고 있어요",
          "speaker_id": "2",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100027672.1.20",
          "form": "이것저것 구경하고 경험하는 것도 좋지만",
          "original_form": "이것저것 구경하고 경험하는 것도 좋지만",
          "speaker_id": "2",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100027672.1.21",
          "form": "쳇바퀴같은 일상에서 잠시 벗어나는 것만으로 충분히 가치가 있으니까요",
          "original_form": "쳇바퀴같은 일상에서 잠시 벗어나는 것만으로 충분히 가치가 있으니까요",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027672.1.22",
          "form": "와..",
          "original_form": "와..",
          "speaker_id": "1",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027672.1.23",
          "form": "말씀을 너무 잘하시네요",
          "original_form": "말씀을 너무 잘하시네요",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027672.1.24",
          "form": "맞습니다!!",
          "original_form": "맞습니다!!",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027672.1.25",
          "form": "전 여행못간지 2년은된거같아요..",
          "original_form": "전 여행못간지 2년은된거같아요..",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027672.1.26",
          "form": "너무 필요합니다ㅠㅠ",
          "original_form": "너무 필요합니다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027672.1.27",
          "form": "다음달에 출장을 가는데요, 출장인데도 신나는거 있죠",
          "original_form": "다음달에 출장을 가는데요, 출장인데도 신나는거 있죠",
          "speaker_id": "2",
          "time": "20210902 15:33"
        },
        {
          "id": "MDRW2100027672.1.28",
          "form": "출장도 2년동안 못갔어요",
          "original_form": "출장도 2년동안 못갔어요",
          "speaker_id": "2",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100027672.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:47"
        },
        {
          "id": "MDRW2100027672.1.30",
          "form": "출장이 신이 나신다니!",
          "original_form": "출장이 신이 나신다니!",
          "speaker_id": "1",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027672.1.31",
          "form": "와 근데 2년만이면 신날만 하시겠네요ㅠㅠ",
          "original_form": "와 근데 2년만이면 신날만 하시겠네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 15:48"
        },
        {
          "id": "MDRW2100027672.1.32",
          "form": "것두 보통때는 밑에 후배들도 데려갔는데 이번에는 저 혼자...",
          "original_form": "것두 보통때는 밑에 후배들도 데려갔는데 이번에는 저 혼자...",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027672.1.33",
          "form": "첨엔 좀 당황했는데 뭐 그냥 자유를 누리려구요ㅎㅎ",
          "original_form": "첨엔 좀 당황했는데 뭐 그냥 자유를 누리려구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100027672.1.34",
          "form": "마침 세미나가 금욜이라 주말에 거기서 쉬려구요",
          "original_form": "마침 세미나가 금욜이라 주말에 거기서 쉬려구요",
          "speaker_id": "2",
          "time": "20210902 15:51"
        },
        {
          "id": "MDRW2100027672.1.35",
          "form": "와 진짜 그럼 여행인데요?!",
          "original_form": "와 진짜 그럼 여행인데요?!",
          "speaker_id": "1",
          "time": "20210902 15:57"
        },
        {
          "id": "MDRW2100027672.1.36",
          "form": "계획같은것두 짜서 가시나요~?",
          "original_form": "계획같은것두 짜서 가시나요~?",
          "speaker_id": "1",
          "time": "20210902 15:57"
        },
        {
          "id": "MDRW2100027672.1.37",
          "form": "그냥 뭐먹을지만요ㅋㅋㅋ",
          "original_form": "그냥 뭐먹을지만요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027672.1.38",
          "form": "막 나돌아다니는건 좀 그러니까 호캉스 위주로 할 생각이에요",
          "original_form": "막 나돌아다니는건 좀 그러니까 호캉스 위주로 할 생각이에요",
          "speaker_id": "2",
          "time": "20210902 16:07"
        },
        {
          "id": "MDRW2100027672.1.39",
          "form": "아아~",
          "original_form": "아아~",
          "speaker_id": "1",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027672.1.40",
          "form": "호캉스 좋죠!!",
          "original_form": "호캉스 좋죠!!",
          "speaker_id": "1",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027672.1.41",
          "form": "회사돈으로 호캉스인가요?!",
          "original_form": "회사돈으로 호캉스인가요?!",
          "speaker_id": "1",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027672.1.42",
          "form": "음... 교통비랑 하루치 숙박비는 주겠죠?",
          "original_form": "음... 교통비랑 하루치 숙박비는 주겠죠?",
          "speaker_id": "2",
          "time": "20210902 16:21"
        },
        {
          "id": "MDRW2100027672.1.43",
          "form": "그 외는 제가 내야죠",
          "original_form": "그 외는 제가 내야죠",
          "speaker_id": "2",
          "time": "20210902 16:21"
        },
        {
          "id": "MDRW2100027672.1.44",
          "form": "아 그쵸그쵸!",
          "original_form": "아 그쵸그쵸!",
          "speaker_id": "1",
          "time": "20210902 16:27"
        }
      ]
    }
  ]
