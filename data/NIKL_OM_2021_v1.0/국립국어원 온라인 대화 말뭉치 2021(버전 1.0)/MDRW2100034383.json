        {
          "id": "MDRW2100034383.1.1",
          "form": "안녕",
          "original_form": "안녕",
          "speaker_id": "2",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100034383.1.2",
          "form": "앞치마 고르는거 도와줘",
          "original_form": "앞치마 고르는거 도와줘",
          "speaker_id": "1",
          "time": "20210828 02:30"
        },
        {
          "id": "MDRW2100034383.1.3",
          "form": "어떤 종류의 앞치마 인데 어떤",
          "original_form": "어떤 종류의 앞치마 인데 어떤",
          "speaker_id": "2",
          "time": "20210828 02:31"
        },
        {
          "id": "MDRW2100034383.1.4",
          "form": "카페에서 쓸 거 같은 예쁜 앞치마야",
          "original_form": "카페에서 쓸 거 같은 예쁜 앞치마야",
          "speaker_id": "1",
          "time": "20210828 02:31"
        },
        {
          "id": "MDRW2100034383.1.5",
          "form": "바리스타들이 쓰는 거?",
          "original_form": "바리스타들이 쓰는 거?",
          "speaker_id": "2",
          "time": "20210828 02:32"
        },
        {
          "id": "MDRW2100034383.1.6",
          "form": "맞아 그래서 이뻐",
          "original_form": "맞아 그래서 이뻐",
          "speaker_id": "1",
          "time": "20210828 02:32"
        },
        {
          "id": "MDRW2100034383.1.7",
          "form": "뭐 드라마에서 화가들이 쓰는 것도 예쁘던데 그런 건가",
          "original_form": "뭐 드라마에서 화가들이 쓰는 것도 예쁘던데 그런 건가",
          "speaker_id": "2",
          "time": "20210828 02:32"
        },
        {
          "id": "MDRW2100034383.1.8",
          "form": "그런데 색깔이 다양해서 고민이야",
          "original_form": "그런데 색깔이 다양해서 고민이야",
          "speaker_id": "1",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100034383.1.9",
          "form": "좀 어두워야 하지 않아 앞치마",
          "original_form": "좀 어두워야 하지 않아 앞치마",
          "speaker_id": "2",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100034383.1.10",
          "form": "갈색이 예쁠까 검은색이 예쁠까",
          "original_form": "갈색이 예쁠까 검은색이 예쁠까",
          "speaker_id": "1",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100034383.1.11",
          "form": "회색은 어때",
          "original_form": "회색은 어때",
          "speaker_id": "1",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100034383.1.12",
          "form": "는 검은색 추천해",
          "original_form": "는 검은색 추천해",
          "speaker_id": "2",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100034383.1.13",
          "form": "회색은 어때",
          "original_form": "회색은 어때",
          "speaker_id": "1",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100034383.1.14",
          "form": "회색 도예쁠 것 같아",
          "original_form": "회색 도예쁠 것 같아",
          "speaker_id": "2",
          "time": "20210828 02:33"
        },
        {
          "id": "MDRW2100034383.1.15",
          "form": "근데 막상 밝은 게 끌리기도 하네",
          "original_form": "근데 막상 밝은 게 끌리기도 하네",
          "speaker_id": "1",
          "time": "20210828 02:34"
        },
        {
          "id": "MDRW2100034383.1.16",
          "form": "또 추천해 갈색",
          "original_form": "또 추천해 갈색",
          "speaker_id": "2",
          "time": "20210828 02:34"
        },
        {
          "id": "MDRW2100034383.1.17",
          "form": "그럼 너 말대로 갈색 사야겠다",
          "original_form": "그럼 너 말대로 갈색 사야겠다",
