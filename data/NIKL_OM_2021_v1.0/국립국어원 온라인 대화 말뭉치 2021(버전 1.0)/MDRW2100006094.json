      "utterance": [
        {
          "id": "MDRW2100006094.1.1",
          "form": "푸라닭 자주 드시는군여?",
          "original_form": "푸라닭 자주 드시는군여?",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100006094.1.2",
          "form": "요즘 치킨브랜드가 하도 많아가지고",
          "original_form": "요즘 치킨브랜드가 하도 많아가지고",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100006094.1.3",
          "form": "선택하기도 힘들어요 ㅎㅎ",
          "original_form": "선택하기도 힘들어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:22"
        },
        {
          "id": "MDRW2100006094.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100006094.1.5",
          "form": "종류도 많아지도",
          "original_form": "종류도 많아지도",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100006094.1.6",
          "form": "지고",
          "original_form": "지고",
          "speaker_id": "2",
          "time": "20210907 20:23"
        },
        {
          "id": "MDRW2100006094.1.7",
          "form": "근데 다 맛있더라구요ㅋㅋㅋ",
          "original_form": "근데 다 맛있더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100006094.1.8",
          "form": "제가 깐부치킨 좋아하는데",
          "original_form": "제가 깐부치킨 좋아하는데",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006094.1.9",
          "form": "배달이 안돼요ㅜㅜ",
          "original_form": "배달이 안돼요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006094.1.10",
          "form": "치킨은 항상 옳아요",
          "original_form": "치킨은 항상 옳아요",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006094.1.11",
          "form": "언제나 맛있고",
          "original_form": "언제나 맛있고",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100006094.1.12",
          "form": "ㅋㅋㅋ 마자요",
          "original_form": "ㅋㅋㅋ 마자요",
          "speaker_id": "1",
          "time": "20210907 20:26"
        },
        {
          "id": "MDRW2100006094.1.13",
          "form": "특별히 좋아하는 음식 있으신가요?",
          "original_form": "특별히 좋아하는 음식 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 20:27"
        },
        {
          "id": "MDRW2100006094.1.14",
          "form": "전 한식파예요",
          "original_form": "전 한식파예요",
          "speaker_id": "2",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100006094.1.15",
          "form": "찌개나 국에 밥?",
          "original_form": "찌개나 국에 밥?",
          "speaker_id": "1",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100006094.1.16",
          "form": "이런패턴 좋아하시나요?",
          "original_form": "이런패턴 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 20:28"
        },
        {
          "id": "MDRW2100006094.1.17",
          "form": "밥을 좋아하죠~",
          "original_form": "밥을 좋아하죠~",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100006094.1.18",
          "form": "밀가루는요?",
          "original_form": "밀가루는요?",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100006094.1.19",
          "form": "면요리",
          "original_form": "면요리",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100006094.1.20",
          "form": "밀가루도 좋아해요^^",
          "original_form": "밀가루도 좋아해요^^",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100006094.1.21",
          "form": "저두요 ㅎㅎ",
          "original_form": "저두요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100006094.1.22",
          "form": "결국 다 좋아한다는 ㅋㅋㅋ",
          "original_form": "결국 다 좋아한다는 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100006094.1.23",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100006094.1.24",
          "form": "싫어하는거",
          "original_form": "싫어하는거",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100006094.1.25",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100006094.1.26",
          "form": "딱히 싫어하는게 없네요 ㅋㅋㅋ",
