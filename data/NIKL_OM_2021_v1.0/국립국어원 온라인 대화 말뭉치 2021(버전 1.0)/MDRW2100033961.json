      "utterance": [
        {
          "id": "MDRW2100033961.1.1",
          "form": "백신 맞으셨나요?",
          "original_form": "백신 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100033961.1.2",
          "form": "저는 어제 1차 맞고",
          "original_form": "저는 어제 1차 맞고",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100033961.1.3",
          "form": "내내 잠만 잤어요",
          "original_form": "내내 잠만 잤어요",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100033961.1.4",
          "form": "백신맞으셨어요?",
          "original_form": "백신맞으셨어요?",
          "speaker_id": "1",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100033961.1.5",
          "form": "화이자요?",
          "original_form": "화이자요?",
          "speaker_id": "1",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100033961.1.6",
          "form": "네 화이자요ㅜ",
          "original_form": "네 화이자요ㅜ",
          "speaker_id": "2",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100033961.1.7",
          "form": "어때요? 부작용없어요?",
          "original_form": "어때요? 부작용없어요?",
          "speaker_id": "1",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100033961.1.8",
          "form": "전 맞기 무서워서 고민중인데",
          "original_form": "전 맞기 무서워서 고민중인데",
          "speaker_id": "1",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100033961.1.9",
          "form": "1차라서 그런지 하나도 안 아파요",
          "original_form": "1차라서 그런지 하나도 안 아파요",
          "speaker_id": "2",
          "time": "20210827 22:18"
        },
        {
          "id": "MDRW2100033961.1.10",
          "form": "잠만 늘었고ㅋㅋㅋㅋㅋㅋ",
          "original_form": "잠만 늘었고ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:18"
        },
        {
          "id": "MDRW2100033961.1.11",
          "form": "진짜요? 며칠은 그래도 지켜봐야되는거아니예요?",
          "original_form": "진짜요? 며칠은 그래도 지켜봐야되는거아니예요?",
          "speaker_id": "1",
          "time": "20210827 22:18"
        },
        {
          "id": "MDRW2100033961.1.12",
          "form": "전 9월30일예약인데",
          "original_form": "전 9월30일예약인데",
          "speaker_id": "1",
          "time": "20210827 22:20"
        },
        {
          "id": "MDRW2100033961.1.13",
          "form": "그렇죠 계속 지켜보긴 해야 되는데",
          "original_form": "그렇죠 계속 지켜보긴 해야 되는데",
          "speaker_id": "2",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100033961.1.14",
          "form": "제 주변도 그렇고 부작용은 없는 것 같아요",
          "original_form": "제 주변도 그렇고 부작용은 없는 것 같아요",
          "speaker_id": "2",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100033961.1.15",
          "form": "2차는 좀 많이 아프대요ㅠ",
          "original_form": "2차는 좀 많이 아프대요ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100033961.1.16",
          "form": "무서운데 다들맞으시네요",
          "original_form": "무서운데 다들맞으시네요",
          "speaker_id": "1",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100033961.1.17",
          "form": "백신도 무섭긴 한데",
          "original_form": "백신도 무섭긴 한데",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100033961.1.18",
          "form": "그만큼 코로나도 무서워서",
          "original_form": "그만큼 코로나도 무서워서",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100033961.1.19",
          "form": "그리고 확률적으로 뭔가 맞는 게 나은 것 같길래 맞았어요",
          "original_form": "그리고 확률적으로 뭔가 맞는 게 나은 것 같길래 맞았어요",
          "speaker_id": "2",
          "time": "20210827 22:54"
        },
        {
          "id": "MDRW2100033961.1.20",
          "form": "잘하셨네요 저도 좀 고민해봐야겠어요",
          "original_form": "잘하셨네요 저도 좀 고민해봐야겠어요",
          "speaker_id": "1",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100033961.1.21",
          "form": "네ㅜ 어떤 분은 의심하다가 그냥 1차 맞았는데 2차 취소하고 싶다 하시더라고요 그거 보고 확실히 고민 많이 하고 결정해야겠다 느꼈어요",
          "original_form": "네ㅜ 어떤 분은 의심하다가 그냥 1차 맞았는데 2차 취소하고 싶다 하시더라고요 그거 보고 확실히 고민 많이 하고 결정해야겠다 느꼈어요",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100033961.1.22",
          "form": "그니까요 대부분은 괜찮겠죠 극히일부에서 그런거니",
          "original_form": "그니까요 대부분은 괜찮겠죠 극히일부에서 그런거니",
          "speaker_id": "1",
          "time": "20210827 23:06"
        }
      ]
