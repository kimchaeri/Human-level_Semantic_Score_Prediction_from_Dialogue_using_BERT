      "utterance": [
        {
          "id": "MDRW2100011631.1.1",
          "form": "취업상태이신가요?",
          "original_form": "취업상태이신가요?",
          "speaker_id": "1",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100011631.1.2",
          "form": "네. 일하고있어요",
          "original_form": "네. 일하고있어요",
          "speaker_id": "2",
          "time": "20210824 15:39"
        },
        {
          "id": "MDRW2100011631.1.3",
          "form": "어떤 직종인지 여쭤봐도 될까요?",
          "original_form": "어떤 직종인지 여쭤봐도 될까요?",
          "speaker_id": "1",
          "time": "20210824 15:40"
        },
        {
          "id": "MDRW2100011631.1.4",
          "form": "사무직일해요~",
          "original_form": "사무직일해요~",
          "speaker_id": "2",
          "time": "20210824 15:40"
        },
        {
          "id": "MDRW2100011631.1.5",
          "form": "저는 취업준비중이에요",
          "original_form": "저는 취업준비중이에요",
          "speaker_id": "1",
          "time": "20210824 15:41"
        },
        {
          "id": "MDRW2100011631.1.6",
          "form": "어느쪽 취업하시려고요?",
          "original_form": "어느쪽 취업하시려고요?",
          "speaker_id": "2",
          "time": "20210824 15:41"
        },
        {
          "id": "MDRW2100011631.1.7",
          "form": "저도 사무직이요ㅎㅎ",
          "original_form": "저도 사무직이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 15:41"
        },
        {
          "id": "MDRW2100011631.1.8",
          "form": "생각하신쪽이 있으신가요?",
          "original_form": "생각하신쪽이 있으신가요?",
          "speaker_id": "2",
          "time": "20210824 15:41"
        },
        {
          "id": "MDRW2100011631.1.9",
          "form": "회계나 인사쪽이요~",
          "original_form": "회계나 인사쪽이요~",
          "speaker_id": "1",
          "time": "20210824 15:42"
        },
        {
          "id": "MDRW2100011631.1.10",
          "form": "자격증도 있으신거에요??",
          "original_form": "자격증도 있으신거에요??",
          "speaker_id": "1",
          "time": "20210824 15:49"
        },
        {
          "id": "MDRW2100011631.1.11",
          "form": "저는 회계인사는 없어요ㅎㅎ",
          "original_form": "저는 회계인사는 없어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 15:49"
        },
        {
          "id": "MDRW2100011631.1.12",
          "form": "아 저도 작년에 국비수업듣고 땄어요",
          "original_form": "아 저도 작년에 국비수업듣고 땄어요",
          "speaker_id": "1",
          "time": "20210824 15:49"
        },
        {
          "id": "MDRW2100011631.1.13",
          "form": "취업난이 많이 심하죠ㅠ",
          "original_form": "취업난이 많이 심하죠ㅠ",
          "speaker_id": "2",
          "time": "20210824 15:50"
        },
        {
          "id": "MDRW2100011631.1.14",
          "form": "네 취업하기 힘드네요",
          "original_form": "네 취업하기 힘드네요",
          "speaker_id": "1",
          "time": "20210824 15:50"
