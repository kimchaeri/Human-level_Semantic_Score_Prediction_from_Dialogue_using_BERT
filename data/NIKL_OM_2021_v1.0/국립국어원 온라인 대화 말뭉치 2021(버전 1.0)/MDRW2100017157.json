      "utterance": [
        {
          "id": "MDRW2100017157.1.1",
          "form": "구매하고싶은 거 있나요?",
          "original_form": "구매하고싶은 거 있나요?",
          "speaker_id": "1",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017157.1.2",
          "form": "게이밍노트북이요 ㅎㅎ",
          "original_form": "게이밍노트북이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017157.1.3",
          "form": "게임 좋아하시나봐요",
          "original_form": "게임 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017157.1.4",
          "form": "저는 갤럭시워치4요",
          "original_form": "저는 갤럭시워치4요",
          "speaker_id": "1",
          "time": "20210826 18:04"
        },
        {
          "id": "MDRW2100017157.1.5",
          "form": "게임을 잘 못하는데 이것저것 해보고 싶어서...",
          "original_form": "게임을 잘 못하는데 이것저것 해보고 싶어서...",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017157.1.6",
          "form": "워치랑 z플립 너무 예쁘던데",
          "original_form": "워치랑 z플립 너무 예쁘던데",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100017157.1.7",
          "form": "맞아요 플립도 갖고 싶어요",
          "original_form": "맞아요 플립도 갖고 싶어요",
          "speaker_id": "1",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017157.1.8",
          "form": "색도 다 너무 예쁘더라구오",
          "original_form": "색도 다 너무 예쁘더라구오",
          "speaker_id": "2",
          "time": "20210826 18:58"
        },
        {
          "id": "MDRW2100017157.1.9",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017157.1.10",
          "form": "맞아요 ㅠㅠ 근데 아직 핸드폰이 너무 멀쩡하네요ㅠ",
          "original_form": "맞아요 ㅠㅠ 근데 아직 핸드폰이 너무 멀쩡하네요ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017157.1.11",
          "form": "최근에 뭐사셨나요?",
          "original_form": "최근에 뭐사셨나요?",
          "speaker_id": "1",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017157.1.12",
          "form": "전자기기는 앱코 키보드 선물 받았어요 ㅎㅎ",
          "original_form": "전자기기는 앱코 키보드 선물 받았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017157.1.13",
          "form": "사용하던 텀블러가 부서져서 새로 하나 샀어요!",
          "original_form": "사용하던 텀블러가 부서져서 새로 하나 샀어요!",
          "speaker_id": "2",
          "time": "20210826 19:09"
        },
        {
          "id": "MDRW2100017157.1.14",
          "form": "텀블러가 부서지기도 하나요??",
          "original_form": "텀블러가 부서지기도 하나요??",
          "speaker_id": "1",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017157.1.15",
          "form": "떨어뜨렸는데 뚜껑이 플라스틱이어서 그런지 ㅠㅠ 박살났어요",
          "original_form": "떨어뜨렸는데 뚜껑이 플라스틱이어서 그런지 ㅠㅠ 박살났어요",
          "speaker_id": "2",
          "time": "20210826 19:10"
        },
        {
          "id": "MDRW2100017157.1.16",
          "form": "헉 슬프네요ㅠ",
          "original_form": "헉 슬프네요ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017157.1.17",
          "form": "앱코 키보드는 어떤가요? 좋나요?",
          "original_form": "앱코 키보드는 어떤가요? 좋나요?",
          "speaker_id": "1",
          "time": "20210826 19:18"
        },
        {
          "id": "MDRW2100017157.1.18",
          "form": "그냥 게임용 같은 기계식이에요",
          "original_form": "그냥 게임용 같은 기계식이에요",
          "speaker_id": "2",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017157.1.19",
          "form": "이런 짤각이는 소리 나는거 처음이라 재밌네요 ㅎㅎ",
          "original_form": "이런 짤각이는 소리 나는거 처음이라 재밌네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017157.1.20",
          "form": "키보드 칠때마다 쾌감 있을 것 같아요 ㅋㅋ",
          "original_form": "키보드 칠때마다 쾌감 있을 것 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:28"
        }
      ]
