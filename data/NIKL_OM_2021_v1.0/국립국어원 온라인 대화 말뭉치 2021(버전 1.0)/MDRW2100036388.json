        {
          "id": "MDRW2100036388.1.1",
          "form": "9월 이번달에 새로 하는 드라마 많네",
          "original_form": "9월 이번달에 새로 하는 드라마 많네",
          "speaker_id": "2",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100036388.1.2",
          "form": "또 뭐 있어?",
          "original_form": "또 뭐 있어?",
          "speaker_id": "1",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100036388.1.3",
          "form": "인격실격이라고 4일 종편 방송",
          "original_form": "인격실격이라고 4일 종편 방송",
          "speaker_id": "2",
          "time": "20210830 22:16"
        },
        {
          "id": "MDRW2100036388.1.4",
          "form": "인격실격 아 전도연 나오는거 같은데?",
          "original_form": "인격실격 아 전도연 나오는거 같은데?",
          "speaker_id": "1",
          "time": "20210830 22:17"
        },
        {
          "id": "MDRW2100036388.1.5",
          "form": "이건 사람들이 다 기대하고 있겠다 인간실격이다 ㅋㅋ",
          "original_form": "이건 사람들이 다 기대하고 있겠다 인간실격이다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:17"
        },
        {
          "id": "MDRW2100036388.1.6",
          "form": "아ㅋㅋ  제목이 좀 이상트라",
          "original_form": "아ㅋㅋ  제목이 좀 이상트라",
          "speaker_id": "1",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100036388.1.7",
          "form": "엉 전도연이랑 류준열",
          "original_form": "엉 전도연이랑 류준열",
          "speaker_id": "2",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100036388.1.8",
          "form": "둘이 어울릴까? ㅋ",
          "original_form": "둘이 어울릴까? ㅋ",
          "speaker_id": "1",
          "time": "20210830 22:18"
        },
        {
          "id": "MDRW2100036388.1.9",
          "form": "김효진이라 박지영 박인환 거기다 양동근도 나온다",
          "original_form": "김효진이라 박지영 박인환 거기다 양동근도 나온다",
          "speaker_id": "2",
          "time": "20210830 22:19"
        },
        {
          "id": "MDRW2100036388.1.10",
          "form": "오 빵빵하네",
          "original_form": "오 빵빵하네",
          "speaker_id": "1",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100036388.1.11",
          "form": "아무것도 되지 못한 마흔의 여자 전도연이랑",
          "original_form": "아무것도 되지 못한 마흔의 여자 전도연이랑",
          "speaker_id": "2",
          "time": "20210830 22:20"
        },
        {
          "id": "MDRW2100036388.1.12",
          "form": "나랑 비슷한냐 ㅋ",
          "original_form": "나랑 비슷한냐 ㅋ",
