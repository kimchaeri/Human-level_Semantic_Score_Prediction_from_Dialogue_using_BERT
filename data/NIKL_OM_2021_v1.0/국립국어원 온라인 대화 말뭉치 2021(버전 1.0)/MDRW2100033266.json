      "utterance": [
        {
          "id": "MDRW2100033266.1.1",
          "form": "모츠나베 한국에도 팔더라고용",
          "original_form": "모츠나베 한국에도 팔더라고용",
          "speaker_id": "1",
          "time": "20210906 20:36"
        },
        {
          "id": "MDRW2100033266.1.2",
          "form": "오 그런가요",
          "original_form": "오 그런가요",
          "speaker_id": "2",
          "time": "20210906 20:58"
        },
        {
          "id": "MDRW2100033266.1.3",
          "form": "한번 찾아봐야겠어요",
          "original_form": "한번 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210906 20:58"
        },
        {
          "id": "MDRW2100033266.1.4",
          "form": "네네 !!",
          "original_form": "네네 !!",
          "speaker_id": "1",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100033266.1.5",
          "form": "내일은 떡볶이 해먹어야겠어요",
          "original_form": "내일은 떡볶이 해먹어야겠어요",
          "speaker_id": "1",
          "time": "20210906 21:16"
        },
        {
          "id": "MDRW2100033266.1.6",
          "form": "오 요리 잘하시나봐요",
          "original_form": "오 요리 잘하시나봐요",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100033266.1.7",
          "form": "맨날 하는것만 잘해요 ㅌㅋㅋ",
          "original_form": "맨날 하는것만 잘해요 ㅌㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100033266.1.8",
          "form": "찌개나 국이나 떡볶이 이런거요 ㅋㅋ",
          "original_form": "찌개나 국이나 떡볶이 이런거요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100033266.1.9",
          "form": "결혼준비 끝이신데요",
          "original_form": "결혼준비 끝이신데요",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100033266.1.10",
          "form": "요리 끝",
          "original_form": "요리 끝",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100033266.1.11",
          "form": "앜ㅋㅋㅋㅋㅋ 그쵸",
          "original_form": "앜ㅋㅋㅋㅋㅋ 그쵸",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100033266.1.12",
          "form": "남친 돌아오기만 하면 바로 하면 되는데",
          "original_form": "남친 돌아오기만 하면 바로 하면 되는데",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100033266.1.13",
          "form": "타이밍이 어렵슴다",
          "original_form": "타이밍이 어렵슴다",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100033266.1.14",
          "form": "요리 잘하는거 큰 매력이죠",
          "original_form": "요리 잘하는거 큰 매력이죠",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100033266.1.15",
          "form": "아 그런가요 다행이네요 ㅋㅋㅋ",
          "original_form": "아 그런가요 다행이네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100033266.1.16",
          "form": "네 또 뭐 잘하시나요",
          "original_form": "네 또 뭐 잘하시나요",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100033266.1.17",
          "form": "음 잔치국수 잘해요",
          "original_form": "음 잔치국수 잘해요",
          "speaker_id": "1",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100033266.1.18",
          "form": "전은 잘 못행요",
          "original_form": "전은 잘 못행요",
          "speaker_id": "1",
          "time": "20210906 22:18"
        },
        {
          "id": "MDRW2100033266.1.19",
          "form": "와우 잔치국수",
          "original_form": "와우 잔치국수",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100033266.1.20",
          "form": "잔치국수 쉽더라고요",
          "original_form": "잔치국수 쉽더라고요",
          "speaker_id": "1",
