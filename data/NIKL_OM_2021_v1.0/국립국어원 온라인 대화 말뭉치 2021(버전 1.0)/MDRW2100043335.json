      "utterance": [
        {
          "id": "MDRW2100043335.1.1",
          "form": "물회 맛있다야",
          "original_form": "물회 맛있다야",
          "speaker_id": "2",
          "time": "20210907 20:24"
        },
        {
          "id": "MDRW2100043335.1.2",
          "form": "오늘도 물회 드셨어요?",
          "original_form": "오늘도 물회 드셨어요?",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043335.1.3",
          "form": "묵밥 먹으려고 했는데 네 아빠가 물회 사왔어",
          "original_form": "묵밥 먹으려고 했는데 네 아빠가 물회 사왔어",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043335.1.4",
          "form": "맛있겠다~~ 국수도 말아드셨나",
          "original_form": "맛있겠다~~ 국수도 말아드셨나",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043335.1.5",
          "form": "말아먹었지 두번이나 리필했다",
          "original_form": "말아먹었지 두번이나 리필했다",
          "speaker_id": "2",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043335.1.6",
          "form": "ㅋㅋㅋㅋ 물회에 국수 맛있죠...",
          "original_form": "ㅋㅋㅋㅋ 물회에 국수 맛있죠...",
          "speaker_id": "1",
          "time": "20210907 20:25"
        },
        {
          "id": "MDRW2100043335.1.7",
          "form": "물회 육수 만들어 먹어도 맛있던데",
          "original_form": "물회 육수 만들어 먹어도 맛있던데",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043335.1.8",
          "form": "제가 레시피 드린거 해보셨어요? ㅎㅎㅎ",
          "original_form": "제가 레시피 드린거 해보셨어요? ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043335.1.9",
          "form": "응 사이다랑 고추장이랑 식초랑",
          "original_form": "응 사이다랑 고추장이랑 식초랑",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043335.1.10",
          "form": "근데 집 고추장 별론데 ㅠ 아마 시판 고추장으로 하면 더 맛있을거예요",
          "original_form": "근데 집 고추장 별론데 ㅠ 아마 시판 고추장으로 하면 더 맛있을거예요",
          "speaker_id": "1",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100043335.1.11",
          "form": "나중에 다 먹고 해보지 뭐",
          "original_form": "나중에 다 먹고 해보지 뭐",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043335.1.12",
          "form": "그래도 맛있게 드셔서 다행이에요 ㅎㅎ ㅁ맨날 사오시는 그건가",
          "original_form": "그래도 맛있게 드셔서 다행이에요 ㅎㅎ ㅁ맨날 사오시는 그건가",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043335.1.13",
          "form": "응 연어랑 광어랑 소라랑 문어 들어있더라",
          "original_form": "응 연어랑 광어랑 소라랑 문어 들어있더라",
          "speaker_id": "2",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100043335.1.14",
          "form": "연어 맛있는데 ㅠㅠ 필렛 사서 먹고싶네요",
          "original_form": "연어 맛있는데 ㅠㅠ 필렛 사서 먹고싶네요",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043335.1.15",
          "form": "아무래도 여기엔 별로 안들었지",
          "original_form": "아무래도 여기엔 별로 안들었지",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043335.1.16",
          "form": "그러니까 떨이로도 싸게 팔고 그러나봐요 ㅋㅋㅋ",
          "original_form": "그러니까 떨이로도 싸게 팔고 그러나봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043335.1.17",
          "form": "마트에서 회뜨는 사람들도 은근 잘하더라",
          "original_form": "마트에서 회뜨는 사람들도 은근 잘하더라",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100043335.1.18",
          "form": "그래서 회가 안남나? 마트회도 맛있긴 한거같아요 양이 적어서 그렇지",
          "original_form": "그래서 회가 안남나? 마트회도 맛있긴 한거같아요 양이 적어서 그렇지",
          "speaker_id": "1",
          "time": "20210907 20:31"
        }
      ]
