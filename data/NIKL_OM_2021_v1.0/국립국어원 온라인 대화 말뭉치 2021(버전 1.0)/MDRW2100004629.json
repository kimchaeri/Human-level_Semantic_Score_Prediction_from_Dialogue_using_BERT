      "utterance": [
        {
          "id": "MDRW2100004629.1.1",
          "form": "이번주 내내 비온다더라",
          "original_form": "이번주 내내 비온다더라",
          "speaker_id": "2",
          "time": "20210821 19:55"
        },
        {
          "id": "MDRW2100004629.1.2",
          "form": "맞아ㅠㅜ 비 계속 오더라",
          "original_form": "맞아ㅠㅜ 비 계속 오더라",
          "speaker_id": "1",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100004629.1.3",
          "form": "그래도 지금은 안 와서 다행이다",
          "original_form": "그래도 지금은 안 와서 다행이다",
          "speaker_id": "1",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100004629.1.4",
          "form": "거기 지금 안 와?",
          "original_form": "거기 지금 안 와?",
          "speaker_id": "2",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100004629.1.5",
          "form": "여기는 오던데",
          "original_form": "여기는 오던데",
          "speaker_id": "2",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100004629.1.6",
          "form": "여기는 조용해!",
          "original_form": "여기는 조용해!",
          "speaker_id": "1",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100004629.1.7",
          "form": "그렇군",
          "original_form": "그렇군",
          "speaker_id": "2",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100004629.1.8",
          "form": "같은 지역인데 다르네",
          "original_form": "같은 지역인데 다르네",
          "speaker_id": "2",
          "time": "20210821 19:57"
        },
        {
          "id": "MDRW2100004629.1.9",
          "form": "그렇네",
          "original_form": "그렇네",
          "speaker_id": "1",
          "time": "20210821 20:00"
        },
        {
          "id": "MDRW2100004629.1.10",
          "form": "낮에는 하늘에 구멍 뚫린것 마냥 내리던데",
          "original_form": "낮에는 하늘에 구멍 뚫린것 마냥 내리던데",
          "speaker_id": "1",
          "time": "20210821 20:00"
        },
        {
          "id": "MDRW2100004629.1.11",
          "form": "지금은 조용하다",
          "original_form": "지금은 조용하다",
          "speaker_id": "1",
          "time": "20210821 20:00"
        },
        {
          "id": "MDRW2100004629.1.12",
          "form": "ㅜㅠ 창문 열어놨는데 쌀쌀하다",
          "original_form": "ㅜㅠ 창문 열어놨는데 쌀쌀하다",
          "speaker_id": "2",
          "time": "20210821 20:00"
        },
        {
          "id": "MDRW2100004629.1.13",
          "form": "맞아!",
          "original_form": "맞아!",
          "speaker_id": "1",
          "time": "20210821 20:00"
        },
        {
          "id": "MDRW2100004629.1.14",
          "form": "일교차가 심한듯",
          "original_form": "일교차가 심한듯",
          "speaker_id": "2",
          "time": "20210821 20:00"
        },
        {
          "id": "MDRW2100004629.1.15",
          "form": "그래도 안 더워서 다행",
          "original_form": "그래도 안 더워서 다행",
          "speaker_id": "2",
          "time": "20210821 20:00"
        },
        {
          "id": "MDRW2100004629.1.16",
          "form": "완전 가을날씨 됐어ㅠㅠ",
          "original_form": "완전 가을날씨 됐어ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100004629.1.17",
          "form": "진짜.. 8월 중순인데 벌써 가을..",
          "original_form": "진짜.. 8월 중순인데 벌써 가을..",
          "speaker_id": "2",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100004629.1.18",
          "form": "가을, 겨울 좋다",
          "original_form": "가을, 겨울 좋다",
          "speaker_id": "1",
          "time": "20210821 20:01"
        },
        {
          "id": "MDRW2100004629.1.19",
          "form": "ㅜㅜ 난 여름이 좋은데",
          "original_form": "ㅜㅜ 난 여름이 좋은데",
          "speaker_id": "2",
          "time": "20210821 20:01"
        }
