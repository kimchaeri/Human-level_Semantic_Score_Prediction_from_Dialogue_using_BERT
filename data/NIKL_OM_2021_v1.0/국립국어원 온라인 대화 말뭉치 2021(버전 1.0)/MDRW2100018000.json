      "utterance": [
        {
          "id": "MDRW2100018000.1.1",
          "form": "서울 한 번 올라와",
          "original_form": "서울 한 번 올라와",
          "speaker_id": "1",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100018000.1.2",
          "form": "맛난거 먹자",
          "original_form": "맛난거 먹자",
          "speaker_id": "1",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100018000.1.3",
          "form": "구내식당 4천원인데 4천원 내고 먹기엔 학교 급식보다 퀄이 낮이서 늘 고민인\r\n헉 넘 좋다",
          "original_form": "구내식당 4천원인데 4천원 내고 먹기엔 학교 급식보다 퀄이 낮이서 늘 고민인\r\n헉 넘 좋다",
          "speaker_id": "2",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100018000.1.4",
          "form": "맛난거 먹부림",
          "original_form": "맛난거 먹부림",
          "speaker_id": "2",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100018000.1.5",
          "form": "이번에 강남에 마라탕집 찾았는데",
          "original_form": "이번에 강남에 마라탕집 찾았는데",
          "speaker_id": "1",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100018000.1.6",
          "form": "마라샹궈가 생각보다 맛있었어",
          "original_form": "마라샹궈가 생각보다 맛있었어",
          "speaker_id": "1",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100018000.1.7",
          "form": "같이 간 친구는 계란볶음밥 엄청 좋아했음",
          "original_form": "같이 간 친구는 계란볶음밥 엄청 좋아했음",
          "speaker_id": "1",
          "time": "20210826 22:06"
        },
        {
          "id": "MDRW2100018000.1.8",
          "form": "오오오 조아조아",
          "original_form": "오오오 조아조아",
          "speaker_id": "2",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100018000.1.9",
          "form": "마라샹궈 먹어본적 없어서 더 궁금하다",
          "original_form": "마라샹궈 먹어본적 없어서 더 궁금하다",
          "speaker_id": "2",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100018000.1.10",
          "form": "밥이랑 비벼먹어...",
          "original_form": "밥이랑 비벼먹어...",
          "speaker_id": "2",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100018000.1.11",
          "form": "아냐 따로먹어 ㅋㅋㅋㅋㅋ",
          "original_form": "아냐 따로먹어 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100018000.1.12",
          "form": "샹궈 집어먹다가",
          "original_form": "샹궈 집어먹다가",
          "speaker_id": "1",
          "time": "20210826 22:08"
        },
        {
          "id": "MDRW2100018000.1.13",
          "form": "좀 짜면 밥으로 달래는 느낌이야",
          "original_form": "좀 짜면 밥으로 달래는 느낌이야",
          "speaker_id": "1",
          "time": "20210826 22:08"
        },
        {
          "id": "MDRW2100018000.1.14",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210826 22:08"
        },
        {
          "id": "MDRW2100018000.1.15",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "1",
          "time": "20210826 22:08"
        },
        {
          "id": "MDRW2100018000.1.16",
          "form": "스테이크 맛집도 찾앗는대",
          "original_form": "스테이크 맛집도 찾앗는대",
          "speaker_id": "1",
          "time": "20210826 22:08"
        },
        {
          "id": "MDRW2100018000.1.17",
          "form": "여긴 가격대가 좀 있어서",
          "original_form": "여긴 가격대가 좀 있어서",
          "speaker_id": "1",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100018000.1.18",
          "form": "데리가 취업하면 가는걸로...",
          "original_form": "데리가 취업하면 가는걸로...",
          "speaker_id": "1",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100018000.1.19",
          "form": "앗......",
          "original_form": "앗......",
          "speaker_id": "2",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100018000.1.20",
          "form": "주말..함 힘내볼까...11월 16일전까진..난..인턴..",
          "original_form": "주말..함 힘내볼까...11월 16일전까진..난..인턴..",
          "speaker_id": "2",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100018000.1.21",
          "form": "아 그러네 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 그러네 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100018000.1.22",
          "form": "어차피 나도 회사다녀서 평일엔 거의 못감...",
          "original_form": "어차피 나도 회사다녀서 평일엔 거의 못감...",
          "speaker_id": "1",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100018000.1.23",
          "form": "함 올라와서 서울 맛집투어 하자",
          "original_form": "함 올라와서 서울 맛집투어 하자",
          "speaker_id": "1",
          "time": "20210826 22:10"
        },
        {
          "id": "MDRW2100018000.1.24",
          "form": "ㅋㅋㅋ큐ㅜㅜ 조아조아.. 백신 좀 맞고 주말에 힘내서 맛집 투어 해버리자 너무 어디든 가고 싶어 맛난것도 먹고.. 디저트 가게도 가고..",
          "original_form": "ㅋㅋㅋ큐ㅜㅜ 조아조아.. 백신 좀 맞고 주말에 힘내서 맛집 투어 해버리자 너무 어디든 가고 싶어 맛난것도 먹고.. 디저트 가게도 가고..",
          "speaker_id": "2",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100018000.1.25",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100018000.1.26",
          "form": "월급 들어온다음에 가자",
          "original_form": "월급 들어온다음에 가자",
          "speaker_id": "1",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100018000.1.27",
          "form": "디저트 맛집도 찾아놧어",
          "original_form": "디저트 맛집도 찾아놧어",
          "speaker_id": "1",
          "time": "20210826 22:12"
        },
        {
          "id": "MDRW2100018000.1.28",
          "form": "헉 조아조아 월급날13일.. 추석 전에 후다닥 갔다오고 싶다ㅠ",
          "original_form": "헉 조아조아 월급날13일.. 추석 전에 후다닥 갔다오고 싶다ㅠ",
          "speaker_id": "2",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100018000.1.29",
          "form": "앗 13일이면 애매하네",
          "original_form": "앗 13일이면 애매하네",
          "speaker_id": "1",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100018000.1.30",
          "form": "맛집들 추석연휴를 주말부터 쉬는 곳이 많아서...",
          "original_form": "맛집들 추석연휴를 주말부터 쉬는 곳이 많아서...",
          "speaker_id": "1",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100018000.1.31",
          "form": "ㅜㅜ 그렇구나... 추석 이후로 가야 하나? ㅠ 상관은 없지만 즐거움을 미뤄둬야하는 슬픔 맛집 탐방...",
          "original_form": "ㅜㅜ 그렇구나... 추석 이후로 가야 하나? ㅠ 상관은 없지만 즐거움을 미뤄둬야하는 슬픔 맛집 탐방...",
