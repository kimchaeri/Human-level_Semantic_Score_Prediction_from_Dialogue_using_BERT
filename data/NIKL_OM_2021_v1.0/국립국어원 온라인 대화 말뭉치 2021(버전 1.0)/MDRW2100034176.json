      "utterance": [
        {
          "id": "MDRW2100034176.1.1",
          "form": "운동 하는거 있으세요?",
          "original_form": "운동 하는거 있으세요?",
          "speaker_id": "1",
          "time": "20210827 20:12"
        },
        {
          "id": "MDRW2100034176.1.2",
          "form": "아뇨 ㅠ 안하는데",
          "original_form": "아뇨 ㅠ 안하는데",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100034176.1.3",
          "form": "해야할 거 같아요",
          "original_form": "해야할 거 같아요",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100034176.1.4",
          "form": "살 많이 쪘어요..",
          "original_form": "살 많이 쪘어요..",
          "speaker_id": "2",
          "time": "20210827 20:13"
        },
        {
          "id": "MDRW2100034176.1.5",
          "form": "저는 오늘 필라테스 30회 끊고 왔습니다...ㅎㅎ",
          "original_form": "저는 오늘 필라테스 30회 끊고 왔습니다...ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100034176.1.6",
          "form": "돈아까워서 할거같아요",
          "original_form": "돈아까워서 할거같아요",
          "speaker_id": "1",
          "time": "20210827 21:44"
        },
        {
          "id": "MDRW2100034176.1.7",
          "form": "헐 ㅠ ㅠㅠㅠ 얼마줬나요",
          "original_form": "헐 ㅠ ㅠㅠㅠ 얼마줬나요",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034176.1.8",
          "form": "저 필테 고민인데",
          "original_form": "저 필테 고민인데",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034176.1.9",
          "form": "가격이 진심....",
          "original_form": "가격이 진심....",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034176.1.10",
          "form": "전에 해봤는데도 아깝더라구오",
          "original_form": "전에 해봤는데도 아깝더라구오",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100034176.1.11",
          "form": "저 회당 만오천원에 끊엇어요",
          "original_form": "저 회당 만오천원에 끊엇어요",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034176.1.12",
          "form": "만원대에 끊지않으면 아깝대요",
          "original_form": "만원대에 끊지않으면 아깝대요",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100034176.1.13",
          "form": "오.. 대박.. 괜찮은 가격대 알려줘서 고마워요",
          "original_form": "오.. 대박.. 괜찮은 가격대 알려줘서 고마워요",
          "speaker_id": "2",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100034176.1.14",
          "form": "운동을 해야하는데 복합적인 감정이 ㅠ",
          "original_form": "운동을 해야하는데 복합적인 감정이 ㅠ",
          "speaker_id": "2",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100034176.1.15",
          "form": "그쵸 돈이 너무많이들어요",
          "original_form": "그쵸 돈이 너무많이들어요",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100034176.1.16",
          "form": "저도 오늘 4개월 30회 끊었는데 45만원 냈어요",
          "original_form": "저도 오늘 4개월 30회 끊었는데 45만원 냈어요",
          "speaker_id": "1",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100034176.1.17",
          "form": "네 ㅠㅠ 4개월 불태워보세오ㅋㅋㅋㅋㅋ",
          "original_form": "네 ㅠㅠ 4개월 불태워보세오ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100034176.1.18",
          "form": "ㅋㅋㅋㅋ 돈 진짜 아까워서 할거같아요 근데",
          "original_form": "ㅋㅋㅋㅋ 돈 진짜 아까워서 할거같아요 근데",
          "speaker_id": "1",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100034176.1.19",
          "form": "월 11만원이라 생각하면 그리 나쁘진 않더라구요",
          "original_form": "월 11만원이라 생각하면 그리 나쁘진 않더라구요",
          "speaker_id": "1",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100034176.1.20",
          "form": "맞아요 그 정도면",
          "original_form": "맞아요 그 정도면",
          "speaker_id": "2",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100034176.1.21",
          "form": "나한테 투자할 가치가 있죠ㅋㅋ",
          "original_form": "나한테 투자할 가치가 있죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100034176.1.22",
          "form": "그쵸 ㅎㅎㅎ 한번 회당 만오천을 찾아보심이..",
          "original_form": "그쵸 ㅎㅎㅎ 한번 회당 만오천을 찾아보심이..",
          "speaker_id": "1",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100034176.1.23",
          "form": "그리고 필라테스는 가격이 싯가더라구요 ㅋㅋㅋㅋ",
          "original_form": "그리고 필라테스는 가격이 싯가더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100034176.1.24",
          "form": "찾아봐야겠어요 ㅋㅋ",
          "original_form": "찾아봐야겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100034176.1.25",
          "form": "기구인가요?",
          "original_form": "기구인가요?",
          "speaker_id": "2",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100034176.1.26",
          "form": "저는 기구에요!",
          "original_form": "저는 기구에요!",
          "speaker_id": "1",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100034176.1.27",
          "form": "가서 가격 물어봐야 하더라고요..",
          "original_form": "가서 가격 물어봐야 하더라고요..",
          "speaker_id": "1",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100034176.1.28",
          "form": "카톡으로는 가격 아예 안 알려주는 곳도 많고..",
          "original_form": "카톡으로는 가격 아예 안 알려주는 곳도 많고..",
          "speaker_id": "1",
          "time": "20210827 22:07"
        },
        {
          "id": "MDRW2100034176.1.29",
          "form": "필라테스 가격 정가제 입법하고싶어요",
          "original_form": "필라테스 가격 정가제 입법하고싶어요",
          "speaker_id": "1",
          "time": "20210827 22:08"
        },
        {
          "id": "MDRW2100034176.1.30",
          "form": "ㅋㅋ 인정요 ~",
          "original_form": "ㅋㅋ 인정요 ~",
          "speaker_id": "2",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100034176.1.31",
          "form": "가격 안알려주려고 하는 거",
          "original_form": "가격 안알려주려고 하는 거",
          "speaker_id": "2",
          "time": "20210827 22:09"
        },
        {
          "id": "MDRW2100034176.1.32",
          "form": "너무해요",
          "original_form": "너무해요",
          "speaker_id": "2",
          "time": "20210827 22:09"
        }
      ]
    }
