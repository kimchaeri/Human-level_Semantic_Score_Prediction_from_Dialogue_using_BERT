        },
        {
          "id": "MDRW2100025859.1.5",
          "form": "진짜 엠티인데",
          "original_form": "진짜 엠티인데",
          "speaker_id": "1",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025859.1.6",
          "form": "노래방이랑 족구못놓는 좀비같아섴ㅋㅋㄱㄱㄱ",
          "original_form": "노래방이랑 족구못놓는 좀비같아섴ㅋㅋㄱㄱㄱ",
          "speaker_id": "1",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025859.1.7",
          "form": "자다일어나노래부르곸ㅋㅋㅋ",
          "original_form": "자다일어나노래부르곸ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025859.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ근데 가수들 놀러가면 다 그러나봐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ근데 가수들 놀러가면 다 그러나봐",
          "speaker_id": "2",
          "time": "20210901 18:00"
        },
        {
          "id": "MDRW2100025859.1.9",
          "form": "127 놀러가서 12시간 넘게 노래불렀대",
          "original_form": "127 놀러가서 12시간 넘게 노래불렀대",
          "speaker_id": "2",
          "time": "20210901 18:00"
        },
        {
          "id": "MDRW2100025859.1.10",
          "form": "우리가 계속 뭔가를 먹는것과 같은 이치인가..",
          "original_form": "우리가 계속 뭔가를 먹는것과 같은 이치인가..",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025859.1.11",
          "form": "ㅋㅋㅋㅋㅋㄱ와",
          "original_form": "ㅋㅋㅋㅋㅋㄱ와",
          "speaker_id": "1",
          "time": "20210901 18:08"
        },
        {
          "id": "MDRW2100025859.1.12",
          "form": "대단ㅋㅋㅋㅋㄱㄱㅋ",
          "original_form": "대단ㅋㅋㅋㅋㄱㄱㅋ",
          "speaker_id": "1",
          "time": "20210901 18:08"
        },
        {
          "id": "MDRW2100025859.1.13",
          "form": "12시간넘게라니",
          "original_form": "12시간넘게라니",
          "speaker_id": "1",
          "time": "20210901 18:08"
        },
        {
          "id": "MDRW2100025859.1.14",
          "form": "우리도 12시간 먹을 수 있잖아^_^...\r\n잘하는건 그럴 수 있어ㅋㅋㅋㅋㅋㅋㅋㅋ..",
          "original_form": "우리도 12시간 먹을 수 있잖아^_^...\r\n잘하는건 그럴 수 있어ㅋㅋㅋㅋㅋㅋㅋㅋ..",
          "speaker_id": "2",
          "time": "20210901 18:09"
        },
        {
          "id": "MDRW2100025859.1.15",
          "form": "그러니깐 루카스 빨리 탈퇴해줘",
          "original_form": "그러니깐 루카스 빨리 탈퇴해줘",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100025859.1.16",
          "form": "가성비 문어다리도 용서못하지만 멤버 까는건 더 용서 못해",
          "original_form": "가성비 문어다리도 용서못하지만 멤버 까는건 더 용서 못해",
          "speaker_id": "2",
          "time": "20210901 18:10"
        },
        {
          "id": "MDRW2100025859.1.17",
          "form": "ㅋㅋㄱㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025859.1.18",
          "form": "맞아 걔는",
          "original_form": "맞아 걔는",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025859.1.19",
          "form": "인성이덜됐더라...",
          "original_form": "인성이덜됐더라...",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025859.1.20",
          "form": "아니 돌았냐고",
          "original_form": "아니 돌았냐고",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025859.1.21",
          "form": "자기멤버욕하고다니는건 무슨인성",
          "original_form": "자기멤버욕하고다니는건 무슨인성",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100025859.1.22",
          "form": "인성 터지고 자존감 낮고 돈도 안쓰고.... 완전체야",
          "original_form": "인성 터지고 자존감 낮고 돈도 안쓰고.... 완전체야",
          "speaker_id": "2",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100025859.1.23",
          "form": "홍콩으로 돌아가렴 루카스야^_ㅜ",
          "original_form": "홍콩으로 돌아가렴 루카스야^_ㅜ",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025859.1.24",
          "form": "아 그친구는",
          "original_form": "아 그친구는",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025859.1.25",
          "form": "홍콩출신...?",
          "original_form": "홍콩출신...?",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025859.1.26",
          "form": "탈퇴해주라",
          "original_form": "탈퇴해주라",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025859.1.27",
          "form": "어디출신이뭐가중요하오리까",
          "original_form": "어디출신이뭐가중요하오리까",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025859.1.28",
          "form": "행실이중요하지",
          "original_form": "행실이중요하지",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025859.1.29",
          "form": "홍콩출신인데 하는 짓은 아주... 음오아예",
          "original_form": "홍콩출신인데 하는 짓은 아주... 음오아예",
          "speaker_id": "2",
          "time": "20210901 18:39"
        },
        {
          "id": "MDRW2100025859.1.30",
          "form": "아 ㄹㅇ 음오아예",
          "original_form": "아 ㄹㅇ 음오아예",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025859.1.31",
          "form": "약간 잭슨같네",
          "original_form": "약간 잭슨같네",
          "speaker_id": "1",
          "time": "20210901 18:40"
        },
        {
          "id": "MDRW2100025859.1.32",
          "form": "아 걔도 홍콩 출신이지만 웨이보에서 그랬지",
