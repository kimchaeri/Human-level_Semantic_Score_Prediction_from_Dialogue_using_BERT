      "utterance": [
        {
          "id": "MDRW2100036997.1.1",
          "form": "네덜란드라거하셨죠?",
          "original_form": "네덜란드라거하셨죠?",
          "speaker_id": "1",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100036997.1.2",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100036997.1.3",
          "form": "아죄송....흑흑 넘헷갈리네요",
          "original_form": "아죄송....흑흑 넘헷갈리네요",
          "speaker_id": "1",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100036997.1.4",
          "form": "여행좋아하시나요",
          "original_form": "여행좋아하시나요",
          "speaker_id": "1",
          "time": "20210831 16:37"
        },
        {
          "id": "MDRW2100036997.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100036997.1.6",
          "form": "기억에 남는 여행지 있으신가요",
          "original_form": "기억에 남는 여행지 있으신가요",
          "speaker_id": "2",
          "time": "20210831 16:41"
        },
        {
          "id": "MDRW2100036997.1.7",
          "form": "전 볼리비아 우유니의 소금사막이요ㅜ",
          "original_form": "전 볼리비아 우유니의 소금사막이요ㅜ",
          "speaker_id": "1",
          "time": "20210831 16:42"
        },
        {
          "id": "MDRW2100036997.1.8",
          "form": "직접 가셨나요??",
          "original_form": "직접 가셨나요??",
          "speaker_id": "2",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100036997.1.9",
          "form": "네 직접갔어요 갔다온지 좀됐는데도 가끔 꿈에서도 생각나뇨",
          "original_form": "네 직접갔어요 갔다온지 좀됐는데도 가끔 꿈에서도 생각나뇨",
          "speaker_id": "1",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100036997.1.10",
          "form": "힘들지 않으셨어요?",
          "original_form": "힘들지 않으셨어요?",
          "speaker_id": "2",
          "time": "20210831 17:00"
        },
        {
          "id": "MDRW2100036997.1.11",
          "form": "완전 완전 고행길이었어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "완전 완전 고행길이었어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:01"
        },
        {
          "id": "MDRW2100036997.1.12",
          "form": "일단 가는길자체가 와 버스를타고갔는데",
          "original_form": "일단 가는길자체가 와 버스를타고갔는데",
          "speaker_id": "1",
          "time": "20210831 17:02"
        },
        {
          "id": "MDRW2100036997.1.13",
          "form": "막 20시간이래요",
          "original_form": "막 20시간이래요",
          "speaker_id": "1",
          "time": "20210831 17:02"
        },
        {
          "id": "MDRW2100036997.1.14",
          "form": "중간에환승도하구여 또 13시간이런거타고",
          "original_form": "중간에환승도하구여 또 13시간이런거타고",
          "speaker_id": "1",
          "time": "20210831 17:02"
        },
        {
          "id": "MDRW2100036997.1.15",
          "form": "우리나라에서요?",
          "original_form": "우리나라에서요?",
          "speaker_id": "2",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100036997.1.16",
          "form": "아뇨아뇨 ㅎㅎ",
          "original_form": "아뇨아뇨 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100036997.1.17",
          "form": "아니면 볼리비아 안에서 이동시간이요?",
          "original_form": "아니면 볼리비아 안에서 이동시간이요?",
          "speaker_id": "2",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100036997.1.18",
          "form": "제가 페루에서부터 볼리비아까지 버스타고이동했었거든여",
          "original_form": "제가 페루에서부터 볼리비아까지 버스타고이동했었거든여",
          "speaker_id": "1",
          "time": "20210831 17:03"
        },
        {
          "id": "MDRW2100036997.1.19",
          "form": "이동시간이 더길었어요 ㅎㅎ..",
          "original_form": "이동시간이 더길었어요 ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100036997.1.20",
          "form": "와 진짜 오래걸리셨겠네요",
          "original_form": "와 진짜 오래걸리셨겠네요",
          "speaker_id": "2",
          "time": "20210831 17:04"
        },
        {
          "id": "MDRW2100036997.1.21",
          "form": "네 버스강도소식도 중간중간 들으면서 긴장과함께 고행길이었어요 ㅋㅋㅋ",
          "original_form": "네 버스강도소식도 중간중간 들으면서 긴장과함께 고행길이었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:05"
        }
      ]
