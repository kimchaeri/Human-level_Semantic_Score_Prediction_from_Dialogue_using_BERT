      "utterance": [
        {
          "id": "MDRW2100003997.1.1",
          "form": "연애와 결혼에 대해 관심이 있으신가요?",
          "original_form": "연애와 결혼에 대해 관심이 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003997.1.2",
          "form": "아니요 저는 아직 없어요ㅋㅋ",
          "original_form": "아니요 저는 아직 없어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003997.1.3",
          "form": "연애나 결혼 생각 있으신가요?",
          "original_form": "연애나 결혼 생각 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 14:31"
        },
        {
          "id": "MDRW2100003997.1.4",
          "form": "ㅋㅋㅋㅋ아니요 저도 없어요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ아니요 저도 없어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003997.1.5",
          "form": "일단 돈부터 모아야 뭘 할 것 같아요ㅋㅋ",
          "original_form": "일단 돈부터 모아야 뭘 할 것 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003997.1.6",
          "form": "이상형은 있으신가요?",
          "original_form": "이상형은 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100003997.1.7",
          "form": "제약이 많네요",
          "original_form": "제약이 많네요",
          "speaker_id": "2",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100003997.1.8",
          "form": "오로지 결혼과 연애라니...",
          "original_form": "오로지 결혼과 연애라니...",
          "speaker_id": "2",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100003997.1.9",
          "form": "아하 그렇군요ㅋㅋㅋㅋ",
          "original_form": "아하 그렇군요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100003997.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 주변에 연애와 결혼을 하는 지인들이 있으신가요?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 주변에 연애와 결혼을 하는 지인들이 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100003997.1.11",
          "form": "아니요ㅋㅋㅋ 다 그런 쪽에는 관심이 없어서..",
          "original_form": "아니요ㅋㅋㅋ 다 그런 쪽에는 관심이 없어서..",
          "speaker_id": "1",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100003997.1.12",
          "form": "결혼 꼭 해야한다면 엄청 나중에 하고 싶어요",
          "original_form": "결혼 꼭 해야한다면 엄청 나중에 하고 싶어요",
          "speaker_id": "1",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100003997.1.13",
          "form": "한 40,50대?",
          "original_form": "한 40,50대?",
          "speaker_id": "1",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100003997.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 좋은 생각이에요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 좋은 생각이에요!",
          "speaker_id": "2",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100003997.1.15",
          "form": "제 친구도 사주를 봤는데 결혼을 늦게해야 좋다고 나왔대요",
          "original_form": "제 친구도 사주를 봤는데 결혼을 늦게해야 좋다고 나왔대요",
          "speaker_id": "2",
          "time": "20210821 14:37"
        },
        {
          "id": "MDRW2100003997.1.16",
          "form": "ㅋㅋㅋㅋㅋ 요즘은 늦게 결혼하는 게 트랜드인 느낌?",
          "original_form": "ㅋㅋㅋㅋㅋ 요즘은 늦게 결혼하는 게 트랜드인 느낌?",
          "speaker_id": "1",
          "time": "20210821 14:41"
        },
        {
          "id": "MDRW2100003997.1.17",
          "form": "맞아요 아무래도 집값도 비싸고 ... ㅠ.ㅠ",
          "original_form": "맞아요 아무래도 집값도 비싸고 ... ㅠ.ㅠ",
          "speaker_id": "2",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100003997.1.18",
          "form": "조금 더 혼자를 즐기고싶어하는 문화도 있는 것 같아요!",
          "original_form": "조금 더 혼자를 즐기고싶어하는 문화도 있는 것 같아요!",
          "speaker_id": "2",
          "time": "20210821 14:43"
        },
        {
          "id": "MDRW2100003997.1.19",
          "form": "맞아요 저도 어린 나이에 육아 이런 걸로 신경쓰고 싶지 않아요ㅜㅜ",
          "original_form": "맞아요 저도 어린 나이에 육아 이런 걸로 신경쓰고 싶지 않아요ㅜㅜ",
