      "utterance": [
        {
          "id": "MDRW2100017305.1.1",
          "form": "반려동물 있으세요?",
          "original_form": "반려동물 있으세요?",
          "speaker_id": "2",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017305.1.2",
          "form": "네 강아지 동생있어요",
          "original_form": "네 강아지 동생있어요",
          "speaker_id": "1",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017305.1.3",
          "form": "대박 부러워요 ㅠㅠ 몇살인가요?",
          "original_form": "대박 부러워요 ㅠㅠ 몇살인가요?",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100017305.1.4",
          "form": "4살이에요 너무 이뻐요",
          "original_form": "4살이에요 너무 이뻐요",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017305.1.5",
          "form": "너무 큰 사랑과행복이라 너무 큰 슬픔이기도 해요ㅠ",
          "original_form": "너무 큰 사랑과행복이라 너무 큰 슬픔이기도 해요ㅠ",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017305.1.6",
          "form": "너무 귀엽겟다ㅠㅠ",
          "original_form": "너무 귀엽겟다ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017305.1.7",
          "form": "아프거나할때ㅠ",
          "original_form": "아프거나할때ㅠ",
          "speaker_id": "1",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017305.1.8",
          "form": "ㅠㅠ아팠어요?",
          "original_form": "ㅠㅠ아팠어요?",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017305.1.9",
          "form": "아직 네살인데ㅠㅠ",
          "original_form": "아직 네살인데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 18:39"
        },
        {
          "id": "MDRW2100017305.1.10",
          "form": "토할때나 그럴때 너무 걱정되어서요",
          "original_form": "토할때나 그럴때 너무 걱정되어서요",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017305.1.11",
          "form": "자그마한 기침 할때도 말을 못하니 너무 걱정되어요",
          "original_form": "자그마한 기침 할때도 말을 못하니 너무 걱정되어요",
          "speaker_id": "1",
          "time": "20210826 18:40"
        },
        {
          "id": "MDRW2100017305.1.12",
          "form": "ㅠㅠ맞아요 말을 할수잇다면 어디가 어떻게 얼마나 아픈지 알수잇을텐데",
          "original_form": "ㅠㅠ맞아요 말을 할수잇다면 어디가 어떻게 얼마나 아픈지 알수잇을텐데",
          "speaker_id": "2",
          "time": "20210826 18:42"
        },
        {
          "id": "MDRW2100017305.1.13",
          "form": "맞아요그게젤 힘들죠",
          "original_form": "맞아요그게젤 힘들죠",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017305.1.14",
          "form": "하지만 매일매일 행복해요 행복함을 미루지말고 즐겨야죠ㅠㅠ",
          "original_form": "하지만 매일매일 행복해요 행복함을 미루지말고 즐겨야죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 18:45"
        },
        {
          "id": "MDRW2100017305.1.15",
          "form": "맞아요 실컷 사랑해주기도 모자란 시간 ㅠㅠ",
          "original_form": "맞아요 실컷 사랑해주기도 모자란 시간 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 18:49"
        },
        {
          "id": "MDRW2100017305.1.16",
          "form": "저는 정말 제가 준비가 되엇을때 반려동물을 맞이하려고 생각중이에요",
          "original_form": "저는 정말 제가 준비가 되엇을때 반려동물을 맞이하려고 생각중이에요",
          "speaker_id": "2",
          "time": "20210826 18:50"
        },
        {
          "id": "MDRW2100017305.1.17",
          "form": "맞습니다 실컷 사랑해쥬ㅓ여죠",
          "original_form": "맞습니다 실컷 사랑해쥬ㅓ여죠",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017305.1.18",
          "form": "어머니가 갑자기 어느날 데리고오셔서",
          "original_form": "어머니가 갑자기 어느날 데리고오셔서",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017305.1.19",
          "form": "삶 자체가 바뀌었어요",
          "original_form": "삶 자체가 바뀌었어요",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017305.1.20",
          "form": "온가족의 일상이 바뀌엇을것같아요",
          "original_form": "온가족의 일상이 바뀌엇을것같아요",
          "speaker_id": "2",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100017305.1.21",
          "form": "보통 거의 대화주제가 반려동물이 되더라구요 ㅋㅋㅋㅋ",
          "original_form": "보통 거의 대화주제가 반려동물이 되더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:56"
        },
        {
          "id": "MDRW2100017305.1.22",
          "form": "맞아요 ㅋㅋ 저희집은 제가 너무 유난이라 다들 삐졌어요",
          "original_form": "맞아요 ㅋㅋ 저희집은 제가 너무 유난이라 다들 삐졌어요",
          "speaker_id": "1",
          "time": "20210826 19:12"
        },
        {
          "id": "MDRW2100017305.1.23",
          "form": "사람보다 강아지먼저라구 ㅋㅋ",
          "original_form": "사람보다 강아지먼저라구 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:12"
        }
      ]
    }
