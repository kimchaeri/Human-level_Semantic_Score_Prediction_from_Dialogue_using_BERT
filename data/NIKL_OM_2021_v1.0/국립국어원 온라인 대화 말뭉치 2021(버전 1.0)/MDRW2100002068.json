      "utterance": [
        {
          "id": "MDRW2100002068.1.1",
          "form": "오늘 아침엔 뭐 먹을까?",
          "original_form": "오늘 아침엔 뭐 먹을까?",
          "speaker_id": "2",
          "time": "20210820 09:14"
        },
        {
          "id": "MDRW2100002068.1.2",
          "form": "계란후라이랑 커피",
          "original_form": "계란후라이랑 커피",
          "speaker_id": "1",
          "time": "20210820 09:15"
        },
        {
          "id": "MDRW2100002068.1.3",
          "form": "오키 그걸로 준비할께",
          "original_form": "오키 그걸로 준비할께",
          "speaker_id": "2",
          "time": "20210820 09:15"
        },
        {
          "id": "MDRW2100002068.1.4",
          "form": "커피는 따뜻하게 주시면 감사 ㅎㅎ",
          "original_form": "커피는 따뜻하게 주시면 감사 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 09:15"
        },
        {
          "id": "MDRW2100002068.1.5",
          "form": "넹 어차피 얼음도 없어 ㅎㅎ",
          "original_form": "넹 어차피 얼음도 없어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 09:15"
        },
        {
          "id": "MDRW2100002068.1.6",
          "form": "날씨가 쌀쌀해져서 따뜻한 메뉴가 땡기네",
          "original_form": "날씨가 쌀쌀해져서 따뜻한 메뉴가 땡기네",
          "speaker_id": "1",
          "time": "20210820 09:16"
        },
        {
          "id": "MDRW2100002068.1.7",
          "form": "나도 그래 ㅎㅎ 아침은 따뜻하게 먹자",
          "original_form": "나도 그래 ㅎㅎ 아침은 따뜻하게 먹자",
          "speaker_id": "2",
          "time": "20210820 09:16"
        },
        {
          "id": "MDRW2100002068.1.8",
          "form": "아참 복숭아도 있던데 같이 먹자",
          "original_form": "아참 복숭아도 있던데 같이 먹자",
          "speaker_id": "1",
          "time": "20210820 09:16"
        },
        {
          "id": "MDRW2100002068.1.9",
          "form": "그럴까? 복숭아도 준비할께",
          "original_form": "그럴까? 복숭아도 준비할께",
          "speaker_id": "2",
          "time": "20210820 09:16"
        },
        {
          "id": "MDRW2100002068.1.10",
          "form": "고마워",
          "original_form": "고마워",
          "speaker_id": "1",
          "time": "20210820 09:16"
        },
        {
          "id": "MDRW2100002068.1.11",
          "form": "뭘유 잘 챙겨먹어야 건강하지 ㅎㅎ",
          "original_form": "뭘유 잘 챙겨먹어야 건강하지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002068.1.12",
          "form": "울 색시가 최고야",
          "original_form": "울 색시가 최고야",
          "speaker_id": "1",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002068.1.13",
          "form": "아침 맛있겠다~ 얼른 준비해놓을께",
          "original_form": "아침 맛있겠다~ 얼른 준비해놓을께",
          "speaker_id": "2",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002068.1.14",
          "form": "ㅇㅇ 내가 맛있게 촵촵 먹을께 ㅎㅎ",
          "original_form": "ㅇㅇ 내가 맛있게 촵촵 먹을께 ㅎㅎ",
