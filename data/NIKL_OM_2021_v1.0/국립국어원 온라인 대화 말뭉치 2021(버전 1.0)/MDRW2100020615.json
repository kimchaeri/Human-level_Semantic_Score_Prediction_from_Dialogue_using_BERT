      "utterance": [
        {
          "id": "MDRW2100020615.1.1",
          "form": "이건 닥후 죠",
          "original_form": "이건 닥후 죠",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020615.1.2",
          "form": "예?",
          "original_form": "예?",
          "speaker_id": "1",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020615.1.3",
          "form": "슈붕은 전자였는데요",
          "original_form": "슈붕은 전자였는데요",
          "speaker_id": "1",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020615.1.4",
          "form": "닥슈.인데요",
          "original_form": "닥슈.인데요",
          "speaker_id": "1",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020615.1.5",
          "form": "하 진짜 뭘 모르시네",
          "original_form": "하 진짜 뭘 모르시네",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020615.1.6",
          "form": "누가 슈붕 먹어요 요즘시대에",
          "original_form": "누가 슈붕 먹어요 요즘시대에",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020615.1.7",
          "form": "팥붕이 원존거 모르세옷 ?",
          "original_form": "팥붕이 원존거 모르세옷 ?",
          "speaker_id": "2",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020615.1.8",
          "form": "슈붕은 트렌드. 입니다",
          "original_form": "슈붕은 트렌드. 입니다",
          "speaker_id": "1",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020615.1.9",
          "form": "헐. 아님",
          "original_form": "헐. 아님",
          "speaker_id": "2",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020615.1.10",
          "form": "누가 그런 트렌드를 따라가죠?",
          "original_form": "누가 그런 트렌드를 따라가죠?",
          "speaker_id": "2",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020615.1.11",
          "form": "저요",
          "original_form": "저요",
          "speaker_id": "1",
          "time": "20210828 10:29"
        },
        {
          "id": "MDRW2100020615.1.12",
          "form": "흠 . 당신은 유행의 선구자가 될 수 없군요",
          "original_form": "흠 . 당신은 유행의 선구자가 될 수 없군요",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020615.1.13",
          "form": "마라탕이 너무 먹고싶은제",
          "original_form": "마라탕이 너무 먹고싶은제",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020615.1.14",
          "form": "데",
          "original_form": "데",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020615.1.15",
          "form": "근처에 시켜먹기가 애매하네요",
          "original_form": "근처에 시켜먹기가 애매하네요",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020615.1.16",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020615.1.17",
          "form": "거리도 그렇고",
          "original_form": "거리도 그렇고",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020615.1.18",
          "form": "1인분만 시켜먹기가 애매해서요",
          "original_form": "1인분만 시켜먹기가 애매해서요",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020615.1.19",
          "form": "저랑 같이 드셔주실래요 ?",
          "original_form": "저랑 같이 드셔주실래요 ?",
          "speaker_id": "2",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020615.1.20",
          "form": "가족분들은 안 좋아하세요?",
          "original_form": "가족분들은 안 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 10:32"
        },
        {
          "id": "MDRW2100020615.1.21",
          "form": "싫어요",
          "original_form": "싫어요",
          "speaker_id": "1",
          "time": "20210828 10:33"
        },
        {
          "id": "MDRW2100020615.1.22",
          "form": "진짜 단호하시네요",
          "original_form": "진짜 단호하시네요",
          "speaker_id": "2",
          "time": "20210828 10:33"
        },
        {
          "id": "MDRW2100020615.1.23",
          "form": "좋아하시는데 시켜먹는건 안좋아하세요",
          "original_form": "좋아하시는데 시켜먹는건 안좋아하세요",
          "speaker_id": "2",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020615.1.24",
          "form": "그 맛이 안난다나 뭐라나",
          "original_form": "그 맛이 안난다나 뭐라나",
          "speaker_id": "2",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020615.1.25",
          "form": "아ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020615.1.26",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020615.1.27",
          "form": "드실거면 오늘 드시는게 나을 것 같은데",
          "original_form": "드실거면 오늘 드시는게 나을 것 같은데",
          "speaker_id": "1",
          "time": "20210828 10:34"
        },
        {
          "id": "MDRW2100020615.1.28",
          "form": "왜요 ?",
          "original_form": "왜요 ?",
          "speaker_id": "2",
          "time": "20210828 10:35"
        },
        {
          "id": "MDRW2100020615.1.29",
          "form": "그냥요",
          "original_form": "그냥요",
          "speaker_id": "1",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020615.1.30",
          "form": "내일 님에게 이동수가 보여서요",
          "original_form": "내일 님에게 이동수가 보여서요",
          "speaker_id": "1",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020615.1.31",
          "form": "오 특별한 능력이 있으신가봐요",
          "original_form": "오 특별한 능력이 있으신가봐요",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020615.1.32",
          "form": "알겠어요 오늘 먹어볼게요",
          "original_form": "알겠어요 오늘 먹어볼게요",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020615.1.33",
          "form": "근데 저 오늘 친구들이랑",
          "original_form": "근데 저 오늘 친구들이랑",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020615.1.34",
          "form": "돼지갈비찜 먹으러 갈거예요",
          "original_form": "돼지갈비찜 먹으러 갈거예요",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020615.1.35",
          "form": "맛있겠죠",
          "original_form": "맛있겠죠",
          "speaker_id": "2",
          "time": "20210828 10:36"
        },
        {
          "id": "MDRW2100020615.1.36",
          "form": "예",
          "original_form": "예",
          "speaker_id": "1",
