      "utterance": [
        {
          "id": "MDRW2100027893.1.1",
          "form": "수업 재밌니",
          "original_form": "수업 재밌니",
          "speaker_id": "1",
          "time": "20210902 15:57"
        },
        {
          "id": "MDRW2100027893.1.2",
          "form": "지금 그냥 스트리밍 해놓고 있어서",
          "original_form": "지금 그냥 스트리밍 해놓고 있어서",
          "speaker_id": "2",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100027893.1.3",
          "form": "잘 모르겠는데;",
          "original_form": "잘 모르겠는데;",
          "speaker_id": "2",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100027893.1.4",
          "form": "스트리밍",
          "original_form": "스트리밍",
          "speaker_id": "1",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100027893.1.5",
          "form": "교수님 조회수 달달하신가요",
          "original_form": "교수님 조회수 달달하신가요",
          "speaker_id": "2",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100027893.1.6",
          "form": "[교수의 일상]vlog 라이브 실황",
          "original_form": "[교수의 일상]vlog 라이브 실황",
          "speaker_id": "1",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100027893.1.7",
          "form": "이런 발언",
          "original_form": "이런 발언",
          "speaker_id": "2",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100027893.1.8",
          "form": "틀린 말은 아니잖아",
          "original_form": "틀린 말은 아니잖아",
          "speaker_id": "1",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100027893.1.9",
          "form": "근데 걍 오티라",
          "original_form": "근데 걍 오티라",
          "speaker_id": "2",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100027893.1.10",
          "form": "저번에 들엇던 교수님이면",
          "original_form": "저번에 들엇던 교수님이면",
          "speaker_id": "2",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100027893.1.11",
          "form": "맨날 햇던 말 또 하고 또 하고 또 하고",
          "original_form": "맨날 햇던 말 또 하고 또 하고 또 하고",
          "speaker_id": "2",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100027893.1.12",
          "form": "아 근데 ㄹㅇ",
          "original_form": "아 근데 ㄹㅇ",
          "speaker_id": "1",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100027893.1.13",
          "form": "저번에 들었던 말 또 하고 또 하고 또 하고",
          "original_form": "저번에 들었던 말 또 하고 또 하고 또 하고",
          "speaker_id": "1",
          "time": "20210902 15:59"
        },
        {
          "id": "MDRW2100027893.1.14",
          "form": "루프물 주인공된 기분",
          "original_form": "루프물 주인공된 기분",
          "speaker_id": "1",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100027893.1.15",
          "form": "그만하쌔요 그런다고 이해하지 않습니다",
          "original_form": "그만하쌔요 그런다고 이해하지 않습니다",
          "speaker_id": "2",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100027893.1.16",
          "form": "걍 대충... 들음",
          "original_form": "걍 대충... 들음",
          "speaker_id": "2",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100027893.1.17",
          "form": "하...",
          "original_form": "하...",
          "speaker_id": "1",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100027893.1.18",
          "form": "수강신청 450만원짜리 랜덤 라이브콘서트 티켓팅이라는 말 진짜 킹받음",
          "original_form": "수강신청 450만원짜리 랜덤 라이브콘서트 티켓팅이라는 말 진짜 킹받음",
          "speaker_id": "1",
          "time": "20210902 16:02"
        },
        {
          "id": "MDRW2100027893.1.19",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210902 16:02"
        },
        {
          "id": "MDRW2100027893.1.20",
          "form": "아니 450이면",
          "original_form": "아니 450이면",
          "speaker_id": "2",
          "time": "20210902 16:02"
        },
        {
          "id": "MDRW2100027893.1.21",
          "form": "확정은 해줘야지 랜덤은 너무하지",
          "original_form": "확정은 해줘야지 랜덤은 너무하지",
          "speaker_id": "2",
          "time": "20210902 16:02"
        },
        {
          "id": "MDRW2100027893.1.22",
          "form": "내말이",
          "original_form": "내말이",
          "speaker_id": "1",
          "time": "20210902 16:02"
        },
        {
          "id": "MDRW2100027893.1.23",
          "form": "천장보다 비싸잖아",
          "original_form": "천장보다 비싸잖아",
          "speaker_id": "2",
          "time": "20210902 16:02"
        },
        {
          "id": "MDRW2100027893.1.24",
          "form": "요즘 게임도 그렇게는 안 나온다",
          "original_form": "요즘 게임도 그렇게는 안 나온다",
          "speaker_id": "1",
          "time": "20210902 16:03"
        },
        {
          "id": "MDRW2100027893.1.25",
          "form": "내 돈 내고 이게 뭐하는 짓이람...",
          "original_form": "내 돈 내고 이게 뭐하는 짓이람...",
          "speaker_id": "2",
          "time": "20210902 16:03"
        },
        {
          "id": "MDRW2100027893.1.26",
          "form": "진짜 짜증나",
          "original_form": "진짜 짜증나",
          "speaker_id": "1",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100027893.1.27",
          "form": "달달하신가요",
          "original_form": "달달하신가요",
          "speaker_id": "1",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100027893.1.28",
          "form": "등록금 루팡이요?",
          "original_form": "등록금 루팡이요?",
          "speaker_id": "2",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100027893.1.29",
          "form": "아니 근데 내가 루팡한 게 아니잖아 생각해보니",
          "original_form": "아니 근데 내가 루팡한 게 아니잖아 생각해보니",
          "speaker_id": "2",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100027893.1.30",
          "form": "총장한테 말한 건데",
          "original_form": "총장한테 말한 건데",
          "speaker_id": "1",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100027893.1.31",
          "form": "대학교 등록금 루팡 멈춰",
          "original_form": "대학교 등록금 루팡 멈춰",
          "speaker_id": "2",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100027893.1.32",
          "form": "ㄹㅇ 멈춰",
          "original_form": "ㄹㅇ 멈춰",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027893.1.33",
          "form": "수업 스트리밍 해둬서 찔렸음",
          "original_form": "수업 스트리밍 해둬서 찔렸음",
          "speaker_id": "2",
