      "utterance": [
        {
          "id": "MDRW2100020016.1.1",
          "form": "전 피스타치오 아몬드봉봉이에요 ㅎㅎ",
          "original_form": "전 피스타치오 아몬드봉봉이에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100020016.1.2",
          "form": "제 베라픽은",
          "original_form": "제 베라픽은",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100020016.1.3",
          "form": "요거트입니다",
          "original_form": "요거트입니다",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100020016.1.4",
          "form": "아 약간 씹는 맛 좋아하시는구나",
          "original_form": "아 약간 씹는 맛 좋아하시는구나",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020016.1.5",
          "form": "저도 그 두가지 좋아합니다",
          "original_form": "저도 그 두가지 좋아합니다",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020016.1.6",
          "form": "요거트!",
          "original_form": "요거트!",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020016.1.7",
          "form": "요거트 좋아하긴 하는데 베라에선 안먹어본것같아요",
          "original_form": "요거트 좋아하긴 하는데 베라에선 안먹어본것같아요",
          "speaker_id": "2",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020016.1.8",
          "form": "대부분 그러시더라구요",
          "original_form": "대부분 그러시더라구요",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100020016.1.9",
          "form": "음  맛은 요맘때보다 더 진한 맛인데",
          "original_form": "음  맛은 요맘때보다 더 진한 맛인데",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020016.1.10",
          "form": "단독으로 먹기보단 입가심으로 고르시는 경우가 많더라구요",
          "original_form": "단독으로 먹기보단 입가심으로 고르시는 경우가 많더라구요",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020016.1.11",
          "form": "저 요맘때 하드중에서 제일 좋아해요",
          "original_form": "저 요맘때 하드중에서 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020016.1.12",
          "form": "맞야요 입가심용 ㅋㅋㅋㅋ",
          "original_form": "맞야요 입가심용 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020016.1.13",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020016.1.14",
          "form": "뻔한질문",
          "original_form": "뻔한질문",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020016.1.15",
          "form": "민초 호 불호",
          "original_form": "민초 호 불호",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020016.1.16",
          "form": "불호!",
          "original_form": "불호!",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020016.1.17",
          "form": "핵불호",
          "original_form": "핵불호",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020016.1.18",
          "form": "저두 불호입니다",
          "original_form": "저두 불호입니다",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020016.1.19",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020016.1.20",
          "form": "조마조마햇다구요",
          "original_form": "조마조마햇다구요",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020016.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020016.1.22",
          "form": "우리 미세하게 가까워졌습니다",
          "original_form": "우리 미세하게 가까워졌습니다",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100020016.1.23",
          "form": "그럼 레샤 호 불호",
          "original_form": "그럼 레샤 호 불호",
          "speaker_id": "1",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100020016.1.24",
          "form": "앗 아작 미세한가여? ㅋㅋㅋㅋㅋ",
          "original_form": "앗 아작 미세한가여? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100020016.1.25",
          "form": "불호.....ㅎ",
          "original_form": "불호.....ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100020016.1.26",
          "form": "name1님 호죠?",
          "original_form": "&name1&님 호죠?",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100020016.1.27",
          "form": "아니요 (단호)",
          "original_form": "아니요 (단호)",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100020016.1.28",
          "form": "어머나세상에",
          "original_form": "어머나세상에",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020016.1.29",
          "form": "저 배라 처음이 레샤였거든요",
          "original_form": "저 배라 처음이 레샤였거든요",
          "speaker_id": "1",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020016.1.30",
          "form": "제가 그걸 왜 골랐나",
          "original_form": "제가 그걸 왜 골랐나",
          "speaker_id": "1",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020016.1.31",
          "form": "호인줄알고 또 조마조마햇습니다",
          "original_form": "호인줄알고 또 조마조마햇습니다",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020016.1.32",
          "form": "제 입이 원망스러웠어요ㅋㅋㅋㅇ",
          "original_form": "제 입이 원망스러웠어요ㅋㅋㅋㅇ",
          "speaker_id": "1",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020016.1.33",
          "form": "이로서 우리 약간 미세하게 더 가까워졌습니다",
          "original_form": "이로서 우리 약간 미세하게 더 가까워졌습니다",
          "speaker_id": "1",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020016.1.34",
          "form": "그럼 하와이안 호 불호",
          "original_form": "그럼 하와이안 호 불호",
