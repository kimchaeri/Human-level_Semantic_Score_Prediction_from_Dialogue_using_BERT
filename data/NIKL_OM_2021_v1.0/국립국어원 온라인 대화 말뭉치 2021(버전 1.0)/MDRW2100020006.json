      "utterance": [
        {
          "id": "MDRW2100020006.1.1",
          "form": "배달 음식 좋아하시나요?!",
          "original_form": "배달 음식 좋아하시나요?!",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020006.1.2",
          "form": "피자 치킨 같은거 좋아해요 요즘은 배달되는 음식이 많아져서 좋아요?",
          "original_form": "피자 치킨 같은거 좋아해요 요즘은 배달되는 음식이 많아져서 좋아요?",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100020006.1.3",
          "form": "자주 시켜서 드시나요?",
          "original_form": "자주 시켜서 드시나요?",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100020006.1.4",
          "form": "자주는 아니지만",
          "original_form": "자주는 아니지만",
          "speaker_id": "2",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020006.1.5",
          "form": "가끔씩 시켜서 먹어요!",
          "original_form": "가끔씩 시켜서 먹어요!",
          "speaker_id": "2",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020006.1.6",
          "form": "저도 피자...치킨... 좋아해요 얼마 전에는 태국 음식 시켜먹었어요!",
          "original_form": "저도 피자...치킨... 좋아해요 얼마 전에는 태국 음식 시켜먹었어요!",
          "speaker_id": "2",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020006.1.7",
          "form": "사실 제가 대학원생이라",
          "original_form": "사실 제가 대학원생이라",
          "speaker_id": "2",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020006.1.8",
          "form": "외식이 자유롭지 않아서",
          "original_form": "외식이 자유롭지 않아서",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020006.1.9",
          "form": "별미 먹고싶을 땐 배달음식을 시키거든요..ㅜ",
          "original_form": "별미 먹고싶을 땐 배달음식을 시키거든요..ㅜ",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020006.1.10",
          "form": "요즘은 외식이 힘드니까 배달 많이 시켜요",
          "original_form": "요즘은 외식이 힘드니까 배달 많이 시켜요",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020006.1.11",
          "form": "별미를 먹고싶을때요 ㅎㅎ",
          "original_form": "별미를 먹고싶을때요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020006.1.12",
          "form": "태국음식은 똠얌꿍인가요?",
          "original_form": "태국음식은 똠얌꿍인가요?",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100020006.1.13",
          "form": "그런것도 배달이 되나요?",
          "original_form": "그런것도 배달이 되나요?",
          "speaker_id": "1",
          "time": "20210827 23:34"
        },
        {
          "id": "MDRW2100020006.1.14",
          "form": "네네!!",
          "original_form": "네네!!",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020006.1.15",
          "form": "맞아요 정확히 똠양꿍이었어요",
          "original_form": "맞아요 정확히 똠양꿍이었어요",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020006.1.16",
          "form": "네 되더라구요...?! 비닐에 포장되어와서 찝찝하기는 하지만...",
          "original_form": "네 되더라구요...?! 비닐에 포장되어와서 찝찝하기는 하지만...",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020006.1.17",
          "form": "아 얼마전에",
          "original_form": "아 얼마전에",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020006.1.18",
          "form": "김밥집 식중독 사건 들으셨어요?",
          "original_form": "김밥집 식중독 사건 들으셨어요?",
          "speaker_id": "2",
          "time": "20210827 23:42"
        },
        {
          "id": "MDRW2100020006.1.19",
          "form": "네 들었어요 ㅠㅠ 계란이 문젠거 같다고 하더라고요 ㅜ",
          "original_form": "네 들었어요 ㅠㅠ 계란이 문젠거 같다고 하더라고요 ㅜ",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020006.1.20",
          "form": "정말 ㅠ 피해자분이 너무 안타까워요",
          "original_form": "정말 ㅠ 피해자분이 너무 안타까워요",
          "speaker_id": "2",
          "time": "20210828 00:54"
        },
        {
          "id": "MDRW2100020006.1.21",
          "form": "김밥 먹고 그렇게 될 줄 누가 알았겠어요...",
          "original_form": "김밥 먹고 그렇게 될 줄 누가 알았겠어요...",
          "speaker_id": "2",
          "time": "20210828 00:54"
        },
        {
          "id": "MDRW2100020006.1.22",
          "form": "그러니까요ㅠㅠ",
          "original_form": "그러니까요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 01:04"
        },
        {
          "id": "MDRW2100020006.1.23",
          "form": "외식이나 배달음식을 안 먹을 순 없지만 자제하고 조심해야겠어요  그래도 이제 추워질거라 다행이기도해요",
          "original_form": "외식이나 배달음식을 안 먹을 순 없지만 자제하고 조심해야겠어요  그래도 이제 추워질거라 다행이기도해요",
          "speaker_id": "1",
          "time": "20210828 01:05"
        },
        {
          "id": "MDRW2100020006.1.24",
          "form": "그럼 주로 해먹는 요리는 무엇인가요..?",
          "original_form": "그럼 주로 해먹는 요리는 무엇인가요..?",
          "speaker_id": "1",
          "time": "20210828 01:06"
        },
        {
          "id": "MDRW2100020006.1.25",
          "form": "저는..... 시간은 좀 오래 걸리지만",
          "original_form": "저는..... 시간은 좀 오래 걸리지만",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020006.1.26",
          "form": "애호박을 얇게 썰어서",
          "original_form": "애호박을 얇게 썰어서",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020006.1.27",
          "form": "소금 조금 넣은 계란물에 부쳐먹으면",
          "original_form": "소금 조금 넣은 계란물에 부쳐먹으면",
          "speaker_id": "2",
          "time": "20210828 01:07"
        },
        {
          "id": "MDRW2100020006.1.28",
          "form": "진짜진짜 맛있어요!!!",
          "original_form": "진짜진짜 맛있어요!!!",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020006.1.29",
          "form": "그거랑... 면 음식 좋아합니다!!",
          "original_form": "그거랑... 면 음식 좋아합니다!!",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020006.1.30",
          "form": "저는 토마토파스타에 고추장이나 볶음김치를 넣어서 해먹는데",
          "original_form": "저는 토마토파스타에 고추장이나 볶음김치를 넣어서 해먹는데",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020006.1.31",
          "form": "오오오???",
          "original_form": "오오오???",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020006.1.32",
          "form": "색다른 맛이랍니다 ㅋㅋㅋㅋ",
          "original_form": "색다른 맛이랍니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020006.1.33",
          "form": "파스타면이랑 소스는 따로따로",
          "original_form": "파스타면이랑 소스는 따로따로",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020006.1.34",
          "form": "구매해서 만드시는거죠???",
          "original_form": "구매해서 만드시는거죠???",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020006.1.35",
          "form": "애호박전 좋아하시는구나 ㅎㅎ",
          "original_form": "애호박전 좋아하시는구나 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020006.1.36",
          "form": "네네 애호박전은 진짜 얇게 써는게 관건이에욬ㅋㅋㅋ",
          "original_form": "네네 애호박전은 진짜 얇게 써는게 관건이에욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:08"
        },
        {
          "id": "MDRW2100020006.1.37",
          "form": "네네 그게 훨씬 효율적이라서요 가격면에서도 ㅎㅎ",
          "original_form": "네네 그게 훨씬 효율적이라서요 가격면에서도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020006.1.38",
          "form": "오오 맞네요 맞네요~!",
          "original_form": "오오 맞네요 맞네요~!",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020006.1.39",
          "form": "저도 면 음식 좋아하는데",
          "original_form": "저도 면 음식 좋아하는데",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020006.1.40",
          "form": "토마토 파스타에",
          "original_form": "토마토 파스타에",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020006.1.41",
          "form": "고추장이랑 볶음김치라니",
          "original_form": "고추장이랑 볶음김치라니",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020006.1.42",
          "form": "정말 따라해보고싶네요 ㅎㅎㅎ",
          "original_form": "정말 따라해보고싶네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 01:09"
        },
        {
          "id": "MDRW2100020006.1.43",
          "form": "시판소스에 방울토마토를 직접 넣어도 맛있어요 ",
          "original_form": "시판소스에 방울토마토를 직접 넣어도 맛있어요 ",
          "speaker_id": "1",
          "time": "20210828 01:10"
        },
        {
          "id": "MDRW2100020006.1.44",
          "form": "아직 애호박철이니까 주말에 얇게 썰어서 부쳐먹을까봐요.. 막걸리와 함께 ㅎㅎ",
          "original_form": "아직 애호박철이니까 주말에 얇게 썰어서 부쳐먹을까봐요.. 막걸리와 함께 ㅎㅎ",
          "speaker_id": "1",
