      "utterance": [
        {
          "id": "MDRW2100019422.1.1",
          "form": "이제 먹으려고요 만둣국",
          "original_form": "이제 먹으려고요 만둣국",
          "speaker_id": "2",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019422.1.2",
          "form": "뭐드시나요?",
          "original_form": "뭐드시나요?",
          "speaker_id": "2",
          "time": "20210827 19:09"
        },
        {
          "id": "MDRW2100019422.1.3",
          "form": "오 만둣국 ! ㅋㅋㅋㅋ 저도 점심에 먹었어요 만둣국",
          "original_form": "오 만둣국 ! ㅋㅋㅋㅋ 저도 점심에 먹었어요 만둣국",
          "speaker_id": "1",
          "time": "20210827 19:17"
        },
        {
          "id": "MDRW2100019422.1.4",
          "form": "젼 꼬막비빔밥 먹어요",
          "original_form": "젼 꼬막비빔밥 먹어요",
          "speaker_id": "1",
          "time": "20210827 19:17"
        },
        {
          "id": "MDRW2100019422.1.5",
          "form": "아 맛있겠네요! 거기꺼예요? 연안식당",
          "original_form": "아 맛있겠네요! 거기꺼예요? 연안식당",
          "speaker_id": "2",
          "time": "20210827 19:56"
        },
        {
          "id": "MDRW2100019422.1.6",
          "form": "냉동 식품 사먹었어요 ㅋㅋㅋ 연안식당거가 훨 맛잇어요..",
          "original_form": "냉동 식품 사먹었어요 ㅋㅋㅋ 연안식당거가 훨 맛잇어요..",
          "speaker_id": "1",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019422.1.7",
          "form": "ㅋㅋ아 맛은 있었죠? 맛없는걸로 배채우면 속상해요",
          "original_form": "ㅋㅋ아 맛은 있었죠? 맛없는걸로 배채우면 속상해요",
          "speaker_id": "2",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019422.1.8",
          "form": "저녁드셨나요??",
          "original_form": "저녁드셨나요??",
          "speaker_id": "2",
          "time": "20210902 18:13"
        },
        {
          "id": "MDRW2100019422.1.9",
          "form": "어 아직요 배고프네욤 푸ㅜ",
          "original_form": "어 아직요 배고프네욤 푸ㅜ",
          "speaker_id": "1",
          "time": "20210902 18:18"
        },
        {
          "id": "MDRW2100019422.1.10",
          "form": "저녁엔 맛난거 드셔야죠!??",
          "original_form": "저녁엔 맛난거 드셔야죠!??",
          "speaker_id": "2",
          "time": "20210902 18:21"
        },
        {
          "id": "MDRW2100019422.1.11",
          "form": "약속있어요 ㅠㅠ 불백 식당간다는데",
          "original_form": "약속있어요 ㅠㅠ 불백 식당간다는데",
          "speaker_id": "1",
          "time": "20210902 18:22"
        },
        {
          "id": "MDRW2100019422.1.12",
          "form": "안땡김 ㅋㅋㅋㅋ",
          "original_form": "안땡김 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:22"
        },
        {
          "id": "MDRW2100019422.1.13",
          "form": "불백 그래도 맛난데요!!! 뭐드시고싶으세요?",
          "original_form": "불백 그래도 맛난데요!!! 뭐드시고싶으세요?",
          "speaker_id": "2",
          "time": "20210902 18:23"
        },
        {
          "id": "MDRW2100019422.1.14",
          "form": "엄마 불백이 맛있고 식당 불백은 입맛에 안맞아요ㅠㅋㅋㅋㅋ",
          "original_form": "엄마 불백이 맛있고 식당 불백은 입맛에 안맞아요ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100019422.1.15",
          "form": "좀 달더라구용",
          "original_form": "좀 달더라구용",
          "speaker_id": "1",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100019422.1.16",
          "form": "파스타먹고싶네요 ㅎㅎ",
          "original_form": "파스타먹고싶네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:24"
        },
        {
          "id": "MDRW2100019422.1.17",
          "form": "ㅋㅋ맞아요 달죠ㅜㅋㅋ",
          "original_form": "ㅋㅋ맞아요 달죠ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:26"
        },
        {
          "id": "MDRW2100019422.1.18",
          "form": "갑자기 엄마  김치찌개 먹고싶어요",
          "original_form": "갑자기 엄마  김치찌개 먹고싶어요",
