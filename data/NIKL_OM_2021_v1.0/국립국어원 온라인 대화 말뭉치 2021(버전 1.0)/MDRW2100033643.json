      "utterance": [
        {
          "id": "MDRW2100033643.1.1",
          "form": "그나마",
          "original_form": "그나마",
          "speaker_id": "1",
          "time": "20210824 19:52"
        },
        {
          "id": "MDRW2100033643.1.2",
          "form": "요즘 어떤 게임을 즐겨하세요?",
          "original_form": "요즘 어떤 게임을 즐겨하세요?",
          "speaker_id": "2",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100033643.1.3",
          "form": "요즘 모바일 게임 중에 <미해결사건부> 해보고 있어요",
          "original_form": "요즘 모바일 게임 중에 <미해결사건부> 해보고 있어요",
          "speaker_id": "1",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100033643.1.4",
          "form": "신작 추천으로 떴길래 며칠 해봤는데 괜찮더라고요",
          "original_form": "신작 추천으로 떴길래 며칠 해봤는데 괜찮더라고요",
          "speaker_id": "1",
          "time": "20210824 20:10"
        },
        {
          "id": "MDRW2100033643.1.5",
          "form": "사건 중심의 게임인가요?",
          "original_form": "사건 중심의 게임인가요?",
          "speaker_id": "2",
          "time": "20210824 21:10"
        },
        {
          "id": "MDRW2100033643.1.6",
          "form": "메인 컨텐츠는 사건 중심이에요!",
          "original_form": "메인 컨텐츠는 사건 중심이에요!",
          "speaker_id": "1",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100033643.1.7",
          "form": "그래도 서브 콘텐츠들도 꽤 많아요",
          "original_form": "그래도 서브 콘텐츠들도 꽤 많아요",
          "speaker_id": "1",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100033643.1.8",
          "form": "스토리 중심의 게임을 선호하시나봐요",
          "original_form": "스토리 중심의 게임을 선호하시나봐요",
          "speaker_id": "2",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100033643.1.9",
          "form": "단순한 게임보다는 스토리가 조금이라도 있는게 더 재밌는 거 같아요",
          "original_form": "단순한 게임보다는 스토리가 조금이라도 있는게 더 재밌는 거 같아요",
          "speaker_id": "1",
          "time": "20210824 21:16"
        },
        {
          "id": "MDRW2100033643.1.10",
          "form": "게임 좋아하시나요?",
          "original_form": "게임 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210824 21:16"
        },
        {
          "id": "MDRW2100033643.1.11",
          "form": "저는 예전에는 포켓몬고 많이 했어요",
          "original_form": "저는 예전에는 포켓몬고 많이 했어요",
          "speaker_id": "2",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100033643.1.12",
          "form": "아 그거 주변에서 많이 했던 거 같아요",
          "original_form": "아 그거 주변에서 많이 했던 거 같아요",
          "speaker_id": "1",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100033643.1.13",
          "form": "pc게임은 안 하시나요?",
          "original_form": "pc게임은 안 하시나요?",
          "speaker_id": "1",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100033643.1.14",
          "form": "컨트롤을 잘 못해서 pc는 잘 안 하게 되더라구요ㅠ",
          "original_form": "컨트롤을 잘 못해서 pc는 잘 안 하게 되더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210824 21:46"
        },
        {
          "id": "MDRW2100033643.1.15",
          "form": "저도 컨트롤 겜 잘 못해요ㅠㅠ",
          "original_form": "저도 컨트롤 겜 잘 못해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 21:47"
        },
        {
          "id": "MDRW2100033643.1.16",
          "form": "열정은 넘치는데 게임에 재능이 없어요ㅋㅋㅋ",
          "original_form": "열정은 넘치는데 게임에 재능이 없어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:47"
        },
        {
          "id": "MDRW2100033643.1.17",
          "form": "에임 맞추기가 너무 어렵더라구요ㅠ",
          "original_form": "에임 맞추기가 너무 어렵더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210824 21:49"
        },
        {
          "id": "MDRW2100033643.1.18",
          "form": "맞아요 배그 몇 판 해봤는데 어지럽기만 하고 너무 어려워서 친구들 열심히 할 때 저는 포기했어요",
          "original_form": "맞아요 배그 몇 판 해봤는데 어지럽기만 하고 너무 어려워서 친구들 열심히 할 때 저는 포기했어요",
          "speaker_id": "1",
          "time": "20210824 21:51"
        },
        {
          "id": "MDRW2100033643.1.19",
          "form": "다른 유저들의 맛있는 한 끼가 되는 느낌...",
          "original_form": "다른 유저들의 맛있는 한 끼가 되는 느낌...",
          "speaker_id": "2",
          "time": "20210824 21:53"
        },
        {
          "id": "MDRW2100033643.1.20",
          "form": "그런 게임은 다른 사람이 하는 걸 보는 게 더 재밌더라고요ㅠㅠ",
          "original_form": "그런 게임은 다른 사람이 하는 걸 보는 게 더 재밌더라고요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 21:54"
        }
      ]
    }
