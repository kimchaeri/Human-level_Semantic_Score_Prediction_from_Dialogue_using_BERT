      "utterance": [
        {
          "id": "MDRW2100008158.1.1",
          "form": "날씨 얘기했어요",
          "original_form": "날씨 얘기했어요",
          "speaker_id": "1",
          "time": "20210822 13:42"
        },
        {
          "id": "MDRW2100008158.1.2",
          "form": "감사해요~!",
          "original_form": "감사해요~!",
          "speaker_id": "2",
          "time": "20210822 13:44"
        },
        {
          "id": "MDRW2100008158.1.3",
          "form": "운동 하시나요?",
          "original_form": "운동 하시나요?",
          "speaker_id": "2",
          "time": "20210822 13:44"
        },
        {
          "id": "MDRW2100008158.1.4",
          "form": "요즘 유산소만 2시간 정도 해요",
          "original_form": "요즘 유산소만 2시간 정도 해요",
          "speaker_id": "1",
          "time": "20210822 13:46"
        },
        {
          "id": "MDRW2100008158.1.5",
          "form": "name2 님은요?",
          "original_form": "&name2& 님은요?",
          "speaker_id": "1",
          "time": "20210822 13:46"
        },
        {
          "id": "MDRW2100008158.1.6",
          "form": "저도 유산소위주로 홈트하거나 걷고 있어요",
          "original_form": "저도 유산소위주로 홈트하거나 걷고 있어요",
          "speaker_id": "2",
          "time": "20210822 13:47"
        },
        {
          "id": "MDRW2100008158.1.7",
          "form": "체력이 워낙 부족해서 건강챙길겸 하고있어요~",
          "original_form": "체력이 워낙 부족해서 건강챙길겸 하고있어요~",
          "speaker_id": "2",
          "time": "20210822 13:47"
        },
        {
          "id": "MDRW2100008158.1.8",
          "form": "저도 그정도 하는 거 같아요",
          "original_form": "저도 그정도 하는 거 같아요",
          "speaker_id": "1",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008158.1.9",
          "form": "원래 운동 좋아하시나요?",
          "original_form": "원래 운동 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 13:49"
        },
        {
          "id": "MDRW2100008158.1.10",
          "form": "최근에 운동 시작했는데 점점 재미있는 것 같아요 조금씩 체력이 붙는 것 같아서요!",
          "original_form": "최근에 운동 시작했는데 점점 재미있는 것 같아요 조금씩 체력이 붙는 것 같아서요!",
          "speaker_id": "2",
          "time": "20210822 13:51"
        },
        {
          "id": "MDRW2100008158.1.11",
          "form": "요즘 거북목이 신경쓰여서 필라테스해볼까 고민중이에요",
          "original_form": "요즘 거북목이 신경쓰여서 필라테스해볼까 고민중이에요",
          "speaker_id": "2",
          "time": "20210822 13:51"
        },
        {
          "id": "MDRW2100008158.1.12",
          "form": "운동도 하면 할 수록 더 하게되고 재밌는 거 같아요",
          "original_form": "운동도 하면 할 수록 더 하게되고 재밌는 거 같아요",
          "speaker_id": "1",
          "time": "20210822 13:52"
        },
        {
          "id": "MDRW2100008158.1.13",
          "form": "필라테스가 교정에 좋죠?",
          "original_form": "필라테스가 교정에 좋죠?",
          "speaker_id": "1",
          "time": "20210822 13:52"
        },
        {
          "id": "MDRW2100008158.1.14",
          "form": "자세교정에 도움이 된다고 하네요",
          "original_form": "자세교정에 도움이 된다고 하네요",
          "speaker_id": "2",
          "time": "20210822 13:54"
        },
        {
          "id": "MDRW2100008158.1.15",
          "form": "유연성이 부족해서 유연성도 조금 키워질 것 같구요",
          "original_form": "유연성이 부족해서 유연성도 조금 키워질 것 같구요",
          "speaker_id": "2",
          "time": "20210822 13:54"
        },
        {
          "id": "MDRW2100008158.1.16",
          "form": "오 괜찮네요",
          "original_form": "오 괜찮네요",
          "speaker_id": "1",
          "time": "20210822 13:55"
        },
        {
          "id": "MDRW2100008158.1.17",
          "form": "저도 중학교 이후로 몸이 다 굳은 거 같아요",
          "original_form": "저도 중학교 이후로 몸이 다 굳은 거 같아요",
          "speaker_id": "1",
          "time": "20210822 13:55"
        },
        {
          "id": "MDRW2100008158.1.18",
          "form": "그쵸 성인이 되니 유연성이 점점 안좋아지는 것 같아요",
          "original_form": "그쵸 성인이 되니 유연성이 점점 안좋아지는 것 같아요",
          "speaker_id": "2",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100008158.1.19",
          "form": "앉아있는 시간이 많아서 틈틈히 스트레칭이라도 해주고 있어요",
          "original_form": "앉아있는 시간이 많아서 틈틈히 스트레칭이라도 해주고 있어요",
          "speaker_id": "2",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100008158.1.20",
          "form": "스트레칭 정말 자주해야 하는 거 같아요",
          "original_form": "스트레칭 정말 자주해야 하는 거 같아요",
          "speaker_id": "1",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100008158.1.21",
          "form": "수영은 좋아하세요?",
          "original_form": "수영은 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 13:58"
        },
        {
          "id": "MDRW2100008158.1.22",
          "form": "수영 어렸을때 배웠는데 다 잊어버린 것 같아요 .. 다시 한번 배워보고싶어요",
          "original_form": "수영 어렸을때 배웠는데 다 잊어버린 것 같아요 .. 다시 한번 배워보고싶어요",
          "speaker_id": "2",
          "time": "20210822 14:00"
        },
        {
          "id": "MDRW2100008158.1.23",
          "form": "name1님은 수영 잘하시나요?",
          "original_form": "&name1&님은 수영 잘하시나요?",
          "speaker_id": "2",
          "time": "20210822 14:00"
        },
        {
          "id": "MDRW2100008158.1.24",
          "form": "사실 배워보고 싶었는데",
          "original_form": "사실 배워보고 싶었는데",
          "speaker_id": "1",
          "time": "20210822 14:02"
        },
        {
          "id": "MDRW2100008158.1.25",
          "form": "코로나 때문에 포기했어요",
          "original_form": "코로나 때문에 포기했어요",
          "speaker_id": "1",
          "time": "20210822 14:02"
        },
        {
          "id": "MDRW2100008158.1.26",
          "form": "빨리 끝나야 뭘 할텐데",
          "original_form": "빨리 끝나야 뭘 할텐데",
          "speaker_id": "1",
          "time": "20210822 14:02"
        },
        {
          "id": "MDRW2100008158.1.27",
          "form": "맞아요 특히 수영은 물에서 하는거라 지금 하기에는 무리가 있더라구요",
          "original_form": "맞아요 특히 수영은 물에서 하는거라 지금 하기에는 무리가 있더라구요",
          "speaker_id": "2",
          "time": "20210822 14:05"
        },
        {
          "id": "MDRW2100008158.1.28",
          "form": "수영이 칼로리소모가 엄청나서 다이어트에도 좋다고하네요",
          "original_form": "수영이 칼로리소모가 엄청나서 다이어트에도 좋다고하네요",
          "speaker_id": "2",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008158.1.29",
          "form": "꼭 한번 배워보고 싶어요",
          "original_form": "꼭 한번 배워보고 싶어요",
          "speaker_id": "1",
          "time": "20210822 14:06"
        }
      ]
