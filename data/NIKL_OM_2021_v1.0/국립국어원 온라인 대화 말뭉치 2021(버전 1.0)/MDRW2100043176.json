      "utterance": [
        {
          "id": "MDRW2100043176.1.1",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100043176.1.2",
          "form": "기념품도 사셨겠네요!",
          "original_form": "기념품도 사셨겠네요!",
          "speaker_id": "2",
          "time": "20210907 15:19"
        },
        {
          "id": "MDRW2100043176.1.3",
          "form": "네 인형이랑 책갈피 이것저것 샀어요!",
          "original_form": "네 인형이랑 책갈피 이것저것 샀어요!",
          "speaker_id": "1",
          "time": "20210907 15:25"
        },
        {
          "id": "MDRW2100043176.1.4",
          "form": "그래서 올림픽보느라고 주변은 못가봤어요",
          "original_form": "그래서 올림픽보느라고 주변은 못가봤어요",
          "speaker_id": "1",
          "time": "20210907 15:26"
        },
        {
          "id": "MDRW2100043176.1.5",
          "form": "아아!!!!",
          "original_form": "아아!!!!",
          "speaker_id": "2",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100043176.1.6",
          "form": "되게 좋은 추억이겠어요!!",
          "original_form": "되게 좋은 추억이겠어요!!",
          "speaker_id": "2",
          "time": "20210907 15:27"
        },
        {
          "id": "MDRW2100043176.1.7",
          "form": "올림픽했던 건물 지금은 관광 명소일까요?",
          "original_form": "올림픽했던 건물 지금은 관광 명소일까요?",
          "speaker_id": "2",
          "time": "20210907 15:28"
        },
        {
          "id": "MDRW2100043176.1.8",
          "form": "어느정도 남아있지 않을까요....?",
          "original_form": "어느정도 남아있지 않을까요....?",
          "speaker_id": "1",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100043176.1.9",
          "form": "기업들 건물이나 식당은 임시건물이라 없을거 같은데 경기장은 있을거 같아요",
          "original_form": "기업들 건물이나 식당은 임시건물이라 없을거 같은데 경기장은 있을거 같아요",
          "speaker_id": "1",
          "time": "20210907 15:52"
        },
        {
          "id": "MDRW2100043176.1.10",
          "form": "아아 임시건물리었구나..",
          "original_form": "아아 임시건물리었구나..",
          "speaker_id": "2",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043176.1.11",
          "form": "보통 여행지가면 기념품 사시나요?",
          "original_form": "보통 여행지가면 기념품 사시나요?",
          "speaker_id": "2",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043176.1.12",
          "form": "네 엽서를 주로사요",
          "original_form": "네 엽서를 주로사요",
          "speaker_id": "1",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043176.1.13",
          "form": "기념품 사시나요?",
          "original_form": "기념품 사시나요?",
          "speaker_id": "1",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043176.1.14",
          "form": "오오 저도 엽서요!",
          "original_form": "오오 저도 엽서요!",
          "speaker_id": "2",
          "time": "20210907 15:53"
        },
        {
          "id": "MDRW2100043176.1.15",
          "form": "가격도 괜찮고 보관도 용이해서요",
          "original_form": "가격도 괜찮고 보관도 용이해서요",
          "speaker_id": "2",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100043176.1.16",
          "form": "일기까지 써놓을 때도잏답니다",
          "original_form": "일기까지 써놓을 때도잏답니다",
          "speaker_id": "2",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100043176.1.17",
          "form": "우와 일기 기억에 오래남겠어요",
          "original_form": "우와 일기 기억에 오래남겠어요",
          "speaker_id": "1",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043176.1.18",
          "form": "괜찮은 방법이네요",
          "original_form": "괜찮은 방법이네요",
          "speaker_id": "1",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043176.1.19",
          "form": "해외는 가보신곳 있으신가요?",
          "original_form": "해외는 가보신곳 있으신가요?",
          "speaker_id": "1",
          "time": "20210907 15:55"
        },
        {
          "id": "MDRW2100043176.1.20",
          "form": "프랑스요!!",
          "original_form": "프랑스요!!",
          "speaker_id": "2",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100043176.1.21",
          "form": "외고나와서 프랑스 2주 가봣습니다 ㅎㅎ",
          "original_form": "외고나와서 프랑스 2주 가봣습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 15:57"
        },
        {
          "id": "MDRW2100043176.1.22",
          "form": "우와 프랑스 어디어디 가셨나요?",
          "original_form": "우와 프랑스 어디어디 가셨나요?",
          "speaker_id": "1",
          "time": "20210907 16:00"
        },
        {
          "id": "MDRW2100043176.1.23",
          "form": "파리랑 어디였는데",
          "original_form": "파리랑 어디였는데",
          "speaker_id": "2",
          "time": "20210907 16:02"
        },
        {
          "id": "MDRW2100043176.1.24",
          "form": "잘기억이안나요...ㅎ",
          "original_form": "잘기억이안나요...ㅎ",
          "speaker_id": "2",
          "time": "20210907 16:02"
        },
        {
          "id": "MDRW2100043176.1.25",
          "form": "파리는 어땠어요?",
          "original_form": "파리는 어땠어요?",
          "speaker_id": "1",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100043176.1.26",
          "form": "학교 버스타고, 홈스테이친구 따라다녀서요 ㅎㅎㅎ",
          "original_form": "학교 버스타고, 홈스테이친구 따라다녀서요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100043176.1.27",
          "form": "웬만한 여행지는 다 가봤는데",
          "original_form": "웬만한 여행지는 다 가봤는데",
          "speaker_id": "2",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100043176.1.28",
          "form": "비둘기많은게 저는 너무 싫었어요 ㅎㅎㅎ",
          "original_form": "비둘기많은게 저는 너무 싫었어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100043176.1.29",
          "form": "식사도 한식을 너무 좋아해서",
          "original_form": "식사도 한식을 너무 좋아해서",
          "speaker_id": "2",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100043176.1.30",
          "form": "힘들었구요",
          "original_form": "힘들었구요",
          "speaker_id": "2",
          "time": "20210907 16:03"
        },
        {
          "id": "MDRW2100043176.1.31",
          "form": "그러셨구나...힘드셨겠다",
          "original_form": "그러셨구나...힘드셨겠다",
          "speaker_id": "1",
          "time": "20210907 16:04"
        }
      ]
