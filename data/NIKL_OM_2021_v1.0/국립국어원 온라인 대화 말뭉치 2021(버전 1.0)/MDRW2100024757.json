      "utterance": [
        {
          "id": "MDRW2100024757.1.1",
          "form": "여행 좋아하시나요?",
          "original_form": "여행 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210831 18:10"
        },
        {
          "id": "MDRW2100024757.1.2",
          "form": "네 좋아합니다",
          "original_form": "네 좋아합니다",
          "speaker_id": "2",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100024757.1.3",
          "form": "국내여행지 추천 해주실 곳 있으실까요~~?",
          "original_form": "국내여행지 추천 해주실 곳 있으실까요~~?",
          "speaker_id": "1",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100024757.1.4",
          "form": "저는 제주도를 이번주에 갑니다",
          "original_form": "저는 제주도를 이번주에 갑니다",
          "speaker_id": "2",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100024757.1.5",
          "form": "흔한곳만 다녀서요",
          "original_form": "흔한곳만 다녀서요",
          "speaker_id": "2",
          "time": "20210831 18:14"
        },
        {
          "id": "MDRW2100024757.1.6",
          "form": "강릉과 대천등",
          "original_form": "강릉과 대천등",
          "speaker_id": "2",
          "time": "20210831 18:14"
        },
        {
          "id": "MDRW2100024757.1.7",
          "form": "강릉!",
          "original_form": "강릉!",
          "speaker_id": "1",
          "time": "20210831 18:14"
        },
        {
          "id": "MDRW2100024757.1.8",
          "form": "강릉은 정말 경치가 좋더라구요",
          "original_form": "강릉은 정말 경치가 좋더라구요",
          "speaker_id": "1",
          "time": "20210831 18:15"
        },
        {
          "id": "MDRW2100024757.1.9",
          "form": "바다도 보고 맛집도 있고해서 자주 가게 되더라구요",
          "original_form": "바다도 보고 맛집도 있고해서 자주 가게 되더라구요",
          "speaker_id": "2",
          "time": "20210831 18:45"
        },
        {
          "id": "MDRW2100024757.1.10",
          "form": "강릉시장도 좋아해요",
          "original_form": "강릉시장도 좋아해요",
          "speaker_id": "2",
          "time": "20210831 18:45"
        },
        {
          "id": "MDRW2100024757.1.11",
          "form": "강릉은 꼭 먹어봐야하는 음식이 있나요?",
          "original_form": "강릉은 꼭 먹어봐야하는 음식이 있나요?",
          "speaker_id": "1",
          "time": "20210831 18:47"
        },
        {
          "id": "MDRW2100024757.1.12",
          "form": "저는 시장에서는 닭강정과 마늘빵을 사먹고",
          "original_form": "저는 시장에서는 닭강정과 마늘빵을 사먹고",
          "speaker_id": "2",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100024757.1.13",
          "form": "꼬막비빔밥과 물회도 잘 사먹어요",
          "original_form": "꼬막비빔밥과 물회도 잘 사먹어요",
          "speaker_id": "2",
          "time": "20210831 18:49"
        },
        {
          "id": "MDRW2100024757.1.14",
          "form": "우와 엄청 맛있겠네요!",
          "original_form": "우와 엄청 맛있겠네요!",
          "speaker_id": "1",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024757.1.15",
          "form": "꼬막비빔밤은 강릉에서도 자주 먹는 음식인가요?",
          "original_form": "꼬막비빔밤은 강릉에서도 자주 먹는 음식인가요?",
          "speaker_id": "1",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024757.1.16",
          "form": "엄지네 꼬막비빔밥드셔보셨나요?",
          "original_form": "엄지네 꼬막비빔밥드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210831 18:54"
        },
        {
          "id": "MDRW2100024757.1.17",
          "form": "아뇨ㅜ\r\n전 벌교쪽에서만 꼬막비빔밥을 먹어봤어요",
          "original_form": "아뇨ㅜ\r\n전 벌교쪽에서만 꼬막비빔밥을 먹어봤어요",
          "speaker_id": "1",
          "time": "20210831 19:16"
        },
        {
          "id": "MDRW2100024757.1.18",
          "form": "제주도 가본곳중 좋았던곳있나요?",
          "original_form": "제주도 가본곳중 좋았던곳있나요?",
          "speaker_id": "2",
          "time": "20210831 19:26"
        },
        {
          "id": "MDRW2100024757.1.19",
          "form": "우도도 가보려고하는데 비가와서 걱정이예요",
          "original_form": "우도도 가보려고하는데 비가와서 걱정이예요",
          "speaker_id": "2",
          "time": "20210831 19:56"
        },
        {
          "id": "MDRW2100024757.1.20",
          "form": "전 제주도는 많이 안가봤는데\r\n성산일출봉옆에 카페에 밖에 앉아서 바다 보는데",
          "original_form": "전 제주도는 많이 안가봤는데\r\n성산일출봉옆에 카페에 밖에 앉아서 바다 보는데",
          "speaker_id": "1",
          "time": "20210831 20:06"
        },
        {
          "id": "MDRW2100024757.1.21",
          "form": "돌고래떼가 지나가더라구요",
          "original_form": "돌고래떼가 지나가더라구요",
          "speaker_id": "1",
          "time": "20210831 20:06"
        },
        {
          "id": "MDRW2100024757.1.22",
          "form": "그게 정말 기억에 오래남더라구요ㅎ",
          "original_form": "그게 정말 기억에 오래남더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210831 20:06"
        }
      ]
    }
