        },
        {
          "id": "MDRW2100032545.1.2",
          "form": "완전 좋아해요ㅋㅋ",
          "original_form": "완전 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:29"
        },
        {
          "id": "MDRW2100032545.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 20:29"
        },
        {
          "id": "MDRW2100032545.1.4",
          "form": "우앜ㅋㅋ 저는 안먹어요 ㅋㅋㅋㅋㅋ",
          "original_form": "우앜ㅋㅋ 저는 안먹어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032545.1.5",
          "form": "베라가면 어떤거 드세요?",
          "original_form": "베라가면 어떤거 드세요?",
          "speaker_id": "1",
          "time": "20210905 20:45"
        },
        {
          "id": "MDRW2100032545.1.6",
          "form": "반민초단이세요?ㅋㅋㅋㅋ",
          "original_form": "반민초단이세요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100032545.1.7",
          "form": "무조건 민초먹어요 그리구 뉴치케",
          "original_form": "무조건 민초먹어요 그리구 뉴치케",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100032545.1.8",
          "form": "바람사",
          "original_form": "바람사",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100032545.1.9",
          "form": "어떤거드세요?",
          "original_form": "어떤거드세요?",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100032545.1.10",
          "form": "와 저랑 똑같아요 바람사!!!",
          "original_form": "와 저랑 똑같아요 바람사!!!",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100032545.1.11",
          "form": "저는 초코 나무슢 먹어욬ㅋㅋ",
          "original_form": "저는 초코 나무슢 먹어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100032545.1.12",
          "form": "녹차 좋아하세여?",
          "original_form": "녹차 좋아하세여?",
          "speaker_id": "1",
          "time": "20210905 20:49"
        },
        {
          "id": "MDRW2100032545.1.13",
          "form": "녹차..호도 불호도 아니에요ㅋㅋ",
          "original_form": "녹차..호도 불호도 아니에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:53"
        },
        {
          "id": "MDRW2100032545.1.14",
          "form": "초코나무숲 도전해볼까요",
          "original_form": "초코나무숲 도전해볼까요",
          "speaker_id": "2",
          "time": "20210905 20:53"
        },
        {
          "id": "MDRW2100032545.1.15",
          "form": "네네!! 초코랑 녹차랑 잘 어울려요 ㅋㅋ",
          "original_form": "네네!! 초코랑 녹차랑 잘 어울려요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100032545.1.16",
          "form": "여름엔 베라 자주 갔는데",
          "original_form": "여름엔 베라 자주 갔는데",
          "speaker_id": "1",
          "time": "20210905 21:01"
        },
        {
          "id": "MDRW2100032545.1.17",
          "form": "여기서도 베라깊콘받으며 가려구요ㅋㅋ",
          "original_form": "여기서도 베라깊콘받으며 가려구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:19"
        },
        {
          "id": "MDRW2100032545.1.18",
          "form": "늘먹던걸 먹을까 새로운걸 먹어볼까 항상고민",
          "original_form": "늘먹던걸 먹을까 새로운걸 먹어볼까 항상고민",
          "speaker_id": "2",
          "time": "20210905 21:19"
        },
        {
          "id": "MDRW2100032545.1.19",
          "form": "신제품 나오나봐요 1!",
          "original_form": "신제품 나오나봐요 1!",
          "speaker_id": "1",
          "time": "20210905 21:33"
        },
        {
          "id": "MDRW2100032545.1.20",
          "form": "저는 안전하게 먹던걸로 먹어요 ㅋㅋ",
          "original_form": "저는 안전하게 먹던걸로 먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:33"
        },
        {
          "id": "MDRW2100032545.1.21",
          "form": "그래요? 궁금한데",
          "original_form": "그래요? 궁금한데",
          "speaker_id": "2",
          "time": "20210905 21:34"
        },
        {
          "id": "MDRW2100032545.1.22",
          "form": "전 맨날 도전했다가 후회해요ㅋㅋ",
          "original_form": "전 맨날 도전했다가 후회해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:35"
        },
        {
          "id": "MDRW2100032545.1.23",
          "form": "오 저는 신제품은 도전하기 겁나요 ㅋㅋㅋㅋ",
          "original_form": "오 저는 신제품은 도전하기 겁나요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032545.1.24",
          "form": "편의점이 많이 나와요 ㅋㅋㅋ 콜라보해서",
          "original_form": "편의점이 많이 나와요 ㅋㅋㅋ 콜라보해서",
          "speaker_id": "1",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100032545.1.25",
          "form": "편의점은 갈때마다 새로운게 있는거같아요ㅋㅋ",
          "original_form": "편의점은 갈때마다 새로운게 있는거같아요ㅋㅋ",
