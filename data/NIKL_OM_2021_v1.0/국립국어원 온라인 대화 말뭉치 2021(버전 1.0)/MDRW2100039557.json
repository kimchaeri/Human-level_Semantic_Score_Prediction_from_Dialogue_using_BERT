        },
        {
          "id": "MDRW2100039557.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210903 19:02"
        },
        {
          "id": "MDRW2100039557.1.3",
          "form": "저는 청소기 돌리고 빨래 개는 것만 해요ㅎ..ㅎ",
          "original_form": "저는 청소기 돌리고 빨래 개는 것만 해요ㅎ..ㅎ",
          "speaker_id": "1",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039557.1.4",
          "form": "요리는 너무 못해요ㅠ",
          "original_form": "요리는 너무 못해요ㅠ",
          "speaker_id": "1",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039557.1.5",
          "form": "그래서 자취할 때 거의 다 사먹었답니다ㅋㅋㅋ",
          "original_form": "그래서 자취할 때 거의 다 사먹었답니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 19:03"
        },
        {
          "id": "MDRW2100039557.1.6",
          "form": "헉..ㅋㅋ 돈 많이 깨졌겠는데요",
          "original_form": "헉..ㅋㅋ 돈 많이 깨졌겠는데요",
          "speaker_id": "2",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100039557.1.7",
          "form": "네ㅠㅜㅜ",
          "original_form": "네ㅠㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100039557.1.8",
          "form": "배달음식 돈이 넘 많이 나가요ㅜ",
          "original_form": "배달음식 돈이 넘 많이 나가요ㅜ",
          "speaker_id": "1",
          "time": "20210903 19:05"
        },
        {
          "id": "MDRW2100039557.1.9",
          "form": "자취하면 약간 배달 시키면 너무 많거나 하진  않으셨어요?",
          "original_form": "자취하면 약간 배달 시키면 너무 많거나 하진  않으셨어요?",
          "speaker_id": "2",
          "time": "20210903 19:07"
        },
        {
          "id": "MDRW2100039557.1.10",
          "form": "아 많아서 항상 그 다음날도 먹거나 했어요!!",
          "original_form": "아 많아서 항상 그 다음날도 먹거나 했어요!!",
          "speaker_id": "1",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100039557.1.11",
          "form": "그래서 친구들이랑 살때가 정말 좋았죠ㅎㅎ",
          "original_form": "그래서 친구들이랑 살때가 정말 좋았죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100039557.1.12",
          "form": "음식 나눠먹을 수 있고 혼자 밥 안먹어도 되는게 진짜 좋았어요ㅠ",
          "original_form": "음식 나눠먹을 수 있고 혼자 밥 안먹어도 되는게 진짜 좋았어요ㅠ",
          "speaker_id": "1",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100039557.1.13",
          "form": "그런건 정말 좋을것 같아요 ㅠ",
          "original_form": "그런건 정말 좋을것 같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039557.1.14",
          "form": "같은거 연속으로 먹으면 물리지 않으셧나요 ㅠ",
          "original_form": "같은거 연속으로 먹으면 물리지 않으셧나요 ㅠ",
          "speaker_id": "2",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039557.1.15",
          "form": "엄청 물리죠ㅜㅠㅠ",
          "original_form": "엄청 물리죠ㅜㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 19:12"
        },
        {
          "id": "MDRW2100039557.1.16",
          "form": "그래서 지금 가족들이랑 사니까 좋긴해요ㅎㅎ",
          "original_form": "그래서 지금 가족들이랑 사니까 좋긴해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:12"
        },
        {
