      "utterance": [
        {
          "id": "MDRW2100017308.1.1",
          "form": "학생인가요?",
          "original_form": "학생인가요?",
          "speaker_id": "1",
          "time": "20210826 18:54"
        },
        {
          "id": "MDRW2100017308.1.2",
          "form": "아뇨 직장인입니다~",
          "original_form": "아뇨 직장인입니다~",
          "speaker_id": "2",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017308.1.3",
          "form": "퇴근 하셨나요?",
          "original_form": "퇴근 하셨나요?",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017308.1.4",
          "form": "퇴근중이에요~ name1님은 학생이신가요?",
          "original_form": "퇴근중이에요~ &name1&님은 학생이신가요?",
          "speaker_id": "2",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017308.1.5",
          "form": "아니요 졸업한 지 꽤 되었고",
          "original_form": "아니요 졸업한 지 꽤 되었고",
          "speaker_id": "1",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100017308.1.6",
          "form": "이직 준비 하고 있어요",
          "original_form": "이직 준비 하고 있어요",
          "speaker_id": "1",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100017308.1.7",
          "form": "아~ 이직하는 이유가 뭔가요?",
          "original_form": "아~ 이직하는 이유가 뭔가요?",
          "speaker_id": "2",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017308.1.8",
          "form": "전 해외 근무 하다가 한국 왔거든요",
          "original_form": "전 해외 근무 하다가 한국 왔거든요",
          "speaker_id": "1",
          "time": "20210826 20:45"
        },
        {
          "id": "MDRW2100017308.1.9",
          "form": "그래서 자연스럽게 이직이에요",
          "original_form": "그래서 자연스럽게 이직이에요",
          "speaker_id": "1",
          "time": "20210826 20:46"
        },
        {
          "id": "MDRW2100017308.1.10",
          "form": "아~ 직종은 동일하게 하시는건가요?",
          "original_form": "아~ 직종은 동일하게 하시는건가요?",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017308.1.11",
          "form": "아마 그럴지도 몰라요",
          "original_form": "아마 그럴지도 몰라요",
          "speaker_id": "1",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100017308.1.12",
          "form": "name2님은 어떤 직무인가요?",
          "original_form": "&name2&님은 어떤 직무인가요?",
          "speaker_id": "1",
          "time": "20210826 20:56"
        },
        {
          "id": "MDRW2100017308.1.13",
          "form": "저는 사무직이요~ 이전에는 연구직이었다가 변경했어요",
          "original_form": "저는 사무직이요~ 이전에는 연구직이었다가 변경했어요",
          "speaker_id": "2",
          "time": "20210827 08:21"
        },
        {
          "id": "MDRW2100017308.1.14",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100017308.1.15",
          "form": "변경한 직무는 괜찮나요? 저도 사무직이었어요",
          "original_form": "변경한 직무는 괜찮나요? 저도 사무직이었어요",
          "speaker_id": "1",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100017308.1.16",
          "form": "변경한 지 3개월밖에 안되긴했지만 아직까진 괜찮아요ㅎㅎ",
          "original_form": "변경한 지 3개월밖에 안되긴했지만 아직까진 괜찮아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 08:32"
        },
        {
          "id": "MDRW2100017308.1.17",
          "form": "두 직무가 다른가요?",
          "original_form": "두 직무가 다른가요?",
          "speaker_id": "1",
          "time": "20210827 08:37"
        },
        {
          "id": "MDRW2100017308.1.18",
          "form": "연구직은 실험을 주로 했었고 사무직은 책상에서 컴퓨터로만 업무를 하는게 가장 큰 차이같아요~",
          "original_form": "연구직은 실험을 주로 했었고 사무직은 책상에서 컴퓨터로만 업무를 하는게 가장 큰 차이같아요~",
          "speaker_id": "2",
          "time": "20210827 08:38"
        }
      ]
