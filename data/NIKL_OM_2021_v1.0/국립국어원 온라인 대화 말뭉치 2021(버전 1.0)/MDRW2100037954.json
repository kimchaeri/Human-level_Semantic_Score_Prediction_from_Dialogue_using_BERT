      "utterance": [
        {
          "id": "MDRW2100037954.1.1",
          "form": "어떤 음식 좋아하세요?",
          "original_form": "어떤 음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 21:32"
        },
        {
          "id": "MDRW2100037954.1.2",
          "form": "전 곱창이여 ㅎㅎ",
          "original_form": "전 곱창이여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100037954.1.3",
          "form": "저도 곱창 막창",
          "original_form": "저도 곱창 막창",
          "speaker_id": "2",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100037954.1.4",
          "form": "부속 쪽 좋아합니다ㅋㅋ",
          "original_form": "부속 쪽 좋아합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100037954.1.5",
          "form": "악 저도여 ㅋㅋ",
          "original_form": "악 저도여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100037954.1.6",
          "form": "많이 먹을수 잇는데 비싼요 ㅜㅠ",
          "original_form": "많이 먹을수 잇는데 비싼요 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100037954.1.7",
          "form": "맞아요~곗돈으로 먹으러 가야해요 ㅋㅋㅋ",
          "original_form": "맞아요~곗돈으로 먹으러 가야해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100037954.1.8",
          "form": "양은 왜케 조금 주는지  훌쩍 ㅋ",
          "original_form": "양은 왜케 조금 주는지  훌쩍 ㅋ",
          "speaker_id": "1",
          "time": "20210901 22:14"
        },
        {
          "id": "MDRW2100037954.1.9",
          "form": "맞아요 그래서 더 먹고 싶어요ㅋㅋㅋ",
          "original_form": "맞아요 그래서 더 먹고 싶어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100037954.1.10",
          "form": "똑같은 마음",
          "original_form": "똑같은 마음",
          "speaker_id": "2",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100037954.1.11",
          "form": "그래서 가끔은  순두부찌개로 (곱창가)  아쉬움을 달래기도  엉엉",
          "original_form": "그래서 가끔은  순두부찌개로 (곱창가)  아쉬움을 달래기도  엉엉",
          "speaker_id": "1",
          "time": "20210901 22:20"
        },
        {
          "id": "MDRW2100037954.1.12",
          "form": "뭐든 맛있게 먹음 됩니다ㅋㅋㅋㅋ",
          "original_form": "뭐든 맛있게 먹음 됩니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100037954.1.13",
          "form": "전근데 모든 게 맛있어요ㅋㅋㅋ",
          "original_form": "전근데 모든 게 맛있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100037954.1.14",
          "form": "ㅋㅋㅋㅋ 저는 근데 회는 못먹어여ㅠㅋ",
          "original_form": "ㅋㅋㅋㅋ 저는 근데 회는 못먹어여ㅠㅋ",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100037954.1.15",
          "form": "저도요!!!",
          "original_form": "저도요!!!",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100037954.1.16",
          "form": "유일하게 못 먹는 게 회에요ㅋㅋㅋ",
          "original_form": "유일하게 못 먹는 게 회에요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100037954.1.17",
          "form": "어멋 ㅋㅋㅋ",
          "original_form": "어멋 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100037954.1.18",
          "form": "오오오 뭐징 신기해요",
          "original_form": "오오오 뭐징 신기해요",
          "speaker_id": "1",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100037954.1.19",
          "form": "부속 좋아하는 사람들은 원래 해도 좋아하는데ㅋㅋㅋㅋ",
          "original_form": "부속 좋아하는 사람들은 원래 해도 좋아하는데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100037954.1.20",
          "form": "회",
          "original_form": "회",
          "speaker_id": "2",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100037954.1.21",
          "form": "신기하네 용ㅋㅋㅋ",
          "original_form": "신기하네 용ㅋㅋㅋ",
