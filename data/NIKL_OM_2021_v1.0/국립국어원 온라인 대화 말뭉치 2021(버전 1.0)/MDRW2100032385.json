        },
        {
          "id": "MDRW2100032385.1.2",
          "form": "사람들 가르치는일요",
          "original_form": "사람들 가르치는일요",
          "speaker_id": "1",
          "time": "20210905 16:42"
        },
        {
          "id": "MDRW2100032385.1.3",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "2",
          "time": "20210905 16:42"
        },
        {
          "id": "MDRW2100032385.1.4",
          "form": "운동요",
          "original_form": "운동요",
          "speaker_id": "1",
          "time": "20210905 16:42"
        },
        {
          "id": "MDRW2100032385.1.5",
          "form": "야구?축구?배구?등등 뭘까요?",
          "original_form": "야구?축구?배구?등등 뭘까요?",
          "speaker_id": "2",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100032385.1.6",
          "form": "헬스예요",
          "original_form": "헬스예요",
          "speaker_id": "1",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100032385.1.7",
          "form": "아..살빼주는",
          "original_form": "아..살빼주는",
          "speaker_id": "2",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100032385.1.8",
          "form": "요즘은 pt라 하죠",
          "original_form": "요즘은 pt라 하죠",
          "speaker_id": "1",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100032385.1.9",
          "form": "개인교습인가요?",
          "original_form": "개인교습인가요?",
          "speaker_id": "2",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100032385.1.10",
          "form": "네.그런개념이예요",
          "original_form": "네.그런개념이예요",
          "speaker_id": "1",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100032385.1.11",
          "form": "살빠져요?",
          "original_form": "살빠져요?",
          "speaker_id": "2",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100032385.1.12",
          "form": "네..20키로까지빼시는분도계세요",
