      "utterance": [
        {
          "id": "MDRW2100002643.1.1",
          "form": "요즘 하는 게임 있어?",
          "original_form": "요즘 하는 게임 있어?",
          "speaker_id": "1",
          "time": "20210820 14:14"
        },
        {
          "id": "MDRW2100002643.1.2",
          "form": "모바일 게임은 하는거 있어",
          "original_form": "모바일 게임은 하는거 있어",
          "speaker_id": "2",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002643.1.3",
          "form": "뭐 하는데??",
          "original_form": "뭐 하는데??",
          "speaker_id": "1",
          "time": "20210820 14:15"
        },
        {
          "id": "MDRW2100002643.1.4",
          "form": "마이홈이랑 랜덤다이스 많이 해",
          "original_form": "마이홈이랑 랜덤다이스 많이 해",
          "speaker_id": "2",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002643.1.5",
          "form": "뭐여뭐야 랜덤다이스 한다고??",
          "original_form": "뭐여뭐야 랜덤다이스 한다고??",
          "speaker_id": "1",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002643.1.6",
          "form": "말해주지!",
          "original_form": "말해주지!",
          "speaker_id": "1",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002643.1.7",
          "form": "나도 그거 꽤 했었는데",
          "original_form": "나도 그거 꽤 했었는데",
          "speaker_id": "1",
          "time": "20210820 14:16"
        },
        {
          "id": "MDRW2100002643.1.8",
          "form": "아 진짜?",
          "original_form": "아 진짜?",
          "speaker_id": "2",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002643.1.9",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002643.1.10",
          "form": "나 이거 동생이 하길래 재밌어보여서 하게 됐어",
          "original_form": "나 이거 동생이 하길래 재밌어보여서 하게 됐어",
          "speaker_id": "2",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002643.1.11",
          "form": "할때 불러~ 같이 하자",
          "original_form": "할때 불러~ 같이 하자",
          "speaker_id": "1",
          "time": "20210820 14:17"
        },
        {
          "id": "MDRW2100002643.1.12",
          "form": "그래 좋아",
          "original_form": "그래 좋아",
          "speaker_id": "2",
          "time": "20210820 14:18"
        },
        {
          "id": "MDRW2100002643.1.13",
          "form": "너는 또 무슨 게임 하는데?",
          "original_form": "너는 또 무슨 게임 하는데?",
          "speaker_id": "2",
          "time": "20210820 14:18"
        },
        {
          "id": "MDRW2100002643.1.14",
          "form": "나 이번에 나온 코노스바라고 그거랑 리제로 하고 있어",
          "original_form": "나 이번에 나온 코노스바라고 그거랑 리제로 하고 있어",
          "speaker_id": "1",
          "time": "20210820 14:18"
        },
        {
          "id": "MDRW2100002643.1.15",
          "form": "아 광고로 본적있어",
          "original_form": "아 광고로 본적있어",
          "speaker_id": "2",
          "time": "20210820 14:19"
        },
        {
          "id": "MDRW2100002643.1.16",
          "form": "리제로보단 이번에 나온 코노스바가 되게 웃기고 재밌더라",
          "original_form": "리제로보단 이번에 나온 코노스바가 되게 웃기고 재밌더라",
          "speaker_id": "1",
          "time": "20210820 14:20"
        },
        {
          "id": "MDRW2100002643.1.17",
          "form": "오 다음에 해봐야겠다",
          "original_form": "오 다음에 해봐야겠다",
          "speaker_id": "2",
          "time": "20210820 14:21"
        },
        {
          "id": "MDRW2100002643.1.18",
          "form": "맞다 너 마이홈도 한다고 했는데 언제부터 했었어?",
          "original_form": "맞다 너 마이홈도 한다고 했는데 언제부터 했었어?",
          "speaker_id": "1",
          "time": "20210820 14:22"
        },
        {
          "id": "MDRW2100002643.1.19",
          "form": "고등학교때 잠깐하고 재미없어서 삭제했다가",
          "original_form": "고등학교때 잠깐하고 재미없어서 삭제했다가",
          "speaker_id": "2",
          "time": "20210820 14:22"
        },
        {
          "id": "MDRW2100002643.1.20",
          "form": "대학교 와서 쭉 하고 있어",
          "original_form": "대학교 와서 쭉 하고 있어",
          "speaker_id": "2",
          "time": "20210820 14:22"
        },
        {
          "id": "MDRW2100002643.1.21",
          "form": "와 열정 엄청나네",
          "original_form": "와 열정 엄청나네",
          "speaker_id": "1",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002643.1.22",
          "form": "어떻게 그걸 계속 하고 있데",
          "original_form": "어떻게 그걸 계속 하고 있데",
          "speaker_id": "1",
          "time": "20210820 14:24"
        },
        {
          "id": "MDRW2100002643.1.23",
          "form": "그냥 출석만 하고 있어ㅋㅋ",
          "original_form": "그냥 출석만 하고 있어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 14:25"
        }
      ]
