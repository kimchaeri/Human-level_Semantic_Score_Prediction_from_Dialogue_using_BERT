      "utterance": [
        {
          "id": "MDRW2100016806.1.1",
          "form": "반가워요",
          "original_form": "반가워요",
          "speaker_id": "1",
          "time": "20210826 15:29"
        },
        {
          "id": "MDRW2100016806.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100016806.1.3",
          "form": "커뮤니티라..",
          "original_form": "커뮤니티라..",
          "speaker_id": "2",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100016806.1.4",
          "form": "커뮤니티 많이 하세요?",
          "original_form": "커뮤니티 많이 하세요?",
          "speaker_id": "1",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016806.1.5",
          "form": "어 많이의 기준이 애매하긴 하지만ㅋㅋㅋ",
          "original_form": "어 많이의 기준이 애매하긴 하지만ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100016806.1.6",
          "form": "하긴합니다 옷 보는거 좋아해가지구요",
          "original_form": "하긴합니다 옷 보는거 좋아해가지구요",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016806.1.7",
          "form": "막 사고 그런건 아닌데",
          "original_form": "막 사고 그런건 아닌데",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016806.1.8",
          "form": "보기만 봐요ㅋㅋㅋ",
          "original_form": "보기만 봐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016806.1.9",
          "form": "어디 하세요?? 무신사같은곳인가??",
          "original_form": "어디 하세요?? 무신사같은곳인가??",
          "speaker_id": "1",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016806.1.10",
          "form": "카페에서 봐요ㅋㅋㅋ",
          "original_form": "카페에서 봐요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016806.1.11",
          "form": "아아!",
          "original_form": "아아!",
          "speaker_id": "1",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100016806.1.12",
          "form": "커뮤하면서 오프라인으로도 많이 만나시나요??",
          "original_form": "커뮤하면서 오프라인으로도 많이 만나시나요??",
          "speaker_id": "1",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100016806.1.13",
          "form": "아뇨ㅋㅋ 그런건 좋아하진 않아요",
          "original_form": "아뇨ㅋㅋ 그런건 좋아하진 않아요",
          "speaker_id": "2",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016806.1.14",
          "form": "정모는 게임하면서 두세번? 가봤네요ㅋㅋㅋ",
          "original_form": "정모는 게임하면서 두세번? 가봤네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016806.1.15",
          "form": "오 정모하면 어때요??",
          "original_form": "오 정모하면 어때요??",
          "speaker_id": "1",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016806.1.16",
          "form": "저도 길드가입은해봤는데",
          "original_form": "저도 길드가입은해봤는데",
          "speaker_id": "1",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016806.1.17",
          "form": "정모는안해봐서",
          "original_form": "정모는안해봐서",
          "speaker_id": "1",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016806.1.18",
          "form": "궁금해요",
          "original_form": "궁금해요",
          "speaker_id": "1",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016806.1.19",
          "form": "음... 뭐 모든 사람이 맘에 들 순 없더라구요ㅋㅋㅋㅋ",
          "original_form": "음... 뭐 모든 사람이 맘에 들 순 없더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016806.1.20",
          "form": "온라인과 오프라인이 다른 사람도 있었고",
          "original_form": "온라인과 오프라인이 다른 사람도 있었고",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016806.1.21",
          "form": "대단한 사람도 있었고 자존감만 뿜뿜인 분도 계셨고",
          "original_form": "대단한 사람도 있었고 자존감만 뿜뿜인 분도 계셨고",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016806.1.22",
          "form": "ㅋㅋㅋ그쵸 어딜가도 사람이 다 비슷하죠",
          "original_form": "ㅋㅋㅋ그쵸 어딜가도 사람이 다 비슷하죠",
          "speaker_id": "1",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016806.1.23",
          "form": "잘 맘 맞으면 괜찮긴 하드라구요",
          "original_form": "잘 맘 맞으면 괜찮긴 하드라구요",
          "speaker_id": "2",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016806.1.24",
          "form": "그래도 정모로 만나서 꽤 친해지는 경우도 있더라구요",
          "original_form": "그래도 정모로 만나서 꽤 친해지는 경우도 있더라구요",
          "speaker_id": "1",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016806.1.25",
          "form": "대신 길드 이런걸로 가서 만나면 파벌 싸움도 나고..",
          "original_form": "대신 길드 이런걸로 가서 만나면 파벌 싸움도 나고..",
          "speaker_id": "2",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016806.1.26",
          "form": "뭐라해야하지",
          "original_form": "뭐라해야하지",
          "speaker_id": "2",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016806.1.27",
          "form": "앗ㅜ",
          "original_form": "앗ㅜ",
          "speaker_id": "1",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016806.1.28",
          "form": "박쥐??",
          "original_form": "박쥐??",
          "speaker_id": "2",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016806.1.29",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016806.1.30",
          "form": "막뒷담하는",
          "original_form": "막뒷담하는",
          "speaker_id": "1",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016806.1.31",
          "form": "그런사람인가",
          "original_form": "그런사람인가",
          "speaker_id": "1",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016806.1.32",
          "form": "괜히 길드 분파시키고 막",
          "original_form": "괜히 길드 분파시키고 막",
          "speaker_id": "2",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016806.1.33",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016806.1.34",
          "form": "다른 길드가자 그러고ㅋㅋㅋ",
          "original_form": "다른 길드가자 그러고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016806.1.35",
          "form": "그런 경우도 있었어요",
          "original_form": "그런 경우도 있었어요",
          "speaker_id": "2",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016806.1.36",
          "form": "진짜 어딜가도 분탕종자는 있는듯..",
          "original_form": "진짜 어딜가도 분탕종자는 있는듯..",
          "speaker_id": "1",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016806.1.37",
          "form": "짜증나셨겠어요!",
          "original_form": "짜증나셨겠어요!",
          "speaker_id": "1",
          "time": "20210826 15:52"
        }
      ]
    }
