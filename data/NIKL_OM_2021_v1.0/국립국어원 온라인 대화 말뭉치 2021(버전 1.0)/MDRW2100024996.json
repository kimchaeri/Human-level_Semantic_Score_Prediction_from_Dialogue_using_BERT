      "utterance": [
        {
          "id": "MDRW2100024996.1.1",
          "form": "근데 그거 먹어본 사람들이",
          "original_form": "근데 그거 먹어본 사람들이",
          "speaker_id": "1",
          "time": "20210831 22:00"
        },
        {
          "id": "MDRW2100024996.1.2",
          "form": "베트남엔 떡이 없으니까 먹는 거고 떡 있으면 떡이나 먹어라",
          "original_form": "베트남엔 떡이 없으니까 먹는 거고 떡 있으면 떡이나 먹어라",
          "speaker_id": "1",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024996.1.3",
          "form": "라고 하던데요!",
          "original_form": "라고 하던데요!",
          "speaker_id": "1",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024996.1.4",
          "form": "그런가요?? 또 어디선 완전 쫄깃한 식감이라서",
          "original_form": "그런가요?? 또 어디선 완전 쫄깃한 식감이라서",
          "speaker_id": "2",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024996.1.5",
          "form": "맛있다고 하던데 ㅋㄱㅋㅋ",
          "original_form": "맛있다고 하던데 ㅋㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024996.1.6",
          "form": "저도한번 해먹어볼까 했거든요",
          "original_form": "저도한번 해먹어볼까 했거든요",
          "speaker_id": "2",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024996.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:01"
        },
        {
          "id": "MDRW2100024996.1.8",
          "form": "약간",
          "original_form": "약간",
          "speaker_id": "1",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024996.1.9",
          "form": "탄수화물이 맛없을리는 없으니",
          "original_form": "탄수화물이 맛없을리는 없으니",
          "speaker_id": "2",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024996.1.10",
          "form": "떡보다는",
          "original_form": "떡보다는",
          "speaker_id": "1",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024996.1.11",
          "form": "타피오카펄 그런 느낌인가봐요",
          "original_form": "타피오카펄 그런 느낌인가봐요",
          "speaker_id": "1",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024996.1.12",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024996.1.13",
          "form": "저는 그",
          "original_form": "저는 그",
          "speaker_id": "1",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024996.1.14",
          "form": "전분같은 느낌의 쫄깃함인가",
          "original_form": "전분같은 느낌의 쫄깃함인가",
          "speaker_id": "2",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024996.1.15",
          "form": "닭가.슴살을 라이스페이퍼로 싸서",
          "original_form": "닭가.슴살을 라이스페이퍼로 싸서",
          "speaker_id": "1",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024996.1.16",
          "form": "튀겨먹는 거 해봤ㄴ느데",
          "original_form": "튀겨먹는 거 해봤ㄴ느데",
          "speaker_id": "1",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024996.1.17",
          "form": "그건엄청맛있었어요",
          "original_form": "그건엄청맛있었어요",
          "speaker_id": "1",
          "time": "20210831 22:02"
        },
        {
          "id": "MDRW2100024996.1.18",
          "form": "말만 들어도 맛있을거같은데용",
          "original_form": "말만 들어도 맛있을거같은데용",
          "speaker_id": "2",
          "time": "20210831 22:03"
        },
        {
          "id": "MDRW2100024996.1.19",
          "form": "그쳐",
          "original_form": "그쳐",
          "speaker_id": "1",
          "time": "20210831 22:03"
        },
        {
          "id": "MDRW2100024996.1.20",
          "form": "그 다이어트용으로 나왔지만",
          "original_form": "그 다이어트용으로 나왔지만",
          "speaker_id": "1",
          "time": "20210831 22:03"
        },
        {
          "id": "MDRW2100024996.1.21",
          "form": "간이 다 된 닭가.슴살이었어요",
          "original_form": "간이 다 된 닭가.슴살이었어요",
          "speaker_id": "1",
          "time": "20210831 22:03"
        },
        {
          "id": "MDRW2100024996.1.22",
          "form": "오 그러면 딱 좋을거같아요",
          "original_form": "오 그러면 딱 좋을거같아요",
          "speaker_id": "2",
          "time": "20210831 22:03"
        },
        {
          "id": "MDRW2100024996.1.23",
          "form": "생으로 된거먹으면 퍽퍽하기만하고 그럴수있는데",
          "original_form": "생으로 된거먹으면 퍽퍽하기만하고 그럴수있는데",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100024996.1.24",
          "form": "요샌 다이어트용 닭가.슴살은 잘나와서",
          "original_form": "요샌 다이어트용 닭가.슴살은 잘나와서",
          "speaker_id": "2",
          "time": "20210831 22:04"
        },
        {
          "id": "MDRW2100024996.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100024996.1.26",
          "form": "맛도좋고 부드럽던데 튀기면 존맛일듯...",
          "original_form": "맛도좋고 부드럽던데 튀기면 존맛일듯...",
          "speaker_id": "2",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100024996.1.27",
          "form": "이게 진짜 다이어트용이맞나?",
          "original_form": "이게 진짜 다이어트용이맞나?",
          "speaker_id": "1",
          "time": "20210831 22:05"
        },
        {
          "id": "MDRW2100024996.1.28",
          "form": "싶은 것도 많은 것 같아요",
          "original_form": "싶은 것도 많은 것 같아요",
          "speaker_id": "1",
          "time": "20210831 22:05"
        }
      ]
    }
