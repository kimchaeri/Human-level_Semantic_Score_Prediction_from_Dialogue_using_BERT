      "utterance": [
        {
          "id": "MDRW2100017800.1.1",
          "form": "혹시 학교 재학중이신가요?",
          "original_form": "혹시 학교 재학중이신가요?",
          "speaker_id": "2",
          "time": "20210826 21:11"
        },
        {
          "id": "MDRW2100017800.1.2",
          "form": "아뇨! 졸업한지 2년정도 됐어요",
          "original_form": "아뇨! 졸업한지 2년정도 됐어요",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017800.1.3",
          "form": "재학중이세요?",
          "original_form": "재학중이세요?",
          "speaker_id": "1",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017800.1.4",
          "form": "아하! 네 전 재학중이에요 ㅎ",
          "original_form": "아하! 네 전 재학중이에요 ㅎ",
          "speaker_id": "2",
          "time": "20210826 21:15"
        },
        {
          "id": "MDRW2100017800.1.5",
          "form": "저는 동생이 신입생이라 가까이서 지켜보는데",
          "original_form": "저는 동생이 신입생이라 가까이서 지켜보는데",
          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017800.1.6",
          "form": "온라인수업도 장난아니더라고요...",
          "original_form": "온라인수업도 장난아니더라고요...",
          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017800.1.7",
          "form": "2학기에 오프수업하세요?",
          "original_form": "2학기에 오프수업하세요?",
          "speaker_id": "1",
          "time": "20210826 21:16"
        },
        {
          "id": "MDRW2100017800.1.8",
          "form": "그쵸 ㅠㅠ 저는 전면 비대면으로해요~",
          "original_form": "그쵸 ㅠㅠ 저는 전면 비대면으로해요~",
          "speaker_id": "2",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017800.1.9",
          "form": "좋겠다..... 이론수업 위주예요?",
          "original_form": "좋겠다..... 이론수업 위주예요?",
          "speaker_id": "1",
          "time": "20210826 21:17"
        },
        {
          "id": "MDRW2100017800.1.10",
          "form": "네 ㅋㅋ 어쩔수없는거죠뭐..",
          "original_form": "네 ㅋㅋ 어쩔수없는거죠뭐..",
          "speaker_id": "2",
          "time": "20210826 21:18"
        },
        {
          "id": "MDRW2100017800.1.11",
          "form": "동생은 실습이있어서 거의 매주 가더라고요ㅠ",
          "original_form": "동생은 실습이있어서 거의 매주 가더라고요ㅠ",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017800.1.12",
          "form": "비대면할줄알고 방을 안 구해서 시외버스 통학해야한대요..",
          "original_form": "비대면할줄알고 방을 안 구해서 시외버스 통학해야한대요..",
          "speaker_id": "1",
          "time": "20210826 21:19"
        },
        {
          "id": "MDRW2100017800.1.13",
          "form": "어머 세상에.. 너무귀찮겟다 ㅜ",
          "original_form": "어머 세상에.. 너무귀찮겟다 ㅜ",
          "speaker_id": "2",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017800.1.14",
          "form": "1학기에도 비대면했어요?",
          "original_form": "1학기에도 비대면했어요?",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017800.1.15",
          "form": "그럼 시험 다 온라인으로 봤겠네요?",
          "original_form": "그럼 시험 다 온라인으로 봤겠네요?",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017800.1.16",
          "form": "네! 다 온라인으로봤어요 ㅜ",
          "original_form": "네! 다 온라인으로봤어요 ㅜ",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017800.1.17",
          "form": "카메라로 주변에 커닝페이퍼없는지",
          "original_form": "카메라로 주변에 커닝페이퍼없는지",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017800.1.18",
          "form": "꼼꼼하게 확인했어요?",
          "original_form": "꼼꼼하게 확인했어요?",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017800.1.19",
          "form": "ㅋㅋㅋ 네 카메라 2대로 시험봐서 컨닝할겨를은 없었네요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 네 카메라 2대로 시험봐서 컨닝할겨를은 없었네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017800.1.20",
          "form": "대박... 캠 하나만 있는사람은요....?",
          "original_form": "대박... 캠 하나만 있는사람은요....?",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017800.1.21",
          "form": "진짜 그렇게 보는 과가 있구나",
          "original_form": "진짜 그렇게 보는 과가 있구나",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017800.1.22",
          "form": "ㅋㅋㅋ 아 대부분 이런줄 알았어요 ㅋㅋ 다행이도 없는사람은 없었어요 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 아 대부분 이런줄 알았어요 ㅋㅋ 다행이도 없는사람은 없었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:30"
        }
      ]
    }
