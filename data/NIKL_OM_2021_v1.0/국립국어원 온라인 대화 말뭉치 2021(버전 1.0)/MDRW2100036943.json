      "utterance": [
        {
          "id": "MDRW2100036943.1.1",
          "form": "우와 정말 여행 가고싶네요ㅠ",
          "original_form": "우와 정말 여행 가고싶네요ㅠ",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100036943.1.2",
          "form": "돈과 시간이 허락해주지 않는 여행...",
          "original_form": "돈과 시간이 허락해주지 않는 여행...",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100036943.1.3",
          "form": "국가마저 허락해주지않는... 여행...",
          "original_form": "국가마저 허락해주지않는... 여행...",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100036943.1.4",
          "form": "어디로 제일 가고싶으세요?",
          "original_form": "어디로 제일 가고싶으세요?",
          "speaker_id": "2",
          "time": "20210901 17:29"
        },
        {
          "id": "MDRW2100036943.1.5",
          "form": "저는 일단",
          "original_form": "저는 일단",
          "speaker_id": "1",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100036943.1.6",
          "form": "언니가 살고 있는 미국이요~!~!!",
          "original_form": "언니가 살고 있는 미국이요~!~!!",
          "speaker_id": "1",
          "time": "20210901 17:30"
        },
        {
          "id": "MDRW2100036943.1.7",
          "form": "미국 시골이긴 한데 시카고랑은 가깝거든요,,,",
          "original_form": "미국 시골이긴 한데 시카고랑은 가깝거든요,,,",
          "speaker_id": "1",
          "time": "20210901 17:31"
        },
        {
          "id": "MDRW2100036943.1.8",
          "form": "미국에 아는 사람이 있는 것부터 일단 부러운데요!",
          "original_form": "미국에 아는 사람이 있는 것부터 일단 부러운데요!",
          "speaker_id": "2",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100036943.1.9",
          "form": "언니가 미국에 계시는 군요ㅠㅠ",
          "original_form": "언니가 미국에 계시는 군요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100036943.1.10",
          "form": "일단 숙소는 확보네요 ㅎㅎ",
          "original_form": "일단 숙소는 확보네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100036943.1.11",
          "form": "네네ㅠ 형부 따라서 언니가 갔어요...",
          "original_form": "네네ㅠ 형부 따라서 언니가 갔어요...",
          "speaker_id": "1",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100036943.1.12",
          "form": "다들 부러워하시긴 하는데",
          "original_form": "다들 부러워하시긴 하는데",
          "speaker_id": "1",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100036943.1.13",
          "form": "뭔가,,, 비행기값이 넉넉 있지 않으면",
          "original_form": "뭔가,,, 비행기값이 넉넉 있지 않으면",
          "speaker_id": "1",
          "time": "20210901 18:16"
        },
        {
          "id": "MDRW2100036943.1.14",
          "form": "생이별하는 느낌이랄까요?!?!",
          "original_form": "생이별하는 느낌이랄까요?!?!",
          "speaker_id": "1",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100036943.1.15",
          "form": "아 그렇겠네요ㅠㅠ",
          "original_form": "아 그렇겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100036943.1.16",
          "form": "미국은 유독 머니까 또 그렇겠어요ㅠㅠ",
          "original_form": "미국은 유독 머니까 또 그렇겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 18:19"
        },
        {
          "id": "MDRW2100036943.1.17",
          "form": "맞아요 맞아요 ㅠ",
          "original_form": "맞아요 맞아요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100036943.1.18",
          "form": "비자도 발급받아야하고....!!!",
          "original_form": "비자도 발급받아야하고....!!!",
          "speaker_id": "1",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100036943.1.19",
          "form": "name2님은 여행 어디 다녀와보셨나요?",
          "original_form": "&name2&님은 여행 어디 다녀와보셨나요?",
          "speaker_id": "1",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100036943.1.20",
          "form": "유럽 돌고, 싱가포르랑 다녀왔어요!",
          "original_form": "유럽 돌고, 싱가포르랑 다녀왔어요!",
          "speaker_id": "2",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100036943.1.21",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100036943.1.22",
          "form": "우와... 진짜 많이 다녀오셨네요!!!!!",
          "original_form": "우와... 진짜 많이 다녀오셨네요!!!!!",
          "speaker_id": "1",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100036943.1.23",
          "form": "저는 해외여행은 일본밖에 안다녀왔어요!",
          "original_form": "저는 해외여행은 일본밖에 안다녀왔어요!",
          "speaker_id": "1",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100036943.1.24",
          "form": "유럽은 기차 타고 다니는거죠?!",
          "original_form": "유럽은 기차 타고 다니는거죠?!",
          "speaker_id": "1",
          "time": "20210901 19:02"
        },
        {
          "id": "MDRW2100036943.1.25",
          "form": "기차도 타고, 차 타고 이동도했고, 비행기 탔던 날도 있는 것 같아요!",
          "original_form": "기차도 타고, 차 타고 이동도했고, 비행기 탔던 날도 있는 것 같아요!",
          "speaker_id": "2",
          "time": "20210901 19:57"
        },
        {
          "id": "MDRW2100036943.1.26",
          "form": "저는 일본은 못 가봤는데 어디가 제일 좋았나요??",
          "original_form": "저는 일본은 못 가봤는데 어디가 제일 좋았나요??",
          "speaker_id": "2",
          "time": "20210901 19:57"
        },
        {
          "id": "MDRW2100036943.1.27",
          "form": "우와 진짜 좋으셨겠어요ㅠ",
          "original_form": "우와 진짜 좋으셨겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100036943.1.28",
          "form": "저는 일본은 도시를 가고싶었는데",
          "original_form": "저는 일본은 도시를 가고싶었는데",
          "speaker_id": "1",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100036943.1.29",
          "form": "학교에서 화산 탐험한다고 시골만 다니더라구요....",
          "original_form": "학교에서 화산 탐험한다고 시골만 다니더라구요....",
          "speaker_id": "1",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100036943.1.30",
          "form": "홋카이도~나가사키 이렇게 다녔어요!",
          "original_form": "홋카이도~나가사키 이렇게 다녔어요!",
          "speaker_id": "1",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100036943.1.31",
          "form": "아니 더 멋있고 좋은데요!\r\n도시여행은 패키지여행도 널렸는데, 그런 곳은 쉽게 갈 수 없잖아요!",
          "original_form": "아니 더 멋있고 좋은데요!\r\n도시여행은 패키지여행도 널렸는데, 그런 곳은 쉽게 갈 수 없잖아요!",
          "speaker_id": "2",
          "time": "20210901 20:10"
        },
        {
          "id": "MDRW2100036943.1.32",
          "form": "활화산 위주로 다니나요??",
          "original_form": "활화산 위주로 다니나요??",
          "speaker_id": "2",
          "time": "20210901 20:11"
        },
        {
          "id": "MDRW2100036943.1.33",
          "form": "그렇게 말씀해주시다니...",
          "original_form": "그렇게 말씀해주시다니...{emoji:♡}",
          "speaker_id": "1",
          "time": "20210901 20:19"
        },
        {
          "id": "MDRW2100036943.1.34",
          "form": "활화산은 좀 위험해서 그런지",
          "original_form": "활화산은 좀 위험해서 그런지",
          "speaker_id": "1",
          "time": "20210901 20:20"
        },
        {
          "id": "MDRW2100036943.1.35",
          "form": "휴화산이었어요!",
          "original_form": "휴화산이었어요!",
          "speaker_id": "1",
          "time": "20210901 20:20"
        },
        {
          "id": "MDRW2100036943.1.36",
          "form": "옛날에 과학시간에 선생님께서 휴화산이 활화산 되면 얌전히 운명을 받아들이라고 하셨던게 생각나네요 ㅋㅋㅋㅋ",
          "original_form": "옛날에 과학시간에 선생님께서 휴화산이 활화산 되면 얌전히 운명을 받아들이라고 하셨던게 생각나네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
