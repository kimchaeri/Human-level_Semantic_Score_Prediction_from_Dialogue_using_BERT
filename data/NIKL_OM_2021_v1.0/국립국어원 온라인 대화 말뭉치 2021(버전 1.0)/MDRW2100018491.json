      "utterance": [
        {
          "id": "MDRW2100018491.1.1",
          "form": "이번 추석땐 여행이고 모고 집에만 있어야겠어요",
          "original_form": "이번 추석땐 여행이고 모고 집에만 있어야겠어요",
          "speaker_id": "1",
          "time": "20210827 06:09"
        },
        {
          "id": "MDRW2100018491.1.2",
          "form": "안그래도 그럴려고",
          "original_form": "안그래도 그럴려고",
          "speaker_id": "2",
          "time": "20210827 06:09"
        },
        {
          "id": "MDRW2100018491.1.3",
          "form": "저도 집에서 쉬려구요",
          "original_form": "저도 집에서 쉬려구요",
          "speaker_id": "1",
          "time": "20210827 06:10"
        },
        {
          "id": "MDRW2100018491.1.4",
          "form": "지금 확진자가 너모 많다",
          "original_form": "지금 확진자가 너모 많다",
          "speaker_id": "2",
          "time": "20210827 07:03"
        },
        {
          "id": "MDRW2100018491.1.5",
          "form": "그러게요 그니까 이번에 어디 가지 말고 집콕 하세요",
          "original_form": "그러게요 그니까 이번에 어디 가지 말고 집콕 하세요",
          "speaker_id": "1",
          "time": "20210827 07:04"
        },
        {
          "id": "MDRW2100018491.1.6",
          "form": "작년에도못모였는데아쉽네",
          "original_form": "작년에도못모였는데아쉽네",
          "speaker_id": "2",
          "time": "20210827 07:05"
        },
        {
          "id": "MDRW2100018491.1.7",
          "form": "할소없죠 대신 얘전에 여행다녔더 사진 다시 보면서 기억을 떠올려보는 건 어떨까요?",
          "original_form": "할소없죠 대신 얘전에 여행다녔더 사진 다시 보면서 기억을 떠올려보는 건 어떨까요?",
          "speaker_id": "1",
          "time": "20210827 07:05"
        },
        {
          "id": "MDRW2100018491.1.8",
          "form": "그것도 좋은 방법 이네",
          "original_form": "그것도 좋은 방법 이네",
          "speaker_id": "2",
          "time": "20210827 07:06"
        },
        {
          "id": "MDRW2100018491.1.9",
          "form": "전 요즘 예전 여행사진 보는데  이렇게 마스크 없이 다녔구나 싶었어요",
          "original_form": "전 요즘 예전 여행사진 보는데  이렇게 마스크 없이 다녔구나 싶었어요",
          "speaker_id": "1",
          "time": "20210827 07:06"
        },
        {
          "id": "MDRW2100018491.1.10",
          "form": "마스크 쓰고 라도 다니면 좋을텐데 그것도 요즘은 위험하니까",
          "original_form": "마스크 쓰고 라도 다니면 좋을텐데 그것도 요즘은 위험하니까",
          "speaker_id": "2",
          "time": "20210827 07:07"
        },
        {
          "id": "MDRW2100018491.1.11",
          "form": "맞아요 델타는 마스크 써도 감염된대요",
          "original_form": "맞아요 델타는 마스크 써도 감염된대요",
          "speaker_id": "1",
          "time": "20210827 07:08"
        },
        {
          "id": "MDRW2100018491.1.12",
          "form": "단풍구경 너랑같이갔을때가 생각나는 구나",
          "original_form": "단풍구경 너랑같이갔을때가 생각나는 구나",
          "speaker_id": "2",
          "time": "20210827 07:09"
        },
        {
          "id": "MDRW2100018491.1.13",
          "form": "내장산가갔었죠",
          "original_form": "내장산가갔었죠",
          "speaker_id": "1",
          "time": "20210827 07:09"
        },
        {
          "id": "MDRW2100018491.1.14",
          "form": "재작년에 뉴욕 다녀오길 잘했어요",
          "original_form": "재작년에 뉴욕 다녀오길 잘했어요",
          "speaker_id": "1",
