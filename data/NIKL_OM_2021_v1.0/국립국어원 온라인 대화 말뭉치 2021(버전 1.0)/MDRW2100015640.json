      "utterance": [
        {
          "id": "MDRW2100015640.1.1",
          "form": "신발이 워낙 많으니까 그런거아냐",
          "original_form": "신발이 워낙 많으니까 그런거아냐",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015640.1.2",
          "form": "그렇긴한데 그냥 버리기엔 아까워서 ... 중고로 판매할까봐",
          "original_form": "그렇긴한데 그냥 버리기엔 아까워서 ... 중고로 판매할까봐",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015640.1.3",
          "form": "무슨 신발인데? 팔릴까?",
          "original_form": "무슨 신발인데? 팔릴까?",
          "speaker_id": "2",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015640.1.4",
          "form": "그 여름용 베툴라샌들인데 예뻐~  근데 옛날에 유행했던거야",
          "original_form": "그 여름용 베툴라샌들인데 예뻐~  근데 옛날에 유행했던거야",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015640.1.5",
          "form": "여름 샌들? 이제 가을 다 되가는데 팔릴까?",
          "original_form": "여름 샌들? 이제 가을 다 되가는데 팔릴까?",
          "speaker_id": "2",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015640.1.6",
          "form": "그래서 가격을 완전 저렴하게 올려보게",
          "original_form": "그래서 가격을 완전 저렴하게 올려보게",
          "speaker_id": "1",
          "time": "20210825 23:59"
        },
        {
          "id": "MDRW2100015640.1.7",
          "form": "얼마나? 아깝지 않겠어? 마음에 들어서 샀던거일거아냐",
          "original_form": "얼마나? 아깝지 않겠어? 마음에 들어서 샀던거일거아냐",
          "speaker_id": "2",
          "time": "20210826 00:00"
        },
        {
          "id": "MDRW2100015640.1.8",
          "form": "이거 살 때 비슷한 디자인 몇켤레 샀었을거야",
          "original_form": "이거 살 때 비슷한 디자인 몇켤레 샀었을거야",
          "speaker_id": "1",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015640.1.9",
          "form": "아마 그래서 안신은듯?ㅋㅋ",
          "original_form": "아마 그래서 안신은듯?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 00:01"
        },
        {
          "id": "MDRW2100015640.1.10",
          "form": "헐 뭘 그렇게 많이샀었어... 적당히사야지",
          "original_form": "헐 뭘 그렇게 많이샀었어... 적당히사야지",
          "speaker_id": "2",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015640.1.11",
          "form": "가격은 한 만원?에 팔까봐ㅋ",
          "original_form": "가격은 한 만원?에 팔까봐ㅋ",
          "speaker_id": "1",
          "time": "20210826 00:02"
        },
        {
          "id": "MDRW2100015640.1.12",
          "form": "얼마에 구매했는데? 만원에 새제품이면 그냥 팔리겠다ㅋ",
          "original_form": "얼마에 구매했는데? 만원에 새제품이면 그냥 팔리겠다ㅋ",
          "speaker_id": "2",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015640.1.13",
          "form": "ㅋ이거 삼만원인가 사만원인가 ㅋㅋ 팔아서 과자 사먹어야징",
          "original_form": "ㅋ이거 삼만원인가 사만원인가 ㅋㅋ 팔아서 과자 사먹어야징",
          "speaker_id": "1",
          "time": "20210826 00:03"
        },
        {
          "id": "MDRW2100015640.1.14",
          "form": "그래 다음부터는 있는거 신고 그만 사~~",
          "original_form": "그래 다음부터는 있는거 신고 그만 사~~",
