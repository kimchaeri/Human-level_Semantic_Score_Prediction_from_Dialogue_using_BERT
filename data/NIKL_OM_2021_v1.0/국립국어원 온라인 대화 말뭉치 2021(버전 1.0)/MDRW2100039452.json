        {
          "id": "MDRW2100039452.1.1",
          "form": "낙새먹어욤!!",
          "original_form": "낙새먹어욤!!",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039452.1.2",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210903 17:58"
        },
        {
          "id": "MDRW2100039452.1.3",
          "form": "오 곱도 넣어쥬ㅓ요",
          "original_form": "오 곱도 넣어쥬ㅓ요",
          "speaker_id": "1",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039452.1.4",
          "form": "저는 남은 땅땅치킨  ㅜㅜ",
          "original_form": "저는 남은 땅땅치킨  ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 17:59"
        },
        {
          "id": "MDRW2100039452.1.5",
          "form": "맛있겠네요",
          "original_form": "맛있겠네요",
          "speaker_id": "2",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039452.1.6",
          "form": "언니가 곱싫어햐여!",
          "original_form": "언니가 곱싫어햐여!",
          "speaker_id": "2",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039452.1.7",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039452.1.8",
          "form": "치킨 너무 느끼해영",
          "original_form": "치킨 너무 느끼해영",
          "speaker_id": "1",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039452.1.9",
          "form": "마싯섯나요",
          "original_form": "마싯섯나요",
          "speaker_id": "1",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039452.1.10",
          "form": "넹넹맛있더르규요!!",
          "original_form": "넹넹맛있더르규요!!",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039452.1.11",
          "form": "또먹고싶어요..",
          "original_form": "또먹고싶어요..",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039452.1.12",
          "form": "볶음밥도 먹었겠죠?",
          "original_form": "볶음밥도 먹었겠죠?",
          "speaker_id": "1",
          "time": "20210903 22:44"
        },
        {
          "id": "MDRW2100039452.1.13",
          "form": "맞아요 ㅋㅋㅋㅋ 맛있더라구요!!",
          "original_form": "맞아요 ㅋㅋㅋㅋ 맛있더라구요!!",
          "speaker_id": "2",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039452.1.14",
          "form": "휴... 먹어서 다행이네요 저도 먹고싶어서요",
          "original_form": "휴... 먹어서 다행이네요 저도 먹고싶어서요",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039452.1.15",
          "form": "맛있겠죠?? 저도 또땡기네요 ㅠㅠ",
          "original_form": "맛있겠죠?? 저도 또땡기네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 09:33"
        },
        {
          "id": "MDRW2100039452.1.16",
          "form": "늦게일어나니 점심 시간이에요 ㅌㅋㅋ",
          "original_form": "늦게일어나니 점심 시간이에요 ㅌㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 11:55"
        },
        {
          "id": "MDRW2100039452.1.17",
          "form": "주말에는 늦잠이짱이죠",
          "original_form": "주말에는 늦잠이짱이죠",
          "speaker_id": "2",
          "time": "20210904 14:29"
        },
        {
          "id": "MDRW2100039452.1.18",
          "form": "맞아요 ㅁ ㅓ 드셨나요",
          "original_form": "맞아요 ㅁ ㅓ 드셨나요",
          "speaker_id": "1",
          "time": "20210906 08:46"
        },
        {
          "id": "MDRW2100039452.1.19",
          "form": "기억이안나요 ㅠ",
          "original_form": "기억이안나요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 08:46"
        },
        {
          "id": "MDRW2100039452.1.20",
          "form": "저는 룽위엔 먹었어요",
          "original_form": "저는 룽위엔 먹었어요",
          "speaker_id": "1",
          "time": "20210906 08:48"
        },
        {
          "id": "MDRW2100039452.1.21",
          "form": "맛있겠네용",
          "original_form": "맛있겠네용",
          "speaker_id": "2",
