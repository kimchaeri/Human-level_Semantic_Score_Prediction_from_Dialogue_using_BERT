      "utterance": [
        {
          "id": "MDRW2100022965.1.1",
          "form": "빨간 양념에 취향따라 초록소스요?",
          "original_form": "빨간 양념에 취향따라 초록소스요?",
          "speaker_id": "2",
          "time": "20210829 19:43"
        },
        {
          "id": "MDRW2100022965.1.2",
          "form": "고기랑 야채가 들어간단 말이죠?",
          "original_form": "고기랑 야채가 들어간단 말이죠?",
          "speaker_id": "2",
          "time": "20210829 19:43"
        },
        {
          "id": "MDRW2100022965.1.3",
          "form": "넵 제가 초록 소스 아주 좋아하지요",
          "original_form": "넵 제가 초록 소스 아주 좋아하지요",
          "speaker_id": "1",
          "time": "20210829 19:43"
        },
        {
          "id": "MDRW2100022965.1.4",
          "form": "정답! 닭가슴살 샐러드?",
          "original_form": "정답! 닭가슴살 샐러드?",
          "speaker_id": "2",
          "time": "20210829 19:44"
        },
        {
          "id": "MDRW2100022965.1.5",
          "form": "빨간 건 칠리소스 초록은 키위소스인가요?",
          "original_form": "빨간 건 칠리소스 초록은 키위소스인가요?",
          "speaker_id": "2",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022965.1.6",
          "form": "닭가슴살 샐러드에 초록색 소스가 들어가던가요? ㅋㅋㅋㅋ",
          "original_form": "닭가슴살 샐러드에 초록색 소스가 들어가던가요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022965.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ젠장",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ젠장",
          "speaker_id": "2",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022965.1.8",
          "form": "혹시 빵이 들어가나요?",
          "original_form": "혹시 빵이 들어가나요?",
          "speaker_id": "2",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022965.1.9",
          "form": "빵 비슷한 게 들어갑니다 ㅋㅋㅋㅋㅋ",
          "original_form": "빵 비슷한 게 들어갑니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022965.1.10",
          "form": "으으으으으으으으음",
          "original_form": "으으으으으으으으음",
          "speaker_id": "2",
          "time": "20210829 19:46"
        },
        {
          "id": "MDRW2100022965.1.11",
          "form": "한식인가요?",
          "original_form": "한식인가요?",
          "speaker_id": "2",
          "time": "20210829 19:47"
        },
        {
          "id": "MDRW2100022965.1.12",
          "form": "전혀 아닙니다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전혀 아닙니다 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:47"
        },
        {
          "id": "MDRW2100022965.1.13",
          "form": "오 그럼 서양식인가요?",
          "original_form": "오 그럼 서양식인가요?",
          "speaker_id": "2",
          "time": "20210829 19:47"
        },
        {
          "id": "MDRW2100022965.1.14",
          "form": "아 이런 애매한데요 서양식도 아닙니다...",
          "original_form": "아 이런 애매한데요 서양식도 아닙니다...",
          "speaker_id": "1",
          "time": "20210829 19:47"
        },
        {
          "id": "MDRW2100022965.1.15",
          "form": "음식 이름이 몇 글자예요?",
          "original_form": "음식 이름이 몇 글자예요?",
          "speaker_id": "2",
          "time": "20210829 19:48"
        },
        {
          "id": "MDRW2100022965.1.16",
          "form": "와 진짜 어렵다",
          "original_form": "와 진짜 어렵다",
          "speaker_id": "2",
          "time": "20210829 19:48"
        },
        {
          "id": "MDRW2100022965.1.17",
          "form": "세글자요ㅋㅋㅋ",
          "original_form": "세글자요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:48"
        },
        {
          "id": "MDRW2100022965.1.18",
          "form": "초록소스가 무슨 맛인가요? 단맛 짠맛 매운맛 등",
          "original_form": "초록소스가 무슨 맛인가요? 단맛 짠맛 매운맛 등",
          "speaker_id": "2",
          "time": "20210829 19:49"
        },
        {
          "id": "MDRW2100022965.1.19",
          "form": "담백하고 부드러운 맛입니다 ㅋㅋㅋㅋ",
          "original_form": "담백하고 부드러운 맛입니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 19:50"
        }
