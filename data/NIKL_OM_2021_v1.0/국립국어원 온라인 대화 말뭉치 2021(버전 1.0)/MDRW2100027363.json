      "utterance": [
        {
          "id": "MDRW2100027363.1.1",
          "form": "안녕하세요? 혹시 종교 있으세요?",
          "original_form": "안녕하세요? 혹시 종교 있으세요?",
          "speaker_id": "1",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100027363.1.2",
          "form": "저는 무교지만 불교좋아해요",
          "original_form": "저는 무교지만 불교좋아해요",
          "speaker_id": "2",
          "time": "20210902 12:21"
        },
        {
          "id": "MDRW2100027363.1.3",
          "form": "불교의 어떤 부분이 좋으신가요?",
          "original_form": "불교의 어떤 부분이 좋으신가요?",
          "speaker_id": "1",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027363.1.4",
          "form": "차분하고 안정적인 부분이요",
          "original_form": "차분하고 안정적인 부분이요",
          "speaker_id": "2",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027363.1.5",
          "form": "사찰 문화도 좋아요 건물도",
          "original_form": "사찰 문화도 좋아요 건물도",
          "speaker_id": "2",
          "time": "20210902 12:23"
        },
        {
          "id": "MDRW2100027363.1.6",
          "form": "아~ 맞아요 절에서 피우는 향냄새가 좋네요",
          "original_form": "아~ 맞아요 절에서 피우는 향냄새가 좋네요",
          "speaker_id": "1",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100027363.1.7",
          "form": "저는 소풍으로만 가본거같아요 ㅎㅎ",
          "original_form": "저는 소풍으로만 가본거같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:25"
        },
        {
          "id": "MDRW2100027363.1.8",
          "form": "아 구것도 좋어요 종교있으세요?",
          "original_form": "아 구것도 좋어요 종교있으세요?",
          "speaker_id": "2",
          "time": "20210902 12:54"
        },
        {
          "id": "MDRW2100027363.1.9",
          "form": "아뇨!! 근데 부모님이 교회 다니세요",
          "original_form": "아뇨!! 근데 부모님이 교회 다니세요",
          "speaker_id": "1",
          "time": "20210902 12:55"
        },
        {
          "id": "MDRW2100027363.1.10",
          "form": "일요일마다 예배 나가시는데 요즘엔 비대면 예배 하세요 ㅎㅎ",
          "original_form": "일요일마다 예배 나가시는데 요즘엔 비대면 예배 하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 12:56"
        },
        {
          "id": "MDRW2100027363.1.11",
          "form": "아 비대면도 있구나 부모님 교회가실때 같이 안가세요?",
          "original_form": "아 비대면도 있구나 부모님 교회가실때 같이 안가세요?",
          "speaker_id": "2",
          "time": "20210902 13:04"
        },
        {
          "id": "MDRW2100027363.1.12",
          "form": "네 전 무교라서 ㅋㅋㅋㅋ",
          "original_form": "네 전 무교라서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:04"
        },
        {
          "id": "MDRW2100027363.1.13",
          "form": "일요일에 교회 나가는게 귀찮아서 믿음이 사라졌네요...",
          "original_form": "일요일에 교회 나가는게 귀찮아서 믿음이 사라졌네요...",
          "speaker_id": "1",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100027363.1.14",
          "form": "어렸을땐 다녔어요!! ㅋㅋㅋㅋ",
          "original_form": "어렸을땐 다녔어요!! ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100027363.1.15",
          "form": "그랬군요",
          "original_form": "그랬군요",
          "speaker_id": "2",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100027363.1.16",
          "form": "종교가 있으면 좋긴한데 크게 믿질않아서",
          "original_form": "종교가 있으면 좋긴한데 크게 믿질않아서",
          "speaker_id": "2",
          "time": "20210902 13:05"
        },
        {
          "id": "MDRW2100027363.1.17",
          "form": "맞아요 ㅠㅠ 막 엄청 큰 믿음을 받지 못해서 잘 못했어요",
          "original_form": "맞아요 ㅠㅠ 막 엄청 큰 믿음을 받지 못해서 잘 못했어요",
          "speaker_id": "1",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100027363.1.18",
          "form": "가서 사람들하고 어울리는건 좋았는데 말이죠",
          "original_form": "가서 사람들하고 어울리는건 좋았는데 말이죠",
          "speaker_id": "1",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100027363.1.19",
          "form": "맞아요 저도 어릴때 교회에 친구들따라 가본거같아요",
          "original_form": "맞아요 저도 어릴때 교회에 친구들따라 가본거같아요",
          "speaker_id": "2",
          "time": "20210902 13:06"
        },
        {
          "id": "MDRW2100027363.1.20",
          "form": "교회에서 간식 얻어먹고 노래 부르고 하는건 좋았어요 ㅋㅋㅋ",
          "original_form": "교회에서 간식 얻어먹고 노래 부르고 하는건 좋았어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100027363.1.21",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100027363.1.22",
          "form": "근데 활동이랑 믿음은 별개더라구요!!",
          "original_form": "근데 활동이랑 믿음은 별개더라구요!!",
          "speaker_id": "1",
          "time": "20210902 13:07"
        },
        {
          "id": "MDRW2100027363.1.23",
          "form": "요즘은 그냥 사람없이 차분한 절이 좋어요",
          "original_form": "요즘은 그냥 사람없이 차분한 절이 좋어요",
          "speaker_id": "2",
          "time": "20210902 13:07"
        }
      ]
    }
