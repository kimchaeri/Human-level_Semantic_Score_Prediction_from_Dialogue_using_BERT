      "utterance": [
        {
          "id": "MDRW2100045716.1.1",
          "form": "민초의 근간을 아무렇지도 않게 뒤흔드는 이야기라구요!!",
          "original_form": "민초의 근간을 아무렇지도 않게 뒤흔드는 이야기라구요!!",
          "speaker_id": "1",
          "time": "20210908 05:57"
        },
        {
          "id": "MDRW2100045716.1.2",
          "form": "녹차도 혹시 좋아하세요?",
          "original_form": "녹차도 혹시 좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 05:57"
        },
        {
          "id": "MDRW2100045716.1.3",
          "form": "녹차 완전 사랑하죠",
          "original_form": "녹차 완전 사랑하죠",
          "speaker_id": "1",
          "time": "20210908 05:57"
        },
        {
          "id": "MDRW2100045716.1.4",
          "form": "name2님도 좋아하세요?",
          "original_form": "&name2&님도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 05:57"
        },
        {
          "id": "MDRW2100045716.1.5",
          "form": "네 녹차 짱 사랑해요",
          "original_form": "네 녹차 짱 사랑해요",
          "speaker_id": "2",
          "time": "20210908 05:58"
        },
        {
          "id": "MDRW2100045716.1.6",
          "form": "녹차도 원래 싫어했는데",
          "original_form": "녹차도 원래 싫어했는데",
          "speaker_id": "2",
          "time": "20210908 05:58"
        },
        {
          "id": "MDRW2100045716.1.7",
          "form": "처음 먹었을 때 약간 파래김맛처럼 느껴졌어요",
          "original_form": "처음 먹었을 때 약간 파래김맛처럼 느껴졌어요",
          "speaker_id": "2",
          "time": "20210908 05:58"
        },
        {
          "id": "MDRW2100045716.1.8",
          "form": "아 뭔지 알 것 같아욬ㅋㅋㅋㅋ",
          "original_form": "아 뭔지 알 것 같아욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:59"
        },
        {
          "id": "MDRW2100045716.1.9",
          "form": "저도 녹차아이스크림 처음 알았을때 좀 충격받았거든요",
          "original_form": "저도 녹차아이스크림 처음 알았을때 좀 충격받았거든요",
          "speaker_id": "1",
          "time": "20210908 06:00"
        },
        {
          "id": "MDRW2100045716.1.10",
          "form": "보성에서 녹차맛 아이스크림 파는걸로 처음 봤는데",
          "original_form": "보성에서 녹차맛 아이스크림 파는걸로 처음 봤는데",
          "speaker_id": "1",
          "time": "20210908 06:00"
        },
        {
          "id": "MDRW2100045716.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 06:00"
        },
        {
          "id": "MDRW2100045716.1.12",
          "form": "너무 충격적이었어요",
          "original_form": "너무 충격적이었어요",
          "speaker_id": "1",
          "time": "20210908 06:02"
        },
        {
          "id": "MDRW2100045716.1.13",
          "form": "ㅋㅋㅋㅋ어떤 부분에서요?",
          "original_form": "ㅋㅋㅋㅋ어떤 부분에서요?",
          "speaker_id": "2",
          "time": "20210908 06:03"
        },
        {
          "id": "MDRW2100045716.1.14",
          "form": "색깔이 요상했나요",
          "original_form": "색깔이 요상했나요",
          "speaker_id": "2",
          "time": "20210908 06:03"
        },
        {
          "id": "MDRW2100045716.1.15",
          "form": "아직 녹차 디저트가 시장에 본격적으로 나오기 전이랔ㅋㅋㅋ",
          "original_form": "아직 녹차 디저트가 시장에 본격적으로 나오기 전이랔ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:03"
        },
        {
          "id": "MDRW2100045716.1.16",
          "form": "매실음료수 알로에주스 아침햇살 이런게 새롭게 나오던 시대였거든욬ㅋㅋㅋ",
          "original_form": "매실음료수 알로에주스 아침햇살 이런게 새롭게 나오던 시대였거든욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:03"
        },
        {
          "id": "MDRW2100045716.1.17",
          "form": "진짜 김맛 아이스크림을 본 느낌이었어요",
          "original_form": "진짜 김맛 아이스크림을 본 느낌이었어요",
          "speaker_id": "1",
          "time": "20210908 06:03"
        },
        {
          "id": "MDRW2100045716.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:05"
        },
        {
          "id": "MDRW2100045716.1.19",
          "form": "쓸데없이 건강하게 만든 괴식처럼 보였겠네요ㅋㅋㅋㅋㅋ",
          "original_form": "쓸데없이 건강하게 만든 괴식처럼 보였겠네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:05"
        },
        {
          "id": "MDRW2100045716.1.20",
          "form": "매실 알로에 아침햇살 하니까 느낌 팍 와요",
          "original_form": "매실 알로에 아침햇살 하니까 느낌 팍 와요",
          "speaker_id": "2",
          "time": "20210908 06:06"
        },
        {
          "id": "MDRW2100045716.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그렇죠 지금은 너무 당연한데",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그렇죠 지금은 너무 당연한데",
          "speaker_id": "1",
          "time": "20210908 06:06"
        },
        {
          "id": "MDRW2100045716.1.22",
          "form": "그게 당연하지 않았던 시대거 있었다는게 놀라워요 이젠",
          "original_form": "그게 당연하지 않았던 시대거 있었다는게 놀라워요 이젠",
          "speaker_id": "1",
          "time": "20210908 06:06"
        },
        {
          "id": "MDRW2100045716.1.23",
          "form": "그러니까요 지금은 없어서 못 먹는디",
          "original_form": "그러니까요 지금은 없어서 못 먹는디",
          "speaker_id": "2",
          "time": "20210908 06:07"
        },
        {
          "id": "MDRW2100045716.1.24",
          "form": "그 옛날에는 너무 센세이션한 조합이었나봐요 ㅋㅋㅋㅋ",
          "original_form": "그 옛날에는 너무 센세이션한 조합이었나봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:07"
        },
        {
          "id": "MDRW2100045716.1.25",
          "form": "이제는 슈퍼마켓 보급아이스크림에도",
          "original_form": "이제는 슈퍼마켓 보급아이스크림에도",
          "speaker_id": "2",
          "time": "20210908 06:08"
        },
        {
          "id": "MDRW2100045716.1.26",
          "form": "녹차맛이 많잖아요",
          "original_form": "녹차맛이 많잖아요",
          "speaker_id": "2",
          "time": "20210908 06:08"
        },
        {
          "id": "MDRW2100045716.1.27",
          "form": "그러니까요 너무 잘 자리잡았는데",
          "original_form": "그러니까요 너무 잘 자리잡았는데",
          "speaker_id": "1",
          "time": "20210908 06:09"
        },
        {
          "id": "MDRW2100045716.1.28",
          "form": "그게 새롭던 시대가 있었다는게 신기해요",
          "original_form": "그게 새롭던 시대가 있었다는게 신기해요",
          "speaker_id": "1",
          "time": "20210908 06:09"
        },
        {
          "id": "MDRW2100045716.1.29",
          "form": "생각해보면 메로나도 메론이 귀하던 시대에 그걸 아이스크림으로 만들겠다는 기발한 발상이 있어서 나올 수 있었다던 것 같고",
          "original_form": "생각해보면 메로나도 메론이 귀하던 시대에 그걸 아이스크림으로 만들겠다는 기발한 발상이 있어서 나올 수 있었다던 것 같고",
          "speaker_id": "1",
          "time": "20210908 06:10"
        },
        {
          "id": "MDRW2100045716.1.30",
          "form": "맞아요 맞아요 아이디어 넘 기발해요 ㅋㅋㅋ",
          "original_form": "맞아요 맞아요 아이디어 넘 기발해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:10"
        },
        {
          "id": "MDRW2100045716.1.31",
          "form": "헉 메로나 하니꺼 생각났는데",
          "original_form": "헉 메로나 하니꺼 생각났는데",
          "speaker_id": "2",
          "time": "20210908 06:11"
        },
        {
          "id": "MDRW2100045716.1.32",
          "form": "메로나 향이 메론 향이 아니라 참외 향인 거 아세요?",
          "original_form": "메로나 향이 메론 향이 아니라 참외 향인 거 아세요?",
          "speaker_id": "2",
          "time": "20210908 06:11"
        },
        {
          "id": "MDRW2100045716.1.33",
          "form": "아 맞아요 들었어요",
          "original_form": "아 맞아요 들었어요",
          "speaker_id": "1",
          "time": "20210908 06:11"
        },
        {
          "id": "MDRW2100045716.1.34",
          "form": "한국인에게 메론향이 생소할까봐 익숙한 참외향으로 했다고 들었던 것 같아요ㅋㅋㅋㅋㅋ",
          "original_form": "한국인에게 메론향이 생소할까봐 익숙한 참외향으로 했다고 들었던 것 같아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:11"
        },
        {
          "id": "MDRW2100045716.1.35",
          "form": "ㅋㅋㅋㅌㅋㅋ근데 진짜",
          "original_form": "ㅋㅋㅋㅌㅋㅋ근데 진짜",
          "speaker_id": "2",
          "time": "20210908 06:11"
        },
        {
          "id": "MDRW2100045716.1.36",
          "form": "참와 눈 감고 먹으면",
          "original_form": "참와 눈 감고 먹으면",
          "speaker_id": "2",
          "time": "20210908 06:12"
        },
        {
          "id": "MDRW2100045716.1.37",
          "form": "메론이렁 은근 비슷해요",
          "original_form": "메론이렁 은근 비슷해요",
          "speaker_id": "2",
          "time": "20210908 06:12"
        },
        {
          "id": "MDRW2100045716.1.38",
          "form": "반대로 메론이 참외같을 때도 있어요 ㅂㄷㅂㄷ",
          "original_form": "반대로 메론이 참외같을 때도 있어요 ㅂㄷㅂㄷ",
