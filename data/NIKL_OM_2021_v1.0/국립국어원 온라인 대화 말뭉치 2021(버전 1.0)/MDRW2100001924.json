      "utterance": [
        {
          "id": "MDRW2100001924.1.1",
          "form": "가을 옷 사야되나",
          "original_form": "가을 옷 사야되나",
          "speaker_id": "1",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001924.1.2",
          "form": "나도 가을맞이 맨투맨 살까싶다ㅋㅋ",
          "original_form": "나도 가을맞이 맨투맨 살까싶다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001924.1.3",
          "form": "운동할때 입을  조거팬츠도 하나사야하는디",
          "original_form": "운동할때 입을  조거팬츠도 하나사야하는디",
          "speaker_id": "2",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001924.1.4",
          "form": "요즘 딱 맨투맨에 반바지 입기 좋은 저녁",
          "original_form": "요즘 딱 맨투맨에 반바지 입기 좋은 저녁",
          "speaker_id": "1",
          "time": "20210819 22:14"
        },
        {
          "id": "MDRW2100001924.1.5",
          "form": "찰나의 패션ㅋㅋ",
          "original_form": "찰나의 패션ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:16"
        },
        {
          "id": "MDRW2100001924.1.6",
          "form": "좋아하는 패션입니다 ㅋㅋ 니트 사고싶다",
          "original_form": "좋아하는 패션입니다 ㅋㅋ 니트 사고싶다",
          "speaker_id": "1",
          "time": "20210819 22:19"
        },
        {
          "id": "MDRW2100001924.1.7",
          "form": "니트 너무 비싸ㅠㅠ",
          "original_form": "니트 너무 비싸ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100001924.1.8",
          "form": "비싼 니트 안입... 아 근데 fw 시즌은 아우터를 많이 사야되는 ㅋㅋㅋ",
          "original_form": "비싼 니트 안입... 아 근데 fw 시즌은 아우터를 많이 사야되는 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001924.1.9",
          "form": "작년엔 뭐입고 다녔나싶네",
          "original_form": "작년엔 뭐입고 다녔나싶네",
          "speaker_id": "2",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001924.1.10",
          "form": "아우터는 또 살만한것도 딱히",
          "original_form": "아우터는 또 살만한것도 딱히",
          "speaker_id": "1",
          "time": "20210819 22:28"
        },
        {
          "id": "MDRW2100001924.1.11",
          "form": "청자켓이랑 야상으로 버텨야...",
          "original_form": "청자켓이랑 야상으로 버텨야...",
          "speaker_id": "2",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001924.1.12",
          "form": "둘다 없는 나...벗고다녀..",
          "original_form": "둘다 없는 나...벗고다녀..",
          "speaker_id": "1",
          "time": "20210819 22:29"
        },
        {
          "id": "MDRW2100001924.1.13",
          "form": "쫌 긴 야상 하나 사고싶은디",
          "original_form": "쫌 긴 야상 하나 사고싶은디",
          "speaker_id": "2",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001924.1.14",
          "form": "마땅한게 읎네",
          "original_form": "마땅한게 읎네",
          "speaker_id": "2",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001924.1.15",
          "form": "청자켓 아직 못 산 사람 나야 나",
          "original_form": "청자켓 아직 못 산 사람 나야 나",
          "speaker_id": "1",
          "time": "20210819 22:32"
        },
        {
          "id": "MDRW2100001924.1.16",
          "form": "하나 사랔ㅋㅋㅋㅋ",
          "original_form": "하나 사랔ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:33"
        },
        {
          "id": "MDRW2100001924.1.17",
          "form": "난 요즘 왜케 남자옷이 이뻐보이냐",
          "original_form": "난 요즘 왜케 남자옷이 이뻐보이냐",
          "speaker_id": "2",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001924.1.18",
          "form": "그니까 산다 산다 하고 못 삼 ㅋㅋㅋ",
          "original_form": "그니까 산다 산다 하고 못 삼 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001924.1.19",
          "form": "남자옷 자주 사는 편인듯 나는 ㅋㅋ",
          "original_form": "남자옷 자주 사는 편인듯 나는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001924.1.20",
          "form": "나도 야상 에잇세컨즈 남자껀데 오버핏으로 이쁘게 맞아ㅋㅋㅋ",
          "original_form": "나도 야상 에잇세컨즈 남자껀데 오버핏으로 이쁘게 맞아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100001924.1.21",
          "form": "가죽자켓도 올세인즈 남자꺼 삼ㅋㅋㅋ",
          "original_form": "가죽자켓도 올세인즈 남자꺼 삼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:35"
        },
        {
          "id": "MDRW2100001924.1.22",
          "form": "올세인츠 위시리스트였는데 이제 가죽... 자제할까 싶음 ㅋㅋ",
          "original_form": "올세인츠 위시리스트였는데 이제 가죽... 자제할까 싶음 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001924.1.23",
          "form": "요즘 페이크 레더도 이쁘게 잘 나오는듯",
          "original_form": "요즘 페이크 레더도 이쁘게 잘 나오는듯",
          "speaker_id": "2",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001924.1.24",
          "form": "아우터는 쏙 마음에 드는게 넘 찾기 힘들어",
          "original_form": "아우터는 쏙 마음에 드는게 넘 찾기 힘들어",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001924.1.25",
          "form": "그르게 아우터는 비싸서 꼭 마음에드는거 사야 돈 아깝단 생각을 안하는데",
          "original_form": "그르게 아우터는 비싸서 꼭 마음에드는거 사야 돈 아깝단 생각을 안하는데",
          "speaker_id": "2",
          "time": "20210819 22:57"
        },
        {
          "id": "MDRW2100001924.1.26",
          "form": "마져 이번엔 청자켓 꼭 사보까 ㅋㅋㅋ",
          "original_form": "마져 이번엔 청자켓 꼭 사보까 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 23:56"
        },
        {
          "id": "MDRW2100001924.1.27",
          "form": "청자켓 이쁜거 많던디 너 맘에 드는거 하나쯤은 찾을 수 있겄지ㅋㅋ",
          "original_form": "청자켓 이쁜거 많던디 너 맘에 드는거 하나쯤은 찾을 수 있겄지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:03"
        },
        {
          "id": "MDRW2100001924.1.28",
          "form": "청자켓 8년전에 구제샵에서 산 거 아직도 입는다ㅋㅋ 만고땡이여ㅋㅋ",
          "original_form": "청자켓 8년전에 구제샵에서 산 거 아직도 입는다ㅋㅋ 만고땡이여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100001924.1.29",
          "form": "청자켓은 클랙식이 최고같다 ㅋㅋ",
          "original_form": "청자켓은 클랙식이 최고같다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:04"
        },
        {
          "id": "MDRW2100001924.1.30",
          "form": "응 그냥 기본 스타일이 유행안타고 좋지",
          "original_form": "응 그냥 기본 스타일이 유행안타고 좋지",
          "speaker_id": "2",
          "time": "20210820 00:05"
        },
        {
          "id": "MDRW2100001924.1.31",
          "form": "나도 예전에 샀음 그리 오래 입었을것을 ㅋㅋㅋ",
          "original_form": "나도 예전에 샀음 그리 오래 입었을것을 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:05"
        },
        {
          "id": "MDRW2100001924.1.32",
          "form": "날 추워지니 앵글부츠 사곱다",
          "original_form": "날 추워지니 앵글부츠 사곱다",
