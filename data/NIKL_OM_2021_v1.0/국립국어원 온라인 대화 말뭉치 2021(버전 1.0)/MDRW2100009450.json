      "utterance": [
        {
          "id": "MDRW2100009450.1.1",
          "form": "필라테스 마스크 헬스 ㅇ얘기했어요!",
          "original_form": "필라테스 마스크 헬스 ㅇ얘기했어요!",
          "speaker_id": "1",
          "time": "20210822 23:30"
        },
        {
          "id": "MDRW2100009450.1.2",
          "form": "요새 코로나라서",
          "original_form": "요새 코로나라서",
          "speaker_id": "2",
          "time": "20210822 23:33"
        },
        {
          "id": "MDRW2100009450.1.3",
          "form": "사실 그전에도 공연은 진짜 가본적없긴해요 ㅋㅋ",
          "original_form": "사실 그전에도 공연은 진짜 가본적없긴해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009450.1.4",
          "form": "저도 공연은 ㅋㅋㅋㅋ",
          "original_form": "저도 공연은 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100009450.1.5",
          "form": "비싸기도해서 잘 안가개되도라고요",
          "original_form": "비싸기도해서 잘 안가개되도라고요",
          "speaker_id": "1",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009450.1.6",
          "form": "근데 요즘 문화활동 좀 빠져서",
          "original_form": "근데 요즘 문화활동 좀 빠져서",
          "speaker_id": "1",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009450.1.7",
          "form": "갑자기 알아보고있어요",
          "original_form": "갑자기 알아보고있어요",
          "speaker_id": "1",
          "time": "20210822 23:36"
        },
        {
          "id": "MDRW2100009450.1.8",
          "form": "오 어떤 문화활동이요?",
          "original_form": "오 어떤 문화활동이요?",
          "speaker_id": "2",
          "time": "20210822 23:41"
        },
        {
          "id": "MDRW2100009450.1.9",
          "form": "뮤지컬이나 전시회요ㅎㅎ",
          "original_form": "뮤지컬이나 전시회요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:44"
        },
        {
          "id": "MDRW2100009450.1.10",
          "form": "잘 모르는데 아무거나 막 가보고싶더라고요",
          "original_form": "잘 모르는데 아무거나 막 가보고싶더라고요",
          "speaker_id": "1",
          "time": "20210822 23:45"
        },
        {
          "id": "MDRW2100009450.1.11",
          "form": "요즘도 하는공연이 있기야 하겠죠?",
          "original_form": "요즘도 하는공연이 있기야 하겠죠?",
          "speaker_id": "2",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009450.1.12",
          "form": "예전에 동생 아이돌콘서트 대신줄서준적있는데",
          "original_form": "예전에 동생 아이돌콘서트 대신줄서준적있는데",
          "speaker_id": "2",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009450.1.13",
          "form": "앞으로는 절대 그런형태 공연은 없을거같아요ㅜ",
          "original_form": "앞으로는 절대 그런형태 공연은 없을거같아요ㅜ",
          "speaker_id": "2",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009450.1.14",
          "form": "맞아요 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009450.1.15",
          "form": "요즘은 공연 하는것도 다 일이더라고요",
          "original_form": "요즘은 공연 하는것도 다 일이더라고요",
          "speaker_id": "1",
          "time": "20210822 23:49"
        },
        {
          "id": "MDRW2100009450.1.16",
          "form": "적자는 안나는지.. 걱정도 드네요",
          "original_form": "적자는 안나는지.. 걱정도 드네요",
          "speaker_id": "1",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009450.1.17",
          "form": "그때보니까 막 가수가 생수병  물도 뿌리고하던데",
          "original_form": "그때보니까 막 가수가 생수병  물도 뿌리고하던데",
          "speaker_id": "2",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009450.1.18",
          "form": "지금은 상상도 못할광경ㅋㅋ아닌가여",
          "original_form": "지금은 상상도 못할광경ㅋㅋ아닌가여",
          "speaker_id": "2",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009450.1.19",
          "form": "세상이 이렇게 변하다니!",
          "original_form": "세상이 이렇게 변하다니!",
          "speaker_id": "2",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009450.1.20",
          "form": "와 진짜 화끈하게 노네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "와 진짜 화끈하게 노네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 06:57"
        },
        {
          "id": "MDRW2100009450.1.21",
          "form": "지금이면.. 기함을 토하죠",
          "original_form": "지금이면.. 기함을 토하죠",
          "speaker_id": "1",
          "time": "20210823 06:57"
        },
        {
          "id": "MDRW2100009450.1.22",
          "form": "요즘하는 공연..",
          "original_form": "요즘하는 공연..",
          "speaker_id": "1",
          "time": "20210823 06:58"
        },
        {
          "id": "MDRW2100009450.1.23",
          "form": "사진전이나 미술전에 관심있어요 전",
          "original_form": "사진전이나 미술전에 관심있어요 전",
          "speaker_id": "1",
          "time": "20210823 06:58"
        },
        {
          "id": "MDRW2100009450.1.24",
          "form": "오 저는 보는눈이 없어서",
          "original_form": "오 저는 보는눈이 없어서",
          "speaker_id": "2",
          "time": "20210823 09:42"
        },
        {
          "id": "MDRW2100009450.1.25",
          "form": "그런 사진전같은건 거리두기만 잘하면 지금도 괜찮을것같은데",
          "original_form": "그런 사진전같은건 거리두기만 잘하면 지금도 괜찮을것같은데",
          "speaker_id": "2",
          "time": "20210823 09:43"
        },
        {
          "id": "MDRW2100009450.1.26",
          "form": "유명한데 아니면 괜찮을것같더라고요",
          "original_form": "유명한데 아니면 괜찮을것같더라고요",
          "speaker_id": "1",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100009450.1.27",
          "form": "막 인스타로 뜬데 아니면 ㅋㅋㅋㅋ",
          "original_form": "막 인스타로 뜬데 아니면 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100009450.1.28",
          "form": "어우 그런데는 사람이 우글우글",
          "original_form": "어우 그런데는 사람이 우글우글",
          "speaker_id": "1",
          "time": "20210823 14:08"
        },
        {
          "id": "MDRW2100009450.1.29",
          "form": "어휴 전시회는 간지 진짜 10년은된듯",
          "original_form": "어휴 전시회는 간지 진짜 10년은된듯",
          "speaker_id": "2",
          "time": "20210823 14:20"
        },
        {
          "id": "MDRW2100009450.1.30",
          "form": "아주 예전에 스누피 전시회 간적은 있어요",
          "original_form": "아주 예전에 스누피 전시회 간적은 있어요",
          "speaker_id": "2",
          "time": "20210823 14:21"
        },
        {
          "id": "MDRW2100009450.1.31",
          "form": "헉 저 그거 너무 가고싶었는데",
          "original_form": "헉 저 그거 너무 가고싶었는데",
          "speaker_id": "1",
          "time": "20210823 14:25"
        },
        {
          "id": "MDRW2100009450.1.32",
          "form": "어떠셨나요?",
          "original_form": "어떠셨나요?",
          "speaker_id": "1",
          "time": "20210823 14:26"
        },
        {
          "id": "MDRW2100009450.1.33",
          "form": "제주를 못가서 ㅠㅠ",
          "original_form": "제주를 못가서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 14:26"
        },
        {
          "id": "MDRW2100009450.1.34",
          "form": "사진 찍을 찬스? 가 많아서 좋았어요",
          "original_form": "사진 찍을 찬스? 가 많아서 좋았어요",
          "speaker_id": "2",
          "time": "20210823 16:30"
        },
        {
          "id": "MDRW2100009450.1.35",
          "form": "포토존이 많나보네요!",
          "original_form": "포토존이 많나보네요!",
          "speaker_id": "1",
          "time": "20210823 18:45"
        }
      ]
    }
