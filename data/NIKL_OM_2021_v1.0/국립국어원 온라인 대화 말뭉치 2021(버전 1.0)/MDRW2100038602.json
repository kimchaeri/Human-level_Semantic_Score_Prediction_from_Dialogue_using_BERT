      "utterance": [
        {
          "id": "MDRW2100038602.1.1",
          "form": "저는 가디건 둘러보고 있는데",
          "original_form": "저는 가디건 둘러보고 있는데",
          "speaker_id": "2",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100038602.1.2",
          "form": "name1님은 사신다면 뭘 사실건가요",
          "original_form": "&name1&님은 사신다면 뭘 사실건가요",
          "speaker_id": "2",
          "time": "20210902 16:44"
        },
        {
          "id": "MDRW2100038602.1.3",
          "form": "음 저는 상의나 원피스 사고 싶어요 \r\n자켓도 좋을거 같고ㅎㅎ",
          "original_form": "음 저는 상의나 원피스 사고 싶어요 \r\n자켓도 좋을거 같고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 16:45"
        },
        {
          "id": "MDRW2100038602.1.4",
          "form": "저는 원피스가 한벌도 없어요...ㅋㅋ",
          "original_form": "저는 원피스가 한벌도 없어요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:46"
        },
        {
          "id": "MDRW2100038602.1.5",
          "form": "치마는 아닌거 같아요",
          "original_form": "치마는 아닌거 같아요",
          "speaker_id": "2",
          "time": "20210902 16:47"
        },
        {
          "id": "MDRW2100038602.1.6",
          "form": "앗 그러시군요ㅎㅎ",
          "original_form": "앗 그러시군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 16:50"
        },
        {
          "id": "MDRW2100038602.1.7",
          "form": "근데 가을 옷은 잘 입으려면 부지런해야겠더라구요!",
          "original_form": "근데 가을 옷은 잘 입으려면 부지런해야겠더라구요!",
          "speaker_id": "1",
          "time": "20210902 16:50"
        },
        {
          "id": "MDRW2100038602.1.8",
          "form": "맞아요 가을은 짧고 나갈일이 있어야 입는거니까요",
          "original_form": "맞아요 가을은 짧고 나갈일이 있어야 입는거니까요",
          "speaker_id": "2",
          "time": "20210902 16:52"
        },
        {
          "id": "MDRW2100038602.1.9",
          "form": "그쵸 가을이 너무나 짧게 지나가서 아쉬워요ㅠㅠ",
          "original_form": "그쵸 가을이 너무나 짧게 지나가서 아쉬워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 16:58"
        },
        {
          "id": "MDRW2100038602.1.10",
          "form": "가을이 길어질 수는 없는걸까요",
          "original_form": "가을이 길어질 수는 없는걸까요",
          "speaker_id": "2",
          "time": "20210902 16:59"
        },
        {
          "id": "MDRW2100038602.1.11",
          "form": "그러니까요 \r\n여름이 짧고 가을이 길어지면 좋겠어요",
          "original_form": "그러니까요 \r\n여름이 짧고 가을이 길어지면 좋겠어요",
          "speaker_id": "1",
          "time": "20210902 17:00"
        },
        {
          "id": "MDRW2100038602.1.12",
          "form": "여름 긴건 진짜ㅠ 싫어요",
          "original_form": "여름 긴건 진짜ㅠ 싫어요",
          "speaker_id": "2",
          "time": "20210902 17:02"
        },
        {
          "id": "MDRW2100038602.1.13",
          "form": "맞아요ㅜㅜ 근데 여름이 점점 길어지고 있네요",
          "original_form": "맞아요ㅜㅜ 근데 여름이 점점 길어지고 있네요",
          "speaker_id": "1",
          "time": "20210902 17:11"
        },
        {
          "id": "MDRW2100038602.1.14",
          "form": "지구온난화 때문일까요...",
          "original_form": "지구온난화 때문일까요...",
          "speaker_id": "2",
          "time": "20210902 17:13"
        },
        {
          "id": "MDRW2100038602.1.15",
          "form": "그렇겠죠..",
          "original_form": "그렇겠죠..",
          "speaker_id": "1",
          "time": "20210902 17:14"
        },
        {
          "id": "MDRW2100038602.1.16",
          "form": "요즘들어 지구온난화로 인한 기후변화가 많이 느껴져요",
          "original_form": "요즘들어 지구온난화로 인한 기후변화가 많이 느껴져요",
          "speaker_id": "1",
          "time": "20210902 17:14"
        },
        {
          "id": "MDRW2100038602.1.17",
          "form": "맞아요 갑자기 돌풍이 불기도 하구요",
          "original_form": "맞아요 갑자기 돌풍이 불기도 하구요",
