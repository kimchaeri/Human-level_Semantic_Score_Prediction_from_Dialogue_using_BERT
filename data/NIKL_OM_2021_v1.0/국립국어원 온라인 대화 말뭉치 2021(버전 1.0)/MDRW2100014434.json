      "utterance": [
        {
          "id": "MDRW2100014434.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210825 19:00"
        },
        {
          "id": "MDRW2100014434.1.2",
          "form": "반갑습니다",
          "original_form": "반갑습니다",
          "speaker_id": "2",
          "time": "20210825 19:01"
        },
        {
          "id": "MDRW2100014434.1.3",
          "form": "요즘 코로나 때문에 참 힘들죠?",
          "original_form": "요즘 코로나 때문에 참 힘들죠?",
          "speaker_id": "2",
          "time": "20210825 19:01"
        },
        {
          "id": "MDRW2100014434.1.4",
          "form": "코로나 때문에 힘드네요",
          "original_form": "코로나 때문에 힘드네요",
          "speaker_id": "1",
          "time": "20210825 19:01"
        },
        {
          "id": "MDRW2100014434.1.5",
          "form": "근무도 확진자 때문에 정상적으로 하기 힘들기도 하고요",
          "original_form": "근무도 확진자 때문에 정상적으로 하기 힘들기도 하고요",
          "speaker_id": "2",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100014434.1.6",
          "form": "거기는 확진자가 많이 나오나요?",
          "original_form": "거기는 확진자가 많이 나오나요?",
          "speaker_id": "2",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100014434.1.7",
          "form": "확진자 가끔 나오네요 마스크 때문에 너무 힘들어요",
          "original_form": "확진자 가끔 나오네요 마스크 때문에 너무 힘들어요",
          "speaker_id": "1",
          "time": "20210825 19:02"
        },
        {
          "id": "MDRW2100014434.1.8",
          "form": "마스크 정말 너무 답답해요 ㅠㅜ 얼른 벗고 싶습니다",
          "original_form": "마스크 정말 너무 답답해요 ㅠㅜ 얼른 벗고 싶습니다",
          "speaker_id": "2",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014434.1.9",
          "form": "트러블도 넘 많이 생기구 불편하지 않나요?",
          "original_form": "트러블도 넘 많이 생기구 불편하지 않나요?",
          "speaker_id": "2",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014434.1.10",
          "form": "저는 피부가 좋아서 괜찮아요",
          "original_form": "저는 피부가 좋아서 괜찮아요",
          "speaker_id": "1",
          "time": "20210825 19:03"
        },
        {
          "id": "MDRW2100014434.1.11",
          "form": "그쪽도?",
          "original_form": "그쪽도?",
          "speaker_id": "1",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014434.1.12",
          "form": "그래요? 부럽습니다 ㅜㅜㅋㅋ 저도 나쁜 편은 아니지만 피부가 전보다는 확실히 안좋아지는 거 같습니다",
          "original_form": "그래요? 부럽습니다 ㅜㅜㅋㅋ 저도 나쁜 편은 아니지만 피부가 전보다는 확실히 안좋아지는 거 같습니다",
          "speaker_id": "2",
          "time": "20210825 19:04"
        },
        {
          "id": "MDRW2100014434.1.13",
          "form": "선생님 빠른 답장 부탁드립니다",
          "original_form": "선생님 빠른 답장 부탁드립니다",
          "speaker_id": "2",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014434.1.14",
          "form": "그러게요 조심해야겠습니다 업무는 괜찮으세요?",
          "original_form": "그러게요 조심해야겠습니다 업무는 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210825 19:05"
        },
        {
          "id": "MDRW2100014434.1.15",
          "form": "저는 코로나 때문에 작년에 한창 엄청 힘들었습니다 ㅠㅠ 올해는 쪼금 더ㅜ나아졌어요",
          "original_form": "저는 코로나 때문에 작년에 한창 엄청 힘들었습니다 ㅠㅠ 올해는 쪼금 더ㅜ나아졌어요",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014434.1.16",
          "form": "그래도 업무량이 적지 않아 스트레스가 심했어요",
          "original_form": "그래도 업무량이 적지 않아 스트레스가 심했어요",
          "speaker_id": "2",
          "time": "20210825 19:06"
        },
        {
          "id": "MDRW2100014434.1.17",
          "form": "저도 너무 많아졌네요 같이 힘냅시다",
          "original_form": "저도 너무 많아졌네요 같이 힘냅시다",
          "speaker_id": "1",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014434.1.18",
          "form": "급여라도 적지 않으면 좋을텐데 급여도 너무 짭니다",
          "original_form": "급여라도 적지 않으면 좋을텐데 급여도 너무 짭니다",
          "speaker_id": "2",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014434.1.19",
          "form": "요즘 워라밸이라도 지켜지는거에 감사해야할런지",
          "original_form": "요즘 워라밸이라도 지켜지는거에 감사해야할런지",
          "speaker_id": "2",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014434.1.20",
          "form": "워라밸 지켜지는거에 만족하는게 좋아보이네요",
          "original_form": "워라밸 지켜지는거에 만족하는게 좋아보이네요",
          "speaker_id": "1",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014434.1.21",
          "form": "워라밸이 짱입니다",
          "original_form": "워라밸이 짱입니다",
          "speaker_id": "1",
          "time": "20210825 19:09"
        }
      ]
