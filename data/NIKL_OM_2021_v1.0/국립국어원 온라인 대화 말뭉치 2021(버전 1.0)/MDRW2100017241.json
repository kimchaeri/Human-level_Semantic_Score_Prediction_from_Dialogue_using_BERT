      "utterance": [
        {
          "id": "MDRW2100017241.1.1",
          "form": "식사하셨나요?",
          "original_form": "식사하셨나요?",
          "speaker_id": "1",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100017241.1.2",
          "form": "지금 카레먹고있어요! 맛있네요ㅎㅎ",
          "original_form": "지금 카레먹고있어요! 맛있네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:14"
        },
        {
          "id": "MDRW2100017241.1.3",
          "form": "식사하셨어여?",
          "original_form": "식사하셨어여?",
          "speaker_id": "2",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017241.1.4",
          "form": "좀 이따가 순두부찌개 데워먹으려고요!",
          "original_form": "좀 이따가 순두부찌개 데워먹으려고요!",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017241.1.5",
          "form": "오~ 순두부찌개 좋죠",
          "original_form": "오~ 순두부찌개 좋죠",
          "speaker_id": "2",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100017241.1.6",
          "form": "카레 맛있겠어요! 직접 만드셨나요?",
          "original_form": "카레 맛있겠어요! 직접 만드셨나요?",
          "speaker_id": "1",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100017241.1.7",
          "form": "ㅎㅎ맛은 역시 엄마의 손길이..",
          "original_form": "ㅎㅎ맛은 역시 엄마의 손길이..",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017241.1.8",
          "form": "요리하시나요?",
          "original_form": "요리하시나요?",
          "speaker_id": "2",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017241.1.9",
          "form": "아ㅎㅎ 저도 할머니가 끓여주셨어요",
          "original_form": "아ㅎㅎ 저도 할머니가 끓여주셨어요",
          "speaker_id": "1",
          "time": "20210826 18:20"
        },
        {
          "id": "MDRW2100017241.1.10",
          "form": "저는 요리를 잘 못해서 거의 설거지만 해요",
          "original_form": "저는 요리를 잘 못해서 거의 설거지만 해요",
          "speaker_id": "1",
          "time": "20210826 18:21"
        },
        {
          "id": "MDRW2100017241.1.11",
          "form": "저도 뒷정리 담당이에요ㅎㅎㅎ",
          "original_form": "저도 뒷정리 담당이에요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:23"
        },
        {
          "id": "MDRW2100017241.1.12",
          "form": "카레에 고기 넣어 드시나요?",
          "original_form": "카레에 고기 넣어 드시나요?",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017241.1.13",
          "form": "네! 돼지고기 넣는거 엄청 좋아해요",
          "original_form": "네! 돼지고기 넣는거 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017241.1.14",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017241.1.15",
          "form": "네네! 돼지고기 소고기 다 좋아해요ㅎㅎ",
          "original_form": "네네! 돼지고기 소고기 다 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 18:24"
        },
        {
          "id": "MDRW2100017241.1.16",
          "form": "만들어 주는대로 잘 먹는 편이에요ㅋㅋㅋ",
          "original_form": "만들어 주는대로 잘 먹는 편이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017241.1.17",
          "form": "과일 좋아하시나요?",
          "original_form": "과일 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210826 18:25"
        },
        {
          "id": "MDRW2100017241.1.18",
          "form": "과일은 딸기 좋아해요!",
          "original_form": "과일은 딸기 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 18:26"
        },
        {
          "id": "MDRW2100017241.1.19",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 18:27"
        },
        {
          "id": "MDRW2100017241.1.20",
          "form": "저는 살구 제일 좋아해요!",
          "original_form": "저는 살구 제일 좋아해요!",
          "speaker_id": "1",
          "time": "20210826 18:28"
        }
      ]
