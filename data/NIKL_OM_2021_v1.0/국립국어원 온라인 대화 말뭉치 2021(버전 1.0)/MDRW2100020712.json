        },
        {
          "id": "MDRW2100020712.1.2",
          "form": "ㄱㅈㅋㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱㅈㄲ",
          "original_form": "ㄱㅈㅋㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱㅈㄱㅈㄲ",
          "speaker_id": "2",
