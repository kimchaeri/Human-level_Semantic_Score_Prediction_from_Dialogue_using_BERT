      "utterance": [
        {
          "id": "MDRW2100022310.1.1",
          "form": "치킨은 어느브랜드 좋아하세요",
          "original_form": "치킨은 어느브랜드 좋아하세요",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022310.1.2",
          "form": "저 후참잘이요",
          "original_form": "저 후참잘이요",
          "speaker_id": "2",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022310.1.3",
          "form": "아세요?",
          "original_form": "아세요?",
          "speaker_id": "2",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022310.1.4",
          "form": "후참인데 양념을 드신다?",
          "original_form": "후참인데 양념을 드신다?",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022310.1.5",
          "form": "ㅋㅋㅋㅋ맞습니다",
          "original_form": "ㅋㅋㅋㅋ맞습니다",
          "speaker_id": "2",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022310.1.6",
          "form": "거기가 처갓집보다 더 맛있어요",
          "original_form": "거기가 처갓집보다 더 맛있어요",
          "speaker_id": "2",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022310.1.7",
          "form": "ㅋㅋㅋㅋ 거기 양념맛있나요",
          "original_form": "ㅋㅋㅋㅋ 거기 양념맛있나요",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022310.1.8",
          "form": "엄청 달달한데",
          "original_form": "엄청 달달한데",
          "speaker_id": "2",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022310.1.9",
          "form": "처가집보다 더?",
          "original_form": "처가집보다 더?",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022310.1.10",
          "form": "또 매콤해요",
          "original_form": "또 매콤해요",
          "speaker_id": "2",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022310.1.11",
          "form": "대박 먹어봐야겠다",
          "original_form": "대박 먹어봐야겠다",
          "speaker_id": "1",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022310.1.12",
          "form": "근데 엄청 매콤하지 않고 적당하게 매콤해요",
          "original_form": "근데 엄청 매콤하지 않고 적당하게 매콤해요",
          "speaker_id": "2",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022310.1.13",
          "form": "닭 어느부위  좋아하세요",
          "original_form": "닭 어느부위  좋아하세요",
          "speaker_id": "1",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022310.1.14",
          "form": "저 순살부위요..",
          "original_form": "저 순살부위요..",
          "speaker_id": "2",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022310.1.15",
          "form": "나머지는 부위로 안쳐줘요",
          "original_form": "나머지는 부위로 안쳐줘요",
          "speaker_id": "2",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022310.1.16",
          "form": "순살?",
          "original_form": "순살?",
          "speaker_id": "1",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022310.1.17",
          "form": "네 즉 닭다리살",
          "original_form": "네 즉 닭다리살",
          "speaker_id": "2",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022310.1.18",
          "form": "아나",
          "original_form": "아나",
          "speaker_id": "1",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022310.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ왜요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ왜요",
