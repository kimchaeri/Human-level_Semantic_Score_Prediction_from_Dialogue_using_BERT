      "utterance": [
        {
          "id": "MDRW2100013533.1.1",
          "form": "군대 갔다오셨어요?",
          "original_form": "군대 갔다오셨어요?",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013533.1.2",
          "form": "저는 여자라서 안 가봤네요",
          "original_form": "저는 여자라서 안 가봤네요",
          "speaker_id": "1",
          "time": "20210825 14:55"
        },
        {
          "id": "MDRW2100013533.1.3",
          "form": "저도 안가봤어요",
          "original_form": "저도 안가봤어요",
          "speaker_id": "2",
          "time": "20210825 14:57"
        },
        {
          "id": "MDRW2100013533.1.4",
          "form": "여군이라도 지원해보실생각잇으신가요??",
          "original_form": "여군이라도 지원해보실생각잇으신가요??",
          "speaker_id": "2",
          "time": "20210825 14:57"
        },
        {
          "id": "MDRW2100013533.1.5",
          "form": "아니욥",
          "original_form": "아니욥",
          "speaker_id": "1",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013533.1.6",
          "form": "군대 가보고 싶으세요?",
          "original_form": "군대 가보고 싶으세요?",
          "speaker_id": "1",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013533.1.7",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013533.1.8",
          "form": "주변에 군대갔다오고나서",
          "original_form": "주변에 군대갔다오고나서",
          "speaker_id": "2",
          "time": "20210825 14:58"
        },
        {
          "id": "MDRW2100013533.1.9",
          "form": "부상입은 사람들이 종종있어서",
          "original_form": "부상입은 사람들이 종종있어서",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013533.1.10",
          "form": "별로 끌리지않네요",
          "original_form": "별로 끌리지않네요",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013533.1.11",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013533.1.12",
          "form": "저는 힘든게 싫어요",
          "original_form": "저는 힘든게 싫어요",
          "speaker_id": "1",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013533.1.13",
          "form": "안가도 되니까 개꿀이죠",
          "original_form": "안가도 되니까 개꿀이죠",
          "speaker_id": "1",
          "time": "20210825 14:59"
        },
        {
          "id": "MDRW2100013533.1.14",
          "form": "군대밥은 먹어보고 싶어요",
          "original_form": "군대밥은 먹어보고 싶어요",
          "speaker_id": "1",
          "time": "20210825 15:03"
        },
        {
          "id": "MDRW2100013533.1.15",
          "form": "사줘요",
          "original_form": "사줘요",
          "speaker_id": "1",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013533.1.16",
          "form": "한번 도전해보시길",
          "original_form": "한번 도전해보시길",
          "speaker_id": "2",
          "time": "20210825 15:04"
        },
        {
          "id": "MDRW2100013533.1.17",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013533.1.18",
          "form": "군대ㅜ체험같는건 없으려나요",
          "original_form": "군대ㅜ체험같는건 없으려나요",
          "speaker_id": "2",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013533.1.19",
          "form": "별로 안 끌리네요",
          "original_form": "별로 안 끌리네요",
          "speaker_id": "1",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013533.1.20",
          "form": "저는 비슷한체험해보니까 너무 별로라서",
          "original_form": "저는 비슷한체험해보니까 너무 별로라서",
          "speaker_id": "2",
          "time": "20210825 15:05"
        },
        {
          "id": "MDRW2100013533.1.21",
