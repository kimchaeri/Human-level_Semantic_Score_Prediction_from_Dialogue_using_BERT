      "utterance": [
        {
          "id": "MDRW2100000360.1.1",
          "form": "혹시 대학생이신가요?",
          "original_form": "혹시 대학생이신가요?",
          "speaker_id": "1",
          "time": "20210819 15:33"
        },
        {
          "id": "MDRW2100000360.1.2",
          "form": "아니요 직장인이에요",
          "original_form": "아니요 직장인이에요",
          "speaker_id": "2",
          "time": "20210819 15:33"
        },
        {
          "id": "MDRW2100000360.1.3",
          "form": "학생이신가요?",
          "original_form": "학생이신가요?",
          "speaker_id": "2",
          "time": "20210819 15:33"
        },
        {
          "id": "MDRW2100000360.1.4",
          "form": "네 전 학생이에요!",
          "original_form": "네 전 학생이에요!",
          "speaker_id": "1",
          "time": "20210819 15:34"
        },
        {
          "id": "MDRW2100000360.1.5",
          "form": "혹시 이번에 갤럭시 플립3 출시된더 보셨어요?",
          "original_form": "혹시 이번에 갤럭시 플립3 출시된더 보셨어요?",
          "speaker_id": "2",
          "time": "20210819 15:35"
        },
        {
          "id": "MDRW2100000360.1.6",
          "form": "헐 네네 봤어요!!",
          "original_form": "헐 네네 봤어요!!",
          "speaker_id": "1",
          "time": "20210819 15:35"
        },
        {
          "id": "MDRW2100000360.1.7",
          "form": "완전 예쁘더라구요",
          "original_form": "완전 예쁘더라구요",
          "speaker_id": "1",
          "time": "20210819 15:35"
        },
        {
          "id": "MDRW2100000360.1.8",
          "form": "그쵸?",
          "original_form": "그쵸?",
          "speaker_id": "2",
          "time": "20210819 15:35"
        },
        {
          "id": "MDRW2100000360.1.9",
          "form": "지금 바꿀 시기면 그걸로 하고싶은데ㅠㅠ",
          "original_form": "지금 바꿀 시기면 그걸로 하고싶은데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 15:35"
        },
        {
          "id": "MDRW2100000360.1.10",
          "form": "저도 너무 이뻐서 사고 싶더라구요ㅠㅜ",
          "original_form": "저도 너무 이뻐서 사고 싶더라구요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210819 15:35"
        },
        {
          "id": "MDRW2100000360.1.11",
          "form": "워치도 너무 탐나더라구요 알아보니 인바디도 된다고 해서",
          "original_form": "워치도 너무 탐나더라구요 알아보니 인바디도 된다고 해서",
          "speaker_id": "2",
          "time": "20210819 15:36"
        },
        {
          "id": "MDRW2100000360.1.12",
          "form": "헐 인바디도 되는구나",
          "original_form": "헐 인바디도 되는구나",
          "speaker_id": "1",
          "time": "20210819 15:37"
        },
        {
          "id": "MDRW2100000360.1.13",
          "form": "혹시 운동하세요?",
          "original_form": "혹시 운동하세요?",
          "speaker_id": "1",
          "time": "20210819 15:37"
        },
        {
          "id": "MDRW2100000360.1.14",
          "form": "아니요ㅋㅋ",
          "original_form": "아니요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 15:40"
        },
        {
          "id": "MDRW2100000360.1.15",
          "form": "운동한다고 신발삿는데 안하게 되네요",
          "original_form": "운동한다고 신발삿는데 안하게 되네요",
          "speaker_id": "2",
          "time": "20210819 15:40"
        },
        {
          "id": "MDRW2100000360.1.16",
          "form": "ㅋㅋㅋㅋㅋ저도 홈트한다고 신발이랑 요가매트에 운동복까지 샀는데",
          "original_form": "ㅋㅋㅋㅋㅋ저도 홈트한다고 신발이랑 요가매트에 운동복까지 샀는데",
          "speaker_id": "1",
          "time": "20210819 15:41"
        },
        {
          "id": "MDRW2100000360.1.17",
          "form": "한 한달하고 쭉 안하고 있어요ㅋㅋㅋ",
          "original_form": "한 한달하고 쭉 안하고 있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 15:41"
        },
        {
          "id": "MDRW2100000360.1.18",
          "form": "앜ㅋㅋ",
          "original_form": "앜ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 15:41"
        },
        {
          "id": "MDRW2100000360.1.19",
          "form": "저는 심지어 홈트한다구 그 핑계로 아이패드 삿는데",
          "original_form": "저는 심지어 홈트한다구 그 핑계로 아이패드 삿는데",
          "speaker_id": "2",
          "time": "20210819 15:41"
        },
        {
          "id": "MDRW2100000360.1.20",
          "form": "홈트는 무슨...넷플 보기 바쁘네요...",
          "original_form": "홈트는 무슨...넷플 보기 바쁘네요...",
          "speaker_id": "2",
          "time": "20210819 15:42"
        },
        {
          "id": "MDRW2100000360.1.21",
          "form": "ㅋㅋㅋㅋㅋ맞아요 돈 쓰고 뽕 뽑은게 넷플이랑 유튜브 프리미엄 뿐....",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 돈 쓰고 뽕 뽑은게 넷플이랑 유튜브 프리미엄 뿐....",
          "speaker_id": "1",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000360.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000360.1.23",
          "form": "딥디크라고 향수아세요?",
          "original_form": "딥디크라고 향수아세요?",
          "speaker_id": "2",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000360.1.24",
          "form": "네네 향수 좋아하세요?",
          "original_form": "네네 향수 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 15:44"
        },
        {
          "id": "MDRW2100000360.1.25",
          "form": "앗 아니요 엊그제 딥디크 향수 세일하길래 샀거든요",
          "original_form": "앗 아니요 엊그제 딥디크 향수 세일하길래 샀거든요",
          "speaker_id": "2",
          "time": "20210819 15:45"
        },
        {
          "id": "MDRW2100000360.1.26",
          "form": "3개 샀는데 쿠폰 먹여서 19만원? 정도로 구매했어요",
          "original_form": "3개 샀는데 쿠폰 먹여서 19만원? 정도로 구매했어요",
          "speaker_id": "2",
          "time": "20210819 15:45"
        },
        {
          "id": "MDRW2100000360.1.27",
          "form": "향수 잘 아는 친구한테 물어보니딴 득템햇다고 하더라구요",
          "original_form": "향수 잘 아는 친구한테 물어보니딴 득템햇다고 하더라구요",
          "speaker_id": "2",
          "time": "20210819 15:46"
        },
        {
          "id": "MDRW2100000360.1.28",
          "form": "3개에 19만원이면 진짜 완전 득템하셨네요!!",
          "original_form": "3개에 19만원이면 진짜 완전 득템하셨네요!!",
          "speaker_id": "1",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000360.1.29",
          "form": "그쵸! 택배도착햇다고 해서 기대되네요ㅎㅎ 시향을 한번도 안해봐가지구..ㅎ",
          "original_form": "그쵸! 택배도착햇다고 해서 기대되네요ㅎㅎ 시향을 한번도 안해봐가지구..ㅎ",
          "speaker_id": "2",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000360.1.30",
          "form": "최근에 구매란 물건 뭐잇으세요?",
          "original_form": "최근에 구매란 물건 뭐잇으세요?",
          "speaker_id": "2",
          "time": "20210819 16:07"
        },
        {
          "id": "MDRW2100000360.1.31",
          "form": "음... 수분크림이요!!",
          "original_form": "음... 수분크림이요!!",
          "speaker_id": "1",
          "time": "20210819 16:17"
        },
        {
          "id": "MDRW2100000360.1.32",
          "form": "오 수분크림이여?",
          "original_form": "오 수분크림이여?",
          "speaker_id": "2",
          "time": "20210819 18:04"
        },
        {
          "id": "MDRW2100000360.1.33",
          "form": "넹 라운드랩 자작나무 수분크림이요ㅋㅋ",
          "original_form": "넹 라운드랩 자작나무 수분크림이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:06"
        },
        {
          "id": "MDRW2100000360.1.34",
          "form": "괜찮더라구요",
          "original_form": "괜찮더라구요",
          "speaker_id": "1",
          "time": "20210819 18:06"
        },
        {
          "id": "MDRW2100000360.1.35",
          "form": "저도 거기 나오는 토너 써봣는데",
          "original_form": "저도 거기 나오는 토너 써봣는데",
          "speaker_id": "2",
          "time": "20210819 18:07"
        },
        {
          "id": "MDRW2100000360.1.36",
          "form": "괜찮더라구요 무난무난",
          "original_form": "괜찮더라구요 무난무난",
