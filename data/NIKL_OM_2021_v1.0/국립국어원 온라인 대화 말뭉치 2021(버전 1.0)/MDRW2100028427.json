      "utterance": [
        {
          "id": "MDRW2100028427.1.1",
          "form": "부산도 비가 매일 와",
          "original_form": "부산도 비가 매일 와",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028427.1.2",
          "form": "여름에 내리지 않았걷ㄴ",
          "original_form": "여름에 내리지 않았걷ㄴ",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028427.1.3",
          "form": "거든",
          "original_form": "거든",
          "speaker_id": "2",
          "time": "20210902 19:51"
        },
        {
          "id": "MDRW2100028427.1.4",
          "form": "그렇다고 못견딜만큼 덥지는 않았어",
          "original_form": "그렇다고 못견딜만큼 덥지는 않았어",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028427.1.5",
          "form": "요즘은 여름이 덜 더워",
          "original_form": "요즘은 여름이 덜 더워",
          "speaker_id": "2",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028427.1.6",
          "form": "하지만 습도가 높아",
          "original_form": "하지만 습도가 높아",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028427.1.7",
          "form": "건조해야 빨래가 잘 마르지",
          "original_form": "건조해야 빨래가 잘 마르지",
          "speaker_id": "2",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028427.1.8",
          "form": "햇살은 따사로워",
          "original_form": "햇살은 따사로워",
          "speaker_id": "1",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100028427.1.9",
          "form": "제주도는 구름이 예뻐",
          "original_form": "제주도는 구름이 예뻐",
          "speaker_id": "2",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028427.1.10",
          "form": "제주도 하늘은 맑아",
          "original_form": "제주도 하늘은 맑아",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028427.1.11",
          "form": "미세먼지도 없디",
          "original_form": "미세먼지도 없디",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028427.1.12",
          "form": "공기가 좋아",
          "original_form": "공기가 좋아",
          "speaker_id": "2",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028427.1.13",
          "form": "여름은 특유의 냄새가 있어",
          "original_form": "여름은 특유의 냄새가 있어",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028427.1.14",
          "form": "겨울도",
          "original_form": "겨울도",
          "speaker_id": "1",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028427.1.15",
          "form": "사계절이 좋아",
          "original_form": "사계절이 좋아",
          "speaker_id": "2",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100028427.1.16",
          "form": "봄 여름 가을 겨울",
          "original_form": "봄 여름 가을 겨울",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028427.1.17",
          "form": "곧 가을이네",
          "original_form": "곧 가을이네",
          "speaker_id": "1",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028427.1.18",
          "form": "처서가 지났어",
          "original_form": "처서가 지났어",
          "speaker_id": "1",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028427.1.19",
          "form": "건조해지겠다",
          "original_form": "건조해지겠다",
          "speaker_id": "2",
          "time": "20210902 19:54"
        },
        {
          "id": "MDRW2100028427.1.20",
          "form": "베트남은 여름만 있어",
          "original_form": "베트남은 여름만 있어",
          "speaker_id": "2",
          "time": "20210902 19:54"
        }
      ]
