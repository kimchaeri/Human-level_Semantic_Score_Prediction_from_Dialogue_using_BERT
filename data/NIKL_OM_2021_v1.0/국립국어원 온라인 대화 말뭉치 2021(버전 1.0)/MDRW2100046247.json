      "utterance": [
        {
          "id": "MDRW2100046247.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211015 11:43"
        },
        {
          "id": "MDRW2100046247.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20211015 11:44"
        },
        {
          "id": "MDRW2100046247.1.3",
          "form": "이번 월요일 대체공휴일 쉬셨나요? ",
          "original_form": "이번 월요일 대체공휴일 쉬셨나요? ",
          "speaker_id": "2",
          "time": "20211015 11:44"
        },
        {
          "id": "MDRW2100046247.1.4",
          "form": "네 쉬었습니다",
          "original_form": "네 쉬었습니다",
          "speaker_id": "1",
          "time": "20211015 11:45"
        },
        {
          "id": "MDRW2100046247.1.5",
          "form": "여러분도 쉬셨나요",
          "original_form": "여러분도 쉬셨나요",
          "speaker_id": "1",
          "time": "20211015 11:45"
        },
        {
          "id": "MDRW2100046247.1.6",
          "form": "다행이네요! ",
          "original_form": "다행이네요! ",
          "speaker_id": "2",
          "time": "20211015 11:45"
        },
        {
          "id": "MDRW2100046247.1.7",
          "form": "네 저희도 쉬었어요",
          "original_form": "네 저희도 쉬었어요",
          "speaker_id": "2",
          "time": "20211015 11:45"
        },
        {
          "id": "MDRW2100046247.1.8",
          "form": "2주 연속 주 4일제로 일했더니",
          "original_form": "2주 연속 주 4일제로 일했더니",
          "speaker_id": "2",
          "time": "20211015 11:46"
        },
        {
          "id": "MDRW2100046247.1.9",
          "form": "다음 주가 걱정이네요 적응못할까봐 ㅋㅋ",
          "original_form": "다음 주가 걱정이네요 적응못할까봐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211015 11:46"
        },
        {
          "id": "MDRW2100046247.1.10",
          "form": "어디서 보니까 올해는 이제 휴일이 없다는데 성탄절은 대체휴일이 없나봐요",
          "original_form": "어디서 보니까 올해는 이제 휴일이 없다는데 성탄절은 대체휴일이 없나봐요",
          "speaker_id": "1",
          "time": "20211015 11:47"
        },
        {
          "id": "MDRW2100046247.1.11",
          "form": "그니까요!! ㅠㅠ",
          "original_form": "그니까요!! ㅠㅠ",
          "speaker_id": "2",
          "time": "20211015 11:48"
        },
        {
          "id": "MDRW2100046247.1.12",
          "form": "다음 빨간날이 설날이라니",
          "original_form": "다음 빨간날이 설날이라니",
          "speaker_id": "2",
          "time": "20211015 11:48"
        },
        {
          "id": "MDRW2100046247.1.13",
          "form": "흙흑",
          "original_form": "흙흑",
          "speaker_id": "2",
          "time": "20211015 11:48"
        },
        {
          "id": "MDRW2100046247.1.14",
          "form": "성탄절도 대체휴일 해주지 아쉽네요",
          "original_form": "성탄절도 대체휴일 해주지 아쉽네요",
          "speaker_id": "1",
          "time": "20211015 11:48"
        },
        {
          "id": "MDRW2100046247.1.15",
          "form": "사장님들은 휴일 많으면 힘들겠죠?ㅠ ",
          "original_form": "사장님들은 휴일 많으면 힘들겠죠?ㅠ ",
          "speaker_id": "2",
          "time": "20211015 11:49"
        },
        {
          "id": "MDRW2100046247.1.16",
          "form": "신정도 토요일이네요",
          "original_form": "신정도 토요일이네요",
          "speaker_id": "1",
          "time": "20211015 11:49"
        },
        {
          "id": "MDRW2100046247.1.17",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "2",
          "time": "20211015 11:49"
        },
        {
          "id": "MDRW2100046247.1.18",
          "form": "그렇겠죠 아무래도",
          "original_form": "그렇겠죠 아무래도",
          "speaker_id": "1",
          "time": "20211015 11:49"
        },
        {
          "id": "MDRW2100046247.1.19",
          "form": "하지만 저는 사장님 될 일이 없어서 계속 대체휴일 추가를 기원하고 있답니다",
          "original_form": "하지만 저는 사장님 될 일이 없어서 계속 대체휴일 추가를 기원하고 있답니다",
          "speaker_id": "2",
          "time": "20211015 11:50"
        },
        {
          "id": "MDRW2100046247.1.20",
          "form": "호홍",
          "original_form": "호홍",
          "speaker_id": "2",
          "time": "20211015 11:50"
        },
        {
          "id": "MDRW2100046247.1.21",
          "form": "성탄절, 신정은 아직 결정이 안된건가요 아니면 대체휴일 없는걸로 결정된건가요",
          "original_form": "성탄절, 신정은 아직 결정이 안된건가요 아니면 대체휴일 없는걸로 결정된건가요",
          "speaker_id": "1",
          "time": "20211015 11:51"
        },
        {
          "id": "MDRW2100046247.1.22",
          "form": "정확히 아시는분 계시는지",
          "original_form": "정확히 아시는분 계시는지",
          "speaker_id": "1",
          "time": "20211015 11:52"
        },
        {
          "id": "MDRW2100046247.1.23",
          "form": "광복절도 거의 다 돼서 결정되던데",
          "original_form": "광복절도 거의 다 돼서 결정되던데",
          "speaker_id": "2",
          "time": "20211015 11:52"
        },
        {
          "id": "MDRW2100046247.1.24",
          "form": "성탄절도 조금은 기대해볼까 생각중이에요",
          "original_form": "성탄절도 조금은 기대해볼까 생각중이에요",
          "speaker_id": "2",
          "time": "20211015 11:52"
        }
      ]
    }
