      "utterance": [
        {
          "id": "MDRW2100019071.1.1",
          "form": "먹는 얘기가 제일이죠",
          "original_form": "먹는 얘기가 제일이죠",
          "speaker_id": "2",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100019071.1.2",
          "form": "할말이 제일 많아요",
          "original_form": "할말이 제일 많아요",
          "speaker_id": "1",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100019071.1.3",
          "form": "먹는 거에 진심인 민족이니까요ㅋㅋ",
          "original_form": "먹는 거에 진심인 민족이니까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100019071.1.4",
          "form": "그러게요ㅋㅋ",
          "original_form": "그러게요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100019071.1.5",
          "form": "요즘 유행하는 복숭아 디저트 먹어보셨어요?",
          "original_form": "요즘 유행하는 복숭아 디저트 먹어보셨어요?",
          "speaker_id": "2",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100019071.1.6",
          "form": "네 고소하고달고 맛있어요",
          "original_form": "네 고소하고달고 맛있어요",
          "speaker_id": "1",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100019071.1.7",
          "form": "오 먹어봐야겠네요",
          "original_form": "오 먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100019071.1.8",
          "form": "네 추천합니다",
          "original_form": "네 추천합니다",
          "speaker_id": "1",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100019071.1.9",
          "form": "커피를 마셨는데도 졸리고 과자땡기네요",
          "original_form": "커피를 마셨는데도 졸리고 과자땡기네요",
          "speaker_id": "2",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100019071.1.10",
          "form": "좋아하는 과자 있으세요?",
          "original_form": "좋아하는 과자 있으세요?",
          "speaker_id": "1",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100019071.1.11",
          "form": "초코맛 꼬북칩이요",
          "original_form": "초코맛 꼬북칩이요",
          "speaker_id": "2",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100019071.1.12",
          "form": "아 저도 그거좋아해요ㅠㅜ",
          "original_form": "아 저도 그거좋아해요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 15:06"
        },
        {
          "id": "MDRW2100019071.1.13",
          "form": "게리치즈맛도 진짜 맛있어요",
          "original_form": "게리치즈맛도 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210827 15:06"
        },
        {
          "id": "MDRW2100019071.1.14",
          "form": "과자 이름이 게린가요?",
          "original_form": "과자 이름이 게린가요?",
          "speaker_id": "1",
          "time": "20210827 15:06"
        },
        {
          "id": "MDRW2100019071.1.15",
          "form": "네ㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:06"
        },
        {
          "id": "MDRW2100019071.1.16",
          "form": "선칩이나 감자칩류 좋아해요",
          "original_form": "선칩이나 감자칩류 좋아해요",
