      "utterance": [
        {
          "id": "MDRW2100032550.1.1",
          "form": "제일 힘들고",
          "original_form": "제일 힘들고",
          "speaker_id": "2",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100032550.1.2",
          "form": "오늘 알바는 괜탄ㄹ았어?",
          "original_form": "오늘 알바는 괜탄ㄹ았어?",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100032550.1.3",
          "form": "응 머가?",
          "original_form": "응 머가?",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100032550.1.4",
          "form": "살 제일 많이 빠지지 않나",
          "original_form": "살 제일 많이 빠지지 않나",
          "speaker_id": "2",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100032550.1.5",
          "form": "아 운동 처음했을때가",
          "original_form": "아 운동 처음했을때가",
          "speaker_id": "2",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100032550.1.6",
          "form": "그렇다고",
          "original_form": "그렇다고",
          "speaker_id": "2",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100032550.1.7",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100032550.1.8",
          "form": "알바...",
          "original_form": "알바...",
          "speaker_id": "2",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100032550.1.9",
          "form": "싫었지",
          "original_form": "싫었지",
          "speaker_id": "2",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100032550.1.10",
          "form": "사장은 뭐 아무말 안했는데",
          "original_form": "사장은 뭐 아무말 안했는데",
          "speaker_id": "2",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100032550.1.11",
          "form": "나 식단은 오래해도 운동은 꾸준히 해본적이없어서...ㅎㅎ><",
          "original_form": "나 식단은 오래해도 운동은 꾸준히 해본적이없어서...ㅎㅎ><",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100032550.1.12",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210905 20:36"
        },
        {
          "id": "MDRW2100032550.1.13",
          "form": "약간 name3처럼 계속 이야기 하지는 않는듯",
          "original_form": "약간 &name3&처럼 계속 이야기 하지는 않는듯",
          "speaker_id": "2",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100032550.1.14",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100032550.1.15",
          "form": "근데 매니저 언니랑 일하는거 너무 불편함",
          "original_form": "근데 매니저 언니랑 일하는거 너무 불편함",
          "speaker_id": "2",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100032550.1.16",
          "form": "뒤끝은 업ㄱ는편?",
          "original_form": "뒤끝은 업ㄱ는편?",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100032550.1.17",
          "form": "왜왜",
          "original_form": "왜왜",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100032550.1.18",
          "form": "약간 name4 느낌?",
          "original_form": "약간 &name4& 느낌?",
          "speaker_id": "2",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100032550.1.19",
          "form": "엥?!?!?",
          "original_form": "엥?!?!?",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100032550.1.20",
          "form": "그 뭐라해야하지",
          "original_form": "그 뭐라해야하지",
          "speaker_id": "2",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100032550.1.21",
          "form": "다...다큰성인이?!?!",
          "original_form": "다...다큰성인이?!?!",
          "speaker_id": "1",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100032550.1.22",
          "form": "그 성격 오락가락 하는거라 해야하나",
          "original_form": "그 성격 오락가락 하는거라 해야하나",
          "speaker_id": "2",
          "time": "20210905 20:37"
        },
        {
          "id": "MDRW2100032550.1.23",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210905 20:38"
        },
        {
          "id": "MDRW2100032550.1.24",
