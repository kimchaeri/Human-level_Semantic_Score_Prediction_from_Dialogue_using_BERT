      "utterance": [
        {
          "id": "MDRW2100035222.1.1",
          "form": "역시 1위는 이시영이다",
          "original_form": "역시 1위는 이시영이다",
          "speaker_id": "2",
          "time": "20210829 12:07"
        },
        {
          "id": "MDRW2100035222.1.2",
          "form": "저번에 넷플릭스 드라마에서 몸이 장난이 아니더라",
          "original_form": "저번에 넷플릭스 드라마에서 몸이 장난이 아니더라",
          "speaker_id": "1",
          "time": "20210829 12:07"
        },
        {
          "id": "MDRW2100035222.1.3",
          "form": "복싱 국가대표 선발전에서 우승까지 했네",
          "original_form": "복싱 국가대표 선발전에서 우승까지 했네",
          "speaker_id": "2",
          "time": "20210829 12:07"
        },
        {
          "id": "MDRW2100035222.1.4",
          "form": "아 맞다. 우승했다",
          "original_form": "아 맞다. 우승했다",
          "speaker_id": "1",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100035222.1.5",
          "form": "전참시에서 나오는거 보니 맨날 운동하더라",
          "original_form": "전참시에서 나오는거 보니 맨날 운동하더라",
          "speaker_id": "2",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100035222.1.6",
          "form": "전에 보니 결혼도 잘했던데",
          "original_form": "전에 보니 결혼도 잘했던데",
          "speaker_id": "1",
          "time": "20210829 12:08"
        },
        {
          "id": "MDRW2100035222.1.7",
          "form": "남편이 운동을 같이 해줘서 결혼했는데 결혼하고 운동을 같이 안한다네",
          "original_form": "남편이 운동을 같이 해줘서 결혼했는데 결혼하고 운동을 같이 안한다네",
          "speaker_id": "2",
          "time": "20210829 12:09"
        },
        {
          "id": "MDRW2100035222.1.8",
          "form": "결혼전이니 했겠지ㅋ",
          "original_form": "결혼전이니 했겠지ㅋ",
          "speaker_id": "1",
          "time": "20210829 12:09"
        },
        {
          "id": "MDRW2100035222.1.9",
          "form": "넷플렉스 애기하네",
          "original_form": "넷플렉스 애기하네",
          "speaker_id": "2",
          "time": "20210829 12:09"
        },
        {
          "id": "MDRW2100035222.1.10",
          "form": "진짜 양악수술이 신의 한수다",
          "original_form": "진짜 양악수술이 신의 한수다",
          "speaker_id": "1",
          "time": "20210829 12:10"
        },
        {
          "id": "MDRW2100035222.1.11",
          "form": "등근육 사진 보니 장난 아니다",
          "original_form": "등근육 사진 보니 장난 아니다",
          "speaker_id": "2",
          "time": "20210829 12:10"
        },
        {
          "id": "MDRW2100035222.1.12",
          "form": "전문가도 인정함",
          "original_form": "전문가도 인정함",
          "speaker_id": "1",
          "time": "20210829 12:10"
        },
        {
          "id": "MDRW2100035222.1.13",
          "form": "아 양악수술 했나? 몰랐다",
          "original_form": "아 양악수술 했나? 몰랐다",
          "speaker_id": "2",
          "time": "20210829 12:10"
        }
