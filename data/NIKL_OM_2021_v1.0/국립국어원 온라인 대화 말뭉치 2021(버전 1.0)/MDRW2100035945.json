      "utterance": [
        {
          "id": "MDRW2100035945.1.1",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "1",
          "time": "20210830 12:30"
        },
        {
          "id": "MDRW2100035945.1.2",
          "form": "식사는 하셨어요?",
          "original_form": "식사는 하셨어요?",
          "speaker_id": "1",
          "time": "20210830 12:30"
        },
        {
          "id": "MDRW2100035945.1.3",
          "form": "곧 먹으려구요 name1님은요?",
          "original_form": "곧 먹으려구요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 12:32"
        },
        {
          "id": "MDRW2100035945.1.4",
          "form": "저는 뭐 먹을지 아직 고민하고 있어용ㅎㅎ",
          "original_form": "저는 뭐 먹을지 아직 고민하고 있어용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100035945.1.5",
          "form": "뭐 드실거에용?",
          "original_form": "뭐 드실거에용?",
          "speaker_id": "1",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100035945.1.6",
          "form": "열무국수 해먹으려구요",
          "original_form": "열무국수 해먹으려구요",
          "speaker_id": "2",
          "time": "20210830 12:44"
        },
        {
          "id": "MDRW2100035945.1.7",
          "form": "오오 맛있겠네용~~",
          "original_form": "오오 맛있겠네용~~",
          "speaker_id": "1",
          "time": "20210830 12:45"
        },
        {
          "id": "MDRW2100035945.1.8",
          "form": "저는 고민 끝냈어요 수제버거 먹으려구요!",
          "original_form": "저는 고민 끝냈어요 수제버거 먹으려구요!",
          "speaker_id": "1",
          "time": "20210830 12:45"
        },
        {
          "id": "MDRW2100035945.1.9",
          "form": "헐 수제버거 맛있겠어요",
          "original_form": "헐 수제버거 맛있겠어요",
          "speaker_id": "2",
          "time": "20210830 12:46"
        },
        {
          "id": "MDRW2100035945.1.10",
          "form": "안먹은지 오래됐는데",
          "original_form": "안먹은지 오래됐는데",
          "speaker_id": "2",
          "time": "20210830 12:47"
        },
        {
          "id": "MDRW2100035945.1.11",
          "form": "수제버거 좋아하시나봐요ㅎㅎ",
          "original_form": "수제버거 좋아하시나봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 12:47"
        },
        {
          "id": "MDRW2100035945.1.12",
          "form": "아무래도 직접 가서 먹는 경우가 많은데 코로나때문에...ㅠ",
          "original_form": "아무래도 직접 가서 먹는 경우가 많은데 코로나때문에...ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:47"
        },
        {
          "id": "MDRW2100035945.1.13",
          "form": "예전에 이태원에서 수제버거 먹어봤는데 너무 좋아서요",
          "original_form": "예전에 이태원에서 수제버거 먹어봤는데 너무 좋아서요",
          "speaker_id": "2",
          "time": "20210830 12:48"
        },
        {
          "id": "MDRW2100035945.1.14",
          "form": "코로나ㅠ 때문에 갈수가 없네요",
          "original_form": "코로나ㅠ 때문에 갈수가 없네요",
          "speaker_id": "2",
          "time": "20210830 12:48"
        },
        {
          "id": "MDRW2100035945.1.15",
          "form": "아 이태원에 수제버거 맛집 진짜 많죠ㅎㅎ",
          "original_form": "아 이태원에 수제버거 맛집 진짜 많죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 12:48"
        },
        {
          "id": "MDRW2100035945.1.16",
          "form": "무슨 음식 좋아하세요?",
          "original_form": "무슨 음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 12:48"
        },
        {
          "id": "MDRW2100035945.1.17",
          "form": "저는 간단하게 라면 좋아해요",
          "original_form": "저는 간단하게 라면 좋아해요",
          "speaker_id": "2",
          "time": "20210831 11:09"
        },
        {
          "id": "MDRW2100035945.1.18",
          "form": "저도 라면 좋아해요! 참깨라면ㅋㅋㅋ",
          "original_form": "저도 라면 좋아해요! 참깨라면ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:54"
        },
        {
          "id": "MDRW2100035945.1.19",
          "form": "고소하더라구요",
          "original_form": "고소하더라구요",
          "speaker_id": "1",
          "time": "20210831 12:54"
        },
        {
          "id": "MDRW2100035945.1.20",
          "form": "맞아요 참깨라면 고추기름 유무에 따라서 맛 다른것도 매력이에요",
          "original_form": "맞아요 참깨라면 고추기름 유무에 따라서 맛 다른것도 매력이에요",
          "speaker_id": "2",
          "time": "20210831 12:56"
        },
        {
          "id": "MDRW2100035945.1.21",
          "form": "네네 저는 꼭 풀어먹어요 계란도 추가하구요ㅎㅎ",
          "original_form": "네네 저는 꼭 풀어먹어요 계란도 추가하구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:56"
        },
        {
          "id": "MDRW2100035945.1.22",
          "form": "맞아요 계란블럭만으로는 부족하죠",
          "original_form": "맞아요 계란블럭만으로는 부족하죠",
          "speaker_id": "2",
          "time": "20210831 12:57"
        }
      ]
