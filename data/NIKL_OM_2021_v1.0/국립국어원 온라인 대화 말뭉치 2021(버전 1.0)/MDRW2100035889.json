      "utterance": [
        {
          "id": "MDRW2100035889.1.1",
          "form": "파스타 필수입니다..",
          "original_form": "파스타 필수입니다..",
          "speaker_id": "2",
          "time": "20210830 10:40"
        },
        {
          "id": "MDRW2100035889.1.2",
          "form": "파스타 뭐 드셨어용??",
          "original_form": "파스타 뭐 드셨어용??",
          "speaker_id": "1",
          "time": "20210830 10:40"
        },
        {
          "id": "MDRW2100035889.1.3",
          "form": "뭐가 맛있나용?",
          "original_form": "뭐가 맛있나용?",
          "speaker_id": "1",
          "time": "20210830 10:40"
        },
        {
          "id": "MDRW2100035889.1.4",
          "form": "까르보나라 먹었는데 맛있었어요!",
          "original_form": "까르보나라 먹었는데 맛있었어요!",
          "speaker_id": "2",
          "time": "20210830 10:41"
        },
        {
          "id": "MDRW2100035889.1.5",
          "form": "다들 치즈오븐스파게티를 드시긴했지만요 ㅋㅋㅋㅋㅋ",
          "original_form": "다들 치즈오븐스파게티를 드시긴했지만요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 10:41"
        },
        {
          "id": "MDRW2100035889.1.6",
          "form": "헐 둘다 맛있겠다ㅠㅠ",
          "original_form": "헐 둘다 맛있겠다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 10:42"
        },
        {
          "id": "MDRW2100035889.1.7",
          "form": "오븐 스파게티에 까르보나라...",
          "original_form": "오븐 스파게티에 까르보나라...",
          "speaker_id": "1",
          "time": "20210830 10:42"
        },
        {
          "id": "MDRW2100035889.1.8",
          "form": "언제 부산으로 식도락 여행을 떠나야겠어요",
          "original_form": "언제 부산으로 식도락 여행을 떠나야겠어요",
          "speaker_id": "1",
          "time": "20210830 10:42"
        },
        {
          "id": "MDRW2100035889.1.9",
          "form": "어차피 코로나라 해외도 못가는데ㅋㅋㅋㅋ 잠잠해지면ㅠㅠ",
          "original_form": "어차피 코로나라 해외도 못가는데ㅋㅋㅋㅋ 잠잠해지면ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 10:42"
        },
        {
          "id": "MDRW2100035889.1.10",
          "form": "진짜로 저 부산에 바다보러갔다가 비와서 그냥 먹으러 다녔는데.... 와 너무 맛있었어요!! 저도요ㅠㅠㅠ",
          "original_form": "진짜로 저 부산에 바다보러갔다가 비와서 그냥 먹으러 다녔는데.... 와 너무 맛있었어요!! 저도요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 10:43"
        },
        {
          "id": "MDRW2100035889.1.11",
          "form": "국내여행이라도 많이가고싶네요ㅠ",
          "original_form": "국내여행이라도 많이가고싶네요ㅠ",
          "speaker_id": "2",
          "time": "20210830 10:43"
        },
        {
          "id": "MDRW2100035889.1.12",
          "form": "부산 돼지국밥도 유명하자나용!",
          "original_form": "부산 돼지국밥도 유명하자나용!",
          "speaker_id": "1",
          "time": "20210830 10:43"
        },
        {
          "id": "MDRW2100035889.1.13",
          "form": "입에 맞으셨어용?",
          "original_form": "입에 맞으셨어용?",
          "speaker_id": "1",
          "time": "20210830 10:43"
        },
        {
          "id": "MDRW2100035889.1.14",
          "form": "저는 국밥을 싫어하는 줄 알았는데 생각보다 맛있더라고요!!!!!!!!!",
          "original_form": "저는 국밥을 싫어하는 줄 알았는데 생각보다 맛있더라고요!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210830 10:44"
        },
        {
          "id": "MDRW2100035889.1.15",
          "form": "아 그럼 찐이네요",
          "original_form": "아 그럼 찐이네요",
          "speaker_id": "1",
          "time": "20210830 10:44"
        },
        {
          "id": "MDRW2100035889.1.16",
          "form": "부산 국밥 인정!",
          "original_form": "부산 국밥 인정!",
          "speaker_id": "1",
          "time": "20210830 10:44"
        },
        {
          "id": "MDRW2100035889.1.17",
          "form": "네! 저 깜짝 놀랐어요 ㅋㅋㅋㅋㅋ",
          "original_form": "네! 저 깜짝 놀랐어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 10:44"
        },
        {
          "id": "MDRW2100035889.1.18",
          "form": "그 밀면도 진짜 맛있었어요",
          "original_form": "그 밀면도 진짜 맛있었어요",
          "speaker_id": "2",
          "time": "20210830 10:44"
        },
        {
          "id": "MDRW2100035889.1.19",
          "form": "저도 국밥은 그렇게 좋아하는편은 아닌데",
          "original_form": "저도 국밥은 그렇게 좋아하는편은 아닌데",
          "speaker_id": "1",
          "time": "20210830 10:45"
        },
        {
          "id": "MDRW2100035889.1.20",
          "form": "돼지국밥 먹으러",
          "original_form": "돼지국밥 먹으러",
          "speaker_id": "1",
          "time": "20210830 10:45"
        },
        {
          "id": "MDRW2100035889.1.21",
          "form": "부산간다는 사람들이 주위에",
          "original_form": "부산간다는 사람들이 주위에",
          "speaker_id": "1",
          "time": "20210830 10:45"
        },
        {
          "id": "MDRW2100035889.1.22",
          "form": "간혹 있어서",
          "original_form": "간혹 있어서",
          "speaker_id": "1",
          "time": "20210830 10:45"
        },
        {
          "id": "MDRW2100035889.1.23",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:45"
        },
        {
          "id": "MDRW2100035889.1.24",
          "form": "저도요 저도요ㅋㅋㅋㅋㅋ 그 특유의 냄새를 진짜 싫어해서ㅜㅜㅋㅋㅋㅋ 와!",
          "original_form": "저도요 저도요ㅋㅋㅋㅋㅋ 그 특유의 냄새를 진짜 싫어해서ㅜㅜㅋㅋㅋㅋ 와!",
          "speaker_id": "2",
          "time": "20210830 10:45"
        },
        {
          "id": "MDRW2100035889.1.25",
          "form": "맞아요 제 주위도 그렇더라고요!",
          "original_form": "맞아요 제 주위도 그렇더라고요!",
          "speaker_id": "2",
          "time": "20210830 10:45"
        },
        {
          "id": "MDRW2100035889.1.26",
          "form": "얼마나 맛있나 싶었겨든요",
          "original_form": "얼마나 맛있나 싶었겨든요",
          "speaker_id": "1",
          "time": "20210830 10:45"
        },
        {
          "id": "MDRW2100035889.1.27",
          "form": "밀면도 메모메모...",
          "original_form": "밀면도 메모메모...",
          "speaker_id": "1",
          "time": "20210830 10:45"
        },
        {
          "id": "MDRW2100035889.1.28",
          "form": "하 진짜...넘 먹고싶네요ㅠㅠ name1님 점심메뉴 정하셨나요?!",
          "original_form": "하 진짜...넘 먹고싶네요ㅠㅠ &name1&님 점심메뉴 정하셨나요?!",
          "speaker_id": "2",
          "time": "20210830 10:46"
        },
        {
          "id": "MDRW2100035889.1.29",
          "form": "저 도미노 피자 먹으려구용!ㅎㅎㅎ",
          "original_form": "저 도미노 피자 먹으려구용!ㅎㅎㅎ",
