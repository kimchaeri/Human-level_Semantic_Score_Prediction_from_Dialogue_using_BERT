      "utterance": [
        {
          "id": "MDRW2100004923.1.1",
          "form": "저녁은 드셨나요?",
          "original_form": "저녁은 드셨나요?",
          "speaker_id": "1",
          "time": "20210821 19:19"
        },
        {
          "id": "MDRW2100004923.1.2",
          "form": "아직이요 점심을 늦게먹어서^^",
          "original_form": "아직이요 점심을 늦게먹어서^^",
          "speaker_id": "2",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004923.1.3",
          "form": "식사하셨어요?",
          "original_form": "식사하셨어요?",
          "speaker_id": "2",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004923.1.4",
          "form": "그렇군요^^ 뭐드시나요? 저는 감자부침개를 해먹으려고요ㅎㅎ",
          "original_form": "그렇군요^^ 뭐드시나요? 저는 감자부침개를 해먹으려고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004923.1.5",
          "form": "오 비오는날 부침개 좋죠",
          "original_form": "오 비오는날 부침개 좋죠",
          "speaker_id": "2",
          "time": "20210821 19:21"
        },
        {
          "id": "MDRW2100004923.1.6",
          "form": "감자전 저도 좋아해요",
          "original_form": "감자전 저도 좋아해요",
          "speaker_id": "2",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004923.1.7",
          "form": "그죠 이상하게 비만오면 부침개가 땡겨요ㅋ",
          "original_form": "그죠 이상하게 비만오면 부침개가 땡겨요ㅋ",
          "speaker_id": "1",
          "time": "20210821 19:22"
        },
        {
          "id": "MDRW2100004923.1.8",
          "form": "빗소리랑 비슷해서 그렇대요",
          "original_form": "빗소리랑 비슷해서 그렇대요",
          "speaker_id": "2",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004923.1.9",
          "form": "부침개 지지는 소리가^^",
          "original_form": "부침개 지지는 소리가^^",
          "speaker_id": "2",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004923.1.10",
          "form": "아ㅋㅋ 그렇네요 정말 신기하네요ㅋㅋ",
          "original_form": "아ㅋㅋ 그렇네요 정말 신기하네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004923.1.11",
          "form": "감자는 채썰어서넣나요?",
          "original_form": "감자는 채썰어서넣나요?",
          "speaker_id": "2",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004923.1.12",
          "form": "네네 저는 재료준비만 도와드리고 ㅎㅎ",
          "original_form": "네네 저는 재료준비만 도와드리고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004923.1.13",
          "form": "갈아서 해도 맛있던데ㅎ 저도 먹고싶네요",
          "original_form": "갈아서 해도 맛있던데ㅎ 저도 먹고싶네요",
          "speaker_id": "2",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004923.1.14",
          "form": "저녁은 감자전으로 끝?",
          "original_form": "저녁은 감자전으로 끝?",
          "speaker_id": "2",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004923.1.15",
          "form": "아마두요ㅎㅎ 밤늦게 배고프면 간식을 먹는걸로ㅎㅎ",
          "original_form": "아마두요ㅎㅎ 밤늦게 배고프면 간식을 먹는걸로ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:28"
        },
        {
          "id": "MDRW2100004923.1.16",
          "form": "초간장도 만드셔야죠ㅎㅎ",
          "original_form": "초간장도 만드셔야죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:28"
        },
        {
          "id": "MDRW2100004923.1.17",
          "form": "ㅋㅋ그러네요 저녁은 뭐드셨어요?",
          "original_form": "ㅋㅋ그러네요 저녁은 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210821 19:30"
        },
        {
          "id": "MDRW2100004923.1.18",
          "form": "저녁은 이따 배고프면 먹을려구요",
          "original_form": "저녁은 이따 배고프면 먹을려구요",
          "speaker_id": "2",
          "time": "20210821 19:31"
        },
        {
          "id": "MDRW2100004923.1.19",
          "form": "제육볶음 꺼내놨는데 녹질않네요",
          "original_form": "제육볶음 꺼내놨는데 녹질않네요",
          "speaker_id": "2",
          "time": "20210821 19:31"
        },
        {
          "id": "MDRW2100004923.1.20",
          "form": "날씨도 선선해져서그런지 음식이 바로 안녹더라구요ㅎㅎ",
          "original_form": "날씨도 선선해져서그런지 음식이 바로 안녹더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004923.1.21",
          "form": "냉장고에 올려놨는데 물에 담가야 되겠어요",
