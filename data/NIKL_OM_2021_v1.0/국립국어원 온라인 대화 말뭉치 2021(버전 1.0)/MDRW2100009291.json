        {
          "id": "MDRW2100009291.1.1",
          "form": "저는 단백질은 다 좋아요.. 고기류",
          "original_form": "저는 단백질은 다 좋아요.. 고기류",
          "speaker_id": "2",
          "time": "20210822 22:36"
        },
        {
          "id": "MDRW2100009291.1.2",
          "form": "직업상 단거는 싫어하시겠네요?",
          "original_form": "직업상 단거는 싫어하시겠네요?",
          "speaker_id": "2",
          "time": "20210822 22:36"
        },
        {
          "id": "MDRW2100009291.1.3",
          "form": "오 직업이랑 단거랑 어떤연관성이 ..?!",
          "original_form": "오 직업이랑 단거랑 어떤연관성이 ..?!",
          "speaker_id": "1",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009291.1.4",
          "form": "아아 ㅋㅋㅋ설마",
          "original_form": "아아 ㅋㅋㅋ설마",
          "speaker_id": "1",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009291.1.5",
          "form": "단거 먹음 충치..",
          "original_form": "단거 먹음 충치..",
          "speaker_id": "2",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009291.1.6",
          "form": "이 썩는거 때문에 ..?",
          "original_form": "이 썩는거 때문에 ..?",
          "speaker_id": "1",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009291.1.7",
          "form": "와우 ..",
          "original_form": "와우 ..",
          "speaker_id": "1",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009291.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ웃기시네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ웃기시네요",
          "speaker_id": "1",
          "time": "20210822 22:37"
        },
        {
          "id": "MDRW2100009291.1.9",
          "form": "단거 안좋아하긴해요!",
          "original_form": "단거 안좋아하긴해요!",
          "speaker_id": "1",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100009291.1.10",
          "form": "ㅋㅋㅋ 그럼 신거 좋아하세요?",
          "original_form": "ㅋㅋㅋ 그럼 신거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100009291.1.11",
          "form": "아뇽 ..",
          "original_form": "아뇽 ..",
          "speaker_id": "1",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100009291.1.12",
          "form": "매콤한거 좋아합니당",
          "original_form": "매콤한거 좋아합니당",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009291.1.13",
          "form": "닭발.. ㅋㅋㅋ",
          "original_form": "닭발.. ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009291.1.14",
          "form": "뭔가 다 이어지네..",
          "original_form": "뭔가 다 이어지네..",
          "speaker_id": "2",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009291.1.15",
          "form": "닭발 쟁여두고 먹어요 ㅠ",
          "original_form": "닭발 쟁여두고 먹어요 ㅠ",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009291.1.16",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009291.1.17",
          "form": "불닭볶음면도 가능하세요?",
          "original_form": "불닭볶음면도 가능하세요?",
          "speaker_id": "2",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009291.1.18",
          "form": "전 징그러운거는 ng요.. 모양이 이뻐야..",
          "original_form": "전 징그러운거는 ng요.. 모양이 이뻐야..",
          "speaker_id": "2",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009291.1.19",
          "form": "당연하죠 치즈올리면 개꿀",
          "original_form": "당연하죠 치즈올리면 개꿀",
          "speaker_id": "1",
          "time": "20210822 22:40"
        },
        {
          "id": "MDRW2100009291.1.20",
          "form": "곱창도 싫나요",
          "original_form": "곱창도 싫나요",
