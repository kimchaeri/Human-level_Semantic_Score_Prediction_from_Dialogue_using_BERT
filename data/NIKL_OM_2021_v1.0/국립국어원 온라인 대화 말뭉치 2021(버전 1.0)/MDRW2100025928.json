      "utterance": [
        {
          "id": "MDRW2100025928.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100025928.1.2",
          "form": "현재 연애 중이신가요?",
          "original_form": "현재 연애 중이신가요?",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100025928.1.3",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100025928.1.4",
          "form": "2년째솔로입니다,,,",
          "original_form": "2년째솔로입니다,,,",
          "speaker_id": "2",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100025928.1.5",
          "form": "아 뭐죠... 전 모쏠인데.....",
          "original_form": "아 뭐죠... 전 모쏠인데.....",
          "speaker_id": "1",
          "time": "20210902 13:50"
        },
        {
          "id": "MDRW2100025928.1.6",
          "form": "앗... 죄송합니다",
          "original_form": "앗... 죄송합니다",
          "speaker_id": "2",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100025928.1.7",
          "form": "실례지만 나이가?",
          "original_form": "실례지만 나이가?",
          "speaker_id": "2",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100025928.1.8",
          "form": "29입니다....",
          "original_form": "29입니다....",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100025928.1.9",
          "form": "마법을 쓸 수 있을 것 같지 않나요?",
          "original_form": "마법을 쓸 수 있을 것 같지 않나요?",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100025928.1.10",
          "form": "언니시네요... 근데 요즘은",
          "original_form": "언니시네요... 근데 요즘은",
          "speaker_id": "2",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100025928.1.11",
          "form": "모쏠의 마법..!",
          "original_form": "모쏠의 마법..!",
          "speaker_id": "1",
          "time": "20210902 13:51"
        },
        {
          "id": "MDRW2100025928.1.12",
          "form": "연애안하는게 추세래요!!!",
          "original_form": "연애안하는게 추세래요!!!",
          "speaker_id": "2",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100025928.1.13",
          "form": "진짜로!!!",
          "original_form": "진짜로!!!",
          "speaker_id": "2",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100025928.1.14",
          "form": "그래요?ㅎㅎㅎㅎ",
          "original_form": "그래요?ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100025928.1.15",
          "form": "전 제 주변은 한명빼곤 다 연애하기는 하는데",
          "original_form": "전 제 주변은 한명빼곤 다 연애하기는 하는데",
          "speaker_id": "1",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100025928.1.16",
          "form": "사실 전 연애할 자신이 없어서",
          "original_form": "사실 전 연애할 자신이 없어서",
          "speaker_id": "1",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100025928.1.17",
          "form": "하게 되더라도 안 할 것 같아요",
          "original_form": "하게 되더라도 안 할 것 같아요",
          "speaker_id": "1",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100025928.1.18",
          "form": "맞아요 안 맞는 사람은",
          "original_form": "맞아요 안 맞는 사람은",
          "speaker_id": "2",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100025928.1.19",
          "form": "되게 안 맞더라구요",
          "original_form": "되게 안 맞더라구요",
          "speaker_id": "2",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100025928.1.20",
          "form": "이상한 사람 만나면 계속 원한품고...",
          "original_form": "이상한 사람 만나면 계속 원한품고...",
          "speaker_id": "2",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100025928.1.21",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "1",
          "time": "20210902 13:52"
        },
        {
          "id": "MDRW2100025928.1.22",
          "form": "저도 23살때 만난사람이 아직까지 저격해요",
          "original_form": "저도 23살때 만난사람이 아직까지 저격해요",
          "speaker_id": "2",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100025928.1.23",
          "form": "요즘 그 문제 심각하죠",
          "original_form": "요즘 그 문제 심각하죠",
          "speaker_id": "1",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100025928.1.24",
          "form": "미치겠어요",
          "original_form": "미치겠어요",
          "speaker_id": "2",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100025928.1.25",
          "form": "헉ㅋㅋㅋㅋㅋ 지금 몇살이신데요?",
          "original_form": "헉ㅋㅋㅋㅋㅋ 지금 몇살이신데요?",
          "speaker_id": "1",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100025928.1.26",
          "form": "ㅋㅋㅋㅋ저 27살이요!!!",
          "original_form": "ㅋㅋㅋㅋ저 27살이요!!!",
          "speaker_id": "2",
          "time": "20210902 13:53"
        },
        {
          "id": "MDRW2100025928.1.27",
          "form": "4년 지났잖아요????",
          "original_form": "4년 지났잖아요????",
          "speaker_id": "1",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100025928.1.28",
          "form": "와 징그럽다",
          "original_form": "와 징그럽다",
          "speaker_id": "1",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100025928.1.29",
          "form": "좋은 사람 뒤끝없는 사람 만나는 것도 참 어려울 것 같아요",
          "original_form": "좋은 사람 뒤끝없는 사람 만나는 것도 참 어려울 것 같아요",
          "speaker_id": "1",
          "time": "20210902 13:55"
        },
        {
          "id": "MDRW2100025928.1.30",
          "form": "ㅋㅋ자기 연애도 하면서 왜 아직 그러는지 모르겠어요 진심 소름ㅠ",
          "original_form": "ㅋㅋ자기 연애도 하면서 왜 아직 그러는지 모르겠어요 진심 소름ㅠ",
          "speaker_id": "2",
          "time": "20210902 13:56"
        },
        {
          "id": "MDRW2100025928.1.31",
          "form": "맞아요 저도 연애를 막 몇십번한건아니지만",
          "original_form": "맞아요 저도 연애를 막 몇십번한건아니지만",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100025928.1.32",
          "form": "연애를 하면서도 그래요?? 와 진짜 소름이다",
          "original_form": "연애를 하면서도 그래요?? 와 진짜 소름이다",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100025928.1.33",
          "form": "애매한사람 만날바엔 안만나는게 좋은거같아요!!",
          "original_form": "애매한사람 만날바엔 안만나는게 좋은거같아요!!",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100025928.1.34",
          "form": "음 모쏠을 위로해주는 좋은 말인데요ㅋㅋㅋㅋㅋ",
          "original_form": "음 모쏠을 위로해주는 좋은 말인데요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100025928.1.35",
          "form": "진짜 솔직히 말하면 심한 말해주고 싶은데 칼맞을까봐 못하겠어요",
          "original_form": "진짜 솔직히 말하면 심한 말해주고 싶은데 칼맞을까봐 못하겠어요",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100025928.1.36",
          "form": "맞아요 요즘 그런거 무섭죠",
          "original_form": "맞아요 요즘 그런거 무섭죠",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100025928.1.37",
          "form": "잘못 걸리면 헤어지기도 무서울 때가 많다고 친구들이 그러더라고요",
          "original_form": "잘못 걸리면 헤어지기도 무서울 때가 많다고 친구들이 그러더라고요",
          "speaker_id": "1",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100025928.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋ진짜루요~!!! 글구 인연 30대에 찾는사람두 많더라구요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ진짜루요~!!! 글구 인연 30대에 찾는사람두 많더라구요",
          "speaker_id": "2",
          "time": "20210902 13:57"
        },
        {
          "id": "MDRW2100025928.1.39",
          "form": "ㅋㅋㅋㅋ저 걔랑 헤어질때 추리하게다녀서 겨우헤어졌어요",
          "original_form": "ㅋㅋㅋㅋ저 걔랑 헤어질때 추리하게다녀서 겨우헤어졌어요",
          "speaker_id": "2",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100025928.1.40",
          "form": "전 연애를 하게되면 굳이 적극적으로 거부하고싶진 않은데",
          "original_form": "전 연애를 하게되면 굳이 적극적으로 거부하고싶진 않은데",
          "speaker_id": "1",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100025928.1.41",
          "form": "모쏠이다보니까",
          "original_form": "모쏠이다보니까",
          "speaker_id": "1",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100025928.1.42",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100025928.1.43",
          "form": "남자보는 눈이 없어서ㅋㅋㅋㅋㅋ",
          "original_form": "남자보는 눈이 없어서ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100025928.1.44",
          "form": "그게 걱정이예요",
          "original_form": "그게 걱정이예요",
          "speaker_id": "1",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100025928.1.45",
          "form": "ㅋㅋㅋㅋㅋㅋ음음 그럼",
          "original_form": "ㅋㅋㅋㅋㅋㅋ음음 그럼",
          "speaker_id": "2",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100025928.1.46",
          "form": "골랐는데 똥차면 어떡해요...",
          "original_form": "골랐는데 똥차면 어떡해요...",
          "speaker_id": "1",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100025928.1.47",
          "form": "소개팅같은 곳 여러곳 가보면서",
          "original_form": "소개팅같은 곳 여러곳 가보면서",
          "speaker_id": "2",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100025928.1.48",
          "form": "안목만 키우시는 것도...?",
          "original_form": "안목만 키우시는 것도...?",
          "speaker_id": "2",
          "time": "20210902 13:58"
        },
        {
          "id": "MDRW2100025928.1.49",
          "form": "아하 경험을 키우는 거군요",
          "original_form": "아하 경험을 키우는 거군요",
          "speaker_id": "1",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100025928.1.50",
          "form": "아예 처음이면 똥차 만나기 쉬우니까ㅠㅠ",
          "original_form": "아예 처음이면 똥차 만나기 쉬우니까ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100025928.1.51",
          "form": "그쵸!!!",
          "original_form": "그쵸!!!",
          "speaker_id": "2",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100025928.1.52",
          "form": "그렇죠ㅠㅠㅠ 그게 제일 걱정이예요",
          "original_form": "그렇죠ㅠㅠㅠ 그게 제일 걱정이예요",
          "speaker_id": "1",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100025928.1.53",
          "form": "일단 자리잡고 나서 잘 탐색하고 만나고 싶은데",
          "original_form": "일단 자리잡고 나서 잘 탐색하고 만나고 싶은데",
          "speaker_id": "1",
          "time": "20210902 13:59"
        },
        {
          "id": "MDRW2100025928.1.54",
          "form": "처음은 뭐든 어렵죠.....",
          "original_form": "처음은 뭐든 어렵죠.....",
          "speaker_id": "1",
