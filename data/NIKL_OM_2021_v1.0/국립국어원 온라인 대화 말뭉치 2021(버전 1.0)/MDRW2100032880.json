      "utterance": [
        {
          "id": "MDRW2100032880.1.1",
          "form": "매번 열심히 마법의 가루의 힘을 빌리고",
          "original_form": "매번 열심히 마법의 가루의 힘을 빌리고",
          "speaker_id": "2",
          "time": "20210906 03:28"
        },
        {
          "id": "MDRW2100032880.1.2",
          "form": "맛도 괜찮고 보기도 괜찮으면 인스타에 사진만 올리는 편이에요!",
          "original_form": "맛도 괜찮고 보기도 괜찮으면 인스타에 사진만 올리는 편이에요!",
          "speaker_id": "2",
          "time": "20210906 03:28"
        },
        {
          "id": "MDRW2100032880.1.3",
          "form": "ㅋㅋㅋㅋ마법의 가루는",
          "original_form": "ㅋㅋㅋㅋ마법의 가루는",
          "speaker_id": "1",
          "time": "20210906 03:28"
        },
        {
          "id": "MDRW2100032880.1.4",
          "form": "미원인가요!?!?",
          "original_form": "미원인가요!?!?",
          "speaker_id": "1",
          "time": "20210906 03:28"
        },
        {
          "id": "MDRW2100032880.1.5",
          "form": "인스타좋죠ㅠㅠ 그런 자기가 요리한걸루",
          "original_form": "인스타좋죠ㅠㅠ 그런 자기가 요리한걸루",
          "speaker_id": "1",
          "time": "20210906 03:28"
        },
        {
          "id": "MDRW2100032880.1.6",
          "form": "피드꾸리는사람 부러워요",
          "original_form": "피드꾸리는사람 부러워요",
          "speaker_id": "1",
          "time": "20210906 03:29"
        },
        {
          "id": "MDRW2100032880.1.7",
          "form": "미원보단 다시다욬ㅋㅋㅋㅋ",
          "original_form": "미원보단 다시다욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:29"
        },
        {
          "id": "MDRW2100032880.1.8",
          "form": "악ㅋㅋㅋ내일다시다사러갑니당",
          "original_form": "악ㅋㅋㅋ내일다시다사러갑니당",
          "speaker_id": "1",
          "time": "20210906 03:30"
        },
        {
          "id": "MDRW2100032880.1.9",
          "form": "아눀ㅋㅋㅋ 저는 처음에 그런 용도가 아니었는데 점점 식사기록용 먹스타그램이 되고 있어욬ㅋㅋㅋㅋ",
          "original_form": "아눀ㅋㅋㅋ 저는 처음에 그런 용도가 아니었는데 점점 식사기록용 먹스타그램이 되고 있어욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:30"
        },
        {
          "id": "MDRW2100032880.1.10",
          "form": "ㅋㅋㅋㅋㅋ아 뭔지 알아요 어쩌다보니 일상계정이 먹스타그램으로",
          "original_form": "ㅋㅋㅋㅋㅋ아 뭔지 알아요 어쩌다보니 일상계정이 먹스타그램으로",
          "speaker_id": "1",
          "time": "20210906 03:30"
        },
        {
          "id": "MDRW2100032880.1.11",
          "form": "국물을 조려야하는건 조리고나서 넣는게 좋아요! 안그러면 간은 맞는데 뭔가 밍밍한 느낌의 맛이나섴ㅋㅋ",
          "original_form": "국물을 조려야하는건 조리고나서 넣는게 좋아요! 안그러면 간은 맞는데 뭔가 밍밍한 느낌의 맛이나섴ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:30"
        },
        {
          "id": "MDRW2100032880.1.12",
          "form": "스토리에 올리는건 사라져서 아쉬워요ㅠ",
          "original_form": "스토리에 올리는건 사라져서 아쉬워요ㅠ",
          "speaker_id": "1",
          "time": "20210906 03:30"
        },
        {
          "id": "MDRW2100032880.1.13",
          "form": "오 이거 꿀팁이네요",
          "original_form": "오 이거 꿀팁이네요",
          "speaker_id": "1",
          "time": "20210906 03:30"
        },
        {
          "id": "MDRW2100032880.1.14",
          "form": "는 오늘 순두부찌개죠....",
          "original_form": "는 오늘 순두부찌개죠....",
          "speaker_id": "2",
          "time": "20210906 03:30"
        },
        {
          "id": "MDRW2100032880.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:31"
        },
        {
          "id": "MDRW2100032880.1.16",
          "form": "제가 이런거 잘 못해서 그런지 스토리 올리는게 더 어렵더라구욬ㅋㅋㅋ",
          "original_form": "제가 이런거 잘 못해서 그런지 스토리 올리는게 더 어렵더라구욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:31"
        },
        {
          "id": "MDRW2100032880.1.17",
          "form": "지워지니까 언제 뭐먹었는지 기억도 안나고... 그래서 그냥 피드에 올리고 있어욬ㅋㅋㅋ",
          "original_form": "지워지니까 언제 뭐먹었는지 기억도 안나고... 그래서 그냥 피드에 올리고 있어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:31"
        },
        {
          "id": "MDRW2100032880.1.18",
          "form": "ㅋㅋㅋㅋ하긴 저도 인스타는 자주 안해서...",
          "original_form": "ㅋㅋㅋㅋ하긴 저도 인스타는 자주 안해서...",
          "speaker_id": "1",
          "time": "20210906 03:32"
        },
        {
          "id": "MDRW2100032880.1.19",
          "form": "피드가 좋기는 해요 자랑도 할겸ㅎ",
          "original_form": "피드가 좋기는 해요 자랑도 할겸ㅎ",
          "speaker_id": "1",
          "time": "20210906 03:32"
        },
        {
          "id": "MDRW2100032880.1.20",
          "form": "저는 장말 기록용처럼써서 해시태그도 안하고 보는것도 친구들 밖에 없어서 자랑이 될지는 모르겠어욬ㅋㅋ",
          "original_form": "저는 장말 기록용처럼써서 해시태그도 안하고 보는것도 친구들 밖에 없어서 자랑이 될지는 모르겠어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 03:33"
        },
        {
          "id": "MDRW2100032880.1.21",
          "form": "ㅋㅋㅋ그래두 먹스타그램이 제일 좋아보이던걸요ㅋㅋ",
          "original_form": "ㅋㅋㅋ그래두 먹스타그램이 제일 좋아보이던걸요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 03:34"
        },
        {
          "id": "MDRW2100032880.1.22",
          "form": "특히 새벽에보면 배고파요",
          "original_form": "특히 새벽에보면 배고파요",
          "speaker_id": "1",
          "time": "20210906 03:34"
        },
        {
          "id": "MDRW2100032880.1.23",
          "form": "아... 배고프다",
          "original_form": "아... 배고프다",
          "speaker_id": "1",
          "time": "20210906 03:34"
        }
      ]
    }
  ]
