      "utterance": [
        {
          "id": "MDRW2100023257.1.1",
          "form": "담임 하면 애들에게 생겨나는 행정업무들이 있고",
          "original_form": "담임 하면 애들에게 생겨나는 행정업무들이 있고",
          "speaker_id": "2",
          "time": "20210829 23:25"
        },
        {
          "id": "MDRW2100023257.1.2",
          "form": "담임만 주어지는 업무 같은게 있어요?",
          "original_form": "담임만 주어지는 업무 같은게 있어요?",
          "speaker_id": "1",
          "time": "20210829 23:25"
        },
        {
          "id": "MDRW2100023257.1.3",
          "form": "행정업무면 출결관리 말고 또 있을까요?",
          "original_form": "행정업무면 출결관리 말고 또 있을까요?",
          "speaker_id": "1",
          "time": "20210829 23:26"
        },
        {
          "id": "MDRW2100023257.1.4",
          "form": "출결관리가 젤 크긴 한데",
          "original_form": "출결관리가 젤 크긴 한데",
          "speaker_id": "2",
          "time": "20210829 23:26"
        },
        {
          "id": "MDRW2100023257.1.5",
          "form": "팝스 입력은 체육 선생님이 하시려나..",
          "original_form": "팝스 입력은 체육 선생님이 하시려나..",
          "speaker_id": "1",
          "time": "20210829 23:26"
        },
        {
          "id": "MDRW2100023257.1.6",
          "form": "뭐 장학금이나 복지나",
          "original_form": "뭐 장학금이나 복지나",
          "speaker_id": "2",
          "time": "20210829 23:26"
        },
        {
          "id": "MDRW2100023257.1.7",
          "form": "이런거 한명씩 챙기는 와중에",
          "original_form": "이런거 한명씩 챙기는 와중에",
          "speaker_id": "2",
          "time": "20210829 23:26"
        },
        {
          "id": "MDRW2100023257.1.8",
          "form": "다른 학교 행정업무를 챙겨야하니깐",
          "original_form": "다른 학교 행정업무를 챙겨야하니깐",
          "speaker_id": "2",
          "time": "20210829 23:26"
        },
        {
          "id": "MDRW2100023257.1.9",
          "form": "버거워하시죠",
          "original_form": "버거워하시죠",
          "speaker_id": "2",
          "time": "20210829 23:27"
        },
        {
          "id": "MDRW2100023257.1.10",
          "form": "근데 초등도 그렇죠?",
          "original_form": "근데 초등도 그렇죠?",
          "speaker_id": "2",
          "time": "20210829 23:27"
        },
        {
          "id": "MDRW2100023257.1.11",
          "form": "초등학교는.. 전 일반학교는 안해봐서 잘모르겠는데",
          "original_form": "초등학교는.. 전 일반학교는 안해봐서 잘모르겠는데",
          "speaker_id": "1",
          "time": "20210829 23:27"
        },
        {
          "id": "MDRW2100023257.1.12",
          "form": "혁신학교의 경우 업무지원팀에서 알아서 해줘요",
          "original_form": "혁신학교의 경우 업무지원팀에서 알아서 해줘요",
          "speaker_id": "1",
          "time": "20210829 23:28"
        },
        {
          "id": "MDRW2100023257.1.13",
          "form": "아 그니까요 저도 일반학교 근무는 그냥 기간제로만 해봐서",
          "original_form": "아 그니까요 저도 일반학교 근무는 그냥 기간제로만 해봐서",
          "speaker_id": "2",
          "time": "20210829 23:28"
        },
        {
          "id": "MDRW2100023257.1.14",
          "form": "근데 저희는 그런거보단",
          "original_form": "근데 저희는 그런거보단",
          "speaker_id": "1",
          "time": "20210829 23:28"
        },
        {
          "id": "MDRW2100023257.1.15",
          "form": "담임을 안해봐서 사실 몰라요",
          "original_form": "담임을 안해봐서 사실 몰라요",
          "speaker_id": "2",
          "time": "20210829 23:28"
        },
        {
          "id": "MDRW2100023257.1.16",
          "form": "학교폭력이나 생활지도 이런게 더 커요^^",
          "original_form": "학교폭력이나 생활지도 이런게 더 커요^^",
          "speaker_id": "1",
          "time": "20210829 23:28"
        },
        {
          "id": "MDRW2100023257.1.17",
          "form": "그런 이유로 담임 기피하시는 분들 좀 계세요",
          "original_form": "그런 이유로 담임 기피하시는 분들 좀 계세요",
          "speaker_id": "1",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100023257.1.18",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "2",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100023257.1.19",
          "form": "학폭 생활지도는 저흰 생활지도부가 또 따로 있으니까",
          "original_form": "학폭 생활지도는 저흰 생활지도부가 또 따로 있으니까",
          "speaker_id": "2",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100023257.1.20",
          "form": "와.. 담임이 해결 안하구여?",
          "original_form": "와.. 담임이 해결 안하구여?",
          "speaker_id": "1",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100023257.1.21",
          "form": "담임은 네..",
          "original_form": "담임은 네..",
          "speaker_id": "2",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100023257.1.22",
          "form": "그거 진짜 좋다..",
          "original_form": "그거 진짜 좋다..",
          "speaker_id": "1",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100023257.1.23",
          "form": "초등도 적용 됐음 좋겠네요 ㅠㅠㅠㅠㅠ",
          "original_form": "초등도 적용 됐음 좋겠네요 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 23:29"
        },
        {
          "id": "MDRW2100023257.1.24",
          "form": "근데 중등에서도 생활지도부 되게 기피부서에요 ㅋㅋㅋ",
          "original_form": "근데 중등에서도 생활지도부 되게 기피부서에요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:30"
        },
        {
          "id": "MDRW2100023257.1.25",
          "form": "저 밤 8시 9시 넘어서까지 학교에 갇혀서 통화한 적도 있네요 ㅠㅠㅠㅠㅠ",
          "original_form": "저 밤 8시 9시 넘어서까지 학교에 갇혀서 통화한 적도 있네요 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 23:30"
        },
        {
          "id": "MDRW2100023257.1.26",
          "form": "헐.. 학폭땜에요?",
          "original_form": "헐.. 학폭땜에요?",
          "speaker_id": "2",
          "time": "20210829 23:30"
        },
        {
          "id": "MDRW2100023257.1.27",
          "form": "네네 ㅜ",
          "original_form": "네네 ㅜ",
          "speaker_id": "1",
          "time": "20210829 23:30"
        },
        {
          "id": "MDRW2100023257.1.28",
          "form": "혹시 그 야자 감독 같은건",
          "original_form": "혹시 그 야자 감독 같은건",
          "speaker_id": "1",
          "time": "20210829 23:30"
        },
        {
          "id": "MDRW2100023257.1.29",
          "form": "업무지정 어떻게 되는거에요?",
          "original_form": "업무지정 어떻게 되는거에요?",
