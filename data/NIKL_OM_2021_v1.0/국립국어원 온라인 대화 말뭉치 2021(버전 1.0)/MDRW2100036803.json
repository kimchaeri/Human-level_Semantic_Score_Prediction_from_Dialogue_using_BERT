      "utterance": [
        {
          "id": "MDRW2100036803.1.1",
          "form": "맘스터치 자주 드시나요?",
          "original_form": "맘스터치 자주 드시나요?",
          "speaker_id": "1",
          "time": "20210831 12:29"
        },
        {
          "id": "MDRW2100036803.1.2",
          "form": "요즘엔 통 안먹어요",
          "original_form": "요즘엔 통 안먹어요",
          "speaker_id": "2",
          "time": "20210831 12:33"
        },
        {
          "id": "MDRW2100036803.1.3",
          "form": "작년에는 자주먹었어요",
          "original_form": "작년에는 자주먹었어요",
          "speaker_id": "2",
          "time": "20210831 12:33"
        },
        {
          "id": "MDRW2100036803.1.4",
          "form": "자주드세요?",
          "original_form": "자주드세요?",
          "speaker_id": "2",
          "time": "20210831 12:33"
        },
        {
          "id": "MDRW2100036803.1.5",
          "form": "아뇨 사람들이 맛있다하길래요",
          "original_form": "아뇨 사람들이 맛있다하길래요",
          "speaker_id": "1",
          "time": "20210831 12:33"
        },
        {
          "id": "MDRW2100036803.1.6",
          "form": "저는 자주안먹어요",
          "original_form": "저는 자주안먹어요",
          "speaker_id": "1",
          "time": "20210831 12:33"
        },
        {
          "id": "MDRW2100036803.1.7",
          "form": "먹어본적 있으세요?",
          "original_form": "먹어본적 있으세요?",
          "speaker_id": "2",
          "time": "20210831 12:35"
        },
        {
          "id": "MDRW2100036803.1.8",
          "form": "네 두세번 먹어봤던 기억이요",
          "original_form": "네 두세번 먹어봤던 기억이요",
          "speaker_id": "1",
          "time": "20210831 12:36"
        },
        {
          "id": "MDRW2100036803.1.9",
          "form": "싸이버거드셨나요?",
          "original_form": "싸이버거드셨나요?",
          "speaker_id": "2",
          "time": "20210831 12:38"
        },
        {
          "id": "MDRW2100036803.1.10",
          "form": "네 그거먹었던거 같아요 다들 그거 맛있다해서요",
          "original_form": "네 그거먹었던거 같아요 다들 그거 맛있다해서요",
          "speaker_id": "1",
          "time": "20210831 12:38"
        },
        {
          "id": "MDRW2100036803.1.11",
          "form": "저도 싸이버거만 먹었어요",
          "original_form": "저도 싸이버거만 먹었어요",
          "speaker_id": "2",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100036803.1.12",
          "form": "요즘은 좀 부실해졌대요",
          "original_form": "요즘은 좀 부실해졌대요",
          "speaker_id": "2",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100036803.1.13",
          "form": "아 그렇군요 순살치킨 8900원 행사한대요",
          "original_form": "아 그렇군요 순살치킨 8900원 행사한대요",
          "speaker_id": "1",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100036803.1.14",
          "form": "우와 가격괜찮네요",
          "original_form": "우와 가격괜찮네요",
          "speaker_id": "2",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100036803.1.15",
          "form": "오늘 백신맞는데 든든하게 그거 사먹을까싶네요",
          "original_form": "오늘 백신맞는데 든든하게 그거 사먹을까싶네요",
          "speaker_id": "2",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100036803.1.16",
          "form": "오 괜찮겠어요",
          "original_form": "오 괜찮겠어요",
          "speaker_id": "1",
          "time": "20210831 12:42"
        },
        {
          "id": "MDRW2100036803.1.17",
          "form": "많이먹어야한대요",
          "original_form": "많이먹어야한대요",
          "speaker_id": "1",
          "time": "20210831 12:42"
        },
        {
          "id": "MDRW2100036803.1.18",
          "form": "좋은정보 감사해요",
          "original_form": "좋은정보 감사해요",
          "speaker_id": "2",
          "time": "20210831 12:44"
        },
        {
          "id": "MDRW2100036803.1.19",
          "form": "피자는 어디꺼 좋아하세요?",
          "original_form": "피자는 어디꺼 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 12:44"
        },
        {
          "id": "MDRW2100036803.1.20",
          "form": "파파존스 맛있던데요",
          "original_form": "파파존스 맛있던데요",
          "speaker_id": "1",
          "time": "20210831 12:45"
        }
      ]
