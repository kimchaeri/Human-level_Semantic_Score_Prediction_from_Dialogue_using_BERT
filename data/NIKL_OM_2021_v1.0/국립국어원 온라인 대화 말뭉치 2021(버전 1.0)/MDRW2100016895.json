      "utterance": [
        {
          "id": "MDRW2100016895.1.1",
          "form": "보시는 유튜브 있으세요?",
          "original_form": "보시는 유튜브 있으세요?",
          "speaker_id": "1",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016895.1.2",
          "form": "저 동물들 나오는 유튜브 즐겨보구요",
          "original_form": "저 동물들 나오는 유튜브 즐겨보구요",
          "speaker_id": "2",
          "time": "20210826 16:08"
        },
        {
          "id": "MDRW2100016895.1.3",
          "form": "홈트할때도 유튜브 참고해요",
          "original_form": "홈트할때도 유튜브 참고해요",
          "speaker_id": "2",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016895.1.4",
          "form": "너무 귀엽죠 ㅜㅜ",
          "original_form": "너무 귀엽죠 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016895.1.5",
          "form": "유튜브 즐겨보세요?",
          "original_form": "유튜브 즐겨보세요?",
          "speaker_id": "2",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016895.1.6",
          "form": "오 누구보세요? 저도 많이 봤어요",
          "original_form": "오 누구보세요? 저도 많이 봤어요",
          "speaker_id": "1",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016895.1.7",
          "form": "맞아요 너무 귀여워요",
          "original_form": "맞아요 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016895.1.8",
          "form": "자주봐요 유튜브",
          "original_form": "자주봐요 유튜브",
          "speaker_id": "1",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016895.1.9",
          "form": "모카우유 보구요",
          "original_form": "모카우유 보구요",
          "speaker_id": "2",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016895.1.10",
          "form": "보더로운생활도 보고",
          "original_form": "보더로운생활도 보고",
          "speaker_id": "2",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016895.1.11",
          "form": "모카우유 이름이 너무 제스타일이네요...",
          "original_form": "모카우유 이름이 너무 제스타일이네요...",
          "speaker_id": "1",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016895.1.12",
          "form": "모카랑 우유가 반려견들이름인데요",
          "original_form": "모카랑 우유가 반려견들이름인데요",
          "speaker_id": "2",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016895.1.13",
          "form": "너무 귀여워요ㅠㅠ",
          "original_form": "너무 귀여워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016895.1.14",
          "form": "어머 짝꿍같이 잘 지었네요",
          "original_form": "어머 짝꿍같이 잘 지었네요",
          "speaker_id": "1",
          "time": "20210826 16:10"
        },
        {
          "id": "MDRW2100016895.1.15",
          "form": "안 키워도 영상으로 볼 수 있으니 넘 조은 것 같아요 ㅋㅋㅋ",
          "original_form": "안 키워도 영상으로 볼 수 있으니 넘 조은 것 같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016895.1.16",
          "form": "맞아요 키우는건 쉽지않으니까 랜선으로나마 보면서 끙끙 앓아요 ㅋㅋㅋㅋ",
          "original_form": "맞아요 키우는건 쉽지않으니까 랜선으로나마 보면서 끙끙 앓아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:12"
        },
        {
          "id": "MDRW2100016895.1.17",
          "form": "전 고양이도 넘 귀엽더라구여 ㅋㅋㅋ",
          "original_form": "전 고양이도 넘 귀엽더라구여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:12"
        }
      ]
