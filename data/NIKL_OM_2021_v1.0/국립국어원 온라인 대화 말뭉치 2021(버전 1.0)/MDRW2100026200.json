      "utterance": [
        {
          "id": "MDRW2100026200.1.1",
          "form": "name2님은 가족이랑 같이 사시나요?",
          "original_form": "&name2&님은 가족이랑 같이 사시나요?",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100026200.1.2",
          "form": "네네 같이살아요!",
          "original_form": "네네 같이살아요!",
          "speaker_id": "2",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100026200.1.3",
          "form": "형제도 있으신가요?",
          "original_form": "형제도 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100026200.1.4",
          "form": "아뇨 저 혼자에요 ㅎ",
          "original_form": "아뇨 저 혼자에요 ㅎ",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026200.1.5",
          "form": "자매있었으면 좋겠어요",
          "original_form": "자매있었으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026200.1.6",
          "form": "아 외동이시구나 전 여동생 있어요 ㅋㅋ",
          "original_form": "아 외동이시구나 전 여동생 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 20:53"
        },
        {
          "id": "MDRW2100026200.1.7",
          "form": "네네 ㅎ혼자는 좋은건 아닌거같아요",
          "original_form": "네네 ㅎ혼자는 좋은건 아닌거같아요",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026200.1.8",
          "form": "부럽다!! 여동생이라니",
          "original_form": "부럽다!! 여동생이라니",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026200.1.9",
          "form": "저도 언니나 여동생있으면 했거든요",
          "original_form": "저도 언니나 여동생있으면 했거든요",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026200.1.10",
          "form": "저는 그 혼자인 느낌을 한번 느껴보고 싶어요 ㅋㅋㅋ",
          "original_form": "저는 그 혼자인 느낌을 한번 느껴보고 싶어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026200.1.11",
          "form": "뭘 사도 다 내꺼인 상태.. 이거 너무 궁금해요 ㅋㅋ",
          "original_form": "뭘 사도 다 내꺼인 상태.. 이거 너무 궁금해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:02"
        },
        {
          "id": "MDRW2100026200.1.12",
          "form": "아ㅋㅋㅋㅋㅋ 그건 그럴수도 있겠네요",
          "original_form": "아ㅋㅋㅋㅋㅋ 그건 그럴수도 있겠네요",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026200.1.13",
          "form": "근데 단점이 너무 많아요 ㅋ집이 잘살지 않는 이상...흑",
          "original_form": "근데 단점이 너무 많아요 ㅋ집이 잘살지 않는 이상...흑",
          "speaker_id": "2",
          "time": "20210901 21:07"
        },
        {
          "id": "MDRW2100026200.1.14",
          "form": "하 그런가요 하긴 근데 나이들수록 의지되는건 있는거 같아요",
          "original_form": "하 그런가요 하긴 근데 나이들수록 의지되는건 있는거 같아요",
          "speaker_id": "1",
          "time": "20210901 21:13"
        },
        {
          "id": "MDRW2100026200.1.15",
          "form": "네 솔직히 어릴땐 몰랐는데 크니깐 그게 너무 부럽더라고요",
          "original_form": "네 솔직히 어릴땐 몰랐는데 크니깐 그게 너무 부럽더라고요",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026200.1.16",
          "form": "저는 오로지 제가 부모님 다 케어해야하니깐 부담이 돼요ㅠ 그리고 혼자 남겨질것도 생각하게되고",
          "original_form": "저는 오로지 제가 부모님 다 케어해야하니깐 부담이 돼요ㅠ 그리고 혼자 남겨질것도 생각하게되고",
          "speaker_id": "2",
          "time": "20210901 21:19"
        },
        {
          "id": "MDRW2100026200.1.17",
          "form": "아 아무래도 그런 부분은 있겠어요 정말 ㅠㅠㅠ",
          "original_form": "아 아무래도 그런 부분은 있겠어요 정말 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100026200.1.18",
          "form": "현실적인 고민이죠..",
          "original_form": "현실적인 고민이죠..",
          "speaker_id": "1",
          "time": "20210901 21:20"
        },
        {
          "id": "MDRW2100026200.1.19",
          "form": "네ㅠ... 요즘들어서 가족 생각 더 하게돼요",
          "original_form": "네ㅠ... 요즘들어서 가족 생각 더 하게돼요",
          "speaker_id": "2",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026200.1.20",
          "form": "엄마는 제가 가정을 이루길 바라는데",
          "original_form": "엄마는 제가 가정을 이루길 바라는데",
          "speaker_id": "2",
          "time": "20210901 21:21"
        },
        {
          "id": "MDRW2100026200.1.21",
          "form": "전..생각이없어서",
          "original_form": "전..생각이없어서",
          "speaker_id": "2",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026200.1.22",
          "form": "아 저도예요 전 여동생이랑 같이 안가고있어서 ㅋㅋㅋ 둘이서 약간 부채를 나눠갖고 있는 거 같아요 ㅋㅋ",
          "original_form": "아 저도예요 전 여동생이랑 같이 안가고있어서 ㅋㅋㅋ 둘이서 약간 부채를 나눠갖고 있는 거 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026200.1.23",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026200.1.24",
          "form": "동생분도 비혼이세요?",
          "original_form": "동생분도 비혼이세요?",
          "speaker_id": "2",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026200.1.25",
          "form": "아 근데 결혼생각은 있는거 같아요 ㅋㅋㅋ 그럼 좀 덜 눈치보일거 같긴 해요 ㅋㅋ 둘다 안가면 너무 튀니까.. ㅋㅋ",
          "original_form": "아 근데 결혼생각은 있는거 같아요 ㅋㅋㅋ 그럼 좀 덜 눈치보일거 같긴 해요 ㅋㅋ 둘다 안가면 너무 튀니까.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026200.1.26",
          "form": "그럼 더 낫네요 ㅎ 아무래도",
          "original_form": "그럼 더 낫네요 ㅎ 아무래도",
