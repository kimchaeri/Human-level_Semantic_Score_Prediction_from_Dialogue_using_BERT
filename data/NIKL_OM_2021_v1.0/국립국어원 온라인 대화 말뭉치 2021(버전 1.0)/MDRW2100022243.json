      "utterance": [
        {
          "id": "MDRW2100022243.1.1",
          "form": "남대문갈치맛도 있어요 ㅋㅋ",
          "original_form": "남대문갈치맛도 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:31"
        },
        {
          "id": "MDRW2100022243.1.2",
          "form": "이름 독특하게 지어놔서 좀 거부감 들긴해요",
          "original_form": "이름 독특하게 지어놔서 좀 거부감 들긴해요",
          "speaker_id": "2",
          "time": "20210829 11:31"
        },
        {
          "id": "MDRW2100022243.1.3",
          "form": "되게 생선맛보다는",
          "original_form": "되게 생선맛보다는",
          "speaker_id": "1",
          "time": "20210829 11:31"
        },
        {
          "id": "MDRW2100022243.1.4",
          "form": "양념맛이 다 하나봐요",
          "original_form": "양념맛이 다 하나봐요",
          "speaker_id": "1",
          "time": "20210829 11:31"
        },
        {
          "id": "MDRW2100022243.1.5",
          "form": "듣고 있으니 저도 되게 궁금해지는 음식이네요",
          "original_form": "듣고 있으니 저도 되게 궁금해지는 음식이네요",
          "speaker_id": "1",
          "time": "20210829 11:32"
        },
        {
          "id": "MDRW2100022243.1.6",
          "form": "어 그런가 싶기도해요",
          "original_form": "어 그런가 싶기도해요",
          "speaker_id": "2",
          "time": "20210829 11:32"
        },
        {
          "id": "MDRW2100022243.1.7",
          "form": "뽈살이 특수부위라던데..",
          "original_form": "뽈살이 특수부위라던데..",
          "speaker_id": "2",
          "time": "20210829 11:32"
        },
        {
          "id": "MDRW2100022243.1.8",
          "form": "특수부위라..",
          "original_form": "특수부위라..",
          "speaker_id": "1",
          "time": "20210829 11:32"
        },
        {
          "id": "MDRW2100022243.1.9",
          "form": "그럼 순대 특수부위도 드시나요",
          "original_form": "그럼 순대 특수부위도 드시나요",
          "speaker_id": "1",
          "time": "20210829 11:33"
        },
        {
          "id": "MDRW2100022243.1.10",
          "form": "어떤거요?",
          "original_form": "어떤거요?",
          "speaker_id": "2",
          "time": "20210829 11:33"
        },
        {
          "id": "MDRW2100022243.1.11",
          "form": "오소리 감투 간 허파 이런거요?",
          "original_form": "오소리 감투 간 허파 이런거요?",
          "speaker_id": "2",
          "time": "20210829 11:33"
        },
        {
          "id": "MDRW2100022243.1.12",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210829 11:33"
        },
        {
          "id": "MDRW2100022243.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 11:33"
        },
        {
          "id": "MDRW2100022243.1.14",
          "form": "드시나요",
          "original_form": "드시나요",
          "speaker_id": "1",
          "time": "20210829 11:33"
        },
        {
          "id": "MDRW2100022243.1.15",
          "form": "저 그런거 별로 안좋아해요 ㅜ",
          "original_form": "저 그런거 별로 안좋아해요 ㅜ",
          "speaker_id": "2",
          "time": "20210829 11:33"
        },
        {
          "id": "MDRW2100022243.1.16",
          "form": "아앗..",
          "original_form": "아앗..",
          "speaker_id": "1",
          "time": "20210829 11:33"
        },
        {
          "id": "MDRW2100022243.1.17",
          "form": "근데 순대국밥에 들어있으면 먹어요",
          "original_form": "근데 순대국밥에 들어있으면 먹어요",
          "speaker_id": "2",
          "time": "20210829 11:33"
        },
        {
          "id": "MDRW2100022243.1.18",
          "form": "그냥은 안먹지만 국밥에 들은건 드시는 타입",
          "original_form": "그냥은 안먹지만 국밥에 들은건 드시는 타입",
          "speaker_id": "1",
          "time": "20210829 11:34"
        },
        {
          "id": "MDRW2100022243.1.19",
          "form": "네 ㅋㅋㅋ 뭔지 모르고 먹는거랑",
          "original_form": "네 ㅋㅋㅋ 뭔지 모르고 먹는거랑",
          "speaker_id": "2",
          "time": "20210829 11:34"
        },
        {
          "id": "MDRW2100022243.1.20",
          "form": "순대 사면서 달라고하는거랑 또 달라요",
          "original_form": "순대 사면서 달라고하는거랑 또 달라요",
          "speaker_id": "2",
          "time": "20210829 11:34"
        },
        {
          "id": "MDRW2100022243.1.21",
          "form": "아 약간 뭔지알아요ㅋㅋㅋ",
          "original_form": "아 약간 뭔지알아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:34"
        },
        {
          "id": "MDRW2100022243.1.22",
          "form": "일부러 찾아먹고싶지는 않지만 ㅠㅠ",
          "original_form": "일부러 찾아먹고싶지는 않지만 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 11:34"
        }
      ]
