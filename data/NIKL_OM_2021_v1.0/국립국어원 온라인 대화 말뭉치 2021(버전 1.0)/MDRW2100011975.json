      "utterance": [
        {
          "id": "MDRW2100011975.1.1",
          "form": "안녕하세요 햄스트링님",
          "original_form": "안녕하세요 햄스트링님",
          "speaker_id": "2",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100011975.1.2",
          "form": "키우는 햄스터가 있나욥~",
          "original_form": "키우는 햄스터가 있나욥~",
          "speaker_id": "2",
          "time": "20210824 19:10"
        },
        {
          "id": "MDRW2100011975.1.3",
          "form": "넹 뚱식이라고 짱 귀여운 햄스터를 키우고 있어요 ㅎㅎ",
          "original_form": "넹 뚱식이라고 짱 귀여운 햄스터를 키우고 있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 19:55"
        },
        {
          "id": "MDRW2100011975.1.4",
          "form": "햄스터 종이 뭔가요~?",
          "original_form": "햄스터 종이 뭔가요~?",
          "speaker_id": "2",
          "time": "20210824 20:02"
        },
        {
          "id": "MDRW2100011975.1.5",
          "form": "정글리안이라고 아세요?",
          "original_form": "정글리안이라고 아세요?",
          "speaker_id": "1",
          "time": "20210824 20:04"
        },
        {
          "id": "MDRW2100011975.1.6",
          "form": "회색쥔데",
          "original_form": "회색쥔데",
          "speaker_id": "1",
          "time": "20210824 20:04"
        },
        {
          "id": "MDRW2100011975.1.7",
          "form": "아유 알죠 저도 정글리안 정말 좋아합답니다ㅎㅎ",
          "original_form": "아유 알죠 저도 정글리안 정말 좋아합답니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:04"
        },
        {
          "id": "MDRW2100011975.1.8",
          "form": "수컷인가요?",
          "original_form": "수컷인가요?",
          "speaker_id": "2",
          "time": "20210824 20:05"
        },
        {
          "id": "MDRW2100011975.1.9",
          "form": "네~수컷은 처음 키워보는데",
          "original_form": "네~수컷은 처음 키워보는데",
          "speaker_id": "1",
          "time": "20210824 20:05"
        },
        {
          "id": "MDRW2100011975.1.10",
          "form": "암컷못지않게 귀엽네유 ㅠㅠ",
          "original_form": "암컷못지않게 귀엽네유 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 20:05"
        },
        {
          "id": "MDRW2100011975.1.11",
          "form": "이름부터 뚱식인데 정말 똥실똥실하고 덩치가 클거같아요!",
          "original_form": "이름부터 뚱식인데 정말 똥실똥실하고 덩치가 클거같아요!",
          "speaker_id": "2",
          "time": "20210824 20:06"
        },
        {
          "id": "MDRW2100011975.1.12",
          "form": "정말 그런가요?",
          "original_form": "정말 그런가요?",
          "speaker_id": "2",
          "time": "20210824 20:07"
        },
        {
          "id": "MDRW2100011975.1.13",
          "form": "ㅋㅋㅋㅋㅋ맞아요 가끔 휙 보면 깜짝놀랍니다..",
          "original_form": "ㅋㅋㅋㅋㅋ맞아요 가끔 휙 보면 깜짝놀랍니다..",
          "speaker_id": "1",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100011975.1.14",
          "form": "애기때에 비해 덩치가..",
          "original_form": "애기때에 비해 덩치가..",
          "speaker_id": "1",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100011975.1.15",
          "form": "ㅋㅋㅋㄱㄱㅋㅋ대단하군요! 저도 한번 보고싶네요ㅠ",
          "original_form": "ㅋㅋㅋㄱㄱㅋㅋ대단하군요! 저도 한번 보고싶네요ㅠ",
          "speaker_id": "2",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100011975.1.16",
          "form": "껄껄 동도리님은 햄스터 키워보신적 있으세요?",
          "original_form": "껄껄 동도리님은 햄스터 키워보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210824 20:08"
        },
        {
          "id": "MDRW2100011975.1.17",
          "form": "저는 동생이 키우기시작해서 같이 키우게됐거든요",
          "original_form": "저는 동생이 키우기시작해서 같이 키우게됐거든요",
          "speaker_id": "2",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100011975.1.18",
          "form": "저는 처음 키워보는거라 실수도 많이했고",
          "original_form": "저는 처음 키워보는거라 실수도 많이했고",
          "speaker_id": "2",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100011975.1.19",
          "form": "그렇게 사랑이 많이 못줬던거 같아서",
          "original_form": "그렇게 사랑이 많이 못줬던거 같아서",
          "speaker_id": "2",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100011975.1.20",
          "form": "많이 아쉬워요",
          "original_form": "많이 아쉬워요",
          "speaker_id": "2",
          "time": "20210824 20:09"
        },
        {
          "id": "MDRW2100011975.1.21",
          "form": "햄스터는 독립적인 동물이니까",
          "original_form": "햄스터는 독립적인 동물이니까",
          "speaker_id": "1",
          "time": "20210824 20:10"
        },
        {
          "id": "MDRW2100011975.1.22",
          "form": "정작 햄찌 본인은 신경안썼을 수도 있어요",
          "original_form": "정작 햄찌 본인은 신경안썼을 수도 있어요",
          "speaker_id": "1",
          "time": "20210824 20:10"
        },
        {
          "id": "MDRW2100011975.1.23",
          "form": "항상 만남엔 아쉬움이 있는 법",
          "original_form": "항상 만남엔 아쉬움이 있는 법",
          "speaker_id": "1",
          "time": "20210824 20:10"
        },
        {
          "id": "MDRW2100011975.1.24",
          "form": "그런가요ㅠ 그렇게 말해주니 고마워요",
          "original_form": "그런가요ㅠ 그렇게 말해주니 고마워요",
          "speaker_id": "2",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100011975.1.25",
          "form": "뚱식이는 건강한가요?",
          "original_form": "뚱식이는 건강한가요?",
          "speaker_id": "2",
          "time": "20210824 20:11"
        },
        {
          "id": "MDRW2100011975.1.26",
          "form": "그럼요 다만 너무 많이 먹어서",
          "original_form": "그럼요 다만 너무 많이 먹어서",
          "speaker_id": "1",
          "time": "20210824 20:14"
        },
        {
          "id": "MDRW2100011975.1.27",
          "form": "비만으로 인한 질병이 오는게 아닌지",
          "original_form": "비만으로 인한 질병이 오는게 아닌지",
          "speaker_id": "1",
          "time": "20210824 20:14"
        },
        {
          "id": "MDRW2100011975.1.28",
          "form": "걱정중입니다..",
          "original_form": "걱정중입니다..",
          "speaker_id": "1",
          "time": "20210824 20:14"
        },
        {
          "id": "MDRW2100011975.1.29",
          "form": "으흠..운동은 스스로 잘 하나요?",
          "original_form": "으흠..운동은 스스로 잘 하나요?",
          "speaker_id": "2",
          "time": "20210824 20:15"
        },
        {
          "id": "MDRW2100011975.1.30",
          "form": "야심한 시각엔 하는 것 같긴 한데..",
          "original_form": "야심한 시각엔 하는 것 같긴 한데..",
          "speaker_id": "1",
          "time": "20210824 20:16"
        },
        {
          "id": "MDRW2100011975.1.31",
          "form": "물증은 없어요",
          "original_form": "물증은 없어요",
          "speaker_id": "1",
          "time": "20210824 20:16"
        },
        {
          "id": "MDRW2100011975.1.32",
          "form": "cctv를 달 수도 없고",
          "original_form": "cctv를 달 수도 없고",
          "speaker_id": "1",
          "time": "20210824 20:16"
        }
      ]
