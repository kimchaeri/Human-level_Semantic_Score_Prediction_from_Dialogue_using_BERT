      "utterance": [
        {
          "id": "MDRW2100007569.1.1",
          "form": "저 다음달에 아르바이트 있어요",
          "original_form": "저 다음달에 아르바이트 있어요",
          "speaker_id": "2",
          "time": "20210822 03:46"
        },
        {
          "id": "MDRW2100007569.1.2",
          "form": "무슨 알바해요?",
          "original_form": "무슨 알바해요?",
          "speaker_id": "1",
          "time": "20210822 03:47"
        },
        {
          "id": "MDRW2100007569.1.3",
          "form": "나 손모델 알바",
          "original_form": "나 손모델 알바",
          "speaker_id": "2",
          "time": "20210822 03:48"
        },
        {
          "id": "MDRW2100007569.1.4",
          "form": "헐# 부러워요. 전 손이 짜리몽땅해서 그런거 못하는데 ㅠ",
          "original_form": "헐# 부러워요. 전 손이 짜리몽땅해서 그런거 못하는데 ㅠ",
          "speaker_id": "1",
          "time": "20210822 03:48"
        },
        {
          "id": "MDRW2100007569.1.5",
          "form": "알바 안 힘들어요?",
          "original_form": "알바 안 힘들어요?",
          "speaker_id": "1",
          "time": "20210822 03:49"
        },
        {
          "id": "MDRW2100007569.1.6",
          "form": "좀 힘든데",
          "original_form": "좀 힘든데",
          "speaker_id": "2",
          "time": "20210822 03:49"
        },
        {
          "id": "MDRW2100007569.1.7",
          "form": "먹고 살려면 해야지 어쩌겠니",
          "original_form": "먹고 살려면 해야지 어쩌겠니",
          "speaker_id": "2",
          "time": "20210822 03:49"
        },
        {
          "id": "MDRW2100007569.1.8",
          "form": "너는 취업 뭐하고싶어?",
          "original_form": "너는 취업 뭐하고싶어?",
          "speaker_id": "2",
          "time": "20210822 03:49"
        },
        {
          "id": "MDRW2100007569.1.9",
          "form": "무슨 인생 다 산 사람처럼 말을 하네요",
          "original_form": "무슨 인생 다 산 사람처럼 말을 하네요",
          "speaker_id": "1",
          "time": "20210822 03:49"
        },
        {
          "id": "MDRW2100007569.1.10",
          "form": "전 흠......미술 쪽으로 하고 싶은데",
          "original_form": "전 흠......미술 쪽으로 하고 싶은데",
          "speaker_id": "1",
          "time": "20210822 03:50"
        },
        {
          "id": "MDRW2100007569.1.11",
          "form": "미술 좋지",
          "original_form": "미술 좋지",
          "speaker_id": "2",
          "time": "20210822 03:50"
        },
        {
          "id": "MDRW2100007569.1.12",
          "form": "밥 벌어먹고 살 수 있을지 모르겠어서..",
          "original_form": "밥 벌어먹고 살 수 있을지 모르겠어서..",
          "speaker_id": "1",
          "time": "20210822 03:50"
        },
        {
          "id": "MDRW2100007569.1.13",
          "form": "고민 중이에요 ㅋㅋ",
          "original_form": "고민 중이에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 03:50"
        },
        {
          "id": "MDRW2100007569.1.14",
          "form": "순수미술 만화창작 이런거아니면",
          "original_form": "순수미술 만화창작 이런거아니면",
          "speaker_id": "2",
          "time": "20210822 03:50"
        },
        {
          "id": "MDRW2100007569.1.15",
          "form": "엥간해선 먹고살거야ㅋㅋ",
          "original_form": "엥간해선 먹고살거야ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 03:50"
        },
        {
          "id": "MDRW2100007569.1.16",
          "form": "아르바이트를한다면 하고싶은 아르바이트 뭐있니",
          "original_form": "아르바이트를한다면 하고싶은 아르바이트 뭐있니",
          "speaker_id": "2",
          "time": "20210822 03:51"
        },
        {
          "id": "MDRW2100007569.1.17",
          "form": "음 편의점이나 카운터 알바는 경험이 있어서 쉬울거 같아서 해보구 싶구",
          "original_form": "음 편의점이나 카운터 알바는 경험이 있어서 쉬울거 같아서 해보구 싶구",
          "speaker_id": "1",
          "time": "20210822 03:52"
        },
        {
          "id": "MDRW2100007569.1.18",
          "form": "동물이랑 관련된 아르바이트 하구 싶어요",
          "original_form": "동물이랑 관련된 아르바이트 하구 싶어요",
          "speaker_id": "1",
          "time": "20210822 03:52"
        },
        {
          "id": "MDRW2100007569.1.19",
          "form": "오 동물이 물거나 하는건 안무섭고?",
          "original_form": "오 동물이 물거나 하는건 안무섭고?",
          "speaker_id": "2",
          "time": "20210822 03:52"
        },
        {
          "id": "MDRW2100007569.1.20",
          "form": "동물이 짖는걸 7시간동안 풀사운드로 들어야한다면 어떨것같아",
          "original_form": "동물이 짖는걸 7시간동안 풀사운드로 들어야한다면 어떨것같아",
          "speaker_id": "2",
          "time": "20210822 03:53"
        },
        {
          "id": "MDRW2100007569.1.21",
          "form": "워낙 동물을 좋아해서 무섭진 않아요",
          "original_form": "워낙 동물을 좋아해서 무섭진 않아요",
          "speaker_id": "1",
          "time": "20210822 03:53"
        },
        {
          "id": "MDRW2100007569.1.22",
          "form": "그럼 뭐 귀가 아프겠죠?",
          "original_form": "그럼 뭐 귀가 아프겠죠?",
          "speaker_id": "1",
          "time": "20210822 03:53"
        },
        {
          "id": "MDRW2100007569.1.23",
          "form": "되게 담담하네",
          "original_form": "되게 담담하네",
          "speaker_id": "2",
          "time": "20210822 03:54"
        }
      ]
    }
  ]
