      "utterance": [
        {
          "id": "MDRW2100039897.1.1",
          "form": "이번주에 서울에 비 많이 왔어?",
          "original_form": "이번주에 서울에 비 많이 왔어?",
          "speaker_id": "1",
          "time": "20210903 22:56"
        },
        {
          "id": "MDRW2100039897.1.2",
          "form": "이번주에는 많이왔어",
          "original_form": "이번주에는 많이왔어",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100039897.1.3",
          "form": "화수는 엄청왔는데",
          "original_form": "화수는 엄청왔는데",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100039897.1.4",
          "form": "오늘은 완전맑음",
          "original_form": "오늘은 완전맑음",
          "speaker_id": "2",
          "time": "20210903 22:57"
        },
        {
          "id": "MDRW2100039897.1.5",
          "form": "그쪽은 비왔어???",
          "original_form": "그쪽은 비왔어???",
          "speaker_id": "2",
          "time": "20210903 22:58"
        },
        {
          "id": "MDRW2100039897.1.6",
          "form": "아 우리도 화수는 비오고",
          "original_form": "아 우리도 화수는 비오고",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100039897.1.7",
          "form": "오늘은 맑더라",
          "original_form": "오늘은 맑더라",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100039897.1.8",
          "form": "오늘 날 덥지않았어??",
          "original_form": "오늘 날 덥지않았어??",
          "speaker_id": "1",
          "time": "20210903 23:01"
        },
        {
          "id": "MDRW2100039897.1.9",
          "form": "낮에는 햇빛이 뜨거웠는데",
          "original_form": "낮에는 햇빛이 뜨거웠는데",
          "speaker_id": "2",
          "time": "20210903 23:02"
        },
        {
          "id": "MDRW2100039897.1.10",
          "form": "생각보다 덥지않았어ㅋ",
          "original_form": "생각보다 덥지않았어ㅋ",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100039897.1.11",
          "form": "습하지않으니깐 너무좋더라",
          "original_form": "습하지않으니깐 너무좋더라",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100039897.1.12",
          "form": "아 ㅁㅈㅁㅈ 바람이 은근 시원하고",
          "original_form": "아 ㅁㅈㅁㅈ 바람이 은근 시원하고",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100039897.1.13",
          "form": "딱 쾌적해 ㅋㅋㅋㅋ",
          "original_form": "딱 쾌적해 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:07"
        },
        {
          "id": "MDRW2100039897.1.14",
          "form": "근데 밤에는 은근 춥더라",
          "original_form": "근데 밤에는 은근 춥더라",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100039897.1.15",
          "form": "바람도 많이 불고",
          "original_form": "바람도 많이 불고",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100039897.1.16",
          "form": "가디건 챙기게 되는 날씨",
          "original_form": "가디건 챙기게 되는 날씨",
          "speaker_id": "2",
          "time": "20210903 23:09"
        },
        {
          "id": "MDRW2100039897.1.17",
          "form": "밤에 창문 열고 자??",
          "original_form": "밤에 창문 열고 자??",
          "speaker_id": "1",
          "time": "20210903 23:10"
        },
        {
          "id": "MDRW2100039897.1.18",
          "form": "살짝 열어놓았는데",
          "original_form": "살짝 열어놓았는데",
          "speaker_id": "2",
          "time": "20210903 23:11"
        },
        {
          "id": "MDRW2100039897.1.19",
          "form": "약간 감기 기운생겨써",
          "original_form": "약간 감기 기운생겨써",
          "speaker_id": "2",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100039897.1.20",
          "form": "너는???",
          "original_form": "너는???",
          "speaker_id": "2",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100039897.1.21",
          "form": "창문닫고자???",
          "original_form": "창문닫고자???",
          "speaker_id": "2",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100039897.1.22",
          "form": "나도 열고자",
          "original_form": "나도 열고자",
          "speaker_id": "1",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100039897.1.23",
          "form": "닫으면 더워서 ㅋㅋㅋ",
          "original_form": "닫으면 더워서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 23:12"
        },
        {
          "id": "MDRW2100039897.1.24",
          "form": "맞아 답답하기도하더라구",
          "original_form": "맞아 답답하기도하더라구",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100039897.1.25",
          "form": "울집은 산이 근처라",
          "original_form": "울집은 산이 근처라",
          "speaker_id": "2",
          "time": "20210903 23:14"
        },
        {
          "id": "MDRW2100039897.1.26",
          "form": "개구리 같은 벌레소리 들려",
          "original_form": "개구리 같은 벌레소리 들려",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039897.1.27",
          "form": "이골이라",
          "original_form": "이골이라",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039897.1.28",
          "form": "시골이라서",
          "original_form": "시골이라서",
          "speaker_id": "2",
          "time": "20210903 23:15"
        },
        {
          "id": "MDRW2100039897.1.29",
          "form": "서울 속 시골이라니",
          "original_form": "서울 속 시골이라니",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100039897.1.30",
          "form": "아주 귀한곳인데요",
          "original_form": "아주 귀한곳인데요",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100039897.1.31",
          "form": "가을이라 귀뚜라미 소리 들리겠다 이제",
          "original_form": "가을이라 귀뚜라미 소리 들리겠다 이제",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100039897.1.32",
          "form": "맞아 귀뚜라미",
          "original_form": "맞아 귀뚜라미",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100039897.1.33",
          "form": "요즘 들리구있어요",
          "original_form": "요즘 들리구있어요",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100039897.1.34",
          "form": "너무시끄러워요",
          "original_form": "너무시끄러워요",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100039897.1.35",
          "form": "아고ㅠㅠㅠ",
          "original_form": "아고ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 23:21"
        },
        {
          "id": "MDRW2100039897.1.36",
          "form": "애들이 빨리 겨울잠을 자야 조용해질텐데요",
          "original_form": "애들이 빨리 겨울잠을 자야 조용해질텐데요",
          "speaker_id": "1",
          "time": "20210903 23:21"
        }
      ]
