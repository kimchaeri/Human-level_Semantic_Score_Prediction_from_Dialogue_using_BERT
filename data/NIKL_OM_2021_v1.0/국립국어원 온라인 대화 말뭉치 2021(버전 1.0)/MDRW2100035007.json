      "utterance": [
        {
          "id": "MDRW2100035007.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210828 23:44"
        },
        {
          "id": "MDRW2100035007.1.2",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100035007.1.3",
          "form": "혹시 홀맨 아세요?",
          "original_form": "혹시 홀맨 아세요?",
          "speaker_id": "1",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100035007.1.4",
          "form": "네 알아요\r\n한 십년도 훨씬 전에 유행했었죠",
          "original_form": "네 알아요\r\n한 십년도 훨씬 전에 유행했었죠",
          "speaker_id": "2",
          "time": "20210828 23:45"
        },
        {
          "id": "MDRW2100035007.1.5",
          "form": "그러면 문구세트 신청하셧엇나요?",
          "original_form": "그러면 문구세트 신청하셧엇나요?",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100035007.1.6",
          "form": "네 폰탈출 그거 말씀하시나봐요",
          "original_form": "네 폰탈출 그거 말씀하시나봐요",
          "speaker_id": "2",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100035007.1.7",
          "form": "네. 하셧나보네여. 그 페이지에 들어가보니까\r\n문구세트를 판매하더라고요.",
          "original_form": "네. 하셧나보네여. 그 페이지에 들어가보니까\r\n문구세트를 판매하더라고요.",
          "speaker_id": "1",
          "time": "20210828 23:47"
        },
        {
          "id": "MDRW2100035007.1.8",
          "form": "아 팝업스토어? 저도 들어가봤어요",
          "original_form": "아 팝업스토어? 저도 들어가봤어요",
          "speaker_id": "2",
          "time": "20210828 23:48"
        },
        {
          "id": "MDRW2100035007.1.9",
          "form": "너무 비싼것같아요",
          "original_form": "너무 비싼것같아요",
          "speaker_id": "1",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100035007.1.10",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100035007.1.11",
          "form": "가격은 안 봤는데 비싸군요",
          "original_form": "가격은 안 봤는데 비싸군요",
          "speaker_id": "2",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100035007.1.12",
          "form": "어느정도였나요?",
          "original_form": "어느정도였나요?",
          "speaker_id": "2",
          "time": "20210828 23:49"
        },
        {
          "id": "MDRW2100035007.1.13",
          "form": "볼첸이 3500원",
          "original_form": "볼첸이 3500원",
          "speaker_id": "1",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100035007.1.14",
          "form": "이고.. 포스트잇도 2천원넘고..",
          "original_form": "이고.. 포스트잇도 2천원넘고..",
          "speaker_id": "1",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100035007.1.15",
          "form": "그렇더라고요.",
          "original_form": "그렇더라고요.",
          "speaker_id": "1",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100035007.1.16",
          "form": "오 생각보다 많이 비싸네요",
          "original_form": "오 생각보다 많이 비싸네요",
          "speaker_id": "2",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100035007.1.17",
          "form": "구매욕구가 확 떨어지는 가격이엇어요",
          "original_form": "구매욕구가 확 떨어지는 가격이엇어요",
          "speaker_id": "1",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100035007.1.18",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100035007.1.19",
          "form": "공짜로 받아서 다행이었더라거여",
          "original_form": "공짜로 받아서 다행이었더라거여",
          "speaker_id": "1",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100035007.1.20",
          "form": "에휴 ㅋㅋ",
          "original_form": "에휴 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:50"
        },
        {
          "id": "MDRW2100035007.1.21",
          "form": "선물로 받았으니 쓰지\r\n구매해서 쓰기에는...",
          "original_form": "선물로 받았으니 쓰지\r\n구매해서 쓰기에는...",
          "speaker_id": "2",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100035007.1.22",
          "form": "마스크스트랩도 너무 비싸고요..",
          "original_form": "마스크스트랩도 너무 비싸고요..",
          "speaker_id": "1",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100035007.1.23",
          "form": "맞아요.",
          "original_form": "맞아요.",
          "speaker_id": "1",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100035007.1.24",
          "form": "혹시 하나도 안쓰셧어요?",
          "original_form": "혹시 하나도 안쓰셧어요?",
          "speaker_id": "1",
          "time": "20210828 23:51"
        },
        {
          "id": "MDRW2100035007.1.25",
          "form": "네 아직이요",
          "original_form": "네 아직이요",
          "speaker_id": "2",
          "time": "20210828 23:51"
        }
      ]
    }
  ]
