      "utterance": [
        {
          "id": "MDRW2100021939.1.1",
          "form": "안녕하세요 혹시 활동하시는 온라인 커뮤니티 있으신가요?",
          "original_form": "안녕하세요 혹시 활동하시는 온라인 커뮤니티 있으신가요?",
          "speaker_id": "1",
          "time": "20210829 00:00"
        },
        {
          "id": "MDRW2100021939.1.2",
          "form": "인스타그램이요~",
          "original_form": "인스타그램이요~",
          "speaker_id": "2",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021939.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021939.1.4",
          "form": "아 저도 인스타그램 친구들이랑 많이 하네요",
          "original_form": "아 저도 인스타그램 친구들이랑 많이 하네요",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021939.1.5",
          "form": "취미활동 모임 같은 건 혹시 없으신가요",
          "original_form": "취미활동 모임 같은 건 혹시 없으신가요",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021939.1.6",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021939.1.7",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210829 00:09"
        },
        {
          "id": "MDRW2100021939.1.8",
          "form": "대학생이라 커뮤니티면",
          "original_form": "대학생이라 커뮤니티면",
          "speaker_id": "2",
          "time": "20210829 00:09"
        },
        {
          "id": "MDRW2100021939.1.9",
          "form": "에타밖에 생각나는게없네요ㅠㅠ",
          "original_form": "에타밖에 생각나는게없네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 00:09"
        },
        {
          "id": "MDRW2100021939.1.10",
          "form": "아 대학생이시구나~",
          "original_form": "아 대학생이시구나~",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021939.1.11",
          "form": "저도 대학다닐 때 에타 많이 했는데요ㅋㅋㅋㅋ",
          "original_form": "저도 대학다닐 때 에타 많이 했는데요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021939.1.12",
          "form": "에타에서 뭐 싸우기도 엄청나게 싸우고 ㅋㅋㅋㅋ",
          "original_form": "에타에서 뭐 싸우기도 엄청나게 싸우고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100021939.1.13",
          "form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021939.1.14",
          "form": "그래서 잘 안들어가긴해요",
          "original_form": "그래서 잘 안들어가긴해요",
          "speaker_id": "2",
          "time": "20210829 00:18"
        },
        {
          "id": "MDRW2100021939.1.15",
          "form": "과끼리도 막 잡을듯이 싸우고",
          "original_form": "과끼리도 막 잡을듯이 싸우고",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021939.1.16",
          "form": "저희 학교는 동아리끼리 그렇게 싸웠어요ㅋㅋㅋㅋ",
          "original_form": "저희 학교는 동아리끼리 그렇게 싸웠어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021939.1.17",
          "form": "지나고 보니 재미있는...",
          "original_form": "지나고 보니 재미있는...",
          "speaker_id": "1",
          "time": "20210829 00:20"
        },
        {
          "id": "MDRW2100021939.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:52"
        },
        {
          "id": "MDRW2100021939.1.19",
          "form": "별걸로 다 싸우네",
          "original_form": "별걸로 다 싸우네",
          "speaker_id": "2",
          "time": "20210829 00:53"
        },
        {
          "id": "MDRW2100021939.1.20",
          "form": "하게되더라구요 ㅋㅋㅋㅋ",
          "original_form": "하게되더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:53"
        },
        {
          "id": "MDRW2100021939.1.21",
          "form": "요새 에타에서는 무슨 얘기 제일 많이 하나요ㅋㅋ 저는 이제 졸업해 버려가지고...",
          "original_form": "요새 에타에서는 무슨 얘기 제일 많이 하나요ㅋㅋ 저는 이제 졸업해 버려가지고...",
          "speaker_id": "1",
          "time": "20210829 19:15"
        },
        {
          "id": "MDRW2100021939.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ 항상같아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 항상같아요",
