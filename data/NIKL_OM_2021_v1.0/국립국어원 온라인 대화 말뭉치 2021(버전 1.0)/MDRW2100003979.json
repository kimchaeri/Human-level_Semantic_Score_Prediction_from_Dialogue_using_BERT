        {
          "id": "MDRW2100003979.1.1",
          "form": "운동이나 식습관 정도?",
          "original_form": "운동이나 식습관 정도?",
          "speaker_id": "2",
          "time": "20210821 14:27"
        },
        {
          "id": "MDRW2100003979.1.2",
          "form": "건강 잘 챙겨야해",
          "original_form": "건강 잘 챙겨야해",
          "speaker_id": "1",
          "time": "20210821 14:27"
        },
        {
