      "utterance": [
        {
          "id": "MDRW2100028548.1.1",
          "form": "나는 우투 정통파 투수들을 너무 좋아하는듯",
          "original_form": "나는 우투 정통파 투수들을 너무 좋아하는듯",
          "speaker_id": "2",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100028548.1.2",
          "form": "어우",
          "original_form": "어우",
          "speaker_id": "1",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100028548.1.3",
          "form": "마치 좌투를 좋아하지 않는 것처럼",
          "original_form": "마치 좌투를 좋아하지 않는 것처럼",
          "speaker_id": "2",
          "time": "20210902 20:43"
        },
        {
          "id": "MDRW2100028548.1.4",
          "form": "엘지 투수 안 좋아할 수 있나?",
          "original_form": "엘지 투수 안 좋아할 수 있나?",
          "speaker_id": "1",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100028548.1.5",
          "form": "없습니다",
          "original_form": "없습니다",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100028548.1.6",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "1",
          "time": "20210902 20:45"
        },
        {
          "id": "MDRW2100028548.1.7",
          "form": "아무래도 그런 편이죠",
          "original_form": "아무래도 그런 편이죠",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028548.1.8",
          "form": "투 수 좋 아",
          "original_form": "투 수 좋 아",
          "speaker_id": "2",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100028548.1.9",
          "form": "정우영 진짜 길죽하다",
          "original_form": "정우영 진짜 길죽하다",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028548.1.10",
          "form": "아니 병살짱",
          "original_form": "아니 병살짱",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100028548.1.11",
          "form": "정우영 넘 잘해",
          "original_form": "정우영 넘 잘해",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028548.1.12",
          "form": "천재 투수야",
          "original_form": "천재 투수야",
          "speaker_id": "2",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028548.1.13",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210902 21:03"
        },
        {
          "id": "MDRW2100028548.1.14",
          "form": "종신엘지해",
          "original_form": "종신엘지해",
          "speaker_id": "1",
          "time": "20210902 21:04"
        },
        {
          "id": "MDRW2100028548.1.15",
          "form": "진심 신인왕 홀드왕 다 따고 종신엘지 해라",
          "original_form": "진심 신인왕 홀드왕 다 따고 종신엘지 해라",
          "speaker_id": "2",
          "time": "20210902 21:05"
        },
        {
          "id": "MDRW2100028548.1.16",
          "form": "이형종 머리 기른 거 아직도 어색해",
          "original_form": "이형종 머리 기른 거 아직도 어색해",
          "speaker_id": "1",
          "time": "20210902 21:06"
        },
        {
          "id": "MDRW2100028548.1.17",
          "form": "난 첨 봤을 때 머리 길었어서",
          "original_form": "난 첨 봤을 때 머리 길었어서",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028548.1.18",
          "form": "긴 게 더 익숙한데",
          "original_form": "긴 게 더 익숙한데",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028548.1.19",
          "form": "그리고 뭣보다",
          "original_form": "그리고 뭣보다",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028548.1.20",
          "form": "머리 길 때 야구를 잘했어.",
          "original_form": "머리 길 때 야구를 잘했어.",
          "speaker_id": "2",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028548.1.21",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028548.1.22",
          "form": "뭐냐 왤케 잘하냐",
          "original_form": "뭐냐 왤케 잘하냐",
          "speaker_id": "1",
          "time": "20210902 21:07"
        },
        {
          "id": "MDRW2100028548.1.23",
          "form": "이형종 첨 봤을 때 완전 충격이었는데...",
          "original_form": "이형종 첨 봤을 때 완전 충격이었는데...",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028548.1.24",
          "form": "긴머리 휘날리며 어쩌고",
          "original_form": "긴머리 휘날리며 어쩌고",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028548.1.25",
          "form": "그때도 수염이 있었나요",
          "original_form": "그때도 수염이 있었나요",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028548.1.26",
          "form": "없었죠",
          "original_form": "없었죠",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028548.1.27",
          "form": "그게 아주 중요했지",
          "original_form": "그게 아주 중요했지",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028548.1.28",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028548.1.29",
          "form": "수염 있었으면 걍 뭐야 저 덥수룩한 아저씨는 이거였지",
          "original_form": "수염 있었으면 걍 뭐야 저 덥수룩한 아저씨는 이거였지",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100028548.1.30",
          "form": "어우 저걸 잡네",
          "original_form": "어우 저걸 잡네",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028548.1.31",
          "form": "머리만 길어서 헐... 간지다 했던 거지....",
          "original_form": "머리만 길어서 헐... 간지다 했던 거지....",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028548.1.32",
          "form": "형종아...",
          "original_form": "형종아...",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028548.1.33",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028548.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028548.1.35",
          "form": "지금은 걍 야구 잘하는 덥수룩한 아저씨",
          "original_form": "지금은 걍 야구 잘하는 덥수룩한 아저씨",
          "speaker_id": "1",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028548.1.36",
          "form": "에휴",
          "original_form": "에휴",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028548.1.37",
          "form": "야구라도 잘하는 게 어디냐",
          "original_form": "야구라도 잘하는 게 어디냐",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100028548.1.38",
          "form": "근데 솔직히 이형종은 서사가 진짜 눈물남",
          "original_form": "근데 솔직히 이형종은 서사가 진짜 눈물남",
          "speaker_id": "2",
          "time": "20210902 21:10"
        },
        {
          "id": "MDRW2100028548.1.39",
          "form": "걍 엘빠들 아픈 손가락",
          "original_form": "걍 엘빠들 아픈 손가락",
          "speaker_id": "2",
          "time": "20210902 21:10"
        }
      ]
    }
