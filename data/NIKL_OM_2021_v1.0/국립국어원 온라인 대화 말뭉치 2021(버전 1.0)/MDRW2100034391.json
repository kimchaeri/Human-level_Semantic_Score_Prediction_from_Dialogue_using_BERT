        },
        {
          "id": "MDRW2100034391.1.2",
          "form": "나는 여행 갈 때 짐을 꼼꼼히 싸",
          "original_form": "나는 여행 갈 때 짐을 꼼꼼히 싸",
          "speaker_id": "1",
          "time": "20210828 02:56"
        },
        {
          "id": "MDRW2100034391.1.3",
          "form": "나도 계획을 세워서 하는 편이야",
          "original_form": "나도 계획을 세워서 하는 편이야",
          "speaker_id": "2",
          "time": "20210828 02:56"
        },
        {
          "id": "MDRW2100034391.1.4",
          "form": "체크 리스트를 작성하는 것을 추천해 줄게",
          "original_form": "체크 리스트를 작성하는 것을 추천해 줄게",
          "speaker_id": "1",
          "time": "20210828 02:56"
        },
        {
          "id": "MDRW2100034391.1.5",
          "form": "마자 그렇게 하면 빠지는 게 없지",
          "original_form": "마자 그렇게 하면 빠지는 게 없지",
          "speaker_id": "2",
          "time": "20210828 02:57"
        },
        {
          "id": "MDRW2100034391.1.6",
          "form": "나는 환전 도 꼭 미리 해",
          "original_form": "나는 환전 도 꼭 미리 해",
          "speaker_id": "1",
          "time": "20210828 02:57"
        },
        {
          "id": "MDRW2100034391.1.7",
          "form": "또 어떤 걸 준비하니",
          "original_form": "또 어떤 걸 준비하니",
          "speaker_id": "2",
          "time": "20210828 02:57"
        },
        {
          "id": "MDRW2100034391.1.8",
          "form": "변학기를 준비해 가",
          "original_form": "변학기를 준비해 가",
          "speaker_id": "1",
          "time": "20210828 02:57"
        },
        {
          "id": "MDRW2100034391.1.9",
          "form": "변압기 맞아 꼭 챙겨야지",
          "original_form": "변압기 맞아 꼭 챙겨야지",
          "speaker_id": "2",
          "time": "20210828 02:57"
        },
        {
          "id": "MDRW2100034391.1.10",
          "form": "그리고 비상식량도 꼭 챙겨",
          "original_form": "그리고 비상식량도 꼭 챙겨",
          "speaker_id": "1",
          "time": "20210828 02:58"
        },
        {
          "id": "MDRW2100034391.1.11",
          "form": "라면 같은 거 말하니",
          "original_form": "라면 같은 거 말하니",
          "speaker_id": "2",
          "time": "20210828 02:58"
        },
        {
          "id": "MDRW2100034391.1.12",
          "form": "맞아 그리고 열량이 높은 초코바",
          "original_form": "맞아 그리고 열량이 높은 초코바",
          "speaker_id": "1",
          "time": "20210828 02:58"
        },
        {
          "id": "MDRW2100034391.1.13",
          "form": "그것도 좋다",
          "original_form": "그것도 좋다",
          "speaker_id": "2",
          "time": "20210828 02:58"
        },
        {
          "id": "MDRW2100034391.1.14",
          "form": "짐을 잘 싸야 여행이 재밌지",
          "original_form": "짐을 잘 싸야 여행이 재밌지",
          "speaker_id": "1",
