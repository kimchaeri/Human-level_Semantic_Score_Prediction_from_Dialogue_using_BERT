      "utterance": [
        {
          "id": "MDRW2100011585.1.1",
          "form": "에듀코카로 해보세요",
          "original_form": "에듀코카로 해보세요",
          "speaker_id": "2",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011585.1.2",
          "form": "여기서 무료강의해줘서요",
          "original_form": "여기서 무료강의해줘서요",
          "speaker_id": "2",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100011585.1.3",
          "form": "험멈머 이런게 있군요",
          "original_form": "험멈머 이런게 있군요",
          "speaker_id": "1",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100011585.1.4",
          "form": "신기하당",
          "original_form": "신기하당",
          "speaker_id": "1",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100011585.1.5",
          "form": "저도 진짜우연히 알았는데",
          "original_form": "저도 진짜우연히 알았는데",
          "speaker_id": "2",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100011585.1.6",
          "form": "초보자가 배우기엔 나름 체계적인거같아요 세금써서 만든건데 봐줘야죠 ㅋㅋ",
          "original_form": "초보자가 배우기엔 나름 체계적인거같아요 세금써서 만든건데 봐줘야죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100011585.1.7",
          "form": "저도 name2님 덕에 우연히 알게되었네용 ㅋㅋ",
          "original_form": "저도 &name2&님 덕에 우연히 알게되었네용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011585.1.8",
          "form": "ㅎㅎ 네네 많이이용하세요",
          "original_form": "ㅎㅎ 네네 많이이용하세요",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011585.1.9",
          "form": "수료증도 주고 가끔 선물이벤트도해요",
          "original_form": "수료증도 주고 가끔 선물이벤트도해요",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011585.1.10",
          "form": "스케치업도 있네",
          "original_form": "스케치업도 있네",
          "speaker_id": "1",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011585.1.11",
          "form": "웹툰 웹소설 드로잉 온갖거 조금씩 건드리더군요",
          "original_form": "웹툰 웹소설 드로잉 온갖거 조금씩 건드리더군요",
          "speaker_id": "2",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011585.1.12",
          "form": "배경도 배우고싶었거든요 넘사벽이라서ㅜㅜ혼자 낑낑하구 있었어용",
          "original_form": "배경도 배우고싶었거든요 넘사벽이라서ㅜㅜ혼자 낑낑하구 있었어용",
          "speaker_id": "1",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011585.1.13",
          "form": "아아 네",
          "original_form": "아아 네",
          "speaker_id": "2",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011585.1.14",
          "form": "근데 배경 파는거 아세요?",
          "original_form": "근데 배경 파는거 아세요?",
          "speaker_id": "2",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011585.1.15",
          "form": "돈주면 다 사겠더라구요",
          "original_form": "돈주면 다 사겠더라구요",
          "speaker_id": "2",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011585.1.16",
          "form": "와 근데 스케치업 프로그램알아야 산거 쓸수있지않나요?",
          "original_form": "와 근데 스케치업 프로그램알아야 산거 쓸수있지않나요?",
          "speaker_id": "1",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011585.1.17",
          "form": "네 어느정도 알아야하는거같아요",
          "original_form": "네 어느정도 알아야하는거같아요",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011585.1.18",
          "form": "아직 저도 잘모릅니다",
          "original_form": "아직 저도 잘모릅니다",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011585.1.19",
          "form": "석정현 작가님 강의두있네요",
          "original_form": "석정현 작가님 강의두있네요",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011585.1.20",
          "form": "유명하신 분인가봐요 무슨 강의 하세요?",
          "original_form": "유명하신 분인가봐요 무슨 강의 하세요?",
          "speaker_id": "2",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011585.1.21",
          "form": "이분은 콜로소?였등가 유료강좌 광고자꾸 뜨던데 여긴 무료네요",
          "original_form": "이분은 콜로소?였등가 유료강좌 광고자꾸 뜨던데 여긴 무료네요",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011585.1.22",
          "form": "오오!",
          "original_form": "오오!",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011585.1.23",
          "form": "주로 유료가 우리에게 노출되니 더그런거같아요",
          "original_form": "주로 유료가 우리에게 노출되니 더그런거같아요",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011585.1.24",
          "form": "인체잘그린다구 소문난 분이신데..흠",
          "original_form": "인체잘그린다구 소문난 분이신데..흠",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011585.1.25",
          "form": "오 강의명이뭐에요?",
          "original_form": "오 강의명이뭐에요?",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011585.1.26",
          "form": "도서관에서 책빌려봤었어요",
          "original_form": "도서관에서 책빌려봤었어요",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011585.1.27",
          "form": "석정현의 그림이야기라는 강좌네요",
          "original_form": "석정현의 그림이야기라는 강좌네요",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011585.1.28",
          "form": "네 저도 웹툰관련 책 읽다가 저 정보를 안거여서요",
          "original_form": "네 저도 웹툰관련 책 읽다가 저 정보를 안거여서요",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011585.1.29",
          "form": "아넵 감사합니다 찾아봐야겠어요",
          "original_form": "아넵 감사합니다 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011585.1.30",
          "form": "아하 뭐니뭐니해도 책이 고급정보네요 ㅎㅎ",
          "original_form": "아하 뭐니뭐니해도 책이 고급정보네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011585.1.31",
          "form": "그렇더라구요",
          "original_form": "그렇더라구요",
          "speaker_id": "2",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100011585.1.32",
          "form": "제가원하는 오프라인이 있는가 살펴봐야겠어요",
          "original_form": "제가원하는 오프라인이 있는가 살펴봐야겠어요",
          "speaker_id": "1",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011585.1.33",
          "form": "네 서울쪽사시면 강의많을거같아요",
