      "utterance": [
        {
          "id": "MDRW2100041989.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100041989.1.2",
          "form": "안녕하세요!!",
          "original_form": "안녕하세요!!",
          "speaker_id": "2",
          "time": "20210906 18:29"
        },
        {
          "id": "MDRW2100041989.1.3",
          "form": "치아바타 좋아하세요?",
          "original_form": "치아바타 좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 22:53"
        },
        {
          "id": "MDRW2100041989.1.4",
          "form": "네넹 빵 정말좋아해요",
          "original_form": "네넹 빵 정말좋아해요",
          "speaker_id": "2",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100041989.1.5",
          "form": "식사용 빵처럼 안 달달한빵을 좋아해요 :)",
          "original_form": "식사용 빵처럼 안 달달한빵을 좋아해요 :)",
          "speaker_id": "2",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100041989.1.6",
          "form": "저도!!!",
          "original_form": "저도!!!",
          "speaker_id": "1",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100041989.1.7",
          "form": "만들어보기도 했어요ㅋㅋ",
          "original_form": "만들어보기도 했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100041989.1.8",
          "form": "오오오오 만들기는 쉽나요??",
          "original_form": "오오오오 만들기는 쉽나요??",
          "speaker_id": "2",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100041989.1.9",
          "form": "저는 제빵까진 엄두가 안나서 해보진 않았는데",
          "original_form": "저는 제빵까진 엄두가 안나서 해보진 않았는데",
          "speaker_id": "2",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100041989.1.10",
          "form": "만들어보고싶어요",
          "original_form": "만들어보고싶어요",
          "speaker_id": "2",
          "time": "20210906 23:09"
        },
        {
          "id": "MDRW2100041989.1.11",
          "form": "그건 무반죽이라 치댈 필요없어요",
          "original_form": "그건 무반죽이라 치댈 필요없어요",
          "speaker_id": "1",
          "time": "20210906 23:09"
        },
        {
          "id": "MDRW2100041989.1.12",
          "form": "ㅅㅣ간만 많이 들어요 발효하느라",
          "original_form": "ㅅㅣ간만 많이 들어요 발효하느라",
          "speaker_id": "1",
          "time": "20210906 23:09"
        },
        {
          "id": "MDRW2100041989.1.13",
          "form": "오 정말요??",
          "original_form": "오 정말요??",
          "speaker_id": "2",
          "time": "20210906 23:09"
        },
        {
          "id": "MDRW2100041989.1.14",
          "form": "발효하기가 만만치 않을거같은데",
          "original_form": "발효하기가 만만치 않을거같은데",
          "speaker_id": "2",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100041989.1.15",
          "form": "역시...",
          "original_form": "역시...",
          "speaker_id": "2",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100041989.1.16",
          "form": "ㅋㅋㅋㅋㅋ빵이 쫄깃하려면",
          "original_form": "ㅋㅋㅋㅋㅋ빵이 쫄깃하려면",
          "speaker_id": "2",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100041989.1.17",
          "form": "발효가필수더라고요",
          "original_form": "발효가필수더라고요",
          "speaker_id": "2",
          "time": "20210906 23:10"
        },
        {
          "id": "MDRW2100041989.1.18",
          "form": "이건 접고 또 접고 계속 반복해요!",
          "original_form": "이건 접고 또 접고 계속 반복해요!",
          "speaker_id": "1",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100041989.1.19",
          "form": "아하 신기하네요",
          "original_form": "아하 신기하네요",
          "speaker_id": "2",
          "time": "20210906 23:11"
        },
        {
          "id": "MDRW2100041989.1.20",
          "form": "뭔가 반죽이 치대야 쫄깃쫄깃할것 같은데...",
          "original_form": "뭔가 반죽이 치대야 쫄깃쫄깃할것 같은데...",
          "speaker_id": "2",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100041989.1.21",
          "form": "대략 4시간정도ㅋㅋ 걸렸던거같아요",
          "original_form": "대략 4시간정도ㅋㅋ 걸렸던거같아요",
          "speaker_id": "1",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100041989.1.22",
          "form": "ㅋㅋㅋㅋㅋ 오래걸리는군요",
          "original_form": "ㅋㅋㅋㅋㅋ 오래걸리는군요",
          "speaker_id": "2",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100041989.1.23",
          "form": "저도한번 만들어볼까봐요...",
          "original_form": "저도한번 만들어볼까봐요...",
          "speaker_id": "2",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100041989.1.24",
          "form": "갓만든 치아바타 맛있을것 같은데",
          "original_form": "갓만든 치아바타 맛있을것 같은데",
          "speaker_id": "2",
          "time": "20210906 23:12"
        },
        {
          "id": "MDRW2100041989.1.25",
          "form": "맛있어요!",
          "original_form": "맛있어요!",
          "speaker_id": "1",
          "time": "20210906 23:15"
        },
        {
          "id": "MDRW2100041989.1.26",
          "form": "오븐이나 에프있으시면 도전하세요!",
          "original_form": "오븐이나 에프있으시면 도전하세요!",
          "speaker_id": "1",
          "time": "20210906 23:15"
        },
        {
          "id": "MDRW2100041989.1.27",
          "form": "그래봐야겠어요",
          "original_form": "그래봐야겠어요",
          "speaker_id": "2",
          "time": "20210906 23:15"
        }
      ]
    }
  ]
