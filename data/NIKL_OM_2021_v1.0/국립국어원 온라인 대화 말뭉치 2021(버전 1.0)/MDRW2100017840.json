      "utterance": [
        {
          "id": "MDRW2100017840.1.1",
          "form": "요즘 영양제 같은 거 먹어?",
          "original_form": "요즘 영양제 같은 거 먹어?",
          "speaker_id": "1",
          "time": "20210826 21:20"
        },
        {
          "id": "MDRW2100017840.1.2",
          "form": "유산균이나 뭐 그런 거",
          "original_form": "유산균이나 뭐 그런 거",
          "speaker_id": "1",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017840.1.3",
          "form": "유산균 루테인 먹어!",
          "original_form": "유산균 루테인 먹어!",
          "speaker_id": "2",
          "time": "20210826 21:21"
        },
        {
          "id": "MDRW2100017840.1.4",
          "form": "루테인은 뭐야?",
          "original_form": "루테인은 뭐야?",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017840.1.5",
          "form": "눈건강에 좋은거!",
          "original_form": "눈건강에 좋은거!",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017840.1.6",
          "form": "오호!",
          "original_form": "오호!",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017840.1.7",
          "form": "아 나도 진짜 눈 나빠진 거 같아...",
          "original_form": "아 나도 진짜 눈 나빠진 거 같아...",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017840.1.8",
          "form": "계속 노트북만 보고 그래서ㅠㅜ",
          "original_form": "계속 노트북만 보고 그래서ㅠㅜ",
          "speaker_id": "1",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017840.1.9",
          "form": "눈 수술 하기위해서",
          "original_form": "눈 수술 하기위해서",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017840.1.10",
          "form": "엄마가 관리하래..ㅡ",
          "original_form": "엄마가 관리하래..ㅡ",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017840.1.11",
          "form": "라식 같은 거?",
          "original_form": "라식 같은 거?",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100017840.1.12",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210826 21:25"
        },
        {
          "id": "MDRW2100017840.1.13",
          "form": "글쿠만",
          "original_form": "글쿠만",
          "speaker_id": "1",
          "time": "20210826 21:26"
        },
        {
          "id": "MDRW2100017840.1.14",
          "form": "나는 종합비타민이랑 유산균 먹고 있어",
          "original_form": "나는 종합비타민이랑 유산균 먹고 있어",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017840.1.15",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017840.1.16",
          "form": "유산균 뭐먹어??",
          "original_form": "유산균 뭐먹어??",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017840.1.17",
          "form": "프로스택인가 거기서 질유산균 사서 먹어",
          "original_form": "프로스택인가 거기서 질유산균 사서 먹어",
          "speaker_id": "1",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100017840.1.18",
          "form": "오오오",
          "original_form": "오오오",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017840.1.19",
          "form": "맛있어??",
          "original_form": "맛있어??",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017840.1.20",
          "form": "피곤하거나 면역력 떨어지면 질염 자주 걸리는데 유산균 먹고 잘 안 걸리는 듯",
          "original_form": "피곤하거나 면역력 떨어지면 질염 자주 걸리는데 유산균 먹고 잘 안 걸리는 듯",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017840.1.21",
          "form": "그냥 너희집에서 먹었던 거랑 비슷해",
          "original_form": "그냥 너희집에서 먹었던 거랑 비슷해",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017840.1.22",
          "form": "오옹",
          "original_form": "오옹",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017840.1.23",
          "form": "맛있겠당",
          "original_form": "맛있겠당",
          "speaker_id": "2",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017840.1.24",
          "form": "웨하스 가루 느낌 + 안 신 레모나 느낌",
          "original_form": "웨하스 가루 느낌 + 안 신 레모나 느낌",
          "speaker_id": "1",
          "time": "20210826 21:28"
        },
        {
          "id": "MDRW2100017840.1.25",
          "form": "약간 새콤한 정도",
          "original_form": "약간 새콤한 정도",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017840.1.26",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017840.1.27",
          "form": "맛있겠다...",
          "original_form": "맛있겠다...",
          "speaker_id": "2",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017840.1.28",
          "form": "먹을만 혀",
          "original_form": "먹을만 혀",
          "speaker_id": "1",
          "time": "20210826 21:29"
        },
        {
          "id": "MDRW2100017840.1.29",
          "form": "약 잘 챙겨먹고 살도 빼서",
          "original_form": "약 잘 챙겨먹고 살도 빼서",
          "speaker_id": "1",
