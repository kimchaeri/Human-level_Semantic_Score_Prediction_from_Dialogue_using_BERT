      "utterance": [
        {
          "id": "MDRW2100026269.1.1",
          "form": "맞아 건강이랑 본업이 우선이야!",
          "original_form": "맞아 건강이랑 본업이 우선이야!",
          "speaker_id": "2",
          "time": "20210901 21:14"
        },
        {
          "id": "MDRW2100026269.1.2",
          "form": "빨리 코로나가 없어지면 좋겠다...",
          "original_form": "빨리 코로나가 없어지면 좋겠다...",
          "speaker_id": "2",
          "time": "20210901 21:15"
        },
        {
          "id": "MDRW2100026269.1.3",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210901 21:16"
        },
        {
          "id": "MDRW2100026269.1.4",
          "form": "코로나없어지면",
          "original_form": "코로나없어지면",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026269.1.5",
          "form": "뭐부터하고싶어?",
          "original_form": "뭐부터하고싶어?",
          "speaker_id": "1",
          "time": "20210901 21:17"
        },
        {
          "id": "MDRW2100026269.1.6",
          "form": "나는 작은언니네 가족이 있는 미국부터 가고싶어ㅠ",
          "original_form": "나는 작은언니네 가족이 있는 미국부터 가고싶어ㅠ",
          "speaker_id": "2",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026269.1.7",
          "form": "진짜 코로나 때문에...",
          "original_form": "진짜 코로나 때문에...",
          "speaker_id": "2",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026269.1.8",
          "form": "너무 못만났어!!!",
          "original_form": "너무 못만났어!!!",
          "speaker_id": "2",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026269.1.9",
          "form": "name1이는???",
          "original_form": "&name1&이는???",
          "speaker_id": "2",
          "time": "20210901 21:18"
        },
        {
          "id": "MDRW2100026269.1.10",
          "form": "나두 친구보러 외국 가구시프..",
          "original_form": "나두 친구보러 외국 가구시프..",
          "speaker_id": "1",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026269.1.11",
          "form": "내년엔 모두가 건강하게 여행 다니면 좋겠다 ㅜ",
          "original_form": "내년엔 모두가 건강하게 여행 다니면 좋겠다 ㅜ",
          "speaker_id": "1",
          "time": "20210901 21:22"
        },
        {
          "id": "MDRW2100026269.1.12",
          "form": "그치 나도야...",
          "original_form": "그치 나도야...",
          "speaker_id": "2",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026269.1.13",
          "form": "name1이네 식구분들은 다 백신 맞으셨어??",
          "original_form": "&name1&이네 식구분들은 다 백신 맞으셨어??",
          "speaker_id": "2",
          "time": "20210901 21:26"
        },
        {
          "id": "MDRW2100026269.1.14",
          "form": "일차는 다 맞았구",
          "original_form": "일차는 다 맞았구",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026269.1.15",
          "form": "이차는 기다리구있오",
          "original_form": "이차는 기다리구있오",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026269.1.16",
          "form": "필라테스에선 뭐가 젤 어려웠어?",
          "original_form": "필라테스에선 뭐가 젤 어려웠어?",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026269.1.17",
          "form": "기구 종류!",
          "original_form": "기구 종류!",
          "speaker_id": "1",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026269.1.18",
          "form": "오오 그러시구나!!! 우리집이랑 비슷하다!!!",
          "original_form": "오오 그러시구나!!! 우리집이랑 비슷하다!!!",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026269.1.19",
          "form": "나 이번에 콤비리포머 하나만 해봤는데",
          "original_form": "나 이번에 콤비리포머 하나만 해봤는데",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026269.1.20",
          "form": "아니 무슨...",
          "original_form": "아니 무슨...",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026269.1.21",
          "form": "아니 사실 다 어려웠어서 죽고싶었어...",
          "original_form": "아니 사실 다 어려웠어서 죽고싶었어...",
          "speaker_id": "2",
          "time": "20210901 22:25"
        },
        {
          "id": "MDRW2100026269.1.22",
          "form": "어렵다는 생각보다는...",
          "original_form": "어렵다는 생각보다는...",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026269.1.23",
          "form": "그냥 너무 아팠어.....",
          "original_form": "그냥 너무 아팠어.....",
          "speaker_id": "2",
          "time": "20210901 22:26"
        },
        {
          "id": "MDRW2100026269.1.24",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026269.1.25",
          "form": "근데 그통증은적응이 잘 안되는 근육통인것 같아..",
          "original_form": "근데 그통증은적응이 잘 안되는 근육통인것 같아..",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026269.1.26",
          "form": "ㅋㅋㅋ뭔가 오묘하게 박살나는 기분",
          "original_form": "ㅋㅋㅋ뭔가 오묘하게 박살나는 기분",
          "speaker_id": "1",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100026269.1.27",
          "form": "내부에서",
          "original_form": "내부에서",
          "speaker_id": "2",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026269.1.28",
          "form": "몸 안에서 일어나는 지진 같아....?",
          "original_form": "몸 안에서 일어나는 지진 같아....?",
          "speaker_id": "2",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100026269.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ귀엽",
          "original_form": "ㅋㅋㅋㅋㅋㅋ귀엽",
          "speaker_id": "1",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026269.1.30",
          "form": "건강히 지내쟈 히히",
          "original_form": "건강히 지내쟈 히히",
          "speaker_id": "1",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026269.1.31",
          "form": "내일은 또 필라테스를 가는 날이야!!!",
          "original_form": "내일은 또 필라테스를 가는 날이야!!!",
