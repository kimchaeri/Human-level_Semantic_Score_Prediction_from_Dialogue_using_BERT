      "utterance": [
        {
          "id": "MDRW2100003647.1.1",
          "form": "최근. 구매하신. 물건있으세요?",
          "original_form": "최근. 구매하신. 물건있으세요?",
          "speaker_id": "1",
          "time": "20210821 10:12"
        },
        {
          "id": "MDRW2100003647.1.2",
          "form": "저방금 먹을거엄청삿어요 ㅋㅋㅋㅋ",
          "original_form": "저방금 먹을거엄청삿어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 10:19"
        },
        {
          "id": "MDRW2100003647.1.3",
          "form": "뭐 사셨어요?ㅎ",
          "original_form": "뭐 사셨어요?ㅎ",
          "speaker_id": "1",
          "time": "20210821 10:19"
        },
        {
          "id": "MDRW2100003647.1.4",
          "form": "성심당에서 빵사고 롯데리아 햄버거사고,,",
          "original_form": "성심당에서 빵사고 롯데리아 햄버거사고,,",
          "speaker_id": "2",
          "time": "20210821 10:19"
        },
        {
          "id": "MDRW2100003647.1.5",
          "form": "크리스피도넛사고,,",
          "original_form": "크리스피도넛사고,,",
          "speaker_id": "2",
          "time": "20210821 10:19"
        },
        {
          "id": "MDRW2100003647.1.6",
          "form": "와~~~~ 맛있겠어요",
          "original_form": "와~~~~ 맛있겠어요",
          "speaker_id": "1",
          "time": "20210821 10:20"
        },
        {
          "id": "MDRW2100003647.1.7",
          "form": "크리스피 오전에가니까 갓나온거라고 하나주도라",
          "original_form": "크리스피 오전에가니까 갓나온거라고 하나주도라",
          "speaker_id": "2",
          "time": "20210822 19:43"
        },
        {
          "id": "MDRW2100003647.1.8",
          "form": "공짜는 언제나 좋네요",
          "original_form": "공짜는 언제나 좋네요",
          "speaker_id": "1",
          "time": "20210822 21:53"
        },
        {
          "id": "MDRW2100003647.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100003647.1.10",
          "form": "저 갤럭시워치4 삿어요 ㅋㅋㅋ",
          "original_form": "저 갤럭시워치4 삿어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100003647.1.11",
          "form": "와~ 워치~~ 편하시죠?ㅋ",
          "original_form": "와~ 워치~~ 편하시죠?ㅋ",
          "speaker_id": "1",
          "time": "20210822 23:00"
        },
        {
          "id": "MDRW2100003647.1.12",
          "form": "인터넷으로사서 배송기다리고잇어여 ㅎ",
          "original_form": "인터넷으로사서 배송기다리고잇어여 ㅎ",
          "speaker_id": "2",
          "time": "20210823 07:56"
        },
        {
          "id": "MDRW2100003647.1.13",
          "form": "기대중",
          "original_form": "기대중",
          "speaker_id": "2",
          "time": "20210823 07:56"
        },
        {
          "id": "MDRW2100003647.1.14",
          "form": "아 얼마주고 사셨어요?",
          "original_form": "아 얼마주고 사셨어요?",
          "speaker_id": "1",
          "time": "20210823 10:46"
        },
        {
          "id": "MDRW2100003647.1.15",
          "form": "26만9천이용 ㅎㅎ",
          "original_form": "26만9천이용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100003647.1.16",
          "form": "직접가서 예약할껄그랫나봐여 ㅠ배송늦어진다고하네요",
          "original_form": "직접가서 예약할껄그랫나봐여 ㅠ배송늦어진다고하네요",
          "speaker_id": "2",
          "time": "20210823 11:00"
        },
        {
          "id": "MDRW2100003647.1.17",
          "form": "에거거ㅠㅠ 기다리는시간이 길겠어여ㅠㅠ",
          "original_form": "에거거ㅠㅠ 기다리는시간이 길겠어여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 11:02"
        },
        {
          "id": "MDRW2100003647.1.18",
          "form": "그니까요 ㅠㅠ목빠지겟어요",
          "original_form": "그니까요 ㅠㅠ목빠지겟어요",
