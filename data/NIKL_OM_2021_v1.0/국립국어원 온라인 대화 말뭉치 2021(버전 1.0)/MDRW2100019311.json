        {
          "id": "MDRW2100019311.1.1",
          "form": "뭐해?",
          "original_form": "뭐해?",
          "speaker_id": "1",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100019311.1.2",
          "form": "나 저녁 먹으려고 할머니집 왔즤",
          "original_form": "나 저녁 먹으려고 할머니집 왔즤",
          "speaker_id": "2",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100019311.1.3",
          "form": "요즘 저녁 몇 시쯤 먹어?",
          "original_form": "요즘 저녁 몇 시쯤 먹어?",
          "speaker_id": "1",
          "time": "20210828 17:00"
        },
        {
          "id": "MDRW2100019311.1.4",
          "form": "음..6시?",
          "original_form": "음..6시?",
          "speaker_id": "2",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100019311.1.5",
          "form": "그렇구만",
          "original_form": "그렇구만",
          "speaker_id": "1",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100019311.1.6",
          "form": "오늘 메뉴는 뭐야?",
          "original_form": "오늘 메뉴는 뭐야?",
          "speaker_id": "1",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100019311.1.7",
          "form": "치킨",
          "original_form": "치킨",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100019311.1.8",
          "form": "족발!",
          "original_form": "족발!",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100019311.1.9",
          "form": "헐랭 맛있겠다",
          "original_form": "헐랭 맛있겠다",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100019311.1.10",
          "form": "막국수 먹고 싶다",
          "original_form": "막국수 먹고 싶다",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100019311.1.11",
          "form": "히히히히",
          "original_form": "히히히히",
          "speaker_id": "2",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100019311.1.12",
          "form": "막국수 좋아해?",
          "original_form": "막국수 좋아해?",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100019311.1.13",
          "form": "엉엉 좋아하지!",
          "original_form": "엉엉 좋아하지!",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100019311.1.14",
          "form": "갹 나도",
          "original_form": "갹 나도",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100019311.1.15",
          "form": "족발보다 막국수가 더 먹고 싶을 때도 있어",
          "original_form": "족발보다 막국수가 더 먹고 싶을 때도 있어",
          "speaker_id": "1",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100019311.1.16",
          "form": "개맛있어",
          "original_form": "개맛있어",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100019311.1.17",
          "form": "ㅇㅈ",
          "original_form": "ㅇㅈ",
          "speaker_id": "2",
          "time": "20210828 19:47"
        },
        {
          "id": "MDRW2100019311.1.18",
          "form": "막국수가 짱이어",
          "original_form": "막국수가 짱이어",
