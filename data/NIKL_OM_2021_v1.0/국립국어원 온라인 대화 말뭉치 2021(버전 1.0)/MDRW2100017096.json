      "utterance": [
        {
          "id": "MDRW2100017096.1.1",
          "form": "활동하시는 동호회가 있나요??",
          "original_form": "활동하시는 동호회가 있나요??",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017096.1.2",
          "form": "독서 동호회도 동호회이지요?ㅎㅎ",
          "original_form": "독서 동호회도 동호회이지요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:01"
        },
        {
          "id": "MDRW2100017096.1.3",
          "form": "독서동호회 대박",
          "original_form": "독서동호회 대박",
          "speaker_id": "1",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017096.1.4",
          "form": "책 좋아하시나봐요",
          "original_form": "책 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100017096.1.5",
          "form": "아뇨ㅋㅋ 그냥 뭐 책은 구실일 뿐ㅋ",
          "original_form": "아뇨ㅋㅋ 그냥 뭐 책은 구실일 뿐ㅋ",
          "speaker_id": "2",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017096.1.6",
          "form": "직장내 동호회에요ㅋㅋ",
          "original_form": "직장내 동호회에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:06"
        },
        {
          "id": "MDRW2100017096.1.7",
          "form": "얼마나 자주 모이나요?",
          "original_form": "얼마나 자주 모이나요?",
          "speaker_id": "1",
          "time": "20210826 19:08"
        },
        {
          "id": "MDRW2100017096.1.8",
          "form": "한달에 한 번 정도인 것 같아요ㅎ",
          "original_form": "한달에 한 번 정도인 것 같아요ㅎ",
          "speaker_id": "2",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017096.1.9",
          "form": "동호회 하세요?",
          "original_form": "동호회 하세요?",
          "speaker_id": "2",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017096.1.10",
          "form": "저는 배드민턴 동호회 가입햇는데",
          "original_form": "저는 배드민턴 동호회 가입햇는데",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017096.1.11",
          "form": "코로나때매 못가고잇어요ㅠ",
          "original_form": "코로나때매 못가고잇어요ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017096.1.12",
          "form": "와배드민턴",
          "original_form": "와배드민턴",
          "speaker_id": "2",
          "time": "20210826 20:12"
        },
        {
          "id": "MDRW2100017096.1.13",
          "form": "알면 알수록 어려워요",
          "original_form": "알면 알수록 어려워요",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017096.1.14",
          "form": "하지만 못가고 있어서 너무 슬퍼요ㅠ",
          "original_form": "하지만 못가고 있어서 너무 슬퍼요ㅠ",
          "speaker_id": "1",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100017096.1.15",
          "form": "책은 그럼 한달에 한권 읽으시는건가요?",
          "original_form": "책은 그럼 한달에 한권 읽으시는건가요?",
          "speaker_id": "1",
          "time": "20210826 20:14"
        },
        {
          "id": "MDRW2100017096.1.16",
          "form": "네 목표는 그렇습니다ㅋㅋ",
          "original_form": "네 목표는 그렇습니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 10:13"
        },
        {
          "id": "MDRW2100017096.1.17",
          "form": "필사도 같이 해요",
          "original_form": "필사도 같이 해요",
          "speaker_id": "2",
          "time": "20210827 10:13"
        },
        {
          "id": "MDRW2100017096.1.18",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210827 10:30"
        },
        {
          "id": "MDRW2100017096.1.19",
          "form": "필사할 책을 정하는 기준이 있나요?",
          "original_form": "필사할 책을 정하는 기준이 있나요?",
          "speaker_id": "1",
          "time": "20210827 10:30"
        },
        {
          "id": "MDRW2100017096.1.20",
          "form": "네 같이 대화해서 선정한답니다ㅎ",
          "original_form": "네 같이 대화해서 선정한답니다ㅎ",
          "speaker_id": "2",
          "time": "20210827 11:40"
        },
        {
          "id": "MDRW2100017096.1.21",
          "form": "필사 재밌을 것 같아요!",
          "original_form": "필사 재밌을 것 같아요!",
          "speaker_id": "1",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100017096.1.22",
          "form": "이번에 선정된 책은 뭔가요?",
          "original_form": "이번에 선정된 책은 뭔가요?",
          "speaker_id": "1",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100017096.1.23",
          "form": "미드나잇 라이브러리 라는 책이에요ㅎ",
          "original_form": "미드나잇 라이브러리 라는 책이에요ㅎ",
          "speaker_id": "2",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100017096.1.24",
          "form": "한 번 읽어보세요",
          "original_form": "한 번 읽어보세요",
          "speaker_id": "2",
          "time": "20210827 11:49"
        }
      ]
