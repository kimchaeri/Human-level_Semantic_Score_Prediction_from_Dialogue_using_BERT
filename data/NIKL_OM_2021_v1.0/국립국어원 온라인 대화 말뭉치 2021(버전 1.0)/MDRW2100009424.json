      "utterance": [
        {
          "id": "MDRW2100009424.1.1",
          "form": "비오네",
          "original_form": "비오네",
          "speaker_id": "1",
          "time": "20210823 08:30"
        },
        {
          "id": "MDRW2100009424.1.2",
          "form": "태풍온다디 쪼메오노",
          "original_form": "태풍온다디 쪼메오노",
          "speaker_id": "1",
          "time": "20210823 08:30"
        },
        {
          "id": "MDRW2100009424.1.3",
          "form": "언니 오늘밤에 지나간다던데",
          "original_form": "언니 오늘밤에 지나간다던데",
          "speaker_id": "2",
          "time": "20210823 08:37"
        },
        {
          "id": "MDRW2100009424.1.4",
          "form": "출근준비중이가?ㅋ",
          "original_form": "출근준비중이가?ㅋ",
          "speaker_id": "2",
          "time": "20210823 08:49"
        },
        {
          "id": "MDRW2100009424.1.5",
          "form": "아 오늘 밤이가 ㅋㅋㅋㅋ",
          "original_form": "아 오늘 밤이가 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 08:54"
        },
        {
          "id": "MDRW2100009424.1.6",
          "form": "출근중 ㅎㅎ",
          "original_form": "출근중 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 08:54"
        },
        {
          "id": "MDRW2100009424.1.7",
          "form": "또 신호 걸려서 ㄷㄷ 하고 있다",
          "original_form": "또 신호 걸려서 ㄷㄷ 하고 있다",
          "speaker_id": "1",
          "time": "20210823 08:54"
        },
        {
          "id": "MDRW2100009424.1.8",
          "form": "날씨 우중충하네",
          "original_form": "날씨 우중충하네",
          "speaker_id": "1",
          "time": "20210823 08:54"
        },
        {
          "id": "MDRW2100009424.1.9",
          "form": "웁스ㅜㅜ언니 어여 조심해서 가라",
          "original_form": "웁스ㅜㅜ언니 어여 조심해서 가라",
          "speaker_id": "2",
          "time": "20210823 08:55"
        },
        {
          "id": "MDRW2100009424.1.10",
          "form": "날씨 여기는 비와서ㅋㅋㅋ아까 첫째 같이 데려다주고",
          "original_form": "날씨 여기는 비와서ㅋㅋㅋ아까 첫째 같이 데려다주고",
          "speaker_id": "2",
          "time": "20210823 08:55"
        },
        {
          "id": "MDRW2100009424.1.11",
          "form": "이제 둘째보내고 나도 일가야된다 ㅋㅋ",
          "original_form": "이제 둘째보내고 나도 일가야된다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 08:55"
        },
        {
          "id": "MDRW2100009424.1.12",
          "form": "날씨는 우중충하지만 홧팅하고ㅋ",
          "original_form": "날씨는 우중충하지만 홧팅하고ㅋ",
          "speaker_id": "2",
          "time": "20210823 08:56"
        },
        {
          "id": "MDRW2100009424.1.13",
          "form": "ㅋㅋㅋ 오늘 일가는 날이구먼",
          "original_form": "ㅋㅋㅋ 오늘 일가는 날이구먼",
          "speaker_id": "1",
          "time": "20210823 09:56"
        },
        {
          "id": "MDRW2100009424.1.14",
          "form": "빗길 안전운전 하시게",
          "original_form": "빗길 안전운전 하시게",
          "speaker_id": "1",
          "time": "20210823 09:56"
        },
        {
          "id": "MDRW2100009424.1.15",
          "form": "응 알바다녀오고 밥먹었다ㅋ",
          "original_form": "응 알바다녀오고 밥먹었다ㅋ",
          "speaker_id": "2",
          "time": "20210823 12:50"
        },
        {
          "id": "MDRW2100009424.1.16",
          "form": "잘했구만요",
          "original_form": "잘했구만요",
          "speaker_id": "1",
          "time": "20210823 13:31"
        },
        {
          "id": "MDRW2100009424.1.17",
          "form": "원투 다 영어학원갔죠??",
          "original_form": "원투 다 영어학원갔죠??",
          "speaker_id": "1",
          "time": "20210823 13:31"
        },
        {
          "id": "MDRW2100009424.1.18",
          "form": "우리집 원도 우산쓰고 가고 있다하네요",
          "original_form": "우리집 원도 우산쓰고 가고 있다하네요",
          "speaker_id": "1",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100009424.1.19",
          "form": "아 언닌줄 ㅋㅋㅋㅋㄱ 정신없다 ㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "아 언닌줄 ㅋㅋㅋㅋㄱ 정신없다 ㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210823 13:32"
        },
        {
          "id": "MDRW2100009424.1.20",
          "form": "ㅋㅋㅋㅋㅋ아 웃겨",
          "original_form": "ㅋㅋㅋㅋㅋ아 웃겨",
          "speaker_id": "2",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100009424.1.21",
          "form": "언니ㅋㅋㅋㅋㅋ",
          "original_form": "언니ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100009424.1.22",
          "form": "바쁜가보네ㅋㅋ",
          "original_form": "바쁜가보네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100009424.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 어후 정신없다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 어후 정신없다",
          "speaker_id": "1",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100009424.1.24",
          "form": "비오니 축쳐지네 ㅋㅋㅋㄱㅋㅋㅋ",
          "original_form": "비오니 축쳐지네 ㅋㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100009424.1.25",
          "form": "ㅋㅋㅋ 언니 퇴근할때는 좀 덜 와야될건데ㅋ",
          "original_form": "ㅋㅋㅋ 언니 퇴근할때는 좀 덜 와야될건데ㅋ",
          "speaker_id": "2",
          "time": "20210823 13:33"
        },
        {
          "id": "MDRW2100009424.1.26",
          "form": "웃겨 나두 그렇다.ㅋㅋ",
          "original_form": "웃겨 나두 그렇다.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 13:34"
        },
        {
          "id": "MDRW2100009424.1.27",
          "form": "ㅋㅋㅋㄹㄹㄱ 정신차릴라고 커피마시고 있음",
          "original_form": "ㅋㅋㅋㄹㄹㄱ 정신차릴라고 커피마시고 있음",
          "speaker_id": "1",
          "time": "20210823 13:53"
        },
        {
          "id": "MDRW2100009424.1.28",
          "form": "근데 계속 잠오네 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 계속 잠오네 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 13:54"
        },
        {
          "id": "MDRW2100009424.1.29",
          "form": "어디갔니??",
          "original_form": "어디갔니??",
          "speaker_id": "1",
          "time": "20210823 15:37"
        },
        {
          "id": "MDRW2100009424.1.30",
          "form": "응 언니 비가와서 그러나 ㅋㅋ 피곤하고 쳐지네 ㅋㅋ",
          "original_form": "응 언니 비가와서 그러나 ㅋㅋ 피곤하고 쳐지네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:55"
        },
        {
          "id": "MDRW2100009424.1.31",
          "form": "오늘 저녁에 태풍온다고하니 조심하자",
          "original_form": "오늘 저녁에 태풍온다고하니 조심하자",
          "speaker_id": "2",
          "time": "20210823 15:57"
        },
        {
          "id": "MDRW2100009424.1.32",
          "form": "엉 아까 비 좀 내리더라",
          "original_form": "엉 아까 비 좀 내리더라",
          "speaker_id": "1",
          "time": "20210823 18:09"
        },
        {
          "id": "MDRW2100009424.1.33",
          "form": "주말에 비 안온다고 해서",
          "original_form": "주말에 비 안온다고 해서",
          "speaker_id": "1",
          "time": "20210823 18:09"
        },
        {
          "id": "MDRW2100009424.1.34",
          "form": "다시 캠핑장 예약함 ㅎㅎ",
          "original_form": "다시 캠핑장 예약함 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 18:09"
        },
        {
          "id": "MDRW2100009424.1.35",
          "form": "ㅋㅋㅋ좋다좋다 ㅋㅋ",
          "original_form": "ㅋㅋㅋ좋다좋다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 18:46"
        },
        {
          "id": "MDRW2100009424.1.36",
          "form": "캠핑장 잘 간다잉ㅋ",
          "original_form": "캠핑장 잘 간다잉ㅋ",
