      "utterance": [
        {
          "id": "MDRW2100002817.1.1",
          "form": "지금 달달구리 커피 한잔 태워서 잠을 이기는 중입니다ㅋ",
          "original_form": "지금 달달구리 커피 한잔 태워서 잠을 이기는 중입니다ㅋ",
          "speaker_id": "1",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002817.1.2",
          "form": "믹스 커피요?",
          "original_form": "믹스 커피요?",
          "speaker_id": "2",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002817.1.3",
          "form": "네~ 이디야 바닐라라떼 ㅎ 봉지에든거요",
          "original_form": "네~ 이디야 바닐라라떼 ㅎ 봉지에든거요",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002817.1.4",
          "form": "아~달달한 향이 전달되네요 ㅎㅎ",
          "original_form": "아~달달한 향이 전달되네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002817.1.5",
          "form": "배는부르지만ㅎ. 커피를 포기할순없어요~",
          "original_form": "배는부르지만ㅎ. 커피를 포기할순없어요~",
          "speaker_id": "1",
          "time": "20210820 15:49"
        },
        {
          "id": "MDRW2100002817.1.6",
          "form": "ㅎㅎ 달달한 커피는 피곤을 잊게 해줘요",
          "original_form": "ㅎㅎ 달달한 커피는 피곤을 잊게 해줘요",
          "speaker_id": "2",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002817.1.7",
          "form": "카페는 어딜 자주 가셔요?",
          "original_form": "카페는 어딜 자주 가셔요?",
          "speaker_id": "1",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002817.1.8",
          "form": "저는 투썸이랑 스벅이요 둘다 가까워서요",
          "original_form": "저는 투썸이랑 스벅이요 둘다 가까워서요",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002817.1.9",
          "form": "전 스벅 자주가는데 근거리에는 없어서ㅠㅠ",
          "original_form": "전 스벅 자주가는데 근거리에는 없어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002817.1.10",
          "form": "아~~아쉽네요~",
          "original_form": "아~~아쉽네요~",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002817.1.11",
          "form": "애가 있으니 커피 한잔 하려고 차 타고 나가기도 그렇고",
          "original_form": "애가 있으니 커피 한잔 하려고 차 타고 나가기도 그렇고",
          "speaker_id": "1",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002817.1.12",
          "form": "이번에 새로나온 루프탑그레이라떼 드셔보셨나요?",
          "original_form": "이번에 새로나온 루프탑그레이라떼 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002817.1.13",
          "form": "그렇죠~저는 믹스커피에 우유타먹는것도 좋아해요",
          "original_form": "그렇죠~저는 믹스커피에 우유타먹는것도 좋아해요",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002817.1.14",
          "form": "아니요~어때요?",
          "original_form": "아니요~어때요?",
          "speaker_id": "2",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002817.1.15",
          "form": "너무 맛있더라구요~",
          "original_form": "너무 맛있더라구요~",
          "speaker_id": "1",
          "time": "20210820 15:56"
        }
      ]
