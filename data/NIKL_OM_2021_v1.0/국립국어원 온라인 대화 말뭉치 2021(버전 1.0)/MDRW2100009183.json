      "utterance": [
        {
          "id": "MDRW2100009183.1.1",
          "form": "온라인 커뮤 하시나요?",
          "original_form": "온라인 커뮤 하시나요?",
          "speaker_id": "1",
          "time": "20210822 22:02"
        },
        {
          "id": "MDRW2100009183.1.2",
          "form": "카페해요",
          "original_form": "카페해요",
          "speaker_id": "2",
          "time": "20210822 22:02"
        },
        {
          "id": "MDRW2100009183.1.3",
          "form": "맘카페요?",
          "original_form": "맘카페요?",
          "speaker_id": "1",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009183.1.4",
          "form": "아녀 지역카페용",
          "original_form": "아녀 지역카페용",
          "speaker_id": "2",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009183.1.5",
          "form": "아.. 어머님들 카페가 활성화되있는거 같네요..",
          "original_form": "아.. 어머님들 카페가 활성화되있는거 같네요..",
          "speaker_id": "1",
          "time": "20210822 22:05"
        },
        {
          "id": "MDRW2100009183.1.6",
          "form": "파파까페도 많아졌음..ㅋㅋ",
          "original_form": "파파까페도 많아졌음..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100009183.1.7",
          "form": "ㅎㅎ 그러게요",
          "original_form": "ㅎㅎ 그러게요",
          "speaker_id": "2",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100009183.1.8",
          "form": "오프라인 모임도 하고 그러세요?",
          "original_form": "오프라인 모임도 하고 그러세요?",
          "speaker_id": "1",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100009183.1.9",
          "form": "아니요 그냥 글만",
          "original_form": "아니요 그냥 글만",
          "speaker_id": "2",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100009183.1.10",
          "form": "님은 어떠세요,",
          "original_form": "님은 어떠세요,",
          "speaker_id": "2",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100009183.1.11",
          "form": "저는 대형커뮤만 해요..",
          "original_form": "저는 대형커뮤만 해요..",
          "speaker_id": "1",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009183.1.12",
          "form": "클리앙 웃대 이런거요",
          "original_form": "클리앙 웃대 이런거요",
          "speaker_id": "1",
          "time": "20210822 22:08"
        },
        {
          "id": "MDRW2100009183.1.13",
          "form": "앗 첨 들어본",
          "original_form": "앗 첨 들어본",
          "speaker_id": "2",
          "time": "20210822 22:09"
        },
        {
          "id": "MDRW2100009183.1.14",
          "form": "어? 정말요?",
          "original_form": "어? 정말요?",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009183.1.15",
          "form": "커뮤니티 탑5인디..",
          "original_form": "커뮤니티 탑5인디..",
          "speaker_id": "1",
          "time": "20210822 22:10"
        },
        {
          "id": "MDRW2100009183.1.16",
          "form": "앗 그런가요ㅠ",
          "original_form": "앗 그런가요ㅠ",
          "speaker_id": "2",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100009183.1.17",
          "form": "지역카페는 어떻게 알고 들어가나요? 검색인가요?",
          "original_form": "지역카페는 어떻게 알고 들어가나요? 검색인가요?",
          "speaker_id": "1",
          "time": "20210822 22:11"
        },
        {
          "id": "MDRW2100009183.1.18",
          "form": "네 검색해서 들어가요",
          "original_form": "네 검색해서 들어가요",
