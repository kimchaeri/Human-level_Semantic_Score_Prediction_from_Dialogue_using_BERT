      "utterance": [
        {
          "id": "MDRW2100039746.1.1",
          "form": "백신맞음 둘째날만 팔 아프고 삼일째부턴 멀쩡했어요!",
          "original_form": "백신맞음 둘째날만 팔 아프고 삼일째부턴 멀쩡했어요!",
          "speaker_id": "2",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100039746.1.2",
          "form": "ㅠㅠ 이런 분들만 계심",
          "original_form": "ㅠㅠ 이런 분들만 계심",
          "speaker_id": "1",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100039746.1.3",
          "form": "접종하겠는데",
          "original_form": "접종하겠는데",
          "speaker_id": "1",
          "time": "20210903 21:08"
        },
        {
          "id": "MDRW2100039746.1.4",
          "form": "주변에 건강하시고 지병도\r\n없던 분이 돌아가시기도 했고",
          "original_form": "주변에 건강하시고 지병도\r\n없던 분이 돌아가시기도 했고",
          "speaker_id": "1",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100039746.1.5",
          "form": "심장이 쪼이는듯한 통증\r\n말하는 분들도 많더라구여ㅠ",
          "original_form": "심장이 쪼이는듯한 통증\r\n말하는 분들도 많더라구여ㅠ",
          "speaker_id": "1",
          "time": "20210903 21:09"
        },
        {
          "id": "MDRW2100039746.1.6",
          "form": "아 정말요? 제 주위엔 그런 분 한명도 없어서",
          "original_form": "아 정말요? 제 주위엔 그런 분 한명도 없어서",
          "speaker_id": "2",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100039746.1.7",
          "form": "다행이죠ㅎㅎ",
          "original_form": "다행이죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100039746.1.8",
          "form": "회사도 친구도 식구들도 모두 멀쩡합니다",
          "original_form": "회사도 친구도 식구들도 모두 멀쩡합니다",
          "speaker_id": "2",
          "time": "20210903 21:10"
        },
        {
          "id": "MDRW2100039746.1.9",
          "form": "걱정마세요",
          "original_form": "걱정마세요",
          "speaker_id": "2",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039746.1.10",
          "form": "정말 다행이네요ㅠㅠ",
          "original_form": "정말 다행이네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039746.1.11",
          "form": "접종 1차 하신거에여?",
          "original_form": "접종 1차 하신거에여?",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100039746.1.12",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100039746.1.13",
          "form": "운동은 좋아하세요?",
          "original_form": "운동은 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 21:47"
        },
        {
          "id": "MDRW2100039746.1.14",
          "form": "운동은 홈트 종종 하구 있어영 ㅋ",
          "original_form": "운동은 홈트 종종 하구 있어영 ㅋ",
          "speaker_id": "1",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100039746.1.15",
          "form": "주로 어느 분꺼 보세요?",
          "original_form": "주로 어느 분꺼 보세요?",
          "speaker_id": "2",
          "time": "20210905 21:55"
        },
        {
          "id": "MDRW2100039746.1.16",
          "form": "저는 땅끄부부꺼 봅니당",
          "original_form": "저는 땅끄부부꺼 봅니당",
          "speaker_id": "1",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100039746.1.17",
          "form": "아 그분들 유명하더라고요",
          "original_form": "아 그분들 유명하더라고요",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100039746.1.18",
          "form": "효과도 있나요?",
          "original_form": "효과도 있나요?",
          "speaker_id": "2",
          "time": "20210905 21:57"
        },
        {
          "id": "MDRW2100039746.1.19",
          "form": "효과를 볼만큼\r\n지속을 못하는게 함정이요ㅠㅋ",
          "original_form": "효과를 볼만큼\r\n지속을 못하는게 함정이요ㅠㅋ",
          "speaker_id": "1",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100039746.1.20",
          "form": "근육통 생기나요?",
          "original_form": "근육통 생기나요?",
          "speaker_id": "2",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100039746.1.21",
          "form": "담날 생활하는데 불편할 정도는\r\n아니고 조금 있긴 해요 ㅋ",
          "original_form": "담날 생활하는데 불편할 정도는\r\n아니고 조금 있긴 해요 ㅋ",
          "speaker_id": "1",
          "time": "20210905 22:01"
        },
        {
          "id": "MDRW2100039746.1.22",
          "form": "아 그럼 효과가 있네요!",
          "original_form": "아 그럼 효과가 있네요!",
          "speaker_id": "2",
          "time": "20210905 22:01"
        }
      ]
    }
