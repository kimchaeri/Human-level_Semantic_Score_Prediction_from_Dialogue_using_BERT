      "utterance": [
        {
          "id": "MDRW2100024202.1.1",
          "form": "요즘 너무 이뻐요ㅎㅎ",
          "original_form": "요즘 너무 이뻐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100024202.1.2",
          "form": "이쁠나이죠 ㅋ",
          "original_form": "이쁠나이죠 ㅋ",
          "speaker_id": "1",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100024202.1.3",
          "form": "말도잘하고ㅎ 놀기도 하고ㅎ",
          "original_form": "말도잘하고ㅎ 놀기도 하고ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100024202.1.4",
          "form": "오늘도 목욕시키는데 얼마나 이쁘던지ㅎㅎ",
          "original_form": "오늘도 목욕시키는데 얼마나 이쁘던지ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100024202.1.5",
          "form": "더 눈에 많이 담아요   너무 빨리커서 ㅎㅎㅎ",
          "original_form": "더 눈에 많이 담아요   너무 빨리커서 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100024202.1.6",
          "form": "아빠랑 목욕할래? 엄마랑 목욕할래?",
          "original_form": "아빠랑 목욕할래? 엄마랑 목욕할래?",
          "speaker_id": "2",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100024202.1.7",
          "form": "아쉬어요",
          "original_form": "아쉬어요",
          "speaker_id": "1",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100024202.1.8",
          "form": "이러니깐ㅎㅎ 아빠랑한대요ㅎㅎ",
          "original_form": "이러니깐ㅎㅎ 아빠랑한대요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100024202.1.9",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:11"
        },
        {
          "id": "MDRW2100024202.1.10",
          "form": "벌써 시간이 이렇게 됫네요ㅠ 너무 아쉬워요ㅠ",
          "original_form": "벌써 시간이 이렇게 됫네요ㅠ 너무 아쉬워요ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100024202.1.11",
          "form": "우리집은 혼자 하셔요  물론  문열고 쳐다보고 있지만요ㅠㅋ",
          "original_form": "우리집은 혼자 하셔요  물론  문열고 쳐다보고 있지만요ㅠㅋ",
          "speaker_id": "1",
          "time": "20210830 23:12"
        },
        {
          "id": "MDRW2100024202.1.12",
          "form": "머리랑 마무리만 해줘여 ㅋ",
          "original_form": "머리랑 마무리만 해줘여 ㅋ",
          "speaker_id": "1",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100024202.1.13",
          "form": "아아ㅎ 그래도 남편분이 많이 도와주시나보네요ㅎ",
          "original_form": "아아ㅎ 그래도 남편분이 많이 도와주시나보네요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100024202.1.14",
          "form": "오오ㅎㅎ 가정적이시군요ㅎ",
          "original_form": "오오ㅎㅎ 가정적이시군요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100024202.1.15",
          "form": "전혀요",
          "original_form": "전혀요",
          "speaker_id": "1",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100024202.1.16",
          "form": "같이 목욕하잖아요ㅎㅎ",
          "original_form": "같이 목욕하잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100024202.1.17",
          "form": "육아 독박이에요",
          "original_form": "육아 독박이에요",
          "speaker_id": "1",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100024202.1.18",
          "form": "그럼 잘도와주는거죠ㅎ",
          "original_form": "그럼 잘도와주는거죠ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100024202.1.19",
          "form": "아이가 혼자 하는데",
          "original_form": "아이가 혼자 하는데",
          "speaker_id": "1",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100024202.1.20",
          "form": "아아ㅠ 그래요?ㅠ",
          "original_form": "아아ㅠ 그래요?ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100024202.1.21",
          "form": "아이가혼자?",
          "original_form": "아이가혼자?",
          "speaker_id": "2",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100024202.1.22",
          "form": "남편분하고아니고요?",
          "original_form": "남편분하고아니고요?",
          "speaker_id": "2",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100024202.1.23",
          "form": "네 저는 지켜보고요 ㅎㅎ",
          "original_form": "네 저는 지켜보고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:13"
        },
        {
          "id": "MDRW2100024202.1.24",
          "form": "아아 그렇구나ㅎ",
          "original_form": "아아 그렇구나ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100024202.1.25",
          "form": "우와 근데 정말 빠른건가요?",
          "original_form": "우와 근데 정말 빠른건가요?",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100024202.1.26",
          "form": "혼자  잘 씩어요 물론 마무리는 제가 합니다 ㅎㅎ",
          "original_form": "혼자  잘 씩어요 물론 마무리는 제가 합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100024202.1.27",
          "form": "엄청 빠르다고 생각이 드는데?",
          "original_form": "엄청 빠르다고 생각이 드는데?",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100024202.1.28",
          "form": "이야ㅎ 대단하네요ㅎ",
          "original_form": "이야ㅎ 대단하네요ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100024202.1.29",
          "form": "엄마가 잘가르쳤네요ㅎㅎ",
          "original_form": "엄마가 잘가르쳤네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100024202.1.30",
          "form": "거품스프레이 쓰니까",
          "original_form": "거품스프레이 쓰니까",
          "speaker_id": "1",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100024202.1.31",
          "form": "스스로 더 하려고 해요 ㅎㅎ",
          "original_form": "스스로 더 하려고 해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:14"
        },
        {
          "id": "MDRW2100024202.1.32",
          "form": "재밋어서 ㅎㅎ",
          "original_form": "재밋어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100024202.1.33",
          "form": "ㅎㅎ 저희도 써요ㅎ 거품ㅎ",
          "original_form": "ㅎㅎ 저희도 써요ㅎ 거품ㅎ",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100024202.1.34",
          "form": "그거 애들 엄청 좋아하잖아요ㅎㅎ",
          "original_form": "그거 애들 엄청 좋아하잖아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100024202.1.35",
          "form": "저도 써요 세안 할때",
          "original_form": "저도 써요 세안 할때",
          "speaker_id": "1",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100024202.1.36",
          "form": "그래도 부럽당ㅎ 우리아이는 아직도 머리캡 하면 난리치는데ㅎㅎ",
          "original_form": "그래도 부럽당ㅎ 우리아이는 아직도 머리캡 하면 난리치는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:15"
        },
        {
          "id": "MDRW2100024202.1.37",
          "form": "폼이 엄청 쫀쫀해서  세안용으로 좋아요 ㅋ",
          "original_form": "폼이 엄청 쫀쫀해서  세안용으로 좋아요 ㅋ",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024202.1.38",
          "form": "생크림 같은 ㅋ",
          "original_form": "생크림 같은 ㅋ",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024202.1.39",
          "form": "아 저더 4살땐 캡이 뭔가요",
          "original_form": "아 저더 4살땐 캡이 뭔가요",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024202.1.40",
          "form": "안고  머리 감겻어여",
          "original_form": "안고  머리 감겻어여",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024202.1.41",
          "form": "아아ㅠㅠ",
          "original_form": "아아ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100024202.1.42",
          "form": "그건 힘들었겟네요ㅠ",
          "original_form": "그건 힘들었겟네요ㅠ",
          "speaker_id": "2",
          "time": "20210830 23:16"
        }
      ]
    }
