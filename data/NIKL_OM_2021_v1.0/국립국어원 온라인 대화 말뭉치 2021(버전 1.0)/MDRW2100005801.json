      "utterance": [
        {
          "id": "MDRW2100005801.1.1",
          "form": "오늘 아침.점심 드셨나요?",
          "original_form": "오늘 아침.점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210907 13:34"
        },
        {
          "id": "MDRW2100005801.1.2",
          "form": "아침먹었어요",
          "original_form": "아침먹었어요",
          "speaker_id": "1",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100005801.1.3",
          "form": "밥 두공기해서",
          "original_form": "밥 두공기해서",
          "speaker_id": "1",
          "time": "20210907 17:57"
        },
        {
          "id": "MDRW2100005801.1.4",
          "form": "비빔밥ㅋㅋ",
          "original_form": "비빔밥ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:58"
        },
        {
          "id": "MDRW2100005801.1.5",
          "form": "비빔밥요ㅡ? 나물넣고요?",
          "original_form": "비빔밥요ㅡ? 나물넣고요?",
          "speaker_id": "2",
          "time": "20210907 18:24"
        },
        {
          "id": "MDRW2100005801.1.6",
          "form": "네네ㅋㅋㅋㅋ나물도 배달해주더라고요",
          "original_form": "네네ㅋㅋㅋㅋ나물도 배달해주더라고요",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100005801.1.7",
          "form": "좋네요 식당인가요",
          "original_form": "좋네요 식당인가요",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100005801.1.8",
          "form": "반찬가게요!",
          "original_form": "반찬가게요!",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100005801.1.9",
          "form": "동네에서 유명해여",
          "original_form": "동네에서 유명해여",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100005801.1.10",
          "form": "아 반찬가게군요 배달도 되구요?",
          "original_form": "아 반찬가게군요 배달도 되구요?",
          "speaker_id": "2",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100005801.1.11",
          "form": "네네~~",
          "original_form": "네네~~",
          "speaker_id": "1",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100005801.1.12",
          "form": "계란말이도 시켰는제 비싸도 맛잇더라구요",
          "original_form": "계란말이도 시켰는제 비싸도 맛잇더라구요",
          "speaker_id": "1",
          "time": "20210908 11:21"
        },
        {
          "id": "MDRW2100005801.1.13",
          "form": "아 식당 계란말이는 비법이 뭘가요 ㅎㅎ 맛있죠",
          "original_form": "아 식당 계란말이는 비법이 뭘가요 ㅎㅎ 맛있죠",
          "speaker_id": "2",
          "time": "20210908 11:22"
        },
        {
          "id": "MDRW2100005801.1.14",
          "form": "학교에서 먹던",
          "original_form": "학교에서 먹던",
          "speaker_id": "1",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100005801.1.15",
          "form": "계란찜비슷한 계란말이요ㅋㅋㅋㅋ",
          "original_form": "계란찜비슷한 계란말이요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:23"
        },
        {
          "id": "MDRW2100005801.1.16",
          "form": "맛있어요",
          "original_form": "맛있어요",
          "speaker_id": "1",
          "time": "20210908 11:24"
        },
        {
          "id": "MDRW2100005801.1.17",
          "form": "식당 계란찜도 완전 맛있고 ㅠㅠ",
          "original_form": "식당 계란찜도 완전 맛있고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:25"
        },
        {
          "id": "MDRW2100005801.1.18",
          "form": "비법 알고싶어요ㅋㅋㅋㅋㅋ",
          "original_form": "비법 알고싶어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:26"
        },
        {
          "id": "MDRW2100005801.1.19",
          "form": "아무도 안 알려줄거야",
          "original_form": "아무도 안 알려줄거야",
          "speaker_id": "1",
          "time": "20210908 11:26"
        }
      ]
