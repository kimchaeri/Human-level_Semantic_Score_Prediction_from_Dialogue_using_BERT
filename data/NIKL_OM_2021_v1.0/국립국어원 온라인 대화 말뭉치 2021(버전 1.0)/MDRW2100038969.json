      "utterance": [
        {
          "id": "MDRW2100038969.1.1",
          "form": "저도 땅끄랑 빅씨스만 봤어요",
          "original_form": "저도 땅끄랑 빅씨스만 봤어요",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100038969.1.2",
          "form": "그외에는 동네걷기만 ㅎㅎ",
          "original_form": "그외에는 동네걷기만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 23:44"
        },
        {
          "id": "MDRW2100038969.1.3",
          "form": "오호 저도 산책 좋아합니다",
          "original_form": "오호 저도 산책 좋아합니다",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100038969.1.4",
          "form": "다른것보다 덜 지루한것같아요",
          "original_form": "다른것보다 덜 지루한것같아요",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100038969.1.5",
          "form": "동네 구경하는 재미가 있죠",
          "original_form": "동네 구경하는 재미가 있죠",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100038969.1.6",
          "form": "넹 노래들으면서 걸으면 금방이구요",
          "original_form": "넹 노래들으면서 걸으면 금방이구요",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100038969.1.7",
          "form": "비가 좀멈춰야 또 걸으러나갈텐대 ㅠ",
          "original_form": "비가 좀멈춰야 또 걸으러나갈텐대 ㅠ",
          "speaker_id": "1",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100038969.1.8",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100038969.1.9",
          "form": "오늘도 비와가지고ㅜㅜ",
          "original_form": "오늘도 비와가지고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100038969.1.10",
          "form": "현재 다이어트중이세요?",
          "original_form": "현재 다이어트중이세요?",
          "speaker_id": "1",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100038969.1.11",
          "form": "입다이어터입니다 ㄱㅋㅋ",
          "original_form": "입다이어터입니다 ㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100038969.1.12",
          "form": "옷에도 관심많으세요?",
          "original_form": "옷에도 관심많으세요?",
          "speaker_id": "1",
          "time": "20210902 23:47"
        },
        {
          "id": "MDRW2100038969.1.13",
          "form": "옷은 평소에는 관심없다가",
          "original_form": "옷은 평소에는 관심없다가",
          "speaker_id": "2",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100038969.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100038969.1.15",
          "form": "꽂히면 꼭 사고싶어요",
          "original_form": "꽂히면 꼭 사고싶어요",
          "speaker_id": "2",
          "time": "20210902 23:48"
        },
        {
          "id": "MDRW2100038969.1.16",
          "form": "오 보통 어떤류 좋아하시나요",
