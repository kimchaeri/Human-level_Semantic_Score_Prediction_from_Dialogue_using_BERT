      "utterance": [
        {
          "id": "MDRW2100013948.1.1",
          "form": "어제 사온 빵 중에 꽈배기가 젤 맛난듯",
          "original_form": "어제 사온 빵 중에 꽈배기가 젤 맛난듯",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100013948.1.2",
          "form": "그래 맛이 괜찮네",
          "original_form": "그래 맛이 괜찮네",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100013948.1.3",
          "form": "옹 ㅋㅋ 기름에 튀긴거라 더 맛나나봄",
          "original_form": "옹 ㅋㅋ 기름에 튀긴거라 더 맛나나봄",
          "speaker_id": "1",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100013948.1.4",
          "form": "그렇지 튀기면 맛없는게 없다",
          "original_form": "그렇지 튀기면 맛없는게 없다",
          "speaker_id": "2",
          "time": "20210825 17:33"
        },
        {
          "id": "MDRW2100013948.1.5",
          "form": "그런듯..ㅋ 살에 찌는 음식이 넘 맛있당",
          "original_form": "그런듯..ㅋ 살에 찌는 음식이 넘 맛있당",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100013948.1.6",
          "form": "조금만 먹으면 괜찮겠지",
          "original_form": "조금만 먹으면 괜찮겠지",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100013948.1.7",
          "form": "마장 ㅋ 비록 왕꽈배기였지만 ㅋㅋ",
          "original_form": "마장 ㅋ 비록 왕꽈배기였지만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100013948.1.8",
          "form": "괜찮다 이따 저녁은 적게 먹어라",
          "original_form": "괜찮다 이따 저녁은 적게 먹어라",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100013948.1.9",
          "form": "구래용 ㅎ 그럼 살안찌겠군",
          "original_form": "구래용 ㅎ 그럼 살안찌겠군",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100013948.1.10",
          "form": "그래 다음에는 사 오지 마라",
          "original_form": "그래 다음에는 사 오지 마라",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100013948.1.11",
          "form": "왱 맛있는뎁~",
          "original_form": "왱 맛있는뎁~",
          "speaker_id": "1",
          "time": "20210825 17:34"
        },
        {
          "id": "MDRW2100013948.1.12",
          "form": "나중에 살 빠지면 사 와라",
          "original_form": "나중에 살 빠지면 사 와라",
          "speaker_id": "2",
          "time": "20210825 17:34"
        },
        {
