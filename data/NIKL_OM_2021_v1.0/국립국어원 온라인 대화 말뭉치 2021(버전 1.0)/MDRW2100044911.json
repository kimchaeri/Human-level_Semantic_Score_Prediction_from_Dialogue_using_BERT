        {
          "id": "MDRW2100044911.1.1",
          "form": "오늘뭐샀엉?",
          "original_form": "오늘뭐샀엉?",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044911.1.2",
          "form": "스티커 샀징",
          "original_form": "스티커 샀징",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044911.1.3",
          "form": "스티커? 어떤스티커말하는거야?",
          "original_form": "스티커? 어떤스티커말하는거야?",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044911.1.4",
          "form": "버블 스티커랑 별 모양 스티커!",
          "original_form": "버블 스티커랑 별 모양 스티커!",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044911.1.5",
          "form": "오옹 어디서 샀어?",
          "original_form": "오옹 어디서 샀어?",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044911.1.6",
          "form": "텐바이텐이라고 있는데 거기서 샀지",
          "original_form": "텐바이텐이라고 있는데 거기서 샀지",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044911.1.7",
          "form": "텐바이텐 매장 잇던데 거기서샀엉?",
          "original_form": "텐바이텐 매장 잇던데 거기서샀엉?",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044911.1.8",
          "form": "텐바이텐에 한 번 빠지면 헤어나올 수가 없어ㅠㅠ",
          "original_form": "텐바이텐에 한 번 빠지면 헤어나올 수가 없어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044911.1.9",
          "form": "아닝 어플도 있어서 어플로 샀어",
          "original_form": "아닝 어플도 있어서 어플로 샀어",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044911.1.10",
          "form": "매장은 대학로에 있어서 너무 멀어ㅜㅠ",
          "original_form": "매장은 대학로에 있어서 너무 멀어ㅜㅠ",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044911.1.11",
          "form": "아항 2개만 산거야?",
          "original_form": "아항 2개만 산거야?",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044911.1.12",
          "form": "아닝 갯수로만 따지면 12개 샀어",
          "original_form": "아닝 갯수로만 따지면 12개 샀어",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044911.1.13",
          "form": "오 많이샀구만 다해서 얼마들었어?ㅋㅋㅋ",
          "original_form": "오 많이샀구만 다해서 얼마들었어?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044911.1.14",
          "form": "스티커 빨리 왔으면 좋겠다ㅠ",
          "original_form": "스티커 빨리 왔으면 좋겠다ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044911.1.15",
          "form": "할인 혜택 받아서 2만 2천원에 샀엉",
          "original_form": "할인 혜택 받아서 2만 2천원에 샀엉",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044911.1.16",
          "form": "스티커에 2만원이라니 플렛스했구먼",
          "original_form": "스티커에 2만원이라니 플렛스했구먼",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044911.1.17",
          "form": "오늘 플렉스 했냉",
          "original_form": "오늘 플렉스 했냉",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044911.1.18",
          "form": "ㅋㅋㅋㅋ 그래도 자제한거야",
          "original_form": "ㅋㅋㅋㅋ 그래도 자제한거야",
