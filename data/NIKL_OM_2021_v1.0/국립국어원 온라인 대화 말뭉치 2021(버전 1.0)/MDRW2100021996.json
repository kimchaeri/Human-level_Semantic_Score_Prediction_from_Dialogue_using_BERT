      "utterance": [
        {
          "id": "MDRW2100021996.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210829 00:34"
        },
        {
          "id": "MDRW2100021996.1.2",
          "form": "요즘 날씨가 어떤가요?",
          "original_form": "요즘 날씨가 어떤가요?",
          "speaker_id": "1",
          "time": "20210829 00:34"
        },
        {
          "id": "MDRW2100021996.1.3",
          "form": "가을 장마 꿉꿉해요 ㅜㅜ",
          "original_form": "가을 장마 꿉꿉해요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021996.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 00:39"
        },
        {
          "id": "MDRW2100021996.1.5",
          "form": "저도 덥지는 않은데 꿉꿉해서 에어컨 돌리고 있어요",
          "original_form": "저도 덥지는 않은데 꿉꿉해서 에어컨 돌리고 있어요",
          "speaker_id": "1",
          "time": "20210829 00:40"
        },
        {
          "id": "MDRW2100021996.1.6",
          "form": "저두요 껏다 켯다",
          "original_form": "저두요 껏다 켯다",
          "speaker_id": "2",
          "time": "20210829 00:43"
        },
        {
          "id": "MDRW2100021996.1.7",
          "form": "못자네요 ㅎㅎ",
          "original_form": "못자네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 00:43"
        },
        {
          "id": "MDRW2100021996.1.8",
          "form": "덥다고 느끼진 않은데 땀날때도 있고 그래요 흑흑",
          "original_form": "덥다고 느끼진 않은데 땀날때도 있고 그래요 흑흑",
          "speaker_id": "1",
          "time": "20210829 00:44"
        },
        {
          "id": "MDRW2100021996.1.9",
          "form": "여름보다 더 싫어요 ㄹㅎ",
          "original_form": "여름보다 더 싫어요 ㄹㅎ",
          "speaker_id": "2",
          "time": "20210829 00:45"
        },
        {
          "id": "MDRW2100021996.1.10",
          "form": "실제 여름에는 이렇게 비 안 왔잖아요ㅜㅜ",
          "original_form": "실제 여름에는 이렇게 비 안 왔잖아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 00:49"
        },
        {
          "id": "MDRW2100021996.1.11",
          "form": "모기도 오히려 요즘 물려요",
          "original_form": "모기도 오히려 요즘 물려요",
          "speaker_id": "1",
          "time": "20210829 00:49"
        },
        {
          "id": "MDRW2100021996.1.12",
          "form": "여름엔 없더니\n이제 모기가 난리네요",
          "original_form": "여름엔 없더니\n이제 모기가 난리네요",
          "speaker_id": "2",
          "time": "20210829 00:53"
        },
        {
          "id": "MDRW2100021996.1.13",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210829 00:55"
        },
        {
          "id": "MDRW2100021996.1.14",
          "form": "올여름 모기 안 물ㄹㅣ고 지나가나 했더니...",
          "original_form": "올여름 모기 안 물ㄹㅣ고 지나가나 했더니...",
          "speaker_id": "1",
          "time": "20210829 00:56"
        },
        {
          "id": "MDRW2100021996.1.15",
          "form": "모기도 넘 더워서 쉬고 잇엇나봄",
          "original_form": "모기도 넘 더워서 쉬고 잇엇나봄",
          "speaker_id": "2",
          "time": "20210829 00:59"
        },
        {
          "id": "MDRW2100021996.1.16",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "1",
          "time": "20210829 01:04"
        },
        {
          "id": "MDRW2100021996.1.17",
          "form": "이제 좀 시원해지니까 나오네요",
          "original_form": "이제 좀 시원해지니까 나오네요",
          "speaker_id": "1",
          "time": "20210829 01:04"
        },
        {
          "id": "MDRW2100021996.1.18",
          "form": "모기나 사람이나 더우면 암것도 하기 시른건 매한가지나봐요 ㄷㄷ",
          "original_form": "모기나 사람이나 더우면 암것도 하기 시른건 매한가지나봐요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100021996.1.19",
          "form": "그런가봐요",
          "original_form": "그런가봐요",
          "speaker_id": "1",
          "time": "20210829 01:10"
        },
        {
          "id": "MDRW2100021996.1.20",
          "form": "더우면 의욕이 확 떨어지잖아요",
          "original_form": "더우면 의욕이 확 떨어지잖아요",
          "speaker_id": "1",
