      "utterance": [
        {
          "id": "MDRW2100031027.1.1",
          "form": "다른 음식은 뭐 좋아하세요?",
          "original_form": "다른 음식은 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100031027.1.2",
          "form": "영화 관람 좋아하세요?",
          "original_form": "영화 관람 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100031027.1.3",
          "form": "영화관 간지 진짜오래됐어요 ㅎ",
          "original_form": "영화관 간지 진짜오래됐어요 ㅎ",
          "speaker_id": "1",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100031027.1.4",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100031027.1.5",
          "form": "저도 몇년된거같아요",
          "original_form": "저도 몇년된거같아요",
          "speaker_id": "2",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100031027.1.6",
          "form": "영화값도 비싸요ㅋㅋ",
          "original_form": "영화값도 비싸요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100031027.1.7",
          "form": "맞아요 ᆢㅜ",
          "original_form": "맞아요 ᆢㅜ",
          "speaker_id": "1",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100031027.1.8",
          "form": "가격이 안오르는게 없네요",
          "original_form": "가격이 안오르는게 없네요",
          "speaker_id": "1",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100031027.1.9",
          "form": "어떤 장르 좋아하세요?",
          "original_form": "어떤 장르 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 15:54"
        },
        {
          "id": "MDRW2100031027.1.10",
          "form": "저는 그냥 고민안하고 볼수있는",
          "original_form": "저는 그냥 고민안하고 볼수있는",
          "speaker_id": "2",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100031027.1.11",
          "form": "쉬운코미디요",
          "original_form": "쉬운코미디요",
          "speaker_id": "2",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100031027.1.12",
          "form": "그런거 좋죠",
          "original_form": "그런거 좋죠",
          "speaker_id": "1",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100031027.1.13",
          "form": "어려우면 다시봐야해서 ㅋㅋ",
          "original_form": "어려우면 다시봐야해서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100031027.1.14",
          "form": "저도 코미디나 음악나오는 뮤지컬장르",
          "original_form": "저도 코미디나 음악나오는 뮤지컬장르",
          "speaker_id": "1",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100031027.1.15",
          "form": "이런거 좋아해요",
          "original_form": "이런거 좋아해요",
          "speaker_id": "1",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100031027.1.16",
          "form": "뮤지컬영화요?",
          "original_form": "뮤지컬영화요?",
          "speaker_id": "2",
          "time": "20210904 15:56"
        },
        {
          "id": "MDRW2100031027.1.17",
          "form": "네네 맘마미아나",
          "original_form": "네네 맘마미아나",
          "speaker_id": "1",
          "time": "20210904 15:57"
        },
        {
          "id": "MDRW2100031027.1.18",
          "form": "레미제라블 같은거요",
          "original_form": "레미제라블 같은거요",
          "speaker_id": "1",
          "time": "20210904 15:57"
        },
        {
          "id": "MDRW2100031027.1.19",
          "form": "아아 저도요",
          "original_form": "아아 저도요",
          "speaker_id": "2",
          "time": "20210904 15:57"
        },
        {
          "id": "MDRW2100031027.1.20",
          "form": "뮤지컬도 관심있으시겠네요",
          "original_form": "뮤지컬도 관심있으시겠네요",
          "speaker_id": "2",
          "time": "20210904 15:57"
        },
        {
          "id": "MDRW2100031027.1.21",
          "form": "네 너무 비싸서 어쩌다한번",
          "original_form": "네 너무 비싸서 어쩌다한번",
          "speaker_id": "1",
          "time": "20210904 15:57"
        },
        {
          "id": "MDRW2100031027.1.22",
          "form": "봐요",
          "original_form": "봐요",
          "speaker_id": "1",
          "time": "20210904 15:57"
        },
        {
          "id": "MDRW2100031027.1.23",
          "form": "좋아하긴해요",
          "original_form": "좋아하긴해요",
          "speaker_id": "1",
          "time": "20210904 15:57"
        }
      ]
