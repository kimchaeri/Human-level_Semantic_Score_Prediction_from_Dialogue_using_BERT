      "utterance": [
        {
          "id": "MDRW2100038785.1.1",
          "form": "ㄴㄴ저것도",
          "original_form": "ㄴㄴ저것도",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100038785.1.2",
          "form": "그 번개장판없는애는",
          "original_form": "그 번개장판없는애는",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100038785.1.3",
          "form": "없애는",
          "original_form": "없애는",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100038785.1.4",
          "form": "그거야",
          "original_form": "그거야",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100038785.1.5",
          "form": "그 퀘는",
          "original_form": "그 퀘는",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038785.1.6",
          "form": "뭐 열어야해?",
          "original_form": "뭐 열어야해?",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038785.1.7",
          "form": "아님 첨부터 있어?",
          "original_form": "아님 첨부터 있어?",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038785.1.8",
          "form": "걍 이나즈마 캐서린한테 가서 말거니까",
          "original_form": "걍 이나즈마 캐서린한테 가서 말거니까",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038785.1.9",
          "form": "주던데?",
          "original_form": "주던데?",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038785.1.10",
          "form": "엥 그래???",
          "original_form": "엥 그래???",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038785.1.11",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038785.1.12",
          "form": "몬드 캐서린한테 가서",
          "original_form": "몬드 캐서린한테 가서",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100038785.1.13",
          "form": "낚시퀘만 받아오고 끝난줄....",
          "original_form": "낚시퀘만 받아오고 끝난줄....",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038785.1.14",
          "form": "저거 퍼즐있고(그닥 안어려움)",
          "original_form": "저거 퍼즐있고(그닥 안어려움)",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038785.1.15",
          "form": "날라다니는거 있어",
          "original_form": "날라다니는거 있어",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038785.1.16",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038785.1.17",
          "form": "들어가서 찾아봐야겟다",
          "original_form": "들어가서 찾아봐야겟다",
          "speaker_id": "1",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100038785.1.18",
          "form": "퍼즐 다 풀면",
          "original_form": "퍼즐 다 풀면",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038785.1.19",
          "form": "누가봐도 보스 등장이요 싶은 원형 경기장같은게",
          "original_form": "누가봐도 보스 등장이요 싶은 원형 경기장같은게",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038785.1.20",
          "form": "생기거든?",
          "original_form": "생기거든?",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038785.1.21",
          "form": "그럼 보스 잡으면 돼",
          "original_form": "그럼 보스 잡으면 돼",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038785.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038785.1.23",
          "form": "난 일단",
          "original_form": "난 일단",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038785.1.24",
          "form": "퀘스트부터 받아야해 ㅋㅋㅋㅋ",
          "original_form": "퀘스트부터 받아야해 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038785.1.25",
          "form": "그리고 세이라이 섬 가운데에 있는 워프포인트",
          "original_form": "그리고 세이라이 섬 가운데에 있는 워프포인트",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100038785.1.26",
          "form": "한개 빼고 다 상공에 있어",
          "original_form": "한개 빼고 다 상공에 있어",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100038785.1.27",
          "form": "안보여도 의아해하지 마",
          "original_form": "안보여도 의아해하지 마",
          "speaker_id": "2",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100038785.1.28",
          "form": "오....",
          "original_form": "오....",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038785.1.29",
          "form": "갈수록 이런 것만 발전하네",
          "original_form": "갈수록 이런 것만 발전하네",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100038785.1.30",
          "form": "ㅇㅇ.......나는 여기도",
          "original_form": "ㅇㅇ.......나는 여기도",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100038785.1.31",
          "form": "해저동굴이 있나 했더니",
          "original_form": "해저동굴이 있나 했더니",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100038785.1.32",
          "form": "죄다 상공에 있더라구......",
          "original_form": "죄다 상공에 있더라구......",
          "speaker_id": "2",
          "time": "20210902 21:21"
        },
        {
          "id": "MDRW2100038785.1.33",
          "form": "근데 허공에 있으면",
          "original_form": "근데 허공에 있으면",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100038785.1.34",
          "form": "어케열어?",
          "original_form": "어케열어?",
          "speaker_id": "1",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100038785.1.35",
          "form": "그 번개씨앗으로",
          "original_form": "그 번개씨앗으로",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100038785.1.36",
          "form": "날아다니는거 있잖아",
          "original_form": "날아다니는거 있잖아",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100038785.1.37",
          "form": "그리고 그",
          "original_form": "그리고 그",
          "speaker_id": "2",
          "time": "20210902 21:23"
        },
        {
          "id": "MDRW2100038785.1.38",
          "form": "새로 열린 섬들에 그",
          "original_form": "새로 열린 섬들에 그",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100038785.1.39",
          "form": "문같은거 있잖아",
          "original_form": "문같은거 있잖아",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100038785.1.40",
          "form": "아 그 핀셋같은거?",
          "original_form": "아 그 핀셋같은거?",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100038785.1.41",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "1",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100038785.1.42",
          "form": "그거타고 날아다녀야돼",
          "original_form": "그거타고 날아다녀야돼",
          "speaker_id": "2",
          "time": "20210902 21:24"
        },
        {
          "id": "MDRW2100038785.1.43",
          "form": "이제 집간다",
          "original_form": "이제 집간다",
          "speaker_id": "1",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100038785.1.44",
          "form": "맵 마저 열어야지",
          "original_form": "맵 마저 열어야지",
          "speaker_id": "1",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100038785.1.45",
          "form": "ㅇ ㅏ 근데 미니게임 진짜 짜증나",
          "original_form": "ㅇ ㅏ 근데 미니게임 진짜 짜증나",
          "speaker_id": "1",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100038785.1.46",
          "form": "죽지만 않으면 되지",
          "original_form": "죽지만 않으면 되지",
          "speaker_id": "1",
          "time": "20210902 21:28"
        },
        {
          "id": "MDRW2100038785.1.47",
          "form": "뭘 피격 횟수까지 카운팅해;;",
          "original_form": "뭘 피격 횟수까지 카운팅해;;",
          "speaker_id": "1",
          "time": "20210902 21:28"
        }
      ]
    }
