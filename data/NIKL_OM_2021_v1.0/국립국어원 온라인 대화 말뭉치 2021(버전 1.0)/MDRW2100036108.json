      "utterance": [
        {
          "id": "MDRW2100036108.1.1",
          "form": "앜ㅋㅋ 어제 저녁으로 불고기덮밥먹었어요~",
          "original_form": "앜ㅋㅋ 어제 저녁으로 불고기덮밥먹었어요~",
          "speaker_id": "2",
          "time": "20210831 13:56"
        },
        {
          "id": "MDRW2100036108.1.2",
          "form": "오늘 식사하셨나요?",
          "original_form": "오늘 식사하셨나요?",
          "speaker_id": "2",
          "time": "20210831 13:56"
        },
        {
          "id": "MDRW2100036108.1.3",
          "form": "네 ㅎ 점심에",
          "original_form": "네 ㅎ 점심에",
          "speaker_id": "1",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100036108.1.4",
          "form": "어묵탕이요 ㅎ",
          "original_form": "어묵탕이요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100036108.1.5",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210831 17:07"
        },
        {
          "id": "MDRW2100036108.1.6",
          "form": "전 점심때는 불고기덮밥먹고 저녁으론 갈비뜯었어요 ㅋㅋㅋㅋ",
          "original_form": "전 점심때는 불고기덮밥먹고 저녁으론 갈비뜯었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 22:34"
        },
        {
          "id": "MDRW2100036108.1.7",
          "form": "헉 갈비이......",
          "original_form": "헉 갈비이......",
          "speaker_id": "1",
          "time": "20210901 01:21"
        },
        {
          "id": "MDRW2100036108.1.8",
          "form": "짱이네유",
          "original_form": "짱이네유",
          "speaker_id": "1",
          "time": "20210901 01:21"
        },
        {
          "id": "MDRW2100036108.1.9",
          "form": "ㅋㅋㅋㅋ 고기가최고죠~~",
          "original_form": "ㅋㅋㅋㅋ 고기가최고죠~~",
          "speaker_id": "2",
          "time": "20210902 00:41"
        },
        {
          "id": "MDRW2100036108.1.10",
          "form": "진심이요",
          "original_form": "진심이요",
          "speaker_id": "1",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100036108.1.11",
          "form": "고기가 최고입니다ㅋㅋㅋㅋ",
          "original_form": "고기가 최고입니다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:43"
        },
        {
          "id": "MDRW2100036108.1.12",
          "form": "어떤종류고기젤좋아하세여??",
          "original_form": "어떤종류고기젤좋아하세여??",
          "speaker_id": "2",
          "time": "20210902 00:54"
        },
        {
          "id": "MDRW2100036108.1.13",
          "form": "전 삼겹이요",
          "original_form": "전 삼겹이요",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100036108.1.14",
          "form": "요새 항정에 빠졌어요",
          "original_form": "요새 항정에 빠졌어요",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100036108.1.15",
          "form": "오우 항정살! 맛잘알이시네요~ㅋㅋㅋ",
          "original_form": "오우 항정살! 맛잘알이시네요~ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100036108.1.16",
          "form": "근데사실저는 닭이나 오리 더 좋아해여",
          "original_form": "근데사실저는 닭이나 오리 더 좋아해여",
          "speaker_id": "2",
          "time": "20210902 01:07"
        },
        {
          "id": "MDRW2100036108.1.17",
          "form": "아 진짜요?",
          "original_form": "아 진짜요?",
          "speaker_id": "1",
          "time": "20210902 07:49"
        },
        {
          "id": "MDRW2100036108.1.18",
          "form": "오리 맛있죠 ㅠㅠ",
          "original_form": "오리 맛있죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 07:49"
        },
        {
          "id": "MDRW2100036108.1.19",
          "form": "부추랑..",
          "original_form": "부추랑..",
          "speaker_id": "1",
          "time": "20210902 07:49"
        },
        {
          "id": "MDRW2100036108.1.20",
          "form": "맞아여 오리는 부추랑먹는게 캡이죵 !ㅋㅋㅋㅋ",
          "original_form": "맞아여 오리는 부추랑먹는게 캡이죵 !ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 13:15"
        },
        {
          "id": "MDRW2100036108.1.21",
          "form": "진짜요 ㅠ 부추랑 무쌈 중",
          "original_form": "진짜요 ㅠ 부추랑 무쌈 중",
          "speaker_id": "1",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100036108.1.22",
          "form": "뭘 더 좋아하시져?",
          "original_form": "뭘 더 좋아하시져?",
          "speaker_id": "1",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100036108.1.23",
          "form": "아 둘다택할순없나요 ㅜㅜ둘다못잃는데 ㅠㅠ",
          "original_form": "아 둘다택할순없나요 ㅜㅜ둘다못잃는데 ㅠㅠ",
          "speaker_id": "2",
