        {
          "id": "MDRW2100030410.1.1",
          "form": "요즘엔 편의점에도 떡볶이 팔아서 좋아요",
          "original_form": "요즘엔 편의점에도 떡볶이 팔아서 좋아요",
          "speaker_id": "2",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100030410.1.2",
          "form": "물 넣고 재료 넣고 전자레인지에 돌리면 조리 끝",
          "original_form": "물 넣고 재료 넣고 전자레인지에 돌리면 조리 끝",
          "speaker_id": "2",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100030410.1.3",
          "form": "까르보나라 떡볶이 맛있죠",
          "original_form": "까르보나라 떡볶이 맛있죠",
          "speaker_id": "1",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100030410.1.4",
          "form": "그건 안 먹어 봤네요",
          "original_form": "그건 안 먹어 봤네요",
          "speaker_id": "2",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100030410.1.5",
          "form": "크림 떡볶이도 좋아요",
          "original_form": "크림 떡볶이도 좋아요",
          "speaker_id": "1",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100030410.1.6",
          "form": "아 먹어 본 것 같기도",
          "original_form": "아 먹어 본 것 같기도",
          "speaker_id": "2",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100030410.1.7",
          "form": "까르보나라 불닭 먹어 봤나요",
          "original_form": "까르보나라 불닭 먹어 봤나요",
          "speaker_id": "1",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100030410.1.8",
          "form": "전에 설빙에 갔는데 떡볶이도 팔더라고요",
          "original_form": "전에 설빙에 갔는데 떡볶이도 팔더라고요",
          "speaker_id": "2",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100030410.1.9",
          "form": "분홍색 소스예요",
          "original_form": "분홍색 소스예요",
          "speaker_id": "1",
          "time": "20210904 00:40"
        },
        {
          "id": "MDRW2100030410.1.10",
          "form": "어울리겠어요",
          "original_form": "어울리겠어요",
          "speaker_id": "1",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030410.1.11",
          "form": "까르보나라 불닭 볶음면 말하는 건가요 좀 맛이에요",
          "original_form": "까르보나라 불닭 볶음면 말하는 건가요 좀 맛이에요",
          "speaker_id": "2",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030410.1.12",
          "form": "떡볶이는 너무 간단하니까",
          "original_form": "떡볶이는 너무 간단하니까",
          "speaker_id": "1",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030410.1.13",
          "form": "좋죠",
          "original_form": "좋죠",
          "speaker_id": "1",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030410.1.14",
          "form": "소스도 먹어 봤어요 그런데 많이는 못 먹었어요",
          "original_form": "소스도 먹어 봤어요 그런데 많이는 못 먹었어요",
          "speaker_id": "2",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030410.1.15",
          "form": "그 소스랑 같아요",
          "original_form": "그 소스랑 같아요",
          "speaker_id": "1",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030410.1.16",
          "form": "대존맛",
          "original_form": "대존맛",
          "speaker_id": "2",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030410.1.17",
          "form": "조금 덜 맵 줘",
          "original_form": "조금 덜 맵 줘",
          "speaker_id": "1",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030410.1.18",
          "form": "떡볶이가 더 맛있어요",
          "original_form": "떡볶이가 더 맛있어요",
          "speaker_id": "1",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030410.1.19",
          "form": "맞아요 맛있어요",
          "original_form": "맞아요 맛있어요",
          "speaker_id": "2",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030410.1.20",
          "form": "치즈 떡볶이 취향입니다",
          "original_form": "치즈 떡볶이 취향입니다",
          "speaker_id": "2",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100030410.1.21",
          "form": "치즈 들어간 거 다 맛있어요",
