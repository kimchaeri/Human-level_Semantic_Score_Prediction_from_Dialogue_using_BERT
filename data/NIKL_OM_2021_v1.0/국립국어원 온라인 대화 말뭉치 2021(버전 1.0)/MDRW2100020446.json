      "utterance": [
        {
          "id": "MDRW2100020446.1.1",
          "form": "요즘 편의점 알바는 진짜 다재다능 해야겠던데요",
          "original_form": "요즘 편의점 알바는 진짜 다재다능 해야겠던데요",
          "speaker_id": "2",
          "time": "20210828 06:02"
        },
        {
          "id": "MDRW2100020446.1.2",
          "form": "특히 원룸가여서",
          "original_form": "특히 원룸가여서",
          "speaker_id": "1",
          "time": "20210828 06:02"
        },
        {
          "id": "MDRW2100020446.1.3",
          "form": "손님이 끊이지 않았어요",
          "original_form": "손님이 끊이지 않았어요",
          "speaker_id": "1",
          "time": "20210828 06:02"
        },
        {
          "id": "MDRW2100020446.1.4",
          "form": "아 그럼 많겠다",
          "original_form": "아 그럼 많겠다",
          "speaker_id": "2",
          "time": "20210828 06:02"
        },
        {
          "id": "MDRW2100020446.1.5",
          "form": "좀 오피스 구역이면",
          "original_form": "좀 오피스 구역이면",
          "speaker_id": "1",
          "time": "20210828 06:02"
        },
        {
          "id": "MDRW2100020446.1.6",
          "form": "퇴근하고나서는 사람이 없거든용",
          "original_form": "퇴근하고나서는 사람이 없거든용",
          "speaker_id": "1",
          "time": "20210828 06:02"
        },
        {
          "id": "MDRW2100020446.1.7",
          "form": "근데 끊임없이 들어와요ㅋㅋㅋ 새벽까지도",
          "original_form": "근데 끊임없이 들어와요ㅋㅋㅋ 새벽까지도",
          "speaker_id": "1",
          "time": "20210828 06:03"
        },
        {
          "id": "MDRW2100020446.1.8",
          "form": "저도 지금 거주지가 대학교 근처라서 원룸이 많아서",
          "original_form": "저도 지금 거주지가 대학교 근처라서 원룸이 많아서",
          "speaker_id": "2",
          "time": "20210828 06:03"
        },
        {
          "id": "MDRW2100020446.1.9",
          "form": "동네 편의점에 늘 사람 많더라구요",
          "original_form": "동네 편의점에 늘 사람 많더라구요",
          "speaker_id": "2",
          "time": "20210828 06:03"
        },
        {
          "id": "MDRW2100020446.1.10",
          "form": "네네 밤에 혼자 술마시고",
          "original_form": "네네 밤에 혼자 술마시고",
          "speaker_id": "1",
          "time": "20210828 06:03"
        },
        {
          "id": "MDRW2100020446.1.11",
          "form": "뭐 그런",
          "original_form": "뭐 그런",
          "speaker_id": "1",
          "time": "20210828 06:03"
        },
        {
          "id": "MDRW2100020446.1.12",
          "form": "앜ㅋㅋㅋ또 새벽에 하셨군요",
          "original_form": "앜ㅋㅋㅋ또 새벽에 하셨군요",
          "speaker_id": "2",
          "time": "20210828 06:03"
        },
        {
          "id": "MDRW2100020446.1.13",
          "form": "네네ㅠㅠ 정확힌 새벽부터 시작이었어용",
          "original_form": "네네ㅠㅠ 정확힌 새벽부터 시작이었어용",
          "speaker_id": "1",
          "time": "20210828 06:03"
        },
        {
          "id": "MDRW2100020446.1.14",
          "form": "새벽에 혼자하고 있으면 좀 무섭지 않나요",
          "original_form": "새벽에 혼자하고 있으면 좀 무섭지 않나요",
          "speaker_id": "2",
          "time": "20210828 06:03"
        },
        {
          "id": "MDRW2100020446.1.15",
          "form": "음 새벽이라 꿀알바겠다 했는데",
          "original_form": "음 새벽이라 꿀알바겠다 했는데",
          "speaker_id": "1",
          "time": "20210828 06:03"
        },
        {
          "id": "MDRW2100020446.1.16",
          "form": "사람이 계속와서 당황ㅋㅋㅋ",
          "original_form": "사람이 계속와서 당황ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 06:03"
        },
        {
          "id": "MDRW2100020446.1.17",
          "form": "맞아여ㅠㅠ",
          "original_form": "맞아여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 06:03"
        },
        {
          "id": "MDRW2100020446.1.18",
          "form": "그래서 그 경찰 부를수있는 버튼 그거",
          "original_form": "그래서 그 경찰 부를수있는 버튼 그거",
          "speaker_id": "1",
          "time": "20210828 06:04"
        },
        {
          "id": "MDRW2100020446.1.19",
          "form": "맨날 주시하고 일했어요",
          "original_form": "맨날 주시하고 일했어요",
          "speaker_id": "1",
          "time": "20210828 06:04"
        },
        {
          "id": "MDRW2100020446.1.20",
          "form": "ㅋㅋㅋ별일 없어서 다행이에요",
          "original_form": "ㅋㅋㅋ별일 없어서 다행이에요",
          "speaker_id": "2",
          "time": "20210828 06:04"
        },
        {
          "id": "MDRW2100020446.1.21",
          "form": "가끔 용돈주고 가세요",
          "original_form": "가끔 용돈주고 가세요",
          "speaker_id": "1",
          "time": "20210828 06:04"
        },
        {
          "id": "MDRW2100020446.1.22",
          "form": "몇개월전에 후배친구가 운영하는 편의점에",
          "original_form": "몇개월전에 후배친구가 운영하는 편의점에",
          "speaker_id": "2",
          "time": "20210828 06:04"
        },
        {
          "id": "MDRW2100020446.1.23",
          "form": "칼부림이 있어서",
          "original_form": "칼부림이 있어서",
          "speaker_id": "2",
          "time": "20210828 06:04"
        },
        {
          "id": "MDRW2100020446.1.24",
          "form": "너무 무섭더라구요",
          "original_form": "너무 무섭더라구요",
          "speaker_id": "2",
          "time": "20210828 06:04"
        }
      ]
    }
