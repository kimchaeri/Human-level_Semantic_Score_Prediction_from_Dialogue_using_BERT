      "utterance": [
        {
          "id": "MDRW2100002512.1.1",
          "form": "근데 너는 결혼 꼭 해야한다구 생각함??",
          "original_form": "근데 너는 결혼 꼭 해야한다구 생각함??",
          "speaker_id": "1",
          "time": "20210820 12:58"
        },
        {
          "id": "MDRW2100002512.1.2",
          "form": "나는 노노",
          "original_form": "나는 노노",
          "speaker_id": "1",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002512.1.3",
          "form": "나도 놉",
          "original_form": "나도 놉",
          "speaker_id": "2",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002512.1.4",
          "form": "걍 이제는 결혼은 선택이야",
          "original_form": "걍 이제는 결혼은 선택이야",
          "speaker_id": "2",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002512.1.5",
          "form": "굳이 해야할 필요성을 못느끼겠어",
          "original_form": "굳이 해야할 필요성을 못느끼겠어",
          "speaker_id": "2",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002512.1.6",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002512.1.7",
          "form": "솔직히 요즘 이상한 남자들 너무 많아서",
          "original_form": "솔직히 요즘 이상한 남자들 너무 많아서",
          "speaker_id": "1",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002512.1.8",
          "form": "엄마아빠도 걱정하심",
          "original_form": "엄마아빠도 걱정하심",
          "speaker_id": "1",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002512.1.9",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "2",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002512.1.10",
          "form": "뉴스만 봐도",
          "original_form": "뉴스만 봐도",
          "speaker_id": "2",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002512.1.11",
          "form": "진짜 너무 많아",
          "original_form": "진짜 너무 많아",
          "speaker_id": "2",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002512.1.12",
          "form": "글고 결혼하면 아기 낳아라 하는데",
          "original_form": "글고 결혼하면 아기 낳아라 하는데",
          "speaker_id": "2",
          "time": "20210820 12:59"
        },
        {
          "id": "MDRW2100002512.1.13",
          "form": "그것도싫어",
          "original_form": "그것도싫어",
          "speaker_id": "2",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002512.1.14",
          "form": "마자..",
          "original_form": "마자..",
          "speaker_id": "1",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002512.1.15",
          "form": "나는 꼭 애를 낳아야하나?",
          "original_form": "나는 꼭 애를 낳아야하나?",
          "speaker_id": "1",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002512.1.16",
          "form": "이생각들어",
          "original_form": "이생각들어",
          "speaker_id": "1",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002512.1.17",
          "form": "우리엄마는 우릴 낳으셨지만..",
          "original_form": "우리엄마는 우릴 낳으셨지만..",
          "speaker_id": "2",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002512.1.18",
          "form": "나도 별로 안낳고싳어",
          "original_form": "나도 별로 안낳고싳어",
          "speaker_id": "2",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002512.1.19",
          "form": "싶어..",
          "original_form": "싶어..",
          "speaker_id": "2",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002512.1.20",
          "form": "나랑 비슷하구만",
          "original_form": "나랑 비슷하구만",
          "speaker_id": "1",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002512.1.21",
          "form": "그냥 나혼자서 재밌게 잘살래",
          "original_form": "그냥 나혼자서 재밌게 잘살래",
          "speaker_id": "1",
          "time": "20210820 13:00"
        },
        {
          "id": "MDRW2100002512.1.22",
          "form": "돈만 많다면 그럴수 있을듯",
          "original_form": "돈만 많다면 그럴수 있을듯",
          "speaker_id": "1",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002512.1.23",
          "form": "인정ㅋㅋㅋㅋ",
          "original_form": "인정ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002512.1.24",
          "form": "고양이 한마리 키우면서",
          "original_form": "고양이 한마리 키우면서",
          "speaker_id": "2",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002512.1.25",
          "form": "재밌게살수있다",
          "original_form": "재밌게살수있다",
          "speaker_id": "2",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002512.1.26",
          "form": "맞아 진짜",
          "original_form": "맞아 진짜",
          "speaker_id": "1",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002512.1.27",
          "form": "고양이 키우면서 살면돼",
          "original_form": "고양이 키우면서 살면돼",
          "speaker_id": "1",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002512.1.28",
          "form": "굳이 결혼 무조건 할 필요없어",
          "original_form": "굳이 결혼 무조건 할 필요없어",
          "speaker_id": "1",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002512.1.29",
          "form": "글고 결혼할때도 돈 많이 들자너~",
          "original_form": "글고 결혼할때도 돈 많이 들자너~",
          "speaker_id": "1",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002512.1.30",
          "form": "마저",
          "original_form": "마저",
          "speaker_id": "2",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002512.1.31",
          "form": "또 신경써야할것도 많고",
          "original_form": "또 신경써야할것도 많고",
          "speaker_id": "2",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002512.1.32",
          "form": "너므너므귀찮아",
          "original_form": "너므너므귀찮아",
          "speaker_id": "2",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002512.1.33",
          "form": "맞어",
          "original_form": "맞어",
          "speaker_id": "1",
          "time": "20210820 13:01"
        },
        {
          "id": "MDRW2100002512.1.34",
          "form": "시댁도 생각해야하고",
          "original_form": "시댁도 생각해야하고",
          "speaker_id": "1",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002512.1.35",
          "form": "축의금 이런것도",
          "original_form": "축의금 이런것도",
          "speaker_id": "1",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002512.1.36",
          "form": "신혼여행 악 생각만해도 머리아프다",
          "original_form": "신혼여행 악 생각만해도 머리아프다",
          "speaker_id": "1",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002512.1.37",
          "form": "마자 근데",
          "original_form": "마자 근데",
          "speaker_id": "2",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002512.1.38",
          "form": "대부분 다 돈이랑 관련되어있네",
          "original_form": "대부분 다 돈이랑 관련되어있네",
          "speaker_id": "2",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002512.1.39",
          "form": "글게",
          "original_form": "글게",
          "speaker_id": "1",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002512.1.40",
          "form": "역시 결혼도 돈이야",
          "original_form": "역시 결혼도 돈이야",
          "speaker_id": "1",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002512.1.41",
          "form": "그냥 나는 그돈모아서",
          "original_form": "그냥 나는 그돈모아서",
          "speaker_id": "1",
          "time": "20210820 13:02"
        },
        {
          "id": "MDRW2100002512.1.42",
          "form": "맛있는거 먹으면서 살랜다",
          "original_form": "맛있는거 먹으면서 살랜다",
          "speaker_id": "1",
          "time": "20210820 13:03"
        }
      ]
    }
  ]
