      "utterance": [
        {
          "id": "MDRW2100004780.1.1",
          "form": "나 지금 할리스에서 음료 마심",
          "original_form": "나 지금 할리스에서 음료 마심",
          "speaker_id": "2",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004780.1.2",
          "form": "맛있겠네",
          "original_form": "맛있겠네",
          "speaker_id": "1",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004780.1.3",
          "form": "나도먹고싶다",
          "original_form": "나도먹고싶다",
          "speaker_id": "1",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004780.1.4",
          "form": "밀크티 라떼 마심 이거 마시러 할리스 온다",
          "original_form": "밀크티 라떼 마심 이거 마시러 할리스 온다",
          "speaker_id": "2",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004780.1.5",
          "form": "할리스는 아바라지",
          "original_form": "할리스는 아바라지",
          "speaker_id": "1",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004780.1.6",
          "form": "뭘모르네",
          "original_form": "뭘모르네",
          "speaker_id": "1",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004780.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 어이없네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 어이없네",
          "speaker_id": "2",
          "time": "20210821 18:27"
        },
        {
          "id": "MDRW2100004780.1.8",
          "form": "투썸에 로얄밀크티도 맛있는데",
          "original_form": "투썸에 로얄밀크티도 맛있는데",
          "speaker_id": "2",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004780.1.9",
          "form": "알못이네",
          "original_form": "알못이네",
          "speaker_id": "1",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004780.1.10",
          "form": "그건인정",
          "original_form": "그건인정",
          "speaker_id": "1",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004780.1.11",
          "form": "헐 근데 할리스 밀크티라떼는 왜 인정 안함?",
          "original_form": "헐 근데 할리스 밀크티라떼는 왜 인정 안함?",
          "speaker_id": "2",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004780.1.12",
          "form": "밀크티라떼는 투썸이최고지",
          "original_form": "밀크티라떼는 투썸이최고지",
          "speaker_id": "1",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004780.1.13",
          "form": "인생 그렇게 살지마라",
          "original_form": "인생 그렇게 살지마라",
          "speaker_id": "2",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004780.1.14",
          "form": "할리스는 이류다",
          "original_form": "할리스는 이류다",
          "speaker_id": "1",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004780.1.15",
          "form": "둘 다 사먹는 내가 일류임",
          "original_form": "둘 다 사먹는 내가 일류임",
          "speaker_id": "2",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004780.1.16",
          "form": "둘다사먹은 니가 삼류다",
          "original_form": "둘다사먹은 니가 삼류다",
          "speaker_id": "1",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004780.1.17",
          "form": "세계서열 0위 그놈이 바로 투썸 로얄 밀크티 라떼?",
          "original_form": "세계서열 0위 그놈이 바로 투썸 로얄 밀크티 라떼?",
          "speaker_id": "2",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004780.1.18",
          "form": "안사먹은내가 일류다",
          "original_form": "안사먹은내가 일류다",
          "speaker_id": "1",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004780.1.19",
          "form": "대박얼척없음",
          "original_form": "대박얼척없음",
          "speaker_id": "1",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004780.1.20",
          "form": "응 자기 합리화 안녕임~",
          "original_form": "응 자기 합리화 안녕임~",
          "speaker_id": "2",
          "time": "20210821 18:29"
        }
