      "utterance": [
        {
          "id": "MDRW2100021317.1.1",
          "form": "학교생활 즐거우세요?",
          "original_form": "학교생활 즐거우세요?",
          "speaker_id": "1",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100021317.1.2",
          "form": "저는 대학생이에요",
          "original_form": "저는 대학생이에요",
          "speaker_id": "1",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100021317.1.3",
          "form": "아 대학생이시구나",
          "original_form": "아 대학생이시구나",
          "speaker_id": "2",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100021317.1.4",
          "form": "학교 교직원 어떻게 생각하세요?",
          "original_form": "학교 교직원 어떻게 생각하세요?",
          "speaker_id": "2",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100021317.1.5",
          "form": "쓰레기들아닐까요?",
          "original_form": "쓰레기들아닐까요?",
          "speaker_id": "1",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100021317.1.6",
          "form": "제등록금으로",
          "original_form": "제등록금으로",
          "speaker_id": "1",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100021317.1.7",
          "form": "뭘하는지,,,",
          "original_form": "뭘하는지,,,",
          "speaker_id": "1",
          "time": "20210828 17:34"
        },
        {
          "id": "MDRW2100021317.1.8",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210828 17:35"
        },
        {
          "id": "MDRW2100021317.1.9",
          "form": "요즘 대학생들한테는 학업 스트레스보다 교직원 스트레스가 더 많다고 하네요,,",
          "original_form": "요즘 대학생들한테는 학업 스트레스보다 교직원 스트레스가 더 많다고 하네요,,",
          "speaker_id": "2",
          "time": "20210828 17:36"
        },
        {
          "id": "MDRW2100021317.1.10",
          "form": "정말 학생들이 많이 참죠,,",
          "original_form": "정말 학생들이 많이 참죠,,",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100021317.1.11",
          "form": "name2님도 대학생이시구나",
          "original_form": "&name2&님도 대학생이시구나",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100021317.1.12",
          "form": "몇학년이세요?",
          "original_form": "몇학년이세요?",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100021317.1.13",
          "form": "저 새내기예요 ㅎㅎ",
          "original_form": "저 새내기예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100021317.1.14",
          "form": "21학번이요",
          "original_form": "21학번이요",
          "speaker_id": "2",
          "time": "20210828 17:38"
        },
        {
          "id": "MDRW2100021317.1.15",
          "form": "헉 저는 20 코로나학번이에요 ㅠ",
          "original_form": "헉 저는 20 코로나학번이에요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100021317.1.16",
          "form": "아쉽겠다.. 학교 가보셨어요?",
          "original_form": "아쉽겠다.. 학교 가보셨어요?",
          "speaker_id": "2",
          "time": "20210828 17:40"
        },
        {
          "id": "MDRW2100021317.1.17",
          "form": "네! 저는 교내근로합니다!",
          "original_form": "네! 저는 교내근로합니다!",
          "speaker_id": "1",
          "time": "20210828 17:41"
        },
        {
          "id": "MDRW2100021317.1.18",
          "form": "저도 교내근로해요",
          "original_form": "저도 교내근로해요",
          "speaker_id": "2",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100021317.1.19",
          "form": "학교 사람들은 어떠신지?",
          "original_form": "학교 사람들은 어떠신지?",
          "speaker_id": "2",
          "time": "20210828 17:42"
        },
        {
          "id": "MDRW2100021317.1.20",
          "form": "같이 근로하는 친구들 아니었으면 진짜 못참았을거에요ㅠ",
          "original_form": "같이 근로하는 친구들 아니었으면 진짜 못참았을거에요ㅠ",
          "speaker_id": "1",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100021317.1.21",
          "form": "저는 근로담당자분이 좋아서 다행이었어요",
          "original_form": "저는 근로담당자분이 좋아서 다행이었어요",
          "speaker_id": "1",
          "time": "20210828 17:50"
        },
        {
          "id": "MDRW2100021317.1.22",
          "form": "어머머 제 근로담당자분도 좋았는데!!",
          "original_form": "어머머 제 근로담당자분도 좋았는데!!",
