      "utterance": [
        {
          "id": "MDRW2100021791.1.1",
          "form": "흠 저는 주로 그거만 보는데",
          "original_form": "흠 저는 주로 그거만 보는데",
          "speaker_id": "2",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100021791.1.2",
          "form": "슬의생2요",
          "original_form": "슬의생2요",
          "speaker_id": "2",
          "time": "20210828 22:33"
        },
        {
          "id": "MDRW2100021791.1.3",
          "form": "슬기로운의사생활이라구",
          "original_form": "슬기로운의사생활이라구",
          "speaker_id": "2",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021791.1.4",
          "form": "아시나요",
          "original_form": "아시나요",
          "speaker_id": "2",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021791.1.5",
          "form": "jtbc 껀데",
          "original_form": "jtbc 껀데",
          "speaker_id": "2",
          "time": "20210828 22:34"
        },
        {
          "id": "MDRW2100021791.1.6",
          "form": "티비엔거아니었어요?!",
          "original_form": "티비엔거아니었어요?!",
          "speaker_id": "1",
          "time": "20210828 22:35"
        },
        {
          "id": "MDRW2100021791.1.7",
          "form": "아맞다 티비엔이예요",
          "original_form": "아맞다 티비엔이예요",
          "speaker_id": "2",
          "time": "20210828 22:35"
        },
        {
          "id": "MDRW2100021791.1.8",
          "form": "시즌1은 봤어요!",
          "original_form": "시즌1은 봤어요!",
          "speaker_id": "1",
          "time": "20210828 22:35"
        },
        {
          "id": "MDRW2100021791.1.9",
          "form": "티빙에서 친구아이디로 봤어요",
          "original_form": "티빙에서 친구아이디로 봤어요",
          "speaker_id": "2",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021791.1.10",
          "form": "시즌1이 더 재미있긴 해요",
          "original_form": "시즌1이 더 재미있긴 해요",
          "speaker_id": "2",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021791.1.11",
          "form": "저는 끝나면 몰아보려구뇨!",
          "original_form": "저는 끝나면 몰아보려구뇨!",
          "speaker_id": "1",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021791.1.12",
          "form": "끝나가나요?",
          "original_form": "끝나가나요?",
          "speaker_id": "1",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021791.1.13",
          "form": "그리고 영드 셜록 좋아합니다",
          "original_form": "그리고 영드 셜록 좋아합니다",
          "speaker_id": "2",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021791.1.14",
          "form": "네 그런데 스포할만한 내용은 없네요",
          "original_form": "네 그런데 스포할만한 내용은 없네요",
          "speaker_id": "2",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021791.1.15",
          "form": "셜록 봤어요!",
          "original_form": "셜록 봤어요!",
          "speaker_id": "1",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021791.1.16",
          "form": "증말 잘생김을 연기하는 ..",
          "original_form": "증말 잘생김을 연기하는 ..",
          "speaker_id": "2",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100021791.1.17",
          "form": "추리물 다 좋아하는데",
          "original_form": "추리물 다 좋아하는데",
          "speaker_id": "2",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100021791.1.18",
          "form": "왓챠같은거",
          "original_form": "왓챠같은거",
          "speaker_id": "2",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100021791.1.19",
          "form": "가입된거 있으세여",
          "original_form": "가입된거 있으세여",
          "speaker_id": "2",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100021791.1.20",
          "form": "왓챠 넷플 티빙 써요!",
          "original_form": "왓챠 넷플 티빙 써요!",
          "speaker_id": "1",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100021791.1.21",
          "form": "와 대박",
          "original_form": "와 대박",
          "speaker_id": "2",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021791.1.22",
          "form": "셋다여?",
          "original_form": "셋다여?",
          "speaker_id": "2",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021791.1.23",
          "form": "네 ㅋㅋㅋ  친구들이랑 나눠씁니다 ㅋㅋ",
          "original_form": "네 ㅋㅋㅋ  친구들이랑 나눠씁니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021791.1.24",
          "form": "종이의집 보셨어요?",
          "original_form": "종이의집 보셨어요?",
          "speaker_id": "2",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021791.1.25",
          "form": "아 이해가 갑니다",
          "original_form": "아 이해가 갑니다",
          "speaker_id": "2",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021791.1.26",
          "form": "아니요 ㅜㅜ 재밌다고는 들었어요",
          "original_form": "아니요 ㅜㅜ 재밌다고는 들었어요",
          "speaker_id": "1",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021791.1.27",
          "form": "서로서로 상호보완이되니까",
          "original_form": "서로서로 상호보완이되니까",
          "speaker_id": "2",
          "time": "20210828 22:39"
        },
        {
          "id": "MDRW2100021791.1.28",
          "form": "닥터후 보셨어요?",
          "original_form": "닥터후 보셨어요?",
          "speaker_id": "1",
          "time": "20210828 22:40"
        },
        {
          "id": "MDRW2100021791.1.29",
          "form": "근데 종이의집은 좀 잔인하기도 해요",
          "original_form": "근데 종이의집은 좀 잔인하기도 해요",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021791.1.30",
          "form": "닥터후",
          "original_form": "닥터후",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021791.1.31",
          "form": "아 괴팍한 아쩌씨",
          "original_form": "아 괴팍한 아쩌씨",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021791.1.32",
          "form": "맞죠",
          "original_form": "맞죠",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021791.1.33",
          "form": "ㅋㅋㅋㅋ 전 추리물보단 닥터후쪽이 맞더라구요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 전 추리물보단 닥터후쪽이 맞더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021791.1.34",
          "form": "오 그런쪽도 잼나죠",
          "original_form": "오 그런쪽도 잼나죠",
          "speaker_id": "2",
          "time": "20210828 22:42"
        },
        {
          "id": "MDRW2100021791.1.35",
          "form": "천재 괴짜",
          "original_form": "천재 괴짜",
          "speaker_id": "2",
          "time": "20210828 22:42"
        },
        {
          "id": "MDRW2100021791.1.36",
          "form": "그런 컨텐츠는 항상 뭔가 특별하게 생각되서 보게되요",
          "original_form": "그런 컨텐츠는 항상 뭔가 특별하게 생각되서 보게되요",
          "speaker_id": "2",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021791.1.37",
          "form": "빅뱅이론 이란 미드는 어떠세요",
          "original_form": "빅뱅이론 이란 미드는 어떠세요",
          "speaker_id": "2",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021791.1.38",
          "form": "보신적 있으신가여",
          "original_form": "보신적 있으신가여",
          "speaker_id": "2",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021791.1.39",
          "form": "헐 너무 재밌게봤어요!",
          "original_form": "헐 너무 재밌게봤어요!",
          "speaker_id": "1",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021791.1.40",
          "form": "근데 뒷시즌으로 갈수록 좀 노잼이라 놨어요 ㅜ",
          "original_form": "근데 뒷시즌으로 갈수록 좀 노잼이라 놨어요 ㅜ",
          "speaker_id": "1",
          "time": "20210828 22:43"
        },
        {
          "id": "MDRW2100021791.1.41",
          "form": "맞아요 뒤로갈수록",
          "original_form": "맞아요 뒤로갈수록",
          "speaker_id": "2",
          "time": "20210828 22:47"
        },
        {
          "id": "MDRW2100021791.1.42",
          "form": "결혼하고 어쩌구 하니까 재미가 없어졌는데",
          "original_form": "결혼하고 어쩌구 하니까 재미가 없어졌는데",
          "speaker_id": "2",
          "time": "20210828 22:47"
        },
        {
          "id": "MDRW2100021791.1.43",
          "form": "쉘든어머니가 그러시자나요",
          "original_form": "쉘든어머니가 그러시자나요",
          "speaker_id": "2",
          "time": "20210828 22:47"
        },
        {
          "id": "MDRW2100021791.1.44",
          "form": "세상은 잘난척하는 사람보다 잘난사람을 더 재수없어 한단다",
          "original_form": "세상은 잘난척하는 사람보다 잘난사람을 더 재수없어 한단다",
          "speaker_id": "2",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021791.1.45",
          "form": "진짜 우꼈어요",
          "original_form": "진짜 우꼈어요",
          "speaker_id": "2",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021791.1.46",
          "form": "ㅋㅋㅋㅋㅋㅋ 똑똑똑 페니! 이거 기억나요 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 똑똑똑 페니! 이거 기억나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 22:48"
        },
        {
          "id": "MDRW2100021791.1.47",
          "form": "아진짜 그런 친구가 있으면 어떨까요",
          "original_form": "아진짜 그런 친구가 있으면 어떨까요",
          "speaker_id": "2",
          "time": "20210828 22:51"
        }
      ]
