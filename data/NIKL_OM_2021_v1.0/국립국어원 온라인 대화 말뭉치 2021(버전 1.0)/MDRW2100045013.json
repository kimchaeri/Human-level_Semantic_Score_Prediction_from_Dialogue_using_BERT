      "utterance": [
        {
          "id": "MDRW2100045013.1.1",
          "form": "일 하시나요?",
          "original_form": "일 하시나요?",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045013.1.2",
          "form": "직장 소중해요",
          "original_form": "직장 소중해요",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100045013.1.3",
          "form": "월급 예금 소중ㅋㅋ",
          "original_form": "월급 예금 소중ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100045013.1.4",
          "form": "직장이 복무는 자유롭고요?",
          "original_form": "직장이 복무는 자유롭고요?",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100045013.1.5",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045013.1.6",
          "form": "일하시나요?",
          "original_form": "일하시나요?",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045013.1.7",
          "form": "좋네요~  네 저도 일해요",
          "original_form": "좋네요~  네 저도 일해요",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100045013.1.8",
          "form": "내일도??ㅜ",
          "original_form": "내일도??ㅜ",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100045013.1.9",
          "form": "네.... 내일은 출장입니다",
          "original_form": "네.... 내일은 출장입니다",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100045013.1.10",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045013.1.11",
          "form": "ㅜㅠ",
          "original_form": "ㅜㅠ",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045013.1.12",
          "form": "자가운전자세요",
          "original_form": "자가운전자세요",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045013.1.13",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100045013.1.14",
          "form": "아뇨 기사 있어요 ㅋㅋㅋ",
          "original_form": "아뇨 기사 있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100045013.1.15",
          "form": "그럼 굿!",
          "original_form": "그럼 굿!",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045013.1.16",
          "form": "날밤을 심싱이와 함께ㅋㅋ",
          "original_form": "날밤을 심싱이와 함께ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045013.1.17",
          "form": "어떤 직종이세요?",
          "original_form": "어떤 직종이세요?",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100045013.1.18",
          "form": "이동하는 동안 좀 자려고요 ㅋ 제 직종이 서비스직도 아닌것이 사무직도 아닌것이 전문직도 아닌 애매한 직종입니다 ㅎㅎ",
          "original_form": "이동하는 동안 좀 자려고요 ㅋ 제 직종이 서비스직도 아닌것이 사무직도 아닌것이 전문직도 아닌 애매한 직종입니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045013.1.19",
          "form": "왠지 멋진 직업같네요",
          "original_form": "왠지 멋진 직업같네요",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100045013.1.20",
          "form": "전 평범",
