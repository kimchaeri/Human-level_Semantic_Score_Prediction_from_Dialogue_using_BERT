      "utterance": [
        {
          "id": "MDRW2100014310.1.1",
          "form": "반려동물이 있으신가요?",
          "original_form": "반려동물이 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 18:19"
        },
        {
          "id": "MDRW2100014310.1.2",
          "form": "네 저는 고양이 4마리랑 살고있어요!",
          "original_form": "네 저는 고양이 4마리랑 살고있어요!",
          "speaker_id": "2",
          "time": "20210825 18:20"
        },
        {
          "id": "MDRW2100014310.1.3",
          "form": "name1은 반려동물 있으신가요?",
          "original_form": "&name1&은 반려동물 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014310.1.4",
          "form": "와 아주 좋으실 것 같아요",
          "original_form": "와 아주 좋으실 것 같아요",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014310.1.5",
          "form": "저는 반려동물과 함께하고 싶은데 여건이 안되서 준비중이에요",
          "original_form": "저는 반려동물과 함께하고 싶은데 여건이 안되서 준비중이에요",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014310.1.6",
          "form": "혹시 이름이 뭔가요?",
          "original_form": "혹시 이름이 뭔가요?",
          "speaker_id": "1",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014310.1.7",
          "form": "아 아이들 이름은 name3, name4, name5, name6에요ㅋㅋ",
          "original_form": "아 아이들 이름은 &name3&, &name4&, &name5&, &name6&에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014310.1.8",
          "form": "이름에 규칙성이 보이시죠ㅎㄹ",
          "original_form": "이름에 규칙성이 보이시죠ㅎㄹ",
          "speaker_id": "2",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014310.1.9",
          "form": "두글자 중복이네요",
          "original_form": "두글자 중복이네요",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014310.1.10",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014310.1.11",
          "form": "치즈냥이도 있나요?",
          "original_form": "치즈냥이도 있나요?",
          "speaker_id": "1",
          "time": "20210825 18:23"
        },
        {
          "id": "MDRW2100014310.1.12",
          "form": "네 두마리가 치즈에요ㅎㅎ",
          "original_form": "네 두마리가 치즈에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014310.1.13",
          "form": "오 친구네 냥이가 치즈라서 얘기해봤는데",
          "original_form": "오 친구네 냥이가 치즈라서 얘기해봤는데",
          "speaker_id": "1",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014310.1.14",
          "form": "한마리는 발이랑 배 빼고 전체 치즈 한마리는 거의 흰색에 머리랑 등 꼬리만 치즈에요ㅎㅎ",
          "original_form": "한마리는 발이랑 배 빼고 전체 치즈 한마리는 거의 흰색에 머리랑 등 꼬리만 치즈에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014310.1.15",
          "form": "치즈가 아주 귀엽더라구오",
          "original_form": "치즈가 아주 귀엽더라구오",
          "speaker_id": "1",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014310.1.16",
          "form": "흰색에 슬쩍 치즈라니 더 귀엽네요",
          "original_form": "흰색에 슬쩍 치즈라니 더 귀엽네요",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014310.1.17",
          "form": "헷갈리지 않게 치즈가 딱 구분되겠어요",
          "original_form": "헷갈리지 않게 치즈가 딱 구분되겠어요",
          "speaker_id": "1",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014310.1.18",
          "form": "치즈 귀엽죠ㅎㅎ 치즈가 붙임성이 좋다던데 저희집 아이 하나는 겁이 너무 많아서 아직 안는것도 쉽지 않아요ㅜㅜ",
          "original_form": "치즈 귀엽죠ㅎㅎ 치즈가 붙임성이 좋다던데 저희집 아이 하나는 겁이 너무 많아서 아직 안는것도 쉽지 않아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 18:26"
        },
        {
          "id": "MDRW2100014310.1.19",
          "form": "아구",
          "original_form": "아구",
          "speaker_id": "1",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014310.1.20",
          "form": "겁쟁이 치즈라니 귀엽긴한데 어서 자라면 좋겠네요",
          "original_form": "겁쟁이 치즈라니 귀엽긴한데 어서 자라면 좋겠네요",
          "speaker_id": "1",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014310.1.21",
          "form": "친구네 치즈냥이는 무서운 편이라 으르신 같아요",
          "original_form": "친구네 치즈냥이는 무서운 편이라 으르신 같아요",
          "speaker_id": "1",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014310.1.22",
          "form": "ㅎㅎㅎ 저희집도 첫째 고등어는 아주 무서운 할배에요ㅎㅎ",
          "original_form": "ㅎㅎㅎ 저희집도 첫째 고등어는 아주 무서운 할배에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014310.1.23",
          "form": "할배라니 ㅎㅎ 귀여운 호칭이네요",
          "original_form": "할배라니 ㅎㅎ 귀여운 호칭이네요",
          "speaker_id": "1",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014310.1.24",
          "form": "그럼 마지막 냥이는 어떤 종인가요?",
          "original_form": "그럼 마지막 냥이는 어떤 종인가요?",
          "speaker_id": "1",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014310.1.25",
          "form": "그 아이는 회색인데 전체회색이 아니고 턱시도같이 입고있어요ㅎㅎ",
          "original_form": "그 아이는 회색인데 전체회색이 아니고 턱시도같이 입고있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100014310.1.26",
          "form": "턱시도!",
          "original_form": "턱시도!",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014310.1.27",
          "form": "길에서 살던 아인데 제일 순하고 붙임성도 좋아요",
          "original_form": "길에서 살던 아인데 제일 순하고 붙임성도 좋아요",
          "speaker_id": "2",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014310.1.28",
          "form": "양말도 신었나요?",
          "original_form": "양말도 신었나요?",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014310.1.29",
          "form": "스트릿 출신에 아무래도 천사가 많다고 들었어요",
          "original_form": "스트릿 출신에 아무래도 천사가 많다고 들었어요",
          "speaker_id": "1",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014310.1.30",
          "form": "네 흰양말 신었어요ㅎㅎ 뒷발은 긴양말ㅎㅎ",
          "original_form": "네 흰양말 신었어요ㅎㅎ 뒷발은 긴양말ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014310.1.31",
          "form": "앗 양말 짝짝이라니",
          "original_form": "앗 양말 짝짝이라니",
          "speaker_id": "1",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014310.1.32",
          "form": "ㅎㅎ 귀여워요",
          "original_form": "ㅎㅎ 귀여워요",
          "speaker_id": "1",
          "time": "20210825 18:36"
        }
      ]
