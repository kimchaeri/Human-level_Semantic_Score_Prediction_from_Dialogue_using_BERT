      "utterance": [
        {
          "id": "MDRW2100020674.1.1",
          "form": "운동 좋아하세요?",
          "original_form": "운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020674.1.2",
          "form": "운동 좋아하진 않고 살려고 해요",
          "original_form": "운동 좋아하진 않고 살려고 해요",
          "speaker_id": "2",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020674.1.3",
          "form": "요즘은 쉬는중이네요 더워서",
          "original_form": "요즘은 쉬는중이네요 더워서",
          "speaker_id": "2",
          "time": "20210828 11:03"
        },
        {
          "id": "MDRW2100020674.1.4",
          "form": "저랑 똑같네요 ㅋㅋㅋㅋ",
          "original_form": "저랑 똑같네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020674.1.5",
          "form": "저 한 2일 전부터 다시 시작했어요 ㅎㅎ",
          "original_form": "저 한 2일 전부터 다시 시작했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020674.1.6",
          "form": "필라테스로 가볍게~",
          "original_form": "필라테스로 가볍게~",
          "speaker_id": "1",
          "time": "20210828 11:05"
        },
        {
          "id": "MDRW2100020674.1.7",
          "form": "저도 이제 다시 하려구요 ㅋㅋ 필라테스 와우 그거 몸  만드는데 좋아보이더라구요",
          "original_form": "저도 이제 다시 하려구요 ㅋㅋ 필라테스 와우 그거 몸  만드는데 좋아보이더라구요",
          "speaker_id": "2",
          "time": "20210828 11:06"
        },
        {
          "id": "MDRW2100020674.1.8",
          "form": "자세교정 군살정리 이런거요~",
          "original_form": "자세교정 군살정리 이런거요~",
          "speaker_id": "2",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020674.1.9",
          "form": "오 ㅋㅋㅋ 사실 저 잘은 몰라요",
          "original_form": "오 ㅋㅋㅋ 사실 저 잘은 몰라요",
          "speaker_id": "1",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020674.1.10",
          "form": "그냥 폼블러랑 요가링 받은게 있어서 쓰는거에요",
          "original_form": "그냥 폼블러랑 요가링 받은게 있어서 쓰는거에요",
          "speaker_id": "1",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020674.1.11",
          "form": "아 그렇구나 전 그런 자세운동은 못 하겠더라구요 유연성이랑 균형감각이 안 좋은가봐요",
          "original_form": "아 그렇구나 전 그런 자세운동은 못 하겠더라구요 유연성이랑 균형감각이 안 좋은가봐요",
          "speaker_id": "2",
          "time": "20210828 11:10"
        },
        {
          "id": "MDRW2100020674.1.12",
          "form": "그 막 어디에 매달리고 하는 그런 거요?",
          "original_form": "그 막 어디에 매달리고 하는 그런 거요?",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020674.1.13",
          "form": "아뇨 요가정도도 안되서요 ㅎㅎ",
          "original_form": "아뇨 요가정도도 안되서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020674.1.14",
          "form": "ㅋㅋㅋ아 저도 비슷해요....",
          "original_form": "ㅋㅋㅋ아 저도 비슷해요....",
          "speaker_id": "1",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020674.1.15",
          "form": "그런거 하면 오히려 허리가 더 아프더라구요 ㅜㅜ",
          "original_form": "그런거 하면 오히려 허리가 더 아프더라구요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020674.1.16",
          "form": "네네 맞아요 ㅜㅜ",
          "original_form": "네네 맞아요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020674.1.17",
          "form": "저는 집에서 실내자전거나 타네요 ㅎㅎ",
          "original_form": "저는 집에서 실내자전거나 타네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 11:15"
        },
        {
          "id": "MDRW2100020674.1.18",
          "form": "이거 인터발 하면 땀 엄청나더라구요",
          "original_form": "이거 인터발 하면 땀 엄청나더라구요",
          "speaker_id": "2",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020674.1.19",
          "form": "아 집에서 인터벌 트레이닝 하시는구나~",
          "original_form": "아 집에서 인터벌 트레이닝 하시는구나~",
          "speaker_id": "1",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020674.1.20",
          "form": "저도 밖에서 달리기할 때 하는데 좋더라구요!",
          "original_form": "저도 밖에서 달리기할 때 하는데 좋더라구요!",
          "speaker_id": "1",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020674.1.21",
          "form": "네네 요즘 런닝 많이하시더라구요!",
          "original_form": "네네 요즘 런닝 많이하시더라구요!",
          "speaker_id": "2",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020674.1.22",
          "form": "런데이 어플 쓰시나요?",
          "original_form": "런데이 어플 쓰시나요?",
          "speaker_id": "2",
          "time": "20210828 11:18"
        },
        {
          "id": "MDRW2100020674.1.23",
          "form": "런데이? 처음 들어 봤어요",
          "original_form": "런데이? 처음 들어 봤어요",
          "speaker_id": "1",
          "time": "20210828 11:19"
        },
        {
          "id": "MDRW2100020674.1.24",
          "form": "어떤 기능이 있는데용?",
          "original_form": "어떤 기능이 있는데용?",
          "speaker_id": "1",
          "time": "20210828 11:19"
        },
        {
          "id": "MDRW2100020674.1.25",
          "form": "아 저도  써보진 않았는데 그거 쓰면 인터발 달리기 하기 편한가보더라구요",
          "original_form": "아 저도  써보진 않았는데 그거 쓰면 인터발 달리기 하기 편한가보더라구요",
          "speaker_id": "2",
          "time": "20210828 11:19"
        },
        {
          "id": "MDRW2100020674.1.26",
          "form": "그거 많이들 하시더라구요 다이어트카페 가 보면",
          "original_form": "그거 많이들 하시더라구요 다이어트카페 가 보면",
          "speaker_id": "2",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020674.1.27",
          "form": "그런 게 있는 줄 몰랐는데 한번 써봐야겠어요!",
          "original_form": "그런 게 있는 줄 몰랐는데 한번 써봐야겠어요!",
          "speaker_id": "1",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020674.1.28",
          "form": "네네 한번 해보시면 좋을것 같아요",
          "original_form": "네네 한번 해보시면 좋을것 같아요",
          "speaker_id": "2",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020674.1.29",
          "form": "저 그냥 한바퀴는 빨리 걷고",
          "original_form": "저 그냥 한바퀴는 빨리 걷고",
          "speaker_id": "1",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020674.1.30",
          "form": "그다음 한바퀴는 빨리 달리고",
          "original_form": "그다음 한바퀴는 빨리 달리고",
          "speaker_id": "1",
          "time": "20210828 11:21"
        },
        {
          "id": "MDRW2100020674.1.31",
          "form": "이 싸이클만 반복했네용",
          "original_form": "이 싸이클만 반복했네용",
          "speaker_id": "1",
          "time": "20210828 11:21"
        },
        {
          "id": "MDRW2100020674.1.32",
          "form": "오 그것도 도움되지 않았을까요? 인터발은 보통 20초 전속력 1분 20초 보통속도 이런 느낌이더라구요",
          "original_form": "오 그것도 도움되지 않았을까요? 인터발은 보통 20초 전속력 1분 20초 보통속도 이런 느낌이더라구요",
          "speaker_id": "2",
          "time": "20210828 11:22"
        }
      ]
    }
