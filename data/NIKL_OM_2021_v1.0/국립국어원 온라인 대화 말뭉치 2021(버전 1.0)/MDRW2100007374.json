      "utterance": [
        {
          "id": "MDRW2100007374.1.1",
          "form": "혹시 치즈고양인가요",
          "original_form": "혹시 치즈고양인가요",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007374.1.2",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007374.1.3",
          "form": "치즈고양이에요",
          "original_form": "치즈고양이에요",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007374.1.4",
          "form": "처음엔 좀 못생겼다 했는데 크니까 예쁘더라구요",
          "original_form": "처음엔 좀 못생겼다 했는데 크니까 예쁘더라구요",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007374.1.5",
          "form": "하 여기 사진이 안보내지는게 한이네요",
          "original_form": "하 여기 사진이 안보내지는게 한이네요",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007374.1.6",
          "form": "감자야 이모다.",
          "original_form": "감자야 이모다.",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007374.1.7",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007374.1.8",
          "form": "네 이모",
          "original_form": "네 이모",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007374.1.9",
          "form": "추르부탁해요",
          "original_form": "추르부탁해요",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007374.1.10",
          "form": "아아아악",
          "original_form": "아아아악",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007374.1.11",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007374.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007374.1.13",
          "form": "추르만 원하니 감자야",
          "original_form": "추르만 원하니 감자야",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007374.1.14",
          "form": "근데 질문이 있어요",
          "original_form": "근데 질문이 있어요",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007374.1.15",
          "form": "네 어떤게 궁금하세요",
          "original_form": "네 어떤게 궁금하세요",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007374.1.16",
          "form": "고양이를 쓰다듬으면",
          "original_form": "고양이를 쓰다듬으면",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007374.1.17",
          "form": "스트레스가",
          "original_form": "스트레스가",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007374.1.18",
          "form": "풀리나요",
          "original_form": "풀리나요",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007374.1.19",
          "form": "마음에 안정이 옵니다",
          "original_form": "마음에 안정이 옵니다",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007374.1.20",
          "form": "name3이는 저를 쪼았어요",
          "original_form": "&name3&이는 저를 쪼았어요",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007374.1.21",
          "form": "역시 고양이가 답인가",
          "original_form": "역시 고양이가 답인가",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007374.1.22",
          "form": "골골송 불러주면",
          "original_form": "골골송 불러주면",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007374.1.23",
          "form": "아주 좋아요",
          "original_form": "아주 좋아요",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007374.1.24",
          "form": "진짜 부럽다",
          "original_form": "진짜 부럽다",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007374.1.25",
          "form": "저런 주인을 쪼다니",
          "original_form": "저런 주인을 쪼다니",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007374.1.26",
          "form": "자랑 더 해줘요",
          "original_form": "자랑 더 해줘요",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007374.1.27",
          "form": "우리 name3이는",
          "original_form": "우리 &name3&이는",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007374.1.28",
          "form": "웃으면 잇몸을 쪼아요",
          "original_form": "웃으면 잇몸을 쪼아요",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007374.1.29",
          "form": "전생에 악어새였던게 아닐지",
          "original_form": "전생에 악어새였던게 아닐지",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007374.1.30",
          "form": "^^...",
          "original_form": "^^...",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007374.1.31",
          "form": "그리고 똥을 아무데나싸요",
          "original_form": "그리고 똥을 아무데나싸요",
          "speaker_id": "1",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007374.1.32",
