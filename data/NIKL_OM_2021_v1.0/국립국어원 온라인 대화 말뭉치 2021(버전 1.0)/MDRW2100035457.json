      "utterance": [
        {
          "id": "MDRW2100035457.1.1",
          "form": "그래도 가족이니ㅠ",
          "original_form": "그래도 가족이니ㅠ",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100035457.1.2",
          "form": "오만정이 다떨어지네요",
          "original_form": "오만정이 다떨어지네요",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100035457.1.3",
          "form": "제편들어주는 사람은 없어요",
          "original_form": "제편들어주는 사람은 없어요",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100035457.1.4",
          "form": "다 제가 이상하다 생각해요",
          "original_form": "다 제가 이상하다 생각해요",
          "speaker_id": "1",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100035457.1.5",
          "form": "ㅋㅋㅋ. 이런ㅋㅋ구성원이 어떻게 되세요?",
          "original_form": "ㅋㅋㅋ. 이런ㅋㅋ구성원이 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210829 17:43"
        },
        {
          "id": "MDRW2100035457.1.6",
          "form": "친척들까지 합쳐서 말하는거에요",
          "original_form": "친척들까지 합쳐서 말하는거에요",
          "speaker_id": "1",
          "time": "20210829 17:44"
        },
        {
          "id": "MDRW2100035457.1.7",
          "form": "아ㅠㅠ이런 어른들끼리의 동맹이신가",
          "original_form": "아ㅠㅠ이런 어른들끼리의 동맹이신가",
          "speaker_id": "2",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100035457.1.8",
          "form": "어머님이 하소연을",
          "original_form": "어머님이 하소연을",
          "speaker_id": "2",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100035457.1.9",
          "form": "그쪽으로 많이 하시나봐요ㅡ엄마의 입장에서",
          "original_form": "그쪽으로 많이 하시나봐요ㅡ엄마의 입장에서",
          "speaker_id": "2",
          "time": "20210829 17:45"
        },
        {
          "id": "MDRW2100035457.1.10",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100035457.1.11",
          "form": "엄마 입장에서",
          "original_form": "엄마 입장에서",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100035457.1.12",
          "form": "그걸 다 거기 감정이입하니 제가 나쁜사람되죠",
          "original_form": "그걸 다 거기 감정이입하니 제가 나쁜사람되죠",
          "speaker_id": "1",
          "time": "20210829 17:46"
        },
        {
          "id": "MDRW2100035457.1.13",
          "form": "토닥토닥ㅠㅠ",
          "original_form": "토닥토닥ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 23:10"
        },
        {
          "id": "MDRW2100035457.1.14",
          "form": "저도 결혼하고 따로 떨어져지내도 서운한거 있더라구요, 가까이. 사는 언니랑 차별하고ㅠ그럴때 되게 속상하더라구요 가족이라도",
          "original_form": "저도 결혼하고 따로 떨어져지내도 서운한거 있더라구요, 가까이. 사는 언니랑 차별하고ㅠ그럴때 되게 속상하더라구요 가족이라도",
          "speaker_id": "2",
          "time": "20210829 23:11"
        },
        {
          "id": "MDRW2100035457.1.15",
          "form": "에구... 네 가족이니 더 서운한거 같아요",
          "original_form": "에구... 네 가족이니 더 서운한거 같아요",
          "speaker_id": "1",
          "time": "20210830 00:18"
        },
        {
          "id": "MDRW2100035457.1.16",
          "form": "가족끼리 알게모르게 주고받는 상처가 많대요",
          "original_form": "가족끼리 알게모르게 주고받는 상처가 많대요",
          "speaker_id": "1",
          "time": "20210830 00:19"
        },
        {
          "id": "MDRW2100035457.1.17",
          "form": "맞아요 진짜 가까운 사이일수록 더 챙겨주고 잘해줘야 되는데 그게 안되더라구요",
          "original_form": "맞아요 진짜 가까운 사이일수록 더 챙겨주고 잘해줘야 되는데 그게 안되더라구요",
          "speaker_id": "2",
          "time": "20210830 00:36"
        },
        {
          "id": "MDRW2100035457.1.18",
          "form": "그죠 가까우면 더 잘해줘야하지 않나요?",
          "original_form": "그죠 가까우면 더 잘해줘야하지 않나요?",
          "speaker_id": "1",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100035457.1.19",
          "form": "전에 외할머니한테 그냥 남처럼 대해달라고 말한 적까지 있어요 ㅋㅋ",
          "original_form": "전에 외할머니한테 그냥 남처럼 대해달라고 말한 적까지 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100035457.1.20",
          "form": "남한테 되게 잘해주고 ㅋㅋ",
          "original_form": "남한테 되게 잘해주고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100035457.1.21",
          "form": "남한테 예의갖추는 그 태도로 저한테 하시는게 나을거같아서요 ㅋㅋㅋ",
          "original_form": "남한테 예의갖추는 그 태도로 저한테 하시는게 나을거같아서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100035457.1.22",
          "form": "근데 안되시죠??",
          "original_form": "근데 안되시죠??",
          "speaker_id": "2",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100035457.1.23",
          "form": "되려 버럭하셨을듯 ㅠㅠ",
          "original_form": "되려 버럭하셨을듯 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100035457.1.24",
          "form": "네 저를 고쳐서 똑바로 바꿔야한다 이런 마인드시라;;",
          "original_form": "네 저를 고쳐서 똑바로 바꿔야한다 이런 마인드시라;;",
          "speaker_id": "1",
          "time": "20210830 00:39"
        },
        {
          "id": "MDRW2100035457.1.25",
          "form": "엄마보다 외할머니가 더 ㅎㅎ 막강하실듯 ㅠㅠ",
          "original_form": "엄마보다 외할머니가 더 ㅎㅎ 막강하실듯 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 00:40"
        }
      ]
