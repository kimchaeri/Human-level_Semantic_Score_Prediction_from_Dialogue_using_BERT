      "utterance": [
        {
          "id": "MDRW2100029968.1.1",
          "form": "저 취업하기싫어요",
          "original_form": "저 취업하기싫어요",
          "speaker_id": "1",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100029968.1.2",
          "form": "저 뭐해먹고 살아야할까요",
          "original_form": "저 뭐해먹고 살아야할까요",
          "speaker_id": "2",
          "time": "20210903 17:25"
        },
        {
          "id": "MDRW2100029968.1.3",
          "form": "현실얘기멈춰",
          "original_form": "현실얘기멈춰",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029968.1.4",
          "form": "대학원가셔야지..",
          "original_form": "대학원가셔야지..",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029968.1.5",
          "form": "대학원..",
          "original_form": "대학원..",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029968.1.6",
          "form": "조랑말님은 아마",
          "original_form": "조랑말님은 아마",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029968.1.7",
          "form": "대학원에 가면 무얼하느냐.",
          "original_form": "대학원에 가면 무얼하느냐.",
          "speaker_id": "2",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029968.1.8",
          "form": "세계를?구하지않을까요?",
          "original_form": "세계를?구하지않을까요?",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029968.1.9",
          "form": "저는 전공이 체인이라ㅜ",
          "original_form": "저는 전공이 체인이라ㅜ",
          "speaker_id": "1",
          "time": "20210903 17:26"
        },
        {
          "id": "MDRW2100029968.1.10",
          "form": "평타는 치는데 좀 좋은곳은 못 가거든요ㅜㅜ 조랑말님은 어떠세요?",
          "original_form": "평타는 치는데 좀 좋은곳은 못 가거든요ㅜㅜ 조랑말님은 어떠세요?",
          "speaker_id": "1",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029968.1.11",
          "form": "아...",
          "original_form": "아...",
          "speaker_id": "2",
          "time": "20210903 17:27"
        },
        {
          "id": "MDRW2100029968.1.12",
          "form": "저는 전공이 마법이라",
          "original_form": "저는 전공이 마법이라",
          "speaker_id": "2",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029968.1.13",
          "form": "근데 마법도 한계치가 있어서.. 제가 목표한 곳으로 가려면",
          "original_form": "근데 마법도 한계치가 있어서.. 제가 목표한 곳으로 가려면",
          "speaker_id": "2",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029968.1.14",
          "form": "전공을 바꿔야될거같은데",
          "original_form": "전공을 바꿔야될거같은데",
          "speaker_id": "2",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029968.1.15",
          "form": "고민이예요...",
          "original_form": "고민이예요...",
          "speaker_id": "2",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029968.1.16",
          "form": "고민이에요\r\n...",
          "original_form": "고민이에요\r\n...",
          "speaker_id": "2",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029968.1.17",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029968.1.18",
          "form": "그래도..마법에..부전공으로 체인있으면..괜찮지않나요..",
          "original_form": "그래도..마법에..부전공으로 체인있으면..괜찮지않나요..",
          "speaker_id": "1",
          "time": "20210903 17:28"
        },
        {
          "id": "MDRW2100029968.1.19",
          "form": "그런가요. ..",
          "original_form": "그런가요. ..",
          "speaker_id": "2",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029968.1.20",
          "form": "어. .그런 저희둘이",
          "original_form": "어. .그런 저희둘이",
          "speaker_id": "2",
          "time": "20210903 17:29"
        },
        {
          "id": "MDRW2100029968.1.21",
