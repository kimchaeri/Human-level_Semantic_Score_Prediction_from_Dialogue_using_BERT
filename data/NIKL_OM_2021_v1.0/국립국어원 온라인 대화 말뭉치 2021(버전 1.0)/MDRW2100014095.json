      "utterance": [
        {
          "id": "MDRW2100014095.1.1",
          "form": "여행지에서도",
          "original_form": "여행지에서도",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014095.1.2",
          "form": "투덜투덜 하는 사람ㅡㅡ",
          "original_form": "투덜투덜 하는 사람ㅡㅡ",
          "speaker_id": "1",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014095.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014095.1.4",
          "form": "맞아요 손절입니다",
          "original_form": "맞아요 손절입니다",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014095.1.5",
          "form": "남편이라도 영서못해요",
          "original_form": "남편이라도 영서못해요",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014095.1.6",
          "form": "전 다 잘먹어요 ㅋㅋ불평제로",
          "original_form": "전 다 잘먹어요 ㅋㅋ불평제로",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014095.1.7",
          "form": "용서 못해요",
          "original_form": "용서 못해요",
          "speaker_id": "2",
          "time": "20210825 17:14"
        },
        {
          "id": "MDRW2100014095.1.8",
          "form": "ㅋㅋ 여행에서도 화목하시겠어요ㅋㅋ",
          "original_form": "ㅋㅋ 여행에서도 화목하시겠어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014095.1.9",
          "form": "여행가고싶네요",
          "original_form": "여행가고싶네요",
          "speaker_id": "1",
          "time": "20210825 17:15"
        },
        {
          "id": "MDRW2100014095.1.10",
          "form": "ㅋㅋ여행에서 음식만 화목해요 ㅋㅋ",
          "original_form": "ㅋㅋ여행에서 음식만 화목해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014095.1.11",
          "form": "전 저녁 불고기에 상추 할래요",
          "original_form": "전 저녁 불고기에 상추 할래요",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014095.1.12",
          "form": "시켜 먹고 싶긴한데",
          "original_form": "시켜 먹고 싶긴한데",
          "speaker_id": "2",
          "time": "20210825 17:16"
        },
        {
          "id": "MDRW2100014095.1.13",
          "form": "요새 넘 배달에 의존해서 밥먹겠습니다",
          "original_form": "요새 넘 배달에 의존해서 밥먹겠습니다",
          "speaker_id": "2",
          "time": "20210825 17:17"
        },
        {
          "id": "MDRW2100014095.1.14",
          "form": "네 요즘은 여행지에서도 배달을 하는 시대라던데ㅋㅋ",
          "original_form": "네 요즘은 여행지에서도 배달을 하는 시대라던데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 17:18"
        },
        {
          "id": "MDRW2100014095.1.15",
          "form": "맞아요 앉아서 공뤈에 있음오구요",
          "original_form": "맞아요 앉아서 공뤈에 있음오구요",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014095.1.16",
          "form": "공원",
          "original_form": "공원",
          "speaker_id": "2",
          "time": "20210825 17:19"
        },
        {
          "id": "MDRW2100014095.1.17",
          "form": "추천하는 관광지가 있으세요?",
          "original_form": "추천하는 관광지가 있으세요?",
          "speaker_id": "1",
          "time": "20210825 17:20"
        },
        {
          "id": "MDRW2100014095.1.18",
          "form": "전 이상온뒤로",
          "original_form": "전 이상온뒤로",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014095.1.19",
          "form": "주변에 절과 산이많아서",
          "original_form": "주변에 절과 산이많아서",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014095.1.20",
          "form": "절구경갔다 산채정식 먹는 코스 잘가요 ㅎㅎ",
          "original_form": "절구경갔다 산채정식 먹는 코스 잘가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:26"
        },
        {
          "id": "MDRW2100014095.1.21",
          "form": "저저번주는 월악산 갔다 왔어요",
          "original_form": "저저번주는 월악산 갔다 왔어요",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014095.1.22",
          "form": "딱히 코로나로 정하고 가진 않아요",
          "original_form": "딱히 코로나로 정하고 가진 않아요",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014095.1.23",
          "form": "어디 가셨어요?",
          "original_form": "어디 가셨어요?",
          "speaker_id": "2",
          "time": "20210825 17:27"
        },
        {
          "id": "MDRW2100014095.1.24",
          "form": "와 별로 못다녀서 코스 추천받고 싶네요",
          "original_form": "와 별로 못다녀서 코스 추천받고 싶네요",
          "speaker_id": "1",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014095.1.25",
          "form": "경기도로 이사를 가신거구나",
          "original_form": "경기도로 이사를 가신거구나",
          "speaker_id": "1",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014095.1.26",
          "form": "아뇨 경기도서 지방으로요",
          "original_form": "아뇨 경기도서 지방으로요",
          "speaker_id": "2",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014095.1.27",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014095.1.28",
          "form": "아 놀기는 여기 좋은것 같아요 ㅎㅎ",
          "original_form": "아 놀기는 여기 좋은것 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014095.1.29",
          "form": "와 삶의 질은 오히려 좋지 않을까요?",
          "original_form": "와 삶의 질은 오히려 좋지 않을까요?",
          "speaker_id": "1",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100014095.1.30",
          "form": "그럼요 좋아요",
          "original_form": "그럼요 좋아요",
          "speaker_id": "2",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100014095.1.31",
          "form": "절과 산이 많다니!!",
          "original_form": "절과 산이 많다니!!",
          "speaker_id": "1",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100014095.1.32",
          "form": "코로나 영향 덜받고",
          "original_form": "코로나 영향 덜받고",
          "speaker_id": "2",
          "time": "20210825 19:10"
        }
      ]
