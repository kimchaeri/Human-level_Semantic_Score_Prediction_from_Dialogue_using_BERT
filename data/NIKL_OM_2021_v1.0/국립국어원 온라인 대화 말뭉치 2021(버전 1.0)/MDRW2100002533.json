      "utterance": [
        {
          "id": "MDRW2100002533.1.1",
          "form": "공연 보러가기 어려운 상황이네요ㅠ",
          "original_form": "공연 보러가기 어려운 상황이네요ㅠ",
          "speaker_id": "1",
          "time": "20210820 13:18"
        },
        {
          "id": "MDRW2100002533.1.2",
          "form": "그쵸 코로나때문에ㅠ",
          "original_form": "그쵸 코로나때문에ㅠ",
          "speaker_id": "2",
          "time": "20210820 13:19"
        },
        {
          "id": "MDRW2100002533.1.3",
          "form": "영화는 가끔 보러가는데",
          "original_form": "영화는 가끔 보러가는데",
          "speaker_id": "2",
          "time": "20210820 13:19"
        },
        {
          "id": "MDRW2100002533.1.4",
          "form": "공연은 코로나후에 한번도 못본거같아요",
          "original_form": "공연은 코로나후에 한번도 못본거같아요",
          "speaker_id": "2",
          "time": "20210820 13:19"
        },
        {
          "id": "MDRW2100002533.1.5",
          "form": "영화관은 어때요? 좌석도 거리두기인가요?",
          "original_form": "영화관은 어때요? 좌석도 거리두기인가요?",
          "speaker_id": "1",
          "time": "20210820 13:20"
        },
        {
          "id": "MDRW2100002533.1.6",
          "form": "그쵸 거리두기 하는데",
          "original_form": "그쵸 거리두기 하는데",
          "speaker_id": "2",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002533.1.7",
          "form": "기준을 잘 모르겠더라구요",
          "original_form": "기준을 잘 모르겠더라구요",
          "speaker_id": "2",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002533.1.8",
          "form": "어떤영화관은 다닥다닥 붙여놓기도 하고..",
          "original_form": "어떤영화관은 다닥다닥 붙여놓기도 하고..",
          "speaker_id": "2",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002533.1.9",
          "form": "아 그럼 사람은 많던가요?",
          "original_form": "아 그럼 사람은 많던가요?",
          "speaker_id": "1",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002533.1.10",
          "form": "다닥다닥이면 신경쓰일 것 같은데",
          "original_form": "다닥다닥이면 신경쓰일 것 같은데",
          "speaker_id": "1",
          "time": "20210820 13:22"
        },
        {
          "id": "MDRW2100002533.1.11",
          "form": "저는 평일 낮시간에",
          "original_form": "저는 평일 낮시간에",
          "speaker_id": "2",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002533.1.12",
          "form": "한가할때만 가서",
          "original_form": "한가할때만 가서",
          "speaker_id": "2",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002533.1.13",
          "form": "사람이 엄청 많진 않아요",
          "original_form": "사람이 엄청 많진 않아요",
          "speaker_id": "2",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002533.1.14",
          "form": "많아야 열명쯤?",
          "original_form": "많아야 열명쯤?",
          "speaker_id": "2",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002533.1.15",
          "form": "근데 최근에 개봉한 모가디슈 보러갔을땐",
          "original_form": "근데 최근에 개봉한 모가디슈 보러갔을땐",
          "speaker_id": "2",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002533.1.16",
          "form": "시간대가 중요하겠네요.",
          "original_form": "시간대가 중요하겠네요.",
          "speaker_id": "1",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002533.1.17",
          "form": "좀 많긴했어요",
          "original_form": "좀 많긴했어요",
          "speaker_id": "2",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002533.1.18",
          "form": "와 모가디슈!",
          "original_form": "와 모가디슈!",
          "speaker_id": "1",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002533.1.19",
          "form": "보셨나요",
          "original_form": "보셨나요",
          "speaker_id": "2",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002533.1.20",
          "form": "재밌던가요?",
          "original_form": "재밌던가요?",
          "speaker_id": "1",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002533.1.21",
          "form": "넵 저는",
          "original_form": "넵 저는",
          "speaker_id": "2",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002533.1.22",
          "form": "말로만 들었지요ㅎㅎ",
          "original_form": "말로만 들었지요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 13:23"
        },
        {
          "id": "MDRW2100002533.1.23",
          "form": "정말 좋았어요",
          "original_form": "정말 좋았어요",
          "speaker_id": "2",
          "time": "20210820 13:23"
        }
      ]
    }
