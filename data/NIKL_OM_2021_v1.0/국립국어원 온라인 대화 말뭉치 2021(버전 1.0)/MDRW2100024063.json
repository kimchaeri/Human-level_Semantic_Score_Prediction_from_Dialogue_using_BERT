      "utterance": [
        {
          "id": "MDRW2100024063.1.1",
          "form": "호캉스할까해요",
          "original_form": "호캉스할까해요",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024063.1.2",
          "form": "같은지역으로다가?",
          "original_form": "같은지역으로다가?",
          "speaker_id": "2",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024063.1.3",
          "form": "음   그건 모르겠어요",
          "original_form": "음   그건 모르겠어요",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024063.1.4",
          "form": "여행가고싶어요 정말",
          "original_form": "여행가고싶어요 정말",
          "speaker_id": "2",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024063.1.5",
          "form": "전 연말에 친구들 만나려구요",
          "original_form": "전 연말에 친구들 만나려구요",
          "speaker_id": "1",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024063.1.6",
          "form": "호캉스식으로요?",
          "original_form": "호캉스식으로요?",
          "speaker_id": "2",
          "time": "20210830 21:48"
        },
        {
          "id": "MDRW2100024063.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024063.1.8",
          "form": "어디요?",
          "original_form": "어디요?",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024063.1.9",
          "form": "음...에어비앤비 예쁜데 ?",
          "original_form": "음...에어비앤비 예쁜데 ?",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024063.1.10",
          "form": "좋으시겠어요",
          "original_form": "좋으시겠어요",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024063.1.11",
          "form": "열심히 몸만ㄷㄹ고 사진찍을 거예요",
          "original_form": "열심히 몸만ㄷㄹ고 사진찍을 거예요",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024063.1.12",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024063.1.13",
          "form": "힘내야지",
          "original_form": "힘내야지",
          "speaker_id": "1",
          "time": "20210830 21:49"
        },
        {
          "id": "MDRW2100024063.1.14",
          "form": "그럽시다",
          "original_form": "그럽시다",
          "speaker_id": "2",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024063.1.15",
          "form": "즐거운기억만 가득한 일년이었으면",
          "original_form": "즐거운기억만 가득한 일년이었으면",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024063.1.16",
          "form": "좋겠네요",
          "original_form": "좋겠네요",
          "speaker_id": "2",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024063.1.17",
          "form": "마무리도 잘합시다",
          "original_form": "마무리도 잘합시다",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024063.1.18",
          "form": "마지막분기 열심히 살고 기념 여행갑시다",
          "original_form": "마지막분기 열심히 살고 기념 여행갑시다",
          "speaker_id": "2",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100024063.1.19",
          "form": "그럽시다!!",
          "original_form": "그럽시다!!",
          "speaker_id": "1",
          "time": "20210830 21:50"
        }
      ]
