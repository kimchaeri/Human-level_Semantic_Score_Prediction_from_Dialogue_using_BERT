        {
          "id": "MDRW2100020884.1.1",
          "form": "우와 해먹어봐야겠어요",
          "original_form": "우와 해먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100020884.1.2",
          "form": "대박대박",
          "original_form": "대박대박",
          "speaker_id": "2",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100020884.1.3",
          "form": "강추합니다ㅋㅋ",
          "original_form": "강추합니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100020884.1.4",
          "form": "ㅋㅋㅋ 샌드위치는 어디꺼 좋아하세요?",
          "original_form": "ㅋㅋㅋ 샌드위치는 어디꺼 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100020884.1.5",
          "form": "저 샌드위치 안 가리고 먹지만ㅋㅋ",
          "original_form": "저 샌드위치 안 가리고 먹지만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100020884.1.6",
          "form": "빵집 스타일 아님 서브웨이 스타일?",
          "original_form": "빵집 스타일 아님 서브웨이 스타일?",
          "speaker_id": "2",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100020884.1.7",
          "form": "서브웨이나 빠리바게트 다 좋아해요ㅋㅋ",
          "original_form": "서브웨이나 빠리바게트 다 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:50"
        },
        {
          "id": "MDRW2100020884.1.8",
          "form": "저두요 빠리바게트는 그 4조각 나있는거 클럽샌드위치? 좋아해요",
          "original_form": "저두요 빠리바게트는 그 4조각 나있는거 클럽샌드위치? 좋아해요",
          "speaker_id": "2",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100020884.1.9",
          "form": "맛있죠ㅠ맛있죠",
          "original_form": "맛있죠ㅠ맛있죠",
          "speaker_id": "1",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100020884.1.10",
          "form": "저는 랩샌드위치요",
          "original_form": "저는 랩샌드위치요",
          "speaker_id": "1",
          "time": "20210828 13:51"
        },
        {
          "id": "MDRW2100020884.1.11",
          "form": "랩 맛있쥬ㅠ 서브웨이도 btd나 에그들어간거도 맛있구요",
          "original_form": "랩 맛있쥬ㅠ 서브웨이도 btd나 에그들어간거도 맛있구요",
          "speaker_id": "2",
          "time": "20210828 13:52"
        },
        {
          "id": "MDRW2100020884.1.12",
          "form": "네 진짜 다맛있어요ㅋㅋㅋ",
          "original_form": "네 진짜 다맛있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:52"
        },
        {
          "id": "MDRW2100020884.1.13",
          "form": "집에서 하면 왜 그맛이 안날까 진짜  의문스러워서",
          "original_form": "집에서 하면 왜 그맛이 안날까 진짜  의문스러워서",
          "speaker_id": "2",
          "time": "20210828 13:52"
        },
        {
          "id": "MDRW2100020884.1.14",
          "form": "그쳐????",
          "original_form": "그쳐????",
          "speaker_id": "1",
          "time": "20210828 13:52"
        },
        {
          "id": "MDRW2100020884.1.15",
          "form": "샌드위치집 알바해보고싶은 심정이에요",
          "original_form": "샌드위치집 알바해보고싶은 심정이에요",
          "speaker_id": "2",
          "time": "20210828 13:52"
        },
        {
          "id": "MDRW2100020884.1.16",
          "form": "샌드위치가 은근 만들기 힘들다니까요",
          "original_form": "샌드위치가 은근 만들기 힘들다니까요",
          "speaker_id": "1",
          "time": "20210828 13:52"
        },
        {
          "id": "MDRW2100020884.1.17",
          "form": "비밀이 있는건가",
          "original_form": "비밀이 있는건가",
          "speaker_id": "2",
          "time": "20210828 13:52"
        },
        {
          "id": "MDRW2100020884.1.18",
          "form": "재료 진짜 많이 필요하구요",
          "original_form": "재료 진짜 많이 필요하구요",
