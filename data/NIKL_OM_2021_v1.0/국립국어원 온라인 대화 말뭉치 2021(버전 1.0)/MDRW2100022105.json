        {
          "id": "MDRW2100022105.1.1",
          "form": "여행가고 싶으시죠?",
          "original_form": "여행가고 싶으시죠?",
          "speaker_id": "2",
          "time": "20210902 05:38"
        },
        {
          "id": "MDRW2100022105.1.2",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "1",
          "time": "20210902 06:08"
        },
        {
          "id": "MDRW2100022105.1.3",
          "form": "우리 내리진 말고 차로 구경하는거 해볼까요?",
          "original_form": "우리 내리진 말고 차로 구경하는거 해볼까요?",
          "speaker_id": "2",
          "time": "20210902 07:18"
        },
        {
          "id": "MDRW2100022105.1.4",
          "form": "그렇게 할까?",
          "original_form": "그렇게 할까?",
          "speaker_id": "1",
          "time": "20210902 07:29"
        },
        {
          "id": "MDRW2100022105.1.5",
          "form": "네 엄마 답답하시잖아요",
          "original_form": "네 엄마 답답하시잖아요",
          "speaker_id": "2",
          "time": "20210902 08:34"
        },
        {
          "id": "MDRW2100022105.1.6",
          "form": "고맙다 딸~",
          "original_form": "고맙다 딸~",
          "speaker_id": "1",
          "time": "20210902 09:12"
        },
        {
          "id": "MDRW2100022105.1.7",
          "form": "뭘요 도시락도 싸가서 차안에서 먹어요",
          "original_form": "뭘요 도시락도 싸가서 차안에서 먹어요",
          "speaker_id": "2",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100022105.1.8",
          "form": "그것도 좋은 생각이구나",
          "original_form": "그것도 좋은 생각이구나",
          "speaker_id": "1",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100022105.1.9",
          "form": "네 아직 내려서 구경하긴 힘드니까요",
          "original_form": "네 아직 내려서 구경하긴 힘드니까요",
          "speaker_id": "2",
          "time": "20210903 07:01"
        },
        {
          "id": "MDRW2100022105.1.10",
          "form": "맞아.. 백신을 다 맞긴했지만 돌파감염사례도 있고",
          "original_form": "맞아.. 백신을 다 맞긴했지만 돌파감염사례도 있고",
          "speaker_id": "1",
          "time": "20210903 07:02"
        },
        {
          "id": "MDRW2100022105.1.11",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210903 07:02"
        },
        {
          "id": "MDRW2100022105.1.12",
          "form": "조심해서 나쁠 건 없지",
          "original_form": "조심해서 나쁠 건 없지",
          "speaker_id": "1",
          "time": "20210903 07:02"
