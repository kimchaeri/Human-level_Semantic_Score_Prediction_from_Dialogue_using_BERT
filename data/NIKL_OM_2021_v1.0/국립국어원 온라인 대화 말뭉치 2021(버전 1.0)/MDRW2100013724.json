      "utterance": [
        {
          "id": "MDRW2100013724.1.1",
          "form": "날씨어때요?",
          "original_form": "날씨어때요?",
          "speaker_id": "1",
          "time": "20210825 15:53"
        },
        {
          "id": "MDRW2100013724.1.2",
          "form": "하루종일 비가 오고 있어요. name1님 동네는요?",
          "original_form": "하루종일 비가 오고 있어요. &name1&님 동네는요?",
          "speaker_id": "2",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013724.1.3",
          "form": "여기도 비오고있어요ㅜㅜ",
          "original_form": "여기도 비오고있어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013724.1.4",
          "form": "장마자나요",
          "original_form": "장마자나요",
          "speaker_id": "1",
          "time": "20210825 15:57"
        },
        {
          "id": "MDRW2100013724.1.5",
          "form": "중간중간 해가 뜨긴하는데 너무 습하네요",
          "original_form": "중간중간 해가 뜨긴하는데 너무 습하네요",
          "speaker_id": "2",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100013724.1.6",
          "form": "맞아요ㅜㅜ너무싫어요 비가와서",
          "original_form": "맞아요ㅜㅜ너무싫어요 비가와서",
          "speaker_id": "1",
          "time": "20210825 15:59"
        },
        {
          "id": "MDRW2100013724.1.7",
          "form": "금요일까지 온다고 하는데 빨리 그쳤으면 좋겠네요",
          "original_form": "금요일까지 온다고 하는데 빨리 그쳤으면 좋겠네요",
          "speaker_id": "2",
          "time": "20210825 16:00"
        },
        {
          "id": "MDRW2100013724.1.8",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013724.1.9",
          "form": "비랑 더위만아니면",
          "original_form": "비랑 더위만아니면",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013724.1.10",
          "form": "날씨좋아요",
          "original_form": "날씨좋아요",
          "speaker_id": "1",
          "time": "20210825 16:02"
        },
        {
          "id": "MDRW2100013724.1.11",
          "form": "그러게요 이제 딱 선선한 가을날씨",
          "original_form": "그러게요 이제 딱 선선한 가을날씨",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013724.1.12",
          "form": "처서 매직인가봐요!",
          "original_form": "처서 매직인가봐요!",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013724.1.13",
          "form": "시간빨리갔으면좋겠어요",
          "original_form": "시간빨리갔으면좋겠어요",
          "speaker_id": "1",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013724.1.14",
          "form": "맞아요! 전 더운거보단 추운게 좋더라구요",
          "original_form": "맞아요! 전 더운거보단 추운게 좋더라구요",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013724.1.15",
          "form": "저두요!!",
          "original_form": "저두요!!",
          "speaker_id": "1",
          "time": "20210825 16:08"
        },
        {
          "id": "MDRW2100013724.1.16",
          "form": "그래서 겨울이 빨리 오면 좋겠어요",
          "original_form": "그래서 겨울이 빨리 오면 좋겠어요",
          "speaker_id": "2",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013724.1.17",
          "form": "곧 있으면 겨울와요ㅎㅎ",
          "original_form": "곧 있으면 겨울와요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:10"
