      "utterance": [
        {
          "id": "MDRW2100036281.1.1",
          "form": "와~~ 대박 ㅋㅋㅋ 보고만 있어도 매울꺼같네요",
          "original_form": "와~~ 대박 ㅋㅋㅋ 보고만 있어도 매울꺼같네요",
          "speaker_id": "2",
          "time": "20210830 20:42"
        },
        {
          "id": "MDRW2100036281.1.2",
          "form": "돈까스도 매운곳 많더라구요",
          "original_form": "돈까스도 매운곳 많더라구요",
          "speaker_id": "1",
          "time": "20210830 20:42"
        },
        {
          "id": "MDRW2100036281.1.3",
          "form": "소스가요",
          "original_form": "소스가요",
          "speaker_id": "2",
          "time": "20210830 20:42"
        },
        {
          "id": "MDRW2100036281.1.4",
          "form": "네네! 돈까스 소스가용ㅋㅋㅋ",
          "original_form": "네네! 돈까스 소스가용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:45"
        },
        {
          "id": "MDRW2100036281.1.5",
          "form": "돈까스 좋아하세용?",
          "original_form": "돈까스 좋아하세용?",
          "speaker_id": "1",
          "time": "20210830 20:45"
        },
        {
          "id": "MDRW2100036281.1.6",
          "form": "저 고구마치즈돈까스 젤 좋아해요",
          "original_form": "저 고구마치즈돈까스 젤 좋아해요",
          "speaker_id": "1",
          "time": "20210830 20:45"
        },
        {
          "id": "MDRW2100036281.1.7",
          "form": "토요일날 치돈시켜먹었어요!!!",
          "original_form": "토요일날 치돈시켜먹었어요!!!",
          "speaker_id": "2",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100036281.1.8",
          "form": "급땡기뎌라구요 ㅋㅋㅋ쫄면이랑 떡볶이같이해서 먹엇어요 ㅋㅋㅋ",
          "original_form": "급땡기뎌라구요 ㅋㅋㅋ쫄면이랑 떡볶이같이해서 먹엇어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:56"
        },
        {
          "id": "MDRW2100036281.1.9",
          "form": "와 돈가스랑 궁합 좋은 음식들로만 잘 드셨네용ㅋㅋㅋㅋ",
          "original_form": "와 돈가스랑 궁합 좋은 음식들로만 잘 드셨네용ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100036281.1.10",
          "form": "돈까스 사보텐이 맛있던데",
          "original_form": "돈까스 사보텐이 맛있던데",
          "speaker_id": "1",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100036281.1.11",
          "form": "지점이 몇없어요ㅠㅠ",
          "original_form": "지점이 몇없어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100036281.1.12",
          "form": "전에 서울살때먹어보고",
          "original_form": "전에 서울살때먹어보고",
          "speaker_id": "2",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100036281.1.13",
          "form": "진짜한 십년된거같네요 안가본지ㅜ",
          "original_form": "진짜한 십년된거같네요 안가본지ㅜ",
          "speaker_id": "2",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100036281.1.14",
          "form": "돈까스 경양식이랑 일식중에",
          "original_form": "돈까스 경양식이랑 일식중에",
          "speaker_id": "1",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100036281.1.15",
          "form": "뭘 더 선호하세용???",
          "original_form": "뭘 더 선호하세용???",
          "speaker_id": "1",
          "time": "20210830 21:04"
        },
        {
          "id": "MDRW2100036281.1.16",
          "form": "박빙이에요 ㅋㅋㅋ각각 땡길때가 잇는거같아요",
          "original_form": "박빙이에요 ㅋㅋㅋ각각 땡길때가 잇는거같아요",
          "speaker_id": "2",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100036281.1.17",
          "form": "저도 둘 다 좋아해요 촉촉한 돈까스가 땡기는날이 있어용",
          "original_form": "저도 둘 다 좋아해요 촉촉한 돈까스가 땡기는날이 있어용",
          "speaker_id": "1",
          "time": "20210830 21:27"
        },
        {
          "id": "MDRW2100036281.1.18",
          "form": "맞아요 ㅋㅋㅋ 각기다른매력이잇어요",
          "original_form": "맞아요 ㅋㅋㅋ 각기다른매력이잇어요",
          "speaker_id": "2",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100036281.1.19",
          "form": "지금 이시간에 배는 출출한데 밥을 차려먹자니 너무 거하고",
          "original_form": "지금 이시간에 배는 출출한데 밥을 차려먹자니 너무 거하고",
          "speaker_id": "1",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100036281.1.20",
          "form": "간단한 간식으로 뭐가 좋을까요?",
          "original_form": "간단한 간식으로 뭐가 좋을까요?",
          "speaker_id": "1",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100036281.1.21",
          "form": "전 지금 거하게 흡입중인데~ ㅎ",
          "original_form": "전 지금 거하게 흡입중인데~ ㅎ",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100036281.1.22",
          "form": "집에 방토잇나요!?!",
          "original_form": "집에 방토잇나요!?!",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100036281.1.23",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100036281.1.24",
          "form": "아 방토말고",
          "original_form": "아 방토말고",
          "speaker_id": "1",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100036281.1.25",
          "form": "아님 고구마말랭이?",
          "original_form": "아님 고구마말랭이?",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100036281.1.26",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100036281.1.27",
          "form": "그냥 토마토능 있어요!",
          "original_form": "그냥 토마토능 있어요!",
          "speaker_id": "1",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100036281.1.28",
          "form": "오 럭키",
          "original_form": "오 럭키",
          "speaker_id": "1",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100036281.1.29",
          "form": "저도 찰토마도 몇알잇는데",
          "original_form": "저도 찰토마도 몇알잇는데",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100036281.1.30",
          "form": "그냥은 잘먹어서 갈아마셔요 ㅋㅋㅋ",
          "original_form": "그냥은 잘먹어서 갈아마셔요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100036281.1.31",
          "form": "토마토라면 드셔보셧어요??",
          "original_form": "토마토라면 드셔보셧어요??",
          "speaker_id": "2",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100036281.1.32",
          "form": "아 설탕넣고 갈아마시면 맛있겠다...",
          "original_form": "아 설탕넣고 갈아마시면 맛있겠다...",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100036281.1.33",
          "form": "그건 뭐에용???",
          "original_form": "그건 뭐에용???",
          "speaker_id": "1",
          "time": "20210830 21:30"
        },
        {
          "id": "MDRW2100036281.1.34",
          "form": "맛있는거 많이 알고계시네요!",
          "original_form": "맛있는거 많이 알고계시네요!",
          "speaker_id": "1",
          "time": "20210830 21:30"
        }
      ]
    }
