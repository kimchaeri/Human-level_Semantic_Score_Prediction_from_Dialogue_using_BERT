      "utterance": [
        {
          "id": "MDRW2100019587.1.1",
          "form": "네 근데 예전보다는 덜 먹었어요",
          "original_form": "네 근데 예전보다는 덜 먹었어요",
          "speaker_id": "2",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100019587.1.2",
          "form": "올해 수박 당도가 좋더라구요",
          "original_form": "올해 수박 당도가 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100019587.1.3",
          "form": "맞죠 ㅎㅎ 먹는거마다 맛있었어요",
          "original_form": "맞죠 ㅎㅎ 먹는거마다 맛있었어요",
          "speaker_id": "1",
          "time": "20210827 20:32"
        },
        {
          "id": "MDRW2100019587.1.4",
          "form": "그 미니수박 드셔보셨나요?",
          "original_form": "그 미니수박 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210827 20:32"
        },
        {
          "id": "MDRW2100019587.1.5",
          "form": "앗 아뇨 미니수박 드셔보셨어요??",
          "original_form": "앗 아뇨 미니수박 드셔보셨어요??",
          "speaker_id": "2",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019587.1.6",
          "form": "아니요 ㅎㅎ 저는 항상 큰 수박 사요 !",
          "original_form": "아니요 ㅎㅎ 저는 항상 큰 수박 사요 !",
          "speaker_id": "1",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019587.1.7",
          "form": "가족들이랑 살아서 ㅎㅎ",
          "original_form": "가족들이랑 살아서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019587.1.8",
          "form": "저도 큰거만 먹어봐서 ㅋㅋㅋ",
          "original_form": "저도 큰거만 먹어봐서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019587.1.9",
          "form": "마트 갈 때마다 와 신기하다~ 하고",
          "original_form": "마트 갈 때마다 와 신기하다~ 하고",
          "speaker_id": "2",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019587.1.10",
          "form": "큰거 집어서 들고와요 ㅋㅋㅋㅋ",
          "original_form": "큰거 집어서 들고와요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019587.1.11",
          "form": "맞아요ㅋㅋ 뭔가 가성비 떨어지는 느낌 ㅎㅎ",
          "original_form": "맞아요ㅋㅋ 뭔가 가성비 떨어지는 느낌 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019587.1.12",
          "form": "막 하나에 5000원씩 하고 그러던데요 ㅎㅎ",
          "original_form": "막 하나에 5000원씩 하고 그러던데요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019587.1.13",
          "form": "맞아요 그냥 먹기엔 너무 비싸요",
          "original_form": "맞아요 그냥 먹기엔 너무 비싸요",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019587.1.14",
          "form": "혼자 살아도 그냥",
          "original_form": "혼자 살아도 그냥",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019587.1.15",
          "form": "큰거 사다가 소분해두고 먹을거같아요ㅋㅋㅋ",
          "original_form": "큰거 사다가 소분해두고 먹을거같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019587.1.16",
          "form": "맞아요 ㅋㅋ 수박은 필수로 있어야죠",
          "original_form": "맞아요 ㅋㅋ 수박은 필수로 있어야죠",
          "speaker_id": "1",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019587.1.17",
          "form": "화채도 해드시나요?",
          "original_form": "화채도 해드시나요?",
          "speaker_id": "1",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019587.1.18",
          "form": "어릴땐 먹었는데 요즘은 해먹은적 없어요",
          "original_form": "어릴땐 먹었는데 요즘은 해먹은적 없어요",
          "speaker_id": "2",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100019587.1.19",
          "form": "저두요 ㅋㅋ 수박 자르는거만으로 귀찮아요 ㅋㅋ",
          "original_form": "저두요 ㅋㅋ 수박 자르는거만으로 귀찮아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019587.1.20",
          "form": "누가 만들어주면 잘먹을텐데 ㅋㅋ",
          "original_form": "누가 만들어주면 잘먹을텐데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:48"
        },
        {
          "id": "MDRW2100019587.1.21",
          "form": "그쵸 ㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:49"
        },
        {
          "id": "MDRW2100019587.1.22",
          "form": "전 사면 미리 다 썰어두긴 하는데",
          "original_form": "전 사면 미리 다 썰어두긴 하는데",
          "speaker_id": "2",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019587.1.23",
          "form": "화채 만들거면 한입크기로 썰어야해서 일이 두배더라구요",
          "original_form": "화채 만들거면 한입크기로 썰어야해서 일이 두배더라구요",
          "speaker_id": "2",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019587.1.24",
          "form": "맞아요 또 음료 비율도 맞춰야되고 ㅋㅋ",
          "original_form": "맞아요 또 음료 비율도 맞춰야되고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019587.1.25",
          "form": "예전엔 밀키스랑 사이다로 많이 해먹었는데",
          "original_form": "예전엔 밀키스랑 사이다로 많이 해먹었는데",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019587.1.26",
          "form": "내년엔 한번 먹어야겠네요 ㅋㅋ",
          "original_form": "내년엔 한번 먹어야겠네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019587.1.27",
          "form": "그러게요 ㅋㅋㅋ",
          "original_form": "그러게요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019587.1.28",
          "form": "한번쯤은 해먹어볼 만 한거같아요",
          "original_form": "한번쯤은 해먹어볼 만 한거같아요",
