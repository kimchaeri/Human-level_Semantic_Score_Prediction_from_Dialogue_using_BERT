      "utterance": [
        {
          "id": "MDRW2100029394.1.1",
          "form": "좋아하는 계절 있나요?",
          "original_form": "좋아하는 계절 있나요?",
          "speaker_id": "1",
          "time": "20210903 06:20"
        },
        {
          "id": "MDRW2100029394.1.2",
          "form": "저는 그래도 겨울보단 여름이 좋네요",
          "original_form": "저는 그래도 겨울보단 여름이 좋네요",
          "speaker_id": "2",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100029394.1.3",
          "form": "봄 가을은 환절기 알러지 때문에 별로구요",
          "original_form": "봄 가을은 환절기 알러지 때문에 별로구요",
          "speaker_id": "2",
          "time": "20210903 14:16"
        },
        {
          "id": "MDRW2100029394.1.4",
          "form": "환절기 알러지도 있군요",
          "original_form": "환절기 알러지도 있군요",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100029394.1.5",
          "form": "몰랐네요",
          "original_form": "몰랐네요",
          "speaker_id": "1",
          "time": "20210903 19:23"
        },
        {
          "id": "MDRW2100029394.1.6",
          "form": "ㅋㅋㅋ꽃가루 알러지 이런거예요",
          "original_form": "ㅋㅋㅋ꽃가루 알러지 이런거예요",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100029394.1.7",
          "form": "꽃피고 낙엽 생기고 이러면 심해져요",
          "original_form": "꽃피고 낙엽 생기고 이러면 심해져요",
          "speaker_id": "2",
          "time": "20210903 19:24"
        },
        {
          "id": "MDRW2100029394.1.8",
          "form": "그렇군요.. 안타깝네요..",
          "original_form": "그렇군요.. 안타깝네요..",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100029394.1.9",
          "form": "저는 가을 단풍을 좋아해서 가을 제일 좋아해요",
          "original_form": "저는 가을 단풍을 좋아해서 가을 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210903 19:25"
        },
        {
          "id": "MDRW2100029394.1.10",
          "form": "단풍 예쁘죠ㅎㅎ",
          "original_form": "단풍 예쁘죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100029394.1.11",
          "form": "점점 가을이 짧아지는게 아쉬우시겠군요",
          "original_form": "점점 가을이 짧아지는게 아쉬우시겠군요",
          "speaker_id": "2",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100029394.1.12",
          "form": "그래요.. 이상기후로 점점 봄가을이 짧아지고 있어서 아쉬워요",
          "original_form": "그래요.. 이상기후로 점점 봄가을이 짧아지고 있어서 아쉬워요",
          "speaker_id": "1",
          "time": "20210903 19:40"
        },
        {
          "id": "MDRW2100029394.1.13",
          "form": "여름이랑 겨울만 길어지고",
          "original_form": "여름이랑 겨울만 길어지고",
          "speaker_id": "1",
          "time": "20210903 19:40"
        },
        {
          "id": "MDRW2100029394.1.14",
          "form": "맞아요ㅠ 저도 여름이 좋다지만",
          "original_form": "맞아요ㅠ 저도 여름이 좋다지만",
          "speaker_id": "2",
          "time": "20210903 21:07"
        },
        {
          "id": "MDRW2100029394.1.15",
          "form": "이번 폭염은 진짜 오바더라구요 ㅎ",
          "original_form": "이번 폭염은 진짜 오바더라구요 ㅎ",
          "speaker_id": "2",
          "time": "20210903 21:07"
        },
        {
          "id": "MDRW2100029394.1.16",
          "form": "너무 심했죠? 에어컨 없었으면 어쩔뻔했을까요",
          "original_form": "너무 심했죠? 에어컨 없었으면 어쩔뻔했을까요",
          "speaker_id": "1",
          "time": "20210903 21:19"
        },
        {
          "id": "MDRW2100029394.1.17",
          "form": "맞아요 전 걍 도서관으로 피신갔어요",
          "original_form": "맞아요 전 걍 도서관으로 피신갔어요",
          "speaker_id": "2",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100029394.1.18",
          "form": "에어컨 빵빵 틀어주는 도서관ㅎㅎ",
          "original_form": "에어컨 빵빵 틀어주는 도서관ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100029394.1.19",
          "form": "겨울엔 그만큼 춥겠죠..ㅜ",
          "original_form": "겨울엔 그만큼 춥겠죠..ㅜ",
          "speaker_id": "2",
          "time": "20210904 00:56"
        },
        {
          "id": "MDRW2100029394.1.20",
          "form": "요새는 여름이 너무 덥고 겨울이 너무 춥고 그러네요",
          "original_form": "요새는 여름이 너무 덥고 겨울이 너무 춥고 그러네요",
          "speaker_id": "1",
          "time": "20210904 05:33"
        },
        {
          "id": "MDRW2100029394.1.21",
          "form": "도서관이 그래도 가까이 있었나봐요.. 겨울에도 이용해보면 좋겠네요",
          "original_form": "도서관이 그래도 가까이 있었나봐요.. 겨울에도 이용해보면 좋겠네요",
          "speaker_id": "1",
          "time": "20210904 05:33"
        },
        {
          "id": "MDRW2100029394.1.22",
          "form": "ㅋㅋㅋㅋ겨울에 따뜻하러 도서관을..?!",
          "original_form": "ㅋㅋㅋㅋ겨울에 따뜻하러 도서관을..?!",
          "speaker_id": "2",
          "time": "20210904 20:18"
        },
        {
          "id": "MDRW2100029394.1.23",
          "form": "좋은 생각이네요",
          "original_form": "좋은 생각이네요",
          "speaker_id": "2",
          "time": "20210904 20:18"
        },
        {
          "id": "MDRW2100029394.1.24",
          "form": "도서관은 다양한 이유로 갈 수 있으니까요..:)",
          "original_form": "도서관은 다양한 이유로 갈 수 있으니까요..:)",
