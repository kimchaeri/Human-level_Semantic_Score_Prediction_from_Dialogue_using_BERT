      "utterance": [
        {
          "id": "MDRW2100021219.1.1",
          "form": "저녁 드셨나요",
          "original_form": "저녁 드셨나요",
          "speaker_id": "2",
          "time": "20210828 19:57"
        },
        {
          "id": "MDRW2100021219.1.2",
          "form": "네. 오늘은 치킨",
          "original_form": "네. 오늘은 치킨",
          "speaker_id": "1",
          "time": "20210907 22:13"
        },
        {
          "id": "MDRW2100021219.1.3",
          "form": "밥은 안 드세요",
          "original_form": "밥은 안 드세요",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100021219.1.4",
          "form": "치킨 먹으니 배불러요",
          "original_form": "치킨 먹으니 배불러요",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100021219.1.5",
          "form": "치킨 좋아해요?",
          "original_form": "치킨 좋아해요?",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100021219.1.6",
          "form": "사랑합니다ㅎㅎ",
          "original_form": "사랑합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100021219.1.7",
          "form": "어디 치킨 드셨나요",
          "original_form": "어디 치킨 드셨나요",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100021219.1.8",
          "form": "멕시카땡 후라이드.",
          "original_form": "멕시카땡 후라이드.",
          "speaker_id": "1",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100021219.1.9",
          "form": "후라이드는 멕시카땡 강추요",
          "original_form": "후라이드는 멕시카땡 강추요",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100021219.1.10",
          "form": "어디 치킨 좋아해요?",
          "original_form": "어디 치킨 좋아해요?",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100021219.1.11",
          "form": "교촌이요! 레드오리지널 좋아해요",
          "original_form": "교촌이요! 레드오리지널 좋아해요",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100021219.1.12",
          "form": "뭘로 드셨나요",
          "original_form": "뭘로 드셨나요",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100021219.1.13",
          "form": "오 저도 최애",
          "original_form": "오 저도 최애",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100021219.1.14",
          "form": "후라이드맍,?",
          "original_form": "후라이드맍,?",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100021219.1.15",
          "form": "오늘은 동생이 후라이드 시켜줬어요",
          "original_form": "오늘은 동생이 후라이드 시켜줬어요",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100021219.1.16",
          "form": "ㅎㅇ착한동생",
          "original_form": "ㅎㅇ착한동생",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100021219.1.17",
          "form": "멕시카나 청양마요도 맛있어요",
          "original_form": "멕시카나 청양마요도 맛있어요",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100021219.1.18",
          "form": "원래 후라이드? 양념?",
          "original_form": "원래 후라이드? 양념?",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100021219.1.19",
          "form": "무슨 파인가요",
          "original_form": "무슨 파인가요",
          "speaker_id": "2",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100021219.1.20",
          "form": "후라이드가 진리죠. 소금파",
          "original_form": "후라이드가 진리죠. 소금파",
          "speaker_id": "1",
          "time": "20210907 22:23"
        },
        {
          "id": "MDRW2100021219.1.21",
          "form": "name2씨는?",
          "original_form": "&name2&씨는?",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100021219.1.22",
          "form": "오오 치잘알",
          "original_form": "오오 치잘알",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100021219.1.23",
          "form": "전 그래도 레드오리지널이요!",
          "original_form": "전 그래도 레드오리지널이요!",
          "speaker_id": "2",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100021219.1.24",
          "form": "그럼요 당연하죠",
          "original_form": "그럼요 당연하죠",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100021219.1.25",
          "form": "님 저랑 취향 좀 맞네요?",
          "original_form": "님 저랑 취향 좀 맞네요?",
          "speaker_id": "1",
          "time": "20210907 22:24"
        },
        {
          "id": "MDRW2100021219.1.26",
          "form": "마요도 좋네요 드셔보셨나요",
          "original_form": "마요도 좋네요 드셔보셨나요",
          "speaker_id": "2",
          "time": "20210907 22:24"
        }
      ]
