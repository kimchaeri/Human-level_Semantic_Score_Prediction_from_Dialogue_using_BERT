      "utterance": [
        {
          "id": "MDRW2100034305.1.1",
          "form": "야식 좋아하세요?",
          "original_form": "야식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100034305.1.2",
          "form": "야식은 잘 안먹으려는 편이예요",
          "original_form": "야식은 잘 안먹으려는 편이예요",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100034305.1.3",
          "form": "야식 좋아하셔요?",
          "original_form": "야식 좋아하셔요?",
          "speaker_id": "2",
          "time": "20210827 23:09"
        },
        {
          "id": "MDRW2100034305.1.4",
          "form": "좋아하진 않는데 가끔먹음 맛잇긴 하더라구요 ㅎ",
          "original_form": "좋아하진 않는데 가끔먹음 맛잇긴 하더라구요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100034305.1.5",
          "form": "어떤 메뉴 드셔요?",
          "original_form": "어떤 메뉴 드셔요?",
          "speaker_id": "2",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100034305.1.6",
          "form": "주로 떡볶이요 ㅎㅎ",
          "original_form": "주로 떡볶이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 23:10"
        },
        {
          "id": "MDRW2100034305.1.7",
          "form": "매운거 좋아해서요",
          "original_form": "매운거 좋아해서요",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100034305.1.8",
          "form": "오 떡볶이의 유혹은 뿌리치기 힘들죠",
          "original_form": "오 떡볶이의 유혹은 뿌리치기 힘들죠",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100034305.1.9",
          "form": "매운건 요즘 속이 쓰려서",
          "original_form": "매운건 요즘 속이 쓰려서",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100034305.1.10",
          "form": "한동안은 많이 먹었어요",
          "original_form": "한동안은 많이 먹었어요",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100034305.1.11",
          "form": "로제가 참 !!!맛있지만 사악한 칼로리",
          "original_form": "로제가 참 !!!맛있지만 사악한 칼로리",
          "speaker_id": "1",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100034305.1.12",
          "form": "맞아요 로제 저도 좋아하는데",
          "original_form": "맞아요 로제 저도 좋아하는데",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034305.1.13",
          "form": "칼로리는 사악한 메뉴죠ㅜ",
          "original_form": "칼로리는 사악한 메뉴죠ㅜ",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100034305.1.14",
          "form": "맛있는거 칼로리가 다 후덜하네여 ㅋ",
          "original_form": "맛있는거 칼로리가 다 후덜하네여 ㅋ",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034305.1.15",
          "form": "네 음식의 맛과 칼로리는 함께하죠",
          "original_form": "네 음식의 맛과 칼로리는 함께하죠",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034305.1.16",
          "form": "name2님은 뭐좋아하셔요",
          "original_form": "&name2&님은 뭐좋아하셔요",
          "speaker_id": "1",
          "time": "20210827 23:18"
        },
        {
          "id": "MDRW2100034305.1.17",
          "form": "저는 족발이요",
          "original_form": "저는 족발이요",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100034305.1.18",
          "form": "남의발이 맛있죠  족발 닭발 !!",
          "original_form": "남의발이 맛있죠  족발 닭발 !!",
          "speaker_id": "1",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100034305.1.19",
          "form": "ㅎㅎ네네 족발 닭발",
          "original_form": "ㅎㅎ네네 족발 닭발",
          "speaker_id": "2",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100034305.1.20",
          "form": "쫄깃쫄깃한 식감이 좋아요",
          "original_form": "쫄깃쫄깃한 식감이 좋아요",
          "speaker_id": "2",
          "time": "20210827 23:32"
        },
        {
          "id": "MDRW2100034305.1.21",
          "form": "아 먹고 싶네여 주먹밥이랑 ㅋㅋ닭발 냠냠",
          "original_form": "아 먹고 싶네여 주먹밥이랑 ㅋㅋ닭발 냠냠",
          "speaker_id": "1",
          "time": "20210827 23:33"
        },
        {
          "id": "MDRW2100034305.1.22",
          "form": "저도 얘기하다보니 닭발 먹고 싶네요",
          "original_form": "저도 얘기하다보니 닭발 먹고 싶네요",
          "speaker_id": "2",
          "time": "20210827 23:33"
        }
