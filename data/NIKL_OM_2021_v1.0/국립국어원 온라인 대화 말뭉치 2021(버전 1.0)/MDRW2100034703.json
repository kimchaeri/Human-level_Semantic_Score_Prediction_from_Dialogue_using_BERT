          "time": "20210829 10:28"
        },
        {
          "id": "MDRW2100034703.1.32",
          "form": "근데 현실에 만족하자",
          "original_form": "근데 현실에 만족하자",
          "speaker_id": "2",
          "time": "20210829 10:28"
        },
        {
          "id": "MDRW2100034703.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:28"
        },
        {
          "id": "MDRW2100034703.1.34",
