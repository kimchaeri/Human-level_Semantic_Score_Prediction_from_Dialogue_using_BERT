      "utterance": [
        {
          "id": "MDRW2100034703.1.1",
          "form": "오케이 과일이 항상 기본타는 하더라 ㅋㅋㅋ",
          "original_form": "오케이 과일이 항상 기본타는 하더라 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100034703.1.2",
          "form": "요즘 카톡으로 주문하는것도 편해져서 좋더라^^",
          "original_form": "요즘 카톡으로 주문하는것도 편해져서 좋더라^^",
          "speaker_id": "2",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100034703.1.3",
          "form": "저녁은 뭐할라고??",
          "original_form": "저녁은 뭐할라고??",
          "speaker_id": "2",
          "time": "20210828 17:51"
        },
        {
          "id": "MDRW2100034703.1.4",
          "form": "응 좋다 맞어ㅋㅋ",
          "original_form": "응 좋다 맞어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100034703.1.5",
          "form": "해물찜포장해서",
          "original_form": "해물찜포장해서",
          "speaker_id": "1",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100034703.1.6",
          "form": "시댁 가서 먹는다ㅋㅋ",
          "original_form": "시댁 가서 먹는다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:31"
        },
        {
          "id": "MDRW2100034703.1.7",
          "form": "이욜~맛나게 묵고와잉^^",
          "original_form": "이욜~맛나게 묵고와잉^^",
          "speaker_id": "2",
          "time": "20210828 18:41"
        },
        {
          "id": "MDRW2100034703.1.8",
          "form": "얍얍 맛있구만ㅋㅋ",
          "original_form": "얍얍 맛있구만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:22"
        },
        {
          "id": "MDRW2100034703.1.9",
          "form": "오늘은 날씨가 꾸물이 하네",
          "original_form": "오늘은 날씨가 꾸물이 하네",
          "speaker_id": "2",
          "time": "20210829 10:22"
        },
        {
          "id": "MDRW2100034703.1.10",
          "form": "응응 오늘 넘피곤하네",
          "original_form": "응응 오늘 넘피곤하네",
          "speaker_id": "1",
          "time": "20210829 10:22"
        },
        {
          "id": "MDRW2100034703.1.11",
          "form": "난 친구 창원 집들이 간다ㅋㅋ",
          "original_form": "난 친구 창원 집들이 간다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:23"
        },
        {
          "id": "MDRW2100034703.1.12",
          "form": "오늘 하루는 이렇게 때 우겠네",
          "original_form": "오늘 하루는 이렇게 때 우겠네",
          "speaker_id": "2",
          "time": "20210829 10:23"
        },
        {
          "id": "MDRW2100034703.1.13",
          "form": "첫째가 또 방 분리해 달라 해서",
          "original_form": "첫째가 또 방 분리해 달라 해서",
          "speaker_id": "2",
          "time": "20210829 10:23"
        },
        {
          "id": "MDRW2100034703.1.14",
          "form": "집안일이 또 산더미 네",
          "original_form": "집안일이 또 산더미 네",
          "speaker_id": "2",
          "time": "20210829 10:23"
        },
        {
          "id": "MDRW2100034703.1.15",
          "form": "오오 좋다잉ㅋㅋ",
          "original_form": "오오 좋다잉ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100034703.1.16",
          "form": "아 웃겨ㅋㅋㅋ요즘 집들이많이가네ㅋㅋ",
          "original_form": "아 웃겨ㅋㅋㅋ요즘 집들이많이가네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100034703.1.17",
          "form": "나는 친구네 온다캐가 ㅋㅋ대충 청소라도 해야지",
          "original_form": "나는 친구네 온다캐가 ㅋㅋ대충 청소라도 해야지",
          "speaker_id": "1",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100034703.1.18",
          "form": "거긴 또 새집이라서 갔다 오면 또 새집 가고 싶을걸",
          "original_form": "거긴 또 새집이라서 갔다 오면 또 새집 가고 싶을걸",
          "speaker_id": "2",
          "time": "20210829 10:24"
        },
        {
          "id": "MDRW2100034703.1.19",
          "form": "그렇지만 현실은 힐데스ㅋㅋㅋㅋㅋ",
          "original_form": "그렇지만 현실은 힐데스ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:25"
        },
        {
          "id": "MDRW2100034703.1.20",
          "form": "ㅋㅋㅋ 현실에 만족하자ㅋㅋ",
          "original_form": "ㅋㅋㅋ 현실에 만족하자ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:25"
        },
        {
          "id": "MDRW2100034703.1.21",
          "form": "울형님댁도 34평이거든?",
          "original_form": "울형님댁도 34평이거든?",
          "speaker_id": "1",
          "time": "20210829 10:26"
        },
        {
          "id": "MDRW2100034703.1.22",
          "form": "그래 나도 너무 만족한다ㅋㅋㅋㅋㅋ",
          "original_form": "그래 나도 너무 만족한다ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:26"
        },
        {
          "id": "MDRW2100034703.1.23",
          "form": "근데 갔다 오면 또 부럽네ㅋㅋㅋㅋ",
          "original_form": "근데 갔다 오면 또 부럽네ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 10:26"
        },
        {
          "id": "MDRW2100034703.1.24",
          "form": "저번에 신랑이랑 또",
          "original_form": "저번에 신랑이랑 또",
          "speaker_id": "1",
          "time": "20210829 10:26"
        },
        {
          "id": "MDRW2100034703.1.25",
          "form": "밥먹으러갔다가",
          "original_form": "밥먹으러갔다가",
          "speaker_id": "1",
          "time": "20210829 10:26"
        },
        {
          "id": "MDRW2100034703.1.26",
          "form": "신랑이 작은방보고",
          "original_form": "신랑이 작은방보고",
          "speaker_id": "1",
          "time": "20210829 10:26"
        },
        {
          "id": "MDRW2100034703.1.27",
          "form": "와 넓다 이래된거야",
          "original_form": "와 넓다 이래된거야",
          "speaker_id": "1",
          "time": "20210829 10:26"
        },
        {
          "id": "MDRW2100034703.1.28",
          "form": "진짜 어른셋이서 앉아가 대화하는데",
