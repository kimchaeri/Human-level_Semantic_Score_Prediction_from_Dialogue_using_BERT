        },
        {
          "id": "MDRW2100043684.1.3",
          "form": "웅웅~",
          "original_form": "웅웅~",
          "speaker_id": "1",
          "time": "20210907 20:00"
        },
        {
          "id": "MDRW2100043684.1.4",
          "form": "피자스쿨 시켜라",
          "original_form": "피자스쿨 시켜라",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043684.1.5",
          "form": "말고 다른곳~",
          "original_form": "말고 다른곳~",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043684.1.6",
          "form": "다른데 뭐가 있는데",
          "original_form": "다른데 뭐가 있는데",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043684.1.7",
          "form": "알볼로 피자라고 맛있다 ㅋㅋ",
          "original_form": "알볼로 피자라고 맛있다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043684.1.8",
          "form": "거기 시켜라 내가 돈 내줄게",
          "original_form": "거기 시켜라 내가 돈 내줄게",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043684.1.9",
          "form": "진짜로?",
          "original_form": "진짜로?",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043684.1.10",
          "form": "그래 같이 먹자",
          "original_form": "그래 같이 먹자",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043684.1.11",
          "form": "웬일 ㅎㅎㅎ",
          "original_form": "웬일 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043684.1.12",
          "form": "오늘만 먹고 내일부터는 다이어트해야지",
          "original_form": "오늘만 먹고 내일부터는 다이어트해야지",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043684.1.13",
          "form": "ㅋㅋ 좋다!",
          "original_form": "ㅋㅋ 좋다!",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043684.1.14",
          "form": "그럼 바로 시킬게",
          "original_form": "그럼 바로 시킬게",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043684.1.15",
          "form": "그래 카드 결제로 시켜라",
