      "utterance": [
        {
          "id": "MDRW2100030465.1.1",
          "form": "디피 보러가고 싶네요",
          "original_form": "디피 보러가고 싶네요",
          "speaker_id": "2",
          "time": "20210904 05:17"
        },
        {
          "id": "MDRW2100030465.1.2",
          "form": "아직 끝까지 보신 건 아니군요. 이것도 시즌제인가요?",
          "original_form": "아직 끝까지 보신 건 아니군요. 이것도 시즌제인가요?",
          "speaker_id": "1",
          "time": "20210904 12:41"
        },
        {
          "id": "MDRW2100030465.1.3",
          "form": "네 6화가 시즌 끝이지만ㅋㅋ",
          "original_form": "네 6화가 시즌 끝이지만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:03"
        },
        {
          "id": "MDRW2100030465.1.4",
          "form": "다음시즌 나오려면 또 한참 기다려야겠네요",
          "original_form": "다음시즌 나오려면 또 한참 기다려야겠네요",
          "speaker_id": "1",
          "time": "20210904 14:28"
        },
        {
          "id": "MDRW2100030465.1.5",
          "form": "네 ㅋㅋ 그럼 딴거 보면 되쥬",
          "original_form": "네 ㅋㅋ 그럼 딴거 보면 되쥬",
          "speaker_id": "2",
          "time": "20210904 15:49"
        },
        {
          "id": "MDRW2100030465.1.6",
          "form": "또 재밌게 보는 거 있으세요?",
          "original_form": "또 재밌게 보는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210904 15:50"
        },
        {
          "id": "MDRW2100030465.1.7",
          "form": "드라마요?ㅋㅋ",
          "original_form": "드라마요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100030465.1.8",
          "form": "아무거나요ㅋㅋ",
          "original_form": "아무거나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:06"
        },
        {
          "id": "MDRW2100030465.1.9",
          "form": "대탈출 런닝맨",
          "original_form": "대탈출 런닝맨",
          "speaker_id": "2",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100030465.1.10",
          "form": "요러케 보는듯요",
          "original_form": "요러케 보는듯요",
          "speaker_id": "2",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100030465.1.11",
          "form": "대탈출 한 번도 안 봤는데 보려면 처음부터 봐야할까요?",
          "original_form": "대탈출 한 번도 안 봤는데 보려면 처음부터 봐야할까요?",
          "speaker_id": "1",
          "time": "20210904 16:46"
        },
        {
          "id": "MDRW2100030465.1.12",
          "form": "네 처음부터 정주행하시는게 좋죠",
          "original_form": "네 처음부터 정주행하시는게 좋죠",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100030465.1.13",
          "form": "막 에피소드가 별개여도",
          "original_form": "막 에피소드가 별개여도",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100030465.1.14",
          "form": "같은 유니버스라서",
          "original_form": "같은 유니버스라서",
          "speaker_id": "2",
          "time": "20210904 16:58"
        },
        {
          "id": "MDRW2100030465.1.15",
          "form": "나중에 날 잡고 달리던지 해야겠네요ㅋㅋ",
          "original_form": "나중에 날 잡고 달리던지 해야겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:06"
        },
        {
          "id": "MDRW2100030465.1.16",
          "form": "네 보시면 금방 보실거에요",
          "original_form": "네 보시면 금방 보실거에요",
          "speaker_id": "2",
          "time": "20210904 17:07"
        },
        {
          "id": "MDRW2100030465.1.17",
          "form": "막 어려운 내용은 아니라",
          "original_form": "막 어려운 내용은 아니라",
          "speaker_id": "2",
          "time": "20210904 17:07"
        },
        {
          "id": "MDRW2100030465.1.18",
          "form": "지니어스 시리스는 다 봤는데 대탈출은 시작도 못했네요ㅋㅋ",
          "original_form": "지니어스 시리스는 다 봤는데 대탈출은 시작도 못했네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:09"
        }
      ]
