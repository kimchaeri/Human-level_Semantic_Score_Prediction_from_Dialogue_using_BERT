      "utterance": [
        {
          "id": "MDRW2100011046.1.1",
          "form": "넷플릭스 보세요?",
          "original_form": "넷플릭스 보세요?",
          "speaker_id": "2",
          "time": "20210824 00:06"
        },
        {
          "id": "MDRW2100011046.1.2",
          "form": "네! 근데 꾸준하게는 잘 안보게 되네요",
          "original_form": "네! 근데 꾸준하게는 잘 안보게 되네요",
          "speaker_id": "1",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100011046.1.3",
          "form": "넷플릭스 많이 보시나요?",
          "original_form": "넷플릭스 많이 보시나요?",
          "speaker_id": "1",
          "time": "20210824 00:09"
        },
        {
          "id": "MDRW2100011046.1.4",
          "form": "음 많이는 아닌 것 같고 좋아하는게 생기면 끝까지 보는것같아요!",
          "original_form": "음 많이는 아닌 것 같고 좋아하는게 생기면 끝까지 보는것같아요!",
          "speaker_id": "2",
          "time": "20210824 00:10"
        },
        {
          "id": "MDRW2100011046.1.5",
          "form": "좋아하는 시리즈 있으세여?",
          "original_form": "좋아하는 시리즈 있으세여?",
          "speaker_id": "2",
          "time": "20210824 00:10"
        },
        {
          "id": "MDRW2100011046.1.6",
          "form": "한국꺼보단 해외껄 자주 봐요 ! 재밌게 봤던건 셜록홈즈랑 산타클라리타 다이어트, 에밀리파리에가다 재밌게 봤어요",
          "original_form": "한국꺼보단 해외껄 자주 봐요 ! 재밌게 봤던건 셜록홈즈랑 산타클라리타 다이어트, 에밀리파리에가다 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210824 00:12"
        },
        {
          "id": "MDRW2100011046.1.7",
          "form": "홈즈... 아이린애들러 잊지못해요ㅠ",
          "original_form": "홈즈... 아이린애들러 잊지못해요ㅠ",
          "speaker_id": "2",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011046.1.8",
          "form": "오 산타클라리타 다이어트 궁금했는데!ㅎㅎ",
          "original_form": "오 산타클라리타 다이어트 궁금했는데!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 00:13"
        },
        {
          "id": "MDRW2100011046.1.9",
          "form": "티비도 많이 보세요?",
          "original_form": "티비도 많이 보세요?",
          "speaker_id": "2",
          "time": "20210824 00:14"
        },
        {
          "id": "MDRW2100011046.1.10",
          "form": "산타클라리타 재밌어요 조금 잔인하긴 해요..!",
          "original_form": "산타클라리타 재밌어요 조금 잔인하긴 해요..!",
          "speaker_id": "1",
          "time": "20210824 00:56"
        },
        {
          "id": "MDRW2100011046.1.11",
          "form": "티비는 대탈출이랑 심야괴담회 챙겨보고있어요ㅎㅎ",
          "original_form": "티비는 대탈출이랑 심야괴담회 챙겨보고있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 00:57"
        },
        {
          "id": "MDRW2100011046.1.12",
          "form": "보는게 다 비슷비슷하죠..?",
          "original_form": "보는게 다 비슷비슷하죠..?",
          "speaker_id": "1",
          "time": "20210824 00:57"
        },
        {
          "id": "MDRW2100011046.1.13",
          "form": "대탈출 재밌죠 ㅋㅋㅋ",
          "original_form": "대탈출 재밌죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:17"
        },
        {
          "id": "MDRW2100011046.1.14",
          "form": "심야괴담회 보고 싶은데 너무 무섭다는데...",
          "original_form": "심야괴담회 보고 싶은데 너무 무섭다는데...",
          "speaker_id": "2",
          "time": "20210824 09:17"
        },
        {
          "id": "MDRW2100011046.1.15",
          "form": "괜찮나요 ??",
          "original_form": "괜찮나요 ??",
          "speaker_id": "2",
          "time": "20210824 09:17"
        },
        {
          "id": "MDRW2100011046.1.16",
          "form": "그래서 항상 가족들이랑 봐요..ㅎㅎ 보고는 싶은데 혼자보기는 무서워서요",
          "original_form": "그래서 항상 가족들이랑 봐요..ㅎㅎ 보고는 싶은데 혼자보기는 무서워서요",
          "speaker_id": "1",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100011046.1.17",
          "form": "요즘 재연을 되게 잘해서 더 무섭더라구요 ㅠㅠ",
          "original_form": "요즘 재연을 되게 잘해서 더 무섭더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100011046.1.18",
          "form": "저도 한번 봐야겠어요 ㅋㅋㅋ",
          "original_form": "저도 한번 봐야겠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 12:41"
        },
        {
          "id": "MDRW2100011046.1.19",
          "form": "귀신...나오나요 !?",
          "original_form": "귀신...나오나요 !?",
          "speaker_id": "2",
          "time": "20210824 12:41"
        },
        {
          "id": "MDRW2100011046.1.20",
          "form": "오 .. 네... 나옵니다 .. 분장이 무서워요 ㅠㅠ",
          "original_form": "오 .. 네... 나옵니다 .. 분장이 무서워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011046.1.21",
          "form": "좀비와는 또다른 느낌이에요..",
          "original_form": "좀비와는 또다른 느낌이에요..",
          "speaker_id": "1",
          "time": "20210824 12:42"
        },
        {
          "id": "MDRW2100011046.1.22",
          "form": "앗... 제가 귀신을 좀 무서워하는데",
          "original_form": "앗... 제가 귀신을 좀 무서워하는데",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011046.1.23",
          "form": "ㅠㅠㅋㅋㅋ 좀비는 오히려 잘 보거든요...",
          "original_form": "ㅠㅠㅋㅋㅋ 좀비는 오히려 잘 보거든요...",
          "speaker_id": "2",
          "time": "20210824 16:41"
        },
        {
          "id": "MDRW2100011046.1.24",
          "form": "혼자보지마시구 가족이나 친구분하고 같이보세요..!",
          "original_form": "혼자보지마시구 가족이나 친구분하고 같이보세요..!",
          "speaker_id": "1",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011046.1.25",
          "form": "조금 괜찮습니다..",
          "original_form": "조금 괜찮습니다..",
          "speaker_id": "1",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100011046.1.26",
          "form": "ㅋㅋㅋㅋㅋ넵넵",
          "original_form": "ㅋㅋㅋㅋㅋ넵넵",
          "speaker_id": "2",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100011046.1.27",
          "form": "대탈출 최근화 보셨어요?",
          "original_form": "대탈출 최근화 보셨어요?",
          "speaker_id": "2",
          "time": "20210824 16:59"
        },
        {
          "id": "MDRW2100011046.1.28",
          "form": "네네 너무 재밌게 봤어요 보셨나요?",
          "original_form": "네네 너무 재밌게 봤어요 보셨나요?",
          "speaker_id": "1",
          "time": "20210824 17:01"
        },
        {
          "id": "MDRW2100011046.1.29",
          "form": "딱 이번화만 아직 못 봤어요",
          "original_form": "딱 이번화만 아직 못 봤어요",
          "speaker_id": "2",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100011046.1.30",
          "form": "좀비 진짜 재밌게 봤어요 !!ㅋㅋㅋ",
          "original_form": "좀비 진짜 재밌게 봤어요 !!ㅋㅋㅋ",
