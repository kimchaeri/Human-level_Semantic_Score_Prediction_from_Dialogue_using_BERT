      "utterance": [
        {
          "id": "MDRW2100021613.1.1",
          "form": "어느 쪽으로 연구 희망하는지 물어보셨나요??",
          "original_form": "어느 쪽으로 연구 희망하는지 물어보셨나요??",
          "speaker_id": "1",
          "time": "20210828 21:00"
        },
        {
          "id": "MDRW2100021613.1.2",
          "form": "네네! ㅎㅎ 기업쪽에서 연구 원하신다고 하셔서요",
          "original_form": "네네! ㅎㅎ 기업쪽에서 연구 원하신다고 하셔서요",
          "speaker_id": "2",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100021613.1.3",
          "form": "음 아직 확정은 아니지만",
          "original_form": "음 아직 확정은 아니지만",
          "speaker_id": "1",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100021613.1.4",
          "form": "로보틱스랑 인공지능",
          "original_form": "로보틱스랑 인공지능",
          "speaker_id": "1",
          "time": "20210828 21:01"
        },
        {
          "id": "MDRW2100021613.1.5",
          "form": "관련해서 연구하고 싶어요!",
          "original_form": "관련해서 연구하고 싶어요!",
          "speaker_id": "1",
          "time": "20210828 21:02"
        },
        {
          "id": "MDRW2100021613.1.6",
          "form": "헉 완전 미래지향적 분야",
          "original_form": "헉 완전 미래지향적 분야",
          "speaker_id": "2",
          "time": "20210828 21:02"
        },
        {
          "id": "MDRW2100021613.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그렇긴 하죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그렇긴 하죠",
          "speaker_id": "1",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100021613.1.8",
          "form": "근데 아직은 덜 활성화? 된 것 같아요",
          "original_form": "근데 아직은 덜 활성화? 된 것 같아요",
          "speaker_id": "1",
          "time": "20210828 21:03"
        },
        {
          "id": "MDRW2100021613.1.9",
          "form": "이제 이끌어 가시면 되지요 넘멋져요",
          "original_form": "이제 이끌어 가시면 되지요 넘멋져요",
          "speaker_id": "2",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100021613.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100021613.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ감사합니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ감사합니다",
          "speaker_id": "1",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100021613.1.12",
          "form": "그쪽 분야는 근데 좀 남초인가요?",
          "original_form": "그쪽 분야는 근데 좀 남초인가요?",
          "speaker_id": "2",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100021613.1.13",
          "form": "네... 완전요",
          "original_form": "네... 완전요",
          "speaker_id": "1",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100021613.1.14",
          "form": "이런... 취업하거나 승진하거나 할 때 좀 신경쓰이시겠어요",
          "original_form": "이런... 취업하거나 승진하거나 할 때 좀 신경쓰이시겠어요",
          "speaker_id": "2",
          "time": "20210828 21:18"
        },
        {
          "id": "MDRW2100021613.1.15",
          "form": "그렇겠죠... 지금도 벌써",
          "original_form": "그렇겠죠... 지금도 벌써",
          "speaker_id": "1",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100021613.1.16",
          "form": "과 소모임에 1학년 여자가 저 포함 두명이라",
          "original_form": "과 소모임에 1학년 여자가 저 포함 두명이라",
          "speaker_id": "1",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100021613.1.17",
          "form": "하하...",
          "original_form": "하하...",
          "speaker_id": "1",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100021613.1.18",
          "form": "어후 이런;;;;;",
          "original_form": "어후 이런;;;;;",
          "speaker_id": "2",
          "time": "20210828 22:46"
        },
        {
          "id": "MDRW2100021613.1.19",
          "form": "같은 업계에 동성 선배가 많이 있어야 힘이 되는데...ㅠㅠ",
          "original_form": "같은 업계에 동성 선배가 많이 있어야 힘이 되는데...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 22:46"
        },
