      "utterance": [
        {
          "id": "MDRW2100034613.1.1",
          "form": "생선을 튀겨먹다니 요리박사시다,,,",
          "original_form": "생선을 튀겨먹다니 요리박사시다,,,",
          "speaker_id": "1",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100034613.1.2",
          "form": "전 굽는것도 간신히 해요",
          "original_form": "전 굽는것도 간신히 해요",
          "speaker_id": "1",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100034613.1.3",
          "form": "기름양의 차이지 하는것 비슷합니다ㅎㅎ",
          "original_form": "기름양의 차이지 하는것 비슷합니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 16:24"
        },
        {
          "id": "MDRW2100034613.1.4",
          "form": "물기제거가 관건이에요",
          "original_form": "물기제거가 관건이에요",
          "speaker_id": "2",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100034613.1.5",
          "form": "맞아요 물이 있음 튀기잖아요",
          "original_form": "맞아요 물이 있음 튀기잖아요",
          "speaker_id": "1",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100034613.1.6",
          "form": "어떤 생선을 튀겨드세요?",
          "original_form": "어떤 생선을 튀겨드세요?",
          "speaker_id": "1",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100034613.1.7",
          "form": "키친티슈로 꼭꼭 닦으셔야해요 저는 요즘은 민어요!",
          "original_form": "키친티슈로 꼭꼭 닦으셔야해요 저는 요즘은 민어요!",
          "speaker_id": "2",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100034613.1.8",
          "form": "두툼해서 잘 닦고 조각내셔야합니다",
          "original_form": "두툼해서 잘 닦고 조각내셔야합니다",
          "speaker_id": "2",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100034613.1.9",
          "form": "헐 요리박사시네",
          "original_form": "헐 요리박사시네",
          "speaker_id": "1",
          "time": "20210828 16:26"
        },
        {
          "id": "MDRW2100034613.1.10",
          "form": "특이한 생선도 드시고 번거롭게 요리하시고",
          "original_form": "특이한 생선도 드시고 번거롭게 요리하시고",
          "speaker_id": "1",
          "time": "20210828 16:27"
        },
        {
          "id": "MDRW2100034613.1.11",
          "form": "ㅎㅎ실제로 보시면 그리 대단한 요리를하지는 않아요ㅋㅋ",
          "original_form": "ㅎㅎ실제로 보시면 그리 대단한 요리를하지는 않아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:29"
        },
        {
          "id": "MDRW2100034613.1.12",
          "form": "name1님은 디져트 마스터 같아요ㅎㅎ",
          "original_form": "&name1&님은 디져트 마스터 같아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100034613.1.13",
          "form": "ㅎㅎ디저트를 더 좋아하긴 해요",
          "original_form": "ㅎㅎ디저트를 더 좋아하긴 해요",
          "speaker_id": "1",
          "time": "20210828 16:31"
        },
        {
          "id": "MDRW2100034613.1.14",
          "form": "ㅎㅎ케잌도 실제로 만드시구!",
          "original_form": "ㅎㅎ케잌도 실제로 만드시구!",
          "speaker_id": "2",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100034613.1.15",
          "form": "민어튀김에 비하면 완전 쉬워요",
          "original_form": "민어튀김에 비하면 완전 쉬워요",
          "speaker_id": "1",
          "time": "20210828 16:32"
        },
        {
          "id": "MDRW2100034613.1.16",
          "form": "기름온도는 어떻게 맞추세요?",
          "original_form": "기름온도는 어떻게 맞추세요?",
          "speaker_id": "1",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100034613.1.17",
          "form": "각자 관심사따라 요리하는거 같아요 ㅎㅎ",
          "original_form": "각자 관심사따라 요리하는거 같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100034613.1.18",
          "form": "그 빵가루나 조각 넣고 가라앉았다 뜨는걸 봐요ㅎㅎ",
          "original_form": "그 빵가루나 조각 넣고 가라앉았다 뜨는걸 봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 16:33"
        },
        {
          "id": "MDRW2100034613.1.19",
          "form": "온도게를 쓰시는지 궁금했어요 ㅋㅋㅋ",
          "original_form": "온도게를 쓰시는지 궁금했어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:34"
        },
        {
          "id": "MDRW2100034613.1.20",
          "form": "ㅎㅎ가정집에서 뭐 별거있나요~~",
          "original_form": "ㅎㅎ가정집에서 뭐 별거있나요~~",
          "speaker_id": "2",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100034613.1.21",
          "form": "오븐요리를 하시는 분은 오븐용 온도게를 쓰시길래",
          "original_form": "오븐요리를 하시는 분은 오븐용 온도게를 쓰시길래",
          "speaker_id": "1",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100034613.1.22",
          "form": "여쭤봤어요 ㅋㅋㅋ",
          "original_form": "여쭤봤어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:35"
        },
        {
          "id": "MDRW2100034613.1.23",
          "form": "그러시구나 그건 못들어봤어요 오븐은 그냥 온도 맞추면 알아서 설정되는줄 알았어요ㅎㅅ",
          "original_form": "그러시구나 그건 못들어봤어요 오븐은 그냥 온도 맞추면 알아서 설정되는줄 알았어요ㅎㅅ",
          "speaker_id": "2",
          "time": "20210828 16:36"
        },
        {
          "id": "MDRW2100034613.1.24",
          "form": "열선 온도가 220도 이래도 위치따라 온도가 다르고 그러니깐요 특히 큰 덩어리 고기 익힐땐",
          "original_form": "열선 온도가 220도 이래도 위치따라 온도가 다르고 그러니깐요 특히 큰 덩어리 고기 익힐땐",
          "speaker_id": "1",
          "time": "20210828 16:37"
        },
        {
          "id": "MDRW2100034613.1.25",
          "form": "안까지 익었나 보려고 침처럼 찌르는 온도계를 쓰더라구요",
          "original_form": "안까지 익었나 보려고 침처럼 찌르는 온도계를 쓰더라구요",
          "speaker_id": "1",
