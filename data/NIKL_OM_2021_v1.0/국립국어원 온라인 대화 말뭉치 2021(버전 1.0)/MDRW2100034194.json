      "utterance": [
        {
          "id": "MDRW2100034194.1.1",
          "form": "매번 아침밥이 고민돼요",
          "original_form": "매번 아침밥이 고민돼요",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100034194.1.2",
          "form": "주로 뭐해서 드세요?",
          "original_form": "주로 뭐해서 드세요?",
          "speaker_id": "2",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100034194.1.3",
          "form": "매일 똑같은것 같아요ㅜㅜ",
          "original_form": "매일 똑같은것 같아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100034194.1.4",
          "form": "전 매일 씨리얼ㅇ이나 오트밀 먹어요 과일도요",
          "original_form": "전 매일 씨리얼ㅇ이나 오트밀 먹어요 과일도요",
          "speaker_id": "1",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100034194.1.5",
          "form": "편하자나요 ㅋㅋ",
          "original_form": "편하자나요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100034194.1.6",
          "form": "저도 그러고 싶네요",
          "original_form": "저도 그러고 싶네요",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100034194.1.7",
          "form": "다들 안먹는데요",
          "original_form": "다들 안먹는데요",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100034194.1.8",
          "form": "그렇다고 많이 먹지도 않는데",
          "original_form": "그렇다고 많이 먹지도 않는데",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100034194.1.9",
          "form": "편하시겠어요 ㅎㅎ",
          "original_form": "편하시겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100034194.1.10",
          "form": "아침에 더 자고 싶어서 그런건데 건강엔 좀 안좋죠",
          "original_form": "아침에 더 자고 싶어서 그런건데 건강엔 좀 안좋죠",
          "speaker_id": "1",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100034194.1.11",
          "form": "오트밀은 그래도 괜찮아요",
          "original_form": "오트밀은 그래도 괜찮아요",
          "speaker_id": "2",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100034194.1.12",
          "form": "과일하고 드시니",
          "original_form": "과일하고 드시니",
          "speaker_id": "2",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100034194.1.13",
          "form": "괜찮을 거예요",
          "original_form": "괜찮을 거예요",
          "speaker_id": "2",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100034194.1.14",
          "form": "짠거 먹는것보다 나아요",
          "original_form": "짠거 먹는것보다 나아요",
          "speaker_id": "2",
          "time": "20210827 20:47"
        },
        {
          "id": "MDRW2100034194.1.15",
          "form": "그럼요 삼김보단 몸에 좋단 생각으로 챙겨먹어요",
          "original_form": "그럼요 삼김보단 몸에 좋단 생각으로 챙겨먹어요",
          "speaker_id": "1",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100034194.1.16",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100034194.1.17",
          "form": "아침에 샌드위치는 가끔 먹어요",
          "original_form": "아침에 샌드위치는 가끔 먹어요",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100034194.1.18",
          "form": "사와서 먹는데 좋아해요 ㅎㅎ",
          "original_form": "사와서 먹는데 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100034194.1.19",
          "form": "제요리가 별로인가봐요 ㅋㅋ",
          "original_form": "제요리가 별로인가봐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100034194.1.20",
          "form": "파는건 재료가 여러가지잖아요 파는 요리가 더 맛있는건 전혀 흠이 아니죠",
          "original_form": "파는건 재료가 여러가지잖아요 파는 요리가 더 맛있는건 전혀 흠이 아니죠",
          "speaker_id": "1",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100034194.1.21",
          "form": "빠바 가맹점비를 생각해보세요 휴,,,",
          "original_form": "빠바 가맹점비를 생각해보세요 휴,,,",
          "speaker_id": "1",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100034194.1.22",
          "form": "네 집에선 그렇게 못해요",
          "original_form": "네 집에선 그렇게 못해요",
          "speaker_id": "2",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100034194.1.23",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210827 20:56"
        },
        {
          "id": "MDRW2100034194.1.24",
          "form": "재료가 풍부해서 맛나요",
          "original_form": "재료가 풍부해서 맛나요",
          "speaker_id": "2",
          "time": "20210827 20:56"
        },
        {
          "id": "MDRW2100034194.1.25",
          "form": "집에서 만들려면 케일 한봉지 양상추 한봉지 이렇게 다 사서 조립ㅎ야되니",
          "original_form": "집에서 만들려면 케일 한봉지 양상추 한봉지 이렇게 다 사서 조립ㅎ야되니",
          "speaker_id": "1",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100034194.1.26",
          "form": "그렇죠 진짜 쓰고 다남고 ㅋㅋ",
          "original_form": "그렇죠 진짜 쓰고 다남고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100034194.1.27",
          "form": "처리 불가돼요",
          "original_form": "처리 불가돼요",
          "speaker_id": "2",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100034194.1.28",
          "form": "사는게 합리적이네요 ㅎㅎ",
          "original_form": "사는게 합리적이네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100034194.1.29",
          "form": "네 그럼요 집에서 만들거면 간단하게 만들어먹어야죠",
          "original_form": "네 그럼요 집에서 만들거면 간단하게 만들어먹어야죠",
          "speaker_id": "1",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100034194.1.30",
          "form": "그렇죠 ㅎㅎ현명 하세요",
          "original_form": "그렇죠 ㅎㅎ현명 하세요",
          "speaker_id": "2",
          "time": "20210827 20:59"
        },
        {
          "id": "MDRW2100034194.1.31",
          "form": "ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:59"
        },
        {
          "id": "MDRW2100034194.1.32",
          "form": "집에 디종머스타드 있으신가요?",
          "original_form": "집에 디종머스타드 있으신가요?",
          "speaker_id": "1",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100034194.1.33",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100034194.1.34",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100034194.1.35",
          "form": "샌드위치에 넣음 맛있는데 그런걸 구비하기어렵죠",
          "original_form": "샌드위치에 넣음 맛있는데 그런걸 구비하기어렵죠",
          "speaker_id": "1",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100034194.1.36",
          "form": "아 그렇죠",
          "original_form": "아 그렇죠",
          "speaker_id": "2",
          "time": "20210827 21:01"
        },
        {
          "id": "MDRW2100034194.1.37",
          "form": "그래서 맛난거군요",
          "original_form": "그래서 맛난거군요",
          "speaker_id": "2",
          "time": "20210827 21:01"
        },
        {
          "id": "MDRW2100034194.1.38",
          "form": "네 여러야채ㅔ 여러 소스에",
          "original_form": "네 여러야채ㅔ 여러 소스에",
          "speaker_id": "1",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100034194.1.39",
          "form": "마요 케찹만 넣어도 맛은 있긴한데",
          "original_form": "마요 케찹만 넣어도 맛은 있긴한데",
          "speaker_id": "1",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100034194.1.40",
          "form": "그래도부족하죠 ㅡㅡ",
          "original_form": "그래도부족하죠 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 21:03"
        },
        {
          "id": "MDRW2100034194.1.41",
          "form": "뭔가 더 들어감 맛나죠 ㅎㅎ",
          "original_form": "뭔가 더 들어감 맛나죠 ㅎㅎ",
