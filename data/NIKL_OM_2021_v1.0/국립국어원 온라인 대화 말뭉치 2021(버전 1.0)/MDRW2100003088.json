      "utterance": [
        {
          "id": "MDRW2100003088.1.1",
          "form": "그럼 배달 음식 좋아하세요?",
          "original_form": "그럼 배달 음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210820 18:28"
        },
        {
          "id": "MDRW2100003088.1.2",
          "form": "저는 로제떡볶이 좋아하거든요",
          "original_form": "저는 로제떡볶이 좋아하거든요",
          "speaker_id": "1",
          "time": "20210820 18:28"
        },
        {
          "id": "MDRW2100003088.1.3",
          "form": "떡볶이 수혈 ㅋㅋㅋ",
          "original_form": "떡볶이 수혈 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:28"
        },
        {
          "id": "MDRW2100003088.1.4",
          "form": "네 로제떡볶이 맛있죠!!",
          "original_form": "네 로제떡볶이 맛있죠!!",
          "speaker_id": "2",
          "time": "20210820 18:30"
        },
        {
          "id": "MDRW2100003088.1.5",
          "form": "엽떡 먹었는데 맵고 맛있더라고요",
          "original_form": "엽떡 먹었는데 맵고 맛있더라고요",
          "speaker_id": "2",
          "time": "20210820 18:30"
        },
        {
          "id": "MDRW2100003088.1.6",
          "form": "엽떡도 좋아유~~~",
          "original_form": "엽떡도 좋아유~~~",
          "speaker_id": "1",
          "time": "20210820 18:31"
        },
        {
          "id": "MDRW2100003088.1.7",
          "form": "그럼 못드시는 음식 있으세요??",
          "original_form": "그럼 못드시는 음식 있으세요??",
          "speaker_id": "1",
          "time": "20210820 18:31"
        },
        {
          "id": "MDRW2100003088.1.8",
          "form": "저는 족발 닭발 이런걸 못먹어요 ㅜㅜ",
          "original_form": "저는 족발 닭발 이런걸 못먹어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 18:31"
        },
        {
          "id": "MDRW2100003088.1.9",
          "form": "부모님 영향이 큰지 안먹어본건 못먹겠더라구요",
          "original_form": "부모님 영향이 큰지 안먹어본건 못먹겠더라구요",
          "speaker_id": "1",
          "time": "20210820 18:31"
        },
        {
          "id": "MDRW2100003088.1.10",
          "form": "저는 개고기나 벌레요리 이런건 안 먹어요...",
          "original_form": "저는 개고기나 벌레요리 이런건 안 먹어요...",
          "speaker_id": "2",
          "time": "20210820 18:32"
        },
        {
          "id": "MDRW2100003088.1.11",
          "form": "발 종류는 안 드시는건가요ㅋㅋ",
          "original_form": "발 종류는 안 드시는건가요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100003088.1.12",
          "form": "어 저두요!!",
          "original_form": "어 저두요!!",
          "speaker_id": "1",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100003088.1.13",
          "form": "ㅋㅋㅋ 넹 발 종류 뭔가 낯설다해야할까요",
          "original_form": "ㅋㅋㅋ 넹 발 종류 뭔가 낯설다해야할까요",
          "speaker_id": "1",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100003088.1.14",
          "form": "배달음식 잘 시켜드세요?",
          "original_form": "배달음식 잘 시켜드세요?",
          "speaker_id": "1",
          "time": "20210820 18:33"
        },
        {
          "id": "MDRW2100003088.1.15",
          "form": "저는 배달어플도 없어요ㅋㅋㅋ",
          "original_form": "저는 배달어플도 없어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:39"
        },
        {
          "id": "MDRW2100003088.1.16",
          "form": "회사에서 누가 시키면 같이 먹어요",
          "original_form": "회사에서 누가 시키면 같이 먹어요",
          "speaker_id": "2",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100003088.1.17",
          "form": "배달음식 자주 드세요",
          "original_form": "배달음식 자주 드세요",
          "speaker_id": "2",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100003088.1.18",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100003088.1.19",
          "form": "아 진짜요?? 저는 요기요 배민에서 자주 시켜먹어요!",
          "original_form": "아 진짜요?? 저는 요기요 배민에서 자주 시켜먹어요!",
          "speaker_id": "1",
          "time": "20210820 18:40"
        },
        {
          "id": "MDRW2100003088.1.20",
          "form": "밥하기 귀찮고 그래서요 ㅜㅜ",
          "original_form": "밥하기 귀찮고 그래서요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 18:41"
        },
        {
          "id": "MDRW2100003088.1.21",
          "form": "여름이라 더 시켜먹는 것두 있어요",
          "original_form": "여름이라 더 시켜먹는 것두 있어요",
          "speaker_id": "1",
          "time": "20210820 18:41"
        },
        {
          "id": "MDRW2100003088.1.22",
          "form": "회사갈 때 도시락 갖구 다니세요?",
          "original_form": "회사갈 때 도시락 갖구 다니세요?",
          "speaker_id": "1",
          "time": "20210820 18:41"
        },
        {
          "id": "MDRW2100003088.1.23",
          "form": "그쵸 불 쓰면 덥고ㅋㅋㅋ",
          "original_form": "그쵸 불 쓰면 덥고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 18:41"
        },
        {
          "id": "MDRW2100003088.1.24",
          "form": "아뇨 저희는 식대 나와서",
          "original_form": "아뇨 저희는 식대 나와서",
          "speaker_id": "2",
          "time": "20210820 18:42"
        },
        {
          "id": "MDRW2100003088.1.25",
          "form": "사먹습니다!!",
          "original_form": "사먹습니다!!",
          "speaker_id": "2",
          "time": "20210820 18:42"
        },
        {
          "id": "MDRW2100003088.1.26",
          "form": "아하 맛있나요?",
          "original_form": "아하 맛있나요?",
          "speaker_id": "1",
          "time": "20210820 18:42"
        },
        {
          "id": "MDRW2100003088.1.27",
          "form": "네 먹는 낙으로 다닙니다ㅋㄲㅋ",
          "original_form": "네 먹는 낙으로 다닙니다ㅋㄲㅋ",
          "speaker_id": "2",
          "time": "20210820 18:42"
        },
        {
          "id": "MDRW2100003088.1.28",
          "form": "뭐 주로 드세요??",
          "original_form": "뭐 주로 드세요??",
          "speaker_id": "2",
          "time": "20210820 18:43"
        },
        {
          "id": "MDRW2100003088.1.29",
          "form": "저는 떡볶이 삼겹살 피자 이런거요 ㅎㅎ",
          "original_form": "저는 떡볶이 삼겹살 피자 이런거요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 18:43"
        },
        {
          "id": "MDRW2100003088.1.30",
          "form": "근데 가리는게 많아서",
          "original_form": "근데 가리는게 많아서",
          "speaker_id": "1",
          "time": "20210820 18:43"
        },
        {
          "id": "MDRW2100003088.1.31",
          "form": "시키는게 한정적이라 힘들어요 ㅋㅋ",
          "original_form": "시키는게 한정적이라 힘들어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 18:43"
        },
        {
          "id": "MDRW2100003088.1.32",
          "form": "저도 빨리 맛있는 밥주는 회사가고싶어요..",
          "original_form": "저도 빨리 맛있는 밥주는 회사가고싶어요..",
          "speaker_id": "1",
          "time": "20210820 18:44"
        },
        {
          "id": "MDRW2100003088.1.33",
          "form": "ㅋㅋ먹을 수 있는거 종류별로 먹음 되죠~~~",
          "original_form": "ㅋㅋ먹을 수 있는거 종류별로 먹음 되죠~~~",
          "speaker_id": "2",
          "time": "20210820 18:44"
        },
        {
          "id": "MDRW2100003088.1.34",
          "form": "식대를 안 주나요?ㅜㅜ",
          "original_form": "식대를 안 주나요?ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 18:44"
        },
        {
          "id": "MDRW2100003088.1.35",
          "form": "아직 취준입니다..!",
          "original_form": "아직 취준입니다..!",
          "speaker_id": "1",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100003088.1.36",
          "form": "이제 막 졸업했어유~",
          "original_form": "이제 막 졸업했어유~",
          "speaker_id": "1",
          "time": "20210820 18:45"
        },
        {
          "id": "MDRW2100003088.1.37",
          "form": "앗 그렇군요!! 꼭 밥 잘 주는 회사 들어가시기를ㅎㅎ",
          "original_form": "앗 그렇군요!! 꼭 밥 잘 주는 회사 들어가시기를ㅎㅎ",
