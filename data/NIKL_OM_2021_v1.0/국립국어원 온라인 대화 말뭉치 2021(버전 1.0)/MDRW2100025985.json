      "utterance": [
        {
          "id": "MDRW2100025985.1.1",
          "form": "아 그 전부 자르는 그건가요?",
          "original_form": "아 그 전부 자르는 그건가요?",
          "speaker_id": "2",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025985.1.2",
          "form": "네 ㅋㅋㅋ 그거 하면 되긴 하는데 책이 너무 아까워요",
          "original_form": "네 ㅋㅋㅋ 그거 하면 되긴 하는데 책이 너무 아까워요",
          "speaker_id": "1",
          "time": "20210901 18:36"
        },
        {
          "id": "MDRW2100025985.1.3",
          "form": "맞아요 ㅠㅠ 다시 스프링으로 제본하긴하덩데",
          "original_form": "맞아요 ㅠㅠ 다시 스프링으로 제본하긴하덩데",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025985.1.4",
          "form": "그것도 돈드니가 뭔가 더 아깝긴하더라구용",
          "original_form": "그것도 돈드니가 뭔가 더 아깝긴하더라구용",
          "speaker_id": "2",
          "time": "20210901 18:38"
        },
        {
          "id": "MDRW2100025985.1.5",
          "form": "아하 다시 스프링으로 제본하는 방법이 있군요 처음 알았어요 ㅋㅋㅋ",
          "original_form": "아하 다시 스프링으로 제본하는 방법이 있군요 처음 알았어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:41"
        },
        {
          "id": "MDRW2100025985.1.6",
          "form": "ㅋㅋㅋㅋ네 최근에 주변에 시간당 가격받구 스캔하는곳 생겼거든요",
          "original_form": "ㅋㅋㅋㅋ네 최근에 주변에 시간당 가격받구 스캔하는곳 생겼거든요",
          "speaker_id": "2",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100025985.1.7",
          "form": "거기서 그렇게하더라구요 ㅋㅋㅋㅋ",
          "original_form": "거기서 그렇게하더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 18:42"
        },
        {
          "id": "MDRW2100025985.1.8",
          "form": "헐 대박 시간당 가격받고 스캔이라니 신세계인데요 저도 거기 가야겠어욬ㅋㅋㅋ",
          "original_form": "헐 대박 시간당 가격받고 스캔이라니 신세계인데요 저도 거기 가야겠어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100025985.1.9",
          "form": "ㅋㅋㅋㅋㅋ네 그런곳 있더라구요 참고해보세요!",
          "original_form": "ㅋㅋㅋㅋㅋ네 그런곳 있더라구요 참고해보세요!",
          "speaker_id": "2",
          "time": "20210901 19:12"
        },
        {
          "id": "MDRW2100025985.1.10",
          "form": "보통 책은 그럼 거의 소장하는편이세요?",
          "original_form": "보통 책은 그럼 거의 소장하는편이세요?",
          "speaker_id": "2",
          "time": "20210901 19:12"
        },
        {
          "id": "MDRW2100025985.1.11",
          "form": "정보감사합니다 ㅜㅜㅜ",
          "original_form": "정보감사합니다 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210901 19:15"
        },
        {
          "id": "MDRW2100025985.1.12",
          "form": "절 살리셨어요",
          "original_form": "절 살리셨어요",
          "speaker_id": "1",
          "time": "20210901 19:15"
        },
        {
          "id": "MDRW2100025985.1.13",
          "form": "저 원래 도서관에서 다 빌려다봐서 책이 적었었는데 이제 도서관다닐 시간도없어서 사다보다보니 그만..",
          "original_form": "저 원래 도서관에서 다 빌려다봐서 책이 적었었는데 이제 도서관다닐 시간도없어서 사다보다보니 그만..",
          "speaker_id": "1",
          "time": "20210901 19:15"
        },
        {
          "id": "MDRW2100025985.1.14",
          "form": "아...어쩔수업이 소장하시는군요..",
          "original_form": "아...어쩔수업이 소장하시는군요..",
          "speaker_id": "2",
          "time": "20210901 19:16"
        },
        {
          "id": "MDRW2100025985.1.15",
          "form": "그럼 책 중고로파세요?",
          "original_form": "그럼 책 중고로파세요?",
          "speaker_id": "2",
          "time": "20210901 19:17"
        },
        {
          "id": "MDRW2100025985.1.16",
          "form": "저랑 관련없는 책은 팔아요!",
          "original_form": "저랑 관련없는 책은 팔아요!",
          "speaker_id": "1",
          "time": "20210901 19:18"
        },
        {
          "id": "MDRW2100025985.1.17",
          "form": "타 전공 개론서 이런건 팔았네요",
          "original_form": "타 전공 개론서 이런건 팔았네요",
          "speaker_id": "1",
          "time": "20210901 19:18"
        },
        {
          "id": "MDRW2100025985.1.18",
          "form": "그렇군요! 전공책 말고는 좋아하시는 장르 있으세요?",
          "original_form": "그렇군요! 전공책 말고는 좋아하시는 장르 있으세요?",
          "speaker_id": "2",
          "time": "20210901 19:19"
        },
        {
          "id": "MDRW2100025985.1.19",
          "form": "음 다른책 본지 너무 오래되어서 기억이 잘 안나요 ㅋㅋㅋ",
          "original_form": "음 다른책 본지 너무 오래되어서 기억이 잘 안나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:22"
        },
        {
          "id": "MDRW2100025985.1.20",
          "form": "좋아하는 장르 있으세요?",
          "original_form": "좋아하는 장르 있으세요?",
          "speaker_id": "1",
          "time": "20210901 19:22"
        },
        {
          "id": "MDRW2100025985.1.21",
          "form": "저도 잘 안보는편이에요 ㅋㅋㅋㅋ가끔 자기계발서랑 에세이봐요",
          "original_form": "저도 잘 안보는편이에요 ㅋㅋㅋㅋ가끔 자기계발서랑 에세이봐요",
          "speaker_id": "2",
          "time": "20210901 19:24"
        }
      ]
    }
