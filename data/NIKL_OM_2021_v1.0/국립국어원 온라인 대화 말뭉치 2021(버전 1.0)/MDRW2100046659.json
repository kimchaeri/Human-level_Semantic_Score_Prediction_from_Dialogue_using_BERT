      "utterance": [
        {
          "id": "MDRW2100046659.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211104 10:30"
        },
        {
          "id": "MDRW2100046659.1.2",
          "form": "안녕하세요! 아침은 드셨나요?",
          "original_form": "안녕하세요! 아침은 드셨나요?",
          "speaker_id": "2",
          "time": "20211104 10:30"
        },
        {
          "id": "MDRW2100046659.1.3",
          "form": "넵  아침먹고 출근했습니다",
          "original_form": "넵  아침먹고 출근했습니다",
          "speaker_id": "1",
          "time": "20211104 10:30"
        },
        {
          "id": "MDRW2100046659.1.4",
          "form": "name2님두  드셨나요",
          "original_form": "&name2&님두  드셨나요",
          "speaker_id": "1",
          "time": "20211104 10:31"
        },
        {
          "id": "MDRW2100046659.1.5",
          "form": "오오 저는 시리얼 조금 먹고 출근했는데!",
          "original_form": "오오 저는 시리얼 조금 먹고 출근했는데!",
          "speaker_id": "2",
          "time": "20211104 10:31"
        },
        {
          "id": "MDRW2100046659.1.6",
          "form": "뭐 드셨나요?",
          "original_form": "뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20211104 10:31"
        },
        {
          "id": "MDRW2100046659.1.7",
          "form": "미역국 먹었네요",
          "original_form": "미역국 먹었네요",
          "speaker_id": "1",
          "time": "20211104 10:31"
        },
        {
          "id": "MDRW2100046659.1.8",
          "form": "생일은 아닙니다 ㅋ",
          "original_form": "생일은 아닙니다 ㅋ",
          "speaker_id": "1",
          "time": "20211104 10:31"
        },
        {
          "id": "MDRW2100046659.1.9",
          "form": "ㅋㅋㅋ맛있고 배부른 아침이셨겠어요",
          "original_form": "ㅋㅋㅋ맛있고 배부른 아침이셨겠어요",
          "speaker_id": "2",
          "time": "20211104 10:32"
        },
        {
          "id": "MDRW2100046659.1.10",
          "form": "아침 먹을 시간이 있으신걸보니 부지런하신가보군요!",
          "original_form": "아침 먹을 시간이 있으신걸보니 부지런하신가보군요!",
          "speaker_id": "2",
          "time": "20211104 10:32"
        },
        {
          "id": "MDRW2100046659.1.11",
          "form": "어머니께서 챙겨주신 거라",
          "original_form": "어머니께서 챙겨주신 거라",
          "speaker_id": "1",
          "time": "20211104 10:33"
        },
        {
          "id": "MDRW2100046659.1.12",
          "form": "어머니 안 계셨으면 굶었을 것 같네쇼",
          "original_form": "어머니 안 계셨으면 굶었을 것 같네쇼",
          "speaker_id": "1",
          "time": "20211104 10:33"
        },
        {
          "id": "MDRW2100046659.1.13",
          "form": "오오 그래도 먹으려는 의지가 있으니 챙겨주신거겠지요!",
          "original_form": "오오 그래도 먹으려는 의지가 있으니 챙겨주신거겠지요!",
          "speaker_id": "2",
          "time": "20211104 10:33"
        },
        {
          "id": "MDRW2100046659.1.14",
          "form": "미역국에 어떤걸 넣으시나요? 소고기?",
          "original_form": "미역국에 어떤걸 넣으시나요? 소고기?",
          "speaker_id": "2",
          "time": "20211104 10:34"
        },
        {
          "id": "MDRW2100046659.1.15",
          "form": "저희 가족은 세끼는 무조건 챙겨먹어야하네요",
          "original_form": "저희 가족은 세끼는 무조건 챙겨먹어야하네요",
          "speaker_id": "1",
          "time": "20211104 10:34"
        },
        {
          "id": "MDRW2100046659.1.16",
          "form": "아하 아침먹는 습관은 정말 좋은 습관 같아요",
          "original_form": "아하 아침먹는 습관은 정말 좋은 습관 같아요",
          "speaker_id": "2",
          "time": "20211104 10:34"
        },
        {
          "id": "MDRW2100046659.1.17",
          "form": "조개 넣고 끓이셨네요",
          "original_form": "조개 넣고 끓이셨네요",
          "speaker_id": "1",
          "time": "20211104 10:34"
        },
        {
          "id": "MDRW2100046659.1.18",
          "form": "오 저희집은 새우 아님 소고기거든요 ㅎㅎ",
          "original_form": "오 저희집은 새우 아님 소고기거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211104 10:34"
        },
        {
          "id": "MDRW2100046659.1.19",
          "form": "소고기 살 돈이 없어서ㅜㅜ",
          "original_form": "소고기 살 돈이 없어서ㅜㅜ",
          "speaker_id": "1",
          "time": "20211104 10:34"
        }
