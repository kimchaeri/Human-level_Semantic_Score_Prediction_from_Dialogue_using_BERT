      "utterance": [
        {
          "id": "MDRW2100030039.1.1",
          "form": "스포츠좋아하세요?",
          "original_form": "스포츠좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 19:37"
        },
        {
          "id": "MDRW2100030039.1.2",
          "form": "아 요즘은",
          "original_form": "아 요즘은",
          "speaker_id": "2",
          "time": "20210904 07:03"
        },
        {
          "id": "MDRW2100030039.1.3",
          "form": "따로 챙겨보진 않네요",
          "original_form": "따로 챙겨보진 않네요",
          "speaker_id": "2",
          "time": "20210904 07:04"
        },
        {
          "id": "MDRW2100030039.1.4",
          "form": "예전에는 야구 봤었는데",
          "original_form": "예전에는 야구 봤었는데",
          "speaker_id": "2",
          "time": "20210904 07:04"
        },
        {
          "id": "MDRW2100030039.1.5",
          "form": "저두요..스포츠뉴스가 제일재미있네요",
          "original_form": "저두요..스포츠뉴스가 제일재미있네요",
          "speaker_id": "1",
          "time": "20210904 08:55"
        },
        {
          "id": "MDRW2100030039.1.6",
          "form": "종목은 주로 뭐 보시나요",
          "original_form": "종목은 주로 뭐 보시나요",
          "speaker_id": "2",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030039.1.7",
          "form": "야구나 축구요",
          "original_form": "야구나 축구요",
          "speaker_id": "1",
          "time": "20210904 13:57"
        },
        {
          "id": "MDRW2100030039.1.8",
          "form": "한국쪽 보시마요 외국쪽 보시나요",
          "original_form": "한국쪽 보시마요 외국쪽 보시나요",
          "speaker_id": "2",
          "time": "20210904 13:58"
        },
        {
          "id": "MDRW2100030039.1.9",
          "form": "한국요",
          "original_form": "한국요",
          "speaker_id": "1",
          "time": "20210904 13:58"
        },
        {
          "id": "MDRW2100030039.1.10",
          "form": "실은그것도잘안봐요",
          "original_form": "실은그것도잘안봐요",
          "speaker_id": "1",
          "time": "20210904 13:59"
        },
        {
          "id": "MDRW2100030039.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:59"
        },
        {
          "id": "MDRW2100030039.1.12",
          "form": "뉴스에서 하이라이트만",
          "original_form": "뉴스에서 하이라이트만",
          "speaker_id": "1",
          "time": "20210904 13:59"
        },
        {
          "id": "MDRW2100030039.1.13",
          "form": "한국보시면",
          "original_form": "한국보시면",
          "speaker_id": "2",
          "time": "20210904 13:59"
        },
        {
          "id": "MDRW2100030039.1.14",
