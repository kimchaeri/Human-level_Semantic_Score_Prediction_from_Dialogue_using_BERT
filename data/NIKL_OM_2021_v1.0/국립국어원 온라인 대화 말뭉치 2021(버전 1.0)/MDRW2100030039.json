          "original_form": "응원하시는 팀은...?",
          "speaker_id": "2",
          "time": "20210904 13:59"
        },
        {
          "id": "MDRW2100030039.1.15",
          "form": "저는 야구는",
          "original_form": "저는 야구는",
          "speaker_id": "2",
          "time": "20210904 13:59"
        },
        {
          "id": "MDRW2100030039.1.16",
          "form": "한화요",
          "original_form": "한화요",
          "speaker_id": "1",
          "time": "20210904 13:59"
        },
