      "utterance": [
        {
          "id": "MDRW2100020031.1.1",
          "form": "결혼했어요?",
          "original_form": "결혼했어요?",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020031.1.2",
          "form": "아뇨 아직 안했어용",
          "original_form": "아뇨 아직 안했어용",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020031.1.3",
          "form": "언제쯤?",
          "original_form": "언제쯤?",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020031.1.4",
          "form": "모르겠어요",
          "original_form": "모르겠어요",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020031.1.5",
          "form": "생각이 없어용 아직",
          "original_form": "생각이 없어용 아직",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020031.1.6",
          "form": "인연이있죠",
          "original_form": "인연이있죠",
          "speaker_id": "1",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020031.1.7",
          "form": "인연을 믿고 기다리기엔 좀 그런거같아요 ㅠㅠ",
          "original_form": "인연을 믿고 기다리기엔 좀 그런거같아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020031.1.8",
          "form": "그래더 반드시 옵니다",
          "original_form": "그래더 반드시 옵니다",
          "speaker_id": "1",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020031.1.9",
          "form": "반드시 올려나요 흠 ㅠㅠ",
          "original_form": "반드시 올려나요 흠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020031.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210827 23:40"
        },
        {
          "id": "MDRW2100020031.1.11",
          "form": "인연이라 생각해도 연애를 하면할수록 아닌거같단 생각을 참 많이해서 그런가",
          "original_form": "인연이라 생각해도 연애를 하면할수록 아닌거같단 생각을 참 많이해서 그런가",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020031.1.12",
          "form": "인연에 큰 의미가 안드네용 ㅠ",
          "original_form": "인연에 큰 의미가 안드네용 ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:41"
        },
        {
          "id": "MDRW2100020031.1.13",
          "form": "나이가 들면 더 그래여",
          "original_form": "나이가 들면 더 그래여",
          "speaker_id": "1",
          "time": "20210827 23:43"
        },
        {
          "id": "MDRW2100020031.1.14",
          "form": "그런가유 ㅠ 클났네요",
          "original_form": "그런가유 ㅠ 클났네요",
          "speaker_id": "2",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020031.1.15",
          "form": "어릴때 후딱",
          "original_form": "어릴때 후딱",
