      "utterance": [
        {
          "id": "MDRW2100011105.1.1",
          "form": "밤에 에어컨 안켜고 자니 너무 좋아요",
          "original_form": "밤에 에어컨 안켜고 자니 너무 좋아요",
          "speaker_id": "2",
          "time": "20210824 01:18"
        },
        {
          "id": "MDRW2100011105.1.2",
          "form": "맞아여 더우면 잠자기 진짜 힘든데",
          "original_form": "맞아여 더우면 잠자기 진짜 힘든데",
          "speaker_id": "1",
          "time": "20210824 01:54"
        },
        {
          "id": "MDRW2100011105.1.3",
          "form": "요즘은 찝찝하지도 않고 좋네요",
          "original_form": "요즘은 찝찝하지도 않고 좋네요",
          "speaker_id": "1",
          "time": "20210824 01:54"
        },
        {
          "id": "MDRW2100011105.1.4",
          "form": "아침도 너무 선선하고 좋네요",
          "original_form": "아침도 너무 선선하고 좋네요",
          "speaker_id": "2",
          "time": "20210824 09:23"
        },
        {
          "id": "MDRW2100011105.1.5",
          "form": "오늘 비 너무 많이와서 습도가 너무 높앗네요 ㅋㅋ",
          "original_form": "오늘 비 너무 많이와서 습도가 너무 높앗네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:27"
        },
        {
          "id": "MDRW2100011105.1.6",
          "form": "지금도요..ㅡ 부슬부슬 끝없이 오네요",
          "original_form": "지금도요..ㅡ 부슬부슬 끝없이 오네요",
          "speaker_id": "2",
          "time": "20210824 20:28"
        },
        {
          "id": "MDRW2100011105.1.7",
          "form": "그러게요 중간중간 멈췄다가 다시내리고요 ㅠ",
          "original_form": "그러게요 중간중간 멈췄다가 다시내리고요 ㅠ",
          "speaker_id": "1",
          "time": "20210824 20:32"
        },
        {
          "id": "MDRW2100011105.1.8",
          "form": "언제까지 온대요?",
          "original_form": "언제까지 온대요?",
          "speaker_id": "2",
          "time": "20210824 20:35"
        },
        {
          "id": "MDRW2100011105.1.9",
          "form": "모르겟는데 아마 이번주 계속 오지않을까요",
          "original_form": "모르겟는데 아마 이번주 계속 오지않을까요",
          "speaker_id": "1",
          "time": "20210824 20:39"
        },
        {
          "id": "MDRW2100011105.1.10",
          "form": "정말 가을 장마군요",
          "original_form": "정말 가을 장마군요",
          "speaker_id": "2",
          "time": "20210824 21:03"
        },
        {
          "id": "MDRW2100011105.1.11",
          "form": "그러게용..",
          "original_form": "그러게용..",
          "speaker_id": "1",
          "time": "20210824 21:33"
        },
        {
          "id": "MDRW2100011105.1.12",
          "form": "곰팡이가 좋아할 날씨입니다",
          "original_form": "곰팡이가 좋아할 날씨입니다",
          "speaker_id": "2",
          "time": "20210824 22:03"
        },
        {
          "id": "MDRW2100011105.1.13",
          "form": "악.. 좀 그렇군요 ㅋㅋ",
          "original_form": "악.. 좀 그렇군요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:16"
        },
        {
          "id": "MDRW2100011105.1.14",
          "form": "네 ㅎㅎ 곰팡이 너무 싫어요",
          "original_form": "네 ㅎㅎ 곰팡이 너무 싫어요",
          "speaker_id": "2",
          "time": "20210824 22:44"
        },
        {
          "id": "MDRW2100011105.1.15",
          "form": "저도요 곰팡이 싫어해요 ㅋㅋ",
          "original_form": "저도요 곰팡이 싫어해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100011105.1.16",
          "form": "아 다 싫어하려나..ㅋㅋㅋㅋ",
          "original_form": "아 다 싫어하려나..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100011105.1.17",
          "form": "곰팡이는 진짜 좋아할수 없죠",
