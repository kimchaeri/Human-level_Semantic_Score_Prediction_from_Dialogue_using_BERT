      "utterance": [
        {
          "id": "MDRW2100032978.1.1",
          "form": "등산하면 단풍을 볼 수 있지요",
          "original_form": "등산하면 단풍을 볼 수 있지요",
          "speaker_id": "2",
          "time": "20210906 11:35"
        },
        {
          "id": "MDRW2100032978.1.2",
          "form": "가로수도 붉게 물들어서 봄보다 가을풍경이 더 좋아요",
          "original_form": "가로수도 붉게 물들어서 봄보다 가을풍경이 더 좋아요",
          "speaker_id": "1",
          "time": "20210906 11:36"
        },
        {
          "id": "MDRW2100032978.1.3",
          "form": "좀 있으면 낙엽이 떨어지기 했어요",
          "original_form": "좀 있으면 낙엽이 떨어지기 했어요",
          "speaker_id": "2",
          "time": "20210906 11:36"
        },
        {
          "id": "MDRW2100032978.1.4",
          "form": "한라산을 등반하면 예쁘겠어요",
          "original_form": "한라산을 등반하면 예쁘겠어요",
          "speaker_id": "1",
          "time": "20210906 11:36"
        },
        {
          "id": "MDRW2100032978.1.5",
          "form": "산이 주는 맑은 기분이 좋아요",
          "original_form": "산이 주는 맑은 기분이 좋아요",
          "speaker_id": "2",
          "time": "20210906 11:36"
        },
        {
          "id": "MDRW2100032978.1.6",
          "form": "바다보다는 산의 선선함이 좋지요",
          "original_form": "바다보다는 산의 선선함이 좋지요",
          "speaker_id": "1",
          "time": "20210906 11:37"
        },
        {
          "id": "MDRW2100032978.1.7",
          "form": "봄에는 등산하기 좋고 요",
          "original_form": "봄에는 등산하기 좋고 요",
          "speaker_id": "2",
          "time": "20210906 11:37"
        },
        {
          "id": "MDRW2100032978.1.8",
          "form": "한국의 사계절이 있는것이 좋은편이에요",
          "original_form": "한국의 사계절이 있는것이 좋은편이에요",
          "speaker_id": "1",
          "time": "20210906 11:37"
        },
        {
          "id": "MDRW2100032978.1.9",
          "form": "이런 나라가 흔치 않아요",
          "original_form": "이런 나라가 흔치 않아요",
          "speaker_id": "2",
          "time": "20210906 11:37"
        },
        {
          "id": "MDRW2100032978.1.10",
          "form": "미세먼지만 없다면 더 좋지요",
          "original_form": "미세먼지만 없다면 더 좋지요",
          "speaker_id": "1",
          "time": "20210906 11:37"
        },
        {
          "id": "MDRW2100032978.1.11",
          "form": "공기가 깨끗해요",
          "original_form": "공기가 깨끗해요",
          "speaker_id": "2",
          "time": "20210906 11:37"
        },
        {
          "id": "MDRW2100032978.1.12",
          "form": "코로나 이후 자연이 많이 정화되었어요",
          "original_form": "코로나 이후 자연이 많이 정화되었어요",
          "speaker_id": "1",
          "time": "20210906 11:38"
        },
        {
          "id": "MDRW2100032978.1.13",
          "form": "지금 계절은 참 이쁘죠",
          "original_form": "지금 계절은 참 이쁘죠",
          "speaker_id": "2",
          "time": "20210906 11:38"
        },
        {
          "id": "MDRW2100032978.1.14",
          "form": "계절에 맞는 옷차림으로 건강을 지켜야죠",
          "original_form": "계절에 맞는 옷차림으로 건강을 지켜야죠",
          "speaker_id": "1",
          "time": "20210906 11:38"
        },
        {
          "id": "MDRW2100032978.1.15",
          "form": "날씨가 제법 선선하지 않나요",
