      "utterance": [
        {
          "id": "MDRW2100032134.1.1",
          "form": "국민지원금 신청담주부터더라",
          "original_form": "국민지원금 신청담주부터더라",
          "speaker_id": "2",
          "time": "20210905 13:24"
        },
        {
          "id": "MDRW2100032134.1.2",
          "form": "우리는 수욜임",
          "original_form": "우리는 수욜임",
          "speaker_id": "1",
          "time": "20210905 13:25"
        },
        {
          "id": "MDRW2100032134.1.3",
          "form": "이번에 받을거 스벅이나 이케아 이런데는 못쓰나봐",
          "original_form": "이번에 받을거 스벅이나 이케아 이런데는 못쓰나봐",
          "speaker_id": "2",
          "time": "20210905 13:25"
        },
        {
          "id": "MDRW2100032134.1.4",
          "form": "근데 저번에 받은것도 그렇지않았나...?",
          "original_form": "근데 저번에 받은것도 그렇지않았나...?",
          "speaker_id": "1",
          "time": "20210905 13:27"
        },
        {
          "id": "MDRW2100032134.1.5",
          "form": "저번에도 지역에서만 쓰게했던거 같은데 기억이 가물가물함",
          "original_form": "저번에도 지역에서만 쓰게했던거 같은데 기억이 가물가물함",
          "speaker_id": "2",
          "time": "20210905 13:31"
        },
        {
          "id": "MDRW2100032134.1.6",
          "form": "아숩긴한데 스버이나 이케아 에서 안써도...충분히 쓸돈이라",
          "original_form": "아숩긴한데 스버이나 이케아 에서 안써도...충분히 쓸돈이라",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100032134.1.7",
          "form": "별생각이 없다",
          "original_form": "별생각이 없다",
          "speaker_id": "1",
          "time": "20210905 13:32"
        },
        {
          "id": "MDRW2100032134.1.8",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210905 13:37"
        },
        {
          "id": "MDRW2100032134.1.9",
          "form": "나는 식당에서 다쓸까봐",
          "original_form": "나는 식당에서 다쓸까봐",
          "speaker_id": "2",
          "time": "20210905 13:39"
        },
        {
          "id": "MDRW2100032134.1.10",
          "form": "나도ㅋㅋㄲㄲ먹는데다 올인할생각...",
          "original_form": "나도ㅋㅋㄲㄲ먹는데다 올인할생각...",
          "speaker_id": "1",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100032134.1.11",
          "form": "부모님은 안경사신대",
          "original_form": "부모님은 안경사신대",
          "speaker_id": "2",
          "time": "20210905 13:40"
        },
        {
          "id": "MDRW2100032134.1.12",
          "form": "나도 안경바꿔야하는데",
          "original_form": "나도 안경바꿔야하는데",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032134.1.13",
          "form": "안경보러갈까...으뜸도 될려나?",
          "original_form": "안경보러갈까...으뜸도 될려나?",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032134.1.14",
          "form": "될걸....?",
          "original_form": "될걸....?",
          "speaker_id": "2",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032134.1.15",
          "form": "프렌차이즈라 안될려나...?",
          "original_form": "프렌차이즈라 안될려나...?",
          "speaker_id": "2",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032134.1.16",
          "form": "나도 그래서 고민....",
          "original_form": "나도 그래서 고민....",
          "speaker_id": "1",
          "time": "20210905 13:41"
        },
        {
          "id": "MDRW2100032134.1.17",
          "form": "근데 스벅 이케아 못쓰면",
          "original_form": "근데 스벅 이케아 못쓰면",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100032134.1.18",
          "form": "개인카페도 사람많아지겠다",
          "original_form": "개인카페도 사람많아지겠다",
          "speaker_id": "1",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100032134.1.19",
          "form": "글네.....",
          "original_form": "글네.....",
          "speaker_id": "2",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100032134.1.20",
          "form": "테이크아웃해야쓰겠다",
          "original_form": "테이크아웃해야쓰겠다",
          "speaker_id": "2",
          "time": "20210905 13:42"
        },
        {
          "id": "MDRW2100032134.1.21",
          "form": "사람 피해...최대한 피해다녀",
          "original_form": "사람 피해...최대한 피해다녀",
          "speaker_id": "1",
          "time": "20210905 13:43"
        }
      ]
