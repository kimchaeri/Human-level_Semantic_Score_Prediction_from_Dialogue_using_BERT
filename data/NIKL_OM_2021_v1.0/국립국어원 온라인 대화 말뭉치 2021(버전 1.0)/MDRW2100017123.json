      "utterance": [
        {
          "id": "MDRW2100017123.1.1",
          "form": "네 즐겨이용해요",
          "original_form": "네 즐겨이용해요",
          "speaker_id": "2",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017123.1.2",
          "form": "어느쇼핑몰 이용하세요 주로??",
          "original_form": "어느쇼핑몰 이용하세요 주로??",
          "speaker_id": "1",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017123.1.3",
          "form": "저는 11번가요",
          "original_form": "저는 11번가요",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017123.1.4",
          "form": "11번가 저렴하죠",
          "original_form": "11번가 저렴하죠",
          "speaker_id": "1",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017123.1.5",
          "form": "쿠팡도 가끔 이용해요",
          "original_form": "쿠팡도 가끔 이용해요",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017123.1.6",
          "form": "쿠팡이 최저가라 그러더라구여",
          "original_form": "쿠팡이 최저가라 그러더라구여",
          "speaker_id": "1",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017123.1.7",
          "form": "어디 이용하세요?",
          "original_form": "어디 이용하세요?",
          "speaker_id": "2",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017123.1.8",
          "form": "저는 그냥 네이버 최저가로 보거나 쿠팡써요",
          "original_form": "저는 그냥 네이버 최저가로 보거나 쿠팡써요",
          "speaker_id": "1",
          "time": "20210826 17:31"
        },
        {
          "id": "MDRW2100017123.1.9",
          "form": "해외직구는 쿠팡이 정말 편하더라구요",
          "original_form": "해외직구는 쿠팡이 정말 편하더라구요",
          "speaker_id": "2",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100017123.1.10",
          "form": "맞아요 진짜 빨리오더라구요",
          "original_form": "맞아요 진짜 빨리오더라구요",
          "speaker_id": "1",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100017123.1.11",
          "form": "11번가는 십일절 행사할때 이것저것 사기 좋아서 써요",
          "original_form": "11번가는 십일절 행사할때 이것저것 사기 좋아서 써요",
          "speaker_id": "2",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017123.1.12",
          "form": "오 십일절은 11월11일 맞나요?",
          "original_form": "오 십일절은 11월11일 맞나요?",
          "speaker_id": "1",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017123.1.13",
          "form": "매달 11일로 알고잇어요",
          "original_form": "매달 11일로 알고잇어요",
