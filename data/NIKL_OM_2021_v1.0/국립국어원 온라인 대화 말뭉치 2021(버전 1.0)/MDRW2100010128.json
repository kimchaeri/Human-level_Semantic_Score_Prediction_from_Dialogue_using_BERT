      "utterance": [
        {
          "id": "MDRW2100010128.1.1",
          "form": "안녕하세요 대화 가능하신가요?",
          "original_form": "안녕하세요 대화 가능하신가요?",
          "speaker_id": "1",
          "time": "20210823 14:37"
        },
        {
          "id": "MDRW2100010128.1.2",
          "form": "네ㅎ",
          "original_form": "네ㅎ",
          "speaker_id": "2",
          "time": "20210823 14:37"
        },
        {
          "id": "MDRW2100010128.1.3",
          "form": "태풍온대서 그런지 비오네요",
          "original_form": "태풍온대서 그런지 비오네요",
          "speaker_id": "2",
          "time": "20210823 14:38"
        },
        {
          "id": "MDRW2100010128.1.4",
          "form": "거기도 비오나요?",
          "original_form": "거기도 비오나요?",
          "speaker_id": "2",
          "time": "20210823 14:38"
        },
        {
          "id": "MDRW2100010128.1.5",
          "form": "오전부터 왔다가 안 왔다가 하네요",
          "original_form": "오전부터 왔다가 안 왔다가 하네요",
          "speaker_id": "1",
          "time": "20210823 14:39"
        },
        {
          "id": "MDRW2100010128.1.6",
          "form": "많이 오시나요?",
          "original_form": "많이 오시나요?",
          "speaker_id": "1",
          "time": "20210823 14:39"
        },
        {
          "id": "MDRW2100010128.1.7",
          "form": "점심먹고나오니 비오더라구요",
          "original_form": "점심먹고나오니 비오더라구요",
          "speaker_id": "2",
          "time": "20210823 14:40"
        },
        {
          "id": "MDRW2100010128.1.8",
          "form": "많이오진 않네요",
          "original_form": "많이오진 않네요",
          "speaker_id": "2",
          "time": "20210823 14:40"
        },
        {
          "id": "MDRW2100010128.1.9",
          "form": "남부지방은 많이오는거같던데",
          "original_form": "남부지방은 많이오는거같던데",
          "speaker_id": "2",
          "time": "20210823 14:40"
        },
        {
          "id": "MDRW2100010128.1.10",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210823 14:41"
        },
        {
          "id": "MDRW2100010128.1.11",
          "form": "큰 피해 없었으면 좋겠어요",
          "original_form": "큰 피해 없었으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210823 14:41"
        },
        {
          "id": "MDRW2100010128.1.12",
          "form": "제주도 많이오나봐요",
          "original_form": "제주도 많이오나봐요",
          "speaker_id": "2",
          "time": "20210823 14:41"
        },
        {
          "id": "MDRW2100010128.1.13",
          "form": "그래도 비와서그런지 선선해져서 좋네요",
          "original_form": "그래도 비와서그런지 선선해져서 좋네요",
          "speaker_id": "2",
          "time": "20210823 14:41"
        },
        {
          "id": "MDRW2100010128.1.14",
          "form": "네 이번 장마 끝나면 가을일 거 같아요",
          "original_form": "네 이번 장마 끝나면 가을일 거 같아요",
          "speaker_id": "1",
          "time": "20210823 14:42"
        },
        {
          "id": "MDRW2100010128.1.15",
          "form": "가을 좋아하세요?",
          "original_form": "가을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 14:42"
        },
        {
          "id": "MDRW2100010128.1.16",
          "form": "가을 제일좋아해요^^",
          "original_form": "가을 제일좋아해요^^",
          "speaker_id": "2",
          "time": "20210823 14:42"
        },
        {
          "id": "MDRW2100010128.1.17",
          "form": "저도요! 시원하고 습하지 않아서 제일 좋아요",
          "original_form": "저도요! 시원하고 습하지 않아서 제일 좋아요",
          "speaker_id": "1",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100010128.1.18",
          "form": "더위나 추위 많이 타시나요?",
          "original_form": "더위나 추위 많이 타시나요?",
          "speaker_id": "1",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100010128.1.19",
          "form": "좀 길면 좋겠는데 이러다 훅 겨울 와서 아쉬워요",
          "original_form": "좀 길면 좋겠는데 이러다 훅 겨울 와서 아쉬워요",
          "speaker_id": "2",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100010128.1.20",
          "form": "맞아요ㅠㅠ 너무 짧아요",
          "original_form": "맞아요ㅠㅠ 너무 짧아요",
          "speaker_id": "1",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100010128.1.21",
          "form": "네 둘타 타는 이상한체질ㅋ",
          "original_form": "네 둘타 타는 이상한체질ㅋ",
          "speaker_id": "2",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100010128.1.22",
          "form": "여름보단 겨울이 낫지 싶어요",
          "original_form": "여름보단 겨울이 낫지 싶어요",
          "speaker_id": "2",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100010128.1.23",
          "form": "파르페님은요?",
          "original_form": "파르페님은요?",
          "speaker_id": "2",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100010128.1.24",
          "form": "매번 고생하시네요ㅠㅠ 전 더위를 더 많이 타서 여름에 정말 죽는 줄 알았어요",
          "original_form": "매번 고생하시네요ㅠㅠ 전 더위를 더 많이 타서 여름에 정말 죽는 줄 알았어요",
          "speaker_id": "1",
          "time": "20210823 14:43"
        },
        {
          "id": "MDRW2100010128.1.25",
          "form": "이번에 너무 덥더라고요",
          "original_form": "이번에 너무 덥더라고요",
          "speaker_id": "1",
          "time": "20210823 14:43"
        }
      ]
    }
