        {
          "id": "MDRW2100022186.1.11",
          "form": "전",
          "original_form": "전",
          "speaker_id": "1",
          "time": "20210829 11:09"
        },
        {
          "id": "MDRW2100022186.1.12",
          "form": "어제\r\n빵\r\n사놓은게\r\n있어서",
          "original_form": "어제\r\n빵\r\n사놓은게\r\n있어서",
          "speaker_id": "1",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100022186.1.13",
          "form": "그거 먹으려구요",
          "original_form": "그거 먹으려구요",
          "speaker_id": "1",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100022186.1.14",
          "form": "빵좋아하시나요",
          "original_form": "빵좋아하시나요",
          "speaker_id": "1",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100022186.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ네 저 빵 좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ네 저 빵 좋아해요",
          "speaker_id": "2",
          "time": "20210829 11:11"
        },
        {
          "id": "MDRW2100022186.1.16",
          "form": "무슨 빵 사놓으셨나요??",
          "original_form": "무슨 빵 사놓으셨나요??",
          "speaker_id": "2",
          "time": "20210829 11:11"
        },
        {
          "id": "MDRW2100022186.1.17",
          "form": "저 요기요행사하길래",
          "original_form": "저 요기요행사하길래",
          "speaker_id": "1",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100022186.1.18",
          "form": "파리바게트가서 열심히 쓸어왔습니다",
          "original_form": "파리바게트가서 열심히 쓸어왔습니다",
          "speaker_id": "1",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100022186.1.19",
          "form": "뽀드득 그릴소시지랑 모카빵이랑 뀐아망 사왔어요",
          "original_form": "뽀드득 그릴소시지랑 모카빵이랑 뀐아망 사왔어요",
          "speaker_id": "1",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100022186.1.20",
          "form": "파리파게트에서 뭐 좋아하세요",
          "original_form": "파리파게트에서 뭐 좋아하세요",
          "speaker_id": "1",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100022186.1.21",
          "form": "저는 과일 들어간 파이 좋아해요",
          "original_form": "저는 과일 들어간 파이 좋아해요",
          "speaker_id": "2",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100022186.1.22",
          "form": "name1님은 어떤거 좋아하시나요??",
          "original_form": "&name1&님은 어떤거 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210829 11:16"
        },
        {
