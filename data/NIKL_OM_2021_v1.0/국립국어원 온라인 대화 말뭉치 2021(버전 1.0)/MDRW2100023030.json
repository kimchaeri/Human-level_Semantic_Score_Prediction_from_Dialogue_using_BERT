      "utterance": [
        {
          "id": "MDRW2100023030.1.1",
          "form": "응 난 매운거 진짜 좋아해",
          "original_form": "응 난 매운거 진짜 좋아해",
          "speaker_id": "1",
          "time": "20210829 20:45"
        },
        {
          "id": "MDRW2100023030.1.2",
          "form": "컵밥 요즘도 먹니",
          "original_form": "컵밥 요즘도 먹니",
          "speaker_id": "2",
          "time": "20210829 20:46"
        },
        {
          "id": "MDRW2100023030.1.3",
          "form": "먹고싶은데 컵밥집이 주변에 없어서 못 먹고 있어",
          "original_form": "먹고싶은데 컵밥집이 주변에 없어서 못 먹고 있어",
          "speaker_id": "1",
          "time": "20210829 20:46"
        },
        {
          "id": "MDRW2100023030.1.4",
          "form": "너무 슬퍼 흑흑",
          "original_form": "너무 슬퍼 흑흑",
          "speaker_id": "1",
          "time": "20210829 20:46"
        },
        {
          "id": "MDRW2100023030.1.5",
          "form": "다행이다",
          "original_form": "다행이다",
          "speaker_id": "2",
          "time": "20210829 20:48"
        },
        {
          "id": "MDRW2100023030.1.6",
          "form": "니 건강에는 도움이 될테니",
          "original_form": "니 건강에는 도움이 될테니",
          "speaker_id": "2",
          "time": "20210829 20:48"
        },
        {
          "id": "MDRW2100023030.1.7",
          "form": "매운거 위에 안좋잖어",
          "original_form": "매운거 위에 안좋잖어",
          "speaker_id": "2",
          "time": "20210829 20:49"
        },
        {
          "id": "MDRW2100023030.1.8",
          "form": "맞아 좋은 게 좋은 거지 하고 긍정적으로 생각중이야",
          "original_form": "맞아 좋은 게 좋은 거지 하고 긍정적으로 생각중이야",
          "speaker_id": "1",
          "time": "20210829 20:49"
        },
        {
          "id": "MDRW2100023030.1.9",
          "form": "요즘 매운거 줄이려고 노력중이거든",
          "original_form": "요즘 매운거 줄이려고 노력중이거든",
          "speaker_id": "1",
          "time": "20210829 20:49"
        },
        {
          "id": "MDRW2100023030.1.10",
          "form": "칭찬해줘",
          "original_form": "칭찬해줘",
          "speaker_id": "1",
          "time": "20210829 20:49"
        },
        {
          "id": "MDRW2100023030.1.11",
          "form": "진짜 대박 노력하는게 느껴져",
          "original_form": "진짜 대박 노력하는게 느껴져",
          "speaker_id": "2",
          "time": "20210829 20:50"
        },
        {
          "id": "MDRW2100023030.1.12",
          "form": "먹고싶으내 참는게 제일 힘들거든",
          "original_form": "먹고싶으내 참는게 제일 힘들거든",
          "speaker_id": "2",
          "time": "20210829 20:50"
        },
        {
          "id": "MDRW2100023030.1.13",
          "form": "고마워",
          "original_form": "고마워",
          "speaker_id": "1",
          "time": "20210829 20:50"
        },
        {
          "id": "MDRW2100023030.1.14",
          "form": "역시 칭찬 들으면 기분이 좋아",
          "original_form": "역시 칭찬 들으면 기분이 좋아",
          "speaker_id": "1",
          "time": "20210829 20:50"
        },
        {
          "id": "MDRW2100023030.1.15",
          "form": "넌 지금 먹고싶은 게 있어?",
          "original_form": "넌 지금 먹고싶은 게 있어?",
          "speaker_id": "1",
          "time": "20210829 20:51"
        },
        {
          "id": "MDRW2100023030.1.16",
          "form": "난 마라탕이 먹고싶어",
          "original_form": "난 마라탕이 먹고싶어",
          "speaker_id": "2",
          "time": "20210829 20:51"
        },
        {
          "id": "MDRW2100023030.1.17",
          "form": "나트륨땜에 참지만",
          "original_form": "나트륨땜에 참지만",
          "speaker_id": "2",
          "time": "20210829 20:51"
        },
        {
          "id": "MDRW2100023030.1.18",
          "form": "마라탕 은근 중독적이더라",
          "original_form": "마라탕 은근 중독적이더라",
          "speaker_id": "1",
          "time": "20210829 20:52"
        },
        {
          "id": "MDRW2100023030.1.19",
          "form": "알싸한 게 좋아",
          "original_form": "알싸한 게 좋아",
          "speaker_id": "1",
          "time": "20210829 20:52"
        },
        {
          "id": "MDRW2100023030.1.20",
          "form": "마라탕 무슨 재료 싫어해?",
          "original_form": "마라탕 무슨 재료 싫어해?",
          "speaker_id": "1",
          "time": "20210829 20:52"
        },
        {
          "id": "MDRW2100023030.1.21",
          "form": "두부랑",
          "original_form": "두부랑",
          "speaker_id": "2",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100023030.1.22",
          "form": "건두부랑ㄷㅈ",
          "original_form": "건두부랑ㄷㅈ",
          "speaker_id": "2",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100023030.1.23",
          "form": "연근",
          "original_form": "연근",
          "speaker_id": "2",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100023030.1.24",
          "form": "건두부식감이 고무같아",
          "original_form": "건두부식감이 고무같아",
          "speaker_id": "2",
          "time": "20210829 20:54"
        },
        {
          "id": "MDRW2100023030.1.25",
          "form": "헉 두부를 안 좋아하다니",
          "original_form": "헉 두부를 안 좋아하다니",
          "speaker_id": "1",
          "time": "20210829 20:56"
        },
        {
          "id": "MDRW2100023030.1.26",
          "form": "그럼 김치찌개 들어가는 두부도 싫어해?",
          "original_form": "그럼 김치찌개 들어가는 두부도 싫어해?",
          "speaker_id": "1",
          "time": "20210829 20:56"
        },
        {
          "id": "MDRW2100023030.1.27",
          "form": "두부는 먹어",
          "original_form": "두부는 먹어",
          "speaker_id": "2",
          "time": "20210829 21:00"
        },
        {
          "id": "MDRW2100023030.1.28",
          "form": "건두부가 이상해",
          "original_form": "건두부가 이상해",
          "speaker_id": "2",
          "time": "20210829 21:00"
        },
        {
          "id": "MDRW2100023030.1.29",
          "form": "그렇구나 난 건두부도 나쁘지 않던데 취향타는 재료였구만",
          "original_form": "그렇구나 난 건두부도 나쁘지 않던데 취향타는 재료였구만",
          "speaker_id": "1",
          "time": "20210829 21:01"
        },
        {
          "id": "MDRW2100023030.1.30",
          "form": "건두부향도 이상하더라",
          "original_form": "건두부향도 이상하더라",
          "speaker_id": "2",
          "time": "20210829 21:01"
        },
        {
          "id": "MDRW2100023030.1.31",
          "form": "그리고 어묵볼가같은것도 싫어",
          "original_form": "그리고 어묵볼가같은것도 싫어",
          "speaker_id": "2",
          "time": "20210829 21:02"
        },
        {
          "id": "MDRW2100023030.1.32",
          "form": "그 분모자가 난 제일 맛있더라",
          "original_form": "그 분모자가 난 제일 맛있더라",
          "speaker_id": "2",
          "time": "20210829 21:02"
        }
      ]
