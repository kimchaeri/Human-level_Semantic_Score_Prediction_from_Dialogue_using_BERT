      "utterance": [
        {
          "id": "MDRW2100015873.1.1",
          "form": "할인소식 뭐없나요",
          "original_form": "할인소식 뭐없나요",
          "speaker_id": "2",
          "time": "20210826 02:09"
        },
        {
          "id": "MDRW2100015873.1.2",
          "form": "나어제 쌀삼 ㅋㅋㅋ",
          "original_form": "나어제 쌀삼 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:10"
        },
        {
          "id": "MDRW2100015873.1.3",
          "form": "쌀?",
          "original_form": "쌀?",
          "speaker_id": "2",
          "time": "20210826 02:11"
        },
        {
          "id": "MDRW2100015873.1.4",
          "form": "ㅇㅇ 쌀할인하길래 ㅋㅋ 이십키로 사만팔천",
          "original_form": "ㅇㅇ 쌀할인하길래 ㅋㅋ 이십키로 사만팔천",
          "speaker_id": "1",
          "time": "20210826 02:11"
        },
        {
          "id": "MDRW2100015873.1.5",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210826 02:11"
        },
        {
          "id": "MDRW2100015873.1.6",
          "form": "쌀괜찮대??",
          "original_form": "쌀괜찮대??",
          "speaker_id": "2",
          "time": "20210826 02:12"
        },
        {
          "id": "MDRW2100015873.1.7",
          "form": "신동진 단일미 ㅋㅋ 요즘 쌀값 비싸잖어 우리집은 쌀많이쓰니까 사뒀지",
          "original_form": "신동진 단일미 ㅋㅋ 요즘 쌀값 비싸잖어 우리집은 쌀많이쓰니까 사뒀지",
          "speaker_id": "1",
          "time": "20210826 02:12"
        },
        {
          "id": "MDRW2100015873.1.8",
          "form": "신공진쌀 좋지",
          "original_form": "신공진쌀 좋지",
          "speaker_id": "2",
          "time": "20210826 02:13"
        },
        {
          "id": "MDRW2100015873.1.9",
          "form": "우리집도 쌀바꿔야되는데",
          "original_form": "우리집도 쌀바꿔야되는데",
          "speaker_id": "2",
          "time": "20210826 02:13"
        },
        {
          "id": "MDRW2100015873.1.10",
          "form": "월향미 섞어쓰는데",
          "original_form": "월향미 섞어쓰는데",
          "speaker_id": "2",
          "time": "20210826 02:14"
        },
        {
          "id": "MDRW2100015873.1.11",
          "form": "쌀을 바꿔?",
          "original_form": "쌀을 바꿔?",
          "speaker_id": "1",
          "time": "20210826 02:14"
        },
        {
          "id": "MDRW2100015873.1.12",
          "form": "월향미 향좋은거 아님? 골든퀸처럼?",
          "original_form": "월향미 향좋은거 아님? 골든퀸처럼?",
          "speaker_id": "1",
          "time": "20210826 02:14"
        },
        {
          "id": "MDRW2100015873.1.13",
          "form": "ㅇㅇ쌀집에서 신동진쌀 사는데",
          "original_form": "ㅇㅇ쌀집에서 신동진쌀 사는데",
          "speaker_id": "2",
          "time": "20210826 02:14"
        },
        {
          "id": "MDRW2100015873.1.14",
          "form": "쌀알이작아",
          "original_form": "쌀알이작아",
          "speaker_id": "2",
          "time": "20210826 02:14"
        },
        {
          "id": "MDRW2100015873.1.15",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "2",
          "time": "20210826 02:14"
        },
        {
          "id": "MDRW2100015873.1.16",
          "form": "약간복불복인거 같아 쌀알은",
          "original_form": "약간복불복인거 같아 쌀알은",
          "speaker_id": "1",
          "time": "20210826 02:15"
        },
        {
          "id": "MDRW2100015873.1.17",
          "form": "늘 같은거 사도 조금씩 다를때가 있어라",
          "original_form": "늘 같은거 사도 조금씩 다를때가 있어라",
          "speaker_id": "1",
          "time": "20210826 02:15"
        },
        {
          "id": "MDRW2100015873.1.18",
          "form": "월향미로만 하면 향이 너무 세서",
          "original_form": "월향미로만 하면 향이 너무 세서",
          "speaker_id": "2",
          "time": "20210826 02:15"
        },
        {
          "id": "MDRW2100015873.1.19",
          "form": "향좋지않아? ㅋㅋㅋ",
          "original_form": "향좋지않아? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:15"
        },
        {
          "id": "MDRW2100015873.1.20",
          "form": "아빠가 싫대ㅜㅜ",
          "original_form": "아빠가 싫대ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 02:16"
        },
        {
          "id": "MDRW2100015873.1.21",
          "form": "섞으면 몰라ㅋㅋㅋ",
          "original_form": "섞으면 몰라ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:17"
        },
        {
          "id": "MDRW2100015873.1.22",
          "form": "향미로 만든 쌀칩 맛있더라 컬리에서사봄",
          "original_form": "향미로 만든 쌀칩 맛있더라 컬리에서사봄",
          "speaker_id": "1",
          "time": "20210826 02:17"
        },
        {
          "id": "MDRW2100015873.1.23",
          "form": "애들먹는 키크는쌀알아?!",
          "original_form": "애들먹는 키크는쌀알아?!",
          "speaker_id": "2",
          "time": "20210826 02:18"
        },
        {
          "id": "MDRW2100015873.1.24",
          "form": "들어는봤어",
          "original_form": "들어는봤어",
          "speaker_id": "1",
          "time": "20210826 02:19"
        },
        {
          "id": "MDRW2100015873.1.25",
          "form": "많이먹이기는한가보다ㄷㄷ",
          "original_form": "많이먹이기는한가보다ㄷㄷ",
          "speaker_id": "2",
          "time": "20210826 02:19"
        }
