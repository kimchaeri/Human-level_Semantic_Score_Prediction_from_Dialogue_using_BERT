      "utterance": [
        {
          "id": "MDRW2100020263.1.1",
          "form": "집꾸미기만 해도",
          "original_form": "집꾸미기만 해도",
          "speaker_id": "2",
          "time": "20210828 00:48"
        },
        {
          "id": "MDRW2100020263.1.2",
          "form": "1년은 하겟능걸",
          "original_form": "1년은 하겟능걸",
          "speaker_id": "2",
          "time": "20210828 00:48"
        },
        {
          "id": "MDRW2100020263.1.3",
          "form": "대박이네",
          "original_form": "대박이네",
          "speaker_id": "1",
          "time": "20210828 00:48"
        },
        {
          "id": "MDRW2100020263.1.4",
          "form": "난 집꾸미는거앤",
          "original_form": "난 집꾸미는거앤",
          "speaker_id": "1",
          "time": "20210828 00:48"
        },
        {
          "id": "MDRW2100020263.1.5",
          "form": "관심이 전혀안가던데",
          "original_form": "관심이 전혀안가던데",
          "speaker_id": "1",
          "time": "20210828 00:48"
        },
        {
          "id": "MDRW2100020263.1.6",
          "form": "저도 막 꼼꼼히 꾸미는거말고",
          "original_form": "저도 막 꼼꼼히 꾸미는거말고",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020263.1.7",
          "form": "좋은가구 선택하는게",
          "original_form": "좋은가구 선택하는게",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020263.1.8",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020263.1.9",
          "form": "조화로운데 기능성이 좋은 가구들로",
          "original_form": "조화로운데 기능성이 좋은 가구들로",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020263.1.10",
          "form": "구성하고싶음",
          "original_form": "구성하고싶음",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020263.1.11",
          "form": "그리고 모니터 두개",
          "original_form": "그리고 모니터 두개",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020263.1.12",
          "form": "글고 굿즈존 만들어야함..",
          "original_form": "글고 굿즈존 만들어야함..",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020263.1.13",
          "form": "굴쿠나",
          "original_form": "굴쿠나",
          "speaker_id": "1",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020263.1.14",
          "form": "하 굿즈존",
          "original_form": "하 굿즈존",
          "speaker_id": "1",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020263.1.15",
          "form": "티비는 욕심 안나요",
          "original_form": "티비는 욕심 안나요",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020263.1.16",
          "form": "티비 옛날에 진짜많이봣는데",
          "original_form": "티비 옛날에 진짜많이봣는데",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020263.1.17",
          "form": "이제 안봄",
          "original_form": "이제 안봄",
          "speaker_id": "2",
          "time": "20210828 00:49"
        },
        {
          "id": "MDRW2100020263.1.18",
          "form": "ㄱㄴㄲ 나도",
          "original_form": "ㄱㄴㄲ 나도",
          "speaker_id": "1",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020263.1.19",
          "form": "스크린 빔프로젝터도 있음 좋겟네",
          "original_form": "스크린 빔프로젝터도 있음 좋겟네",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020263.1.20",
          "form": "티비는 안살거같다",
          "original_form": "티비는 안살거같다",
          "speaker_id": "1",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020263.1.21",
          "form": "카펫은 싫어요",
          "original_form": "카펫은 싫어요",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020263.1.22",
          "form": "더럽고 관리어려움",
          "original_form": "더럽고 관리어려움",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020263.1.23",
          "form": "오 신기하다",
          "original_form": "오 신기하다",
          "speaker_id": "1",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020263.1.24",
          "form": "카펫까지 생각하다니",
          "original_form": "카펫까지 생각하다니",
          "speaker_id": "1",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020263.1.25",
          "form": "커튼은 암막커튼을 한번 사보고싶네",
          "original_form": "커튼은 암막커튼을 한번 사보고싶네",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020263.1.26",
          "form": "쇼파는요",
          "original_form": "쇼파는요",
          "speaker_id": "1",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020263.1.27",
          "form": "별로면 당마에팔어",
          "original_form": "별로면 당마에팔어",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020263.1.28",
          "form": "쇼파.. 굳이? 싶지만",
          "original_form": "쇼파.. 굳이? 싶지만",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020263.1.29",
          "form": "산다면 가죽은 싫어요",
          "original_form": "산다면 가죽은 싫어요",
          "speaker_id": "2",
          "time": "20210828 00:50"
        },
        {
          "id": "MDRW2100020263.1.30",
          "form": "나도 가죽쇼파 싫다",
          "original_form": "나도 가죽쇼파 싫다",
          "speaker_id": "1",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020263.1.31",
          "form": "그리고 너무 푹 가라앉는거 말고",
          "original_form": "그리고 너무 푹 가라앉는거 말고",
          "speaker_id": "2",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020263.1.32",
          "form": "느낌이 구려",
          "original_form": "느낌이 구려",
          "speaker_id": "1",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020263.1.33",
          "form": "좀 적당히 단단하게 잦아주는",
          "original_form": "좀 적당히 단단하게 잦아주는",
          "speaker_id": "2",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020263.1.34",
          "form": "심플한 소파 샤고싶도",
          "original_form": "심플한 소파 샤고싶도",
          "speaker_id": "2",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020263.1.35",
          "form": "테이블은 무조건 네모",
          "original_form": "테이블은 무조건 네모",
          "speaker_id": "2",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020263.1.36",
          "form": "ㅋ",
          "original_form": "ㅋ",
          "speaker_id": "1",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020263.1.37",
          "form": "동그라미 테이블은 세상에서 없어져랴함",
          "original_form": "동그라미 테이블은 세상에서 없어져랴함",
          "speaker_id": "2",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020263.1.38",
          "form": "하 ㄹㅇ",
          "original_form": "하 ㄹㅇ",
          "speaker_id": "1",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020263.1.39",
          "form": "지구를 둘러보면 온통 네모난 것들뿐인데",
          "original_form": "지구를 둘러보면 온통 네모난 것들뿐인데",
          "speaker_id": "2",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020263.1.40",
          "form": "통그란거 왜있지",
          "original_form": "통그란거 왜있지",
          "speaker_id": "1",
          "time": "20210828 00:51"
        },
        {
          "id": "MDRW2100020263.1.41",
          "form": "그건 네모의꿈일지몰라",
          "original_form": "그건 네모의꿈일지몰라",
          "speaker_id": "2",
          "time": "20210828 00:51"
        }
      ]
    }
