      "utterance": [
        {
          "id": "MDRW2100046921.1.1",
          "form": "내가 항상 말했듯이",
          "original_form": "내가 항상 말했듯이",
          "speaker_id": "1",
          "time": "20211010 16:39"
        },
        {
          "id": "MDRW2100046921.1.2",
          "form": "40살 은퇴계획은 변함없다",
          "original_form": "40살 은퇴계획은 변함없다",
          "speaker_id": "1",
          "time": "20211010 16:39"
        },
        {
          "id": "MDRW2100046921.1.3",
          "form": "그게 나중에 알고보니 파이어족이라고 용어가 있더만",
          "original_form": "그게 나중에 알고보니 파이어족이라고 용어가 있더만",
          "speaker_id": "1",
          "time": "20211010 16:39"
        },
        {
          "id": "MDRW2100046921.1.4",
          "form": "난 요새 그래서",
          "original_form": "난 요새 그래서",
          "speaker_id": "2",
          "time": "20211010 16:39"
        },
        {
          "id": "MDRW2100046921.1.5",
          "form": "재테크 공부에",
          "original_form": "재테크 공부에",
          "speaker_id": "2",
          "time": "20211010 16:39"
        },
        {
          "id": "MDRW2100046921.1.6",
          "form": "한창이야",
          "original_form": "한창이야",
          "speaker_id": "2",
          "time": "20211010 16:39"
        },
        {
          "id": "MDRW2100046921.1.7",
          "form": "월급외에 부수적인 수입 올리는것에",
          "original_form": "월급외에 부수적인 수입 올리는것에",
          "speaker_id": "2",
          "time": "20211010 16:40"
        },
        {
          "id": "MDRW2100046921.1.8",
          "form": "열중해보려고",
          "original_form": "열중해보려고",
          "speaker_id": "2",
          "time": "20211010 16:40"
        },
        {
          "id": "MDRW2100046921.1.9",
          "form": "그게 진짜 어려운거일듯",
          "original_form": "그게 진짜 어려운거일듯",
          "speaker_id": "1",
          "time": "20211010 16:40"
        },
        {
          "id": "MDRW2100046921.1.10",
          "form": "진짜 한 30정도만 한달에 부수입 들어와도",
          "original_form": "진짜 한 30정도만 한달에 부수입 들어와도",
          "speaker_id": "1",
          "time": "20211010 16:40"
        },
        {
          "id": "MDRW2100046921.1.11",
          "form": "좀 숨통트일거같은데",
          "original_form": "좀 숨통트일거같은데",
          "speaker_id": "1",
          "time": "20211010 16:40"
        },
        {
          "id": "MDRW2100046921.1.12",
          "form": "넉넉하지",
          "original_form": "넉넉하지",
          "speaker_id": "2",
          "time": "20211010 16:40"
        },
        {
          "id": "MDRW2100046921.1.13",
          "form": "30이 지금 월급만큼 커지면",
          "original_form": "30이 지금 월급만큼 커지면",
          "speaker_id": "1",
          "time": "20211010 16:40"
        },
        {
          "id": "MDRW2100046921.1.14",
          "form": "가치투자 하는게 너무 어렵더라",
          "original_form": "가치투자 하는게 너무 어렵더라",
          "speaker_id": "2",
          "time": "20211010 16:40"
        },
        {
          "id": "MDRW2100046921.1.15",
          "form": "파이어족 해도되나",
          "original_form": "파이어족 해도되나",
          "speaker_id": "1",
          "time": "20211010 16:40"
        },
        {
          "id": "MDRW2100046921.1.16",
          "form": "그것보단 더 늘어야지 ㅎㅎ",
          "original_form": "그것보단 더 늘어야지 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211010 16:40"
        },
        {
          "id": "MDRW2100046921.1.17",
          "form": "2배 이상은 되야지",
          "original_form": "2배 이상은 되야지",
          "speaker_id": "2",
          "time": "20211010 16:40"
        },
        {
          "id": "MDRW2100046921.1.18",
          "form": "40 은퇴면",
          "original_form": "40 은퇴면",
          "speaker_id": "2",
          "time": "20211010 16:41"
        },
        {
          "id": "MDRW2100046921.1.19",
          "form": "월급 x 240개월 이",
          "original_form": "월급 x 240개월 이",
          "speaker_id": "1",
          "time": "20211010 16:41"
        },
        {
          "id": "MDRW2100046921.1.20",
          "form": "20년정도 근로소득 포기할 정도는",
          "original_form": "20년정도 근로소득 포기할 정도는",
          "speaker_id": "2",
          "time": "20211010 16:41"
        },
        {
          "id": "MDRW2100046921.1.21",
          "form": "되야하는데",
          "original_form": "되야하는데",
          "speaker_id": "2",
          "time": "20211010 16:41"
        },
        {
          "id": "MDRW2100046921.1.22",
          "form": "리스크 감수하려면 2배는 되야할듯",
          "original_form": "리스크 감수하려면 2배는 되야할듯",
          "speaker_id": "2",
          "time": "20211010 16:41"
        },
        {
          "id": "MDRW2100046921.1.23",
          "form": "기준이래 은퇴하는데",
          "original_form": "기준이래 은퇴하는데",
          "speaker_id": "1",
          "time": "20211010 16:41"
        },
        {
          "id": "MDRW2100046921.1.24",
          "form": "아그래?",
          "original_form": "아그래?",
          "speaker_id": "2",
          "time": "20211010 16:41"
        },
        {
          "id": "MDRW2100046921.1.25",
          "form": "물가상승대비 근로소득이 너무",
          "original_form": "물가상승대비 근로소득이 너무",
          "speaker_id": "2",
          "time": "20211010 16:41"
        },
        {
          "id": "MDRW2100046921.1.26",
          "form": "안늘어",
          "original_form": "안늘어",
          "speaker_id": "2",
          "time": "20211010 16:41"
        },
        {
          "id": "MDRW2100046921.1.27",
          "form": "물가 진짜",
          "original_form": "물가 진짜",
          "speaker_id": "1",
          "time": "20211010 16:42"
        },
        {
          "id": "MDRW2100046921.1.28",
          "form": "하늘로 치솟음",
          "original_form": "하늘로 치솟음",
          "speaker_id": "1",
          "time": "20211010 16:42"
        },
        {
          "id": "MDRW2100046921.1.29",
          "form": "매주 1회정도씩",
          "original_form": "매주 1회정도씩",
          "speaker_id": "2",
          "time": "20211010 16:42"
        },
        {
          "id": "MDRW2100046921.1.30",
          "form": "경제나 재테크 ㅡ에 관련돤",
          "original_form": "경제나 재테크 ㅡ에 관련돤",
          "speaker_id": "2",
          "time": "20211010 16:42"
        },
        {
          "id": "MDRW2100046921.1.31",
          "form": "토의를 해보자",
          "original_form": "토의를 해보자",
          "speaker_id": "2",
          "time": "20211010 16:42"
        },
        {
          "id": "MDRW2100046921.1.32",
          "form": "그게 진짜 준비지",
          "original_form": "그게 진짜 준비지",
          "speaker_id": "1",
          "time": "20211010 16:42"
        },
        {
          "id": "MDRW2100046921.1.33",
          "form": "스터디를 들어갈까",
          "original_form": "스터디를 들어갈까",
          "speaker_id": "1",
          "time": "20211010 16:42"
        },
        {
          "id": "MDRW2100046921.1.34",
          "form": "스터디 들어가기전에",
          "original_form": "스터디 들어가기전에",
          "speaker_id": "2",
          "time": "20211010 16:42"
        },
        {
          "id": "MDRW2100046921.1.35",
          "form": "기본적인 경제 공부하고 들어가자",
          "original_form": "기본적인 경제 공부하고 들어가자",
          "speaker_id": "2",
          "time": "20211010 16:42"
        },
        {
          "id": "MDRW2100046921.1.36",
          "form": "파이어족 7년남앗는데",
          "original_form": "파이어족 7년남앗는데",
          "speaker_id": "2",
          "time": "20211010 16:43"
        },
        {
          "id": "MDRW2100046921.1.37",
          "form": "열심히 해봐야지",
          "original_form": "열심히 해봐야지",
          "speaker_id": "2",
          "time": "20211010 16:43"
        },
        {
          "id": "MDRW2100046921.1.38",
          "form": "시간은 쏜살같다",
          "original_form": "시간은 쏜살같다",
          "speaker_id": "1",
          "time": "20211010 16:44"
        },
        {
          "id": "MDRW2100046921.1.39",
          "form": "근데 파이어족의 파이어가 뭔뜻이야",
          "original_form": "근데 파이어족의 파이어가 뭔뜻이야",
          "speaker_id": "1",
          "time": "20211010 16:44"
        },
        {
          "id": "MDRW2100046921.1.40",
          "form": "fire 인가",
          "original_form": "fire 인가",
          "speaker_id": "1",
          "time": "20211010 16:44"
        },
        {
          "id": "MDRW2100046921.1.41",
          "form": "F 파이낸스",
          "original_form": "F 파이낸스",
          "speaker_id": "2",
          "time": "20211010 16:45"
        },
        {
          "id": "MDRW2100046921.1.42",
          "form": "이렇게 F.I.R.E",
          "original_form": "이렇게 F.I.R.E",
          "speaker_id": "2",
          "time": "20211010 16:45"
        },
        {
          "id": "MDRW2100046921.1.43",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20211010 16:45"
        },
        {
          "id": "MDRW2100046921.1.44",
          "form": "직장인이라면 누구나 사직서 가슴에 품고 다닌다는데",
          "original_form": "직장인이라면 누구나 사직서 가슴에 품고 다닌다는데",
          "speaker_id": "1",
          "time": "20211010 16:46"
        },
        {
          "id": "MDRW2100046921.1.45",
          "form": "그순간에 아쉬운 소리하지않게",
          "original_form": "그순간에 아쉬운 소리하지않게",
          "speaker_id": "1",
          "time": "20211010 16:46"
        },
        {
          "id": "MDRW2100046921.1.46",
          "form": "열심히 하자 파이어족을 향해 화이팅 !",
          "original_form": "열심히 하자 파이어족을 향해 화이팅 !",
          "speaker_id": "1",
          "time": "20211010 16:46"
        },
        {
          "id": "MDRW2100046921.1.47",
          "form": "우리 모두 화이팅!",
          "original_form": "우리 모두 화이팅!",
          "speaker_id": "2",
          "time": "20211010 16:47"
        },
        {
          "id": "MDRW2100046921.1.48",
          "form": "다음에 경제관련 스터디도 들어가보자구!",
          "original_form": "다음에 경제관련 스터디도 들어가보자구!",
          "speaker_id": "2",
          "time": "20211010 16:47"
        }
      ]
