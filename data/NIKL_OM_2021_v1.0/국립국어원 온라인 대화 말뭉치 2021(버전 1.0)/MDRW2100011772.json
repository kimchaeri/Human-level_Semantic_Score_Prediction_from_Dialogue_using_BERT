      "utterance": [
        {
          "id": "MDRW2100011772.1.1",
          "form": "메타버스가 뭐야",
          "original_form": "메타버스가 뭐야",
          "speaker_id": "1",
          "time": "20210824 15:57"
        },
        {
          "id": "MDRW2100011772.1.2",
          "form": "찾아봤는데",
          "original_form": "찾아봤는데",
          "speaker_id": "2",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011772.1.3",
          "form": "영화 후아유라고 아니",
          "original_form": "영화 후아유라고 아니",
          "speaker_id": "2",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011772.1.4",
          "form": "모른다",
          "original_form": "모른다",
          "speaker_id": "1",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011772.1.5",
          "form": "대충 그런 가상세계에서 아바타같은걸로 나를 대신하는 그런건가봐",
          "original_form": "대충 그런 가상세계에서 아바타같은걸로 나를 대신하는 그런건가봐",
          "speaker_id": "2",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011772.1.6",
          "form": "메타버스가 가상현실이랑 뭐가 다른지 모르겠어",
          "original_form": "메타버스가 가상현실이랑 뭐가 다른지 모르겠어",
          "speaker_id": "1",
          "time": "20210824 16:00"
        },
        {
          "id": "MDRW2100011772.1.7",
          "form": "그게 뭐라고 관련주까지 나오는거지",
          "original_form": "그게 뭐라고 관련주까지 나오는거지",
          "speaker_id": "1",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011772.1.8",
          "form": "가수들이 팬사인회도하고 그렇다더라",
          "original_form": "가수들이 팬사인회도하고 그렇다더라",
          "speaker_id": "2",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011772.1.9",
          "form": "컨텐츠를 접목시키는거아닐까",
          "original_form": "컨텐츠를 접목시키는거아닐까",
          "speaker_id": "2",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011772.1.10",
          "form": "최근에 선미가 이런식으로 프로모션하는건 봤어",
          "original_form": "최근에 선미가 이런식으로 프로모션하는건 봤어",
          "speaker_id": "2",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011772.1.11",
          "form": "그게 메타버스인지는 몰랐지만",
          "original_form": "그게 메타버스인지는 몰랐지만",
          "speaker_id": "2",
          "time": "20210824 16:01"
        },
        {
          "id": "MDRW2100011772.1.12",
          "form": "정말 따라잡기 어렵다",
          "original_form": "정말 따라잡기 어렵다",
          "speaker_id": "1",
          "time": "20210824 16:02"
        },
        {
          "id": "MDRW2100011772.1.13",
          "form": "왜 공대생이지 나",
          "original_form": "왜 공대생이지 나",
          "speaker_id": "1",
          "time": "20210824 16:03"
        },
        {
          "id": "MDRW2100011772.1.14",
          "form": "메타버스로 스쿠버다이빙하고싶다",
          "original_form": "메타버스로 스쿠버다이빙하고싶다",
          "speaker_id": "1",
          "time": "20210824 16:03"
        },
        {
          "id": "MDRW2100011772.1.15",
          "form": "그런것도 가능해지지않을까",
          "original_form": "그런것도 가능해지지않을까",
          "speaker_id": "2",
          "time": "20210824 16:03"
        },
        {
          "id": "MDRW2100011772.1.16",
          "form": "나도 애니나 영화에서나 많이 본듯ㅋㅋ",
          "original_form": "나도 애니나 영화에서나 많이 본듯ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 16:03"
        },
        {
          "id": "MDRW2100011772.1.17",
          "form": "정말 먼 미래가 되면 메타버스 자체가 우리 일상생활에 들어올수도있을것같아",
          "original_form": "정말 먼 미래가 되면 메타버스 자체가 우리 일상생활에 들어올수도있을것같아",
          "speaker_id": "2",
          "time": "20210824 16:04"
        },
        {
          "id": "MDRW2100011772.1.18",
          "form": "더 방콕러들이 되겠군",
          "original_form": "더 방콕러들이 되겠군",
          "speaker_id": "1",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011772.1.19",
          "form": "예를들면 인테리어를 미니홈피처럼 매일매일꾸밀수 있다거나",
          "original_form": "예를들면 인테리어를 미니홈피처럼 매일매일꾸밀수 있다거나",
          "speaker_id": "2",
          "time": "20210824 16:05"
        },
        {
          "id": "MDRW2100011772.1.20",
          "form": "그치만 난 아직까지는 현실세계가 좋은걸~",
          "original_form": "그치만 난 아직까지는 현실세계가 좋은걸~",
          "speaker_id": "2",
          "time": "20210824 16:06"
        },
        {
          "id": "MDRW2100011772.1.21",
          "form": "그럼그럼",
          "original_form": "그럼그럼",
          "speaker_id": "1",
          "time": "20210824 16:08"
        },
        {
          "id": "MDRW2100011772.1.22",
          "form": "눈나빠진다~",
          "original_form": "눈나빠진다~",
          "speaker_id": "1",
          "time": "20210824 16:08"
        },
        {
          "id": "MDRW2100011772.1.23",
          "form": "코로나시대라서 그런지 점점 주목을 받나봐 물리적 제약이 없는~",
          "original_form": "코로나시대라서 그런지 점점 주목을 받나봐 물리적 제약이 없는~",
          "speaker_id": "2",
          "time": "20210824 16:09"
        },
        {
          "id": "MDRW2100011772.1.24",
          "form": "코로나가 없었다면 과.연",
          "original_form": "코로나가 없었다면 과.연",
          "speaker_id": "1",
          "time": "20210824 17:20"
        }
      ]
    }
