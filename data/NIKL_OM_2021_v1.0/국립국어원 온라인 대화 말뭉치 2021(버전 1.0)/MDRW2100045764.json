      "utterance": [
        {
          "id": "MDRW2100045764.1.1",
          "form": "아앗 다양한 과...진짜 어려워요",
          "original_form": "아앗 다양한 과...진짜 어려워요",
          "speaker_id": "2",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045764.1.2",
          "form": "공대신가요?",
          "original_form": "공대신가요?",
          "speaker_id": "2",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045764.1.3",
          "form": "이과 아니시랬지 참",
          "original_form": "이과 아니시랬지 참",
          "speaker_id": "2",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045764.1.4",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045764.1.5",
          "form": "그런 어려운과로 사람 보내지 말아주세요ㅠㅠㅠㅠㅠ",
          "original_form": "그런 어려운과로 사람 보내지 말아주세요ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045764.1.6",
          "form": "이과 문과 무서워요ㅠㅠ",
          "original_form": "이과 문과 무서워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045764.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:49"
        },
        {
          "id": "MDRW2100045764.1.8",
          "form": "그 전공에서 무엇을 배우나요?",
          "original_form": "그 전공에서 무엇을 배우나요?",
          "speaker_id": "2",
          "time": "20210908 06:49"
        },
        {
          "id": "MDRW2100045764.1.9",
          "form": "정말 이것저것 배웠어요",
          "original_form": "정말 이것저것 배웠어요",
          "speaker_id": "1",
          "time": "20210908 06:49"
        },
        {
          "id": "MDRW2100045764.1.10",
          "form": "하지만 전공으로 하지 않는다면 정말 필요없는 지식이지만요ㅠ",
          "original_form": "하지만 전공으로 하지 않는다면 정말 필요없는 지식이지만요ㅠ",
          "speaker_id": "1",
          "time": "20210908 06:50"
        },
        {
          "id": "MDRW2100045764.1.11",
          "form": "으으으으음 ㅋㅋㅋ진짜 궁금해요",
          "original_form": "으으으으음 ㅋㅋㅋ진짜 궁금해요",
          "speaker_id": "2",
          "time": "20210908 06:51"
        },
        {
          "id": "MDRW2100045764.1.12",
          "form": "이과 문과 미대 체대 음대 아니라면...",
          "original_form": "이과 문과 미대 체대 음대 아니라면...",
          "speaker_id": "2",
          "time": "20210908 06:51"
        },
        {
          "id": "MDRW2100045764.1.13",
          "form": "힘을내봐!! 그래 힘들내봐!! 용기를 내봐~!!",
          "original_form": "힘을내봐!! 그래 힘들내봐!! 용기를 내봐~!!",
          "speaker_id": "1",
          "time": "20210908 06:51"
        },
        {
          "id": "MDRW2100045764.1.14",
          "form": "피카츄!!",
          "original_form": "피카츄!!",
          "speaker_id": "1",
          "time": "20210908 06:51"
        },
        {
          "id": "MDRW2100045764.1.15",
          "form": "함내세요!!",
          "original_form": "함내세요!!",
          "speaker_id": "1",
          "time": "20210908 06:52"
        },
        {
          "id": "MDRW2100045764.1.16",
          "form": "피카츄..파업하고싶어...",
          "original_form": "피카츄..파업하고싶어...",
          "speaker_id": "2",
          "time": "20210908 06:54"
        },
        {
          "id": "MDRW2100045764.1.17",
          "form": "ㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:54"
        },
        {
          "id": "MDRW2100045764.1.18",
          "form": "힌트 하나 더 주세요!",
          "original_form": "힌트 하나 더 주세요!",
          "speaker_id": "2",
          "time": "20210908 06:54"
        },
        {
          "id": "MDRW2100045764.1.19",
          "form": "진짜 모르겠어요 뭐지",
          "original_form": "진짜 모르겠어요 뭐지",
          "speaker_id": "2",
          "time": "20210908 06:54"
        },
        {
          "id": "MDRW2100045764.1.20",
          "form": "사는데 필요한 필수요소 중 하나와 밀접해있어요!",
          "original_form": "사는데 필요한 필수요소 중 하나와 밀접해있어요!",
          "speaker_id": "1",
          "time": "20210908 06:55"
        },
        {
          "id": "MDRW2100045764.1.21",
          "form": "의식주중에 하나인가요?",
          "original_form": "의식주중에 하나인가요?",
          "speaker_id": "2",
          "time": "20210908 06:57"
        },
        {
          "id": "MDRW2100045764.1.22",
          "form": "넵!",
          "original_form": "넵!",
          "speaker_id": "1",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045764.1.23",
          "form": "의식주중의 한개와 관련돼있어요",
          "original_form": "의식주중의 한개와 관련돼있어요",
          "speaker_id": "1",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045764.1.24",
          "form": "앗 혹시 요리?!",
          "original_form": "앗 혹시 요리?!",
          "speaker_id": "2",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045764.1.25",
          "form": "그쪽이에요!",
          "original_form": "그쪽이에요!",
          "speaker_id": "1",
          "time": "20210908 06:59"
        },
        {
          "id": "MDRW2100045764.1.26",
          "form": "호텔조리학과이신가요?",
          "original_form": "호텔조리학과이신가요?",
          "speaker_id": "2",
          "time": "20210908 07:00"
        },
        {
          "id": "MDRW2100045764.1.27",
          "form": "땡!!",
          "original_form": "땡!!",
          "speaker_id": "1",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045764.1.28",
          "form": "되게 전반적인거에요 큰툴!!",
          "original_form": "되게 전반적인거에요 큰툴!!",
          "speaker_id": "1",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045764.1.29",
          "form": "전반적인....! 전반적인..!",
          "original_form": "전반적인....! 전반적인..!",
          "speaker_id": "2",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100045764.1.30",
          "form": "식품과..?",
          "original_form": "식품과..?",
          "speaker_id": "2",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100045764.1.31",
          "form": "식품과도 아니에요ㅠ",
          "original_form": "식품과도 아니에요ㅠ",
          "speaker_id": "1",
          "time": "20210908 07:04"
        },
        {
          "id": "MDRW2100045764.1.32",
          "form": "어떻겦보면 생소할 수도 있어요",
