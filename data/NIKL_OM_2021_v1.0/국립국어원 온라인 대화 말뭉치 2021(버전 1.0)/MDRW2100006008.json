      "utterance": [
        {
          "id": "MDRW2100006008.1.1",
          "form": "전 저녁 먹엇어요",
          "original_form": "전 저녁 먹엇어요",
          "speaker_id": "1",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100006008.1.2",
          "form": "저녁 드셧나요",
          "original_form": "저녁 드셧나요",
          "speaker_id": "1",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100006008.1.3",
          "form": "한끼를 3끼저럼 먹었어요",
          "original_form": "한끼를 3끼저럼 먹었어요",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100006008.1.4",
          "form": "한글로치니 나쁜말이라네요 ㅡㅡ",
          "original_form": "한글로치니 나쁜말이라네요 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100006008.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100006008.1.6",
          "form": "한끼를 뭘 드셧나요",
          "original_form": "한끼를 뭘 드셧나요",
          "speaker_id": "1",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100006008.1.7",
          "form": "컵라면 편의점도시락 너겟 햄버거2개 먹었습니다",
          "original_form": "컵라면 편의점도시락 너겟 햄버거2개 먹었습니다",
          "speaker_id": "2",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100006008.1.8",
          "form": "행복하네요",
          "original_form": "행복하네요",
          "speaker_id": "2",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100006008.1.9",
          "form": "왘ㅋㅋㅋㅋㅋㅋ정말 한끼를 3끼처럼 드셧네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "왘ㅋㅋㅋㅋㅋㅋ정말 한끼를 3끼처럼 드셧네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100006008.1.10",
          "form": "밥먹을 시간도없이 ㅜㅜㅜ",
          "original_form": "밥먹을 시간도없이 ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100006008.1.11",
          "form": "여긴 비가좀오네요",
          "original_form": "여긴 비가좀오네요",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100006008.1.12",
          "form": "아 그래요? 쌀쌀하겟네요 ㅠㅠ",
          "original_form": "아 그래요? 쌀쌀하겟네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100006008.1.13",
          "form": "전 시원해서좋아요ㅎㅎㅎ",
          "original_form": "전 시원해서좋아요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100006008.1.14",
          "form": "음식 뭐좋아해요?",
          "original_form": "음식 뭐좋아해요?",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100006008.1.15",
          "form": "웬만한건 다 잘먹어요 ㅋㅋㅋㅋ",
          "original_form": "웬만한건 다 잘먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100006008.1.16",
          "form": "음식 뭐 좋아하세요?",
          "original_form": "음식 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100006008.1.17",
          "form": "치킨을 젤좋아해요",
          "original_form": "치킨을 젤좋아해요",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100006008.1.18",
          "form": "닭요리!",
          "original_form": "닭요리!",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100006008.1.19",
          "form": "안가리시는군여",
          "original_form": "안가리시는군여",
          "speaker_id": "2",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100006008.1.20",
          "form": "네 안가리고 잘먹어요",
          "original_form": "네 안가리고 잘먹어요",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100006008.1.21",
          "form": "물론 닭요리도 엄청좋아합니다",
          "original_form": "물론 닭요리도 엄청좋아합니다",
          "speaker_id": "1",
          "time": "20210907 20:20"
        },
        {
          "id": "MDRW2100006008.1.22",
          "form": "매콤한 닭볶음탕이 땡기는군요",
          "original_form": "매콤한 닭볶음탕이 땡기는군요",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100006008.1.23",
          "form": "크",
          "original_form": "크",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100006008.1.24",
          "form": "닭볶음탕 너무맛잇죠",
          "original_form": "닭볶음탕 너무맛잇죠",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006008.1.25",
          "form": "감자도 잔뜩 들어잇어야합니다",
          "original_form": "감자도 잔뜩 들어잇어야합니다",
