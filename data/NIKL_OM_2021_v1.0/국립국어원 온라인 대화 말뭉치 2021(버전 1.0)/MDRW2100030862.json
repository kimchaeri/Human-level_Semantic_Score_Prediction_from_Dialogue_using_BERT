      "utterance": [
        {
          "id": "MDRW2100030862.1.1",
          "form": "sf 뭐좋아하세요?",
          "original_form": "sf 뭐좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100030862.1.2",
          "form": "테드창이라고",
          "original_form": "테드창이라고",
          "speaker_id": "1",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030862.1.3",
          "form": "좋아하는 작가 있어요",
          "original_form": "좋아하는 작가 있어요",
          "speaker_id": "1",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100030862.1.4",
          "form": "어디서 들어봤는데...영화에서....",
          "original_form": "어디서 들어봤는데...영화에서....",
          "speaker_id": "2",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030862.1.5",
          "form": "권상우나오는..",
          "original_form": "권상우나오는..",
          "speaker_id": "2",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030862.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100030862.1.7",
          "form": "한번 검색해볼게요",
          "original_form": "한번 검색해볼게요",
          "speaker_id": "2",
          "time": "20210904 13:39"
        },
        {
          "id": "MDRW2100030862.1.8",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "1",
          "time": "20210904 13:39"
        },
        {
          "id": "MDRW2100030862.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:39"
        },
        {
          "id": "MDRW2100030862.1.10",
          "form": "아 그 사람보",
          "original_form": "아 그 사람보",
          "speaker_id": "1",
          "time": "20210904 13:39"
        },
        {
          "id": "MDRW2100030862.1.11",
          "form": "사람도",
          "original_form": "사람도",
          "speaker_id": "1",
          "time": "20210904 13:39"
        },
        {
          "id": "MDRW2100030862.1.12",
          "form": "테드창이죠",
          "original_form": "테드창이죠",
          "speaker_id": "1",
          "time": "20210904 13:39"
        },
        {
          "id": "MDRW2100030862.1.13",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:39"
        },
        {
          "id": "MDRW2100030862.1.14",
          "form": "바빌론의 탑 재밌어요?",
          "original_form": "바빌론의 탑 재밌어요?",
          "speaker_id": "2",
          "time": "20210904 13:40"
        },
        {
          "id": "MDRW2100030862.1.15",
          "form": "공상과학 이런거 좋아하시나봐요~!?",
          "original_form": "공상과학 이런거 좋아하시나봐요~!?",
          "speaker_id": "2",
          "time": "20210904 13:41"
        },
        {
          "id": "MDRW2100030862.1.16",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210904 13:41"
        },
        {
          "id": "MDRW2100030862.1.17",
          "form": "예 그렇죠",
          "original_form": "예 그렇죠",
          "speaker_id": "1",
          "time": "20210904 13:41"
        },
        {
          "id": "MDRW2100030862.1.18",
          "form": "바빌론의 탑은 뭐랄까",
          "original_form": "바빌론의 탑은 뭐랄까",
          "speaker_id": "1",
          "time": "20210904 13:41"
        },
        {
          "id": "MDRW2100030862.1.19",
          "form": "환상소설스러운",
          "original_form": "환상소설스러운",
          "speaker_id": "1",
          "time": "20210904 13:41"
        },
        {
          "id": "MDRW2100030862.1.20",
          "form": "느낌에 가까워요",
          "original_form": "느낌에 가까워요",
          "speaker_id": "1",
          "time": "20210904 13:41"
        },
        {
          "id": "MDRW2100030862.1.21",
          "form": "그럼 테드창의 책 중에 뭘 좋아하세요?",
          "original_form": "그럼 테드창의 책 중에 뭘 좋아하세요?",
          "speaker_id": "2",
          "time": "20210904 13:41"
        },
        {
          "id": "MDRW2100030862.1.22",
          "form": "나중에 읽어보고싶어요",
          "original_form": "나중에 읽어보고싶어요",
          "speaker_id": "2",
          "time": "20210904 13:41"
        },
        {
          "id": "MDRW2100030862.1.23",
          "form": "한번도 안읽어봄 부류라서",
          "original_form": "한번도 안읽어봄 부류라서",
          "speaker_id": "2",
          "time": "20210904 13:42"
        },
        {
          "id": "MDRW2100030862.1.24",
          "form": "네 인생의 이야기 좋아해요",
          "original_form": "네 인생의 이야기 좋아해요",
          "speaker_id": "1",
          "time": "20210904 13:42"
        },
        {
          "id": "MDRW2100030862.1.25",
          "form": "영화 콘텍트 원작입니다",
          "original_form": "영화 콘텍트 원작입니다",
          "speaker_id": "1",
          "time": "20210904 13:42"
        },
        {
          "id": "MDRW2100030862.1.26",
          "form": "와 그거 유명하던데",
          "original_form": "와 그거 유명하던데",
          "speaker_id": "2",
          "time": "20210904 13:43"
        },
        {
          "id": "MDRW2100030862.1.27",
          "form": "한번 읽어볼게요",
          "original_form": "한번 읽어볼게요",
          "speaker_id": "2",
          "time": "20210904 13:43"
        },
        {
          "id": "MDRW2100030862.1.28",
          "form": "영화 보지도 않았지만ㅎㅎ",
          "original_form": "영화 보지도 않았지만ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 13:43"
        },
        {
          "id": "MDRW2100030862.1.29",
          "form": "놀랍게도",
          "original_form": "놀랍게도",
          "speaker_id": "1",
          "time": "20210904 13:44"
        },
        {
          "id": "MDRW2100030862.1.30",
          "form": "영화는",
          "original_form": "영화는",
          "speaker_id": "1",
          "time": "20210904 13:44"
        },
        {
          "id": "MDRW2100030862.1.31",
          "form": "저도 안 봤...",
          "original_form": "저도 안 봤...",
          "speaker_id": "1",
          "time": "20210904 13:44"
        },
        {
          "id": "MDRW2100030862.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ왜요??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ왜요??",
          "speaker_id": "2",
          "time": "20210904 13:44"
        },
        {
          "id": "MDRW2100030862.1.33",
          "form": "...",
          "original_form": "...",
          "speaker_id": "1",
          "time": "20210904 13:45"
        },
        {
          "id": "MDRW2100030862.1.34",
          "form": "어",
          "original_form": "어",
          "speaker_id": "1",
          "time": "20210904 13:45"
        },
        {
          "id": "MDRW2100030862.1.35",
          "form": "그러게요...?",
          "original_form": "그러게요...?",
          "speaker_id": "1",
          "time": "20210904 13:45"
        },
        {
          "id": "MDRW2100030862.1.36",
          "form": "왜 안 봤지...",
          "original_form": "왜 안 봤지...",
          "speaker_id": "1",
          "time": "20210904 13:45"
        },
        {
          "id": "MDRW2100030862.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ이미 책으로 봐서 흥미가 떨어지셨나",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ이미 책으로 봐서 흥미가 떨어지셨나",
