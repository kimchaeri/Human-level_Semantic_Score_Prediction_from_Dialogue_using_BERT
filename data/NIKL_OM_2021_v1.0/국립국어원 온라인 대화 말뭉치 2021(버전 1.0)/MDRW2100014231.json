      "utterance": [
        {
          "id": "MDRW2100014231.1.1",
          "form": "무슨봉사하고싶음?",
          "original_form": "무슨봉사하고싶음?",
          "speaker_id": "1",
          "time": "20210825 17:52"
        },
        {
          "id": "MDRW2100014231.1.2",
          "form": "봉사 무슨 봉사 하고싶냐면 나는 교육봉사 원츄",
          "original_form": "봉사 무슨 봉사 하고싶냐면 나는 교육봉사 원츄",
          "speaker_id": "2",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014231.1.3",
          "form": "오옹 내가 그때 추천한거",
          "original_form": "오옹 내가 그때 추천한거",
          "speaker_id": "1",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014231.1.4",
          "form": "모레가 마감이얌",
          "original_form": "모레가 마감이얌",
          "speaker_id": "1",
          "time": "20210825 17:54"
        },
        {
          "id": "MDRW2100014231.1.5",
          "form": "아 근데 그거 너무 쫄려서 못해쪄^,,,^",
          "original_form": "아 근데 그거 너무 쫄려서 못해쪄^,,,^",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014231.1.6",
          "form": "자신없졍",
          "original_form": "자신없졍",
          "speaker_id": "2",
          "time": "20210825 17:55"
        },
        {
          "id": "MDRW2100014231.1.7",
          "form": "할수이따!!! 오늘부터 같이쓰장 ㅎㅎ",
          "original_form": "할수이따!!! 오늘부터 같이쓰장 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100014231.1.8",
          "form": "나는 학술동아리 들어가고싶은데",
          "original_form": "나는 학술동아리 들어가고싶은데",
          "speaker_id": "1",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014231.1.9",
          "form": "다 생각보다 너무 열심히허더라",
          "original_form": "다 생각보다 너무 열심히허더라",
          "speaker_id": "1",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014231.1.10",
          "form": "써보구... 맘에 안들면 휴지통행",
          "original_form": "써보구... 맘에 안들면 휴지통행",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014231.1.11",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 17:59"
        },
        {
          "id": "MDRW2100014231.1.12",
          "form": "취업을 향해 달려가는 대외활동 사람들 멋져 갓생.",
          "original_form": "취업을 향해 달려가는 대외활동 사람들 멋져 갓생.",
          "speaker_id": "2",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014231.1.13",
          "form": "인정. 역시 대학생의 꽃은 대외활동이지.",
          "original_form": "인정. 역시 대학생의 꽃은 대외활동이지.",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014231.1.14",
          "form": "내일모레 활동비가 들어오다니 신나는군.",
          "original_form": "내일모레 활동비가 들어오다니 신나는군.",
          "speaker_id": "1",
          "time": "20210825 18:00"
        },
        {
          "id": "MDRW2100014231.1.15",
          "form": "너는 너무 갓생이야 쉬엄쉬엄 하도록해.",
          "original_form": "너는 너무 갓생이야 쉬엄쉬엄 하도록해.",
          "speaker_id": "2",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014231.1.16",
          "form": "감쟈링 너둥",
          "original_form": "감쟈링 너둥",
          "speaker_id": "1",
          "time": "20210825 18:02"
        },
        {
          "id": "MDRW2100014231.1.17",
