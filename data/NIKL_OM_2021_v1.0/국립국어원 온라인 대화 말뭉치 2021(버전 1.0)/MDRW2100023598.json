      "utterance": [
        {
          "id": "MDRW2100023598.1.1",
          "form": "너무 우락부락한건 제가 또 싫어해서요 ㅎㅎ",
          "original_form": "너무 우락부락한건 제가 또 싫어해서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 12:29"
        },
        {
          "id": "MDRW2100023598.1.2",
          "form": "저두요!ㅋㅋㅋ잔근육이 좋아요!",
          "original_form": "저두요!ㅋㅋㅋ잔근육이 좋아요!",
          "speaker_id": "1",
          "time": "20210830 12:32"
        },
        {
          "id": "MDRW2100023598.1.3",
          "form": "암벽등반하면 살 많이 빠지나요?",
          "original_form": "암벽등반하면 살 많이 빠지나요?",
          "speaker_id": "1",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100023598.1.4",
          "form": "그건 잘...",
          "original_form": "그건 잘...",
          "speaker_id": "2",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100023598.1.5",
          "form": "지방은 빠지겠죠? 근육은 붙고",
          "original_form": "지방은 빠지겠죠? 근육은 붙고",
          "speaker_id": "2",
          "time": "20210830 12:33"
        },
        {
          "id": "MDRW2100023598.1.6",
          "form": "그치만 몸무게 자체는 잘 모르겠네요",
          "original_form": "그치만 몸무게 자체는 잘 모르겠네요",
          "speaker_id": "2",
          "time": "20210830 12:34"
        },
        {
          "id": "MDRW2100023598.1.7",
          "form": "사실 전 살면서 무게변동은 거의 없이 살아서요",
          "original_form": "사실 전 살면서 무게변동은 거의 없이 살아서요",
          "speaker_id": "2",
          "time": "20210830 12:34"
        },
        {
          "id": "MDRW2100023598.1.8",
          "form": "와..대단하시네요ㅠ",
          "original_form": "와..대단하시네요ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:37"
        },
        {
          "id": "MDRW2100023598.1.9",
          "form": "전 몸무게가 조금씩 계속 올라가는중이라서\n부럽네요ㅜ",
          "original_form": "전 몸무게가 조금씩 계속 올라가는중이라서\n부럽네요ㅜ",
          "speaker_id": "1",
          "time": "20210830 12:37"
        },
        {
          "id": "MDRW2100023598.1.10",
          "form": "음... 생각해보니 입이 짧아서 그런거같네요",
          "original_form": "음... 생각해보니 입이 짧아서 그런거같네요",
          "speaker_id": "2",
          "time": "20210830 12:37"
        },
        {
          "id": "MDRW2100023598.1.11",
          "form": "아하! 그럴수있겠네요\n저는 뭐든 잘먹어서..ㅠ",
          "original_form": "아하! 그럴수있겠네요\n저는 뭐든 잘먹어서..ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:38"
        },
        {
          "id": "MDRW2100023598.1.12",
          "form": "간식 별로 안좋아하고 스트레스받으면 입맛 뚝떨어지고",
          "original_form": "간식 별로 안좋아하고 스트레스받으면 입맛 뚝떨어지고",
          "speaker_id": "2",
          "time": "20210830 12:38"
        },
        {
          "id": "MDRW2100023598.1.13",
          "form": "진짜 딱 살안찌는 타입이시네요",
          "original_form": "진짜 딱 살안찌는 타입이시네요",
          "speaker_id": "1",
          "time": "20210830 12:39"
        },
        {
          "id": "MDRW2100023598.1.14",
          "form": "저랑 완전 반대예요ㅋㅋㅋ",
          "original_form": "저랑 완전 반대예요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:39"
        },
        {
          "id": "MDRW2100023598.1.15",
          "form": "전 간식 좋아하고 스트레스받으면 더먹는 스타일인데ㅠ",
          "original_form": "전 간식 좋아하고 스트레스받으면 더먹는 스타일인데ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:39"
        },
        {
          "id": "MDRW2100023598.1.16",
          "form": "어쩌면 그게 더 나을지도 몰라요",
          "original_form": "어쩌면 그게 더 나을지도 몰라요",
          "speaker_id": "2",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100023598.1.17",
          "form": "역시 살 잘안찌는분들은 이유가 있네요",
          "original_form": "역시 살 잘안찌는분들은 이유가 있네요",
          "speaker_id": "1",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100023598.1.18",
          "form": "그런가요?ㅠ",
          "original_form": "그런가요?ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100023598.1.19",
          "form": "저 예전에 스트레스 엄청 받았을때 있었는데",
          "original_form": "저 예전에 스트레스 엄청 받았을때 있었는데",
          "speaker_id": "2",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100023598.1.20",
          "form": "평소 반만 먹었는데도 밥이 더 안들어가고...",
          "original_form": "평소 반만 먹었는데도 밥이 더 안들어가고...",
          "speaker_id": "2",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100023598.1.21",
          "form": "그때 건강 많이 상했어요",
          "original_form": "그때 건강 많이 상했어요",
          "speaker_id": "2",
          "time": "20210830 12:40"
        },
        {
          "id": "MDRW2100023598.1.22",
          "form": "먹어야 그래도 기운을 내는데 먹지를 못해...",
          "original_form": "먹어야 그래도 기운을 내는데 먹지를 못해...",
          "speaker_id": "2",
          "time": "20210830 12:41"
        },
        {
          "id": "MDRW2100023598.1.23",
          "form": "진짜 그렇겠네요ㅠㅠ",
          "original_form": "진짜 그렇겠네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 12:41"
        },
        {
          "id": "MDRW2100023598.1.24",
          "form": "몸도 같이 상하겠어요ㅠ",
          "original_form": "몸도 같이 상하겠어요ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:42"
        },
        {
          "id": "MDRW2100023598.1.25",
          "form": "전 그때 제 위에 문제가 생긴줄 알고 내시경도 받았어요",
          "original_form": "전 그때 제 위에 문제가 생긴줄 알고 내시경도 받았어요",
          "speaker_id": "2",
          "time": "20210830 12:42"
        },
        {
          "id": "MDRW2100023598.1.26",
          "form": "깨끗했지만요 ㅎ",
          "original_form": "깨끗했지만요 ㅎ",
          "speaker_id": "2",
          "time": "20210830 12:43"
        },
        {
          "id": "MDRW2100023598.1.27",
          "form": "에구ㅠ\n아ㅋㅋㅋㅋㅋ",
          "original_form": "에구ㅠ\n아ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:43"
        },
        {
          "id": "MDRW2100023598.1.28",
          "form": "다행이네요!",
          "original_form": "다행이네요!",
          "speaker_id": "1",
          "time": "20210830 12:43"
        }
      ]
    }
  ]
