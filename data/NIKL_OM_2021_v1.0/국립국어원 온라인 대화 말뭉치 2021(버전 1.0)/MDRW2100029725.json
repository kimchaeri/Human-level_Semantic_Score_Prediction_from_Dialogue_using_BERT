      "utterance": [
        {
          "id": "MDRW2100029725.1.1",
          "form": "니 픽들 셋 다 좋다",
          "original_form": "니 픽들 셋 다 좋다",
          "speaker_id": "1",
          "time": "20210903 12:20"
        },
        {
          "id": "MDRW2100029725.1.2",
          "form": "밥은 먹었어?",
          "original_form": "밥은 먹었어?",
          "speaker_id": "1",
          "time": "20210903 12:21"
        },
        {
          "id": "MDRW2100029725.1.3",
          "form": "엉 잔치국수 먹었어",
          "original_form": "엉 잔치국수 먹었어",
          "speaker_id": "2",
          "time": "20210903 12:24"
        },
        {
          "id": "MDRW2100029725.1.4",
          "form": "잘했어",
          "original_form": "잘했어",
          "speaker_id": "1",
          "time": "20210903 12:27"
        },
        {
          "id": "MDRW2100029725.1.5",
          "form": "다른것들도 다 샇어?",
          "original_form": "다른것들도 다 샇어?",
          "speaker_id": "1",
          "time": "20210903 12:28"
        },
        {
          "id": "MDRW2100029725.1.6",
          "form": "올패 올라 지놈",
          "original_form": "올패 올라 지놈",
          "speaker_id": "1",
          "time": "20210903 12:28"
        },
        {
          "id": "MDRW2100029725.1.7",
          "form": "올릭스만 보유중이야",
          "original_form": "올릭스만 보유중이야",
          "speaker_id": "2",
          "time": "20210903 13:55"
        },
        {
          "id": "MDRW2100029725.1.8",
          "form": "나머지는 관망중",
          "original_form": "나머지는 관망중",
          "speaker_id": "2",
          "time": "20210903 13:55"
        },
        {
          "id": "MDRW2100029725.1.9",
          "form": "가격 더 빠질때까지?",
          "original_form": "가격 더 빠질때까지?",
          "speaker_id": "1",
          "time": "20210903 13:56"
        },
        {
          "id": "MDRW2100029725.1.10",
          "form": "어 그리고 한번에 여러개할만큼 자금흐름이 원활하지못해...",
          "original_form": "어 그리고 한번에 여러개할만큼 자금흐름이 원활하지못해...",
          "speaker_id": "2",
          "time": "20210903 14:00"
        },
        {
          "id": "MDRW2100029725.1.11",
          "form": "그렇고만",
          "original_form": "그렇고만",
          "speaker_id": "1",
          "time": "20210903 14:02"
        },
        {
          "id": "MDRW2100029725.1.12",
          "form": "살때 얘기해줘",
          "original_form": "살때 얘기해줘",
          "speaker_id": "1",
          "time": "20210903 14:02"
        },
        {
          "id": "MDRW2100029725.1.13",
          "form": "ㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210903 14:02"
        },
        {
          "id": "MDRW2100029725.1.14",
          "form": "name2",
          "original_form": "&name2&",
          "speaker_id": "1",
          "time": "20210907 08:49"
        },
        {
          "id": "MDRW2100029725.1.15",
          "form": "야스",
          "original_form": "야스",
          "speaker_id": "2",
          "time": "20210907 08:52"
        },
        {
          "id": "MDRW2100029725.1.16",
          "form": "주말에 쇼핑했어?",
          "original_form": "주말에 쇼핑했어?",
          "speaker_id": "1",
          "time": "20210907 09:14"
        },
        {
          "id": "MDRW2100029725.1.17",
          "form": "로봇청소기 샀어?",
          "original_form": "로봇청소기 샀어?",
          "speaker_id": "1",
          "time": "20210907 09:14"
        },
        {
          "id": "MDRW2100029725.1.18",
          "form": "아니 안샀어",
          "original_form": "아니 안샀어",
          "speaker_id": "2",
          "time": "20210907 10:08"
        },
        {
          "id": "MDRW2100029725.1.19",
          "form": "엄마가 아직 힘 있다고 나중에 사달래",
          "original_form": "엄마가 아직 힘 있다고 나중에 사달래",
          "speaker_id": "2",
          "time": "20210907 10:08"
        },
        {
          "id": "MDRW2100029725.1.20",
          "form": "ㅋㅋㄱㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100029725.1.21",
          "form": "차는 어때 잘나가?",
          "original_form": "차는 어때 잘나가?",
          "speaker_id": "1",
          "time": "20210907 10:11"
        },
        {
          "id": "MDRW2100029725.1.22",
          "form": "어 잘산거같아  한달에 1200넘데타는데 기름값 12만원",
          "original_form": "어 잘산거같아  한달에 1200넘데타는데 기름값 12만원",
          "speaker_id": "2",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100029725.1.23",
          "form": "하이브리드인가?",
          "original_form": "하이브리드인가?",
          "speaker_id": "1",
          "time": "20210907 10:23"
        },
        {
