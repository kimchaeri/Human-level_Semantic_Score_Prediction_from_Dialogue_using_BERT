      "utterance": [
        {
          "id": "MDRW2100007052.1.1",
          "form": "와 비오더니 선선함",
          "original_form": "와 비오더니 선선함",
          "speaker_id": "1",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007052.1.2",
          "form": "오 여기는 좀 습습",
          "original_form": "오 여기는 좀 습습",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007052.1.3",
          "form": "가을이 아닌 것 같아",
          "original_form": "가을이 아닌 것 같아",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007052.1.4",
          "form": "아 지금 습하긴한데 여름의 그 습기랑은 달러",
          "original_form": "아 지금 습하긴한데 여름의 그 습기랑은 달러",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007052.1.5",
          "form": "비 그만와",
          "original_form": "비 그만와",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007052.1.6",
          "form": "아침에 자다가 빗소리 엄청커서 놀랬음",
          "original_form": "아침에 자다가 빗소리 엄청커서 놀랬음",
          "speaker_id": "1",
          "time": "20210822 00:24"
        },
        {
          "id": "MDRW2100007052.1.7",
          "form": "오 비오면 좀 졸리지 않아?",
          "original_form": "오 비오면 좀 졸리지 않아?",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007052.1.8",
          "form": "백색소음이래",
          "original_form": "백색소음이래",
          "speaker_id": "2",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007052.1.9",
          "form": "내일도 흐리다는데 비 안왔으면 좋겠네",
          "original_form": "내일도 흐리다는데 비 안왔으면 좋겠네",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007052.1.10",
          "form": "그래서 늦잠잤음ㅇㅇ",
          "original_form": "그래서 늦잠잤음ㅇㅇ",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007052.1.11",
          "form": "내일 비오지말아라 빨래할거니까",
          "original_form": "내일 비오지말아라 빨래할거니까",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007052.1.12",
          "form": "날씨가 비가 오다가 안오다가 그러니깐",
          "original_form": "날씨가 비가 오다가 안오다가 그러니깐",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007052.1.13",
          "form": "옷차림이.너무 애매해",
          "original_form": "옷차림이.너무 애매해",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007052.1.14",
          "form": "아 거렁뱅이 입을옷 없어서 미쳐",
          "original_form": "아 거렁뱅이 입을옷 없어서 미쳐",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007052.1.15",
          "form": "거기다가 우산 필수",
          "original_form": "거기다가 우산 필수",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007052.1.16",
          "form": "날이.좋으면 또 얀산 필수",
          "original_form": "날이.좋으면 또 얀산 필수",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007052.1.17",
          "form": "양산",
          "original_form": "양산",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007052.1.18",
          "form": "날씨가 정말 이상해",
          "original_form": "날씨가 정말 이상해",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007052.1.19",
          "form": "그래서 매일 가방 무거움",
          "original_form": "그래서 매일 가방 무거움",
          "speaker_id": "1",
          "time": "20210822 00:32"
        },
        {
          "id": "MDRW2100007052.1.20",
          "form": "나는 겨울이 좋아",
          "original_form": "나는 겨울이 좋아",
          "speaker_id": "2",
          "time": "20210822 00:33"
        },
        {
          "id": "MDRW2100007052.1.21",
          "form": "하 겨울 듣기만 해도 발시려",
          "original_form": "하 겨울 듣기만 해도 발시려",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007052.1.22",
          "form": "나는 봄,가을 좋아",
          "original_form": "나는 봄,가을 좋아",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007052.1.23",
          "form": "봄 가을이 왜 좋아?",
          "original_form": "봄 가을이 왜 좋아?",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007052.1.24",
          "form": "따뜻하고 선선하고 꽃이피고 단풍이 물드니까~~~",
          "original_form": "따뜻하고 선선하고 꽃이피고 단풍이 물드니까~~~",
          "speaker_id": "1",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007052.1.25",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007052.1.26",
          "form": "나는 겨울옷이 예쁘고 땀이 안나서 좋아",
          "original_form": "나는 겨울옷이 예쁘고 땀이 안나서 좋아",
          "speaker_id": "2",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007052.1.27",
          "form": "겨울옷 너무비쌈...근데 이쁜건 인정",
          "original_form": "겨울옷 너무비쌈...근데 이쁜건 인정",
          "speaker_id": "1",
          "time": "20210822 00:45"
        }
      ]
    }
