      "utterance": [
        {
          "id": "MDRW2100007366.1.1",
          "form": "마지막 콘서트가 뭐야?ㅠㅠ",
          "original_form": "마지막 콘서트가 뭐야?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007366.1.2",
          "form": "백예린 콘서트... 하하",
          "original_form": "백예린 콘서트... 하하",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007366.1.3",
          "form": "재작년 12월 24일에 다녀왔어",
          "original_form": "재작년 12월 24일에 다녀왔어",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007366.1.4",
          "form": "재작년ㅜㅜㅜ",
          "original_form": "재작년ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007366.1.5",
          "form": "크리스마스이브라 아직도 생생하네....",
          "original_form": "크리스마스이브라 아직도 생생하네....",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007366.1.6",
          "form": "하 진짜 이 시국이 다 바꿔놨네ㅜㅜ",
          "original_form": "하 진짜 이 시국이 다 바꿔놨네ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007366.1.7",
          "form": "슬프다 그때 진짜 좋았는데",
          "original_form": "슬프다 그때 진짜 좋았는데",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007366.1.8",
          "form": "맘만 먹으면 뭐든 보러갈수있었는데ㅜ",
          "original_form": "맘만 먹으면 뭐든 보러갈수있었는데ㅜ",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007366.1.9",
          "form": "좋아하는 아티스트 콘서트 어디 가봤어?",
          "original_form": "좋아하는 아티스트 콘서트 어디 가봤어?",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007366.1.10",
          "form": "난 김동률 콘을 엄청 좋아해서 매번갔고",
          "original_form": "난 김동률 콘을 엄청 좋아해서 매번갔고",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007366.1.11",
          "form": "이적도 다녀와보니 좋더라",
          "original_form": "이적도 다녀와보니 좋더라",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007366.1.12",
          "form": "와 김동률",
          "original_form": "와 김동률",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007366.1.13",
          "form": "김동률 목소리 진짜 좋아하는데",
          "original_form": "김동률 목소리 진짜 좋아하는데",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007366.1.14",
          "form": "응ㅜ 너무 좋아 그 목소리",
          "original_form": "응ㅜ 너무 좋아 그 목소리",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007366.1.15",
          "form": "출발이 최애곡",
          "original_form": "출발이 최애곡",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007366.1.16",
          "form": "백예린도 목소리 너무 이쁘잖아",
          "original_form": "백예린도 목소리 너무 이쁘잖아",
