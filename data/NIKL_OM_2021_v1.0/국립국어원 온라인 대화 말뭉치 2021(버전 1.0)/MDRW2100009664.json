      "utterance": [
        {
          "id": "MDRW2100009664.1.1",
          "form": "현 과외중인 선생님이랑",
          "original_form": "현 과외중인 선생님이랑",
          "speaker_id": "2",
          "time": "20210823 02:41"
        },
        {
          "id": "MDRW2100009664.1.2",
          "form": "영어과외를 하고 있나요?",
          "original_form": "영어과외를 하고 있나요?",
          "speaker_id": "1",
          "time": "20210823 02:41"
        },
        {
          "id": "MDRW2100009664.1.3",
          "form": "그렇게 하고 있는 거 같아요",
          "original_form": "그렇게 하고 있는 거 같아요",
          "speaker_id": "2",
          "time": "20210823 02:41"
        },
        {
          "id": "MDRW2100009664.1.4",
          "form": "네 그렇다는 거 같으요",
          "original_form": "네 그렇다는 거 같으요",
          "speaker_id": "2",
          "time": "20210823 02:41"
        },
        {
          "id": "MDRW2100009664.1.5",
          "form": "과외를 두개를 하고싶어 하는 건가요?",
          "original_form": "과외를 두개를 하고싶어 하는 건가요?",
          "speaker_id": "1",
          "time": "20210823 02:42"
        },
        {
          "id": "MDRW2100009664.1.6",
          "form": "그거는 이번 주 화요일에 끝나고",
          "original_form": "그거는 이번 주 화요일에 끝나고",
          "speaker_id": "2",
          "time": "20210823 02:42"
        },
        {
          "id": "MDRW2100009664.1.7",
          "form": "금요일부터 저랑 하게 되는 거래요",
          "original_form": "금요일부터 저랑 하게 되는 거래요",
          "speaker_id": "2",
          "time": "20210823 02:42"
        },
        {
          "id": "MDRW2100009664.1.8",
          "form": "학부모의 니즈 파악이 중요해요",
          "original_form": "학부모의 니즈 파악이 중요해요",
          "speaker_id": "1",
          "time": "20210823 02:42"
        },
        {
          "id": "MDRW2100009664.1.9",
          "form": "저는 처음 상담때 학부모가 왜 과외를 바꿨는지 물어봐요",
          "original_form": "저는 처음 상담때 학부모가 왜 과외를 바꿨는지 물어봐요",
          "speaker_id": "1",
          "time": "20210823 02:42"
        },
        {
          "id": "MDRW2100009664.1.10",
          "form": "정확히는 이전 과외의 불만 사항을 물어봐요",
          "original_form": "정확히는 이전 과외의 불만 사항을 물어봐요",
          "speaker_id": "1",
          "time": "20210823 02:42"
        },
        {
          "id": "MDRW2100009664.1.11",
          "form": "오오...!! 전문성 대박이에요",
          "original_form": "오오...!! 전문성 대박이에요",
          "speaker_id": "2",
          "time": "20210823 02:43"
        },
        {
          "id": "MDRW2100009664.1.12",
          "form": "단어장 하나 기본으로 돌리시고",
          "original_form": "단어장 하나 기본으로 돌리시고",
          "speaker_id": "1",
          "time": "20210823 02:43"
        },
        {
          "id": "MDRW2100009664.1.13",
          "form": "자이스토리 독해 기본 / 실전 / 어법, 어휘 기본/ 실전",
          "original_form": "자이스토리 독해 기본 / 실전 / 어법, 어휘 기본/ 실전",
          "speaker_id": "1",
          "time": "20210823 02:43"
        },
        {
          "id": "MDRW2100009664.1.14",
          "form": "교육청&사설모의고사 1&2학년 4개년",
          "original_form": "교육청&사설모의고사 1&2학년 4개년",
          "speaker_id": "1",
          "time": "20210823 02:43"
        },
        {
          "id": "MDRW2100009664.1.15",
          "form": "문제집을 많이 풀게 숙제 시키고 많이많이 나가세요",
          "original_form": "문제집을 많이 풀게 숙제 시키고 많이많이 나가세요",
          "speaker_id": "1",
          "time": "20210823 02:44"
        },
        {
          "id": "MDRW2100009664.1.16",
          "form": "갑자기",
          "original_form": "갑자기",
          "speaker_id": "2",
          "time": "20210823 02:44"
        },
        {
          "id": "MDRW2100009664.1.17",
          "form": "갑자기 확 안심돼요",
          "original_form": "갑자기 확 안심돼요",
          "speaker_id": "2",
          "time": "20210823 02:44"
        },
        {
          "id": "MDRW2100009664.1.18",
          "form": "ㅅ혹시 수업준비는",
          "original_form": "ㅅ혹시 수업준비는",
          "speaker_id": "2",
          "time": "20210823 02:44"
        },
        {
          "id": "MDRW2100009664.1.19",
          "form": "몇 시간 정도 하세요?",
          "original_form": "몇 시간 정도 하세요?",
          "speaker_id": "2",
          "time": "20210823 02:45"
        },
        {
          "id": "MDRW2100009664.1.20",
          "form": "저는 여러개 하다 보니까",
          "original_form": "저는 여러개 하다 보니까",
          "speaker_id": "1",
          "time": "20210823 02:46"
        },
        {
          "id": "MDRW2100009664.1.21",
          "form": "이미 수업준비가 되어 있어서 별로 안해요",
          "original_form": "이미 수업준비가 되어 있어서 별로 안해요",
          "speaker_id": "1",
          "time": "20210823 02:46"
        },
        {
          "id": "MDRW2100009664.1.22",
          "form": "name2님도 과외 많이 하다보면 수업준비가 점점 줄어들게 되실 거에요",
          "original_form": "&name2&님도 과외 많이 하다보면 수업준비가 점점 줄어들게 되실 거에요",
          "speaker_id": "1",
          "time": "20210823 02:47"
        },
        {
          "id": "MDRW2100009664.1.23",
          "form": "그런 거 겠죠 역시?",
          "original_form": "그런 거 겠죠 역시?",
          "speaker_id": "2",
          "time": "20210823 02:47"
        },
        {
          "id": "MDRW2100009664.1.24",
          "form": "사실 진짜 첫첫과외라",
          "original_form": "사실 진짜 첫첫과외라",
          "speaker_id": "2",
          "time": "20210823 02:47"
        },
        {
          "id": "MDRW2100009664.1.25",
          "form": "나무...  긴장됩니다",
          "original_form": "나무...  긴장됩니다",
          "speaker_id": "2",
          "time": "20210823 02:47"
        },
        {
          "id": "MDRW2100009664.1.26",
          "form": "유튜브 영상 추천드려요",
          "original_form": "유튜브 영상 추천드려요",
          "speaker_id": "1",
          "time": "20210823 02:48"
        },
        {
          "id": "MDRW2100009664.1.27",
          "form": "과외 유튜브요?!",
          "original_form": "과외 유튜브요?!",
          "speaker_id": "2",
          "time": "20210823 02:48"
        },
        {
          "id": "MDRW2100009664.1.28",
          "form": "대학생이 과외로 월 200 버는 법",
          "original_form": "대학생이 과외로 월 200 버는 법",
          "speaker_id": "1",
          "time": "20210823 02:49"
        },
        {
          "id": "MDRW2100009664.1.29",
          "form": "이 영상 정말 추천드리고",
          "original_form": "이 영상 정말 추천드리고",
          "speaker_id": "1",
          "time": "20210823 02:49"
        },
        {
          "id": "MDRW2100009664.1.30",
          "form": "와 월 200",
          "original_form": "와 월 200",
          "speaker_id": "2",
          "time": "20210823 02:50"
        },
        {
          "id": "MDRW2100009664.1.31",
          "form": "꼭 볼게요 내일",
          "original_form": "꼭 볼게요 내일",
