      "utterance": [
        {
          "id": "MDRW2100039968.1.1",
          "form": "안녕하세요 \r\n저녁드셨나요",
          "original_form": "안녕하세요 \r\n저녁드셨나요",
          "speaker_id": "2",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100039968.1.2",
          "form": "이미 아까 먹고 지금은 야식 먹고 있어요",
          "original_form": "이미 아까 먹고 지금은 야식 먹고 있어요",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039968.1.3",
          "form": "저녁은 뭐 드셨었나요?",
          "original_form": "저녁은 뭐 드셨었나요?",
          "speaker_id": "2",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100039968.1.4",
          "form": "피자 먹었어요",
          "original_form": "피자 먹었어요",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039968.1.5",
          "form": "이렇게 보니까 완전 기름지게 먹었네요 저",
          "original_form": "이렇게 보니까 완전 기름지게 먹었네요 저",
          "speaker_id": "1",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039968.1.6",
          "form": "오 피자 맛있죠!",
          "original_form": "오 피자 맛있죠!",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039968.1.7",
          "form": "어느 종류로 주문하셨나요?",
          "original_form": "어느 종류로 주문하셨나요?",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039968.1.8",
          "form": "그냥 치즈 피자로 먹었어요",
          "original_form": "그냥 치즈 피자로 먹었어요",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039968.1.9",
          "form": "평소에 치즈피자 좋아하시나요?\r\n저는 개인적으로 포테이토피자를 제일 좋아해요",
          "original_form": "평소에 치즈피자 좋아하시나요?\r\n저는 개인적으로 포테이토피자를 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100039968.1.10",
          "form": "크게 땡기는거 없으면 치즈피자 먹어요",
          "original_form": "크게 땡기는거 없으면 치즈피자 먹어요",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100039968.1.11",
          "form": "그다음으로 포테이토 가장 많이 먹어요",
          "original_form": "그다음으로 포테이토 가장 많이 먹어요",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100039968.1.12",
          "form": "오 그러시군요\r\n야식도 드신다고 하셨는데 어떤 메뉴인가요?",
          "original_form": "오 그러시군요\r\n야식도 드신다고 하셨는데 어떤 메뉴인가요?",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100039968.1.13",
          "form": "기름떡볶이 만들어 먹었어요",
          "original_form": "기름떡볶이 만들어 먹었어요",
          "speaker_id": "1",
          "time": "20210904 00:00"
        },
        {
          "id": "MDRW2100039968.1.14",
          "form": "기름떡볶이 평소에도 가끔 만들어드시나요?",
          "original_form": "기름떡볶이 평소에도 가끔 만들어드시나요?",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100039968.1.15",
          "form": "네ㅎㅎ 엄청 좋아해요",
          "original_form": "네ㅎㅎ 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100039968.1.16",
          "form": "저는 아직 한 번도 안 만들어봤어요",
          "original_form": "저는 아직 한 번도 안 만들어봤어요",
          "speaker_id": "2",
          "time": "20210904 00:07"
        }
      ]
