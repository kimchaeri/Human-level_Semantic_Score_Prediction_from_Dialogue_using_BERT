        {
          "id": "MDRW2100008179.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210822 13:51"
        },
        {
          "id": "MDRW2100008179.1.2",
          "form": "요리나 설거지 세탁 등은 해요",
          "original_form": "요리나 설거지 세탁 등은 해요",
          "speaker_id": "2",
          "time": "20210822 13:52"
        },
        {
          "id": "MDRW2100008179.1.3",
          "form": "요리 잘해뇨?",
          "original_form": "요리 잘해뇨?",
          "speaker_id": "1",
          "time": "20210822 13:52"
        },
        {
          "id": "MDRW2100008179.1.4",
          "form": "못하진 않는데 잘하지는 더 않아요",
          "original_form": "못하진 않는데 잘하지는 더 않아요",
          "speaker_id": "2",
          "time": "20210822 13:56"
        },
        {
          "id": "MDRW2100008179.1.5",
          "form": "맛은 ?",
          "original_form": "맛은 ?",
          "speaker_id": "1",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100008179.1.6",
          "form": "사실 냉동식품 데워먹거나 진짜 간단한 조리하는 정도라서 맛은 있어요ㅋㅋ",
          "original_form": "사실 냉동식품 데워먹거나 진짜 간단한 조리하는 정도라서 맛은 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100008179.1.7",
          "form": "그럼돼요",
          "original_form": "그럼돼요",
          "speaker_id": "1",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100008179.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:12"
        },
        {
          "id": "MDRW2100008179.1.9",
          "form": "맞아요 먹을 때 맛있으면 되죠ㅋㅋㅋ",
          "original_form": "맞아요 먹을 때 맛있으면 되죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 14:13"
        },
        {
          "id": "MDRW2100008179.1.10",
          "form": "것  때문에 샇죠",
          "original_form": "것  때문에 샇죠",
          "speaker_id": "1",
          "time": "20210822 14:14"
        },
        {
          "id": "MDRW2100008179.1.11",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210822 14:17"
        },
        {
          "id": "MDRW2100008179.1.12",
          "form": "맛나게 먹으면 행복해요",
          "original_form": "맛나게 먹으면 행복해요",
          "speaker_id": "1",
          "time": "20210822 14:18"
        },
        {
          "id": "MDRW2100008179.1.13",
          "form": "맞아요 그래서 요리하는거죠",
          "original_form": "맞아요 그래서 요리하는거죠",
          "speaker_id": "2",
