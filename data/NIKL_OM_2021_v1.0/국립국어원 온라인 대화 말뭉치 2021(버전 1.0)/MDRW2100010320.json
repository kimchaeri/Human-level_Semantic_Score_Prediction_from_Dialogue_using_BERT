      "utterance": [
        {
          "id": "MDRW2100010320.1.1",
          "form": "결혼하셨던가요?ㅎㅎ",
          "original_form": "결혼하셨던가요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 17:26"
        },
        {
          "id": "MDRW2100010320.1.2",
          "form": "아니요 미혼입니다ㅋ",
          "original_form": "아니요 미혼입니다ㅋ",
          "speaker_id": "1",
          "time": "20210823 17:28"
        },
        {
          "id": "MDRW2100010320.1.3",
          "form": "아 연애도 쉬는중이셨죠!!",
          "original_form": "아 연애도 쉬는중이셨죠!!",
          "speaker_id": "2",
          "time": "20210823 18:04"
        },
        {
          "id": "MDRW2100010320.1.4",
          "form": "얘기를 했나요?ㅋ 어떤분이랑 얘기를 하신건지 ㅋ",
          "original_form": "얘기를 했나요?ㅋ 어떤분이랑 얘기를 하신건지 ㅋ",
          "speaker_id": "1",
          "time": "20210823 18:05"
        },
        {
          "id": "MDRW2100010320.1.5",
          "form": "쉬고있는 중은 맞아요ㅋ",
          "original_form": "쉬고있는 중은 맞아요ㅋ",
          "speaker_id": "1",
          "time": "20210823 18:06"
        },
        {
          "id": "MDRW2100010320.1.6",
          "form": "ㅋㅋ처음에 말했던거 같아요",
          "original_form": "ㅋㅋ처음에 말했던거 같아요",
          "speaker_id": "2",
          "time": "20210823 18:38"
        },
        {
          "id": "MDRW2100010320.1.7",
          "form": "소개팅은 자주 하세요?",
          "original_form": "소개팅은 자주 하세요?",
          "speaker_id": "2",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100010320.1.8",
          "form": "아니요 ㅎ 소개팅한지가 언젠지 기억도 안나요",
          "original_form": "아니요 ㅎ 소개팅한지가 언젠지 기억도 안나요",
          "speaker_id": "1",
          "time": "20210823 18:45"
        },
        {
          "id": "MDRW2100010320.1.9",
          "form": "연애결혼하셨나요?",
          "original_form": "연애결혼하셨나요?",
          "speaker_id": "1",
          "time": "20210823 18:45"
        },
        {
          "id": "MDRW2100010320.1.10",
          "form": "ㅋㅋ언젠가 인연이 오겠죠!",
          "original_form": "ㅋㅋ언젠가 인연이 오겠죠!",
          "speaker_id": "2",
          "time": "20210823 18:57"
        },
        {
          "id": "MDRW2100010320.1.11",
          "form": "네 회사에서 만났어요",
          "original_form": "네 회사에서 만났어요",
          "speaker_id": "2",
          "time": "20210823 18:57"
        },
        {
          "id": "MDRW2100010320.1.12",
          "form": "아 사내 커플이시군요",
          "original_form": "아 사내 커플이시군요",
          "speaker_id": "1",
          "time": "20210823 19:10"
        },
        {
          "id": "MDRW2100010320.1.13",
          "form": "네 ㅎㅎ 장단점이 있어요",
          "original_form": "네 ㅎㅎ 장단점이 있어요",
          "speaker_id": "2",
          "time": "20210823 19:28"
        },
        {
          "id": "MDRW2100010320.1.14",
          "form": "단점은 뭔가요? ㅎ",
          "original_form": "단점은 뭔가요? ㅎ",
          "speaker_id": "1",
          "time": "20210823 19:29"
        },
        {
          "id": "MDRW2100010320.1.15",
          "form": "매일 봐야되는거?",
          "original_form": "매일 봐야되는거?",
          "speaker_id": "1",
          "time": "20210823 19:29"
        },
        {
          "id": "MDRW2100010320.1.16",
          "form": "그게 장점인가",
          "original_form": "그게 장점인가",
          "speaker_id": "1",
          "time": "20210823 19:29"
        },
        {
          "id": "MDRW2100010320.1.17",
          "form": "장점이자 단점...",
          "original_form": "장점이자 단점...",
          "speaker_id": "2",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010320.1.18",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010320.1.19",
          "form": "그런거군요ㅎ",
          "original_form": "그런거군요ㅎ",
          "speaker_id": "1",
          "time": "20210823 19:44"
        },
        {
          "id": "MDRW2100010320.1.20",
          "form": "그래도 사내커플이면 같은팀 배정은 안하니",
          "original_form": "그래도 사내커플이면 같은팀 배정은 안하니",
          "speaker_id": "1",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010320.1.21",
          "form": "자주볼일은 없을듯요",
          "original_form": "자주볼일은 없을듯요",
          "speaker_id": "1",
          "time": "20210823 19:45"
        },
        {
          "id": "MDRW2100010320.1.22",
          "form": "아 그쵸 ㅎㅎ",
          "original_form": "아 그쵸 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 20:07"
        },
        {
          "id": "MDRW2100010320.1.23",
          "form": "멀리 있어요!",
          "original_form": "멀리 있어요!",
          "speaker_id": "2",
          "time": "20210823 20:07"
        },
        {
          "id": "MDRW2100010320.1.24",
          "form": "단점은 월급체계를 안다는거?ㅎㅎ",
          "original_form": "단점은 월급체계를 안다는거?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 20:08"
        },
        {
          "id": "MDRW2100010320.1.25",
          "form": "비상금만들기 어렵겠어요",
          "original_form": "비상금만들기 어렵겠어요",
          "speaker_id": "1",
          "time": "20210823 20:09"
        },
        {
          "id": "MDRW2100010320.1.26",
          "form": "네 제 사내망을 알고 있습니다 ㅎㅎ",
          "original_form": "네 제 사내망을 알고 있습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 20:35"
        },
        {
          "id": "MDRW2100010320.1.27",
          "form": "ㅋㅋ 투명한게 서로 좋은거죠",
          "original_form": "ㅋㅋ 투명한게 서로 좋은거죠",
          "speaker_id": "1",
          "time": "20210823 20:48"
        }
      ]
    }
  ]
