      "utterance": [
        {
          "id": "MDRW2100020692.1.1",
          "form": "그리고 양파도 좋아해요",
          "original_form": "그리고 양파도 좋아해요",
          "speaker_id": "2",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020692.1.2",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020692.1.3",
          "form": "아삭아삭",
          "original_form": "아삭아삭",
          "speaker_id": "1",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020692.1.4",
          "form": "양파채 꼭 있어야 돼요",
          "original_form": "양파채 꼭 있어야 돼요",
          "speaker_id": "2",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020692.1.5",
          "form": "햇양파는 특히 좀 달달하잖아요",
          "original_form": "햇양파는 특히 좀 달달하잖아요",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020692.1.6",
          "form": "진짜 삼겹살이랑 같이먹으면",
          "original_form": "진짜 삼겹살이랑 같이먹으면",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020692.1.7",
          "form": "그쵸 튼실한거",
          "original_form": "그쵸 튼실한거",
          "speaker_id": "2",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020692.1.8",
          "form": "그것이 천국",
          "original_form": "그것이 천국",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020692.1.9",
          "form": "하 콩나물무침은 드시나요",
          "original_form": "하 콩나물무침은 드시나요",
          "speaker_id": "2",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020692.1.10",
          "form": "혹시 삼겹살 드시고",
          "original_form": "혹시 삼겹살 드시고",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020692.1.11",
          "form": "네 콩나물좋아해요",
          "original_form": "네 콩나물좋아해요",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020692.1.12",
          "form": "김치는요 구워서 같이 드시나요",
          "original_form": "김치는요 구워서 같이 드시나요",
          "speaker_id": "1",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020692.1.13",
          "form": "구워먹으면 짱맛있는데",
          "original_form": "구워먹으면 짱맛있는데",
          "speaker_id": "2",
          "time": "20210828 11:13"
        },
        {
          "id": "MDRW2100020692.1.14",
          "form": "그냥 먹는거 좋아하시나봐",
          "original_form": "그냥 먹는거 좋아하시나봐",
          "speaker_id": "1",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020692.1.15",
          "form": "저 젓갈안들어간 제 김치는 구워먹어요",
          "original_form": "저 젓갈안들어간 제 김치는 구워먹어요",
          "speaker_id": "2",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020692.1.16",
          "form": "...",
          "original_form": "...",
          "speaker_id": "2",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020692.1.17",
          "form": "들켰네..",
          "original_form": "들켰네..",
          "speaker_id": "2",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020692.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020692.1.19",
          "form": "그냥먹는게 좀 더 프레시하긴해요",
          "original_form": "그냥먹는게 좀 더 프레시하긴해요",
          "speaker_id": "1",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020692.1.20",
          "form": "아녜요 저 고기랑은",
          "original_form": "아녜요 저 고기랑은",
          "speaker_id": "2",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020692.1.21",
          "form": "존중합니다",
          "original_form": "존중합니다",
          "speaker_id": "1",
          "time": "20210828 11:14"
        },
        {
          "id": "MDRW2100020692.1.22",
          "form": "김치구운거 좋아해묘",
          "original_form": "김치구운거 좋아해묘",
