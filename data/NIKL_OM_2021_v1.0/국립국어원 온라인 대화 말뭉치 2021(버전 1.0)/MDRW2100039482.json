      "utterance": [
        {
          "id": "MDRW2100039482.1.1",
          "form": "머사고싶은거 잇으신가요",
          "original_form": "머사고싶은거 잇으신가요",
          "speaker_id": "1",
          "time": "20210903 18:06"
        },
        {
          "id": "MDRW2100039482.1.2",
          "form": "가을옷 하나 사서 사고 싶은건 이제 없어요",
          "original_form": "가을옷 하나 사서 사고 싶은건 이제 없어요",
          "speaker_id": "2",
          "time": "20210903 18:07"
        },
        {
          "id": "MDRW2100039482.1.3",
          "form": "머사셧어요",
          "original_form": "머사셧어요",
          "speaker_id": "1",
          "time": "20210903 18:08"
        },
        {
          "id": "MDRW2100039482.1.4",
          "form": "가디건 하나 샀어요",
          "original_form": "가디건 하나 샀어요",
          "speaker_id": "2",
          "time": "20210903 18:10"
        },
        {
          "id": "MDRW2100039482.1.5",
          "form": "우왕..무슨색이요",
          "original_form": "우왕..무슨색이요",
          "speaker_id": "1",
          "time": "20210903 18:11"
        },
        {
          "id": "MDRW2100039482.1.6",
          "form": "깔끔하게 베이지색 샀어요",
          "original_form": "깔끔하게 베이지색 샀어요",
          "speaker_id": "2",
          "time": "20210903 18:15"
        },
        {
          "id": "MDRW2100039482.1.7",
          "form": "예쁘겟당 .. 개시하셧나요",
          "original_form": "예쁘겟당 .. 개시하셧나요",
          "speaker_id": "1",
          "time": "20210903 18:16"
        },
        {
          "id": "MDRW2100039482.1.8",
          "form": "아직이요ㅋㅋ",
          "original_form": "아직이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100039482.1.9",
          "form": "name1 님은 뭐 사셨나요",
          "original_form": "&name1& 님은 뭐 사셨나요",
          "speaker_id": "2",
          "time": "20210903 18:17"
        },
        {
          "id": "MDRW2100039482.1.10",
          "form": "밤에가끔 입어용..",
          "original_form": "밤에가끔 입어용..",
          "speaker_id": "1",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100039482.1.11",
          "form": "물욕이 적으신편인가요",
          "original_form": "물욕이 적으신편인가요",
          "speaker_id": "1",
          "time": "20210903 18:18"
        },
        {
          "id": "MDRW2100039482.1.12",
          "form": "적은 편은 아닌데 많이 참고 있죠",
          "original_form": "적은 편은 아닌데 많이 참고 있죠",
          "speaker_id": "2",
          "time": "20210903 18:20"
        },
        {
          "id": "MDRW2100039482.1.13",
          "form": "ㅋㅋ 이제 사고싶은거 없다하셔서 ㅋㅋㅋ",
          "original_form": "ㅋㅋ 이제 사고싶은거 없다하셔서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 18:21"
        },
        {
          "id": "MDRW2100039482.1.14",
          "form": "한번 사기 시작하면 끝도 없어요",
          "original_form": "한번 사기 시작하면 끝도 없어요",
          "speaker_id": "2",
          "time": "20210903 18:29"
        },
        {
          "id": "MDRW2100039482.1.15",
          "form": "ㅋㅋㅋㅋㅋ 글킨하죠...애초에차단을해야...",
          "original_form": "ㅋㅋㅋㅋㅋ 글킨하죠...애초에차단을해야...",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039482.1.16",
          "form": "그래서 네이버 쇼핑 탭을 치워버렸어요ㅋㅋ",
          "original_form": "그래서 네이버 쇼핑 탭을 치워버렸어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:43"
        },
        {
          "id": "MDRW2100039482.1.17",
          "form": "ㅋㅋㅋ 중고거래도 하시나요",
          "original_form": "ㅋㅋㅋ 중고거래도 하시나요",
          "speaker_id": "1",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100039482.1.18",
          "form": "당근을 깔아놓긴 했는데",
          "original_form": "당근을 깔아놓긴 했는데",
          "speaker_id": "2",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100039482.1.19",
          "form": "써본적은 없어요",
          "original_form": "써본적은 없어요",
          "speaker_id": "2",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100039482.1.20",
          "form": "ㅋㅋ사놓고 안쓰는거도 넘 많아서..",
