      "utterance": [
        {
          "id": "MDRW2100035725.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210829 23:33"
        },
        {
          "id": "MDRW2100035725.1.2",
          "form": "저녁 맛있는거 드셨나요",
          "original_form": "저녁 맛있는거 드셨나요",
          "speaker_id": "1",
          "time": "20210829 23:34"
        },
        {
          "id": "MDRW2100035725.1.3",
          "form": "어....팥빙수랑 복숭아랑 포도 먹었어요!ㅎㅎ",
          "original_form": "어....팥빙수랑 복숭아랑 포도 먹었어요!ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100035725.1.4",
          "form": "완전 여름 특식이네요",
          "original_form": "완전 여름 특식이네요",
          "speaker_id": "1",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100035725.1.5",
          "form": "복숭아 좋아하세요?",
          "original_form": "복숭아 좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 23:35"
        },
        {
          "id": "MDRW2100035725.1.6",
          "form": "네 복숭아 진짜 좋아해요",
          "original_form": "네 복숭아 진짜 좋아해요",
          "speaker_id": "2",
          "time": "20210829 23:38"
        },
        {
          "id": "MDRW2100035725.1.7",
          "form": "그럼 복숭아 하면 꼭  필요한 질문이 있죠",
          "original_form": "그럼 복숭아 하면 꼭  필요한 질문이 있죠",
          "speaker_id": "2",
          "time": "20210829 23:38"
        },
        {
          "id": "MDRW2100035725.1.8",
          "form": "딱복파세요?물복파세요?",
          "original_form": "딱복파세요?물복파세요?",
          "speaker_id": "2",
          "time": "20210829 23:38"
        },
        {
          "id": "MDRW2100035725.1.9",
          "form": "제가 물어보려고 했는데ㅋㅋㅋ역시 잘알이시네요",
          "original_form": "제가 물어보려고 했는데ㅋㅋㅋ역시 잘알이시네요",
          "speaker_id": "1",
          "time": "20210829 23:39"
        },
        {
          "id": "MDRW2100035725.1.10",
          "form": "저는 물복이요!",
          "original_form": "저는 물복이요!",
          "speaker_id": "1",
          "time": "20210829 23:39"
        },
        {
          "id": "MDRW2100035725.1.11",
          "form": "크으 말랑말랑한 물복을 좋아하시는군요",
          "original_form": "크으 말랑말랑한 물복을 좋아하시는군요",
          "speaker_id": "2",
          "time": "20210829 23:40"
        },
        {
          "id": "MDRW2100035725.1.12",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 23:40"
        },
        {
          "id": "MDRW2100035725.1.13",
          "form": "저는 공평한 처묵파 입니다^^",
          "original_form": "저는 공평한 처묵파 입니다^^",
          "speaker_id": "2",
          "time": "20210829 23:40"
        },
        {
          "id": "MDRW2100035725.1.14",
          "form": "그냥....그냥...너무 맛있어요...",
          "original_form": "그냥....그냥...너무 맛있어요...",
          "speaker_id": "2",
          "time": "20210829 23:41"
        },
        {
          "id": "MDRW2100035725.1.15",
          "form": "앜ㅋㄲㅋㅋㅋ",
          "original_form": "앜ㅋㄲㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 23:41"
        },
        {
          "id": "MDRW2100035725.1.16",
          "form": "복숭아 언제나 맛있죠...",
          "original_form": "복숭아 언제나 맛있죠...",
          "speaker_id": "1",
          "time": "20210829 23:41"
        },
        {
          "id": "MDRW2100035725.1.17",
          "form": "조금있으면. 안나올거 생각하니까 벌써 아쉽네요",
          "original_form": "조금있으면. 안나올거 생각하니까 벌써 아쉽네요",
          "speaker_id": "1",
          "time": "20210829 23:41"
        },
        {
          "id": "MDRW2100035725.1.18",
          "form": "정말요ㅠㅠ",
          "original_form": "정말요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 23:41"
        },
        {
          "id": "MDRW2100035725.1.19",
          "form": "그래서 맛 없는 복숭아 모아서 갈아, 쨈으로 두고두고 먹으려구요...",
          "original_form": "그래서 맛 없는 복숭아 모아서 갈아, 쨈으로 두고두고 먹으려구요...",
          "speaker_id": "2",
          "time": "20210829 23:42"
        },
        {
          "id": "MDRW2100035725.1.20",
          "form": "여름이 너무 아쉬워요",
          "original_form": "여름이 너무 아쉬워요",
          "speaker_id": "2",
          "time": "20210829 23:42"
        },
        {
          "id": "MDRW2100035725.1.21",
          "form": "쨈 좋네요 저는 청만들까 싶어요",
          "original_form": "쨈 좋네요 저는 청만들까 싶어요",
          "speaker_id": "1",
          "time": "20210829 23:42"
        },
        {
          "id": "MDRW2100035725.1.22",
          "form": "여름 안좋아하는데...",
          "original_form": "여름 안좋아하는데...",
          "speaker_id": "1",
          "time": "20210829 23:42"
        },
        {
          "id": "MDRW2100035725.1.23",
          "form": "복숭아는 너무 아쉬워요",
          "original_form": "복숭아는 너무 아쉬워요",
          "speaker_id": "1",
          "time": "20210829 23:42"
        },
        {
          "id": "MDRW2100035725.1.24",
          "form": "저도 계절 자체는 안 좋아하는데 여름과일이 너무 좋아요ㅠㅠ",
          "original_form": "저도 계절 자체는 안 좋아하는데 여름과일이 너무 좋아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 23:43"
        },
        {
          "id": "MDRW2100035725.1.25",
          "form": "그래도 조금있으면 청포도 많이나와서 저는 청포도로 만족하려구요",
          "original_form": "그래도 조금있으면 청포도 많이나와서 저는 청포도로 만족하려구요",
          "speaker_id": "1",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100035725.1.26",
          "form": "아 청포도도 너무 맛있죠...그리고 가을은 밤이 나오잖아요...밤페이스트도 제가 너무 좋아해서...",
          "original_form": "아 청포도도 너무 맛있죠...그리고 가을은 밤이 나오잖아요...밤페이스트도 제가 너무 좋아해서...",
          "speaker_id": "2",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100035725.1.27",
          "form": "밤 너무 맛있죠ㅠㅠ",
          "original_form": "밤 너무 맛있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 23:47"
        }
      ]
    }
