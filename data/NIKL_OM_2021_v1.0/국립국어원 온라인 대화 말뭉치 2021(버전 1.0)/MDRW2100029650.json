      "utterance": [
        {
          "id": "MDRW2100029650.1.1",
          "form": "요즘 코로나 시국이라 공연이나 전시회 관람이 힘들죠",
          "original_form": "요즘 코로나 시국이라 공연이나 전시회 관람이 힘들죠",
          "speaker_id": "2",
          "time": "20210903 11:23"
        },
        {
          "id": "MDRW2100029650.1.2",
          "form": "그 전엔 자주 다니셨나요?",
          "original_form": "그 전엔 자주 다니셨나요?",
          "speaker_id": "2",
          "time": "20210903 11:24"
        },
        {
          "id": "MDRW2100029650.1.3",
          "form": "아주 가끔 갔어요",
          "original_form": "아주 가끔 갔어요",
          "speaker_id": "1",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100029650.1.4",
          "form": "그런데 지난주 금요일에",
          "original_form": "그런데 지난주 금요일에",
          "speaker_id": "1",
          "time": "20210903 11:51"
        },
        {
          "id": "MDRW2100029650.1.5",
          "form": "옥상달빛 공연에 당첨되서 아내랑 다녀왔어요",
          "original_form": "옥상달빛 공연에 당첨되서 아내랑 다녀왔어요",
          "speaker_id": "1",
          "time": "20210903 11:52"
        },
        {
          "id": "MDRW2100029650.1.6",
          "form": "좋았겠어요",
          "original_form": "좋았겠어요",
          "speaker_id": "2",
          "time": "20210903 12:49"
        },
        {
          "id": "MDRW2100029650.1.7",
          "form": "저도 당첨돼서 장윤정 콘서트 다녀온적 있었네요 코로나 전에",
          "original_form": "저도 당첨돼서 장윤정 콘서트 다녀온적 있었네요 코로나 전에",
          "speaker_id": "2",
          "time": "20210903 12:49"
        },
        {
          "id": "MDRW2100029650.1.8",
          "form": "아내가 오랜만에 공연봐서 너무 좋아하더라고요",
          "original_form": "아내가 오랜만에 공연봐서 너무 좋아하더라고요",
          "speaker_id": "1",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100029650.1.9",
          "form": "장윤정 재밌겄다",
          "original_form": "장윤정 재밌겄다",
          "speaker_id": "1",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100029650.1.10",
          "form": "장윤정 좋아하세요?",
          "original_form": "장윤정 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 12:50"
        },
        {
          "id": "MDRW2100029650.1.11",
          "form": "트로트를 좋아해서 장윤정도 좋아해요",
          "original_form": "트로트를 좋아해서 장윤정도 좋아해요",
          "speaker_id": "2",
          "time": "20210903 12:57"
        },
        {
          "id": "MDRW2100029650.1.12",
          "form": "근데 사실 전 제 돈주고 공연간 적은 없어요",
          "original_form": "근데 사실 전 제 돈주고 공연간 적은 없어요",
          "speaker_id": "2",
          "time": "20210903 12:57"
        },
        {
          "id": "MDRW2100029650.1.13",
          "form": "그정도의 애정은 없나봐요",
          "original_form": "그정도의 애정은 없나봐요",
          "speaker_id": "2",
          "time": "20210903 12:57"
        },
        {
          "id": "MDRW2100029650.1.14",
          "form": "아 그건 저도 그래요ㅎㅎ",
          "original_form": "아 그건 저도 그래요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100029650.1.15",
          "form": "기회되고 시간되면 공연 찾아가보고 하겠지만 무리해서 가고 그러진 않아요",
          "original_form": "기회되고 시간되면 공연 찾아가보고 하겠지만 무리해서 가고 그러진 않아요",
          "speaker_id": "1",
          "time": "20210903 12:58"
        },
        {
          "id": "MDRW2100029650.1.16",
          "form": "공연 보는 거 자체는 좋아하지만.. 뭐 돈써가면서 문화생활을 즐길 정도는 아니고",
          "original_form": "공연 보는 거 자체는 좋아하지만.. 뭐 돈써가면서 문화생활을 즐길 정도는 아니고",
          "speaker_id": "2",
          "time": "20210903 13:00"
        },
        {
          "id": "MDRW2100029650.1.17",
          "form": "그래도 기회가 되면 잡는 편이에요.. 좋긴 좋으니까",
          "original_form": "그래도 기회가 되면 잡는 편이에요.. 좋긴 좋으니까",
          "speaker_id": "2",
          "time": "20210903 13:00"
        },
        {
          "id": "MDRW2100029650.1.18",
          "form": "저도 비슷해요",
          "original_form": "저도 비슷해요",
          "speaker_id": "1",
          "time": "20210903 13:10"
        },
        {
          "id": "MDRW2100029650.1.19",
          "form": "기회되는거는 참여해서 관람하는 편이죠",
          "original_form": "기회되는거는 참여해서 관람하는 편이죠",
          "speaker_id": "1",
          "time": "20210903 13:10"
        },
        {
          "id": "MDRW2100029650.1.20",
          "form": "예전에.. 명성황후 뮤지컬 봤는데 정말 좋더라구요",
          "original_form": "예전에.. 명성황후 뮤지컬 봤는데 정말 좋더라구요",
          "speaker_id": "2",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100029650.1.21",
          "form": "사람들이 돈주고 보는 이유를 좀 이해했어요",
          "original_form": "사람들이 돈주고 보는 이유를 좀 이해했어요",
          "speaker_id": "2",
          "time": "20210903 13:21"
        },
        {
          "id": "MDRW2100029650.1.22",
          "form": "그쵸 뮤지컬 진짜 종합엔테테이먼트",
          "original_form": "그쵸 뮤지컬 진짜 종합엔테테이먼트",
          "speaker_id": "1",
          "time": "20210903 13:22"
        },
        {
          "id": "MDRW2100029650.1.23",
          "form": "저는 위키드 본 적 있는데",
          "original_form": "저는 위키드 본 적 있는데",
          "speaker_id": "1",
          "time": "20210903 13:23"
        },
        {
          "id": "MDRW2100029650.1.24",
          "form": "음악도 음악이지만",
          "original_form": "음악도 음악이지만",
          "speaker_id": "1",
          "time": "20210903 13:23"
        },
        {
          "id": "MDRW2100029650.1.25",
          "form": "무대연출 정말 훌륭",
          "original_form": "무대연출 정말 훌륭",
          "speaker_id": "1",
          "time": "20210903 13:23"
        },
        {
          "id": "MDRW2100029650.1.26",
          "form": "네.. 그런데 그만큼 티켓가격이 만만치 않더라구요",
          "original_form": "네.. 그런데 그만큼 티켓가격이 만만치 않더라구요",
          "speaker_id": "2",
          "time": "20210903 13:26"
        },
        {
          "id": "MDRW2100029650.1.27",
          "form": "전 누가 보여줘서 봤지 제 돈으로는 못봤을 거에요",
          "original_form": "전 누가 보여줘서 봤지 제 돈으로는 못봤을 거에요",
          "speaker_id": "2",
          "time": "20210903 13:26"
        },
        {
          "id": "MDRW2100029650.1.28",
          "form": "근데 할인하는거 찾아보면 많아요",
          "original_form": "근데 할인하는거 찾아보면 많아요",
          "speaker_id": "1",
          "time": "20210903 13:26"
        },
        {
          "id": "MDRW2100029650.1.29",
          "form": "그런거 이용해서 보면 될 듯해요",
          "original_form": "그런거 이용해서 보면 될 듯해요",
