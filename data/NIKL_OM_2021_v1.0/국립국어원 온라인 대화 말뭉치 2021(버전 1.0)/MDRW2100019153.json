      "utterance": [
        {
          "id": "MDRW2100019153.1.1",
          "form": "종교는 있으신가용",
          "original_form": "종교는 있으신가용",
          "speaker_id": "1",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019153.1.2",
          "form": "ㅋㅋ 집안에 종교는 있는데 저는 무교같아요",
          "original_form": "ㅋㅋ 집안에 종교는 있는데 저는 무교같아요",
          "speaker_id": "2",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019153.1.3",
          "form": "종교있으세요?",
          "original_form": "종교있으세요?",
          "speaker_id": "2",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019153.1.4",
          "form": "저도요 ㅋㅋㅋ 모태신앙이엇는데",
          "original_form": "저도요 ㅋㅋㅋ 모태신앙이엇는데",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019153.1.5",
          "form": "무교요 ㅋㅋㅋ",
          "original_form": "무교요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019153.1.6",
          "form": "어떤거였어요? ㅋㅋ",
          "original_form": "어떤거였어요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019153.1.7",
          "form": "ㅋㅋ 종교가지면 맘은 편안한거 같은데",
          "original_form": "ㅋㅋ 종교가지면 맘은 편안한거 같은데",
          "speaker_id": "2",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100019153.1.8",
          "form": "전 불교요",
          "original_form": "전 불교요",
          "speaker_id": "2",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100019153.1.9",
          "form": "저도 믿진 않는데 그런게 있을것같아요",
          "original_form": "저도 믿진 않는데 그런게 있을것같아요",
          "speaker_id": "1",
          "time": "20210827 15:44"
        },
        {
          "id": "MDRW2100019153.1.10",
          "form": "불교는 특히!",
          "original_form": "불교는 특히!",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100019153.1.11",
          "form": "마음수련같은게 돼서 도움될것같아요 ㅋㅋㅋ",
          "original_form": "마음수련같은게 돼서 도움될것같아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100019153.1.12",
          "form": "진짜요? 나중에 불교나 천주교 믿어볼까... ㅎㅎ",
          "original_form": "진짜요? 나중에 불교나 천주교 믿어볼까... ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100019153.1.13",
          "form": "ㅋㅋㅋㅋ 전 천주교였어요..집안이 천주교",
          "original_form": "ㅋㅋㅋㅋ 전 천주교였어요..집안이 천주교",
          "speaker_id": "1",
          "time": "20210827 15:45"
        },
        {
          "id": "MDRW2100019153.1.14",
          "form": "성당 다니라구 계속하는데 귀찮아요 ㅋㅋ",
          "original_form": "성당 다니라구 계속하는데 귀찮아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019153.1.15",
          "form": "근데 사람이 불안정할때 의지가 될듯 ㅋㅋ",
          "original_form": "근데 사람이 불안정할때 의지가 될듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019153.1.16",
          "form": "천주교 멋진데요!",
          "original_form": "천주교 멋진데요!",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019153.1.17",
          "form": "묵주반지 멋져요",
          "original_form": "묵주반지 멋져요",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019153.1.18",
          "form": "앜ㅋㅋ묵주반지 저 성당 다니지도 않으면서 엄마꺼 뺏어 끼고다녔어요",
          "original_form": "앜ㅋㅋ묵주반지 저 성당 다니지도 않으면서 엄마꺼 뺏어 끼고다녔어요",
          "speaker_id": "1",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100019153.1.19",
          "form": "왠지 멋있고요ㅎㅎ 세례명도 있으시죠?",
          "original_form": "왠지 멋있고요ㅎㅎ 세례명도 있으시죠?",
          "speaker_id": "2",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019153.1.20",
          "form": "넹ㅋㅋㅋ전 별로 안조아햇어요 ㅋㅋㅋ 맘에안들어서",
          "original_form": "넹ㅋㅋㅋ전 별로 안조아햇어요 ㅋㅋㅋ 맘에안들어서",
          "speaker_id": "1",
          "time": "20210827 16:04"
        },
        {
          "id": "MDRW2100019153.1.21",
          "form": "ㅋㅋ 제 친군 지금은 교회다니는데 대학때 불교수업듣고 법명 받았어요 name3",
          "original_form": "ㅋㅋ 제 친군 지금은 교회다니는데 대학때 불교수업듣고 법명 받았어요 &name3&",
          "speaker_id": "2",
          "time": "20210827 16:15"
        },
        {
          "id": "MDRW2100019153.1.22",
          "form": "앜ㅋㅋㅋㅋㅋㅋ법명잇는지 몰랏어요 진짜 간지난다 name3ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ법명잇는지 몰랏어요 진짜 간지난다 &name3&ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:17"
        },
        {
          "id": "MDRW2100019153.1.23",
          "form": "나중에 저도 퇴직하면ㅎㅎ 봉사도 하고 좋을거같아요",
