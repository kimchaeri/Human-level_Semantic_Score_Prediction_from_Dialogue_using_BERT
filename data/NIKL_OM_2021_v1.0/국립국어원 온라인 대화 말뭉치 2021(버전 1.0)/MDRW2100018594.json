      "utterance": [
        {
          "id": "MDRW2100018594.1.1",
          "form": "수사어땠어요",
          "original_form": "수사어땠어요",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100018594.1.2",
          "form": "그냥..",
          "original_form": "그냥..",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100018594.1.3",
          "form": "전 해산물을 못먹어서 궁금하긴하..",
          "original_form": "전 해산물을 못먹어서 궁금하긴하..",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100018594.1.4",
          "form": "넵.",
          "original_form": "넵.",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100018594.1.5",
          "form": "아앗",
          "original_form": "아앗",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100018594.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100018594.1.7",
          "form": "ㅋ ㅋ ㅋ ㅋ ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋ ㅋ ㅋ ㅋ ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100018594.1.8",
          "form": "말을 더하지 않겠슴다",
          "original_form": "말을 더하지 않겠슴다",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100018594.1.9",
          "form": "넵.ㅎ",
          "original_form": "넵.ㅎ",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100018594.1.10",
          "form": "ㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100018594.1.11",
          "form": "ㅋㅋㅋㅋㅋ  별로인게 액정을 뚫고 나온다",
          "original_form": "ㅋㅋㅋㅋㅋ  별로인게 액정을 뚫고 나온다",
          "speaker_id": "1",
          "time": "20210828 19:24"
        },
        {
          "id": "MDRW2100018594.1.12",
          "form": "그냥 음... 뭐더라 초밥뷔페 또 있잖아요",
          "original_form": "그냥 음... 뭐더라 초밥뷔페 또 있잖아요",
          "speaker_id": "2",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100018594.1.13",
          "form": "4글자짜리..",
          "original_form": "4글자짜리..",
          "speaker_id": "2",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100018594.1.14",
          "form": "거기랑 비슷해요",
          "original_form": "거기랑 비슷해요",
          "speaker_id": "2",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100018594.1.15",
          "form": "쿠우쿠우",
          "original_form": "쿠우쿠우",
          "speaker_id": "1",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100018594.1.16",
          "form": "말하는건가요",
          "original_form": "말하는건가요",
          "speaker_id": "1",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100018594.1.17",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100018594.1.18",
          "form": "먹지도 못하면서 이름 기억만 오질나게 잘하는 편",
          "original_form": "먹지도 못하면서 이름 기억만 오질나게 잘하는 편",
          "speaker_id": "1",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100018594.1.19",
          "form": "전 이름도 기억 못하는  편.",
          "original_form": "전 이름도 기억 못하는  편.",
          "speaker_id": "2",
          "time": "20210828 19:25"
        },
        {
          "id": "MDRW2100018594.1.20",
          "form": "ㅋㅋㅋㅋ  수사가 더 비쌀텐데 쿠우쿠우랑 비슷한거면...",
          "original_form": "ㅋㅋㅋㅋ  수사가 더 비쌀텐데 쿠우쿠우랑 비슷한거면...",
          "speaker_id": "1",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100018594.1.21",
          "form": "음 겁나별로군요",
          "original_form": "음 겁나별로군요",
          "speaker_id": "1",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100018594.1.22",
          "form": "ㅎ...ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅎ...ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100018594.1.23",
          "form": "전 육삼빌딩에 뷔페도 정말 별로였어요",
          "original_form": "전 육삼빌딩에 뷔페도 정말 별로였어요",
          "speaker_id": "2",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100018594.1.24",
          "form": "아 저 저녁먹으러 가야할듯요",
          "original_form": "아 저 저녁먹으러 가야할듯요",
          "speaker_id": "1",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100018594.1.25",
          "form": "넵넵!! 맛있게드세요",
          "original_form": "넵넵!! 맛있게드세요",
          "speaker_id": "2",
          "time": "20210828 19:26"
        }
      ]
