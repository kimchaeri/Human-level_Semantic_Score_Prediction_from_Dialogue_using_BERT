      "utterance": [
        {
          "id": "MDRW2100026392.1.1",
          "form": "며칠전 드셨던 메뉴군여?",
          "original_form": "며칠전 드셨던 메뉴군여?",
          "speaker_id": "2",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026392.1.2",
          "form": "기억하시는구나..ㅎㅎㅎ",
          "original_form": "기억하시는구나..ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 21:58"
        },
        {
          "id": "MDRW2100026392.1.3",
          "form": "가자미를 10마리나 샀고",
          "original_form": "가자미를 10마리나 샀고",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026392.1.4",
          "form": "감자가 아직도 한박스가 남았어요",
          "original_form": "감자가 아직도 한박스가 남았어요",
          "speaker_id": "1",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026392.1.5",
          "form": "헛 10마리",
          "original_form": "헛 10마리",
          "speaker_id": "2",
          "time": "20210901 21:59"
        },
        {
          "id": "MDRW2100026392.1.6",
          "form": "감자조림도 해드세요 ㅎㅎ",
          "original_form": "감자조림도 해드세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026392.1.7",
          "form": "ㅎㅎㅎ 시장에서 장보면 그렇게 사게 돼요",
          "original_form": "ㅎㅎㅎ 시장에서 장보면 그렇게 사게 돼요",
          "speaker_id": "1",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026392.1.8",
          "form": "좋은 물건 싸게 나왔을때 좀 많이 사게 되죠",
          "original_form": "좋은 물건 싸게 나왔을때 좀 많이 사게 되죠",
          "speaker_id": "1",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026392.1.9",
          "form": "감자채볶음 감자전 등등 오만 감자요리 먹고 있답니다",
          "original_form": "감자채볶음 감자전 등등 오만 감자요리 먹고 있답니다",
          "speaker_id": "1",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026392.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026392.1.11",
          "form": "그냥 감자를 쪄서 먹기도 해요",
          "original_form": "그냥 감자를 쪄서 먹기도 해요",
          "speaker_id": "1",
          "time": "20210901 22:00"
        },
        {
          "id": "MDRW2100026392.1.12",
          "form": "감자파티네요",
          "original_form": "감자파티네요",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026392.1.13",
          "form": "하지감자를 세박스 받아서 어쩔 수 없어요",
          "original_form": "하지감자를 세박스 받아서 어쩔 수 없어요",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026392.1.14",
          "form": "이미 싹이 나려고 하고 있어요",
          "original_form": "이미 싹이 나려고 하고 있어요",
          "speaker_id": "1",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100026392.1.15",
          "form": "싹나면 안되는데",
          "original_form": "싹나면 안되는데",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026392.1.16",
          "form": "더 격하게 감자파티를",
          "original_form": "더 격하게 감자파티를",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026392.1.17",
          "form": "하셔야겠네요 ㅋㅋㅋ",
          "original_form": "하셔야겠네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100026392.1.18",
          "form": "그니까요.. 이번달까지는 다 처리해야할텐데요..",
          "original_form": "그니까요.. 이번달까지는 다 처리해야할텐데요..",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100026392.1.19",
          "form": "감자굴림 감자튀김 다하고 있어요",
          "original_form": "감자굴림 감자튀김 다하고 있어요",
          "speaker_id": "1",
          "time": "20210901 22:03"
        },
        {
          "id": "MDRW2100026392.1.20",
          "form": "감자튀김까지요?",
          "original_form": "감자튀김까지요?",
          "speaker_id": "2",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100026392.1.21",
          "form": "감자요리 질리겠는데요?ㅋㅋ",
          "original_form": "감자요리 질리겠는데요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:05"
        },
        {
          "id": "MDRW2100026392.1.22",
          "form": "감자세상에 들어온 기분이죠..ㅎㅎ",
          "original_form": "감자세상에 들어온 기분이죠..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:06"
        },
        {
          "id": "MDRW2100026392.1.23",
          "form": "아침도 직접해서 주세요?",
          "original_form": "아침도 직접해서 주세요?",
          "speaker_id": "2",
          "time": "20210901 22:08"
        },
        {
          "id": "MDRW2100026392.1.24",
          "form": "그럼요.. 매일 아침 차려준답니다",
          "original_form": "그럼요.. 매일 아침 차려준답니다",
          "speaker_id": "1",
          "time": "20210902 06:03"
        },
        {
          "id": "MDRW2100026392.1.25",
          "form": "와 일찍 일어나시네요",
          "original_form": "와 일찍 일어나시네요",
          "speaker_id": "2",
          "time": "20210902 06:28"
        },
        {
          "id": "MDRW2100026392.1.26",
          "form": "네.. 저희 식구들이 일찍 잃어나는 타입이에요",
          "original_form": "네.. 저희 식구들이 일찍 잃어나는 타입이에요",
          "speaker_id": "1",
          "time": "20210902 06:30"
        },
        {
          "id": "MDRW2100026392.1.27",
          "form": "이런....",
          "original_form": "이런....",
          "speaker_id": "2",
          "time": "20210902 07:48"
        },
        {
          "id": "MDRW2100026392.1.28",
          "form": "출근때문이 아니구요?",
          "original_form": "출근때문이 아니구요?",
          "speaker_id": "2",
          "time": "20210902 07:48"
        },
        {
          "id": "MDRW2100026392.1.29",
          "form": "네.. 아침형인간들...ㅋㅋㅋ",
          "original_form": "네.. 아침형인간들...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 07:49"
        },
        {
          "id": "MDRW2100026392.1.30",
          "form": "전 회사땜에 5시반 일어나요",
          "original_form": "전 회사땜에 5시반 일어나요",
          "speaker_id": "2",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100026392.1.31",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100026392.1.32",
          "form": "피곤해죽겠어요",
          "original_form": "피곤해죽겠어요",
          "speaker_id": "2",
          "time": "20210902 13:44"
        },
        {
          "id": "MDRW2100026392.1.33",
          "form": "앗.. 더 심한 아침형이네요?",
          "original_form": "앗.. 더 심한 아침형이네요?",
          "speaker_id": "1",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100026392.1.34",
          "form": "아니 출근땜에 어쩔 수 없는 거겠죠...?",
          "original_form": "아니 출근땜에 어쩔 수 없는 거겠죠...?",
          "speaker_id": "1",
          "time": "20210902 18:19"
        },
        {
          "id": "MDRW2100026392.1.35",
          "form": "그래도 아침은 챙겨드시고 가는 건가요?",
          "original_form": "그래도 아침은 챙겨드시고 가는 건가요?",
          "speaker_id": "1",
          "time": "20210902 18:20"
        },
        {
          "id": "MDRW2100026392.1.36",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100026392.1.37",
          "form": "아침은 시리얼로 매일",
          "original_form": "아침은 시리얼로 매일",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100026392.1.38",
          "form": "저 원래 아침 밥국 먹었는데 요새는 요거트에 견과류랑 과일채서 썷어넣어서 먹고 있네요",
          "original_form": "저 원래 아침 밥국 먹었는데 요새는 요거트에 견과류랑 과일채서 썷어넣어서 먹고 있네요",
          "speaker_id": "1",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100026392.1.39",
          "form": "오늘 저녁은 뭐 드시나요?",
          "original_form": "오늘 저녁은 뭐 드시나요?",
          "speaker_id": "1",
          "time": "20210902 19:58"
        },
        {
          "id": "MDRW2100026392.1.40",
          "form": "오징어볶음에 각종 반찬과 밥이요 ㅎㅎ",
          "original_form": "오징어볶음에 각종 반찬과 밥이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 19:59"
        },
        {
          "id": "MDRW2100026392.1.41",
          "form": "맛있는 집밥이네요~",
          "original_form": "맛있는 집밥이네요~",
          "speaker_id": "1",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100026392.1.42",
          "form": "전 양념돼지갈비를 먹었답니다",
          "original_form": "전 양념돼지갈비를 먹었답니다",
          "speaker_id": "1",
          "time": "20210902 20:03"
        },
        {
          "id": "MDRW2100026392.1.43",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210902 20:08"
        },
        {
          "id": "MDRW2100026392.1.44",
          "form": "그 맛있는걸",
          "original_form": "그 맛있는걸",
          "speaker_id": "2",
          "time": "20210902 20:08"
        },
        {
          "id": "MDRW2100026392.1.45",
          "form": "집에 고기전용불판이 있어서 거기에 구웠어요.. 아주 맛있었죠..ㅎㅎ",
          "original_form": "집에 고기전용불판이 있어서 거기에 구웠어요.. 아주 맛있었죠..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:09"
        },
        {
          "id": "MDRW2100026392.1.46",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "2",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100026392.1.47",
          "form": "제가 고기 입문을 돼지갈비로 했거든요",
          "original_form": "제가 고기 입문을 돼지갈비로 했거든요",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100026392.1.48",
          "form": "그래서 더 먹고싶네요",
          "original_form": "그래서 더 먹고싶네요",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100026392.1.49",
          "form": "오.. 그랬군요.. 첫인상이 강렬하죠..ㅎㅎ",
          "original_form": "오.. 그랬군요.. 첫인상이 강렬하죠..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100026392.1.50",
          "form": "이번주엔 돼지갈비 먹어야할거같아요",
          "original_form": "이번주엔 돼지갈비 먹어야할거같아요",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100026392.1.51",
          "form": "제 식욕을 자극하셨다는 ....",
          "original_form": "제 식욕을 자극하셨다는 ....",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100026392.1.52",
          "form": "ㅎㅎㅎ 정말 맛있어요.. 사실 맛없기가 어려운 메뉴같아요",
          "original_form": "ㅎㅎㅎ 정말 맛있어요.. 사실 맛없기가 어려운 메뉴같아요",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100026392.1.53",
          "form": "가족들도 다 만족했나요?",
          "original_form": "가족들도 다 만족했나요?",
          "speaker_id": "2",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100026392.1.54",
          "form": "저희집은 엄마가 고기를 안드시거든요",
          "original_form": "저희집은 엄마가 고기를 안드시거든요",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100026392.1.55",
          "form": "그래서 고기반찬이 거의 없어요",
          "original_form": "그래서 고기반찬이 거의 없어요",
