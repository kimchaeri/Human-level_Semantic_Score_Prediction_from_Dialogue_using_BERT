      "utterance": [
        {
          "id": "MDRW2100014470.1.1",
          "form": "안녕하세요~\r\n혹시 좋아하는 책 있으신가요?",
          "original_form": "안녕하세요~\r\n혹시 좋아하는 책 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 19:15"
        },
        {
          "id": "MDRW2100014470.1.2",
          "form": "저 소설 좋아해요!",
          "original_form": "저 소설 좋아해요!",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014470.1.3",
          "form": "무슨 책 좋아하세요?",
          "original_form": "무슨 책 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014470.1.4",
          "form": "저도 소설책 좋아해요!!",
          "original_form": "저도 소설책 좋아해요!!",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014470.1.5",
          "form": "최근에 재미있게 읽으신 책 있나요??",
          "original_form": "최근에 재미있게 읽으신 책 있나요??",
          "speaker_id": "1",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014470.1.6",
          "form": "달러구트 꿈 백화점 맞나..? 혹시 그 책 아시나요???",
          "original_form": "달러구트 꿈 백화점 맞나..? 혹시 그 책 아시나요???",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100014470.1.7",
          "form": "그거 봤어요",
          "original_form": "그거 봤어요",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100014470.1.8",
          "form": "최근에 무슨 책 읽으셨어요??",
          "original_form": "최근에 무슨 책 읽으셨어요??",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100014470.1.9",
          "form": "오 저도 그거 1권 봤어요",
          "original_form": "오 저도 그거 1권 봤어요",
          "speaker_id": "1",
          "time": "20210825 21:49"
        },
        {
          "id": "MDRW2100014470.1.10",
          "form": "이번에 2권 나왔는데 바로 베스트셀러 됐더라구요",
          "original_form": "이번에 2권 나왔는데 바로 베스트셀러 됐더라구요",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014470.1.11",
          "form": "2권도 보셨나요?",
          "original_form": "2권도 보셨나요?",
          "speaker_id": "1",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014470.1.12",
          "form": "2권 나왔어요?!???!?????????",
          "original_form": "2권 나왔어요?!???!?????????",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014470.1.13",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014470.1.14",
          "form": "봐야겠네요 ㅠ",
          "original_form": "봐야겠네요 ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014470.1.15",
          "form": "보셨어요 2권??",
          "original_form": "보셨어요 2권??",
          "speaker_id": "2",
          "time": "20210825 21:50"
        },
        {
          "id": "MDRW2100014470.1.16",
          "form": "저도 아직 못봤어요 ㅠㅠㅋㅋㅋㅋ",
          "original_form": "저도 아직 못봤어요 ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100014470.1.17",
          "form": "나온지 얼마안됐어요!!!",
          "original_form": "나온지 얼마안됐어요!!!",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100014470.1.18",
          "form": "빨리 봐야겠네요...!",
          "original_form": "빨리 봐야겠네요...!",
          "speaker_id": "2",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100014470.1.19",
          "form": "2권도 재밌을 것 같아서 기대중이예요",
          "original_form": "2권도 재밌을 것 같아서 기대중이예요",
          "speaker_id": "1",
          "time": "20210825 21:52"
        },
        {
          "id": "MDRW2100014470.1.20",
          "form": "혹시 추리물도 보시나요??",
          "original_form": "혹시 추리물도 보시나요??",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100014470.1.21",
          "form": "넹넹 ㅋㅋㅋㅋㅋ 추리물 좋아해요 ㅠㅠ",
          "original_form": "넹넹 ㅋㅋㅋㅋㅋ 추리물 좋아해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100014470.1.22",
          "form": "추천해주실 소설 있으신가요??",
          "original_form": "추천해주실 소설 있으신가요??",
          "speaker_id": "1",
          "time": "20210825 21:54"
