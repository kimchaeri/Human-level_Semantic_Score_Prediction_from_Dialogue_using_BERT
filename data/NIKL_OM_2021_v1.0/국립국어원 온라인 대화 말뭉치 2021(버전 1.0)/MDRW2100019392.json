      "utterance": [
        {
          "id": "MDRW2100019392.1.1",
          "form": "그렇죠 ㅎㅎ",
          "original_form": "그렇죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 18:42"
        },
        {
          "id": "MDRW2100019392.1.2",
          "form": "육아 중요하죠ㅋㅋ",
          "original_form": "육아 중요하죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:43"
        },
        {
          "id": "MDRW2100019392.1.3",
          "form": "저희는 육아휴직 돌아가면서",
          "original_form": "저희는 육아휴직 돌아가면서",
          "speaker_id": "1",
          "time": "20210827 18:43"
        },
        {
          "id": "MDRW2100019392.1.4",
          "form": "쓸 계획이에요 ㅎㅎ",
          "original_form": "쓸 계획이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:43"
        },
        {
          "id": "MDRW2100019392.1.5",
          "form": "그렇게 하는게 좋죠",
          "original_form": "그렇게 하는게 좋죠",
          "speaker_id": "2",
          "time": "20210827 18:44"
        },
        {
          "id": "MDRW2100019392.1.6",
          "form": "한사람만 쓰면 너무 부담돼요",
          "original_form": "한사람만 쓰면 너무 부담돼요",
          "speaker_id": "2",
          "time": "20210827 18:44"
        },
        {
          "id": "MDRW2100019392.1.7",
          "form": "회사에서도 쓰라곤해도 돌아가며 적당히 쓰면좋죠 ㅎㅎ",
          "original_form": "회사에서도 쓰라곤해도 돌아가며 적당히 쓰면좋죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 18:45"
        },
        {
          "id": "MDRW2100019392.1.8",
          "form": "아이낳으면 잘도와 주실것 같아요",
          "original_form": "아이낳으면 잘도와 주실것 같아요",
          "speaker_id": "2",
          "time": "20210827 18:45"
        },
        {
          "id": "MDRW2100019392.1.9",
          "form": "맞벌이라서 돕는게 아니라!ㅋㅋ",
          "original_form": "맞벌이라서 돕는게 아니라!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:46"
        },
        {
          "id": "MDRW2100019392.1.10",
          "form": "같이죠!ㅋㅋ",
          "original_form": "같이죠!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:46"
        },
        {
          "id": "MDRW2100019392.1.11",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "2",
          "time": "20210827 18:46"
        },
        {
          "id": "MDRW2100019392.1.12",
          "form": "휴직기간은 어찌 하시게요?",
          "original_form": "휴직기간은 어찌 하시게요?",
          "speaker_id": "2",
          "time": "20210827 18:46"
        },
        {
          "id": "MDRW2100019392.1.13",
          "form": "2년인가요?1년인가요?",
          "original_form": "2년인가요?1년인가요?",
          "speaker_id": "2",
          "time": "20210827 18:46"
        },
        {
          "id": "MDRW2100019392.1.14",
          "form": "얼마나 쓸수있어요",
          "original_form": "얼마나 쓸수있어요",
          "speaker_id": "2",
          "time": "20210827 19:05"
        },
        {
          "id": "MDRW2100019392.1.15",
          "form": "유급이면 좋은데",
          "original_form": "유급이면 좋은데",
          "speaker_id": "2",
          "time": "20210827 19:05"
        },
        {
          "id": "MDRW2100019392.1.16",
          "form": "3년까지인데",
          "original_form": "3년까지인데",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019392.1.17",
          "form": "유급은 1년일거에요ㅎㅎ",
          "original_form": "유급은 1년일거에요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019392.1.18",
          "form": "그렇더라고요",
          "original_form": "그렇더라고요",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019392.1.19",
          "form": "진짜 기네요",
          "original_form": "진짜 기네요",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019392.1.20",
          "form": "ㅋㅋ 1년만 해야죠 무급은 힘들어서ㅋㅋ",
          "original_form": "ㅋㅋ 1년만 해야죠 무급은 힘들어서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019392.1.21",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019392.1.22",
          "form": "무급은 힘들어요",
          "original_form": "무급은 힘들어요",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019392.1.23",
          "form": "일년씩 돌아가며 딱좋네요 ㅎㅎ",
          "original_form": "일년씩 돌아가며 딱좋네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019392.1.24",
          "form": "아직 단지 계획일뿐. . .ㅋㅋ",
          "original_form": "아직 단지 계획일뿐. . .ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100019392.1.25",
          "form": "일단 애기부터 ㅋㅋ",
          "original_form": "일단 애기부터 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100019392.1.26",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:56"
        },
        {
          "id": "MDRW2100019392.1.27",
          "form": "그러게요 금새 생겨서 휴가 쓰실거예요",
          "original_form": "그러게요 금새 생겨서 휴가 쓰실거예요",
          "speaker_id": "2",
          "time": "20210827 20:56"
        },
        {
          "id": "MDRW2100019392.1.28",
          "form": "노력하면 이ㄴ아기 생기겠죠",
          "original_form": "노력하면 이ㄴ아기 생기겠죠",
          "speaker_id": "2",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100019392.1.29",
          "form": "이쁜아기",
          "original_form": "이쁜아기",
          "speaker_id": "2",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100019392.1.30",
          "form": "이제 월급도 많이 받고 승진도 하셔서 많이 많이 버세요 ㅎㅎ",
          "original_form": "이제 월급도 많이 받고 승진도 하셔서 많이 많이 버세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100019392.1.31",
          "form": "만반의 준비를",
          "original_form": "만반의 준비를",
          "speaker_id": "2",
          "time": "20210827 20:57"
        },
        {
          "id": "MDRW2100019392.1.32",
          "form": "ㅋㅋ공기업이라 승진이 느려서",
          "original_form": "ㅋㅋ공기업이라 승진이 느려서",
          "speaker_id": "1",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100019392.1.33",
          "form": "포기했습니다 ㅎㅎ",
          "original_form": "포기했습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:26"
        },
        {
          "id": "MDRW2100019392.1.34",
          "form": "그렇긴 하네요 공기업 느리죠",
          "original_form": "그렇긴 하네요 공기업 느리죠",
          "speaker_id": "2",
          "time": "20210827 21:27"
        },
        {
          "id": "MDRW2100019392.1.35",
          "form": "그래도 좋은데 다니시니",
          "original_form": "그래도 좋은데 다니시니",
          "speaker_id": "2",
          "time": "20210827 21:27"
        },
        {
          "id": "MDRW2100019392.1.36",
          "form": "앞으로 쭉 좋은데요 뭐",
          "original_form": "앞으로 쭉 좋은데요 뭐",
          "speaker_id": "2",
          "time": "20210827 21:27"
        }
      ]
