      "utterance": [
        {
          "id": "MDRW2100019079.1.1",
          "form": "오늘은 주문해야지",
          "original_form": "오늘은 주문해야지",
          "speaker_id": "1",
          "time": "20210827 15:08"
        },
        {
          "id": "MDRW2100019079.1.2",
          "form": "마스크팩 2만원어치 사면 40장인가?",
          "original_form": "마스크팩 2만원어치 사면 40장인가?",
          "speaker_id": "2",
          "time": "20210827 15:12"
        },
        {
          "id": "MDRW2100019079.1.3",
          "form": "그럴걸",
          "original_form": "그럴걸",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019079.1.4",
          "form": "한참 쓰겄네ㅋㅋ",
          "original_form": "한참 쓰겄네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019079.1.5",
          "form": "집에도 좀 있긴한데 쑥을 사야햇",
          "original_form": "집에도 좀 있긴한데 쑥을 사야햇",
          "speaker_id": "1",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019079.1.6",
          "form": "난 일단 토너패드부터 사고 포인트 더 쌓아서 마스크팩 사야겠다",
          "original_form": "난 일단 토너패드부터 사고 포인트 더 쌓아서 마스크팩 사야겠다",
          "speaker_id": "2",
          "time": "20210827 15:55"
        },
        {
          "id": "MDRW2100019079.1.7",
          "form": "이번달 출첵 한번 빼먹음 크흡",
          "original_form": "이번달 출첵 한번 빼먹음 크흡",
          "speaker_id": "1",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019079.1.8",
          "form": "엥 그럼 타격이 상당히 큰데",
          "original_form": "엥 그럼 타격이 상당히 큰데",
          "speaker_id": "2",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100019079.1.9",
          "form": "이미 월초에 빼먹은것 ㅋㅋㅋ",
          "original_form": "이미 월초에 빼먹은것 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:14"
        },
        {
          "id": "MDRW2100019079.1.10",
          "form": "슬슬 찬바람부니 슈퍼바이탈 크림을 꺼내써야겠다",
          "original_form": "슬슬 찬바람부니 슈퍼바이탈 크림을 꺼내써야겠다",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019079.1.11",
          "form": "촥촥하게 발라줘라 ㅋㅋ",
          "original_form": "촥촥하게 발라줘라 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:27"
        },
        {
          "id": "MDRW2100019079.1.12",
          "form": "발라도 그때뿐이고 훅훅 늙는다ㅋㅋ",
          "original_form": "발라도 그때뿐이고 훅훅 늙는다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:22"
        },
        {
          "id": "MDRW2100019079.1.13",
          "form": "그걸 안 했으면 더 늙었을거야 ㅋㅋㅋ 그래 생각해야된다 ㅋㅋㅋㅋ",
          "original_form": "그걸 안 했으면 더 늙었을거야 ㅋㅋㅋ 그래 생각해야된다 ㅋㅋㅋㅋ",
