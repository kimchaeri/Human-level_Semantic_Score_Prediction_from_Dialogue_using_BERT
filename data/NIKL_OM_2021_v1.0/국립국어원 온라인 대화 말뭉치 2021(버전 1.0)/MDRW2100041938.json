        },
        {
          "id": "MDRW2100041938.1.2",
          "form": "언니는 딱 치킨까지ㅋㅋㅋㅋ",
          "original_form": "언니는 딱 치킨까지ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041938.1.3",
          "form": "치킨까지도 힘들 듯ㅋㅋㅋ",
          "original_form": "치킨까지도 힘들 듯ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041938.1.4",
          "form": "ㅋㅋ 내일 니도 보면은 좋은데",
          "original_form": "ㅋㅋ 내일 니도 보면은 좋은데",
          "speaker_id": "1",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041938.1.5",
          "form": "아쉽다",
          "original_form": "아쉽다",
          "speaker_id": "1",
          "time": "20210906 17:53"
        },
        {
          "id": "MDRW2100041938.1.6",
          "form": "name3 언니 시험 치고 나면",
          "original_form": "&name3& 언니 시험 치고 나면",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041938.1.7",
          "form": "목요일날 다 같이 한 봅세",
          "original_form": "목요일날 다 같이 한 봅세",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041938.1.8",
          "form": "내몫까지 맛있게 드시라",
          "original_form": "내몫까지 맛있게 드시라",
          "speaker_id": "2",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041938.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:56"
        },
        {
          "id": "MDRW2100041938.1.10",
          "form": "그러니깐은",
          "original_form": "그러니깐은",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041938.1.11",
          "form": "시험 치고 나면은 다같이 함 보자 ㅋ",
          "original_form": "시험 치고 나면은 다같이 함 보자 ㅋ",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041938.1.12",
          "form": "그때는 주사도 맞았구 괜찮지 않겠나",
          "original_form": "그때는 주사도 맞았구 괜찮지 않겠나",
          "speaker_id": "1",
          "time": "20210906 17:59"
        },
        {
          "id": "MDRW2100041938.1.13",
          "form": "그래 그렇다ㅋㅋㅋㅋ",
          "original_form": "그래 그렇다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:01"
        },
        {
          "id": "MDRW2100041938.1.14",
          "form": "내일 제일 신나게 놀겠고만ㅋㅋㅋㅋㅋ",
