      "utterance": [
        {
          "id": "MDRW2100009747.1.1",
          "form": "어떤 계절 제일 좋아하세요?",
          "original_form": "어떤 계절 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210823 09:18"
        },
        {
          "id": "MDRW2100009747.1.2",
          "form": "저는 겨울을 좋아해요~",
          "original_form": "저는 겨울을 좋아해요~",
          "speaker_id": "2",
          "time": "20210823 09:19"
        },
        {
          "id": "MDRW2100009747.1.3",
          "form": "헐 저도 겨울 제일 좋아하는데",
          "original_form": "헐 저도 겨울 제일 좋아하는데",
          "speaker_id": "1",
          "time": "20210823 09:20"
        },
        {
          "id": "MDRW2100009747.1.4",
          "form": "여름은 더워서 싫더라고요",
          "original_form": "여름은 더워서 싫더라고요",
          "speaker_id": "1",
          "time": "20210823 09:20"
        },
        {
          "id": "MDRW2100009747.1.5",
          "form": "아하 그죠 이번 여름 힘드셨겠어요",
          "original_form": "아하 그죠 이번 여름 힘드셨겠어요",
          "speaker_id": "2",
          "time": "20210823 09:27"
        },
        {
          "id": "MDRW2100009747.1.6",
          "form": "맞아요 이제 슬슬 시원해져서 좋아요",
          "original_form": "맞아요 이제 슬슬 시원해져서 좋아요",
          "speaker_id": "1",
          "time": "20210823 10:01"
        },
        {
          "id": "MDRW2100009747.1.7",
          "form": "오늘 태풍온다던데 밖에 나가시나요?",
          "original_form": "오늘 태풍온다던데 밖에 나가시나요?",
          "speaker_id": "2",
          "time": "20210823 10:16"
        },
        {
          "id": "MDRW2100009747.1.8",
          "form": "지금 나왔다가 집에 들어가는 중이에요",
          "original_form": "지금 나왔다가 집에 들어가는 중이에요",
          "speaker_id": "1",
          "time": "20210823 10:16"
        },
        {
          "id": "MDRW2100009747.1.9",
          "form": "가는동안 비가 안오길 빌고 있어요",
          "original_form": "가는동안 비가 안오길 빌고 있어요",
          "speaker_id": "1",
          "time": "20210823 10:17"
        },
        {
          "id": "MDRW2100009747.1.10",
          "form": "일찍 들어가시네요~",
          "original_form": "일찍 들어가시네요~",
          "speaker_id": "2",
          "time": "20210823 10:18"
        },
        {
          "id": "MDRW2100009747.1.11",
          "form": "저 있는 곳은 벌써부터 조금씩 비가 내리는중이에요ㅜㅜ",
          "original_form": "저 있는 곳은 벌써부터 조금씩 비가 내리는중이에요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 10:18"
        },
        {
          "id": "MDRW2100009747.1.12",
          "form": "헐 저는 왜 몰랐죠?",
          "original_form": "헐 저는 왜 몰랐죠?",
          "speaker_id": "1",
          "time": "20210823 10:22"
        },
        {
          "id": "MDRW2100009747.1.13",
          "form": "오늘 일기예보에 비예고 있었나요?",
          "original_form": "오늘 일기예보에 비예고 있었나요?",
          "speaker_id": "1",
          "time": "20210823 10:23"
        },
        {
          "id": "MDRW2100009747.1.14",
          "form": "네 저희동네는 오후부터 비온다고~ 되어있었는데 벌써부터 부슬부슬 내리고있네요",
          "original_form": "네 저희동네는 오후부터 비온다고~ 되어있었는데 벌써부터 부슬부슬 내리고있네요",
          "speaker_id": "2",
          "time": "20210823 10:27"
        },
        {
          "id": "MDRW2100009747.1.15",
          "form": "제쪽도 이슬비 내리고 있는데 빨리 집에 가야겠어요",
          "original_form": "제쪽도 이슬비 내리고 있는데 빨리 집에 가야겠어요",
          "speaker_id": "1",
          "time": "20210823 10:55"
        },
        {
          "id": "MDRW2100009747.1.16",
          "form": "이제 진짜 가을이 오고 있네요",
          "original_form": "이제 진짜 가을이 오고 있네요",
          "speaker_id": "1",
          "time": "20210823 10:56"
        },
        {
          "id": "MDRW2100009747.1.17",
          "form": "비 안 맞고 잘 들어가셨나요?",
          "original_form": "비 안 맞고 잘 들어가셨나요?",
          "speaker_id": "2",
          "time": "20210825 02:58"
        },
        {
          "id": "MDRW2100009747.1.18",
          "form": "그냥 이번 주 내내 비가 내릴 거 같네요 ㅜㅜ",
          "original_form": "그냥 이번 주 내내 비가 내릴 거 같네요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 02:58"
        },
        {
          "id": "MDRW2100009747.1.19",
          "form": "그 때는 심하게는 안 맞고 갔는데 오늘이 또 문제네요",
          "original_form": "그 때는 심하게는 안 맞고 갔는데 오늘이 또 문제네요",
          "speaker_id": "1",
          "time": "20210825 10:03"
        },
        {
          "id": "MDRW2100009747.1.20",
          "form": "여기는 비가 내리고 있어요 그 쪽은 어떤가요?",
          "original_form": "여기는 비가 내리고 있어요 그 쪽은 어떤가요?",
          "speaker_id": "1",
          "time": "20210825 10:04"
        },
        {
          "id": "MDRW2100009747.1.21",
          "form": "여기도 비가 오락가락 하네요ㅜㅜ",
          "original_form": "여기도 비가 오락가락 하네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 13:26"
        },
        {
          "id": "MDRW2100009747.1.22",
          "form": "안 와서 나가볼까 하면 갑자기 내리고 또 그치고 반복이에요",
          "original_form": "안 와서 나가볼까 하면 갑자기 내리고 또 그치고 반복이에요",
          "speaker_id": "2",
          "time": "20210825 13:26"
        }
      ]
