      "utterance": [
        {
          "id": "MDRW2100007684.1.1",
          "form": "결혼했어요?",
          "original_form": "결혼했어요?",
          "speaker_id": "1",
          "time": "20210822 09:36"
        },
        {
          "id": "MDRW2100007684.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210822 09:37"
        },
        {
          "id": "MDRW2100007684.1.3",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210822 09:37"
        },
        {
          "id": "MDRW2100007684.1.4",
          "form": "집안일에 회사에 힘드시겠어요",
          "original_form": "집안일에 회사에 힘드시겠어요",
          "speaker_id": "2",
          "time": "20210822 09:40"
        },
        {
          "id": "MDRW2100007684.1.5",
          "form": "남편분 많이 도와주세요 ?",
          "original_form": "남편분 많이 도와주세요 ?",
          "speaker_id": "2",
          "time": "20210822 09:40"
        },
        {
          "id": "MDRW2100007684.1.6",
          "form": "아내 많이 도와줍니다",
          "original_form": "아내 많이 도와줍니다",
          "speaker_id": "1",
          "time": "20210822 09:41"
        },
        {
          "id": "MDRW2100007684.1.7",
          "form": "다행입니다",
          "original_form": "다행입니다",
          "speaker_id": "2",
          "time": "20210822 09:42"
        },
        {
          "id": "MDRW2100007684.1.8",
          "form": "안도와주면 서로 감덩 쌓여요",
          "original_form": "안도와주면 서로 감덩 쌓여요",
          "speaker_id": "2",
          "time": "20210822 09:42"
        },
        {
          "id": "MDRW2100007684.1.9",
          "form": "감정",
          "original_form": "감정",
          "speaker_id": "2",
          "time": "20210822 09:42"
        },
        {
          "id": "MDRW2100007684.1.10",
          "form": "보니까 요샌 다들 잘 도와 주더라고요",
          "original_form": "보니까 요샌 다들 잘 도와 주더라고요",
          "speaker_id": "2",
          "time": "20210822 09:43"
        },
        {
          "id": "MDRW2100007684.1.11",
          "form": "주로 뭐 담당하세요",
          "original_form": "주로 뭐 담당하세요",
          "speaker_id": "2",
          "time": "20210822 09:44"
        },
        {
          "id": "MDRW2100007684.1.12",
          "form": "청소담당이요",
          "original_form": "청소담당이요",
          "speaker_id": "1",
          "time": "20210822 09:44"
        },
        {
          "id": "MDRW2100007684.1.13",
          "form": "ㅎㅎ 다들 청소 빨래 담당이던데",
          "original_form": "ㅎㅎ 다들 청소 빨래 담당이던데",
          "speaker_id": "2",
          "time": "20210822 09:44"
        },
        {
          "id": "MDRW2100007684.1.14",
          "form": "깔끔하게 잘할것 같아요",
          "original_form": "깔끔하게 잘할것 같아요",
          "speaker_id": "2",
          "time": "20210822 09:45"
        },
        {
          "id": "MDRW2100007684.1.15",
          "form": "와이프는 그래도 불만이죠",
          "original_form": "와이프는 그래도 불만이죠",
          "speaker_id": "1",
          "time": "20210822 09:45"
        },
        {
          "id": "MDRW2100007684.1.16",
          "form": "ㅋㅋ안도와주는 분 못봐서 그래요",
          "original_form": "ㅋㅋ안도와주는 분 못봐서 그래요",
          "speaker_id": "2",
          "time": "20210822 09:45"
        },
        {
          "id": "MDRW2100007684.1.17",
          "form": "많이 도와 주세요",
          "original_form": "많이 도와 주세요",
          "speaker_id": "2",
          "time": "20210822 09:46"
        },
        {
          "id": "MDRW2100007684.1.18",
          "form": "제 동생은 제부가 음식까지 다하긴해요 동생이 다 못해요",
          "original_form": "제 동생은 제부가 음식까지 다하긴해요 동생이 다 못해요",
          "speaker_id": "2",
          "time": "20210822 09:46"
        },
        {
          "id": "MDRW2100007684.1.19",
          "form": "한다고 하는데 못마땅한 봐요",
          "original_form": "한다고 하는데 못마땅한 봐요",
          "speaker_id": "1",
          "time": "20210822 09:46"
        },
        {
          "id": "MDRW2100007684.1.20",
          "form": "다들 서로 만족 못하죠",
          "original_form": "다들 서로 만족 못하죠",
          "speaker_id": "2",
          "time": "20210822 09:47"
        },
        {
          "id": "MDRW2100007684.1.21",
          "form": "결혼도 어차피 서로 만족하며 사는분이 별로 없을걸요",
          "original_form": "결혼도 어차피 서로 만족하며 사는분이 별로 없을걸요",
          "speaker_id": "2",
          "time": "20210822 09:47"
        },
        {
          "id": "MDRW2100007684.1.22",
          "form": "맞쳐야죠",
          "original_form": "맞쳐야죠",
          "speaker_id": "1",
          "time": "20210822 09:48"
        }
      ]
