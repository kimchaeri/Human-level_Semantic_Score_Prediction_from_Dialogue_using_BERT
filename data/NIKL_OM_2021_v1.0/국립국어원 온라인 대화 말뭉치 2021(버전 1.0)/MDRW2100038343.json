      "utterance": [
        {
          "id": "MDRW2100038343.1.1",
          "form": "위장을 너무 곱게 키우셨군요",
          "original_form": "위장을 너무 곱게 키우셨군요",
          "speaker_id": "2",
          "time": "20210902 10:35"
        },
        {
          "id": "MDRW2100038343.1.2",
          "form": "라면 하나가 정량인가요 설마",
          "original_form": "라면 하나가 정량인가요 설마",
          "speaker_id": "2",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100038343.1.3",
          "form": "짜파구리는 혁명이죠ㅎ",
          "original_form": "짜파구리는 혁명이죠ㅎ",
          "speaker_id": "2",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100038343.1.4",
          "form": "악 위장을곱게ㅋㅈㄱㅈㅋㄱㄲㅈㄱ",
          "original_form": "악 위장을곱게ㅋㅈㄱㅈㅋㄱㄲㅈㄱ",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100038343.1.5",
          "form": "진라면으로 해도 괜찮은데 면 굵기 땜시ㅎ",
          "original_form": "진라면으로 해도 괜찮은데 면 굵기 땜시ㅎ",
          "speaker_id": "2",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100038343.1.6",
          "form": "2봉은컨디션매우좋으면가능합니다.",
          "original_form": "2봉은컨디션매우좋으면가능합니다.",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100038343.1.7",
          "form": "ㅋㅋㅋㄱㅋㄴ",
          "original_form": "ㅋㅋㅋㄱㅋㄴ",
          "speaker_id": "1",
          "time": "20210902 10:36"
        },
        {
          "id": "MDRW2100038343.1.8",
          "form": "맞아요면굵기도비슷해야맛나더라구요",
          "original_form": "맞아요면굵기도비슷해야맛나더라구요",
          "speaker_id": "1",
          "time": "20210902 10:37"
        },
        {
          "id": "MDRW2100038343.1.9",
          "form": "오늘은 컨디션이 최상은 아니시군요ㅎㅎㅎ",
          "original_form": "오늘은 컨디션이 최상은 아니시군요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:48"
        },
        {
          "id": "MDRW2100038343.1.10",
          "form": "옙\r\n그리고\r\n보통저녁에 최상이어서\r\n살찝니다..",
          "original_form": "옙\r\n그리고\r\n보통저녁에 최상이어서\r\n살찝니다..",
          "speaker_id": "1",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100038343.1.11",
          "form": "ㅋㅋㄲㄱㅋ",
          "original_form": "ㅋㅋㄲㄱㅋ",
          "speaker_id": "1",
          "time": "20210902 11:14"
        },
        {
          "id": "MDRW2100038343.1.12",
          "form": "ㅋㅋ 사람 인생사 다 똑같죠ㅋㅋ",
          "original_form": "ㅋㅋ 사람 인생사 다 똑같죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:17"
        },
        {
          "id": "MDRW2100038343.1.13",
          "form": "ㅋㅋㅋㅋㅋㄱ하 이럼안되는데 왜 최상은 저녁에만.",
          "original_form": "ㅋㅋㅋㅋㅋㄱ하 이럼안되는데 왜 최상은 저녁에만.",
          "speaker_id": "1",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100038343.1.14",
          "form": "..흑흑",
          "original_form": "..흑흑",
          "speaker_id": "1",
          "time": "20210902 12:03"
        },
        {
          "id": "MDRW2100038343.1.15",
          "form": "컨디션 최상일 지금은 잘 드셨나요!!",
          "original_form": "컨디션 최상일 지금은 잘 드셨나요!!",
          "speaker_id": "2",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100038343.1.16",
          "form": "네겁나배부르게ㅣ먹고쉬고있습니다ㅋㅋㅋ\r\n저녁잘드셨나요!!!",
          "original_form": "네겁나배부르게ㅣ먹고쉬고있습니다ㅋㅋㅋ\r\n저녁잘드셨나요!!!",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100038343.1.17",
          "form": "ㅎㅎ 백신맞느라 회사 안가니",
          "original_form": "ㅎㅎ 백신맞느라 회사 안가니",
          "speaker_id": "2",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100038343.1.18",
          "form": "간만에 이 시간에 집밥 먹었네요ㅎ",
          "original_form": "간만에 이 시간에 집밥 먹었네요ㅎ",
          "speaker_id": "2",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100038343.1.19",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038343.1.20",
          "form": "아 집밥드셨군용*_*)~ \r\n저3분카레...과자...ㅋㅋㅋㄱㄲㄱㅋㄲ",
          "original_form": "아 집밥드셨군용*_*)~ \r\n저3분카레...과자...ㅋㅋㅋㄱㄲㄱㅋㄲ",
          "speaker_id": "1",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100038343.1.21",
          "form": "??겁나 배부르게 드셨다면서요",
          "original_form": "??겁나 배부르게 드셨다면서요",
          "speaker_id": "2",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100038343.1.22",
