      "utterance": [
        {
          "id": "MDRW2100020123.1.1",
          "form": "사고싶은물건있어?",
          "original_form": "사고싶은물건있어?",
          "speaker_id": "1",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020123.1.2",
          "form": "yes..",
          "original_form": "yes..",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020123.1.3",
          "form": "나는 태블릿 거치대랑",
          "original_form": "나는 태블릿 거치대랑",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020123.1.4",
          "form": "무선이어폰",
          "original_form": "무선이어폰",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020123.1.5",
          "form": "일단 뭐 이정도?",
          "original_form": "일단 뭐 이정도?",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100020123.1.6",
          "form": "넌??",
          "original_form": "넌??",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020123.1.7",
          "form": "나는 무선청소기",
          "original_form": "나는 무선청소기",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020123.1.8",
          "form": "2층 청소하게",
          "original_form": "2층 청소하게",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020123.1.9",
          "form": "그리고 워치같은거 알지",
          "original_form": "그리고 워치같은거 알지",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020123.1.10",
          "form": "애플워치같은거",
          "original_form": "애플워치같은거",
          "speaker_id": "1",
          "time": "20210828 00:07"
        },
        {
          "id": "MDRW2100020123.1.11",
          "form": "나중에 사고싶어 진짜 나중에? 운동 꾸준히 할 때",
          "original_form": "나중에 사고싶어 진짜 나중에? 운동 꾸준히 할 때",
          "speaker_id": "1",
          "time": "20210828 00:08"
        },
        {
          "id": "MDRW2100020123.1.12",
          "form": "아아갤워치 이번에 나왔던데",
          "original_form": "아아갤워치 이번에 나왔던데",
          "speaker_id": "2",
          "time": "20210828 00:10"
        },
        {
          "id": "MDRW2100020123.1.13",
          "form": "근데 너무 비싸지않아??",
          "original_form": "근데 너무 비싸지않아??",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020123.1.14",
          "form": "애플꺼는 스트랩 미포함이래ㅋㅋㅋㅋㅋ",
          "original_form": "애플꺼는 스트랩 미포함이래ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 00:11"
        },
        {
          "id": "MDRW2100020123.1.15",
          "form": "맞아 너무 비싸서 싼거 사려고",
          "original_form": "맞아 너무 비싸서 싼거 사려고",
          "speaker_id": "1",
          "time": "20210828 09:51"
        },
        {
          "id": "MDRW2100020123.1.16",
          "form": "애플은 너무 욕심부린다",
          "original_form": "애플은 너무 욕심부린다",
          "speaker_id": "1",
          "time": "20210828 09:51"
        },
        {
          "id": "MDRW2100020123.1.17",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020123.1.18",
          "form": "양아치야",
          "original_form": "양아치야",
          "speaker_id": "2",
          "time": "20210828 11:16"
        },
        {
          "id": "MDRW2100020123.1.19",
          "form": "그러게",
          "original_form": "그러게",
          "speaker_id": "1",
          "time": "20210828 12:46"
        },
        {
          "id": "MDRW2100020123.1.20",
          "form": "최근에 구매한 물건 있어?",
          "original_form": "최근에 구매한 물건 있어?",
          "speaker_id": "1",
          "time": "20210828 12:46"
        },
        {
          "id": "MDRW2100020123.1.21",
          "form": "태블릿",
          "original_form": "태블릿",
          "speaker_id": "2",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100020123.1.22",
          "form": "있으니까 좋더라",
          "original_form": "있으니까 좋더라",
          "speaker_id": "2",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100020123.1.23",
          "form": "푸헿",
          "original_form": "푸헿",
          "speaker_id": "2",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100020123.1.24",
          "form": "너는 뭐 삼??",
          "original_form": "너는 뭐 삼??",
          "speaker_id": "2",
          "time": "20210828 12:48"
        },
        {
          "id": "MDRW2100020123.1.25",
          "form": "나는 이케아에서 책상이랑 서랍샀어",
          "original_form": "나는 이케아에서 책상이랑 서랍샀어",
          "speaker_id": "1",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100020123.1.26",
          "form": "책상괜찮던데",
          "original_form": "책상괜찮던데",
          "speaker_id": "2",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100020123.1.27",
          "form": "나도 그거 사고싶다",
          "original_form": "나도 그거 사고싶다",
          "speaker_id": "2",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020123.1.28",
          "form": "내꺼 너무 흔들려",
          "original_form": "내꺼 너무 흔들려",
          "speaker_id": "2",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020123.1.29",
          "form": "그치 5만원인데 디자인도 괜찮고",
          "original_form": "그치 5만원인데 디자인도 괜찮고",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020123.1.30",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020123.1.31",
          "form": "근데 저것도 흔들려",
          "original_form": "근데 저것도 흔들려",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020123.1.32",
          "form": "가격도 괜찮네",
          "original_form": "가격도 괜찮네",
          "speaker_id": "2",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020123.1.33",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020123.1.34",
          "form": "저거는 무슨",
          "original_form": "저거는 무슨",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020123.1.35",
          "form": "책상다리 아래에 돌리는게 있는데 그거로 조절해야 될 듯",
          "original_form": "책상다리 아래에 돌리는게 있는데 그거로 조절해야 될 듯",
          "speaker_id": "1",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020123.1.36",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100020123.1.37",
          "form": "그런거 있으면 됐지",
          "original_form": "그런거 있으면 됐지",
          "speaker_id": "2",
          "time": "20210828 13:15"
        }
      ]
