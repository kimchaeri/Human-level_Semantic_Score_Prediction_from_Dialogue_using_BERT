      "utterance": [
        {
          "id": "MDRW2100047740.1.1",
          "form": "저는 땅끄를 제일 많이 봤어요",
          "original_form": "저는 땅끄를 제일 많이 봤어요",
          "speaker_id": "2",
          "time": "20210827 11:56"
        },
        {
          "id": "MDRW2100047740.1.2",
          "form": "주식 어떤거 보세요?",
          "original_form": "주식 어떤거 보세요?",
          "speaker_id": "2",
          "time": "20210827 11:56"
        },
        {
          "id": "MDRW2100047740.1.3",
          "form": "저도 땅끄 봤었어요",
          "original_form": "저도 땅끄 봤었어요",
          "speaker_id": "1",
          "time": "20210827 11:56"
        },
        {
          "id": "MDRW2100047740.1.4",
          "form": "오오! 유명한가봐요?",
          "original_form": "오오! 유명한가봐요?",
          "speaker_id": "2",
          "time": "20210827 11:56"
        },
        {
          "id": "MDRW2100047740.1.5",
          "form": "칼소폭 너무 힘들어서 때려쳤어요 ㅋㅋ",
          "original_form": "칼소폭 너무 힘들어서 때려쳤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:57"
        },
        {
          "id": "MDRW2100047740.1.6",
          "form": "전 주식 경제적 자유민족 봐요",
          "original_form": "전 주식 경제적 자유민족 봐요",
          "speaker_id": "1",
          "time": "20210827 11:57"
        },
        {
          "id": "MDRW2100047740.1.7",
          "form": "공모주만 해서요 ㅎㅎ",
          "original_form": "공모주만 해서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 11:57"
        },
        {
          "id": "MDRW2100047740.1.8",
          "form": "유명해요 땅끄",
          "original_form": "유명해요 땅끄",
          "speaker_id": "1",
          "time": "20210827 11:57"
        },
        {
          "id": "MDRW2100047740.1.9",
          "form": "ㅋㅋ맞아요 힘들어요ㅜㅜ",
          "original_form": "ㅋㅋ맞아요 힘들어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 11:59"
        },
        {
          "id": "MDRW2100047740.1.10",
          "form": "유명하구나.. 추천받았던거라 그냥 별생각없이 했었네요",
          "original_form": "유명하구나.. 추천받았던거라 그냥 별생각없이 했었네요",
          "speaker_id": "2",
          "time": "20210827 11:59"
        },
        {
          "id": "MDRW2100047740.1.11",
          "form": "와 잘 따라 하시나봐요",
          "original_form": "와 잘 따라 하시나봐요",
          "speaker_id": "1",
          "time": "20210827 12:00"
        },
        {
          "id": "MDRW2100047740.1.12",
          "form": "전 못해요 ㅜㅜ",
          "original_form": "전 못해요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 12:00"
        },
        {
          "id": "MDRW2100047740.1.13",
          "form": "어쩌다 한 번 하다보니 괜찮은거 같아요",
          "original_form": "어쩌다 한 번 하다보니 괜찮은거 같아요",
          "speaker_id": "2",
          "time": "20210827 12:01"
        },
        {
          "id": "MDRW2100047740.1.14",
          "form": "힘든건 좀 뛰어넘고요..ㅎㅎ",
          "original_form": "힘든건 좀 뛰어넘고요..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 12:01"
        },
        {
          "id": "MDRW2100047740.1.15",
          "form": "저도 맨날 뛰어넘으니 할게 없어요 ㅋㅋ",
          "original_form": "저도 맨날 뛰어넘으니 할게 없어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100047740.1.16",
          "form": "전 쉬운 엄마티비 봤어요",
          "original_form": "전 쉬운 엄마티비 봤어요",
          "speaker_id": "1",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100047740.1.17",
          "form": "관절에 부담 없는거 봐요",
          "original_form": "관절에 부담 없는거 봐요",
          "speaker_id": "1",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100047740.1.18",
          "form": "오! 관절에 부담 없는 영상인가보네요",
          "original_form": "오! 관절에 부담 없는 영상인가보네요",
          "speaker_id": "2",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100047740.1.19",
          "form": "유튜브 자주 챙겨보세요?",
          "original_form": "유튜브 자주 챙겨보세요?",
          "speaker_id": "2",
          "time": "20210827 12:03"
        },
        {
          "id": "MDRW2100047740.1.20",
