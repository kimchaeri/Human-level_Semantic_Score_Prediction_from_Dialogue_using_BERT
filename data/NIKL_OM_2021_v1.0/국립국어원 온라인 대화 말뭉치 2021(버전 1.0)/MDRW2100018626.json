      "utterance": [
        {
          "id": "MDRW2100018626.1.1",
          "form": "최근에 드라마보신거 있으신가요",
          "original_form": "최근에 드라마보신거 있으신가요",
          "speaker_id": "1",
          "time": "20210827 09:16"
        },
        {
          "id": "MDRW2100018626.1.2",
          "form": "유투브 자주 보세요?",
          "original_form": "유투브 자주 보세요?",
          "speaker_id": "2",
          "time": "20210827 09:16"
        },
        {
          "id": "MDRW2100018626.1.3",
          "form": "저 드라마 오월의청춘 봤어요",
          "original_form": "저 드라마 오월의청춘 봤어요",
          "speaker_id": "2",
          "time": "20210827 09:16"
        },
        {
          "id": "MDRW2100018626.1.4",
          "form": "유튜브 예능 클립 자주봐요",
          "original_form": "유튜브 예능 클립 자주봐요",
          "speaker_id": "1",
          "time": "20210827 09:16"
        },
        {
          "id": "MDRW2100018626.1.5",
          "form": "헉 저도 그거봤어요",
          "original_form": "헉 저도 그거봤어요",
          "speaker_id": "1",
          "time": "20210827 09:17"
        },
        {
          "id": "MDRW2100018626.1.6",
          "form": "오월의 청춘 눈물줄줄",
          "original_form": "오월의 청춘 눈물줄줄",
          "speaker_id": "1",
          "time": "20210827 09:17"
        },
        {
          "id": "MDRW2100018626.1.7",
          "form": "저 마지막화만 못봤어요",
          "original_form": "저 마지막화만 못봤어요",
          "speaker_id": "2",
          "time": "20210827 09:17"
        },
        {
          "id": "MDRW2100018626.1.8",
          "form": "저 이상하게... 마지막화는 못보는 병에 걸려서",
          "original_form": "저 이상하게... 마지막화는 못보는 병에 걸려서",
          "speaker_id": "2",
          "time": "20210827 09:17"
        },
        {
          "id": "MDRW2100018626.1.9",
          "form": "헉..",
          "original_form": "헉..",
          "speaker_id": "1",
          "time": "20210827 09:18"
        },
        {
          "id": "MDRW2100018626.1.10",
          "form": "챙겨보던 드라마는 마지막회를 못보고",
          "original_form": "챙겨보던 드라마는 마지막회를 못보고",
          "speaker_id": "2",
          "time": "20210827 09:18"
        },
        {
          "id": "MDRW2100018626.1.11",
          "form": "그럼.. 명희씨 죽는거까지만 본거네요",
          "original_form": "그럼.. 명희씨 죽는거까지만 본거네요",
          "speaker_id": "1",
          "time": "20210827 09:18"
        },
        {
          "id": "MDRW2100018626.1.12",
          "form": "어쩌다 가끔 보는거만 마지막화를 봐요",
          "original_form": "어쩌다 가끔 보는거만 마지막화를 봐요",
          "speaker_id": "2",
          "time": "20210827 09:18"
        },
        {
          "id": "MDRW2100018626.1.13",
          "form": "히헤 2개 못봤나? 전 아직 명희씨 안죽었어요..",
          "original_form": "히헤 2개 못봤나? 전 아직 명희씨 안죽었어요..",
          "speaker_id": "2",
          "time": "20210827 09:19"
        },
        {
          "id": "MDRW2100018626.1.14",
          "form": "마지막화까지 본 드라마는 무엇인강ㆍ...",
          "original_form": "마지막화까지 본 드라마는 무엇인강ㆍ...",
          "speaker_id": "1",
          "time": "20210827 09:19"
        },
        {
          "id": "MDRW2100018626.1.15",
          "form": "세상에 죄송해요",
          "original_form": "세상에 죄송해요",
          "speaker_id": "1",
          "time": "20210827 09:19"
        },
        {
          "id": "MDRW2100018626.1.16",
          "form": "스포를해버렸네",
          "original_form": "스포를해버렸네",
          "speaker_id": "1",
          "time": "20210827 09:19"
        },
        {
          "id": "MDRW2100018626.1.17",
          "form": "괜찮아요 이미 끝난드라만데요 머!!",
          "original_form": "괜찮아요 이미 끝난드라만데요 머!!",
          "speaker_id": "2",
          "time": "20210827 09:19"
        },
        {
          "id": "MDRW2100018626.1.18",
          "form": "김과장 마지막회 봤고",
          "original_form": "김과장 마지막회 봤고",
          "speaker_id": "2",
          "time": "20210827 09:19"
        },
        {
          "id": "MDRW2100018626.1.19",
          "form": "김과장 재밌게봤어요 또 뭐있나요",
          "original_form": "김과장 재밌게봤어요 또 뭐있나요",
          "speaker_id": "1",
          "time": "20210827 09:20"
        },
        {
          "id": "MDRW2100018626.1.20",
          "form": "음 마지막회 본건 잘 생각 안나고 안본건 질투의화신 뷰티인사이드 별에서온그대 생각나네요 마지막회 못봤어요",
          "original_form": "음 마지막회 본건 잘 생각 안나고 안본건 질투의화신 뷰티인사이드 별에서온그대 생각나네요 마지막회 못봤어요",
          "speaker_id": "2",
          "time": "20210827 09:20"
        },
        {
          "id": "MDRW2100018626.1.21",
          "form": "그 중 가장 좋았던 드라마는 뭐였나요",
          "original_form": "그 중 가장 좋았던 드라마는 뭐였나요",
          "speaker_id": "1",
          "time": "20210827 09:21"
        },
        {
          "id": "MDRW2100018626.1.22",
          "form": "오월의청춘이요....",
          "original_form": "오월의청춘이요....",
          "speaker_id": "2",
          "time": "20210827 09:21"
        },
        {
          "id": "MDRW2100018626.1.23",
          "form": "오청을 감명깊게 보셨구나",
          "original_form": "오청을 감명깊게 보셨구나",
          "speaker_id": "1",
          "time": "20210827 09:21"
        },
        {
          "id": "MDRW2100018626.1.24",
          "form": "한 4회까지 너무 달달하고 좋았는데 자꾸 5월이 다가오는걸 캐릭터들이 인지시켜줘서... 너무 속상했어요",
          "original_form": "한 4회까지 너무 달달하고 좋았는데 자꾸 5월이 다가오는걸 캐릭터들이 인지시켜줘서... 너무 속상했어요",
          "speaker_id": "2",
          "time": "20210827 09:21"
        },
        {
          "id": "MDRW2100018626.1.25",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 09:22"
        },
        {
          "id": "MDRW2100018626.1.26",
          "form": "약간 잔인하죠",
          "original_form": "약간 잔인하죠",
          "speaker_id": "1",
          "time": "20210827 09:22"
        },
        {
          "id": "MDRW2100018626.1.27",
          "form": "행복했던 거리가 하루아침에 피바다가 되는 현실 너무 속상하더라구요",
          "original_form": "행복했던 거리가 하루아침에 피바다가 되는 현실 너무 속상하더라구요",
          "speaker_id": "1",
          "time": "20210827 09:22"
        },
        {
          "id": "MDRW2100018626.1.28",
