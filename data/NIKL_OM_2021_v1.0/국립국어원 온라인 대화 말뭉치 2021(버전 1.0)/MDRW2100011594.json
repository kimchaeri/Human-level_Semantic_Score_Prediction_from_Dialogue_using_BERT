      "utterance": [
        {
          "id": "MDRW2100011594.1.1",
          "form": "ㅎㅎ 음 애들은 쿠키를 좋아할 것 같아요",
          "original_form": "ㅎㅎ 음 애들은 쿠키를 좋아할 것 같아요",
          "speaker_id": "1",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100011594.1.2",
          "form": "맛있기도 하고 쉽고... 쿠키틀로 찍거나 모양 만드는 것도 재밌어서요!",
          "original_form": "맛있기도 하고 쉽고... 쿠키틀로 찍거나 모양 만드는 것도 재밌어서요!",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011594.1.3",
          "form": "맞아요 너무 좋아해요",
          "original_form": "맞아요 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210824 14:39"
        },
        {
          "id": "MDRW2100011594.1.4",
          "form": "엄마인 저늕번거롭구요",
          "original_form": "엄마인 저늕번거롭구요",
          "speaker_id": "2",
          "time": "20210824 14:39"
        },
        {
          "id": "MDRW2100011594.1.5",
          "form": "아ㅋㅋㅋㅋㅠㅠ 그렇긴 하죠",
          "original_form": "아ㅋㅋㅋㅋㅠㅠ 그렇긴 하죠",
          "speaker_id": "1",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011594.1.6",
          "form": "만들 땐 재밌는데 뒷정리가 귀찮아요",
          "original_form": "만들 땐 재밌는데 뒷정리가 귀찮아요",
          "speaker_id": "1",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011594.1.7",
          "form": "네 그리고 버터넣어서",
          "original_form": "네 그리고 버터넣어서",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011594.1.8",
          "form": "너무 번들거리구요",
          "original_form": "너무 번들거리구요",
          "speaker_id": "2",
          "time": "20210824 15:06"
        },
        {
          "id": "MDRW2100011594.1.9",
          "form": "아 맞아요ㅠㅠ",
          "original_form": "아 맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 15:43"
        },
        {
          "id": "MDRW2100011594.1.10",
          "form": "요즘엔 버터 대신 식용유 넣는 레시피도 있더라구요! 버터보단 좀 더 좋으려나...?",
          "original_form": "요즘엔 버터 대신 식용유 넣는 레시피도 있더라구요! 버터보단 좀 더 좋으려나...?",
          "speaker_id": "1",
          "time": "20210824 15:44"
        },
        {
          "id": "MDRW2100011594.1.11",
          "form": "아 그렇게도 넣어요?",
          "original_form": "아 그렇게도 넣어요?",
          "speaker_id": "2",
          "time": "20210824 15:49"
        },
        {
          "id": "MDRW2100011594.1.12",
          "form": "식용유로 대신할생각은 못했는데요",
          "original_form": "식용유로 대신할생각은 못했는데요",
          "speaker_id": "2",
          "time": "20210824 15:49"
        },
        {
          "id": "MDRW2100011594.1.13",
          "form": "레시피대로만 넣으려고 ㅎㅎ",
          "original_form": "레시피대로만 넣으려고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 15:49"
        },
        {
          "id": "MDRW2100011594.1.14",
          "form": "주로어떤거 만드세요?",
          "original_form": "주로어떤거 만드세요?",
          "speaker_id": "2",
          "time": "20210824 15:52"
        },
        {
          "id": "MDRW2100011594.1.15",
          "form": "스콘?",
          "original_form": "스콘?",
          "speaker_id": "2",
          "time": "20210824 15:52"
        },
        {
          "id": "MDRW2100011594.1.16",
          "form": "전엔 주로 쿠키나 스콘같은 제과류 만들었다가 얼마전부터는 제빵 해보는 중이에요!",
          "original_form": "전엔 주로 쿠키나 스콘같은 제과류 만들었다가 얼마전부터는 제빵 해보는 중이에요!",
          "speaker_id": "1",
          "time": "20210824 17:13"
        },
        {
          "id": "MDRW2100011594.1.17",
          "form": "와 그럼 효모? 이런것도 넣구요?",
          "original_form": "와 그럼 효모? 이런것도 넣구요?",
          "speaker_id": "2",
          "time": "20210824 17:14"
        },
        {
          "id": "MDRW2100011594.1.18",
          "form": "네! 효모도 처음 사본지 얼마 안됐어요ㅎㅎ",
          "original_form": "네! 효모도 처음 사본지 얼마 안됐어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 17:16"
        },
        {
          "id": "MDRW2100011594.1.19",
          "form": "오오 전문가의 길로 가시는군요",
          "original_form": "오오 전문가의 길로 가시는군요",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011594.1.20",
          "form": "맘같이 잘되나요?",
          "original_form": "맘같이 잘되나요?",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011594.1.21",
          "form": "이효리가 생각나네요 ㅎㅎ",
          "original_form": "이효리가 생각나네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 17:32"
        }
      ]
    }
