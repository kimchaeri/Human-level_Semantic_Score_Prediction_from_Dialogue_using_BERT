        {
          "id": "MDRW2100013945.1.1",
          "form": "블루 레몬에이드 오늘 마지막 남은 하나 다먹엇어",
          "original_form": "블루 레몬에이드 오늘 마지막 남은 하나 다먹엇어",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013945.1.2",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013945.1.3",
          "form": "얼음 안넣어두 맛잇더라!",
          "original_form": "얼음 안넣어두 맛잇더라!",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013945.1.4",
          "form": "한개 남았었나봐네",
          "original_form": "한개 남았었나봐네",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013945.1.5",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013945.1.6",
          "form": "맛있었겠다",
          "original_form": "맛있었겠다",
          "speaker_id": "1",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013945.1.7",
          "form": "한번더 살까 생각중",
          "original_form": "한번더 살까 생각중",
          "speaker_id": "2",
          "time": "20210825 16:30"
        },
        {
          "id": "MDRW2100013945.1.8",
          "form": "블루레몬에이드 시원하고맛있잖아",
          "original_form": "블루레몬에이드 시원하고맛있잖아",
          "speaker_id": "1",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013945.1.9",
          "form": "나도 포인트로 살수있자나~",
          "original_form": "나도 포인트로 살수있자나~",
          "speaker_id": "2",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013945.1.10",
          "form": "응응 그치",
          "original_form": "응응 그치",
          "speaker_id": "2",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013945.1.11",
          "form": "언니야도 쟈뎅에서 사게?",
          "original_form": "언니야도 쟈뎅에서 사게?",
          "speaker_id": "1",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013945.1.12",
          "form": "어어",
          "original_form": "어어",
          "speaker_id": "2",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013945.1.13",
          "form": "좋다",
          "original_form": "좋다",
          "speaker_id": "1",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013945.1.14",
          "form": "거기 포인트 아직잇어가지구",
          "original_form": "거기 포인트 아직잇어가지구",
          "speaker_id": "2",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013945.1.15",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "2",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013945.1.16",
          "form": "개좋음",
          "original_form": "개좋음",
          "speaker_id": "2",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013945.1.17",
          "form": "이번엔 다른거 사봐",
          "original_form": "이번엔 다른거 사봐",
          "speaker_id": "1",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013945.1.18",
          "form": "다른거 어떤거?",
          "original_form": "다른거 어떤거?",
          "speaker_id": "2",
          "time": "20210825 16:31"
        },
        {
          "id": "MDRW2100013945.1.19",
          "form": "과일음료같은거",
          "original_form": "과일음료같은거",
