      "utterance": [
        {
          "id": "MDRW2100001713.1.1",
          "form": "어제는 시원했는데",
          "original_form": "어제는 시원했는데",
          "speaker_id": "1",
          "time": "20210819 18:19"
        },
        {
          "id": "MDRW2100001713.1.2",
          "form": "오늘도시원하지않니",
          "original_form": "오늘도시원하지않니",
          "speaker_id": "2",
          "time": "20210819 18:19"
        },
        {
          "id": "MDRW2100001713.1.3",
          "form": "이제 날씨가 시원해져서 느린노래를 들을수있어",
          "original_form": "이제 날씨가 시원해져서 느린노래를 들을수있어",
          "speaker_id": "2",
          "time": "20210819 18:20"
        },
        {
          "id": "MDRW2100001713.1.4",
          "form": "어제가 더 시원했는데",
          "original_form": "어제가 더 시원했는데",
          "speaker_id": "1",
          "time": "20210819 18:24"
        },
        {
          "id": "MDRW2100001713.1.5",
          "form": "아쉽",
          "original_form": "아쉽",
          "speaker_id": "1",
          "time": "20210819 18:24"
        },
        {
          "id": "MDRW2100001713.1.6",
          "form": "어제 저녁에 산책하는데 바람불고 진짜 너무좋았어",
          "original_form": "어제 저녁에 산책하는데 바람불고 진짜 너무좋았어",
          "speaker_id": "1",
          "time": "20210819 18:24"
        },
        {
          "id": "MDRW2100001713.1.7",
          "form": "아마자마자",
          "original_form": "아마자마자",
          "speaker_id": "2",
          "time": "20210819 18:24"
        },
        {
          "id": "MDRW2100001713.1.8",
          "form": "이제운동하고나와서 집갈때",
          "original_form": "이제운동하고나와서 집갈때",
          "speaker_id": "2",
          "time": "20210819 18:24"
        },
        {
          "id": "MDRW2100001713.1.9",
          "form": "땀이 바로 다 식어버려",
          "original_form": "땀이 바로 다 식어버려",
          "speaker_id": "2",
          "time": "20210819 18:24"
        },
        {
          "id": "MDRW2100001713.1.10",
          "form": "ㅋㅋㅋ진짜 행복..",
          "original_form": "ㅋㅋㅋ진짜 행복..",
          "speaker_id": "1",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001713.1.11",
          "form": "처서 지나면 더 시원해질까?",
          "original_form": "처서 지나면 더 시원해질까?",
          "speaker_id": "1",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001713.1.12",
          "form": "당연",
          "original_form": "당연",
          "speaker_id": "2",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001713.1.13",
          "form": "처서는 찐이야",
          "original_form": "처서는 찐이야",
          "speaker_id": "2",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001713.1.14",
          "form": "이번주주말에도 비오던데",
          "original_form": "이번주주말에도 비오던데",
          "speaker_id": "2",
          "time": "20210819 18:26"
        },
        {
          "id": "MDRW2100001713.1.15",
          "form": "그리거바로 처서니까",
          "original_form": "그리거바로 처서니까",
          "speaker_id": "2",
          "time": "20210819 18:27"
        },
        {
          "id": "MDRW2100001713.1.16",
          "form": "밤에는 추워지지낞을까",
          "original_form": "밤에는 추워지지낞을까",
          "speaker_id": "2",
          "time": "20210819 18:27"
        },
        {
          "id": "MDRW2100001713.1.17",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "1",
          "time": "20210819 18:35"
        },
        {
          "id": "MDRW2100001713.1.18",
          "form": "비까지오면ㅋㅋㅋㅋㅋ",
          "original_form": "비까지오면ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:35"
        },
        {
          "id": "MDRW2100001713.1.19",
          "form": "제발...제발 처서매직",
          "original_form": "제발...제발 처서매직",
          "speaker_id": "1",
          "time": "20210819 18:35"
        },
        {
          "id": "MDRW2100001713.1.20",
          "form": "마자 믿을수있어",
          "original_form": "마자 믿을수있어",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100001713.1.21",
          "form": "빨리 추워젺으면",
          "original_form": "빨리 추워젺으면",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100001713.1.22",
          "form": "좋겠어",
          "original_form": "좋겠어",
          "speaker_id": "2",
          "time": "20210819 18:51"
        },
        {
          "id": "MDRW2100001713.1.23",
          "form": "ㅋㅋ근데 또 추워지면 욕하겠지...??",
          "original_form": "ㅋㅋ근데 또 추워지면 욕하겠지...??",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100001713.1.24",
          "form": "사람은 정말 웃겨ㅋㅋㅋㅋㅋㅋ",
          "original_form": "사람은 정말 웃겨ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100001713.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ맞아",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ맞아",
          "speaker_id": "2",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100001713.1.26",
          "form": "하지만 빨리",
          "original_form": "하지만 빨리",
          "speaker_id": "2",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100001713.1.27",
          "form": "적당히 추웠으면좋겠다",
          "original_form": "적당히 추웠으면좋겠다",
          "speaker_id": "1",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100001713.1.28",
          "form": "패딩입고싶어",
          "original_form": "패딩입고싶어",
          "speaker_id": "2",
          "time": "20210819 18:52"
        },
        {
          "id": "MDRW2100001713.1.29",
          "form": "가을이최고야",
          "original_form": "가을이최고야",
          "speaker_id": "1",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100001713.1.30",
          "form": "마ㅂ자",
          "original_form": "마ㅂ자",
          "speaker_id": "2",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100001713.1.31",
          "form": "대신 미세먼지없는가을",
          "original_form": "대신 미세먼지없는가을",
          "speaker_id": "2",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100001713.1.32",
          "form": "난늦가을",
          "original_form": "난늦가을",
          "speaker_id": "2",
          "time": "20210819 18:53"
        },
        {
          "id": "MDRW2100001713.1.33",
          "form": "ㅋㅋㅋ미세먼지 진짜 없어야됨",
          "original_form": "ㅋㅋㅋ미세먼지 진짜 없어야됨",
          "speaker_id": "1",
          "time": "20210819 18:53"
        }
      ]
    }
