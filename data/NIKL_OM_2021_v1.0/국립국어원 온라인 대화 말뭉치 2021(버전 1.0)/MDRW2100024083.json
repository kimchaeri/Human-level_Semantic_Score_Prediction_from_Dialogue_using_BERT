        {
          "id": "MDRW2100024083.1.1",
          "form": "메타버스가자",
          "original_form": "메타버스가자",
          "speaker_id": "2",
          "time": "20210830 22:00"
        },
        {
          "id": "MDRW2100024083.1.2",
          "form": "어떤거?",
          "original_form": "어떤거?",
          "speaker_id": "1",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100024083.1.3",
          "form": "마소",
          "original_form": "마소",
          "speaker_id": "2",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100024083.1.4",
          "form": "야 너무 비싸지 않나",
          "original_form": "야 너무 비싸지 않나",
          "speaker_id": "1",
          "time": "20210830 22:12"
        },
        {
          "id": "MDRW2100024083.1.5",
          "form": "근데 계속 더오를거같어....",
          "original_form": "근데 계속 더오를거같어....",
          "speaker_id": "2",
          "time": "20210830 22:27"
        },
        {
          "id": "MDRW2100024083.1.6",
          "form": "샀어?",
          "original_form": "샀어?",
          "speaker_id": "1",
          "time": "20210830 22:34"
        },
        {
          "id": "MDRW2100024083.1.7",
          "form": "못사 비싸서 ㅜㅜ왜케가난할까",
          "original_form": "못사 비싸서 ㅜㅜ왜케가난할까",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024083.1.8",
          "form": "너무비싸",
          "original_form": "너무비싸",
          "speaker_id": "1",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024083.1.9",
          "form": "요즘 미국주식 소숫점 단위로파는",
          "original_form": "요즘 미국주식 소숫점 단위로파는",
          "speaker_id": "1",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024083.1.10",
          "form": "증권 어플도 있다는데",
          "original_form": "증권 어플도 있다는데",
          "speaker_id": "1",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100024083.1.11",
          "form": "한툰가 미래에셋에 있너",
          "original_form": "한툰가 미래에셋에 있너",
          "speaker_id": "2",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100024083.1.12",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "1",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100024083.1.13",
          "form": "페이스북도 메타버스라는데",
          "original_form": "페이스북도 메타버스라는데",
          "speaker_id": "1",
          "time": "20210830 22:38"
        },
        {
          "id": "MDRW2100024083.1.14",
          "form": "로봇청소기 써봤어??",
          "original_form": "로봇청소기 써봤어??",
          "speaker_id": "2",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100024083.1.15",
          "form": "아니 쓰고있어?",
          "original_form": "아니 쓰고있어?",
          "speaker_id": "1",
          "time": "20210902 16:03"
        },
        {
          "id": "MDRW2100024083.1.16",
          "form": "아니  많이들 사서 쓴다는데",
          "original_form": "아니  많이들 사서 쓴다는데",
          "speaker_id": "2",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100024083.1.17",
          "form": "주변에는 없네",
          "original_form": "주변에는 없네",
          "speaker_id": "2",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100024083.1.18",
          "form": "후기들어보고 사볼까했었는데",
          "original_form": "후기들어보고 사볼까했었는데",
