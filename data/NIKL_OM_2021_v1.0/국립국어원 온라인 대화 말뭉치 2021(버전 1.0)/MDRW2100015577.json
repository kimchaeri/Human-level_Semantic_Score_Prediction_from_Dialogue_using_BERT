      "utterance": [
        {
          "id": "MDRW2100015577.1.1",
          "form": "name2님 회 조아하세요?",
          "original_form": "&name2&님 회 조아하세요?",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015577.1.2",
          "form": "넵!! 해물은 다 좋아해요",
          "original_form": "넵!! 해물은 다 좋아해요",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015577.1.3",
          "form": "회 좋아하세용?",
          "original_form": "회 좋아하세용?",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015577.1.4",
          "form": "헐 저두요~~",
          "original_form": "헐 저두요~~",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015577.1.5",
          "form": "해물찜도 좋구!",
          "original_form": "해물찜도 좋구!",
          "speaker_id": "1",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015577.1.6",
          "form": "저는 갑각류도 좋아해요ㅋㅋ",
          "original_form": "저는 갑각류도 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015577.1.7",
          "form": "특히 대게는 진짜 사랑해요",
          "original_form": "특히 대게는 진짜 사랑해요",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015577.1.8",
          "form": "헐 저두요..",
          "original_form": "헐 저두요..",
          "speaker_id": "1",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015577.1.9",
          "form": "갑각류는 그리고 뜬금없이 알러지 생기는 경우도 있어서",
          "original_form": "갑각류는 그리고 뜬금없이 알러지 생기는 경우도 있어서",
          "speaker_id": "1",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015577.1.10",
          "form": "먹을 수 있을 때 많이 먹어둬야된다고 생각해요..ㅎ",
          "original_form": "먹을 수 있을 때 많이 먹어둬야된다고 생각해요..ㅎ",
          "speaker_id": "1",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015577.1.11",
          "form": "헐 몰랐어요",
          "original_form": "헐 몰랐어요",
          "speaker_id": "2",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015577.1.12",
          "form": "더 열심히 먹어야겠어요ㅋㅋㅋ",
          "original_form": "더 열심히 먹어야겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015577.1.13",
          "form": "새우도 좋아하세요?",
          "original_form": "새우도 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015577.1.14",
          "form": "전 새우도 진짜 조아해요ㅠㅠ",
          "original_form": "전 새우도 진짜 조아해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015577.1.15",
          "form": "넵ㅎㅎ 이제 대하철 와서 넘 좋아용",
          "original_form": "넵ㅎㅎ 이제 대하철 와서 넘 좋아용",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015577.1.16",
          "form": "혹시 갯가재 아세요?",
          "original_form": "혹시 갯가재 아세요?",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015577.1.17",
          "form": "들어보긴 했는데 먹어보진 못했어요",
          "original_form": "들어보긴 했는데 먹어보진 못했어요",
          "speaker_id": "1",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015577.1.18",
          "form": "맛있나요?!",
          "original_form": "맛있나요?!",
          "speaker_id": "1",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015577.1.19",
          "form": "쪄먹으면 진짜 맛있어요",
          "original_form": "쪄먹으면 진짜 맛있어요",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015577.1.20",
          "form": "가격도 저렴하고",
          "original_form": "가격도 저렴하고",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015577.1.21",
          "form": "꼭 드셔보세요!!",
          "original_form": "꼭 드셔보세요!!",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015577.1.22",
          "form": "찾아봐야겠어요",
          "original_form": "찾아봐야겠어요",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015577.1.23",
          "form": "소금구이도 맛있고ㅠㅠ",
          "original_form": "소금구이도 맛있고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015577.1.24",
          "form": "머리로 버터구이 하면 진짜 존맛탱이죠ㅎㅎ",
          "original_form": "머리로 버터구이 하면 진짜 존맛탱이죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015577.1.25",
          "form": "하 진짜 뭘 아시네요",
          "original_form": "하 진짜 뭘 아시네요",
          "speaker_id": "1",
          "time": "20210825 23:45"
        }
      ]
    }
