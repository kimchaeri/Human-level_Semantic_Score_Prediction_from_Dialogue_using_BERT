      "utterance": [
        {
          "id": "MDRW2100023656.1.1",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "1",
          "time": "20210830 13:51"
        },
        {
          "id": "MDRW2100023656.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100023656.1.3",
          "form": "아직야ㅡ",
          "original_form": "아직야ㅡ",
          "speaker_id": "2",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100023656.1.4",
          "form": "이요",
          "original_form": "이요",
          "speaker_id": "2",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100023656.1.5",
          "form": "엇 안드시나요?",
          "original_form": "엇 안드시나요?",
          "speaker_id": "1",
          "time": "20210830 13:56"
        },
        {
          "id": "MDRW2100023656.1.6",
          "form": "출근하다보니 잘못챙겨먹더라구요",
          "original_form": "출근하다보니 잘못챙겨먹더라구요",
          "speaker_id": "2",
          "time": "20210830 13:57"
        },
        {
          "id": "MDRW2100023656.1.7",
          "form": "아ㅠ 간식은요?",
          "original_form": "아ㅠ 간식은요?",
          "speaker_id": "1",
          "time": "20210830 14:56"
        },
        {
          "id": "MDRW2100023656.1.8",
          "form": "음료수 마십니다 간식은 눈치보여서 못먹구요",
          "original_form": "음료수 마십니다 간식은 눈치보여서 못먹구요",
          "speaker_id": "2",
          "time": "20210830 15:12"
        },
        {
          "id": "MDRW2100023656.1.9",
          "form": "그렇구나ㅜ",
          "original_form": "그렇구나ㅜ",
          "speaker_id": "1",
          "time": "20210830 15:13"
        },
        {
          "id": "MDRW2100023656.1.10",
          "form": "저녁까지 아직 꽤 남았는데 배고시프시겠어요",
          "original_form": "저녁까지 아직 꽤 남았는데 배고시프시겠어요",
          "speaker_id": "1",
          "time": "20210830 15:13"
        },
        {
          "id": "MDRW2100023656.1.11",
          "form": "직장생활이 다그렇죠...",
          "original_form": "직장생활이 다그렇죠...",
          "speaker_id": "2",
          "time": "20210830 15:13"
        },
        {
          "id": "MDRW2100023656.1.12",
          "form": "배고프죠 참아야해요ㅠ",
          "original_form": "배고프죠 참아야해요ㅠ",
          "speaker_id": "2",
          "time": "20210830 15:14"
        },
        {
          "id": "MDRW2100023656.1.13",
          "form": "그쵸ㅜㅠ",
          "original_form": "그쵸ㅜㅠ",
          "speaker_id": "1",
          "time": "20210830 15:15"
        },
        {
          "id": "MDRW2100023656.1.14",
          "form": "님은 요리도 하시나요?",
          "original_form": "님은 요리도 하시나요?",
          "speaker_id": "1",
          "time": "20210830 15:15"
        },
        {
          "id": "MDRW2100023656.1.15",
          "form": "요리 가끔 해먹습니다",
          "original_form": "요리 가끔 해먹습니다",
          "speaker_id": "2",
          "time": "20210830 15:16"
        },
        {
          "id": "MDRW2100023656.1.16",
          "form": "오~ 어떤거 잘하세요?",
          "original_form": "오~ 어떤거 잘하세요?",
          "speaker_id": "1",
          "time": "20210830 15:16"
        },
        {
          "id": "MDRW2100023656.1.17",
          "form": "레시피만 있으면 다잘해요ㅋㅋㅋ",
          "original_form": "레시피만 있으면 다잘해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:17"
        },
        {
          "id": "MDRW2100023656.1.18",
          "form": "없으면...계란정도?",
          "original_form": "없으면...계란정도?",
          "speaker_id": "2",
          "time": "20210830 15:18"
        },
        {
          "id": "MDRW2100023656.1.19",
          "form": "오ㅋㅋㅋㅋ 능력자시네요",
          "original_form": "오ㅋㅋㅋㅋ 능력자시네요",
