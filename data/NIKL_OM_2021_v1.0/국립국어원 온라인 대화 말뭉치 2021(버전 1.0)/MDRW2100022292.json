      "utterance": [
        {
          "id": "MDRW2100022292.1.1",
          "form": "저 고수 완전 잘먹어요 ㅋㅋㅋ",
          "original_form": "저 고수 완전 잘먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:10"
        },
        {
          "id": "MDRW2100022292.1.2",
          "form": "둘 다 드시나요.. 대박적.... 고수",
          "original_form": "둘 다 드시나요.. 대박적.... 고수",
          "speaker_id": "1",
          "time": "20210829 12:10"
        },
        {
          "id": "MDRW2100022292.1.3",
          "form": "동남아 여행 가기 제격이죠?",
          "original_form": "동남아 여행 가기 제격이죠?",
          "speaker_id": "2",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.4",
          "form": "무슨맛인가요",
          "original_form": "무슨맛인가요",
          "speaker_id": "1",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.5",
          "form": "그러게요ㅋㅋㅋ",
          "original_form": "그러게요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.6",
          "form": "고수는 그냥 풀맛 아니에요?",
          "original_form": "고수는 그냥 풀맛 아니에요?",
          "speaker_id": "2",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.7",
          "form": "잘 모르겠어요 뭔지 ㅠㅠㅠ",
          "original_form": "잘 모르겠어요 뭔지 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.8",
          "form": "그럴리가",
          "original_form": "그럴리가",
          "speaker_id": "1",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.9",
          "form": "퐁퐁맛나는데",
          "original_form": "퐁퐁맛나는데",
          "speaker_id": "1",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.10",
          "form": "이게 안느껴지시나요",
          "original_form": "이게 안느껴지시나요",
          "speaker_id": "1",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.11",
          "form": "퐁퐁..ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ??",
          "original_form": "퐁퐁..ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ??",
          "speaker_id": "2",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.12",
          "form": "전혀 안느껴지던데요",
          "original_form": "전혀 안느껴지던데요",
          "speaker_id": "2",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.13",
          "form": "아닌가요",
          "original_form": "아닌가요",
          "speaker_id": "1",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.14",
          "form": "저 베트남에서",
          "original_form": "저 베트남에서",
          "speaker_id": "2",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.15",
          "form": "쌀로 만든 국수에다가",
          "original_form": "쌀로 만든 국수에다가",
          "speaker_id": "2",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.16",
          "form": "저 그래서 쌀국수 먹자마자 뱉었어요",
          "original_form": "저 그래서 쌀국수 먹자마자 뱉었어요",
          "speaker_id": "1",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.17",
          "form": "엄청 추가해서 먹었어요",
          "original_form": "엄청 추가해서 먹었어요",
          "speaker_id": "2",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.19",
          "form": "추가했다고요?",
          "original_form": "추가했다고요?",
          "speaker_id": "1",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ님이랑 동남아 여행가게 되면",
          "original_form": "ㅋㅋㅋㅋㅋㅋ님이랑 동남아 여행가게 되면",
          "speaker_id": "2",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.21",
          "form": "대박적",
          "original_form": "대박적",
          "speaker_id": "1",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.22",
          "form": "제가 다 먹어드릴게요",
          "original_form": "제가 다 먹어드릴게요",
          "speaker_id": "2",
          "time": "20210829 12:11"
        },
        {
          "id": "MDRW2100022292.1.23",
          "form": "앗.. 진짜요,",
          "original_form": "앗.. 진짜요,",
          "speaker_id": "1",
          "time": "20210829 12:12"
        },
        {
          "id": "MDRW2100022292.1.24",
          "form": "고수 쯔란 다 주세요 저한테",
          "original_form": "고수 쯔란 다 주세요 저한테",
          "speaker_id": "2",
          "time": "20210829 12:12"
        },
        {
          "id": "MDRW2100022292.1.25",
          "form": "고맙습니다",
          "original_form": "고맙습니다",
          "speaker_id": "1",
          "time": "20210829 12:12"
        }
      ]
