      "utterance": [
        {
          "id": "MDRW2100010846.1.1",
          "form": "여행지 어디에 가고 싶나요?",
          "original_form": "여행지 어디에 가고 싶나요?",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010846.1.2",
          "form": "저는 영천 가고 싶어요 별보러",
          "original_form": "저는 영천 가고 싶어요 별보러",
          "speaker_id": "2",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010846.1.3",
          "form": "오우 저도 영천 좋아요",
          "original_form": "오우 저도 영천 좋아요",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010846.1.4",
          "form": "별이 너무 이뻐요",
          "original_form": "별이 너무 이뻐요",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010846.1.5",
          "form": "별 보는 거 좋아하세요?",
          "original_form": "별 보는 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010846.1.6",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "2",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010846.1.7",
          "form": "네 정말 좋아해요",
          "original_form": "네 정말 좋아해요",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010846.1.8",
          "form": "놀이공원은",
          "original_form": "놀이공원은",
          "speaker_id": "2",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010846.1.9",
          "form": "어떠세요",
          "original_form": "어떠세요",
          "speaker_id": "2",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010846.1.10",
          "form": "놀이공원도 정말정말 좋아해요",
          "original_form": "놀이공원도 정말정말 좋아해요",
          "speaker_id": "1",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010846.1.11",
          "form": "어디 가보셨나요",
          "original_form": "어디 가보셨나요",
          "speaker_id": "2",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010846.1.12",
          "form": "경주월드랑 에버랜드에 가봤어요",
          "original_form": "경주월드랑 에버랜드에 가봤어요",
          "speaker_id": "1",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010846.1.13",
          "form": "롯데월드는요?",
          "original_form": "롯데월드는요?",
          "speaker_id": "2",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010846.1.14",
          "form": "아직 안 가봤어요ㅠㅠ",
          "original_form": "아직 안 가봤어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010846.1.15",
          "form": "헐~",
          "original_form": "헐~",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010846.1.16",
          "form": "님은요???",
          "original_form": "님은요???",
          "speaker_id": "1",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010846.1.17",
          "form": "저는 롯데월드도 가봤어요",
          "original_form": "저는 롯데월드도 가봤어요",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010846.1.18",
          "form": "우와 좋겠다",
          "original_form": "우와 좋겠다",
          "speaker_id": "1",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010846.1.19",
          "form": "가서 뭐하셨나요",
          "original_form": "가서 뭐하셨나요",
          "speaker_id": "1",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010846.1.20",
          "form": "저 놀이기구탔어요",
          "original_form": "저 놀이기구탔어요",
          "speaker_id": "2",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010846.1.21",
          "form": "혜성특급",
          "original_form": "혜성특급",
          "speaker_id": "2",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010846.1.22",
          "form": "어떤 놀이기구요?",
          "original_form": "어떤 놀이기구요?",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010846.1.23",
          "form": "아틀란티스는 못탔어요",
          "original_form": "아틀란티스는 못탔어요",
          "speaker_id": "2",
          "time": "20210823 22:28"
        }
      ]
