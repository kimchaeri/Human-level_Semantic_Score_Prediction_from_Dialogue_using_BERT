      "utterance": [
        {
          "id": "MDRW2100022206.1.1",
          "form": "아직이요 아침드셨어요?",
          "original_form": "아직이요 아침드셨어요?",
          "speaker_id": "2",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100022206.1.2",
          "form": "저도 아직 안먹었는데 왜 안드셨어용?",
          "original_form": "저도 아직 안먹었는데 왜 안드셨어용?",
          "speaker_id": "1",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100022206.1.3",
          "form": "주말이라 늦잠자서요 점심부터 먹을거예요",
          "original_form": "주말이라 늦잠자서요 점심부터 먹을거예요",
          "speaker_id": "2",
          "time": "20210829 11:11"
        },
        {
          "id": "MDRW2100022206.1.4",
          "form": "왜 안드셨어요?",
          "original_form": "왜 안드셨어요?",
          "speaker_id": "2",
          "time": "20210829 11:11"
        },
        {
          "id": "MDRW2100022206.1.5",
          "form": "저도 같은 이유에요 ㅋㅋㅋ",
          "original_form": "저도 같은 이유에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100022206.1.6",
          "form": "좀 늦잠자가지고.. 그냥 점심 때 먹는 게 낫겠다 싶었어요",
          "original_form": "좀 늦잠자가지고.. 그냥 점심 때 먹는 게 낫겠다 싶었어요",
          "speaker_id": "1",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100022206.1.7",
          "form": "점심은 뭐 드시게요?",
          "original_form": "점심은 뭐 드시게요?",
          "speaker_id": "1",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100022206.1.8",
          "form": "글쎄요 아직 안정하긴했는데 간단하게 샌드위치 해먹을거 같아요",
          "original_form": "글쎄요 아직 안정하긴했는데 간단하게 샌드위치 해먹을거 같아요",
          "speaker_id": "2",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100022206.1.9",
          "form": "뭐 드실거예요?",
          "original_form": "뭐 드실거예요?",
          "speaker_id": "2",
          "time": "20210829 11:20"
        },
        {
          "id": "MDRW2100022206.1.10",
          "form": "저 참치뽈찜 먹으려고요!!!",
          "original_form": "저 참치뽈찜 먹으려고요!!!",
          "speaker_id": "1",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100022206.1.11",
          "form": "이거 먹어봤어요?",
          "original_form": "이거 먹어봤어요?",
          "speaker_id": "1",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100022206.1.12",
          "form": "네! 맛있죠 시켜드시나요?",
          "original_form": "네! 맛있죠 시켜드시나요?",
          "speaker_id": "2",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100022206.1.13",
          "form": "아뇽~ 저 이거 냉동식품 있어서",
          "original_form": "아뇽~ 저 이거 냉동식품 있어서",
          "speaker_id": "1",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100022206.1.14",
          "form": "어제 해동시켜놨어요!!",
          "original_form": "어제 해동시켜놨어요!!",
          "speaker_id": "1",
          "time": "20210829 11:28"
        },
        {
          "id": "MDRW2100022206.1.15",
          "form": "아하 간편하고 좋네요",
          "original_form": "아하 간편하고 좋네요",
          "speaker_id": "2",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100022206.1.16",
          "form": "저도 알아봐야겠어요 괜찮네요",
          "original_form": "저도 알아봐야겠어요 괜찮네요",
          "speaker_id": "2",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100022206.1.17",
          "form": "생선 좋아하세요?",
          "original_form": "생선 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100022206.1.18",
          "form": "생선 예전에 좋아했는데 일본 방사능 사건 이후 안먹어요",
          "original_form": "생선 예전에 좋아했는데 일본 방사능 사건 이후 안먹어요",
          "speaker_id": "1",
          "time": "20210829 11:43"
        },
        {
          "id": "MDRW2100022206.1.19",
          "form": "그래도 종종 연어나 회같은 건 여전히 먹구요...",
          "original_form": "그래도 종종 연어나 회같은 건 여전히 먹구요...",
          "speaker_id": "1",
          "time": "20210829 11:43"
        },
        {
          "id": "MDRW2100022206.1.20",
          "form": "아무래도 걱정돼죠...",
          "original_form": "아무래도 걱정돼죠...",
          "speaker_id": "2",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100022206.1.21",
          "form": "그럼 고기류는 자주드시나요?",
          "original_form": "그럼 고기류는 자주드시나요?",
          "speaker_id": "2",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100022206.1.22",
          "form": "저 엄청 자주 먹어요",
          "original_form": "저 엄청 자주 먹어요",
          "speaker_id": "1",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022206.1.23",
          "form": "거의 매일 먹늗나 해도 과언이 아니에요",
          "original_form": "거의 매일 먹늗나 해도 과언이 아니에요",
          "speaker_id": "1",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022206.1.24",
          "form": "어떤고기를 그렇게 자주 드시나요?",
          "original_form": "어떤고기를 그렇게 자주 드시나요?",
          "speaker_id": "2",
          "time": "20210829 14:12"
        },
        {
          "id": "MDRW2100022206.1.25",
          "form": "저 돼지고기ㅣ요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 돼지고기ㅣ요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:13"
        },
        {
          "id": "MDRW2100022206.1.26",
          "form": "저렴한 부위 왕창 사서 먹어요 ㅠ",
          "original_form": "저렴한 부위 왕창 사서 먹어요 ㅠ",
          "speaker_id": "1",
          "time": "20210829 14:13"
        }
      ]
    }
