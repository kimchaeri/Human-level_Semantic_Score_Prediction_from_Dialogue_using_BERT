      "utterance": [
        {
          "id": "MDRW2100014316.1.1",
          "form": "얼른 코로나끝나고 공연 가고싶어",
          "original_form": "얼른 코로나끝나고 공연 가고싶어",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014316.1.2",
          "form": "어나도나도ㅜㅜ콘서트진짜가고싶다",
          "original_form": "어나도나도ㅜㅜ콘서트진짜가고싶다",
          "speaker_id": "1",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014316.1.3",
          "form": "김재환이...ㅜㅜ",
          "original_form": "김재환이...ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 18:25"
        },
        {
          "id": "MDRW2100014316.1.4",
          "form": "얼른 콘서트가고싶어 아스트로",
          "original_form": "얼른 콘서트가고싶어 아스트로",
          "speaker_id": "2",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014316.1.5",
          "form": "저번에 티켓 예약 도와줘서 고마워",
          "original_form": "저번에 티켓 예약 도와줘서 고마워",
          "speaker_id": "2",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014316.1.6",
          "form": "그것이 언제지...",
          "original_form": "그것이 언제지...",
          "speaker_id": "1",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014316.1.7",
          "form": "나기억이안나 ...",
          "original_form": "나기억이안나 ...",
          "speaker_id": "1",
          "time": "20210825 18:27"
        },
        {
          "id": "MDRW2100014316.1.8",
          "form": "여러개도와줬어",
          "original_form": "여러개도와줬어",
          "speaker_id": "2",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100014316.1.9",
          "form": "하두오래돼서 까먹었다ㅠㅡㅠㅋㅋㅋㅋ",
          "original_form": "하두오래돼서 까먹었다ㅠㅡㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014316.1.10",
          "form": "우리그때콘서트 같이갔을때 완전 재밌었는데",
          "original_form": "우리그때콘서트 같이갔을때 완전 재밌었는데",
          "speaker_id": "1",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014316.1.11",
          "form": "맞아 짱 재밌었어 다음에도 같이가자",
          "original_form": "맞아 짱 재밌었어 다음에도 같이가자",
          "speaker_id": "2",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100014316.1.12",
          "form": "어어 줄서서응원봉도사고 나눔도 받고",
          "original_form": "어어 줄서서응원봉도사고 나눔도 받고",
          "speaker_id": "1",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100014316.1.13",
          "form": "포토카드랑 나눔 받아서 좋았어",
          "original_form": "포토카드랑 나눔 받아서 좋았어",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014316.1.14",
          "form": "응응 다들 굿즈 잘뽑아낸다",
          "original_form": "응응 다들 굿즈 잘뽑아낸다",
          "speaker_id": "1",
          "time": "20210825 18:34"
        },
        {
          "id": "MDRW2100014316.1.15",
          "form": "코로나빨리끝나야하는데ㅜㅜ힝",
          "original_form": "코로나빨리끝나야하는데ㅜㅜ힝",
          "speaker_id": "1",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014316.1.16",
          "form": "그래 코로나가 끝나야 콘서트 가지",
          "original_form": "그래 코로나가 끝나야 콘서트 가지",
          "speaker_id": "2",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100014316.1.17",
          "form": "그니까 띄어앉기도 한계가있구",
          "original_form": "그니까 띄어앉기도 한계가있구",
          "speaker_id": "1",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014316.1.18",
          "form": "이석훈뮤지컬보러가고싶어",
          "original_form": "이석훈뮤지컬보러가고싶어",
          "speaker_id": "1",
