      "utterance": [
        {
          "id": "MDRW2100019546.1.1",
          "form": "영화보는거 좋아하세요?",
          "original_form": "영화보는거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019546.1.2",
          "form": "네 종종 몰아서봐요ㅋㅋㅋ",
          "original_form": "네 종종 몰아서봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019546.1.3",
          "form": "영화 어떤장르 좋아하세요?",
          "original_form": "영화 어떤장르 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019546.1.4",
          "form": "저는 미스터리 스릴러 sf 판타지 약간 이런쪽이요",
          "original_form": "저는 미스터리 스릴러 sf 판타지 약간 이런쪽이요",
          "speaker_id": "2",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019546.1.5",
          "form": "어떤 장르 주로 보시나요ㅎㅎ",
          "original_form": "어떤 장르 주로 보시나요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 20:51"
        },
        {
          "id": "MDRW2100019546.1.6",
          "form": "저도 판타지 좋아해요ㅋㅋㅋ 그리고 재난영화들..?",
          "original_form": "저도 판타지 좋아해요ㅋㅋㅋ 그리고 재난영화들..?",
          "speaker_id": "1",
          "time": "20210827 20:52"
        },
        {
          "id": "MDRW2100019546.1.7",
          "form": "오 저도요ㅋㅋ 판타지 어떤 영화 재밌게 보셨어요?",
          "original_form": "오 저도요ㅋㅋ 판타지 어떤 영화 재밌게 보셨어요?",
          "speaker_id": "2",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019546.1.8",
          "form": "역시 판타지 하면.. 시작은 해리포터...",
          "original_form": "역시 판타지 하면.. 시작은 해리포터...",
          "speaker_id": "1",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019546.1.9",
          "form": "저 나이에 안맞게 디즈니나 팀버튼 감독거도 좋아합니다..ㅋㅋㅋㅋㅋㅋㅎㅎ",
          "original_form": "저 나이에 안맞게 디즈니나 팀버튼 감독거도 좋아합니다..ㅋㅋㅋㅋㅋㅋㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019546.1.10",
          "form": "그쵸 해리포터를 빼놓을 수가 없죠",
          "original_form": "그쵸 해리포터를 빼놓을 수가 없죠",
          "speaker_id": "2",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100019546.1.11",
          "form": "저도 디즈니랑 팀버튼 영화 너무 좋아해요ㅋㅋ",
          "original_form": "저도 디즈니랑 팀버튼 영화 너무 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:55"
        },
        {
          "id": "MDRW2100019546.1.12",
          "form": "디즈니실사화 영화 너무 좋고 이번에 나온 루카랑 크루엘라도 봤네요..!",
          "original_form": "디즈니실사화 영화 너무 좋고 이번에 나온 루카랑 크루엘라도 봤네요..!",
          "speaker_id": "2",
          "time": "20210827 20:56"
        },
        {
          "id": "MDRW2100019546.1.13",
          "form": "앗 저 그거 못봤는데 기대중이에요",
          "original_form": "앗 저 그거 못봤는데 기대중이에요",
          "speaker_id": "1",
          "time": "20210827 20:56"
        },
        {
          "id": "MDRW2100019546.1.14",
          "form": "재미있었나요??",
          "original_form": "재미있었나요??",
          "speaker_id": "1",
          "time": "20210827 20:56"
        },
        {
          "id": "MDRW2100019546.1.15",
          "form": "네네 둘다 재밌게 봤어요ㅎㅎ 시간되시면 꼭 보셔요!",
          "original_form": "네네 둘다 재밌게 봤어요ㅎㅎ 시간되시면 꼭 보셔요!",
          "speaker_id": "2",
          "time": "20210827 20:58"
        },
        {
          "id": "MDRW2100019546.1.16",
          "form": "네 빨리 봐야겠어요ㅋㅋㅋ",
          "original_form": "네 빨리 봐야겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100019546.1.17",
          "form": "실내자전거 타면서 영화보는데 가벼운 영화가 집중하기 좋더라구요",
          "original_form": "실내자전거 타면서 영화보는데 가벼운 영화가 집중하기 좋더라구요",
          "speaker_id": "1",
          "time": "20210827 21:00"
        },
        {
          "id": "MDRW2100019546.1.18",
          "form": "운동할땐 가벼운 영화가 보기좋죠!",
          "original_form": "운동할땐 가벼운 영화가 보기좋죠!",
          "speaker_id": "2",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100019546.1.19",
          "form": "저는 최근에 넷플릭스에서 메이즈러너 내려간다해서 다시 한번 봤어요!",
          "original_form": "저는 최근에 넷플릭스에서 메이즈러너 내려간다해서 다시 한번 봤어요!",
          "speaker_id": "2",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100019546.1.20",
          "form": "혹시 보셨나요?",
          "original_form": "혹시 보셨나요?",
          "speaker_id": "2",
          "time": "20210827 21:02"
        },
        {
          "id": "MDRW2100019546.1.21",
          "form": "앗 당근이죠! 메이즈러너는 진짜 초반편들이 재밋었던거 같아요",
          "original_form": "앗 당근이죠! 메이즈러너는 진짜 초반편들이 재밋었던거 같아요",
          "speaker_id": "1",
          "time": "20210827 21:05"
        },
        {
          "id": "MDRW2100019546.1.22",
          "form": "민호 너무 멋있게 나와요",
          "original_form": "민호 너무 멋있게 나와요",
          "speaker_id": "1",
          "time": "20210827 21:05"
        },
        {
          "id": "MDRW2100019546.1.23",
          "form": "오 맞아요! 1편봤는데 넘 재밌더라구요 민호 그 발음이 너무 좋아여 .. 캐릭터도 멋있고 ㅠㅠ",
          "original_form": "오 맞아요! 1편봤는데 넘 재밌더라구요 민호 그 발음이 너무 좋아여 .. 캐릭터도 멋있고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100019546.1.24",
          "form": "맞아요ㅋㅋㅋㅋㅋㅋ감독이 어떻게 발굴했나 싶어요",
          "original_form": "맞아요ㅋㅋㅋㅋㅋㅋ감독이 어떻게 발굴했나 싶어요",
          "speaker_id": "1",
          "time": "20210827 21:08"
        }
      ]
