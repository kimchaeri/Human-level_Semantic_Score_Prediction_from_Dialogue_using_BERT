      "utterance": [
        {
          "id": "MDRW2100026436.1.1",
          "form": "건강식품 챙겨드시는거 있으세요?",
          "original_form": "건강식품 챙겨드시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210902 07:42"
        },
        {
          "id": "MDRW2100026436.1.2",
          "form": "네네 영양제 챙겨먹으려고 노력하고있어요.ㅋ",
          "original_form": "네네 영양제 챙겨먹으려고 노력하고있어요.ㅋ",
          "speaker_id": "1",
          "time": "20210902 08:06"
        },
        {
          "id": "MDRW2100026436.1.3",
          "form": "저도요 근데 매일 깜박해요ㅋㅋ 먹으면서 이게 효과가 있나 의문이들어서 그런것같아요",
          "original_form": "저도요 근데 매일 깜박해요ㅋㅋ 먹으면서 이게 효과가 있나 의문이들어서 그런것같아요",
          "speaker_id": "2",
          "time": "20210902 08:07"
        },
        {
          "id": "MDRW2100026436.1.4",
          "form": "네 맞아요. 저도ㅜ",
          "original_form": "네 맞아요. 저도ㅜ",
          "speaker_id": "1",
          "time": "20210902 08:53"
        },
        {
          "id": "MDRW2100026436.1.5",
          "form": "몸이 좀 안좋다싶을때 먹고ㅠ아니면 잊어먹고 그래요.ㅋㅋ",
          "original_form": "몸이 좀 안좋다싶을때 먹고ㅠ아니면 잊어먹고 그래요.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 08:53"
        },
        {
          "id": "MDRW2100026436.1.6",
          "form": "사람은 절실 절박하지않으면 뭐든나태해져요",
          "original_form": "사람은 절실 절박하지않으면 뭐든나태해져요",
          "speaker_id": "2",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100026436.1.7",
          "form": "오늘은 잊지말고 챙겨드세요",
          "original_form": "오늘은 잊지말고 챙겨드세요",
          "speaker_id": "2",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100026436.1.8",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100026436.1.9",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "1",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100026436.1.10",
          "form": "아뇨ㅋㅋ숨쉬기 걷기만요ㅠ",
          "original_form": "아뇨ㅋㅋ숨쉬기 걷기만요ㅠ",
          "speaker_id": "2",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100026436.1.11",
          "form": "하세요?",
          "original_form": "하세요?",
          "speaker_id": "2",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100026436.1.12",
          "form": "아 저는 만보걷기랑 줌바댄스요ㅋ",
          "original_form": "아 저는 만보걷기랑 줌바댄스요ㅋ",
          "speaker_id": "1",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100026436.1.13",
          "form": "매일은 못하구요ㅋ",
          "original_form": "매일은 못하구요ㅋ",
          "speaker_id": "1",
          "time": "20210902 09:44"
        },
        {
          "id": "MDRW2100026436.1.14",
          "form": "만보는 보통 몇 km나와여",
          "original_form": "만보는 보통 몇 km나와여",
          "speaker_id": "2",
          "time": "20210902 14:32"
        },
        {
          "id": "MDRW2100026436.1.15",
          "form": "아님 몇시간정도 걸어야할까요",
          "original_form": "아님 몇시간정도 걸어야할까요",
          "speaker_id": "2",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100026436.1.16",
          "form": "만보걷은지...까마득해요",
          "original_form": "만보걷은지...까마득해요",
          "speaker_id": "2",
          "time": "20210902 14:33"
        },
        {
          "id": "MDRW2100026436.1.17",
          "form": "흠 50분 정도 걸으면 만보찍히던데요??ㅋ",
          "original_form": "흠 50분 정도 걸으면 만보찍히던데요??ㅋ",
          "speaker_id": "1",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100026436.1.18",
          "form": "그정도로 걸으면 딱 좋더라구요 ㅋㅋ",
          "original_form": "그정도로 걸으면 딱 좋더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:35"
        },
        {
          "id": "MDRW2100026436.1.19",
          "form": "진짜여?ㅎㅎ이제날도풀리고 저녁운동좀해야겠어요",
          "original_form": "진짜여?ㅎㅎ이제날도풀리고 저녁운동좀해야겠어요",
          "speaker_id": "2",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100026436.1.20",
          "form": "네네 ㅋㅋ어제 애들본다고 정신없었네요ㅋㅋ저녁운동 좋죠ㅋㅋ",
          "original_form": "네네 ㅋㅋ어제 애들본다고 정신없었네요ㅋㅋ저녁운동 좋죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:25"
        },
        {
          "id": "MDRW2100026436.1.21",
          "form": "진짜 살안찌는 체질이긴한데 건강해지고싶어요ㅋㅋ",
          "original_form": "진짜 살안찌는 체질이긴한데 건강해지고싶어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 08:27"
        },
        {
          "id": "MDRW2100026436.1.22",
          "form": "저는 퇴근후 운동진짜해야하는데요ㅠ",
          "original_form": "저는 퇴근후 운동진짜해야하는데요ㅠ",
          "speaker_id": "2",
          "time": "20210903 08:27"
        }
      ]
