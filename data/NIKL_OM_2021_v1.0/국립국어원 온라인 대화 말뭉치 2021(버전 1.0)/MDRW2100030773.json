      "utterance": [
        {
          "id": "MDRW2100030773.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210904 12:24"
        },
        {
          "id": "MDRW2100030773.1.2",
          "form": "하이여 심심",
          "original_form": "하이여 심심",
          "speaker_id": "2",
          "time": "20210904 12:31"
        },
        {
          "id": "MDRW2100030773.1.3",
          "form": "넹넹ㅎㅎ밥드셨어요?",
          "original_form": "넹넹ㅎㅎ밥드셨어요?",
          "speaker_id": "1",
          "time": "20210904 12:36"
        },
        {
          "id": "MDRW2100030773.1.4",
          "form": "아침먹고 점심패스요",
          "original_form": "아침먹고 점심패스요",
          "speaker_id": "2",
          "time": "20210904 12:51"
        },
        {
          "id": "MDRW2100030773.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210904 12:51"
        },
        {
          "id": "MDRW2100030773.1.6",
          "form": "저는 아직..",
          "original_form": "저는 아직..",
          "speaker_id": "1",
          "time": "20210904 13:09"
        },
        {
          "id": "MDRW2100030773.1.7",
          "form": "배고플텐데",
          "original_form": "배고플텐데",
          "speaker_id": "2",
          "time": "20210904 13:12"
        },
        {
          "id": "MDRW2100030773.1.8",
          "form": "메뉴는 정했어요?",
          "original_form": "메뉴는 정했어요?",
          "speaker_id": "2",
          "time": "20210904 13:12"
        },
        {
          "id": "MDRW2100030773.1.9",
          "form": "ㅎㅎ칼국수가 당겨서 지인 꼬시고 있어요",
          "original_form": "ㅎㅎ칼국수가 당겨서 지인 꼬시고 있어요",
          "speaker_id": "1",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030773.1.10",
          "form": "어떤 메뉴 좋아하세요?",
          "original_form": "어떤 메뉴 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030773.1.11",
          "form": "전 아무래도 육식 고기파에요",
          "original_form": "전 아무래도 육식 고기파에요",
          "speaker_id": "2",
          "time": "20210904 13:13"
        },
        {
          "id": "MDRW2100030773.1.12",
          "form": "칼국수는 만두와함께..",
          "original_form": "칼국수는 만두와함께..",
          "speaker_id": "2",
          "time": "20210904 13:14"
        },
        {
          "id": "MDRW2100030773.1.13",
          "form": "ㅎㅎㅎ고기만두랑 먹음 좋죠",
          "original_form": "ㅎㅎㅎ고기만두랑 먹음 좋죠",
          "speaker_id": "1",
          "time": "20210904 13:15"
        },
        {
          "id": "MDRW2100030773.1.14",
          "form": "오 땡기네요 육수 진하게해서..",
          "original_form": "오 땡기네요 육수 진하게해서..",
          "speaker_id": "2",
          "time": "20210904 13:16"
        },
        {
          "id": "MDRW2100030773.1.15",
          "form": "식사를 늦게하시네요",
          "original_form": "식사를 늦게하시네요",
          "speaker_id": "2",
          "time": "20210904 13:16"
        },
        {
          "id": "MDRW2100030773.1.16",
          "form": "사실 아직 퇴근못했어요ㅠ",
          "original_form": "사실 아직 퇴근못했어요ㅠ",
          "speaker_id": "1",
          "time": "20210904 13:16"
        },
        {
          "id": "MDRW2100030773.1.17",
          "form": "아휴ㅎㅎ",
          "original_form": "아휴ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 13:16"
        },
        {
          "id": "MDRW2100030773.1.18",
          "form": "앗 토욜인데 근무하시는구나",
          "original_form": "앗 토욜인데 근무하시는구나",
          "speaker_id": "2",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100030773.1.19",
          "form": "근데 점심을 안먹어요?!!",
          "original_form": "근데 점심을 안먹어요?!!",
          "speaker_id": "2",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100030773.1.20",
          "form": "ㅎㅎㅎ이제가려고요!",
          "original_form": "ㅎㅎㅎ이제가려고요!",
          "speaker_id": "1",
          "time": "20210904 13:30"
        }
      ]
