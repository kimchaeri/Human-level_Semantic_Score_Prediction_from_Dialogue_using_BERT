      "utterance": [
        {
          "id": "MDRW2100022074.1.1",
          "form": "안녕하세영 저녁 드셨나영",
          "original_form": "안녕하세영 저녁 드셨나영",
          "speaker_id": "1",
          "time": "20210829 02:08"
        },
        {
          "id": "MDRW2100022074.1.2",
          "form": "네..!!엄마표 된장찌개 먹었어요 ㅎㅎ\r\nname1은요?",
          "original_form": "네..!!엄마표 된장찌개 먹었어요 ㅎㅎ\r\n&name1&은요?",
          "speaker_id": "2",
          "time": "20210829 02:08"
        },
        {
          "id": "MDRW2100022074.1.3",
          "form": "우왕",
          "original_form": "우왕",
          "speaker_id": "1",
          "time": "20210829 02:09"
        },
        {
          "id": "MDRW2100022074.1.4",
          "form": "저는 스팸볶음밥 해먹었어오..!",
          "original_form": "저는 스팸볶음밥 해먹었어오..!",
          "speaker_id": "1",
          "time": "20210829 02:09"
        },
        {
          "id": "MDRW2100022074.1.5",
          "form": "오..직접 요리하신거세여?",
          "original_form": "오..직접 요리하신거세여?",
          "speaker_id": "2",
          "time": "20210829 02:09"
        },
        {
          "id": "MDRW2100022074.1.6",
          "form": "진짜 맛잇는데 ㅎㅎ",
          "original_form": "진짜 맛잇는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 02:09"
        },
        {
          "id": "MDRW2100022074.1.7",
          "form": "네 그냥 스팸만 볶구 밥 넣구 볶았어여 ㅎㅅㅎ",
          "original_form": "네 그냥 스팸만 볶구 밥 넣구 볶았어여 ㅎㅅㅎ",
          "speaker_id": "1",
          "time": "20210829 02:10"
        },
        {
          "id": "MDRW2100022074.1.8",
          "form": "스팸볶음밥 해드셔보셨나여?",
          "original_form": "스팸볶음밥 해드셔보셨나여?",
          "speaker_id": "1",
          "time": "20210829 02:10"
        },
        {
          "id": "MDRW2100022074.1.9",
          "form": "네 저는 양파랑 청양고.추 도 잘게썰어봣어요",
          "original_form": "네 저는 양파랑 청양고.추 도 잘게썰어봣어요",
          "speaker_id": "2",
          "time": "20210829 02:11"
        },
        {
          "id": "MDRW2100022074.1.10",
          "form": "오 청양 고.추 맛있겠네요",
          "original_form": "오 청양 고.추 맛있겠네요",
          "speaker_id": "1",
          "time": "20210829 02:12"
        },
        {
          "id": "MDRW2100022074.1.11",
          "form": "ㅋㅋㅋㅋ 생각만해도 너무 맛잇겟네요",
          "original_form": "ㅋㅋㅋㅋ 생각만해도 너무 맛잇겟네요",
          "speaker_id": "2",
          "time": "20210829 02:12"
        },
        {
          "id": "MDRW2100022074.1.12",
          "form": "맵싹하니..!",
          "original_form": "맵싹하니..!",
          "speaker_id": "1",
          "time": "20210829 02:12"
        },
        {
          "id": "MDRW2100022074.1.13",
          "form": "매콤하고 맛잇어요 ㅋㅋㅋ \r\n아 부대찌개 먹고싶어요",
          "original_form": "매콤하고 맛잇어요 ㅋㅋㅋ \r\n아 부대찌개 먹고싶어요",
          "speaker_id": "2",
          "time": "20210829 02:12"
        },
        {
          "id": "MDRW2100022074.1.14",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:12"
        },
        {
          "id": "MDRW2100022074.1.15",
          "form": "부대찌개 맛있죠ㅠㅠㅠㅠㅠ",
          "original_form": "부대찌개 맛있죠ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 02:13"
        },
        {
          "id": "MDRW2100022074.1.16",
          "form": "전 송탄부대찌개 제일 좋아해요",
          "original_form": "전 송탄부대찌개 제일 좋아해요",
          "speaker_id": "1",
          "time": "20210829 02:13"
        },
        {
          "id": "MDRW2100022074.1.17",
          "form": "오.. 땡스부대찌개는 드셔보셧어요?",
          "original_form": "오.. 땡스부대찌개는 드셔보셧어요?",
