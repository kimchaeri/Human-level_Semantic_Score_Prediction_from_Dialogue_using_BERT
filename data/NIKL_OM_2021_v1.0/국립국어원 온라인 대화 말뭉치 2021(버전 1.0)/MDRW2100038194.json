      "utterance": [
        {
          "id": "MDRW2100038194.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100038194.1.2",
          "form": "안녕하세요!!",
          "original_form": "안녕하세요!!",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100038194.1.3",
          "form": "요즘에 사신 물건 있으세요??",
          "original_form": "요즘에 사신 물건 있으세요??",
          "speaker_id": "2",
          "time": "20210902 00:28"
        },
        {
          "id": "MDRW2100038194.1.4",
          "form": "좀전에 씨제이에서 냉동식품 샀어요 ㅎ",
          "original_form": "좀전에 씨제이에서 냉동식품 샀어요 ㅎ",
          "speaker_id": "1",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100038194.1.5",
          "form": "45퍼 쿠폰받아서요",
          "original_form": "45퍼 쿠폰받아서요",
          "speaker_id": "1",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100038194.1.6",
          "form": "거짓말아니라 저는 마지막에 산게 청정원 냉동식품이었어요ㅋㅋㅋ",
          "original_form": "거짓말아니라 저는 마지막에 산게 청정원 냉동식품이었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100038194.1.7",
          "form": "맛있져...",
          "original_form": "맛있져...",
          "speaker_id": "2",
          "time": "20210902 00:30"
        },
        {
          "id": "MDRW2100038194.1.8",
          "form": "ㅎㅎ글쵸",
          "original_form": "ㅎㅎ글쵸",
          "speaker_id": "1",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100038194.1.9",
          "form": "씨제이 냉동식품중에 추천 상품 있으신가요ㅋㅋㅋ",
          "original_form": "씨제이 냉동식품중에 추천 상품 있으신가요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100038194.1.10",
          "form": "거긴 할인있어요?",
          "original_form": "거긴 할인있어요?",
          "speaker_id": "1",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100038194.1.11",
          "form": "오십퍼센트 할인 쿠폰 받아놓은게 있었거든요",
          "original_form": "오십퍼센트 할인 쿠폰 받아놓은게 있었거든요",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100038194.1.12",
          "form": "거기도 가끔 행사해요",
          "original_form": "거기도 가끔 행사해요",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100038194.1.13",
          "form": "아 그런쿠폰은 어디서 받아요?",
          "original_form": "아 그런쿠폰은 어디서 받아요?",
          "speaker_id": "1",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100038194.1.14",
          "form": "이벤트 창으로 가면 있더라고요",
          "original_form": "이벤트 창으로 가면 있더라고요",
          "speaker_id": "2",
          "time": "20210902 00:31"
        },
        {
          "id": "MDRW2100038194.1.15",
          "form": "정원이샵?인가요?",
          "original_form": "정원이샵?인가요?",
          "speaker_id": "1",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100038194.1.16",
          "form": "씨제이는 비비고라인이라 ㅎㅎ",
          "original_form": "씨제이는 비비고라인이라 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100038194.1.17",
          "form": "네 신규회원 쿠폰으로 받아야됐어요",
          "original_form": "네 신규회원 쿠폰으로 받아야됐어요",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100038194.1.18",
          "form": "전 김샀어요 ㅋㅋ",
          "original_form": "전 김샀어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100038194.1.19",
          "form": "조금만담아도 금액이 훌쩍 ㅎㅎ",
          "original_form": "조금만담아도 금액이 훌쩍 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:32"
        },
        {
          "id": "MDRW2100038194.1.20",
          "form": "저는 꼬마돈까스 치킨 ㅎㅎ",
          "original_form": "저는 꼬마돈까스 치킨 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100038194.1.21",
          "form": "아 씨제이 꼬마돈까스 맛있더라고요 ㅋㅋ 간만에 먹었더니 그랬나",
          "original_form": "아 씨제이 꼬마돈까스 맛있더라고요 ㅋㅋ 간만에 먹었더니 그랬나",
          "speaker_id": "2",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100038194.1.22",
          "form": "맞아요 조그마한게 맛나요",
          "original_form": "맞아요 조그마한게 맛나요",
          "speaker_id": "1",
          "time": "20210902 00:33"
        },
        {
          "id": "MDRW2100038194.1.23",
          "form": "근데 새우까스는 별로였어요 오뎅이 나아여 ㅋㅋ",
          "original_form": "근데 새우까스는 별로였어요 오뎅이 나아여 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100038194.1.24",
          "form": "조리도 쉽고",
          "original_form": "조리도 쉽고",
          "speaker_id": "1",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100038194.1.25",
          "form": "치킨도 맛있겠어요",
          "original_form": "치킨도 맛있겠어요",
          "speaker_id": "2",
          "time": "20210902 00:34"
        },
        {
          "id": "MDRW2100038194.1.26",
          "form": "치킨도 두종류샀어요",
          "original_form": "치킨도 두종류샀어요",
          "speaker_id": "1",
          "time": "20210902 00:34"
        }
      ]
    }
