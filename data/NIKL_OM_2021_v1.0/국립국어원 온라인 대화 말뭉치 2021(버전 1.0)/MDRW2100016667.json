        },
        {
          "id": "MDRW2100016667.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100016667.1.3",
          "form": "카페가면 머시키세요?",
          "original_form": "카페가면 머시키세요?",
          "speaker_id": "2",
          "time": "20210826 15:09"
        },
        {
          "id": "MDRW2100016667.1.4",
          "form": "자몽 음료요",
          "original_form": "자몽 음료요",
          "speaker_id": "1",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100016667.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100016667.1.6",
          "form": "전 아아 아니면 그가게의 시그니쳐 음료를 시켜봐요",
          "original_form": "전 아아 아니면 그가게의 시그니쳐 음료를 시켜봐요",
          "speaker_id": "2",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016667.1.7",
          "form": "비오는날은 무조건 시럽안넣은 라떼에 시나몬",
          "original_form": "비오는날은 무조건 시럽안넣은 라떼에 시나몬",
          "speaker_id": "2",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016667.1.8",
          "form": "커피 마니아시군요",
          "original_form": "커피 마니아시군요",
          "speaker_id": "1",
          "time": "20210826 15:45"
        },
        {
          "id": "MDRW2100016667.1.9",
          "form": "마니아까지는 아닌지만 즐겨 마셔요",
          "original_form": "마니아까지는 아닌지만 즐겨 마셔요",
          "speaker_id": "2",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016667.1.10",
          "form": "어느 브랜드 좋아하세요?",
          "original_form": "어느 브랜드 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016667.1.11",
          "form": "저는 커피 못마셔요",
          "original_form": "저는 커피 못마셔요",
          "speaker_id": "1",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016667.1.12",
          "form": "아~ 그렇구나 은근 많으시더라구요",
          "original_form": "아~ 그렇구나 은근 많으시더라구요",
          "speaker_id": "2",
          "time": "20210826 16:12"
        },
        {
          "id": "MDRW2100016667.1.13",
          "form": "카페인 때문에 심장이 벌렁벌렁거려요ㅋㅋ",
          "original_form": "카페인 때문에 심장이 벌렁벌렁거려요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100016667.1.14",
          "form": "저두 스벅 그린티계열은 그렇더라구요",
          "original_form": "저두 스벅 그린티계열은 그렇더라구요",
          "speaker_id": "2",
          "time": "20210826 18:05"
        },
        {
          "id": "MDRW2100016667.1.15",
          "form": "맛집 좋아하세요?",
          "original_form": "맛집 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 18:32"
        },
        {
          "id": "MDRW2100016667.1.16",
          "form": "네 이왕이면 맛집",
          "original_form": "네 이왕이면 맛집",
