      "utterance": [
        {
          "id": "MDRW2100019348.1.1",
          "form": "김치볶음밥 생각중",
          "original_form": "김치볶음밥 생각중",
          "speaker_id": "2",
          "time": "20210827 18:00"
        },
        {
          "id": "MDRW2100019348.1.2",
          "form": "오늘 불금인데",
          "original_form": "오늘 불금인데",
          "speaker_id": "2",
          "time": "20210827 18:00"
        },
        {
          "id": "MDRW2100019348.1.3",
          "form": "넌 뭐먹을거야",
          "original_form": "넌 뭐먹을거야",
          "speaker_id": "2",
          "time": "20210827 18:00"
        },
        {
          "id": "MDRW2100019348.1.4",
          "form": "오 난 부채살",
          "original_form": "오 난 부채살",
          "speaker_id": "1",
          "time": "20210827 18:02"
        },
        {
          "id": "MDRW2100019348.1.5",
          "form": "구워먹으려고",
          "original_form": "구워먹으려고",
          "speaker_id": "1",
          "time": "20210827 18:02"
        },
        {
          "id": "MDRW2100019348.1.6",
          "form": "몸보신을하지ㅋㅋ",
          "original_form": "몸보신을하지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:02"
        },
        {
          "id": "MDRW2100019348.1.7",
          "form": "헐 마쉿겠다",
          "original_form": "헐 마쉿겠다",
          "speaker_id": "2",
          "time": "20210827 18:05"
        },
        {
          "id": "MDRW2100019348.1.8",
          "form": "몸보신 어제 미리 치킨 먹음 ",
          "original_form": "몸보신 어제 미리 치킨 먹음 ",
          "speaker_id": "2",
          "time": "20210827 18:06"
        },
        {
          "id": "MDRW2100019348.1.9",
          "form": "헐 뭐먹었어",
          "original_form": "헐 뭐먹었어",
          "speaker_id": "1",
          "time": "20210827 18:09"
        },
        {
          "id": "MDRW2100019348.1.10",
          "form": "가마치 통닭 ㅋㅋㅋㅋ",
          "original_form": "가마치 통닭 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100019348.1.11",
          "form": "크 잘했네 존맛이지",
          "original_form": "크 잘했네 존맛이지",
          "speaker_id": "1",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100019348.1.12",
          "form": "김볶은 직접해먹게?",
          "original_form": "김볶은 직접해먹게?",
          "speaker_id": "1",
          "time": "20210827 18:12"
        },
        {
          "id": "MDRW2100019348.1.13",
          "form": "엄마가 어제",
          "original_form": "엄마가 어제",
          "speaker_id": "2",
          "time": "20210827 18:16"
        },
        {
          "id": "MDRW2100019348.1.14",
          "form": "왕창 볶아놓으셔서",
          "original_form": "왕창 볶아놓으셔서",
          "speaker_id": "2",
          "time": "20210827 18:16"
        },
        {
          "id": "MDRW2100019348.1.15",
          "form": "밥만 넣고 볶음 돼 ㅋㅋㅋ",
          "original_form": "밥만 넣고 볶음 돼 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:16"
        },
        {
          "id": "MDRW2100019348.1.16",
          "form": "선견지명ㅋㅋㅋ",
          "original_form": "선견지명ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:18"
        },
        {
          "id": "MDRW2100019348.1.17",
          "form": "부재료는돼지?참치?",
          "original_form": "부재료는돼지?참치?",
          "speaker_id": "1",
          "time": "20210827 18:18"
        },
        {
          "id": "MDRW2100019348.1.18",
          "form": "울집은 늘 참치!",
          "original_form": "울집은 늘 참치!",
          "speaker_id": "2",
          "time": "20210827 18:18"
        },
        {
          "id": "MDRW2100019348.1.19",
          "form": "아맛있겠다ㅜㅜ",
          "original_form": "아맛있겠다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 18:19"
        },
        {
          "id": "MDRW2100019348.1.20",
          "form": "오랜만에 참치넣은거 먹고싶네",
          "original_form": "오랜만에 참치넣은거 먹고싶네",
          "speaker_id": "1",
          "time": "20210827 18:19"
        },
        {
          "id": "MDRW2100019348.1.21",
          "form": "혼자먹니",
          "original_form": "혼자먹니",
          "speaker_id": "1",
          "time": "20210827 18:19"
        },
        {
          "id": "MDRW2100019348.1.22",
          "form": "부모님은 별로 안 좋아하셔서 안드실걸?",
          "original_form": "부모님은 별로 안 좋아하셔서 안드실걸?",
          "speaker_id": "2",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019348.1.23",
          "form": "밥 같이 먹을거 같긴 한데",
          "original_form": "밥 같이 먹을거 같긴 한데",
          "speaker_id": "2",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019348.1.24",
          "form": "ㅋㅋㅋㅋ어머니는 해놓고왜안드셬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ어머니는 해놓고왜안드셬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:32"
        },
        {
          "id": "MDRW2100019348.1.25",
          "form": "어제 내 저녁으로 먹으라고 해놓으셨는데",
          "original_form": "어제 내 저녁으로 먹으라고 해놓으셨는데",
          "speaker_id": "2",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100019348.1.26",
          "form": "내가 퇴근길에 치킨 사가버림 ㅋㅋㅋ",
          "original_form": "내가 퇴근길에 치킨 사가버림 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100019348.1.27",
          "form": "그래서 같이 치킨 먹었쥐",
          "original_form": "그래서 같이 치킨 먹었쥐",
          "speaker_id": "2",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100019348.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋ잘해따",
          "original_form": "ㅋㅋㅋㅋㅋㅋ잘해따",
          "speaker_id": "1",
          "time": "20210827 18:34"
        }
      ]
    }
  ]
