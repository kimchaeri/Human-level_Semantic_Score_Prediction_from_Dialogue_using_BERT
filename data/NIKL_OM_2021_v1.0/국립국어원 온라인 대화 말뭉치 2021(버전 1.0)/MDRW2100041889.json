      "utterance": [
        {
          "id": "MDRW2100041889.1.1",
          "form": "과자 드시나요?ㅋㅋㅋ",
          "original_form": "과자 드시나요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:11"
        },
        {
          "id": "MDRW2100041889.1.2",
          "form": "과자의 세계는 넓고넓습니다 ㅋㅋ",
          "original_form": "과자의 세계는 넓고넓습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:12"
        },
        {
          "id": "MDRW2100041889.1.3",
          "form": "원래 관심없었어요 ㅋㅋ 과자 탄산음료 ㅋㅋ",
          "original_form": "원래 관심없었어요 ㅋㅋ 과자 탄산음료 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:12"
        },
        {
          "id": "MDRW2100041889.1.4",
          "form": "근데 지금은 없어서 못먹..ㅋㅋ",
          "original_form": "근데 지금은 없어서 못먹..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:12"
        },
        {
          "id": "MDRW2100041889.1.5",
          "form": "역시 사람 입맛이 변하네옄ㅋㅋㅋㅋㅋ",
          "original_form": "역시 사람 입맛이 변하네옄ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:12"
        },
        {
          "id": "MDRW2100041889.1.6",
          "form": "저 요즘에 다시 뽀또 먹는데 너무 맛있더라고요",
          "original_form": "저 요즘에 다시 뽀또 먹는데 너무 맛있더라고요",
          "speaker_id": "1",
          "time": "20210906 17:13"
        },
        {
          "id": "MDRW2100041889.1.7",
          "form": "네 ㅋㅋ맞아요 ㅋㅋ 안먹던거 다먹어지고 신기해요ㅋㅋ",
          "original_form": "네 ㅋㅋ맞아요 ㅋㅋ 안먹던거 다먹어지고 신기해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:13"
        },
        {
          "id": "MDRW2100041889.1.8",
          "form": "앗 뽀또!ㅎㅎㅎㅎㅎ",
          "original_form": "앗 뽀또!ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 17:13"
        },
        {
          "id": "MDRW2100041889.1.9",
          "form": "새로나온 과자들도 많더라구요",
          "original_form": "새로나온 과자들도 많더라구요",
          "speaker_id": "2",
          "time": "20210906 17:13"
        },
        {
          "id": "MDRW2100041889.1.10",
          "form": "도전의식ㅋㅋㅋ",
          "original_form": "도전의식ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:13"
        },
        {
          "id": "MDRW2100041889.1.11",
          "form": "초등학생때 자주 먹던 과자 다시 먹어욬ㅋㅋㅋ",
          "original_form": "초등학생때 자주 먹던 과자 다시 먹어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:13"
        },
        {
          "id": "MDRW2100041889.1.12",
          "form": "새로나온 과자 뭐 드셔보셨어요??",
          "original_form": "새로나온 과자 뭐 드셔보셨어요??",
          "speaker_id": "1",
          "time": "20210906 17:14"
        },
        {
          "id": "MDRW2100041889.1.13",
          "form": "초코렐라만 먹어봤는데",
          "original_form": "초코렐라만 먹어봤는데",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100041889.1.14",
          "form": "넘맛나더라구왜",
          "original_form": "넘맛나더라구왜",
          "speaker_id": "2",
          "time": "20210906 19:07"
        },
        {
          "id": "MDRW2100041889.1.15",
          "form": "빼빼로 꼬깔콘맛/팔도비빔칩/허니버터팝콘\r\n 먹어보고 싶네요 ㅋ",
          "original_form": "빼빼로 꼬깔콘맛/팔도비빔칩/허니버터팝콘\r\n 먹어보고 싶네요 ㅋ",
          "speaker_id": "2",
          "time": "20210906 19:09"
        },
        {
          "id": "MDRW2100041889.1.16",
          "form": "와 꼬깔콘맛 빼고 다 처음 들어봐요",
          "original_form": "와 꼬깔콘맛 빼고 다 처음 들어봐요",
          "speaker_id": "1",
          "time": "20210906 19:54"
        },
        {
          "id": "MDRW2100041889.1.17",
          "form": "오타는 저도 잘 나서 알아요알아요ㅋㅋㅋ",
          "original_form": "오타는 저도 잘 나서 알아요알아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:54"
        },
        {
          "id": "MDRW2100041889.1.18",
          "form": "팔도비빔칩 궁금은 한데 상상은 가는 맛이에욬ㅋㅋㅋ",
          "original_form": "팔도비빔칩 궁금은 한데 상상은 가는 맛이에욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:55"
        },
        {
          "id": "MDRW2100041889.1.19",
          "form": "그쵸ㅋㅋㅋ 그래도 먹어보고파요ㅠㅠ",
          "original_form": "그쵸ㅋㅋㅋ 그래도 먹어보고파요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100041889.1.20",
          "form": "방금  name1님하고 대화하다가 급 궁금해져서",
          "original_form": "방금  &name1&님하고 대화하다가 급 궁금해져서",
          "speaker_id": "2",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100041889.1.21",
          "form": "신상과자 검색했더니",
          "original_form": "신상과자 검색했더니",
          "speaker_id": "2",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100041889.1.22",
          "form": "대박이네요?ㅋㅋㅋ",
          "original_form": "대박이네요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:57"
        },
        {
          "id": "MDRW2100041889.1.23",
          "form": "전부 처음보는..ㅎㅎ;",
          "original_form": "전부 처음보는..ㅎㅎ;",
          "speaker_id": "2",
          "time": "20210906 19:58"
        },
        {
          "id": "MDRW2100041889.1.24",
          "form": "저도 한번 검색 해봐야겠어욬ㅋㅋㅋ",
          "original_form": "저도 한번 검색 해봐야겠어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 20:57"
        },
        {
          "id": "MDRW2100041889.1.25",
          "form": "보니까 먹고싶어요",
          "original_form": "보니까 먹고싶어요",
          "speaker_id": "2",
          "time": "20210906 21:07"
        },
        {
          "id": "MDRW2100041889.1.26",
          "form": "ㅋㅋㅋ 저도요 저 몽쉘 솔티카라멜 먹어보려고 했는데",
          "original_form": "ㅋㅋㅋ 저도요 저 몽쉘 솔티카라멜 먹어보려고 했는데",
          "speaker_id": "1",
          "time": "20210906 21:17"
        },
        {
          "id": "MDRW2100041889.1.27",
          "form": "막상 사려니까 안팔더라고욬ㅋㅋ",
          "original_form": "막상 사려니까 안팔더라고욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 21:18"
        },
        {
          "id": "MDRW2100041889.1.28",
          "form": "오 솔티피넛인가요?",
          "original_form": "오 솔티피넛인가요?",
          "speaker_id": "2",
          "time": "20210906 22:23"
