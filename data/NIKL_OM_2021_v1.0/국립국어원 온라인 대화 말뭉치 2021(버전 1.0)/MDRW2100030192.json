      "utterance": [
        {
          "id": "MDRW2100030192.1.1",
          "form": "오늘 동아리 갓다왔어?",
          "original_form": "오늘 동아리 갓다왔어?",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030192.1.2",
          "form": "비대면 동아리 활동 말야",
          "original_form": "비대면 동아리 활동 말야",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030192.1.3",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030192.1.4",
          "form": "비대면 동아리 하구왓어?",
          "original_form": "비대면 동아리 하구왓어?",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030192.1.5",
          "form": "오늘은 안했어",
          "original_form": "오늘은 안했어",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030192.1.6",
          "form": "그렇구나...",
          "original_form": "그렇구나...",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030192.1.7",
          "form": "비대면 동아리는 어떤식으루 하는뎅?",
          "original_form": "비대면 동아리는 어떤식으루 하는뎅?",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030192.1.8",
          "form": "zoom 있자나",
          "original_form": "zoom 있자나",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030192.1.9",
          "form": "그걸로 카메라 키고 선생님이 진행한대",
          "original_form": "그걸로 카메라 키고 선생님이 진행한대",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030192.1.10",
          "form": "사실 나두 해본적은없어",
          "original_form": "사실 나두 해본적은없어",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030192.1.11",
          "form": "줌 알쥐 나도 싸강듣는 대학생이니깐",
          "original_form": "줌 알쥐 나도 싸강듣는 대학생이니깐",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030192.1.12",
          "form": "들은거야",
          "original_form": "들은거야",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030192.1.13",
          "form": "카메라를 킨다니..",
          "original_form": "카메라를 킨다니..",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030192.1.14",
          "form": "좋겠다",
          "original_form": "좋겠다",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030192.1.15",
          "form": "부담스럽겟다",
          "original_form": "부담스럽겟다",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030192.1.16",
          "form": "그러게 ㅋㅋ",
          "original_form": "그러게 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030192.1.17",
          "form": "진짜 스트레스받겠다",
          "original_form": "진짜 스트레스받겠다",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030192.1.18",
          "form": "그라도 학교 직접가는거보단 나은거같아",
          "original_form": "그라도 학교 직접가는거보단 나은거같아",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030192.1.19",
          "form": "맞아 ㅎㅎ",
          "original_form": "맞아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030192.1.20",
          "form": "귀찮구 등교길이 귀찮잖아 ㅎㅎ",
          "original_form": "귀찮구 등교길이 귀찮잖아 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030192.1.21",
          "form": "학교가는길이 정말 귀찮지..",
          "original_form": "학교가는길이 정말 귀찮지..",
          "speaker_id": "2",
          "time": "20210903 22:53"
        },
        {
          "id": "MDRW2100030192.1.22",
          "form": "인정ㅋㅋ",
          "original_form": "인정ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 22:54"
        },
        {
          "id": "MDRW2100030192.1.23",
          "form": "다음주부터 고등학생은 전면등교라고 하더라구",
          "original_form": "다음주부터 고등학생은 전면등교라고 하더라구",
          "speaker_id": "1",
          "time": "20210903 23:07"
        }
      ]
