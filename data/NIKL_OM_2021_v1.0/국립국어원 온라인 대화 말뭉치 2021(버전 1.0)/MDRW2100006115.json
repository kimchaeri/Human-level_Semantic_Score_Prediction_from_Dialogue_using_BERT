      "utterance": [
        {
          "id": "MDRW2100006115.1.1",
          "form": "독서 좋아하시나요?",
          "original_form": "독서 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100006115.1.2",
          "form": "독서 당연히 좋아합니다.",
          "original_form": "독서 당연히 좋아합니다.",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100006115.1.3",
          "form": "웹소설부터 고전 소설까지 좋아해요~",
          "original_form": "웹소설부터 고전 소설까지 좋아해요~",
          "speaker_id": "2",
          "time": "20210907 20:34"
        },
        {
          "id": "MDRW2100006115.1.4",
          "form": "와 고전소설이라면",
          "original_form": "와 고전소설이라면",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100006115.1.5",
          "form": "오헨리같은거요?",
          "original_form": "오헨리같은거요?",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100006115.1.6",
          "form": "저 조지오웰 소설 좋아해요",
          "original_form": "저 조지오웰 소설 좋아해요",
          "speaker_id": "2",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100006115.1.7",
          "form": "1984도 좋고 동물농장도 좋아해요",
          "original_form": "1984도 좋고 동물농장도 좋아해요",
          "speaker_id": "2",
          "time": "20210907 20:38"
        },
        {
          "id": "MDRW2100006115.1.8",
          "form": "아 저도 1984 감명깊게봤어요",
          "original_form": "아 저도 1984 감명깊게봤어요",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100006115.1.9",
          "form": "동물농장은 읽었는데 기억이안나네요",
          "original_form": "동물농장은 읽었는데 기억이안나네요",
          "speaker_id": "1",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100006115.1.10",
          "form": "동물농장은 그 무슨 악역 돼지가 다른 동물들 사이에서 왕이 될려고 정치를 하는 이야기였어요~!",
          "original_form": "동물농장은 그 무슨 악역 돼지가 다른 동물들 사이에서 왕이 될려고 정치를 하는 이야기였어요~!",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100006115.1.11",
          "form": "1984에서 마지막에 정신적으로도 세뇌되잖아요",
          "original_form": "1984에서 마지막에 정신적으로도 세뇌되잖아요",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100006115.1.12",
          "form": "그게 충격이더라구요",
          "original_form": "그게 충격이더라구요",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100006115.1.13",
          "form": "아 맞다",
          "original_form": "아 맞다",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100006115.1.14",
          "form": "그래서 반란일으키고 그런거죠",
          "original_form": "그래서 반란일으키고 그런거죠",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100006115.1.15",
          "form": "어디든 독재자는 존재하려고 드릉드릉하군요",
          "original_form": "어디든 독재자는 존재하려고 드릉드릉하군요",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100006115.1.16",
          "form": "제재하는쪽이없으면",
          "original_form": "제재하는쪽이없으면",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100006115.1.17",
          "form": "네네 결국 우리안의 동물들은 인간에게 먹히는 입장인데도 그렇게 서로 싸우는 장면을 보면 참담하더라고요",
          "original_form": "네네 결국 우리안의 동물들은 인간에게 먹히는 입장인데도 그렇게 서로 싸우는 장면을 보면 참담하더라고요",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100006115.1.18",
          "form": "맞아요 1984 엔딩도 충격적이었죠... 결국 독재자를 위한 세뇌가 그정도까지 통한다는 결말이 뇌리에 아직도 남아요",
          "original_form": "맞아요 1984 엔딩도 충격적이었죠... 결국 독재자를 위한 세뇌가 그정도까지 통한다는 결말이 뇌리에 아직도 남아요",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100006115.1.19",
          "form": "조지오웰 소설 저도 특이해서 좋아하는데",
          "original_form": "조지오웰 소설 저도 특이해서 좋아하는데",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006115.1.20",
          "form": "또 아시는작품있나요?",
          "original_form": "또 아시는작품있나요?",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100006115.1.21",
          "form": "정유정 작가님의 28도 정말 즐겁게 봤었구",
          "original_form": "정유정 작가님의 28도 정말 즐겁게 봤었구",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100006115.1.22",
          "form": "한강 작가님의 소년의 온다도 울면서 봤던거 같네요",
          "original_form": "한강 작가님의 소년의 온다도 울면서 봤던거 같네요",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100006115.1.23",
          "form": "아 저는 그 상받았던 초기작만 읽었는데",
          "original_form": "아 저는 그 상받았던 초기작만 읽었는데",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100006115.1.24",
          "form": "그분 작품도 굉장히 독특했던 기억이나요",
          "original_form": "그분 작품도 굉장히 독특했던 기억이나요",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100006115.1.25",
          "form": "정유정은 7년의 밤인가 그거 읽었구요",
          "original_form": "정유정은 7년의 밤인가 그거 읽었구요",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100006115.1.26",
          "form": "7년의 밤이요?? 헉 그거 영화화된 거 맞죠",
          "original_form": "7년의 밤이요?? 헉 그거 영화화된 거 맞죠",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006115.1.27",
          "form": "네 영화는 못봤는데",
          "original_form": "네 영화는 못봤는데",
          "speaker_id": "1",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006115.1.28",
          "form": "재미있게 읽었던기억이나네요",
          "original_form": "재미있게 읽었던기억이나네요",
          "speaker_id": "1",
          "time": "20210907 20:52"
        }
      ]
    }
