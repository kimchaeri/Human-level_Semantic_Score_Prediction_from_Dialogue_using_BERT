        {
          "id": "MDRW2100039050.1.1",
          "form": "엉? 요즘 안한다",
          "original_form": "엉? 요즘 안한다",
          "speaker_id": "2",
          "time": "20210903 07:23"
        },
        {
          "id": "MDRW2100039050.1.2",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210903 07:23"
        },
        {
          "id": "MDRW2100039050.1.3",
          "form": "뭐  사람들 만날 일 이 있어야 말이지",
          "original_form": "뭐  사람들 만날 일 이 있어야 말이지",
          "speaker_id": "2",
          "time": "20210903 07:23"
        },
        {
          "id": "MDRW2100039050.1.4",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210903 07:23"
        },
        {
          "id": "MDRW2100039050.1.5",
          "form": "그래서 머리가 하얘",
          "original_form": "그래서 머리가 하얘",
          "speaker_id": "2",
          "time": "20210903 07:23"
        },
        {
          "id": "MDRW2100039050.1.6",
          "form": "저런",
          "original_form": "저런",
          "speaker_id": "1",
          "time": "20210903 07:23"
        },
        {
          "id": "MDRW2100039050.1.7",
          "form": "너는 커트안할거니?",
          "original_form": "너는 커트안할거니?",
          "speaker_id": "2",
          "time": "20210903 07:23"
        },
        {
          "id": "MDRW2100039050.1.8",
          "form": "일단 길으려구요",
          "original_form": "일단 길으려구요",
          "speaker_id": "1",
          "time": "20210903 07:24"
        },
        {
          "id": "MDRW2100039050.1.9",
          "form": "머리 너무 길면 귀찮지 안아?",
          "original_form": "머리 너무 길면 귀찮지 안아?",
          "speaker_id": "2",
          "time": "20210903 07:24"
        },
        {
          "id": "MDRW2100039050.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210903 07:24"
        },
        {
          "id": "MDRW2100039050.1.11",
          "form": "엄마는 머리가 짧아야 편하던데",
          "original_form": "엄마는 머리가 짧아야 편하던데",
          "speaker_id": "2",
          "time": "20210903 07:24"
        },
        {
          "id": "MDRW2100039050.1.12",
          "form": "머리 올리면 그것도 편해요",
          "original_form": "머리 올리면 그것도 편해요",
          "speaker_id": "1",
          "time": "20210903 07:24"
        },
        {
          "id": "MDRW2100039050.1.13",
          "form": "그렇구나",
          "original_form": "그렇구나",
          "speaker_id": "2",
          "time": "20210903 07:24"
        },
        {
          "id": "MDRW2100039050.1.14",
          "form": "머리올리는 도구들만 늘구있어요",
