      "utterance": [
        {
          "id": "MDRW2100016351.1.1",
          "form": "드라마 좋아하시나여??",
          "original_form": "드라마 좋아하시나여??",
          "speaker_id": "1",
          "time": "20210826 12:10"
        },
        {
          "id": "MDRW2100016351.1.2",
          "form": "안녕하세요 저는 시간이 별루엄서서 왓챠서 영화봐요 유트브나",
          "original_form": "안녕하세요 저는 시간이 별루엄서서 왓챠서 영화봐요 유트브나",
          "speaker_id": "2",
          "time": "20210826 12:18"
        },
        {
          "id": "MDRW2100016351.1.3",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016351.1.4",
          "form": "최근에 보신 영화 있으세요??",
          "original_form": "최근에 보신 영화 있으세요??",
          "speaker_id": "1",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016351.1.5",
          "form": "모가디슈보고왔어요",
          "original_form": "모가디슈보고왔어요",
          "speaker_id": "2",
          "time": "20210826 13:37"
        },
        {
          "id": "MDRW2100016351.1.6",
          "form": "모가다슈 재밌죠ㅜ",
          "original_form": "모가다슈 재밌죠ㅜ",
          "speaker_id": "1",
          "time": "20210826 13:37"
        },
        {
          "id": "MDRW2100016351.1.7",
          "form": "보셨나요",
          "original_form": "보셨나요",
          "speaker_id": "2",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100016351.1.8",
          "form": "네ㅜㅠ",
          "original_form": "네ㅜㅠ",
          "speaker_id": "1",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100016351.1.9",
          "form": "아프간 보니까 많이 생각나더라구요",
          "original_form": "아프간 보니까 많이 생각나더라구요",
          "speaker_id": "1",
          "time": "20210826 13:38"
        },
        {
          "id": "MDRW2100016351.1.10",
          "form": "저도요 보고나니까 터졌더라구요",
          "original_form": "저도요 보고나니까 터졌더라구요",
          "speaker_id": "2",
          "time": "20210826 13:42"
        },
        {
          "id": "MDRW2100016351.1.11",
          "form": "참.. 21세기에 이런 일이..",
          "original_form": "참.. 21세기에 이런 일이..",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016351.1.12",
          "form": "너무 신기해요...",
          "original_form": "너무 신기해요...",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016351.1.13",
          "form": "어케 이런 국제사회사건이 딱 우연히 터지네요",
          "original_form": "어케 이런 국제사회사건이 딱 우연히 터지네요",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016351.1.14",
          "form": "그래도 좀 더 보러가는거도 있을거같아요!",
          "original_form": "그래도 좀 더 보러가는거도 있을거같아요!",
          "speaker_id": "1",
          "time": "20210826 13:48"
        },
        {
          "id": "MDRW2100016351.1.15",
          "form": "영화 잘봤는데 대박나문좋겠어요",
          "original_form": "영화 잘봤는데 대박나문좋겠어요",
          "speaker_id": "2",
          "time": "20210826 13:50"
        },
        {
          "id": "MDRW2100016351.1.16",
          "form": "지금 250만 넘은거 같은데",
          "original_form": "지금 250만 넘은거 같은데",
          "speaker_id": "1",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016351.1.17",
          "form": "아마 올해 최고인거 같아요!",
          "original_form": "아마 올해 최고인거 같아요!",
          "speaker_id": "1",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016351.1.18",
          "form": "와우~~~",
          "original_form": "와우~~~",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016351.1.19",
          "form": "한국영화 짜란다",
          "original_form": "한국영화 짜란다",
          "speaker_id": "2",
          "time": "20210826 13:54"
        },
        {
          "id": "MDRW2100016351.1.20",
          "form": "요즘 영화산업이 정말 안되간했죠...ㅜ",
          "original_form": "요즘 영화산업이 정말 안되간했죠...ㅜ",
          "speaker_id": "1",
          "time": "20210826 14:47"
        },
        {
          "id": "MDRW2100016351.1.21",
          "form": "코로나땜시ㅜㅜ",
          "original_form": "코로나땜시ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 14:51"
        },
        {
          "id": "MDRW2100016351.1.22",
          "form": "네ㅠㅜ",
          "original_form": "네ㅠㅜ",
          "speaker_id": "1",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100016351.1.23",
          "form": "저도 솔직히 영화보러가기 좀 꺼러지긴 하더라구요ㅎ",
          "original_form": "저도 솔직히 영화보러가기 좀 꺼러지긴 하더라구요ㅎ",
          "speaker_id": "1",
          "time": "20210826 16:12"
        },
        {
          "id": "MDRW2100016351.1.24",
          "form": "네 요새 사람없어서 전세낸거같아요",
          "original_form": "네 요새 사람없어서 전세낸거같아요",
          "speaker_id": "2",
          "time": "20210826 16:23"
        },
        {
          "id": "MDRW2100016351.1.25",
          "form": "가끔 그런 느낌은 좋더라구요ㅋㅋ",
          "original_form": "가끔 그런 느낌은 좋더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016351.1.26",
          "form": "혼자 영화관에서 영화보는ㅊ거",
          "original_form": "혼자 영화관에서 영화보는ㅊ거",
          "speaker_id": "1",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016351.1.27",
          "form": "네 그건 좀 좋아요ㅎㅎ근데 팝콘도 못먹게하더라구요",
          "original_form": "네 그건 좀 좋아요ㅎㅎ근데 팝콘도 못먹게하더라구요",
          "speaker_id": "2",
          "time": "20210826 16:37"
        },
        {
          "id": "MDRW2100016351.1.28",
          "form": "네ㅜ 원래 영화보면서 팝콘이 진리인데ㅜ",
          "original_form": "네ㅜ 원래 영화보면서 팝콘이 진리인데ㅜ",
          "speaker_id": "1",
          "time": "20210826 16:40"
        }
      ]
    }
