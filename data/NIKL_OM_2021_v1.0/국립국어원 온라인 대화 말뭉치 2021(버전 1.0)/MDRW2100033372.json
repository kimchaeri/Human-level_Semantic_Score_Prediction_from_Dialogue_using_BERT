      "utterance": [
        {
          "id": "MDRW2100033372.1.1",
          "form": "안녕 name2",
          "original_form": "안녕 &name2&",
          "speaker_id": "1",
          "time": "20210906 23:40"
        },
        {
          "id": "MDRW2100033372.1.2",
          "form": "나 왔엉",
          "original_form": "나 왔엉",
          "speaker_id": "1",
          "time": "20210906 23:40"
        },
        {
          "id": "MDRW2100033372.1.3",
          "form": "아 커뮤뛰고싶당....",
          "original_form": "아 커뮤뛰고싶당....",
          "speaker_id": "1",
          "time": "20210906 23:40"
        },
        {
          "id": "MDRW2100033372.1.4",
          "form": "안녕 왔니",
          "original_form": "안녕 왔니",
          "speaker_id": "2",
          "time": "20210906 23:40"
        },
        {
          "id": "MDRW2100033372.1.5",
          "form": "아... 나도...",
          "original_form": "아... 나도...",
          "speaker_id": "2",
          "time": "20210906 23:40"
        },
        {
          "id": "MDRW2100033372.1.6",
          "form": "커뮤 뛰고 싶음",
          "original_form": "커뮤 뛰고 싶음",
          "speaker_id": "2",
          "time": "20210906 23:40"
        },
        {
          "id": "MDRW2100033372.1.7",
          "form": "이번에 찾아온 비엘커",
          "original_form": "이번에 찾아온 비엘커",
          "speaker_id": "2",
          "time": "20210906 23:40"
        },
        {
          "id": "MDRW2100033372.1.8",
          "form": "동결날 것 같음",
          "original_form": "동결날 것 같음",
          "speaker_id": "2",
          "time": "20210906 23:41"
        },
        {
          "id": "MDRW2100033372.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ 에에..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 에에..",
          "speaker_id": "1",
          "time": "20210906 23:42"
        },
        {
          "id": "MDRW2100033372.1.10",
          "form": "그럼 뱀파이어 커뮤니티 찾을까",
          "original_form": "그럼 뱀파이어 커뮤니티 찾을까",
          "speaker_id": "1",
          "time": "20210906 23:42"
        },
        {
          "id": "MDRW2100033372.1.11",
          "form": "그것도 나쁘진 않잖아",
          "original_form": "그것도 나쁘진 않잖아",
          "speaker_id": "1",
          "time": "20210906 23:42"
        },
        {
          "id": "MDRW2100033372.1.12",
          "form": "자작 캐릭터 대화는 즐거운데",
          "original_form": "자작 캐릭터 대화는 즐거운데",
          "speaker_id": "1",
          "time": "20210906 23:42"
        },
        {
          "id": "MDRW2100033372.1.13",
          "form": "항상..커뮤찾는게 문제죠",
          "original_form": "항상..커뮤찾는게 문제죠",
          "speaker_id": "1",
          "time": "20210906 23:42"
        },
        {
          "id": "MDRW2100033372.1.14",
          "form": "뭐든 커뮤니티 러닝은 즐거운데",
          "original_form": "뭐든 커뮤니티 러닝은 즐거운데",
          "speaker_id": "2",
          "time": "20210906 23:43"
        },
        {
          "id": "MDRW2100033372.1.15",
          "form": "막상 시작하려니까",
          "original_form": "막상 시작하려니까",
          "speaker_id": "2",
          "time": "20210906 23:43"
        },
        {
          "id": "MDRW2100033372.1.16",
          "form": "온 몸에 힘이 쫙 빠지는 그런 느낌",
          "original_form": "온 몸에 힘이 쫙 빠지는 그런 느낌",
          "speaker_id": "2",
          "time": "20210906 23:43"
        },
        {
          "id": "MDRW2100033372.1.17",
          "form": "ㄴㅋㅋㅋㅋㅋㅋㅋㅋ그니까....",
          "original_form": "ㄴㅋㅋㅋㅋㅋㅋㅋㅋ그니까....",
          "speaker_id": "1",
          "time": "20210906 23:44"
        },
        {
          "id": "MDRW2100033372.1.18",
          "form": "크악...",
          "original_form": "크악...",
          "speaker_id": "1",
          "time": "20210906 23:44"
        },
        {
          "id": "MDRW2100033372.1.19",
          "form": "좋은 커뮤 사람 만나고 싶다",
          "original_form": "좋은 커뮤 사람 만나고 싶다",
          "speaker_id": "1",
          "time": "20210906 23:45"
        },
        {
          "id": "MDRW2100033372.1.20",
          "form": "좋은 커뮤니티에 가서 이쁜 사랑을 하고 싶어....",
          "original_form": "좋은 커뮤니티에 가서 이쁜 사랑을 하고 싶어....",
          "speaker_id": "2",
          "time": "20210906 23:45"
        },
        {
          "id": "MDRW2100033372.1.21",
          "form": "어디 일주일 이하의 가볍게 즐길 수 있는 일상 커뮤니티 없나..",
          "original_form": "어디 일주일 이하의 가볍게 즐길 수 있는 일상 커뮤니티 없나..",
          "speaker_id": "2",
          "time": "20210906 23:46"
        },
        {
          "id": "MDRW2100033372.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋ ㅜㅜㅜㅜㅜname2아 유니콘은 어떤 색이 좋니",
          "original_form": "ㅋㅋㅋㅋㅋㅋ ㅜㅜㅜㅜㅜ&name2&아 유니콘은 어떤 색이 좋니",
          "speaker_id": "1",
          "time": "20210906 23:47"
        },
        {
          "id": "MDRW2100033372.1.23",
          "form": "유니콘을 참 좋아해 name2이",
          "original_form": "유니콘을 참 좋아해 &name2&이",
          "speaker_id": "1",
          "time": "20210906 23:47"
        },
        {
          "id": "MDRW2100033372.1.24",
          "form": "유니콘??",
          "original_form": "유니콘??",
          "speaker_id": "2",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100033372.1.25",
          "form": "무지개 빤짝이샥",
          "original_form": "무지개 빤짝이샥",
          "speaker_id": "2",
          "time": "20210906 23:48"
        },
        {
          "id": "MDRW2100033372.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:09"
        },
        {
          "id": "MDRW2100033372.1.27",
          "form": "우리가 원하는 거 유니콘 커뮤임 거의",
          "original_form": "우리가 원하는 거 유니콘 커뮤임 거의",
          "speaker_id": "1",
          "time": "20210907 00:09"
        },
        {
          "id": "MDRW2100033372.1.28",
          "form": "아이고.. 그렇네... 환상 속 커뮤니티다 정말..",
          "original_form": "아이고.. 그렇네... 환상 속 커뮤니티다 정말..",
          "speaker_id": "2",
          "time": "20210907 00:10"
        },
        {
          "id": "MDRW2100033372.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 00:13"
        },
        {
          "id": "MDRW2100033372.1.30",
