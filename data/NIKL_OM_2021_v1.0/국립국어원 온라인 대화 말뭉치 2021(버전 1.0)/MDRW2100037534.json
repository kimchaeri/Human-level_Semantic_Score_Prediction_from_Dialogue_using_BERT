      "utterance": [
        {
          "id": "MDRW2100037534.1.1",
          "form": "단풍 구경 좋아하나요?",
          "original_form": "단풍 구경 좋아하나요?",
          "speaker_id": "1",
          "time": "20210901 06:58"
        },
        {
          "id": "MDRW2100037534.1.2",
          "form": "네 좋아해요ㅋㅋ 단풍구경 좋아하세요?",
          "original_form": "네 좋아해요ㅋㅋ 단풍구경 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 08:32"
        },
        {
          "id": "MDRW2100037534.1.3",
          "form": "단풍때문에 가을을 제일 좋아하죠",
          "original_form": "단풍때문에 가을을 제일 좋아하죠",
          "speaker_id": "1",
          "time": "20210901 10:09"
        },
        {
          "id": "MDRW2100037534.1.4",
          "form": "네 단풍구경 너무좋죠ㄱㅋ",
          "original_form": "네 단풍구경 너무좋죠ㄱㅋ",
          "speaker_id": "2",
          "time": "20210901 10:09"
        },
        {
          "id": "MDRW2100037534.1.5",
          "form": "저도봄.가을이 좋아요ㅋㅋ",
          "original_form": "저도봄.가을이 좋아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 10:09"
        },
        {
          "id": "MDRW2100037534.1.6",
          "form": "근데 점점 짧아져서 슬프네요",
          "original_form": "근데 점점 짧아져서 슬프네요",
          "speaker_id": "1",
          "time": "20210901 10:14"
        },
        {
          "id": "MDRW2100037534.1.7",
          "form": "맞아요ㅠ짧아지는게 딱 느껴져요, ㅜ",
          "original_form": "맞아요ㅠ짧아지는게 딱 느껴져요, ㅜ",
          "speaker_id": "2",
          "time": "20210901 10:16"
        },
        {
          "id": "MDRW2100037534.1.8",
          "form": "여행은 자주가시나요?",
          "original_form": "여행은 자주가시나요?",
          "speaker_id": "2",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100037534.1.9",
          "form": "코로나때문에 힘드시죠?",
          "original_form": "코로나때문에 힘드시죠?",
          "speaker_id": "2",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100037534.1.10",
          "form": "최근에 여행 어디가셨나요?",
          "original_form": "최근에 여행 어디가셨나요?",
          "speaker_id": "2",
          "time": "20210901 17:40"
        },
        {
          "id": "MDRW2100037534.1.11",
          "form": "코로나 이후로는 정말 아무데도 못갔어요..",
          "original_form": "코로나 이후로는 정말 아무데도 못갔어요..",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100037534.1.12",
          "form": "예전에 단풍구경하러 내장산 갔을 떄가 가장 좋았어요",
          "original_form": "예전에 단풍구경하러 내장산 갔을 떄가 가장 좋았어요",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100037534.1.13",
          "form": "ㅠㅠ저두요,   정말 확진자 조금나오면",
          "original_form": "ㅠㅠ저두요,   정말 확진자 조금나오면",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100037534.1.14",
          "form": "여행가긴했지만ㅜ",
          "original_form": "여행가긴했지만ㅜ",
          "speaker_id": "2",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100037534.1.15",
          "form": "사람많은데는 피하게되더라구요",
          "original_form": "사람많은데는 피하게되더라구요",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100037534.1.16",
          "form": "아니면 펜션에서놀던지ㅜ",
          "original_form": "아니면 펜션에서놀던지ㅜ",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100037534.1.17",
          "form": "이번에 접종률 높고 그러면 내년엔 좀 여행도 다닐 수 있지 않을까 기대해봅니다",
          "original_form": "이번에 접종률 높고 그러면 내년엔 좀 여행도 다닐 수 있지 않을까 기대해봅니다",
          "speaker_id": "1",
          "time": "20210901 21:56"
        },
        {
          "id": "MDRW2100037534.1.18",
          "form": "네 그러니깐요 ㅋㅋ 정말 여행을 마음껏 다녀보고싶어요 ㅋㅋ",
          "original_form": "네 그러니깐요 ㅋㅋ 정말 여행을 마음껏 다녀보고싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100037534.1.19",
          "form": "해외는 아직 어렵겠지만 ㅋㅋ 국내라도 ㅋㅋ",
          "original_form": "해외는 아직 어렵겠지만 ㅋㅋ 국내라도 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:01"
        },
        {
          "id": "MDRW2100037534.1.20",
          "form": "그래요.. 국내라도 맘편히 다니면 좋겠네요",
          "original_form": "그래요.. 국내라도 맘편히 다니면 좋겠네요",
          "speaker_id": "1",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100037534.1.21",
          "form": "맞아요 ㅠㅠ ㅋㅋㅋ 그때까지 힘내봅시다 ㅋㅋㅋ 히히",
          "original_form": "맞아요 ㅠㅠ ㅋㅋㅋ 그때까지 힘내봅시다 ㅋㅋㅋ 히히",
          "speaker_id": "2",
          "time": "20210901 22:04"
        },
        {
          "id": "MDRW2100037534.1.22",
          "form": "꼭 내년엔 여행 다닐 수 있길~",
          "original_form": "꼭 내년엔 여행 다닐 수 있길~",
          "speaker_id": "1",
          "time": "20210901 22:05"
        }
      ]
    }
  ]
