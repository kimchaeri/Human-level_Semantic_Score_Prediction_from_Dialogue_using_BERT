      "utterance": [
        {
          "id": "MDRW2100032757.1.1",
          "form": "인터넷 구매가 필요한가",
          "original_form": "인터넷 구매가 필요한가",
          "speaker_id": "1",
          "time": "20210905 23:11"
        },
        {
          "id": "MDRW2100032757.1.2",
          "form": "뭐사게??",
          "original_form": "뭐사게??",
          "speaker_id": "2",
          "time": "20210905 23:13"
        },
        {
          "id": "MDRW2100032757.1.3",
          "form": "넌 살게 잏어",
          "original_form": "넌 살게 잏어",
          "speaker_id": "1",
          "time": "20210905 23:14"
        },
        {
          "id": "MDRW2100032757.1.4",
          "form": "나 탐폰 사야하긴 해",
          "original_form": "나 탐폰 사야하긴 해",
          "speaker_id": "2",
          "time": "20210905 23:45"
        },
        {
          "id": "MDRW2100032757.1.5",
          "form": "인터넷으로 알아보고 구매해",
          "original_form": "인터넷으로 알아보고 구매해",
          "speaker_id": "1",
          "time": "20210906 06:38"
        },
        {
          "id": "MDRW2100032757.1.6",
          "form": "그래야겠네",
          "original_form": "그래야겠네",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100032757.1.7",
          "form": "요즘 어디서 사는게 싸려나",
          "original_form": "요즘 어디서 사는게 싸려나",
          "speaker_id": "2",
          "time": "20210906 14:04"
        },
        {
          "id": "MDRW2100032757.1.8",
          "form": "그래도 쿠팡이 저렴하지 않을까 ?",
          "original_form": "그래도 쿠팡이 저렴하지 않을까 ?",
          "speaker_id": "1",
          "time": "20210906 18:11"
        },
        {
          "id": "MDRW2100032757.1.9",
          "form": "근가? 찾아보ㅏ야겠네",
          "original_form": "근가? 찾아보ㅏ야겠네",
          "speaker_id": "2",
          "time": "20210906 20:19"
        },
        {
          "id": "MDRW2100032757.1.10",
          "form": "나중에ㅎㅎ",
          "original_form": "나중에ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100032757.1.11",
          "form": "엄마는 뭐 안사?",
          "original_form": "엄마는 뭐 안사?",
          "speaker_id": "2",
          "time": "20210906 20:20"
        },
        {
          "id": "MDRW2100032757.1.12",
          "form": "머리 탈모방지 샴푸 사야지",
          "original_form": "머리 탈모방지 샴푸 사야지",
          "speaker_id": "1",
          "time": "20210906 20:33"
        },
        {
          "id": "MDRW2100032757.1.13",
          "form": "아 맞다 그 김희철 광고하는거",
          "original_form": "아 맞다 그 김희철 광고하는거",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100032757.1.14",
          "form": "그루트인가?",
          "original_form": "그루트인가?",
          "speaker_id": "2",
          "time": "20210906 21:13"
        },
        {
          "id": "MDRW2100032757.1.15",
          "form": "김희철광고 하는거 맞아",
          "original_form": "김희철광고 하는거 맞아",
          "speaker_id": "1",
          "time": "20210906 21:15"
        },
        {
          "id": "MDRW2100032757.1.16",
          "form": "웅웅 그거 좋다더라",
          "original_form": "웅웅 그거 좋다더라",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100032757.1.17",
          "form": "세일안하나 모르겠넹",
          "original_form": "세일안하나 모르겠넹",
          "speaker_id": "2",
          "time": "20210906 21:22"
        },
        {
          "id": "MDRW2100032757.1.18",
          "form": "세일하는곳을 찾아봐야지",
          "original_form": "세일하는곳을 찾아봐야지",
          "speaker_id": "1",
          "time": "20210906 21:24"
        },
        {
          "id": "MDRW2100032757.1.19",
          "form": "그래그래 마트도 가보고 온라인도 찾아보지 뭐",
          "original_form": "그래그래 마트도 가보고 온라인도 찾아보지 뭐",
          "speaker_id": "2",
          "time": "20210907 00:39"
        }
      ]
