      "utterance": [
        {
          "id": "MDRW2100020046.1.1",
          "form": "저는 이미 장만했답니다 ㅋㅋ",
          "original_form": "저는 이미 장만했답니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100020046.1.2",
          "form": "사실 올해 봄 환절기때",
          "original_form": "사실 올해 봄 환절기때",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100020046.1.3",
          "form": "야상을 살까했는데",
          "original_form": "야상을 살까했는데",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020046.1.4",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020046.1.5",
          "form": "고민하고 일주일도 안돼서 갑자기 기온이 팍 오르더라구요",
          "original_form": "고민하고 일주일도 안돼서 갑자기 기온이 팍 오르더라구요",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020046.1.6",
          "form": "아아 ㅠㅠ",
          "original_form": "아아 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020046.1.7",
          "form": "반팔만 입어도 더워가지고...",
          "original_form": "반팔만 입어도 더워가지고...",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020046.1.8",
          "form": "날씨 변덕이 심하죠 간절기에는",
          "original_form": "날씨 변덕이 심하죠 간절기에는",
          "speaker_id": "1",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020046.1.9",
          "form": "이렇게 더울 줄 모르고 긴바지를 많이샀는데",
          "original_form": "이렇게 더울 줄 모르고 긴바지를 많이샀는데",
          "speaker_id": "2",
          "time": "20210827 23:27"
        },
        {
          "id": "MDRW2100020046.1.10",
          "form": "올여름 진짜 더워서 반바지 안사고 후회했어요ㅋㅋㅋㅋ",
          "original_form": "올여름 진짜 더워서 반바지 안사고 후회했어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020046.1.11",
          "form": "ㅋㅋㅋㅋㅋ 반바지가 젤 시원해요 스커트랑",
          "original_form": "ㅋㅋㅋㅋㅋ 반바지가 젤 시원해요 스커트랑",
          "speaker_id": "1",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020046.1.12",
          "form": "긴바지는 진짜 쪄죽더라구요..",
          "original_form": "긴바지는 진짜 쪄죽더라구요..",
          "speaker_id": "1",
          "time": "20210827 23:28"
        },
        {
          "id": "MDRW2100020046.1.13",
          "form": "그나마 올해 산것중 제일 얇은 냉장고바지로 어떻게  버텼어요ㅋㅋㅋㅋ",
          "original_form": "그나마 올해 산것중 제일 얇은 냉장고바지로 어떻게  버텼어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020046.1.14",
          "form": "냉장고바지가 최고에요 ㅋㅋㅋ",
          "original_form": "냉장고바지가 최고에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100020046.1.15",
          "form": "그래도 이제 많이 시원해져서",
          "original_form": "그래도 이제 많이 시원해져서",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020046.1.16",
          "form": "드디어 맘놓고 긴바지 입겠어요ㅋㅋㅋ",
          "original_form": "드디어 맘놓고 긴바지 입겠어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:30"
        },
        {
          "id": "MDRW2100020046.1.17",
          "form": "맞아요 이제서야 ㅋㅋㅋ",
          "original_form": "맞아요 이제서야 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:30"
        }
      ]
