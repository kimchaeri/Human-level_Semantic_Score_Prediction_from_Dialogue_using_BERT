          "speaker_id": "2",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027326.1.12",
          "form": "물복",
          "original_form": "물복",
          "speaker_id": "2",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027326.1.13",
          "form": "어느파시죠?",
          "original_form": "어느파시죠?",
          "speaker_id": "2",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027326.1.14",
          "form": "다복요",
          "original_form": "다복요",
          "speaker_id": "1",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027326.1.15",
          "form": "복숭아 이즈 뭔들",
          "original_form": "복숭아 이즈 뭔들",
          "speaker_id": "1",
          "time": "20210902 11:32"
        },
        {
          "id": "MDRW2100027326.1.16",
          "form": "전달면 둘다 먹습니다",
          "original_form": "전달면 둘다 먹습니다",
          "speaker_id": "2",
          "time": "20210902 11:32"
        },
        {
          "id": "MDRW2100027326.1.17",
          "form": "복숭아 음료도 좋아해요",
          "original_form": "복숭아 음료도 좋아해요",
          "speaker_id": "2",
          "time": "20210902 11:32"
        },
        {
          "id": "MDRW2100027326.1.18",
          "form": "근데 물복을 좀  더 좋아해요",
          "original_form": "근데 물복을 좀  더 좋아해요",
          "speaker_id": "1",
          "time": "20210902 11:32"
        },
        {
          "id": "MDRW2100027326.1.19",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210902 11:32"
        },
        {
          "id": "MDRW2100027326.1.20",
          "form": "모구모구",
