        {
          "id": "MDRW2100027326.1.1",
          "form": "맛집이에요",
          "original_form": "맛집이에요",
          "speaker_id": "2",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027326.1.2",
          "form": "가야할 맛집이 많네요",
          "original_form": "가야할 맛집이 많네요",
          "speaker_id": "1",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027326.1.3",
          "form": "옆에 꽈배기 맛난곳도있고",
          "original_form": "옆에 꽈배기 맛난곳도있고",
          "speaker_id": "2",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027326.1.4",
          "form": "타르트 맛집도 있는데",
          "original_form": "타르트 맛집도 있는데",
          "speaker_id": "2",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027326.1.5",
          "form": "타르트!",
          "original_form": "타르트!",
          "speaker_id": "1",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027326.1.6",
          "form": "먹고싶다",
          "original_form": "먹고싶다",
          "speaker_id": "1",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027326.1.7",
          "form": "복숭아 타르트 맛있어 보이더라구요",
          "original_form": "복숭아 타르트 맛있어 보이더라구요",
          "speaker_id": "2",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027326.1.8",
          "form": "복숭아 좋아하시나요",
          "original_form": "복숭아 좋아하시나요",
          "speaker_id": "2",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027326.1.9",
          "form": "와 네!",
          "original_form": "와 네!",
          "speaker_id": "1",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027326.1.10",
          "form": "엄청요",
          "original_form": "엄청요",
          "speaker_id": "1",
          "time": "20210902 11:31"
        },
        {
          "id": "MDRW2100027326.1.11",
          "form": "딱복",
