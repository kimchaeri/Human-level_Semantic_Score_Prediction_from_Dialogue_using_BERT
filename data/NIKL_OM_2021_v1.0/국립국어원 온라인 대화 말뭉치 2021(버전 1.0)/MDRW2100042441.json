      "utterance": [
        {
          "id": "MDRW2100042441.1.1",
          "form": "근데 그거",
          "original_form": "근데 그거",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042441.1.2",
          "form": "하루에 너무 많이먹지는마",
          "original_form": "하루에 너무 많이먹지는마",
          "speaker_id": "1",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042441.1.3",
          "form": "아 맞아",
          "original_form": "아 맞아",
          "speaker_id": "2",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042441.1.4",
          "form": "하류 한포가 적당하다더라",
          "original_form": "하류 한포가 적당하다더라",
          "speaker_id": "2",
          "time": "20210906 22:10"
        },
        {
          "id": "MDRW2100042441.1.5",
          "form": "맞아 설탕대신 아스파탐 들었는데",
          "original_form": "맞아 설탕대신 아스파탐 들었는데",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042441.1.6",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042441.1.7",
          "form": "많이먹으먄 장에 안좋을수도잇음",
          "original_form": "많이먹으먄 장에 안좋을수도잇음",
          "speaker_id": "1",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042441.1.8",
          "form": "오 그렇구나",
          "original_form": "오 그렇구나",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042441.1.9",
          "form": "안그래도",
          "original_form": "안그래도",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042441.1.10",
          "form": "피티쌤이",
          "original_form": "피티쌤이",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042441.1.11",
          "form": "그런거에 너무 의존하면 안좋다 해서",
          "original_form": "그런거에 너무 의존하면 안좋다 해서",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042441.1.12",
          "form": "알바할때나 공부할때 가끔 먹어야 할듯",
          "original_form": "알바할때나 공부할때 가끔 먹어야 할듯",
          "speaker_id": "2",
          "time": "20210906 22:11"
        },
        {
          "id": "MDRW2100042441.1.13",
          "form": "요즘 차 마시는 중",
          "original_form": "요즘 차 마시는 중",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042441.1.14",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042441.1.15",
          "form": "무슨차??",
          "original_form": "무슨차??",
          "speaker_id": "1",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042441.1.16",
          "form": "사촌오빠준건데",
          "original_form": "사촌오빠준건데",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042441.1.17",
          "form": "녹차랑 복숭아맛 같이 나는거",
          "original_form": "녹차랑 복숭아맛 같이 나는거",
          "speaker_id": "2",
          "time": "20210906 22:12"
        },
        {
          "id": "MDRW2100042441.1.18",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042441.1.19",
          "form": "근데 물 2리터 마시는것도",
          "original_form": "근데 물 2리터 마시는것도",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042441.1.20",
          "form": "너무 어렵지 않아?",
          "original_form": "너무 어렵지 않아?",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042441.1.21",
          "form": "맞아 어려워ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아 어려워ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042441.1.22",
          "form": "그래도 크리스탈 저거 마시면서",
          "original_form": "그래도 크리스탈 저거 마시면서",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042441.1.23",
          "form": "음수량도 늘었어",
          "original_form": "음수량도 늘었어",
          "speaker_id": "1",
          "time": "20210906 22:14"
        },
        {
          "id": "MDRW2100042441.1.24",
          "form": "나도 그냥",
          "original_form": "나도 그냥",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042441.1.25",
          "form": "텀블러에 먹다가",
          "original_form": "텀블러에 먹다가",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042441.1.26",
          "form": "물통 큰거 하나에",
          "original_form": "물통 큰거 하나에",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042441.1.27",
          "form": "티백 하나 넣고 먹는중",
          "original_form": "티백 하나 넣고 먹는중",
          "speaker_id": "2",
          "time": "20210906 22:15"
        },
        {
          "id": "MDRW2100042441.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ굿굿",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ굿굿",
          "speaker_id": "1",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042441.1.29",
          "form": "어떤사람은 2L 페트병에다가 한포 넣어서",
          "original_form": "어떤사람은 2L 페트병에다가 한포 넣어서",
          "speaker_id": "1",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042441.1.30",
          "form": "물병째로 마신다더라",
          "original_form": "물병째로 마신다더라",
          "speaker_id": "1",
          "time": "20210906 22:16"
        },
        {
          "id": "MDRW2100042441.1.31",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042441.1.32",
          "form": "근데 눈에 목표치가 딱 보이니까",
          "original_form": "근데 눈에 목표치가 딱 보이니까",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042441.1.33",
          "form": "좋은것 같음",
          "original_form": "좋은것 같음",
          "speaker_id": "2",
          "time": "20210906 22:17"
        },
        {
          "id": "MDRW2100042441.1.34",
