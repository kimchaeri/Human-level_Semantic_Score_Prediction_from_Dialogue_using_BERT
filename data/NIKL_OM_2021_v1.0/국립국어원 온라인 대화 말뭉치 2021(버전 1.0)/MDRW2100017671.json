        {
          "id": "MDRW2100017671.1.1",
          "form": "오 동호회 활동 하세요?",
          "original_form": "오 동호회 활동 하세요?",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017671.1.2",
          "form": "요즘 동호회 활동은 못하고 있어요",
          "original_form": "요즘 동호회 활동은 못하고 있어요",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017671.1.3",
          "form": "name1은 동호회 활동 하시나요?",
          "original_form": "&name1&은 동호회 활동 하시나요?",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017671.1.4",
          "form": "저는 낯가려서 잘 안해요",
          "original_form": "저는 낯가려서 잘 안해요",
          "speaker_id": "1",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017671.1.5",
          "form": "그냥 직장동료들과의 모임 몇개",
          "original_form": "그냥 직장동료들과의 모임 몇개",
          "speaker_id": "1",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017671.1.6",
          "form": "오 어떤 모임 하세요?",
          "original_form": "오 어떤 모임 하세요?",
          "speaker_id": "2",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100017671.1.7",
          "form": "업무와 관련된 모임이라 재미는 없어요ㅋㅋㅋ",
          "original_form": "업무와 관련된 모임이라 재미는 없어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:53"
        },
        {
          "id": "MDRW2100017671.1.8",
          "form": "아 학습공동체 같은 건가요?",
          "original_form": "아 학습공동체 같은 건가요?",
          "speaker_id": "2",
          "time": "20210826 20:59"
        },
        {
          "id": "MDRW2100017671.1.9",
          "form": "아 네네!",
          "original_form": "아 네네!",
          "speaker_id": "1",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100017671.1.10",
          "form": "동호회 뭐 하세요?",
          "original_form": "동호회 뭐 하세요?",
          "speaker_id": "1",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100017671.1.11",
          "form": "어떤 동호회가 의미있을까 모르겠어서 활동을 못하겠어요",
          "original_form": "어떤 동호회가 의미있을까 모르겠어서 활동을 못하겠어요",
          "speaker_id": "1",
          "time": "20210826 21:00"
        },
        {
          "id": "MDRW2100017671.1.12",
          "form": "저는 지금은 안하고",
          "original_form": "저는 지금은 안하고",
          "speaker_id": "2",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100017671.1.13",
          "form": "그 전에 독서모임이랑 스윙댄스 동호회 했어요",
          "original_form": "그 전에 독서모임이랑 스윙댄스 동호회 했어요",
          "speaker_id": "2",
          "time": "20210826 21:01"
        },
        {
          "id": "MDRW2100017671.1.14",
          "form": "우와 뭔가 인싸신거 같네요",
          "original_form": "우와 뭔가 인싸신거 같네요",
          "speaker_id": "1",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100017671.1.15",
          "form": "스윙댄스라니 멋져요",
          "original_form": "스윙댄스라니 멋져요",
          "speaker_id": "1",
          "time": "20210826 21:03"
        },
        {
          "id": "MDRW2100017671.1.16",
          "form": "아니예요",
          "original_form": "아니예요",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017671.1.17",
          "form": "인싸체험 하러 가봤으나",
          "original_form": "인싸체험 하러 가봤으나",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017671.1.18",
          "form": "두 텀 하고 그만뒀어요",
          "original_form": "두 텀 하고 그만뒀어요",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017671.1.19",
          "form": "마침 코로나도 생겼고ㅜ",
          "original_form": "마침 코로나도 생겼고ㅜ",
          "speaker_id": "2",
          "time": "20210826 21:12"
        },
        {
          "id": "MDRW2100017671.1.20",
          "form": "아쉬워라 저도 탱고 이런거 배우러가고싶네요 급ㅋㅋㅋ",
          "original_form": "아쉬워라 저도 탱고 이런거 배우러가고싶네요 급ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017671.1.21",
          "form": "오 탱고 멋있죠",
          "original_form": "오 탱고 멋있죠",
          "speaker_id": "2",
          "time": "20210826 21:22"
        },
        {
          "id": "MDRW2100017671.1.22",
          "form": "특이한 거 배워보고싶어요 ㅋㅋ",
          "original_form": "특이한 거 배워보고싶어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:41"
        },
        {
          "id": "MDRW2100017671.1.23",
          "form": "요즘은 런데이로 러닝클럽 활동이 젤 활발한 것 같더라구요",
          "original_form": "요즘은 런데이로 러닝클럽 활동이 젤 활발한 것 같더라구요",
