      "utterance": [
        {
          "id": "MDRW2100010741.1.1",
          "form": "스위치 있었으면 좋겠다",
          "original_form": "스위치 있었으면 좋겠다",
          "speaker_id": "2",
          "time": "20210823 21:23"
        },
        {
          "id": "MDRW2100010741.1.2",
          "form": "그렇게 막 사고픈건 아닌데",
          "original_form": "그렇게 막 사고픈건 아닌데",
          "speaker_id": "2",
          "time": "20210823 21:23"
        },
        {
          "id": "MDRW2100010741.1.3",
          "form": "야숨하고싶음",
          "original_form": "야숨하고싶음",
          "speaker_id": "2",
          "time": "20210823 21:23"
        },
        {
          "id": "MDRW2100010741.1.4",
          "form": "언제 모일때 야숨 들고갈가",
          "original_form": "언제 모일때 야숨 들고갈가",
          "speaker_id": "1",
          "time": "20210823 21:23"
        },
        {
          "id": "MDRW2100010741.1.5",
          "form": "아님 빌려갈래?",
          "original_form": "아님 빌려갈래?",
          "speaker_id": "1",
          "time": "20210823 21:24"
        },
        {
          "id": "MDRW2100010741.1.6",
          "form": "특송 부탁드립니다",
          "original_form": "특송 부탁드립니다",
          "speaker_id": "2",
          "time": "20210823 21:24"
        },
        {
          "id": "MDRW2100010741.1.7",
          "form": "진자여??",
          "original_form": "진자여??",
          "speaker_id": "2",
          "time": "20210823 21:24"
        },
        {
          "id": "MDRW2100010741.1.8",
          "form": "요새 안하니깐 상관없음",
          "original_form": "요새 안하니깐 상관없음",
          "speaker_id": "1",
          "time": "20210823 21:24"
        },
        {
          "id": "MDRW2100010741.1.9",
          "form": "부수면 다시 사오시고요",
          "original_form": "부수면 다시 사오시고요",
          "speaker_id": "1",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100010741.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100010741.1.11",
          "form": "안대여",
          "original_form": "안대여",
          "speaker_id": "2",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100010741.1.12",
          "form": "안부서여",
          "original_form": "안부서여",
          "speaker_id": "2",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100010741.1.13",
          "form": "근데 야숨 좀 오래해야해서",
          "original_form": "근데 야숨 좀 오래해야해서",
          "speaker_id": "2",
          "time": "20210823 21:25"
        },
        {
          "id": "MDRW2100010741.1.14",
          "form": "바로는 못돌려줄듯",
          "original_form": "바로는 못돌려줄듯",
          "speaker_id": "2",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100010741.1.15",
          "form": "나 필드 돌아댕기기만 하고",
          "original_form": "나 필드 돌아댕기기만 하고",
          "speaker_id": "1",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100010741.1.16",
          "form": "메인퀘 열시미 안함",
          "original_form": "메인퀘 열시미 안함",
          "speaker_id": "1",
          "time": "20210823 21:26"
        },
        {
          "id": "MDRW2100010741.1.17",
          "form": "다양하게 게임오버해보기ㅋㅋㅋ",
          "original_form": "다양하게 게임오버해보기ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010741.1.18",
          "form": "근데 그런게 재미있음 게임은",
          "original_form": "근데 그런게 재미있음 게임은",
          "speaker_id": "2",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010741.1.19",
          "form": "내가 재미있는 방향으로 해서 재미있으면",
          "original_form": "내가 재미있는 방향으로 해서 재미있으면",
          "speaker_id": "2",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010741.1.20",
          "form": "된거야 그게",
          "original_form": "된거야 그게",
          "speaker_id": "2",
          "time": "20210823 21:27"
        },
        {
          "id": "MDRW2100010741.1.21",
          "form": "그치 짱 재밌음",
          "original_form": "그치 짱 재밌음",
          "speaker_id": "1",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100010741.1.22",
          "form": "멘스 거의 100시간 넘게 해야된다든데?",
          "original_form": "멘스 거의 100시간 넘게 해야된다든데?",
          "speaker_id": "1",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100010741.1.23",
          "form": "아니 에바네",
          "original_form": "아니 에바네",
          "speaker_id": "2",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100010741.1.24",
          "form": "사실 게임내 퍼즐류는 별 관심없고",
          "original_form": "사실 게임내 퍼즐류는 별 관심없고",
          "speaker_id": "2",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100010741.1.25",
          "form": "오픈월드 느낌이 좋아서 해보고픈건디",
          "original_form": "오픈월드 느낌이 좋아서 해보고픈건디",
          "speaker_id": "2",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100010741.1.26",
          "form": "퍼즐류 그럭저럭 재밌음",
          "original_form": "퍼즐류 그럭저럭 재밌음",
          "speaker_id": "1",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100010741.1.27",
          "form": "안 풀리는거 많아서 화남...",
          "original_form": "안 풀리는거 많아서 화남...",
          "speaker_id": "1",
          "time": "20210823 21:31"
        },
        {
          "id": "MDRW2100010741.1.28",
          "form": "암튼 젤다에서 젤 재밌는건",
          "original_form": "암튼 젤다에서 젤 재밌는건",
          "speaker_id": "1",
          "time": "20210823 21:53"
        },
        {
          "id": "MDRW2100010741.1.29",
          "form": "게임오버 당하는거였음",
          "original_form": "게임오버 당하는거였음",
          "speaker_id": "1",
          "time": "20210823 21:53"
        },
        {
          "id": "MDRW2100010741.1.30",
          "form": "말타는거랑",
          "original_form": "말타는거랑",
          "speaker_id": "1",
          "time": "20210823 21:53"
        },
        {
          "id": "MDRW2100010741.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 21:54"
        },
        {
          "id": "MDRW2100010741.1.32",
          "form": "세키로도 해보고싶다",
          "original_form": "세키로도 해보고싶다",
          "speaker_id": "2",
          "time": "20210823 21:54"
        }
      ]
    }
  ]
