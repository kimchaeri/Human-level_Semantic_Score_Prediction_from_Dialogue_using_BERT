      "utterance": [
        {
          "id": "MDRW2100018492.1.1",
          "form": "안녕하세용 가장 좋아하는 영화 있으세요?",
          "original_form": "안녕하세용 가장 좋아하는 영화 있으세요?",
          "speaker_id": "1",
          "time": "20210827 06:17"
        },
        {
          "id": "MDRW2100018492.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210827 06:17"
        },
        {
          "id": "MDRW2100018492.1.3",
          "form": "저는 해리포터요!ㅎ.ㅎ 어떤영화를 가장 좋아하세요?",
          "original_form": "저는 해리포터요!ㅎ.ㅎ 어떤영화를 가장 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 06:17"
        },
        {
          "id": "MDRW2100018492.1.4",
          "form": "오 저도 해리포터 시리즈 전체 다 봤어요",
          "original_form": "오 저도 해리포터 시리즈 전체 다 봤어요",
          "speaker_id": "1",
          "time": "20210827 06:17"
        },
        {
          "id": "MDRW2100018492.1.5",
          "form": "저는 마블 영화좋아해요",
          "original_form": "저는 마블 영화좋아해요",
          "speaker_id": "1",
          "time": "20210827 06:17"
        },
        {
          "id": "MDRW2100018492.1.6",
          "form": "오! 저두 마블 좋아해요",
          "original_form": "오! 저두 마블 좋아해요",
          "speaker_id": "2",
          "time": "20210827 06:18"
        },
        {
          "id": "MDRW2100018492.1.7",
          "form": "최근 블랙위도우 보셨나요??",
          "original_form": "최근 블랙위도우 보셨나요??",
          "speaker_id": "2",
          "time": "20210827 06:18"
        },
        {
          "id": "MDRW2100018492.1.8",
          "form": "어떤 캐릭터 좋아하세요?",
          "original_form": "어떤 캐릭터 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 06:18"
        },
        {
          "id": "MDRW2100018492.1.9",
          "form": "네네 봤어요! 너무 재밌더라구요ㅋㅋ",
          "original_form": "네네 봤어요! 너무 재밌더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 06:18"
        },
        {
          "id": "MDRW2100018492.1.10",
          "form": "ㅋㅋㅋㅋ저두 최근에 봤는데 너무 멋지더라구요",
          "original_form": "ㅋㅋㅋㅋ저두 최근에 봤는데 너무 멋지더라구요",
          "speaker_id": "2",
          "time": "20210827 06:20"
        },
        {
          "id": "MDRW2100018492.1.11",
          "form": "액션이 볼만했어요",
          "original_form": "액션이 볼만했어요",
          "speaker_id": "1",
          "time": "20210827 06:20"
        },
        {
          "id": "MDRW2100018492.1.12",
          "form": "영화관에서 보셨나요?",
          "original_form": "영화관에서 보셨나요?",
          "speaker_id": "2",
          "time": "20210827 06:21"
        },
        {
          "id": "MDRW2100018492.1.13",
          "form": "저는 집에서 vod로 봤어용!",
          "original_form": "저는 집에서 vod로 봤어용!",
          "speaker_id": "1",
          "time": "20210827 06:21"
        },
        {
          "id": "MDRW2100018492.1.14",
          "form": "넷플릭스 보세요?",
          "original_form": "넷플릭스 보세요?",
          "speaker_id": "1",
          "time": "20210827 06:21"
        },
        {
          "id": "MDRW2100018492.1.15",
          "form": "아 넷플릭스 봐요!",
          "original_form": "아 넷플릭스 봐요!",
          "speaker_id": "2",
          "time": "20210827 06:22"
        },
        {
          "id": "MDRW2100018492.1.16",
          "form": "넷플릭스랑 웨이브 보고 있어요!",
          "original_form": "넷플릭스랑 웨이브 보고 있어요!",
          "speaker_id": "2",
          "time": "20210827 06:22"
        },
        {
          "id": "MDRW2100018492.1.17",
          "form": "최근에 뭐 보셨어요?",
          "original_form": "최근에 뭐 보셨어요?",
          "speaker_id": "1",
          "time": "20210827 06:22"
        },
        {
          "id": "MDRW2100018492.1.18",
          "form": "오징어게임 기다리고있어용",
          "original_form": "오징어게임 기다리고있어용",
          "speaker_id": "1",
          "time": "20210827 06:22"
        },
        {
          "id": "MDRW2100018492.1.19",
          "form": "오호 그게 뭐예요? 오징어게임? 외국꺼예요?",
          "original_form": "오호 그게 뭐예요? 오징어게임? 외국꺼예요?",
          "speaker_id": "2",
          "time": "20210827 06:22"
        },
        {
          "id": "MDRW2100018492.1.20",
          "form": "이정재 배우 나오는 작품인데",
          "original_form": "이정재 배우 나오는 작품인데",
          "speaker_id": "1",
          "time": "20210827 06:23"
        },
        {
          "id": "MDRW2100018492.1.21",
          "form": "넷플릭스 오리지널이에용",
          "original_form": "넷플릭스 오리지널이에용",
