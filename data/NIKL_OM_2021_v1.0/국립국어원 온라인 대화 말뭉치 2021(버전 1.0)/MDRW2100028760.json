      "utterance": [
        {
          "id": "MDRW2100028760.1.1",
          "form": "헉 반민초파시구나",
          "original_form": "헉 반민초파시구나",
          "speaker_id": "2",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100028760.1.2",
          "form": "저는 민초파입니다,,,,",
          "original_form": "저는 민초파입니다,,,,",
          "speaker_id": "2",
          "time": "20210902 21:53"
        },
        {
          "id": "MDRW2100028760.1.3",
          "form": "오 민초 좋아하시는구나!!",
          "original_form": "오 민초 좋아하시는구나!!",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028760.1.4",
          "form": "그럼 배라 가면 주로 민초 드시나요??",
          "original_form": "그럼 배라 가면 주로 민초 드시나요??",
          "speaker_id": "1",
          "time": "20210902 21:56"
        },
        {
          "id": "MDRW2100028760.1.5",
          "form": "네 민초도 넣는편이에요! ㅋㅋ",
          "original_form": "네 민초도 넣는편이에요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100028760.1.6",
          "form": "민초말고 자주 드시는맛 있으신가요??",
          "original_form": "민초말고 자주 드시는맛 있으신가요??",
          "speaker_id": "1",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100028760.1.7",
          "form": "뉴치케랑 사빠딸이요!",
          "original_form": "뉴치케랑 사빠딸이요!",
          "speaker_id": "2",
          "time": "20210903 01:39"
        },
        {
          "id": "MDRW2100028760.1.8",
          "form": "아 맞다ㅎㅎ",
          "original_form": "아 맞다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100028760.1.9",
          "form": "까먹었어요ㅎㅎ",
          "original_form": "까먹었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100028760.1.10",
          "form": "ㅋㅋ아님다",
          "original_form": "ㅋㅋ아님다",
          "speaker_id": "2",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100028760.1.11",
          "form": "초코는 안좋아하시나요??",
          "original_form": "초코는 안좋아하시나요??",
          "speaker_id": "1",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100028760.1.12",
          "form": "콜라 /  사이다",
          "original_form": "콜라 /  사이다",
          "speaker_id": "2",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100028760.1.13",
          "form": "초코는 굳이 배라에서 안사먹는것같아요",
          "original_form": "초코는 굳이 배라에서 안사먹는것같아요",
          "speaker_id": "2",
          "time": "20210903 01:40"
        },
        {
          "id": "MDRW2100028760.1.14",
          "form": "오 그럴수도 있겠네요ㅎㅎ",
          "original_form": "오 그럴수도 있겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:41"
        },
        {
          "id": "MDRW2100028760.1.15",
          "form": "저는 사이다 좋아하는데",
          "original_form": "저는 사이다 좋아하는데",
          "speaker_id": "1",
          "time": "20210903 01:41"
        },
        {
          "id": "MDRW2100028760.1.16",
          "form": "뭘 더 좋아하세요?",
          "original_form": "뭘 더 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 01:41"
        },
        {
          "id": "MDRW2100028760.1.17",
          "form": "앗 저는 콜라파입니다",
          "original_form": "앗 저는 콜라파입니다",
          "speaker_id": "2",
          "time": "20210903 01:41"
        },
        {
          "id": "MDRW2100028760.1.18",
          "form": "ㅎㅎ요즘엔 사이다가 맛있더라구요",
          "original_form": "ㅎㅎ요즘엔 사이다가 맛있더라구요",
          "speaker_id": "1",
          "time": "20210903 01:41"
        },
        {
          "id": "MDRW2100028760.1.19",
          "form": "제로 콜라나 사이나 드셔보셨나요?",
          "original_form": "제로 콜라나 사이나 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210903 01:41"
        },
        {
          "id": "MDRW2100028760.1.20",
          "form": "그치만 사이다도 좋아해요ㅋㅋ",
          "original_form": "그치만 사이다도 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 01:42"
        },
        {
          "id": "MDRW2100028760.1.21",
          "form": "제로콜라먹어요 요즘!",
          "original_form": "제로콜라먹어요 요즘!",
          "speaker_id": "2",
          "time": "20210903 01:42"
        },
        {
          "id": "MDRW2100028760.1.22",
          "form": "펩시랑 코카콜라중에 뭐 드세요??!",
          "original_form": "펩시랑 코카콜라중에 뭐 드세요??!",
          "speaker_id": "1",
          "time": "20210903 01:42"
        },
        {
          "id": "MDRW2100028760.1.23",
          "form": "펩시랑 코카둘다 좋아해요!",
          "original_form": "펩시랑 코카둘다 좋아해요!",
          "speaker_id": "2",
          "time": "20210903 01:42"
        }
      ]
