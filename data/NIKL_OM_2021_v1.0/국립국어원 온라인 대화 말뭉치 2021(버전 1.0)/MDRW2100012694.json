          "speaker_id": "1",
          "time": "20210825 00:00"
        },
        {
          "id": "MDRW2100012694.1.2",
          "form": "ㅋㅋ군알못 ㅋㅋㅋ 다녀오셨나요",
          "original_form": "ㅋㅋ군알못 ㅋㅋㅋ 다녀오셨나요",
          "speaker_id": "2",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100012694.1.3",
          "form": "ㅠ\n네 공군",
          "original_form": "ㅠ\n네 공군",
          "speaker_id": "1",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100012694.1.4",
          "form": "아 멋지네요 공군 ㅎㅎ",
          "original_form": "아 멋지네요 공군 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:26"
        },
        {
          "id": "MDRW2100012694.1.5",
          "form": "길어요",
          "original_form": "길어요",
          "speaker_id": "1",
          "time": "20210825 07:09"
        },
        {
          "id": "MDRW2100012694.1.6",
          "form": "대신 휴가가 길어서 좋지 않아요?",
          "original_form": "대신 휴가가 길어서 좋지 않아요?",
          "speaker_id": "2",
          "time": "20210825 12:07"
        },
        {
          "id": "MDRW2100012694.1.7",
          "form": "군생활이 길죠",
          "original_form": "군생활이 길죠",
          "speaker_id": "1",
          "time": "20210825 15:39"
        },
        {
          "id": "MDRW2100012694.1.8",
          "form": "그쵸ㅜㅠ 고생많으셨습니다  ㅠㅠ",
          "original_form": "그쵸ㅜㅠ 고생많으셨습니다  ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 15:49"
        },
        {
          "id": "MDRW2100012694.1.9",
          "form": "재대하니 괜찮아요",
          "original_form": "재대하니 괜찮아요",
          "speaker_id": "1",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100012694.1.10",
          "form": "ㅎㅎㅎ 후련하시겠네요",
          "original_form": "ㅎㅎㅎ 후련하시겠네요",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100012694.1.11",
          "form": "네 편해요",
          "original_form": "네 편해요",
          "speaker_id": "1",
          "time": "20210825 16:00"
