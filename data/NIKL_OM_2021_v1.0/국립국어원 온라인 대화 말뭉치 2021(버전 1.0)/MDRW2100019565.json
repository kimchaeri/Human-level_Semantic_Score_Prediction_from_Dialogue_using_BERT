      "utterance": [
        {
          "id": "MDRW2100019565.1.1",
          "form": "가족이 제일 싫을때는 언제세요?",
          "original_form": "가족이 제일 싫을때는 언제세요?",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019565.1.2",
          "form": "음 제 의견 다 묵살하고 남자혈육들만 신경쓸 때?",
          "original_form": "음 제 의견 다 묵살하고 남자혈육들만 신경쓸 때?",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019565.1.3",
          "form": "속상하죠 많이",
          "original_form": "속상하죠 많이",
          "speaker_id": "1",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100019565.1.4",
          "form": "전 사생활없이",
          "original_form": "전 사생활없이",
          "speaker_id": "2",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100019565.1.5",
          "form": "문이 벅벅 열릴때 인것같아요",
          "original_form": "문이 벅벅 열릴때 인것같아요",
          "speaker_id": "2",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100019565.1.6",
          "form": "어우 맞아요",
          "original_form": "어우 맞아요",
          "speaker_id": "1",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100019565.1.7",
          "form": "나를 개인의 자아로 존중해주지않는것이 제일 속상하네요",
          "original_form": "나를 개인의 자아로 존중해주지않는것이 제일 속상하네요",
          "speaker_id": "2",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019565.1.8",
          "form": "그것도 되게 속상하더라구요",
          "original_form": "그것도 되게 속상하더라구요",
          "speaker_id": "1",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019565.1.9",
          "form": "맞아요 저도 얼마 전에",
          "original_form": "맞아요 저도 얼마 전에",
          "speaker_id": "1",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019565.1.10",
          "form": "가족한테 저를 누구의 아랫사람 말고 하나의 개인으로 존중해달라고",
          "original_form": "가족한테 저를 누구의 아랫사람 말고 하나의 개인으로 존중해달라고",
          "speaker_id": "1",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019565.1.11",
          "form": "선포한 적 있어요ㅋㅋㅋ 먹혔는지는 몰라도",
          "original_form": "선포한 적 있어요ㅋㅋㅋ 먹혔는지는 몰라도",
          "speaker_id": "1",
          "time": "20210827 20:23"
        },
        {
          "id": "MDRW2100019565.1.12",
          "form": "적어도 제 입장표명은 하는 게 좋을 것 같아서",
          "original_form": "적어도 제 입장표명은 하는 게 좋을 것 같아서",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019565.1.13",
          "form": "계속 안먹히더라도 어필하는것이 많이 중요하대요",
          "original_form": "계속 안먹히더라도 어필하는것이 많이 중요하대요",
          "speaker_id": "2",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019565.1.14",
          "form": "그쵸 아무래도 가만히 있으면",
          "original_form": "그쵸 아무래도 가만히 있으면",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019565.1.15",
          "form": "아무것도 안 이루어지니까",
          "original_form": "아무것도 안 이루어지니까",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019565.1.16",
          "form": "적어도 내가 여기 있다고 계속 소리를 내긴 해야겠더라구요",
          "original_form": "적어도 내가 여기 있다고 계속 소리를 내긴 해야겠더라구요",
          "speaker_id": "1",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100019565.1.17",
          "form": "가족과 함께 사는건 어떠세요",
          "original_form": "가족과 함께 사는건 어떠세요",
          "speaker_id": "2",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100019565.1.18",
          "form": "장단점이 있어요",
          "original_form": "장단점이 있어요",
          "speaker_id": "1",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100019565.1.19",
          "form": "자취하면 거의 뭐",
          "original_form": "자취하면 거의 뭐",
          "speaker_id": "1",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100019565.1.20",
          "form": "유료호흡이라는 말이 있을 정도로 돈이 안 모이지만",
          "original_form": "유료호흡이라는 말이 있을 정도로 돈이 안 모이지만",
          "speaker_id": "1",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019565.1.21",
          "form": "대신 자기만의 사생활이 있잖아요",
          "original_form": "대신 자기만의 사생활이 있잖아요",
          "speaker_id": "1",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019565.1.22",
          "form": "본가에 살면 딱 그 반대더라구요",
          "original_form": "본가에 살면 딱 그 반대더라구요",
          "speaker_id": "1",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019565.1.23",
          "form": "돈 모으기는 좋은데",
          "original_form": "돈 모으기는 좋은데",
          "speaker_id": "1",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019565.1.24",
          "form": "그것도 그래요",
          "original_form": "그것도 그래요",
          "speaker_id": "2",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019565.1.25",
          "form": "사생활이 없어요ㅋㅋ",
          "original_form": "사생활이 없어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019565.1.26",
          "form": "돈이 숨만쉬어도 나가니까",
          "original_form": "돈이 숨만쉬어도 나가니까",
          "speaker_id": "2",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019565.1.27",
          "form": "또 집안일이 귀찮은게 많죠",
          "original_form": "또 집안일이 귀찮은게 많죠",
          "speaker_id": "2",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100019565.1.28",
          "form": "맞아요 분담할 수 없고",
          "original_form": "맞아요 분담할 수 없고",
          "speaker_id": "1",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100019565.1.29",
          "form": "혼자 다 처리해야 하는 일이 많긴 해요",
          "original_form": "혼자 다 처리해야 하는 일이 많긴 해요",
          "speaker_id": "1",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100019565.1.30",
          "form": "하지만 정신건강에는",
          "original_form": "하지만 정신건강에는",
          "speaker_id": "1",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100019565.1.31",
          "form": "사생활이 성인이 되면서 많이 중요해졌어요",
          "original_form": "사생활이 성인이 되면서 많이 중요해졌어요",
          "speaker_id": "2",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100019565.1.32",
          "form": "좀 좋긴 해요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "좀 좋긴 해요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100019565.1.33",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100019565.1.34",
          "form": "가족은 어른이 되면 개념이 바뀌는것 같아요",
          "original_form": "가족은 어른이 되면 개념이 바뀌는것 같아요",
          "speaker_id": "2",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100019565.1.35",
          "form": "name2님은 어떻게 바뀌셨어요?",
          "original_form": "&name2&님은 어떻게 바뀌셨어요?",
          "speaker_id": "1",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100019565.1.36",
          "form": "전 일상공동체이자 마음의 80프로 지분이였는데",
          "original_form": "전 일상공동체이자 마음의 80프로 지분이였는데",
          "speaker_id": "2",
          "time": "20210827 20:28"
        },
        {
          "id": "MDRW2100019565.1.37",
          "form": "성인이 되니 돈을 벌면서",
          "original_form": "성인이 되니 돈을 벌면서",
          "speaker_id": "2",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100019565.1.38",
          "form": "제 자신의 자아가",
          "original_form": "제 자신의 자아가",
          "speaker_id": "2",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100019565.1.39",
          "form": "개인의 것이 된 기분입니다",
          "original_form": "개인의 것이 된 기분입니다",
          "speaker_id": "2",
          "time": "20210827 20:29"
        },
        {
          "id": "MDRW2100019565.1.40",
          "form": "심적으로 독립하시게 됐구나",
          "original_form": "심적으로 독립하시게 됐구나",
          "speaker_id": "1",
          "time": "20210827 20:30"
        },
        {
          "id": "MDRW2100019565.1.41",
          "form": "저는 이전부터 가정에서 탈출하고 싶었던 것 같아요ㅋㅋ",
          "original_form": "저는 이전부터 가정에서 탈출하고 싶었던 것 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100019565.1.42",
          "form": "예전에 한 40%였다면",
          "original_form": "예전에 한 40%였다면",
          "speaker_id": "1",
          "time": "20210827 20:31"
        },
        {
          "id": "MDRW2100019565.1.43",
          "form": "지금 10~20%정도?",
          "original_form": "지금 10~20%정도?",
          "speaker_id": "1",
          "time": "20210827 20:32"
        },
        {
          "id": "MDRW2100019565.1.44",
          "form": "애착관계가 형성이 안 됐나 쨌든",
          "original_form": "애착관계가 형성이 안 됐나 쨌든",
          "speaker_id": "1",
          "time": "20210827 20:32"
        },
        {
          "id": "MDRW2100019565.1.45",
          "form": "우연히 함께 하게 된 사람들이란 생각이예요",
          "original_form": "우연히 함께 하게 된 사람들이란 생각이예요",
          "speaker_id": "1",
          "time": "20210827 20:32"
        },
        {
          "id": "MDRW2100019565.1.46",
          "form": "개인이 바로서야 가족관계도 건강해지는것 같네요",
          "original_form": "개인이 바로서야 가족관계도 건강해지는것 같네요",
          "speaker_id": "2",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100019565.1.47",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100019565.1.48",
          "form": "가족구성원이",
          "original_form": "가족구성원이",
          "speaker_id": "1",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100019565.1.49",
          "form": "저는 혈육만 가족이 아니라고 생각해요",
          "original_form": "저는 혈육만 가족이 아니라고 생각해요",
          "speaker_id": "2",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100019565.1.50",
          "form": "다른 어느쪽에 의존하면",
          "original_form": "다른 어느쪽에 의존하면",
          "speaker_id": "1",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100019565.1.51",
          "form": "그때부터 환장쇼 하게 되는 것 같아요",
          "original_form": "그때부터 환장쇼 하게 되는 것 같아요",
          "speaker_id": "1",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100019565.1.52",
          "form": "친한친구랑 40년 같이살면 그것도 가족이죠",
          "original_form": "친한친구랑 40년 같이살면 그것도 가족이죠",
          "speaker_id": "2",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100019565.1.53",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 20:33"
        },
        {
          "id": "MDRW2100019565.1.54",
          "form": "저도 그렇게 생각해요ㅋㅋㅋ",
          "original_form": "저도 그렇게 생각해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:34"
        },
        {
          "id": "MDRW2100019565.1.55",
