      "utterance": [
        {
          "id": "MDRW2100038361.1.1",
          "form": "나도 그렇다",
          "original_form": "나도 그렇다",
          "speaker_id": "2",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100038361.1.2",
          "form": "너무 시각적인 것만 좋아하는 것 같애",
          "original_form": "너무 시각적인 것만 좋아하는 것 같애",
          "speaker_id": "2",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100038361.1.3",
          "form": "다시 동화책모드로",
          "original_form": "다시 동화책모드로",
          "speaker_id": "1",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100038361.1.4",
          "form": "맞다",
          "original_form": "맞다",
          "speaker_id": "1",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100038361.1.5",
          "form": "책은 정말 관심 없거든",
          "original_form": "책은 정말 관심 없거든",
          "speaker_id": "2",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100038361.1.6",
          "form": "둘째도",
          "original_form": "둘째도",
          "speaker_id": "1",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100038361.1.7",
          "form": "그리고 일단 읽어줘 도 이해를 못하니까",
          "original_form": "그리고 일단 읽어줘 도 이해를 못하니까",
          "speaker_id": "2",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100038361.1.8",
          "form": "오자마자",
          "original_form": "오자마자",
          "speaker_id": "1",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100038361.1.9",
          "form": "나도 좀 짜증나 고",
          "original_form": "나도 좀 짜증나 고",
          "speaker_id": "2",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100038361.1.10",
          "form": "티비이러거든",
          "original_form": "티비이러거든",
          "speaker_id": "1",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100038361.1.11",
          "form": "네 우리도",
          "original_form": "네 우리도",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038361.1.12",
          "form": "맨날 유튜브 유튜브",
          "original_form": "맨날 유튜브 유튜브",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038361.1.13",
          "form": "자동차유튜브보여달라고",
          "original_form": "자동차유튜브보여달라고",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038361.1.14",
          "form": "맞어",
          "original_form": "맞어",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038361.1.15",
          "form": "엄마 주말되면 유튜브 볼 수 있지??",
          "original_form": "엄마 주말되면 유튜브 볼 수 있지??",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038361.1.16",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038361.1.17",
          "form": "주말 기다리는 이유가",
          "original_form": "주말 기다리는 이유가",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038361.1.18",
          "form": "유튜브 보려고",
          "original_form": "유튜브 보려고",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038361.1.19",
          "form": "나도 막 보여주니ㅜ",
          "original_form": "나도 막 보여주니ㅜ",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038361.1.20",
          "form": "근데 이게 또 큰 애가 있으니까 완전 차단이 안돼",
          "original_form": "근데 이게 또 큰 애가 있으니까 완전 차단이 안돼",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038361.1.21",
          "form": "그러니깐ㅜㅜ",
          "original_form": "그러니깐ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038361.1.22",
          "form": "첫째도 폰중독이고",
          "original_form": "첫째도 폰중독이고",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038361.1.23",
          "form": "그래서 평일에는 티비나 유튜브를 안 틀어주고",
          "original_form": "그래서 평일에는 티비나 유튜브를 안 틀어주고",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038361.1.24",
          "form": "주말에만 틀어주려고 하거든",
          "original_form": "주말에만 틀어주려고 하거든",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038361.1.25",
          "form": "근데 주말에는 또 한정 없이 보니까",
          "original_form": "근데 주말에는 또 한정 없이 보니까",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038361.1.26",
          "form": "그러니 우리 첫째도 중독이다",
          "original_form": "그러니 우리 첫째도 중독이다",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100038361.1.27",
          "form": "맞어ㅜ",
          "original_form": "맞어ㅜ",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100038361.1.28",
          "form": "아 문제다",
          "original_form": "아 문제다",
          "speaker_id": "1",
          "time": "20210902 11:21"
        }
      ]
    }
  ]
