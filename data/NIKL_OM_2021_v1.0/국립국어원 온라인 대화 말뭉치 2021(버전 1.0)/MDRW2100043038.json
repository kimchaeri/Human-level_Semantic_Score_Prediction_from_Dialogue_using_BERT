      "utterance": [
        {
          "id": "MDRW2100043038.1.1",
          "form": "식사 하셨나여??",
          "original_form": "식사 하셨나여??",
          "speaker_id": "1",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100043038.1.2",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "2",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100043038.1.3",
          "form": "오 머드셨어요?",
          "original_form": "오 머드셨어요?",
          "speaker_id": "1",
          "time": "20210907 13:23"
        },
        {
          "id": "MDRW2100043038.1.4",
          "form": "구내식당에서 점심 먹고왓슴다",
          "original_form": "구내식당에서 점심 먹고왓슴다",
          "speaker_id": "2",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100043038.1.5",
          "form": "그냥 한식이욥",
          "original_form": "그냥 한식이욥",
          "speaker_id": "2",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100043038.1.6",
          "form": "ㅋㅋ 저는 샌드위치먹었습니다",
          "original_form": "ㅋㅋ 저는 샌드위치먹었습니다",
          "speaker_id": "1",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100043038.1.7",
          "form": "맛잇겟네욤",
          "original_form": "맛잇겟네욤",
          "speaker_id": "2",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100043038.1.8",
          "form": "서브웨이?",
          "original_form": "서브웨이?",
          "speaker_id": "2",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100043038.1.9",
          "form": "집에서 싸왔어요!",
          "original_form": "집에서 싸왔어요!",
          "speaker_id": "1",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100043038.1.10",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100043038.1.11",
          "form": "부지런하셔라!!",
          "original_form": "부지런하셔라!!",
          "speaker_id": "2",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100043038.1.12",
          "form": "코스트코 치아바타빵에 베이컨 계란 양상추등..ㅎㅎ",
          "original_form": "코스트코 치아바타빵에 베이컨 계란 양상추등..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100043038.1.13",
          "form": "ㅋㅋㅋ저는 똥손이에유",
          "original_form": "ㅋㅋㅋ저는 똥손이에유",
          "speaker_id": "2",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100043038.1.14",
          "form": "아닙니다ㅋㅋㅋ",
          "original_form": "아닙니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100043038.1.15",
          "form": "저두그래요..ㅋㅋㅋ",
          "original_form": "저두그래요..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100043038.1.16",
          "form": "잠와서 또 커피수혈해야겟어요",
          "original_form": "잠와서 또 커피수혈해야겟어요",
          "speaker_id": "2",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100043038.1.17",
          "form": "차한잔하고계신가요?",
          "original_form": "차한잔하고계신가요?",
          "speaker_id": "1",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100043038.1.18",
          "form": "앗 ㅋㅋㅋ 저도 녹차마셔요",
          "original_form": "앗 ㅋㅋㅋ 저도 녹차마셔요",
          "speaker_id": "1",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100043038.1.19",
          "form": "저는 무조건 얼죽아에요",
          "original_form": "저는 무조건 얼죽아에요",
          "speaker_id": "2",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100043038.1.20",
          "form": "ㅋㅋ출근하자마자 한잔",
          "original_form": "ㅋㅋ출근하자마자 한잔",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100043038.1.21",
          "form": "오후에 한잔",
          "original_form": "오후에 한잔",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100043038.1.22",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100043038.1.23",
          "form": "저도 그런줄알았는데 쌀쌀해지니 귀신같이 뜨아 찾더라구여 ㅋㅋㅋ",
          "original_form": "저도 그런줄알았는데 쌀쌀해지니 귀신같이 뜨아 찾더라구여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100043038.1.24",
          "form": "커피없이 일못해유",
          "original_form": "커피없이 일못해유",
          "speaker_id": "2",
          "time": "20210907 13:26"
        }
      ]
