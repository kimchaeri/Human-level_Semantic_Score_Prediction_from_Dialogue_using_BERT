      "utterance": [
        {
          "id": "MDRW2100006890.1.1",
          "form": "오늘 하루종일 비가 오더라고요",
          "original_form": "오늘 하루종일 비가 오더라고요",
          "speaker_id": "1",
          "time": "20210821 23:51"
        },
        {
          "id": "MDRW2100006890.1.2",
          "form": "계신 곳 날씨는 어때요?",
          "original_form": "계신 곳 날씨는 어때요?",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100006890.1.3",
          "form": "저희도 계속 비가왔어요.",
          "original_form": "저희도 계속 비가왔어요.",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100006890.1.4",
          "form": "내일도 비온다네요. ㅜㅜ",
          "original_form": "내일도 비온다네요. ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100006890.1.5",
          "form": "헉 주말인데 또 비가 오나요?",
          "original_form": "헉 주말인데 또 비가 오나요?",
          "speaker_id": "1",
          "time": "20210821 23:52"
        },
        {
          "id": "MDRW2100006890.1.6",
          "form": "비는 평일에 왔으면 좋겠어요 ㅋㅋ 주말엔 집 앞이라도 나가게",
          "original_form": "비는 평일에 왔으면 좋겠어요 ㅋㅋ 주말엔 집 앞이라도 나가게",
          "speaker_id": "1",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100006890.1.7",
          "form": "그래도 많이 시원해져서 좋아요. 얼마 전까진 에어컨",
          "original_form": "그래도 많이 시원해져서 좋아요. 얼마 전까진 에어컨",
          "speaker_id": "1",
          "time": "20210821 23:53"
        },
        {
          "id": "MDRW2100006890.1.8",
          "form": "에어컨을 하루종일 틀었는데",
          "original_form": "에어컨을 하루종일 틀었는데",
          "speaker_id": "1",
          "time": "20210821 23:54"
        },
        {
          "id": "MDRW2100006890.1.9",
          "form": "네 맞아요.",
          "original_form": "네 맞아요.",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100006890.1.10",
          "form": "요즘 시원해져서 좋아요ㅋ",
          "original_form": "요즘 시원해져서 좋아요ㅋ",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100006890.1.11",
          "form": "더운건 너무 싫어하거든요ㅋ",
          "original_form": "더운건 너무 싫어하거든요ㅋ",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100006890.1.12",
          "form": "어우 저두요",
          "original_form": "어우 저두요",
          "speaker_id": "1",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100006890.1.13",
          "form": "지금 날씨가 딱 좋았는데 비가와서ㅜㅜ",
          "original_form": "지금 날씨가 딱 좋았는데 비가와서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 23:55"
        },
        {
          "id": "MDRW2100006890.1.14",
          "form": "더우면 머리가 다 띵해요",
          "original_form": "더우면 머리가 다 띵해요",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006890.1.15",
          "form": "맞아요.ㅋ",
          "original_form": "맞아요.ㅋ",
          "speaker_id": "2",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006890.1.16",
          "form": "맞아요. 미세먼지도 없고",
          "original_form": "맞아요. 미세먼지도 없고",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006890.1.17",
          "form": "에어컨바람때문에 ㅋㅋ더 그래요",
          "original_form": "에어컨바람때문에 ㅋㅋ더 그래요",
          "speaker_id": "2",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006890.1.18",
          "form": "습도 낮고 기온도 낮아져서",
          "original_form": "습도 낮고 기온도 낮아져서",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006890.1.19",
          "form": "저 진짜 좋았거든요",
          "original_form": "저 진짜 좋았거든요",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006890.1.20",
          "form": "일년내내 요즘만 같아라 했는데",
          "original_form": "일년내내 요즘만 같아라 했는데",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006890.1.21",
          "form": "비오고 습도 높아져서 쫌..",
          "original_form": "비오고 습도 높아져서 쫌..",
          "speaker_id": "1",
          "time": "20210821 23:56"
        },
        {
          "id": "MDRW2100006890.1.22",
          "form": "네 저도요.",
          "original_form": "네 저도요.",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100006890.1.23",
          "form": "시원해져서 좋았다가ㅜㅜ",
          "original_form": "시원해져서 좋았다가ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100006890.1.24",
          "form": "일기예보보니",
          "original_form": "일기예보보니",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100006890.1.25",
          "form": "일주일 비오더라구요.",
          "original_form": "일주일 비오더라구요.",
          "speaker_id": "2",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100006890.1.26",
          "form": "일주일이요? ㅠㅠ세상에",
          "original_form": "일주일이요? ㅠㅠ세상에",
          "speaker_id": "1",
          "time": "20210821 23:57"
        },
        {
          "id": "MDRW2100006890.1.27",
          "form": "우중충하겠어요...",
          "original_form": "우중충하겠어요...",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006890.1.28",
          "form": "네 ㅠ 제습기도 틀고",
          "original_form": "네 ㅠ 제습기도 틀고",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006890.1.29",
          "form": "해야될것같아요.",
          "original_form": "해야될것같아요.",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006890.1.30",
          "form": "너무 습해서요",
          "original_form": "너무 습해서요",
          "speaker_id": "2",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006890.1.31",
          "form": "에어컨이 가고 제습기의 계절인가요 ㅎㅎ",
          "original_form": "에어컨이 가고 제습기의 계절인가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:58"
        },
        {
          "id": "MDRW2100006890.1.32",
          "form": "습도만 높지 않아도 정말 쾌적하더라고요",
          "original_form": "습도만 높지 않아도 정말 쾌적하더라고요",
          "speaker_id": "1",
          "time": "20210821 23:59"
        }
      ]
    }
