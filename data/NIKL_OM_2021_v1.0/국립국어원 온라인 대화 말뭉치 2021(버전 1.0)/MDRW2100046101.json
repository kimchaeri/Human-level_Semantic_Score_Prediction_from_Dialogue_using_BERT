        {
          "id": "MDRW2100046101.1.1",
          "form": "몽쉘좋아해?",
          "original_form": "몽쉘좋아해?",
          "speaker_id": "2",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046101.1.2",
          "form": "오 몽쉘 너무 좋아",
          "original_form": "오 몽쉘 너무 좋아",
          "speaker_id": "1",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046101.1.3",
          "form": "몽쉘이 감미료 쓰는것도나온다던데",
          "original_form": "몽쉘이 감미료 쓰는것도나온다던데",
          "speaker_id": "2",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046101.1.4",
          "form": "대체 감미료?",
          "original_form": "대체 감미료?",
          "speaker_id": "1",
          "time": "20210908 10:21"
        },
        {
          "id": "MDRW2100046101.1.5",
          "form": "응응 당뇨환자들한테좋겠다",
          "original_form": "응응 당뇨환자들한테좋겠다",
          "speaker_id": "2",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046101.1.6",
          "form": "오 그러겠다 신기한 몽쉘이네",
          "original_form": "오 그러겠다 신기한 몽쉘이네",
          "speaker_id": "1",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046101.1.7",
          "form": "맛이 똑같을까?",
          "original_form": "맛이 똑같을까?",
          "speaker_id": "2",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046101.1.8",
          "form": "거의 비슷하지않을까?",
          "original_form": "거의 비슷하지않을까?",
          "speaker_id": "1",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046101.1.9",
          "form": "나도 사실 다이어트중이라",
          "original_form": "나도 사실 다이어트중이라",
          "speaker_id": "2",
          "time": "20210908 10:22"
        },
        {
          "id": "MDRW2100046101.1.10",
          "form": "오 음식 조절중이구나",
          "original_form": "오 음식 조절중이구나",
          "speaker_id": "1",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046101.1.11",
          "form": "그래서 설탕을줄이고있어",
          "original_form": "그래서 설탕을줄이고있어",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100046101.1.12",
          "form": "넌 설탕좋아해?",
          "original_form": "넌 설탕좋아해?",
          "speaker_id": "2",
          "time": "20210908 10:25"
        },
        {
          "id": "MDRW2100046101.1.13",
          "form": "안그래돔줄이려고",
          "original_form": "안그래돔줄이려고",
