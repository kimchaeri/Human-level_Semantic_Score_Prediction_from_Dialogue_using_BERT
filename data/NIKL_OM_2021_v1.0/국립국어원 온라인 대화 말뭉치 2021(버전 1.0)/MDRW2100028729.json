      "utterance": [
        {
          "id": "MDRW2100028729.1.1",
          "form": "팔뚝 운동 하고있지?",
          "original_form": "팔뚝 운동 하고있지?",
          "speaker_id": "1",
          "time": "20210902 21:38"
        },
        {
          "id": "MDRW2100028729.1.2",
          "form": "아니 요즘 운동 안 하고",
          "original_form": "아니 요즘 운동 안 하고",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028729.1.3",
          "form": "식이조절만 함",
          "original_form": "식이조절만 함",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028729.1.4",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100028729.1.5",
          "form": "난 운동하고 식이를 안해 점심에 양껏 먹어",
          "original_form": "난 운동하고 식이를 안해 점심에 양껏 먹어",
          "speaker_id": "1",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100028729.1.6",
          "form": "나는 점심은 간단하게 하고 저녁을 잘 먹는 중이야",
          "original_form": "나는 점심은 간단하게 하고 저녁을 잘 먹는 중이야",
          "speaker_id": "2",
          "time": "20210903 10:12"
        },
        {
          "id": "MDRW2100028729.1.7",
          "form": "반대가되면 좋을텐데",
          "original_form": "반대가되면 좋을텐데",
          "speaker_id": "1",
          "time": "20210903 10:16"
        },
        {
          "id": "MDRW2100028729.1.8",
          "form": "배고프면 잠이 안 와",
          "original_form": "배고프면 잠이 안 와",
          "speaker_id": "2",
          "time": "20210903 12:09"
        },
        {
          "id": "MDRW2100028729.1.9",
          "form": "운동하니?",
          "original_form": "운동하니?",
          "speaker_id": "2",
          "time": "20210903 12:09"
        },
        {
          "id": "MDRW2100028729.1.10",
          "form": "요즘 헬스장 가서 gx랑 요가해",
          "original_form": "요즘 헬스장 가서 gx랑 요가해",
          "speaker_id": "1",
          "time": "20210903 13:09"
        },
        {
          "id": "MDRW2100028729.1.11",
          "form": "요가는 하기로 했어?",
          "original_form": "요가는 하기로 했어?",
          "speaker_id": "1",
          "time": "20210903 13:09"
        },
        {
          "id": "MDRW2100028729.1.12",
          "form": "돈 없어서 홈트하려고",
          "original_form": "돈 없어서 홈트하려고",
          "speaker_id": "2",
          "time": "20210903 13:10"
        },
        {
          "id": "MDRW2100028729.1.13",
          "form": "넌 얼마 주고 요가해",
          "original_form": "넌 얼마 주고 요가해",
          "speaker_id": "2",
          "time": "20210903 13:10"
        },
        {
          "id": "MDRW2100028729.1.14",
          "form": "난 1년 헬스권에 요가랑  gx포함으로 60만원",
          "original_form": "난 1년 헬스권에 요가랑  gx포함으로 60만원",
          "speaker_id": "1",
          "time": "20210903 13:11"
        },
        {
          "id": "MDRW2100028729.1.15",
          "form": "요가전문은 아니야 근데 할만해",
          "original_form": "요가전문은 아니야 근데 할만해",
          "speaker_id": "1",
          "time": "20210903 13:11"
        },
        {
          "id": "MDRW2100028729.1.16",
          "form": "저렴하다",
          "original_form": "저렴하다",
          "speaker_id": "2",
          "time": "20210903 13:11"
        },
        {
          "id": "MDRW2100028729.1.17",
          "form": "맞아 이제 헬스장 가격 다 공개된거 아니?",
          "original_form": "맞아 이제 헬스장 가격 다 공개된거 아니?",
          "speaker_id": "1",
          "time": "20210903 13:11"
        },
        {
          "id": "MDRW2100028729.1.18",
          "form": "전화랑 방문으로 안물어봐도 된다",
          "original_form": "전화랑 방문으로 안물어봐도 된다",
          "speaker_id": "1",
          "time": "20210903 13:11"
        },
        {
          "id": "MDRW2100028729.1.19",
          "form": "좋다",
          "original_form": "좋다",
          "speaker_id": "2",
          "time": "20210903 13:12"
        },
        {
          "id": "MDRW2100028729.1.20",
          "form": "헬스 너무 비싸",
          "original_form": "헬스 너무 비싸",
          "speaker_id": "2",
          "time": "20210903 13:12"
        },
        {
          "id": "MDRW2100028729.1.21",
          "form": "응 PT는 받아볼생각 없니",
          "original_form": "응 PT는 받아볼생각 없니",
          "speaker_id": "1",
          "time": "20210903 13:38"
        },
        {
