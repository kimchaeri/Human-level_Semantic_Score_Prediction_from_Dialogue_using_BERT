      "utterance": [
        {
          "id": "MDRW2100000967.1.1",
          "form": "콘서트같은데 가본적있으신가요?",
          "original_form": "콘서트같은데 가본적있으신가요?",
          "speaker_id": "1",
          "time": "20210819 20:57"
        },
        {
          "id": "MDRW2100000967.1.2",
          "form": "앤마리 콘서트가 마지막이었어요!",
          "original_form": "앤마리 콘서트가 마지막이었어요!",
          "speaker_id": "2",
          "time": "20210819 20:58"
        },
        {
          "id": "MDRW2100000967.1.3",
          "form": "오 어떠셨어요?",
          "original_form": "오 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210819 21:00"
        },
        {
          "id": "MDRW2100000967.1.4",
          "form": "너무너무 좋았어요.. 앤마리 노래 아시나요?",
          "original_form": "너무너무 좋았어요.. 앤마리 노래 아시나요?",
          "speaker_id": "2",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100000967.1.5",
          "form": "라이브를 정말 잘해요",
          "original_form": "라이브를 정말 잘해요",
          "speaker_id": "2",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100000967.1.6",
          "form": "내한올때 가샸던건가요?",
          "original_form": "내한올때 가샸던건가요?",
          "speaker_id": "1",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000967.1.7",
          "form": "잘은모르는데 들어는본것같아요",
          "original_form": "잘은모르는데 들어는본것같아요",
          "speaker_id": "1",
          "time": "20210819 21:03"
        },
        {
          "id": "MDRW2100000967.1.8",
          "form": "네! 내한이었어요.. 티켓팅이 힘들었지만",
          "original_form": "네! 내한이었어요.. 티켓팅이 힘들었지만",
          "speaker_id": "2",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000967.1.9",
          "form": "너무 만족스러웠어요",
          "original_form": "너무 만족스러웠어요",
          "speaker_id": "2",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000967.1.10",
          "form": "2002 노래.. 들어는 보셨을거예요 ㅎㅎ",
          "original_form": "2002 노래.. 들어는 보셨을거예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000967.1.11",
          "form": "전 외국 가수를 좋아해서",
          "original_form": "전 외국 가수를 좋아해서",
          "speaker_id": "2",
          "time": "20210819 21:04"
        },
        {
          "id": "MDRW2100000967.1.12",
          "form": "당분간.. 은 콘서트 못가겠죠...",
          "original_form": "당분간.. 은 콘서트 못가겠죠...",
          "speaker_id": "2",
          "time": "20210819 21:05"
        },
        {
          "id": "MDRW2100000967.1.13",
          "form": "ㅜㅜ네에ㅜㅜ넘아쉬운 외국가수면 특히 더 힘드니까여",
          "original_form": "ㅜㅜ네에ㅜㅜ넘아쉬운 외국가수면 특히 더 힘드니까여",
          "speaker_id": "1",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100000967.1.14",
          "form": "전 자우림콘 갔다왔는데 넘좋더라구요",
          "original_form": "전 자우림콘 갔다왔는데 넘좋더라구요",
          "speaker_id": "1",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100000967.1.15",
          "form": "코로나시국에 취소됐던거 다시열린콘이었는데",
          "original_form": "코로나시국에 취소됐던거 다시열린콘이었는데",
          "speaker_id": "1",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100000967.1.16",
          "form": "클래식연주회느낌으로 감상한기분이었는데도 감동받고 넘좋았어요",
          "original_form": "클래식연주회느낌으로 감상한기분이었는데도 감동받고 넘좋았어요",
          "speaker_id": "1",
          "time": "20210819 21:06"
        },
        {
          "id": "MDRW2100000967.1.17",
          "form": "박수만 칠수있지만 손에불나도록 친것같아요",
          "original_form": "박수만 칠수있지만 손에불나도록 친것같아요",
          "speaker_id": "1",
          "time": "20210819 21:07"
        },
        {
          "id": "MDRW2100000967.1.18",
          "form": "어머 대박!! 저 자우림 너무 좋아해요",
          "original_form": "어머 대박!! 저 자우림 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100000967.1.19",
          "form": "지방살아서.. 그때 못가서",
          "original_form": "지방살아서.. 그때 못가서",
          "speaker_id": "2",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100000967.1.20",
          "form": "너무 아쉬웠어요",
          "original_form": "너무 아쉬웠어요",
          "speaker_id": "2",
          "time": "20210819 21:10"
        },
        {
          "id": "MDRW2100000967.1.21",
          "form": "윤아님 첫소절부르실때 눈물좔좔 감동좔좔",
          "original_form": "윤아님 첫소절부르실때 눈물좔좔 감동좔좔",
          "speaker_id": "1",
          "time": "20210819 21:11"
        },
        {
          "id": "MDRW2100000967.1.22",
          "form": "저도 지방사는데 이번콘 더 의미있는것같아서 무리하게갔다왔었오요ㅠ",
          "original_form": "저도 지방사는데 이번콘 더 의미있는것같아서 무리하게갔다왔었오요ㅠ",
          "speaker_id": "1",
          "time": "20210819 21:12"
        },
        {
          "id": "MDRW2100000967.1.23",
          "form": "와....ㅠ 진짜 부러워요 자우림콘.. 상상만해도 소름돋아요",
          "original_form": "와....ㅠ 진짜 부러워요 자우림콘.. 상상만해도 소름돋아요",
          "speaker_id": "2",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100000967.1.24",
          "form": "코로나때문에 공연 자체도 그렇고",
          "original_form": "코로나때문에 공연 자체도 그렇고",
          "speaker_id": "2",
          "time": "20210819 23:12"
        },
        {
          "id": "MDRW2100000967.1.25",
          "form": "타지 이동이 힘들어지니... 콘서트가 더 그립네요",
          "original_form": "타지 이동이 힘들어지니... 콘서트가 더 그립네요",
          "speaker_id": "2",
          "time": "20210819 23:13"
        },
        {
          "id": "MDRW2100000967.1.26",
          "form": "막 락페스티발도 가보고싶고..",
          "original_form": "막 락페스티발도 가보고싶고..",
          "speaker_id": "1",
          "time": "20210819 23:16"
        },
        {
          "id": "MDRW2100000967.1.27",
          "form": "소규모 공연장도 가보고싶네요",
          "original_form": "소규모 공연장도 가보고싶네요",
          "speaker_id": "1",
          "time": "20210819 23:17"
        },
        {
          "id": "MDRW2100000967.1.28",
          "form": "락페 좋죠ㅠㅠㅠ 부산 락페.. 무료에다 규모도 커서 너무 좋은데..",
          "original_form": "락페 좋죠ㅠㅠㅠ 부산 락페.. 무료에다 규모도 커서 너무 좋은데..",
          "speaker_id": "2",
          "time": "20210819 23:19"
        },
        {
          "id": "MDRW2100000967.1.29",
          "form": "코로나 얼른 사라졌으면 좋겠어요",
          "original_form": "코로나 얼른 사라졌으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210819 23:19"
        },
        {
          "id": "MDRW2100000967.1.30",
          "form": "재작년인가..",
          "original_form": "재작년인가..",
          "speaker_id": "2",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100000967.1.31",
          "form": "플라워 공연보러 작은 공연장갔었는데 좋았어요",
          "original_form": "플라워 공연보러 작은 공연장갔었는데 좋았어요",
          "speaker_id": "2",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100000967.1.32",
          "form": "코앞에서보고.. 도란도란 대화하는 분위기 ㅋㅋ",
          "original_form": "코앞에서보고.. 도란도란 대화하는 분위기 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 23:20"
        },
        {
          "id": "MDRW2100000967.1.33",
          "form": "흑흑진짜 가보고싶은마음만 커지네요",
          "original_form": "흑흑진짜 가보고싶은마음만 커지네요",
          "speaker_id": "1",
          "time": "20210819 23:20"
        }
      ]
    }
