      "utterance": [
        {
          "id": "MDRW2100009536.1.1",
          "form": "오 추억이 샘솟네요",
          "original_form": "오 추억이 샘솟네요",
          "speaker_id": "2",
          "time": "20210823 00:20"
        },
        {
          "id": "MDRW2100009536.1.2",
          "form": "예전엔 일기도 썼죠",
          "original_form": "예전엔 일기도 썼죠",
          "speaker_id": "2",
          "time": "20210823 00:20"
        },
        {
          "id": "MDRW2100009536.1.3",
          "form": "싸이에 많이",
          "original_form": "싸이에 많이",
          "speaker_id": "2",
          "time": "20210823 00:20"
        },
        {
          "id": "MDRW2100009536.1.4",
          "form": "name2님도 싸이월드 세대이시군용",
          "original_form": "&name2&님도 싸이월드 세대이시군용",
          "speaker_id": "1",
          "time": "20210823 00:27"
        },
        {
          "id": "MDRW2100009536.1.5",
          "form": "이번에 계정 복구 신청하셨나요?",
          "original_form": "이번에 계정 복구 신청하셨나요?",
          "speaker_id": "1",
          "time": "20210823 00:27"
        },
        {
          "id": "MDRW2100009536.1.6",
          "form": "음 아니요 거기까진 안 생각해봤어요",
          "original_form": "음 아니요 거기까진 안 생각해봤어요",
          "speaker_id": "2",
          "time": "20210823 00:30"
        },
        {
          "id": "MDRW2100009536.1.7",
          "form": "그럼 옛날글들도 볼수있는건가요",
          "original_form": "그럼 옛날글들도 볼수있는건가요",
          "speaker_id": "2",
          "time": "20210823 00:30"
        },
        {
          "id": "MDRW2100009536.1.8",
          "form": "저는 폰번호가 변경되서\r\n복구신청을 못해서\r\n정확히는 모르겠는데\r\n앨범이랑 다이어리는\r\n볼 수 있는거 같더라구용",
          "original_form": "저는 폰번호가 변경되서\r\n복구신청을 못해서\r\n정확히는 모르겠는데\r\n앨범이랑 다이어리는\r\n볼 수 있는거 같더라구용",
          "speaker_id": "1",
          "time": "20210823 00:42"
        },
        {
          "id": "MDRW2100009536.1.9",
          "form": "와 대박 흑역사일지도요",
          "original_form": "와 대박 흑역사일지도요",
          "speaker_id": "2",
          "time": "20210823 00:45"
        },
        {
          "id": "MDRW2100009536.1.10",
          "form": "ㅋㅋㅋ암요 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ암요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:46"
        },
        {
          "id": "MDRW2100009536.1.11",
          "form": "지금은 나쁜놈인 전남친과의 꽁냥꽁냥이나 ㅋㅋ",
          "original_form": "지금은 나쁜놈인 전남친과의 꽁냥꽁냥이나 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 00:46"
        },
        {
          "id": "MDRW2100009536.1.12",
          "form": "저는 왠지 아직 열어보진\r\n않았지만 이불킥감ㅋㅋㅋㅋ",
          "original_form": "저는 왠지 아직 열어보진\r\n않았지만 이불킥감ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:46"
        },
        {
          "id": "MDRW2100009536.1.13",
          "form": "나눈 눈물을 흘륀다 같은",
          "original_form": "나눈 눈물을 흘륀다 같은",
          "speaker_id": "2",
          "time": "20210823 00:46"
        },
        {
          "id": "MDRW2100009536.1.14",
          "form": "10대감성 튀어나올까 무섭네요",
          "original_form": "10대감성 튀어나올까 무섭네요",
          "speaker_id": "2",
          "time": "20210823 00:46"
        },
        {
          "id": "MDRW2100009536.1.15",
          "form": "나는 가끔씩 눈물을 흘린다ㅋㅋㅋㅋ",
          "original_form": "나는 가끔씩 눈물을 흘린다ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:47"
        },
        {
          "id": "MDRW2100009536.1.16",
          "form": "나는 이런 내가 좋다",
          "original_form": "나는 이런 내가 좋다",
          "speaker_id": "1",
          "time": "20210823 00:47"
        },
        {
          "id": "MDRW2100009536.1.17",
          "form": "이런거요?ㅋㅋㅋㅋㅋㅋ",
          "original_form": "이런거요?ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:47"
        },
        {
          "id": "MDRW2100009536.1.18",
          "form": "왜 한때 그런글들이 넘쳐났었죠",
          "original_form": "왜 한때 그런글들이 넘쳐났었죠",
          "speaker_id": "2",
          "time": "20210823 00:47"
        },
        {
          "id": "MDRW2100009536.1.19",
          "form": "갬성글들ㅋㅋ",
          "original_form": "갬성글들ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009536.1.20",
          "form": "감수성 진짜 풍부했더라는",
          "original_form": "감수성 진짜 풍부했더라는",
          "speaker_id": "2",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009536.1.21",
          "form": "그땐 그때의 갬성인데",
          "original_form": "그땐 그때의 갬성인데",
          "speaker_id": "1",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009536.1.22",
          "form": "문제는 넘 오래되서 나도 내가 뭘썼나 기억이 ㅋ",
          "original_form": "문제는 넘 오래되서 나도 내가 뭘썼나 기억이 ㅋ",
          "speaker_id": "2",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009536.1.23",
          "form": "그러게요ㅋㅋㅋㅋ",
          "original_form": "그러게요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009536.1.24",
          "form": "얼마전에 학교 다닐때\r\n친구들이 써 준 손편지들\r\n읽어봤는데ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "얼마전에 학교 다닐때\r\n친구들이 써 준 손편지들\r\n읽어봤는데ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:48"
        },
        {
          "id": "MDRW2100009536.1.25",
          "form": "얘네를 보니\r\n그때 제가 어땠을지\r\n어느정도 가늠이 되는ㅋㅋㅋ",
          "original_form": "얘네를 보니\r\n그때 제가 어땠을지\r\n어느정도 가늠이 되는ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 00:49"
        }
      ]
    }
