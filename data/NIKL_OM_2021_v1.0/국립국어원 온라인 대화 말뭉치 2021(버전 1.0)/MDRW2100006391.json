      "utterance": [
        {
          "id": "MDRW2100006391.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100006391.1.2",
          "form": "안녕하게요",
          "original_form": "안녕하게요",
          "speaker_id": "2",
          "time": "20210907 23:36"
        },
        {
          "id": "MDRW2100006391.1.3",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100006391.1.4",
          "form": "가끔 해요!",
          "original_form": "가끔 해요!",
          "speaker_id": "2",
          "time": "20210907 23:37"
        },
        {
          "id": "MDRW2100006391.1.5",
          "form": "게임 좋아하시나요?",
          "original_form": "게임 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100006391.1.6",
          "form": "예전에는 많이 했는데 요즘은 저도 가끔 해요",
          "original_form": "예전에는 많이 했는데 요즘은 저도 가끔 해요",
          "speaker_id": "1",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100006391.1.7",
          "form": "어떤거 하시나요?",
          "original_form": "어떤거 하시나요?",
          "speaker_id": "2",
          "time": "20210907 23:39"
        },
        {
          "id": "MDRW2100006391.1.8",
          "form": "콘솔 게임도 하고 모바일 게임도 해요",
          "original_form": "콘솔 게임도 하고 모바일 게임도 해요",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100006391.1.9",
          "form": "모바일 최근에 하신거 있나요?",
          "original_form": "모바일 최근에 하신거 있나요?",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100006391.1.10",
          "form": "꿈의 정원 가끔씩 해요",
          "original_form": "꿈의 정원 가끔씩 해요",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100006391.1.11",
          "form": "name2은 최근 머 해보셨어요?",
          "original_form": "&name2&은 최근 머 해보셨어요?",
          "speaker_id": "1",
          "time": "20210907 23:42"
        },
        {
          "id": "MDRW2100006391.1.12",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100006391.1.13",
          "form": "전 검은사막 했었어요!",
          "original_form": "전 검은사막 했었어요!",
          "speaker_id": "2",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100006391.1.14",
          "form": "오 지금은 안하세요?",
          "original_form": "오 지금은 안하세요?",
          "speaker_id": "1",
          "time": "20210907 23:43"
        },
        {
          "id": "MDRW2100006391.1.15",
          "form": "넵 ㅠ",
          "original_form": "넵 ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100006391.1.16",
          "form": "이런 류 겜들이 돈을 엄청 쓰지 않는 이성",
          "original_form": "이런 류 겜들이 돈을 엄청 쓰지 않는 이성",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100006391.1.17",
          "form": "이상",
          "original_form": "이상",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100006391.1.18",
          "form": "초반에만 재밌고 갈수록 진도가 너무 더디더라구요",
          "original_form": "초반에만 재밌고 갈수록 진도가 너무 더디더라구요",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100006391.1.19",
          "form": "아 글쿤요",
          "original_form": "아 글쿤요",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100006391.1.20",
          "form": "저도 요즘 나오는 게임들은 다 너무 하기가 무거워서 저도 손이 안가더라구요 ㅠ",
          "original_form": "저도 요즘 나오는 게임들은 다 너무 하기가 무거워서 저도 손이 안가더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100006391.1.21",
          "form": "그쵸... 다들 각잡고 해야해서...",
          "original_form": "그쵸... 다들 각잡고 해야해서...",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100006391.1.22",
          "form": "좀 가볍고 아기자기한걸 하고싶은데",
          "original_form": "좀 가볍고 아기자기한걸 하고싶은데",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100006391.1.23",
          "form": "그런게 드물어요...",
          "original_form": "그런게 드물어요...",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100006391.1.24",
          "form": "그런 건 돈이 안되나봐요 ㅠ",
          "original_form": "그런 건 돈이 안되나봐요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100006391.1.25",
          "form": "그렇겠죠 ㅠ 제가 생각해도...ㅋㅋㅋㅋ ㅠ",
          "original_form": "그렇겠죠 ㅠ 제가 생각해도...ㅋㅋㅋㅋ ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100006391.1.26",
          "form": "인디게임들이 그래서 요즘 더 참신한 듯 ㅎ",
          "original_form": "인디게임들이 그래서 요즘 더 참신한 듯 ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:48"
        }
      ]
