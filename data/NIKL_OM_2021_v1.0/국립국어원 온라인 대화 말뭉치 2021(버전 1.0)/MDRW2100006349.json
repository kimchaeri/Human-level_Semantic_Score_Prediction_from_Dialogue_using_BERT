      "utterance": [
        {
          "id": "MDRW2100006349.1.1",
          "form": "핸드폰 어떤거쓰세요?",
          "original_form": "핸드폰 어떤거쓰세요?",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100006349.1.2",
          "form": "플립 넘 이쁜거같아요.",
          "original_form": "플립 넘 이쁜거같아요.",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100006349.1.3",
          "form": "전 s10",
          "original_form": "전 s10",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100006349.1.4",
          "form": "이번에 z플립 핫하던데",
          "original_form": "이번에 z플립 핫하던데",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100006349.1.5",
          "form": "저도 폰바꾸고싶네요",
          "original_form": "저도 폰바꾸고싶네요",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100006349.1.6",
          "form": "넘 이뻐요",
          "original_form": "넘 이뻐요",
          "speaker_id": "2",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100006349.1.7",
          "form": "플립이 작은거죠?",
          "original_form": "플립이 작은거죠?",
          "speaker_id": "1",
          "time": "20210907 23:02"
        },
        {
          "id": "MDRW2100006349.1.8",
          "form": "폴드가 큰거고",
          "original_form": "폴드가 큰거고",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006349.1.9",
          "form": "예전꺼보단 덜한데   잘깨진데요.",
          "original_form": "예전꺼보단 덜한데   잘깨진데요.",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006349.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006349.1.11",
          "form": "위아래로 접히는거",
          "original_form": "위아래로 접히는거",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006349.1.12",
          "form": "아 그래요? 넘비싸서 엄두가안나요",
          "original_form": "아 그래요? 넘비싸서 엄두가안나요",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006349.1.13",
          "form": "겉에 넘 이뻐요.",
          "original_form": "겉에 넘 이뻐요.",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006349.1.14",
          "form": "회사 대리 플립으로 바꿨던데",
          "original_form": "회사 대리 플립으로 바꿨던데",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006349.1.15",
          "form": "비싸긴하죠~~",
          "original_form": "비싸긴하죠~~",
          "speaker_id": "2",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006349.1.16",
          "form": "구경하러 매장함 가봐야겠네요",
          "original_form": "구경하러 매장함 가봐야겠네요",
          "speaker_id": "1",
          "time": "20210907 23:03"
        },
        {
          "id": "MDRW2100006349.1.17",
          "form": "저도 구경만하고왔어요,",
          "original_form": "저도 구경만하고왔어요,",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100006349.1.18",
          "form": "아직 제대로 못봤어요 광고로만보고",
          "original_form": "아직 제대로 못봤어요 광고로만보고",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100006349.1.19",
          "form": "사은품도 지금사면 많이줄듯한데",
          "original_form": "사은품도 지금사면 많이줄듯한데",
          "speaker_id": "1",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100006349.1.20",
          "form": "저도 실물은 못봤어요",
          "original_form": "저도 실물은 못봤어요",
          "speaker_id": "2",
          "time": "20210907 23:04"
        },
        {
          "id": "MDRW2100006349.1.21",
          "form": "사전예약할때보니 버즈도 준거같은데...반값인지 공짠지 그렇더라구요",
          "original_form": "사전예약할때보니 버즈도 준거같은데...반값인지 공짠지 그렇더라구요",
