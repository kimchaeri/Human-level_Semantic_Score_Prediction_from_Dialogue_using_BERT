      "utterance": [
        {
          "id": "MDRW2100028454.1.1",
          "form": "너는 온라인 피트니스 서비스가 뭔지 아니?",
          "original_form": "너는 온라인 피트니스 서비스가 뭔지 아니?",
          "speaker_id": "1",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100028454.1.2",
          "form": "유튜브같응거 보면서 운동하는거 아닐까",
          "original_form": "유튜브같응거 보면서 운동하는거 아닐까",
          "speaker_id": "2",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028454.1.3",
          "form": "그렇구나 홈트레이닝이네",
          "original_form": "그렇구나 홈트레이닝이네",
          "speaker_id": "1",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028454.1.4",
          "form": "워치에 헬스앱 같은 거?",
          "original_form": "워치에 헬스앱 같은 거?",
          "speaker_id": "2",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028454.1.5",
          "form": "그게 온라인인가?",
          "original_form": "그게 온라인인가?",
          "speaker_id": "1",
          "time": "20210902 20:08"
        },
        {
          "id": "MDRW2100028454.1.6",
          "form": "그건 온라인 아니니? 안써봐서 모르겠다",
          "original_form": "그건 온라인 아니니? 안써봐서 모르겠다",
          "speaker_id": "2",
          "time": "20210902 20:10"
        },
        {
          "id": "MDRW2100028454.1.7",
          "form": "나도 안 써봐서 모르겠오",
          "original_form": "나도 안 써봐서 모르겠오",
          "speaker_id": "1",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100028454.1.8",
          "form": "온라인",
          "original_form": "온라인",
          "speaker_id": "1",
          "time": "20210902 20:11"
        },
        {
          "id": "MDRW2100028454.1.9",
          "form": "그럼 유튜브를 열심히 보자",
          "original_form": "그럼 유튜브를 열심히 보자",
          "speaker_id": "2",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028454.1.10",
          "form": "좋아 온라인 트레이닝 뭐해?",
          "original_form": "좋아 온라인 트레이닝 뭐해?",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028454.1.11",
          "form": "난 유튜브 말고 없어",
          "original_form": "난 유튜브 말고 없어",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028454.1.12",
          "form": "넌 하는거 있니?",
          "original_form": "넌 하는거 있니?",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028454.1.13",
          "form": "나도 유투브로 트레이닝해",
          "original_form": "나도 유투브로 트레이닝해",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028454.1.14",
          "form": "운동 열심히 하자",
          "original_form": "운동 열심히 하자",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028454.1.15",
          "form": "구래",
          "original_form": "구래",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028454.1.16",
          "form": "온라인 피트니스에 대해서 아는게.없어서 대화가 안되는 거 같응디",
          "original_form": "온라인 피트니스에 대해서 아는게.없어서 대화가 안되는 거 같응디",
