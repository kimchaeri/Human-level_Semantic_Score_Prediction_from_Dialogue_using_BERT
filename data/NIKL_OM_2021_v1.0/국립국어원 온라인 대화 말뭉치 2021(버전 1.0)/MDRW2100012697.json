      "utterance": [
        {
          "id": "MDRW2100012697.1.1",
          "form": "아 그 뭐였지 하튼 풀스 재밌습니다",
          "original_form": "아 그 뭐였지 하튼 풀스 재밌습니다",
          "speaker_id": "2",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100012697.1.2",
          "form": "그...",
          "original_form": "그...",
          "speaker_id": "2",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100012697.1.3",
          "form": "나모 님이신가?",
          "original_form": "나모 님이신가?",
          "speaker_id": "1",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100012697.1.4",
          "form": "모나?",
          "original_form": "모나?",
          "speaker_id": "1",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100012697.1.5",
          "form": "럽오헤는 좀 불호가 있긴한데 풀스는 거의 아",
          "original_form": "럽오헤는 좀 불호가 있긴한데 풀스는 거의 아",
          "speaker_id": "2",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100012697.1.6",
          "form": "니모님",
          "original_form": "니모님",
          "speaker_id": "2",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100012697.1.7",
          "form": "그분 그림 진짜 이쁘지안ㄹ나요",
          "original_form": "그분 그림 진짜 이쁘지안ㄹ나요",
          "speaker_id": "1",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100012697.1.8",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100012697.1.9",
          "form": "하 저 진짜 너무 젛아했거든요",
          "original_form": "하 저 진짜 너무 젛아했거든요",
          "speaker_id": "2",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100012697.1.10",
          "form": "포타...전부구매했는데...",
          "original_form": "포타...전부구매했는데...",
          "speaker_id": "2",
          "time": "20210825 00:01"
        },
        {
          "id": "MDRW2100012697.1.11",
          "form": "카게히나 싹 날라가버린걸 보며....",
          "original_form": "카게히나 싹 날라가버린걸 보며....",
          "speaker_id": "2",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100012697.1.12",
          "form": "하...진짜....피눈물 흘렸답니다",
          "original_form": "하...진짜....피눈물 흘렸답니다",
          "speaker_id": "2",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100012697.1.13",
          "form": "네?!?!!??!!?",
          "original_form": "네?!?!!??!!?",
          "speaker_id": "1",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012697.1.14",
          "form": "포타 그거 작가님이 지우며누못보나요?!!",
          "original_form": "포타 그거 작가님이 지우며누못보나요?!!",
          "speaker_id": "1",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012697.1.15",
          "form": "아나",
          "original_form": "아나",
          "speaker_id": "1",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012697.1.16",
          "form": "오마이갓오마이갓",
          "original_form": "오마이갓오마이갓",
          "speaker_id": "1",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012697.1.17",
          "form": "하 네...",
          "original_form": "하 네...",
          "speaker_id": "2",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012697.1.18",
          "form": "오마이갓",
          "original_form": "오마이갓",
          "speaker_id": "1",
          "time": "20210825 00:07"
        },
        {
          "id": "MDRW2100012697.1.19",
          "form": "님 그거 모르시는구나 작가님이 설정해놔야 볼 수 있는데",
          "original_form": "님 그거 모르시는구나 작가님이 설정해놔야 볼 수 있는데",
          "speaker_id": "2",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012697.1.20",
          "form": "그럼 구매하는 의미가 읍잔아요",
          "original_form": "그럼 구매하는 의미가 읍잔아요",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012697.1.21",
          "form": "아니 소장하게해줘",
          "original_form": "아니 소장하게해줘",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012697.1.22",
          "form": "책으로 내줘",
          "original_form": "책으로 내줘",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012697.1.23",
          "form": "그걸 영구 삭제해버리면 절대 못보게 되어있어요",
          "original_form": "그걸 영구 삭제해버리면 절대 못보게 되어있어요",
          "speaker_id": "2",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012697.1.24",
          "form": "너무해",
          "original_form": "너무해",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012697.1.25",
          "form": "저는 진짜 다른건 몰라도 카게히나 그 낙서모음집 너무 좋아했거든요",
          "original_form": "저는 진짜 다른건 몰라도 카게히나 그 낙서모음집 너무 좋아했거든요",
          "speaker_id": "2",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012697.1.26",
          "form": "근데 재록본이라도...내주지....",
          "original_form": "근데 재록본이라도...내주지....",
          "speaker_id": "2",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012697.1.27",
          "form": "맞아요 낙서",
          "original_form": "맞아요 낙서",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012697.1.28",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012697.1.29",
          "form": "울었다고요 어느순간...날아가버린....",
          "original_form": "울었다고요 어느순간...날아가버린....",
          "speaker_id": "2",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012697.1.30",
          "form": "님한텐 부끄러운 과거일 수 있어요",
          "original_form": "님한텐 부끄러운 과거일 수 있어요",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012697.1.31",
          "form": "남에겐 행복이랍니다",
          "original_form": "남에겐 행복이랍니다",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100012697.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012697.1.33",
          "form": "엉엉엉",
          "original_form": "엉엉엉",
          "speaker_id": "1",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012697.1.34",
          "form": "님 진짜 포타 사놓은 거 자주 보세요",
          "original_form": "님 진짜 포타 사놓은 거 자주 보세요",
          "speaker_id": "2",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012697.1.35",
          "form": "저...오랜만에 ㅎㅎ 봐야지 했다가",
          "original_form": "저...오랜만에 ㅎㅎ 봐야지 했다가",
          "speaker_id": "2",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012697.1.36",
          "form": "삭 증발한거보고 안돼....안돼...",
          "original_form": "삭 증발한거보고 안돼....안돼...",
          "speaker_id": "2",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012697.1.37",
          "form": "전 회지를 샀어요",
          "original_form": "전 회지를 샀어요",
          "speaker_id": "1",
          "time": "20210825 00:09"
        },
        {
          "id": "MDRW2100012697.1.38",
          "form": "안돼!!!!!!!!!!!!!",
          "original_form": "안돼!!!!!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210825 00:09"
        },
