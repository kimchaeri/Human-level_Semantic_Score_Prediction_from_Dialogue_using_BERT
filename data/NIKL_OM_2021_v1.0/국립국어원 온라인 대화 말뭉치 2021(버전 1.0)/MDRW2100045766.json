      "utterance": [
        {
          "id": "MDRW2100045766.1.1",
          "form": "전 샌드위치요!!",
          "original_form": "전 샌드위치요!!",
          "speaker_id": "2",
          "time": "20210908 06:50"
        },
        {
          "id": "MDRW2100045766.1.2",
          "form": "name1님은 생각해두신거 있나요?",
          "original_form": "&name1&님은 생각해두신거 있나요?",
          "speaker_id": "2",
          "time": "20210908 06:51"
        },
        {
          "id": "MDRW2100045766.1.3",
          "form": "저는 시리얼 먹으려구요!",
          "original_form": "저는 시리얼 먹으려구요!",
          "speaker_id": "1",
          "time": "20210908 06:51"
        },
        {
          "id": "MDRW2100045766.1.4",
          "form": "오 샌드위치 너무 맛있을 거 같네요",
          "original_form": "오 샌드위치 너무 맛있을 거 같네요",
          "speaker_id": "1",
          "time": "20210908 06:51"
        },
        {
          "id": "MDRW2100045766.1.5",
          "form": "안에 뭐 넣을지 고민이에요!",
          "original_form": "안에 뭐 넣을지 고민이에요!",
          "speaker_id": "2",
          "time": "20210908 06:53"
        },
        {
          "id": "MDRW2100045766.1.6",
          "form": "격렬하게 고민중이에요",
          "original_form": "격렬하게 고민중이에요",
          "speaker_id": "2",
          "time": "20210908 06:53"
        },
        {
          "id": "MDRW2100045766.1.7",
          "form": "닭찌와 닭안심과 소고기중에",
          "original_form": "닭찌와 닭안심과 소고기중에",
          "speaker_id": "2",
          "time": "20210908 06:53"
        },
        {
          "id": "MDRW2100045766.1.8",
          "form": "음...소고기 한 표요!",
          "original_form": "음...소고기 한 표요!",
          "speaker_id": "1",
          "time": "20210908 06:54"
        },
        {
          "id": "MDRW2100045766.1.9",
          "form": "소고기 너무 좋잖아요ㅋㅋㅋ",
          "original_form": "소고기 너무 좋잖아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:54"
        },
        {
          "id": "MDRW2100045766.1.10",
          "form": "근데 해동해야해욬ㅋㅋㅋ",
          "original_form": "근데 해동해야해욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:58"
        },
        {
          "id": "MDRW2100045766.1.11",
          "form": "답정너였네요 안심은 구워둔게 있어섴ㅋㅋㅋㅋ",
          "original_form": "답정너였네요 안심은 구워둔게 있어섴ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 06:59"
        },
        {
          "id": "MDRW2100045766.1.12",
          "form": "아잇ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아잇ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:59"
        },
        {
          "id": "MDRW2100045766.1.13",
          "form": "결국 닭 드시겠네요ㅋㅋㅋ",
          "original_form": "결국 닭 드시겠네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 06:59"
        },
        {
          "id": "MDRW2100045766.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045766.1.15",
          "form": "닭은 언제나 옳죠",
          "original_form": "닭은 언제나 옳죠",
          "speaker_id": "2",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045766.1.16",
          "form": "소도 옳지만",
          "original_form": "소도 옳지만",
          "speaker_id": "2",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045766.1.17",
          "form": "사무실에서 상사같은 분이 닭 싫어하셔서 평소에 닭을 잘 못먹거든요",
          "original_form": "사무실에서 상사같은 분이 닭 싫어하셔서 평소에 닭을 잘 못먹거든요",
          "speaker_id": "2",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045766.1.18",
          "form": "이럴때 먹어둬야만!",
          "original_form": "이럴때 먹어둬야만!",
          "speaker_id": "2",
          "time": "20210908 07:01"
        },
        {
          "id": "MDRW2100045766.1.19",
          "form": "아아 헐 그러네요",
          "original_form": "아아 헐 그러네요",
          "speaker_id": "1",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045766.1.20",
          "form": "아 그럼 메뉴 선택에 제한이 너무 많겠어요ㅠㅠ",
          "original_form": "아 그럼 메뉴 선택에 제한이 너무 많겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 07:03"
        },
        {
          "id": "MDRW2100045766.1.21",
          "form": "그래도 닭빼고는 다 잘 드세요 다행히ㅋㅋㅋㅋ",
          "original_form": "그래도 닭빼고는 다 잘 드세요 다행히ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100045766.1.22",
          "form": "안계실때 빨리 치킨을 먹어야만",
          "original_form": "안계실때 빨리 치킨을 먹어야만",
          "speaker_id": "2",
          "time": "20210908 07:09"
        },
        {
          "id": "MDRW2100045766.1.23",
          "form": "그래도 닭ㅠㅠㅠㅠㅠ흑ㅠㅠㅠ",
          "original_form": "그래도 닭ㅠㅠㅠㅠㅠ흑ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100045766.1.24",
          "form": "주말에 꼭 치킨드세요!!",
          "original_form": "주말에 꼭 치킨드세요!!",
          "speaker_id": "1",
          "time": "20210908 07:10"
        },
        {
          "id": "MDRW2100045766.1.25",
          "form": "넵 치킨 옳죠!!",
          "original_form": "넵 치킨 옳죠!!",
          "speaker_id": "2",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100045766.1.26",
          "form": "바사삭 치킨까지는 가고싶었는데",
          "original_form": "바사삭 치킨까지는 가고싶었는데",
          "speaker_id": "2",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100045766.1.27",
          "form": "그냥 소소하게 내돈내산 해도 될 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "그냥 소소하게 내돈내산 해도 될 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 07:16"
        },
        {
          "id": "MDRW2100045766.1.28",
          "form": "치킨은 내돈내산도 즐겁죠ㅋㅋㅋ",
          "original_form": "치킨은 내돈내산도 즐겁죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:18"
        },
        {
          "id": "MDRW2100045766.1.29",
          "form": "치킨 어떤 거 좋아하세요?",
          "original_form": "치킨 어떤 거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 07:18"
        },
        {
          "id": "MDRW2100045766.1.30",
          "form": "저는 뿌링클이요!!!",
          "original_form": "저는 뿌링클이요!!!",
          "speaker_id": "2",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100045766.1.31",
          "form": "너무 맛있더라구요ㅠㅠㅠㅠ 그 과자가루같은 맛이",
          "original_form": "너무 맛있더라구요ㅠㅠㅠㅠ 그 과자가루같은 맛이",
          "speaker_id": "2",
          "time": "20210908 07:21"
        },
        {
          "id": "MDRW2100045766.1.32",
          "form": "와 저도 그거 좋아해요ㅠㅠㅠㅠ",
          "original_form": "와 저도 그거 좋아해요ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 07:22"
        },
        {
          "id": "MDRW2100045766.1.33",
          "form": "지코바도 좋아하구요!",
          "original_form": "지코바도 좋아하구요!",
          "speaker_id": "1",
          "time": "20210908 07:22"
        }
      ]
