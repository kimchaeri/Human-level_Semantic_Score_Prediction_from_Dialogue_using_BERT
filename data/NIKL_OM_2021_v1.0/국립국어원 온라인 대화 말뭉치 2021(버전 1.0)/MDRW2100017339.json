      "utterance": [
        {
          "id": "MDRW2100017339.1.1",
          "form": "싸이월드 부활한거",
          "original_form": "싸이월드 부활한거",
          "speaker_id": "1",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017339.1.2",
          "form": "알고있었음?",
          "original_form": "알고있었음?",
          "speaker_id": "1",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017339.1.3",
          "form": "띠용",
          "original_form": "띠용",
          "speaker_id": "2",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017339.1.4",
          "form": "언제 부활했대?",
          "original_form": "언제 부활했대?",
          "speaker_id": "2",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017339.1.5",
          "form": "부활한다는거 인터넷 뉴스로만 보고",
          "original_form": "부활한다는거 인터넷 뉴스로만 보고",
          "speaker_id": "2",
          "time": "20210826 18:47"
        },
        {
          "id": "MDRW2100017339.1.6",
          "form": "그 뒤로 관심을 안가져서 몰랐음 ㅋㅋㅋ",
          "original_form": "그 뒤로 관심을 안가져서 몰랐음 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017339.1.7",
          "form": "얼마안됐을걸",
          "original_form": "얼마안됐을걸",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017339.1.8",
          "form": "논란있었는데 딱히",
          "original_form": "논란있었는데 딱히",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017339.1.9",
          "form": "보고싶진않네..",
          "original_form": "보고싶진않네..",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017339.1.10",
          "form": "흑역사있나",
          "original_form": "흑역사있나",
          "speaker_id": "1",
          "time": "20210826 18:48"
        },
        {
          "id": "MDRW2100017339.1.11",
          "form": "판도라의 상자 아닌가...",
          "original_form": "판도라의 상자 아닌가...",
          "speaker_id": "2",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017339.1.12",
          "form": "굳이 열어보고 싶지 않은데 ㅋㅋㅋㅋ",
          "original_form": "굳이 열어보고 싶지 않은데 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017339.1.13",
          "form": "ㅋㅋㅋㅋ진짜",
          "original_form": "ㅋㅋㅋㅋ진짜",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017339.1.14",
          "form": "알고싶지않은걸 알게될거같은데",
          "original_form": "알고싶지않은걸 알게될거같은데",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017339.1.15",
          "form": "중학교때쯤 열심히했지않나",
          "original_form": "중학교때쯤 열심히했지않나",
          "speaker_id": "1",
          "time": "20210826 18:55"
        },
        {
          "id": "MDRW2100017339.1.16",
          "form": "나는 초등학교때 ㅋㅋㅋ",
          "original_form": "나는 초등학교때 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017339.1.17",
          "form": "중딩때는 아예 안했던거 같은데",
          "original_form": "중딩때는 아예 안했던거 같은데",
          "speaker_id": "2",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017339.1.18",
          "form": "비지엠 뭐였어",
          "original_form": "비지엠 뭐였어",
          "speaker_id": "1",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017339.1.19",
          "form": "나 비지엠 골때렸음",
          "original_form": "나 비지엠 골때렸음",
          "speaker_id": "2",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017339.1.20",
          "form": "왜왜",
          "original_form": "왜왜",
          "speaker_id": "1",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017339.1.21",
          "form": "나 장금이 노래 그 오나라오나라 패러디곡",
          "original_form": "나 장금이 노래 그 오나라오나라 패러디곡",
          "speaker_id": "2",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017339.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ엥",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ엥",
          "speaker_id": "1",
          "time": "20210826 18:59"
        },
        {
          "id": "MDRW2100017339.1.23",
          "form": "패러디는뭐야",
          "original_form": "패러디는뭐야",
          "speaker_id": "1",
          "time": "20210826 19:00"
        },
        {
          "id": "MDRW2100017339.1.24",
          "form": "몰라 기억 잘 안나는데 ㅋㅋㅋ 막 이상하게 개사해서 부르는 그런 노래였음",
          "original_form": "몰라 기억 잘 안나는데 ㅋㅋㅋ 막 이상하게 개사해서 부르는 그런 노래였음",
          "speaker_id": "2",
          "time": "20210826 20:50"
        },
        {
          "id": "MDRW2100017339.1.25",
          "form": "ㅋㅋㅋㅋ궁금하네",
          "original_form": "ㅋㅋㅋㅋ궁금하네",
          "speaker_id": "1",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100017339.1.26",
          "form": "나는 국가대표 오슷이었음",
          "original_form": "나는 국가대표 오슷이었음",
          "speaker_id": "1",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100017339.1.27",
          "form": "오 좋은 노래했잖아",
          "original_form": "오 좋은 노래했잖아",
          "speaker_id": "2",
          "time": "20210826 20:51"
        },
        {
          "id": "MDRW2100017339.1.28",
          "form": "ㅋㅋㅋ그래도 흑역사는 흑역사",
          "original_form": "ㅋㅋㅋ그래도 흑역사는 흑역사",
          "speaker_id": "1",
          "time": "20210826 20:51"
        }
