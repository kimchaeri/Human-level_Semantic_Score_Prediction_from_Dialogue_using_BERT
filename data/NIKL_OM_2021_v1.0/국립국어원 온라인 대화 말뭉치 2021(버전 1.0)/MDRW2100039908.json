      "utterance": [
        {
          "id": "MDRW2100039908.1.1",
          "form": "저장저장해야겠어요",
          "original_form": "저장저장해야겠어요",
          "speaker_id": "2",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100039908.1.2",
          "form": "의사가 가장 좋은 목디스크 치료는 자면서 하는거래요",
          "original_form": "의사가 가장 좋은 목디스크 치료는 자면서 하는거래요",
          "speaker_id": "1",
          "time": "20210903 22:37"
        },
        {
          "id": "MDRW2100039908.1.3",
          "form": "저는 잘때도 몸에 힘이들어가요",
          "original_form": "저는 잘때도 몸에 힘이들어가요",
          "speaker_id": "2",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039908.1.4",
          "form": "일어나면 어깨도 뭉치고 목 뒤도 아프고",
          "original_form": "일어나면 어깨도 뭉치고 목 뒤도 아프고",
          "speaker_id": "2",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039908.1.5",
          "form": "그러다 전 오른팔을 못쓸 정도가 됐었거든요",
          "original_form": "그러다 전 오른팔을 못쓸 정도가 됐었거든요",
          "speaker_id": "1",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039908.1.6",
          "form": "헉 심하셨네요",
          "original_form": "헉 심하셨네요",
          "speaker_id": "2",
          "time": "20210903 22:38"
        },
        {
          "id": "MDRW2100039908.1.7",
          "form": "근데 저 베개 들이고 팔이 차츰 돌아왔어요",
          "original_form": "근데 저 베개 들이고 팔이 차츰 돌아왔어요",
          "speaker_id": "1",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039908.1.8",
          "form": "처음 저 베개에 누으니 목 전체가 스트레칭 되는 시원함이 있었거든요",
          "original_form": "처음 저 베개에 누으니 목 전체가 스트레칭 되는 시원함이 있었거든요",
          "speaker_id": "1",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039908.1.9",
          "form": "그정도로 제 목이 안좋았던거겠죠",
          "original_form": "그정도로 제 목이 안좋았던거겠죠",
          "speaker_id": "1",
          "time": "20210903 22:39"
        },
        {
          "id": "MDRW2100039908.1.10",
          "form": "시가 어른들도 그렇고",
          "original_form": "시가 어른들도 그렇고",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039908.1.11",
          "form": "저도 정착못햤는데 노란베개 사봐야겠어요",
          "original_form": "저도 정착못햤는데 노란베개 사봐야겠어요",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100039908.1.12",
          "form": "저도 다른 유명한 베개도 들여봤는데 효과 못봤어요 ㅠ",
          "original_form": "저도 다른 유명한 베개도 들여봤는데 효과 못봤어요 ㅠ",
          "speaker_id": "1",
          "time": "20210903 22:41"
        },
        {
          "id": "MDRW2100039908.1.13",
          "form": "18만원주고 시어른들 사셨는데 그닥이라하시더라고요",
          "original_form": "18만원주고 시어른들 사셨는데 그닥이라하시더라고요",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039908.1.14",
          "form": "우유베개였나...",
          "original_form": "우유베개였나...",
          "speaker_id": "2",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039908.1.15",
          "form": "저도 그거 알아요",
          "original_form": "저도 그거 알아요",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039908.1.16",
          "form": "그건 너무 비싸서 같은 모양인 저렴이로 샀는데 별로였어요",
          "original_form": "그건 너무 비싸서 같은 모양인 저렴이로 샀는데 별로였어요",
          "speaker_id": "1",
          "time": "20210903 22:45"
        },
        {
          "id": "MDRW2100039908.1.17",
          "form": "저는 그래서 베개 효과는 안믿었는데",
          "original_form": "저는 그래서 베개 효과는 안믿었는데",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100039908.1.18",
          "form": "급 궁금해졌오요 ㅎㅎ",
          "original_form": "급 궁금해졌오요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 22:46"
        },
        {
          "id": "MDRW2100039908.1.19",
          "form": "암튼 목 커브만 잘 유지되면 목 건강을 유지할 수 있는거거든요",
          "original_form": "암튼 목 커브만 잘 유지되면 목 건강을 유지할 수 있는거거든요",
          "speaker_id": "1",
          "time": "20210903 22:47"
        },
        {
          "id": "MDRW2100039908.1.20",
          "form": "이건 커브는 확실히 유지되는지 2월부터 여태 잘 쓰고 있어요",
          "original_form": "이건 커브는 확실히 유지되는지 2월부터 여태 잘 쓰고 있어요",
          "speaker_id": "1",
          "time": "20210903 22:48"
        },
        {
          "id": "MDRW2100039908.1.21",
          "form": "하긴 습관적으로 거북목처럼 되면 아프던데 커브가 중요하니ㅔ요",
          "original_form": "하긴 습관적으로 거북목처럼 되면 아프던데 커브가 중요하니ㅔ요",
          "speaker_id": "2",
          "time": "20210903 22:48"
        }
      ]
    }
