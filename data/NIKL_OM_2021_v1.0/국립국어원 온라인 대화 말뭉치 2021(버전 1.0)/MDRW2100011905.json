      "utterance": [
        {
          "id": "MDRW2100011905.1.1",
          "form": "페럴럼픽이 이제 개막했다하던디유?",
          "original_form": "페럴럼픽이 이제 개막했다하던디유?",
          "speaker_id": "1",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100011905.1.2",
          "form": "아구래요?",
          "original_form": "아구래요?",
          "speaker_id": "2",
          "time": "20210824 17:15"
        },
        {
          "id": "MDRW2100011905.1.3",
          "form": "개막식은 보셨나요??ㅎ.ㅎ",
          "original_form": "개막식은 보셨나요??ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210824 17:16"
        },
        {
          "id": "MDRW2100011905.1.4",
          "form": "아니요ㅠㅠㅜㅜㅠ못봤어요",
          "original_form": "아니요ㅠㅠㅜㅜㅠ못봤어요",
          "speaker_id": "1",
          "time": "20210824 17:16"
        },
        {
          "id": "MDRW2100011905.1.5",
          "form": "그냥 올림픽 폐막식만보궄ㅋㅋㅋ",
          "original_form": "그냥 올림픽 폐막식만보궄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 17:16"
        },
        {
          "id": "MDRW2100011905.1.6",
          "form": "올림픽 양궁 한참볼때가 한달이 되었다니",
          "original_form": "올림픽 양궁 한참볼때가 한달이 되었다니",
          "speaker_id": "2",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011905.1.7",
          "form": "믿기지않아요ㅋㅋㅋ",
          "original_form": "믿기지않아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011905.1.8",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011905.1.9",
          "form": "한달이라구요????",
          "original_form": "한달이라구요????",
          "speaker_id": "1",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011905.1.10",
          "form": "말도안돼...",
          "original_form": "말도안돼...",
          "speaker_id": "1",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011905.1.11",
          "form": "시간이 그렇게빨리지나가다니",
          "original_form": "시간이 그렇게빨리지나가다니",
          "speaker_id": "1",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011905.1.12",
          "form": "그쵸 양궁보고 배구본게 얼마 안된거같은데...",
          "original_form": "그쵸 양궁보고 배구본게 얼마 안된거같은데...",
          "speaker_id": "2",
          "time": "20210824 17:18"
        },
        {
          "id": "MDRW2100011905.1.13",
          "form": "내 8월을 누가 도려내갔네요",
          "original_form": "내 8월을 누가 도려내갔네요",
          "speaker_id": "1",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011905.1.14",
          "form": "와..배구보던때가 급 그리워지고",
          "original_form": "와..배구보던때가 급 그리워지고",
          "speaker_id": "1",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011905.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그러게요 그땐 더웠는데 이제 쌀쌀해졌어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그러게요 그땐 더웠는데 이제 쌀쌀해졌어요",
          "speaker_id": "2",
          "time": "20210824 17:19"
        },
        {
          "id": "MDRW2100011905.1.16",
          "form": "배구 볼때 아주 손에 땀나가지구",
          "original_form": "배구 볼때 아주 손에 땀나가지구",
          "speaker_id": "2",
          "time": "20210824 17:20"
        },
        {
          "id": "MDRW2100011905.1.17",
          "form": "직관하면 더 재밌겠죠",
          "original_form": "직관하면 더 재밌겠죠",
          "speaker_id": "2",
          "time": "20210824 17:20"
        },
        {
          "id": "MDRW2100011905.1.18",
          "form": "ㅠㅠㅠ직관하러 가고싳어요",
          "original_form": "ㅠㅠㅠ직관하러 가고싳어요",
          "speaker_id": "1",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011905.1.19",
          "form": "우리지역팀도 있더라구요",
          "original_form": "우리지역팀도 있더라구요",
          "speaker_id": "1",
          "time": "20210824 17:22"
        },
        {
          "id": "MDRW2100011905.1.20",
          "form": "아진짜요?? 예매한번 해보세요!",
          "original_form": "아진짜요?? 예매한번 해보세요!",
          "speaker_id": "2",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100011905.1.21",
          "form": "저는 직관해본 기억이 없는거같아요",
          "original_form": "저는 직관해본 기억이 없는거같아요",
          "speaker_id": "2",
          "time": "20210824 17:27"
        },
        {
          "id": "MDRW2100011905.1.22",
          "form": "해보신적있으세요?",
          "original_form": "해보신적있으세요?",
          "speaker_id": "2",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100011905.1.23",
          "form": "없어요!!!이번에",
          "original_form": "없어요!!!이번에",
          "speaker_id": "1",
          "time": "20210824 17:28"
        },
        {
          "id": "MDRW2100011905.1.24",
          "form": "해보고싶은데 코로나라",
          "original_form": "해보고싶은데 코로나라",
          "speaker_id": "1",
          "time": "20210824 17:29"
        },
        {
          "id": "MDRW2100011905.1.25",
          "form": "무관중이라던데요ㅠㅠㅠ힝",
          "original_form": "무관중이라던데요ㅠㅠㅠ힝",
          "speaker_id": "1",
          "time": "20210824 17:29"
        },
        {
          "id": "MDRW2100011905.1.26",
          "form": "ㅠㅠㅠㅠ흑...올림픽도 무관중하지않았나요?",
          "original_form": "ㅠㅠㅠㅠ흑...올림픽도 무관중하지않았나요?",
          "speaker_id": "2",
          "time": "20210824 17:29"
        },
        {
          "id": "MDRW2100011905.1.27",
          "form": "평창때 구경갔어야했는데..",
          "original_form": "평창때 구경갔어야했는데..",
          "speaker_id": "2",
          "time": "20210824 17:29"
        },
        {
          "id": "MDRW2100011905.1.28",
          "form": "그땐 생각하지못했어요",
          "original_form": "그땐 생각하지못했어요",
          "speaker_id": "2",
          "time": "20210824 17:29"
        },
        {
          "id": "MDRW2100011905.1.29",
          "form": "그니까요..ㅎ...어리석은 나..",
          "original_form": "그니까요..ㅎ...어리석은 나..",
          "speaker_id": "1",
          "time": "20210824 17:30"
        },
        {
          "id": "MDRW2100011905.1.30",
          "form": "ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 17:30"
        },
        {
          "id": "MDRW2100011905.1.31",
          "form": "코로나 때문에 경기도못보고",
          "original_form": "코로나 때문에 경기도못보고",
          "speaker_id": "1",
          "time": "20210824 17:30"
        },
        {
          "id": "MDRW2100011905.1.32",
          "form": "ㅠㅠㅠㅠㅠ흑..그니까요 스포츠는 또 눈앞에서 보면",
          "original_form": "ㅠㅠㅠㅠㅠ흑..그니까요 스포츠는 또 눈앞에서 보면",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011905.1.33",
          "form": "그 생동감이 다를텐데",
          "original_form": "그 생동감이 다를텐데",
          "speaker_id": "2",
          "time": "20210824 17:31"
        },
        {
          "id": "MDRW2100011905.1.34",
          "form": "마자여....ㅠㅠ",
          "original_form": "마자여....ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 17:34"
        },
        {
          "id": "MDRW2100011905.1.35",
          "form": "그리고 그 하나됨을 느끼곸ㅋㅋ응원가랑",
          "original_form": "그리고 그 하나됨을 느끼곸ㅋㅋ응원가랑",
          "speaker_id": "1",
          "time": "20210824 17:34"
        },
        {
          "id": "MDRW2100011905.1.36",
          "form": "ㅋㅋㅋㅋ야구팬들은 더욱 아쉽겠어요..야구는 진짜 응원가 부르면서 봐야 맛이라던데!",
          "original_form": "ㅋㅋㅋㅋ야구팬들은 더욱 아쉽겠어요..야구는 진짜 응원가 부르면서 봐야 맛이라던데!",
          "speaker_id": "2",
          "time": "20210824 17:35"
        },
        {
          "id": "MDRW2100011905.1.37",
          "form": "오! 야구장은 가봤는데",
          "original_form": "오! 야구장은 가봤는데",
          "speaker_id": "1",
          "time": "20210824 17:35"
        },
        {
          "id": "MDRW2100011905.1.38",
          "form": "ㄹㅇ진짜 재밌어요",
          "original_form": "ㄹㅇ진짜 재밌어요",
          "speaker_id": "1",
          "time": "20210824 17:35"
        }
      ]
    }
