      "utterance": [
        {
          "id": "MDRW2100037790.1.1",
          "form": "여행 좋아하세요??",
          "original_form": "여행 좋아하세요??",
          "speaker_id": "1",
          "time": "20210901 18:24"
        },
        {
          "id": "MDRW2100037790.1.2",
          "form": "좋아하는편입니다",
          "original_form": "좋아하는편입니다",
          "speaker_id": "2",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037790.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 18:26"
        },
        {
          "id": "MDRW2100037790.1.4",
          "form": "네 그런데 막 활동적으로 돌아다니는 편은 아니고 휴식형 좋아해요ㅎㅎ",
          "original_form": "네 그런데 막 활동적으로 돌아다니는 편은 아니고 휴식형 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100037790.1.5",
          "form": "오 저두그래요",
          "original_form": "오 저두그래요",
          "speaker_id": "2",
          "time": "20210901 18:28"
        },
        {
          "id": "MDRW2100037790.1.6",
          "form": "저질체력이라서 많이 못돌아댕겨요ㅜ",
          "original_form": "저질체력이라서 많이 못돌아댕겨요ㅜ",
          "speaker_id": "2",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100037790.1.7",
          "form": "그니까요 나이드니 호캉스가 제일좋구",
          "original_form": "그니까요 나이드니 호캉스가 제일좋구",
          "speaker_id": "1",
          "time": "20210901 18:29"
        },
        {
          "id": "MDRW2100037790.1.8",
          "form": "호캉스 최고ㅜㅜ",
          "original_form": "호캉스 최고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100037790.1.9",
          "form": "언능 코로나 끝나구 휴양지가고싶어요",
          "original_form": "언능 코로나 끝나구 휴양지가고싶어요",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100037790.1.10",
          "form": "ㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100037790.1.11",
          "form": "저도 패키지로 휴양지 갈 생각이에요ㅋㅋ 천국!",
          "original_form": "저도 패키지로 휴양지 갈 생각이에요ㅋㅋ 천국!",
          "speaker_id": "1",
          "time": "20210901 18:30"
        },
        {
          "id": "MDRW2100037790.1.12",
          "form": "저는 마사지좋아해서 방콕 자주갓엇어요ㅜ",
          "original_form": "저는 마사지좋아해서 방콕 자주갓엇어요ㅜ",
          "speaker_id": "2",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100037790.1.13",
          "form": "또가고싶어요ㅜㅜ흑",
          "original_form": "또가고싶어요ㅜㅜ흑",
          "speaker_id": "2",
          "time": "20210901 18:31"
        },
        {
          "id": "MDRW2100037790.1.14",
          "form": "방콕ㅜㅠ 진짜 동남아가 만만한데ㅜㅠ 너무 안타깝고ㅜㅠ",
          "original_form": "방콕ㅜㅠ 진짜 동남아가 만만한데ㅜㅠ 너무 안타깝고ㅜㅠ",
          "speaker_id": "1",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100037790.1.15",
          "form": "홍콩 가보셨어요??",
          "original_form": "홍콩 가보셨어요??",
          "speaker_id": "1",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100037790.1.16",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100037790.1.17",
          "form": "홍콩도좋앗어요ㅜㅜ",
          "original_form": "홍콩도좋앗어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100037790.1.18",
          "form": "호캉스랑",
          "original_form": "호캉스랑",
          "speaker_id": "2",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100037790.1.19",
          "form": "음식도 넘 맛잇엇어요",
          "original_form": "음식도 넘 맛잇엇어요",
          "speaker_id": "2",
          "time": "20210901 18:32"
        },
        {
          "id": "MDRW2100037790.1.20",
          "form": "홍콩 딤섬이랑 진짜 맛있고 분위기도 좋았는데요!!!",
          "original_form": "홍콩 딤섬이랑 진짜 맛있고 분위기도 좋았는데요!!!",
          "speaker_id": "1",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100037790.1.21",
          "form": "피크랑 다 가보셨죠??",
          "original_form": "피크랑 다 가보셨죠??",
          "speaker_id": "1",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100037790.1.22",
          "form": "네ㅎㅎ",
          "original_form": "네ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:33"
        },
        {
          "id": "MDRW2100037790.1.23",
          "form": "그홍콩스런 분위기도 좋앗어요",
          "original_form": "그홍콩스런 분위기도 좋앗어요",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100037790.1.24",
          "form": "딤섬 짱맛 ㅎㅅㅎ",
          "original_form": "딤섬 짱맛 ㅎㅅㅎ",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100037790.1.25",
          "form": "맞아요 엄청 복잡한데 멋스러운ㅎㅎ 새우 든 딤섬 너무 맛있어요!",
          "original_form": "맞아요 엄청 복잡한데 멋스러운ㅎㅎ 새우 든 딤섬 너무 맛있어요!",
          "speaker_id": "1",
