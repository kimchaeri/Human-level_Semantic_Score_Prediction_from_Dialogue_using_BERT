      "utterance": [
        {
          "id": "MDRW2100025487.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100025487.1.2",
          "form": "네~많이 오네요",
          "original_form": "네~많이 오네요",
          "speaker_id": "2",
          "time": "20210901 12:23"
        },
        {
          "id": "MDRW2100025487.1.3",
          "form": "여기더요",
          "original_form": "여기더요",
          "speaker_id": "1",
          "time": "20210901 12:40"
        },
        {
          "id": "MDRW2100025487.1.4",
          "form": "오~여긴 방금 그쳤어요.내일도 많이 온다니 조심하게요.",
          "original_form": "오~여긴 방금 그쳤어요.내일도 많이 온다니 조심하게요.",
          "speaker_id": "2",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100025487.1.5",
          "form": "걱정이에요",
          "original_form": "걱정이에요",
          "speaker_id": "1",
          "time": "20210901 12:57"
        },
        {
          "id": "MDRW2100025487.1.6",
          "form": "뭐가 제일 걱정인가요?",
          "original_form": "뭐가 제일 걱정인가요?",
          "speaker_id": "2",
          "time": "20210901 13:01"
        },
        {
          "id": "MDRW2100025487.1.7",
          "form": "너무 많이 오니",
          "original_form": "너무 많이 오니",
          "speaker_id": "1",
          "time": "20210901 13:07"
        },
        {
          "id": "MDRW2100025487.1.8",
          "form": "집이 주택이예요?아니면,출근할때 힘들어서..?",
          "original_form": "집이 주택이예요?아니면,출근할때 힘들어서..?",
          "speaker_id": "2",
          "time": "20210901 22:02"
        },
        {
          "id": "MDRW2100025487.1.9",
          "form": "출퇴근이요",
          "original_form": "출퇴근이요",
          "speaker_id": "1",
          "time": "20210901 22:16"
        },
        {
          "id": "MDRW2100025487.1.10",
          "form": "지하철이나 버스인지,혹은 자가용인지 모르겠지만 그건 눈오는날도 힘들긴하지요.",
          "original_form": "지하철이나 버스인지,혹은 자가용인지 모르겠지만 그건 눈오는날도 힘들긴하지요.",
          "speaker_id": "2",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100025487.1.11",
          "form": "네 마찬가지요",
          "original_form": "네 마찬가지요",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100025487.1.12",
          "form": "그렇군요....",
          "original_form": "그렇군요....",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100025487.1.13",
          "form": "그냥 출근이 싫어서",
          "original_form": "그냥 출근이 싫어서",
