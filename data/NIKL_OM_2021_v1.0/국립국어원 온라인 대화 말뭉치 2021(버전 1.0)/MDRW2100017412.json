      "utterance": [
        {
          "id": "MDRW2100017412.1.1",
          "form": "안녕하세요 주로 하시는 운동 있으신가요?",
          "original_form": "안녕하세요 주로 하시는 운동 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 19:14"
        },
        {
          "id": "MDRW2100017412.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017412.1.3",
          "form": "한동안 매주 테니스를 쳤었어요!",
          "original_form": "한동안 매주 테니스를 쳤었어요!",
          "speaker_id": "2",
          "time": "20210826 19:31"
        },
        {
          "id": "MDRW2100017412.1.4",
          "form": "메이저한 운동은 아니죠..? ㅎㅎ",
          "original_form": "메이저한 운동은 아니죠..? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100017412.1.5",
          "form": "아니요 테니스 너무 멋있어요 저도 한번 배워 보고 싶은 운동이에요ㅎ",
          "original_form": "아니요 테니스 너무 멋있어요 저도 한번 배워 보고 싶은 운동이에요ㅎ",
          "speaker_id": "1",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100017412.1.6",
          "form": "테니스가 생각보다 활동량이 엄청 많고",
          "original_form": "테니스가 생각보다 활동량이 엄청 많고",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017412.1.7",
          "form": "칠때마다 시원시원해서",
          "original_form": "칠때마다 시원시원해서",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017412.1.8",
          "form": "스트레스 날리기 좋아요!",
          "original_form": "스트레스 날리기 좋아요!",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017412.1.9",
          "form": "꼭 한번 배워보세요!",
          "original_form": "꼭 한번 배워보세요!",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017412.1.10",
          "form": "name1님은 하시는 운동 있나요?",
          "original_form": "&name1&님은 하시는 운동 있나요?",
          "speaker_id": "2",
          "time": "20210826 19:36"
        },
        {
          "id": "MDRW2100017412.1.11",
          "form": "내일 꼭 한번 해 볼게요 저는 수영을 제일 좋아하는데 코로나 터지고 수영장을 못 가서 너무 슬퍼요ㅠ",
          "original_form": "내일 꼭 한번 해 볼게요 저는 수영을 제일 좋아하는데 코로나 터지고 수영장을 못 가서 너무 슬퍼요ㅠ",
          "speaker_id": "1",
          "time": "20210826 19:37"
        },
        {
          "id": "MDRW2100017412.1.12",
          "form": "아... 그거 참 슬프네요",
          "original_form": "아... 그거 참 슬프네요",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017412.1.13",
          "form": "마스크를 쓰고 물에 들어갈수도 없으니..",
          "original_form": "마스크를 쓰고 물에 들어갈수도 없으니..",
          "speaker_id": "2",
          "time": "20210826 19:43"
        },
        {
          "id": "MDRW2100017412.1.14",
          "form": "수영은 얼마나 하셨나요?",
          "original_form": "수영은 얼마나 하셨나요?",
          "speaker_id": "2",
          "time": "20210826 19:44"
        },
        {
          "id": "MDRW2100017412.1.15",
          "form": "수영은 대학생때 처음 배워서 한 6년 넘은것같아용",
          "original_form": "수영은 대학생때 처음 배워서 한 6년 넘은것같아용",
          "speaker_id": "1",
          "time": "20210826 20:03"
        },
        {
          "id": "MDRW2100017412.1.16",
          "form": "오 엄청 오래하셨네요!",
          "original_form": "오 엄청 오래하셨네요!",
          "speaker_id": "2",
          "time": "20210826 20:21"
        },
        {
          "id": "MDRW2100017412.1.17",
          "form": "저도 예전에 한 1년 배웠었어요!",
          "original_form": "저도 예전에 한 1년 배웠었어요!",
          "speaker_id": "2",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100017412.1.18",
          "form": "가장 좋아하는 영법이 무엇인가요?",
          "original_form": "가장 좋아하는 영법이 무엇인가요?",
          "speaker_id": "2",
          "time": "20210826 20:22"
        },
        {
          "id": "MDRW2100017412.1.19",
          "form": "자유형으로 처음배워서 자유형으로 해요ㅋㅋㅋ",
          "original_form": "자유형으로 처음배워서 자유형으로 해요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017412.1.20",
          "form": "배영은 배운적도 없네요ㅠㅠㅋㅋㅋ",
          "original_form": "배영은 배운적도 없네요ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017412.1.21",
          "form": "접영 평영도",
          "original_form": "접영 평영도",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017412.1.22",
          "form": "안해보신건가요!?",
          "original_form": "안해보신건가요!?",
          "speaker_id": "2",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100017412.1.23",
          "form": "두개는 배워는 봤는데 저랑은 좀 안맞더라구요ㅋㅋㅋ",
          "original_form": "두개는 배워는 봤는데 저랑은 좀 안맞더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017412.1.24",
          "form": "접영 평영이 멋있긴 한데",
          "original_form": "접영 평영이 멋있긴 한데",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017412.1.25",
          "form": "효율은 아주 안좋다고 하잖아요",
          "original_form": "효율은 아주 안좋다고 하잖아요",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017412.1.26",
          "form": "자유형보단 ㅎㅎ",
          "original_form": "자유형보단 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017412.1.27",
          "form": "6년 했으면 자유형 엄청 빠르시겠어요",
          "original_form": "6년 했으면 자유형 엄청 빠르시겠어요",
          "speaker_id": "2",
          "time": "20210826 20:47"
        },
        {
          "id": "MDRW2100017412.1.28",
          "form": "아 그래요? 몰랐네요ㅋㅋㅋ 약간 체력 기르려고 하는거라",
          "original_form": "아 그래요? 몰랐네요ㅋㅋㅋ 약간 체력 기르려고 하는거라",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017412.1.29",
          "form": "시간은 안재봤는데",
          "original_form": "시간은 안재봤는데",
          "speaker_id": "1",
          "time": "20210826 20:48"
        },
        {
          "id": "MDRW2100017412.1.30",
          "form": "느리진 않은것같아용",
          "original_form": "느리진 않은것같아용",
          "speaker_id": "1",
          "time": "20210826 20:48"
        }
      ]
