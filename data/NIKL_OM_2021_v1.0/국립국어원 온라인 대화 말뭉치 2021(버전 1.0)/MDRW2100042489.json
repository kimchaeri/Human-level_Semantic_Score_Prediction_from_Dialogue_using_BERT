      "utterance": [
        {
          "id": "MDRW2100042489.1.1",
          "form": "고 김자옥 박영규 이태란",
          "original_form": "고 김자옥 박영규 이태란",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042489.1.2",
          "form": "인기가 없었나봐요",
          "original_form": "인기가 없었나봐요",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042489.1.3",
          "form": "김사랑 유준상도 나왔는데 전혀 기억도 안남",
          "original_form": "김사랑 유준상도 나왔는데 전혀 기억도 안남",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042489.1.4",
          "form": "진짜 모르겠어요ㅋ",
          "original_form": "진짜 모르겠어요ㅋ",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042489.1.5",
          "form": "41부작인데 심지어 시청률로 없어",
          "original_form": "41부작인데 심지어 시청률로 없어",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042489.1.6",
          "form": "41부작이면 일일이나 주말이네요",
          "original_form": "41부작이면 일일이나 주말이네요",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042489.1.7",
          "form": "선녀와 사기꾼을 찍는데 여기서 만났나보네",
          "original_form": "선녀와 사기꾼을 찍는데 여기서 만났나보네",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042489.1.8",
          "form": "선녀와 사기꾼은 들어본 듯 하기도 하고",
          "original_form": "선녀와 사기꾼은 들어본 듯 하기도 하고",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042489.1.9",
          "form": "일요드라마래 반년이상 했는데",
          "original_form": "일요드라마래 반년이상 했는데",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042489.1.10",
          "form": "거기에 둘이 나왔나봐요",
          "original_form": "거기에 둘이 나왔나봐요",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042489.1.11",
          "form": "아까 소이현 출연작에 있엇어 안재욱 나오는",
          "original_form": "아까 소이현 출연작에 있엇어 안재욱 나오는",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042489.1.12",
          "form": "인기는 없었죠?  기억이 안나서",
          "original_form": "인기는 없었죠?  기억이 안나서",
          "speaker_id": "1",
          "time": "20210906 22:28"
        },
        {
          "id": "MDRW2100042489.1.13",
          "form": "계속 조연이네 그여름의 태풍 여기에 아는 배우 많이 나왓네",
          "original_form": "계속 조연이네 그여름의 태풍 여기에 아는 배우 많이 나왓네",
          "speaker_id": "2",
