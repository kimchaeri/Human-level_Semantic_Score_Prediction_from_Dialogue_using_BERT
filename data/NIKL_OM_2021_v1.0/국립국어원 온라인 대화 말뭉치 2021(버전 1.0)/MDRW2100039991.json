      "utterance": [
        {
          "id": "MDRW2100039991.1.1",
          "form": "저희언니도 장난감 모사려다 이마논 사기당했어요",
          "original_form": "저희언니도 장난감 모사려다 이마논 사기당했어요",
          "speaker_id": "2",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100039991.1.2",
          "form": "근데 신고가 너무 복잡해서 그냥 잊자고",
          "original_form": "근데 신고가 너무 복잡해서 그냥 잊자고",
          "speaker_id": "2",
          "time": "20210904 00:01"
        },
        {
          "id": "MDRW2100039991.1.3",
          "form": "이게 돈보다 기분이 너무 안좋은거죠",
          "original_form": "이게 돈보다 기분이 너무 안좋은거죠",
          "speaker_id": "1",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100039991.1.4",
          "form": "당했다는 그 자체가 ㅠㅜ",
          "original_form": "당했다는 그 자체가 ㅠㅜ",
          "speaker_id": "1",
          "time": "20210904 00:02"
        },
        {
          "id": "MDRW2100039991.1.5",
          "form": "맞아요 남얘긴줄알앗는데",
          "original_form": "맞아요 남얘긴줄알앗는데",
          "speaker_id": "2",
          "time": "20210904 00:03"
        },
        {
          "id": "MDRW2100039991.1.6",
          "form": "당하려면 그냥 머에 씌인듯이",
          "original_form": "당하려면 그냥 머에 씌인듯이",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100039991.1.7",
          "form": "호로록 당하더라구요",
          "original_form": "호로록 당하더라구요",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100039991.1.8",
          "form": "네 깨닫고 나면 이미 당해잇잖아요",
          "original_form": "네 깨닫고 나면 이미 당해잇잖아요",
          "speaker_id": "2",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100039991.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 00:06"
        },
        {
          "id": "MDRW2100039991.1.10",
          "form": "그래서 요즘은 당근으로 해요",
          "original_form": "그래서 요즘은 당근으로 해요",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100039991.1.11",
          "form": "왠만하믄 당근 직거래가 맘편한거 같아요",
          "original_form": "왠만하믄 당근 직거래가 맘편한거 같아요",
          "speaker_id": "1",
          "time": "20210904 00:08"
        },
        {
          "id": "MDRW2100039991.1.12",
          "form": "네 택배비 신경안쓰고 계좌필요없으니까 좋아요",
          "original_form": "네 택배비 신경안쓰고 계좌필요없으니까 좋아요",
          "speaker_id": "2",
          "time": "20210904 00:09"
        },
        {
          "id": "MDRW2100039991.1.13",
          "form": "시간 맞으면 만나서 주고 받고",
          "original_form": "시간 맞으면 만나서 주고 받고",
          "speaker_id": "1",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100039991.1.14",
          "form": "요즘은 당근도 비대면 원하는분들도 많더라구요",
          "original_form": "요즘은 당근도 비대면 원하는분들도 많더라구요",
          "speaker_id": "1",
          "time": "20210904 00:13"
        },
        {
          "id": "MDRW2100039991.1.15",
          "form": "그래서 아파트 무인택배함 이용도 했어요",
          "original_form": "그래서 아파트 무인택배함 이용도 했어요",
          "speaker_id": "1",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100039991.1.16",
          "form": "찾아가더라고요",
          "original_form": "찾아가더라고요",
          "speaker_id": "2",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100039991.1.17",
          "form": "무인택배함도 좋네요",
          "original_form": "무인택배함도 좋네요",
          "speaker_id": "2",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100039991.1.18",
          "form": "전 우편함만 해봣는데",
          "original_form": "전 우편함만 해봣는데",
          "speaker_id": "2",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100039991.1.19",
          "form": "저는 무인택배함에 넣어두고 입금받고 비번 알려줬어요",
          "original_form": "저는 무인택배함에 넣어두고 입금받고 비번 알려줬어요",
          "speaker_id": "1",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100039991.1.20",
          "form": "그분이 야간근무하신다고 아침에 오신다고 해서",
          "original_form": "그분이 야간근무하신다고 아침에 오신다고 해서",
          "speaker_id": "1",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100039991.1.21",
          "form": "괜찮네요 그거",
          "original_form": "괜찮네요 그거",
          "speaker_id": "2",
          "time": "20210904 00:15"
        },
        {
          "id": "MDRW2100039991.1.22",
          "form": "저는 출근해야한다고 ㅎㅎㅎ 택배함에서 찾아가시라고",
          "original_form": "저는 출근해야한다고 ㅎㅎㅎ 택배함에서 찾아가시라고",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100039991.1.23",
          "form": "택배아저씨들은 무인안쓰던데ㅋ",
          "original_form": "택배아저씨들은 무인안쓰던데ㅋ",
          "speaker_id": "2",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100039991.1.24",
          "form": "서로 시간 안맞춰도 된니 괜찮더라구요",
          "original_form": "서로 시간 안맞춰도 된니 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100039991.1.25",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100039991.1.26",
          "form": "정작 택배아저씨들은 안쓰시죠 ㅎㅎㅎ",
          "original_form": "정작 택배아저씨들은 안쓰시죠 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:16"
        },
        {
          "id": "MDRW2100039991.1.27",
          "form": "집에 던져놓고 가심 ㅎㅎ",
          "original_form": "집에 던져놓고 가심 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100039991.1.28",
          "form": "네 그게 편하긴 하죠",
          "original_form": "네 그게 편하긴 하죠",
          "speaker_id": "2",
          "time": "20210904 00:17"
        },
        {
          "id": "MDRW2100039991.1.29",
          "form": "근데 어떤분은 거부하심요 ㅎㅎ",
          "original_form": "근데 어떤분은 거부하심요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:18"
        },
        {
          "id": "MDRW2100039991.1.30",
          "form": "연세가 좀 있으신지...시간이 안맞길래",
          "original_form": "연세가 좀 있으신지...시간이 안맞길래",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100039991.1.31",
          "form": "택배함에 넣어두겠다고 찾아가시고 입금해달라하니",
          "original_form": "택배함에 넣어두겠다고 찾아가시고 입금해달라하니",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100039991.1.32",
          "form": "물건을 보고 사야지요....이러심 ㅋ",
          "original_form": "물건을 보고 사야지요....이러심 ㅋ",
          "speaker_id": "1",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100039991.1.33",
          "form": "아ㅋㅋ 진짜 나이있으셨나 보네요",
          "original_form": "아ㅋㅋ 진짜 나이있으셨나 보네요",
          "speaker_id": "2",
          "time": "20210904 00:19"
        },
        {
          "id": "MDRW2100039991.1.34",
          "form": "암튼 별별사람 많은듯요 ㅎㅎㅎ",
          "original_form": "암튼 별별사람 많은듯요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100039991.1.35",
          "form": "시간이 늦었네요 오늘 즐거웠어요 덕분에 ㅎ",
          "original_form": "시간이 늦었네요 오늘 즐거웠어요 덕분에 ㅎ",
          "speaker_id": "1",
          "time": "20210904 00:21"
        },
        {
          "id": "MDRW2100039991.1.36",
          "form": "굿밤되시고 담에 또 뵈요~^^",
          "original_form": "굿밤되시고 담에 또 뵈요~^^",
          "speaker_id": "1",
