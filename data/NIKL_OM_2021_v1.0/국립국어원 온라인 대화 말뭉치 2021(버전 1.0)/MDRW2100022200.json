        {
          "id": "MDRW2100022200.1.1",
          "form": "여행조아하시나요",
          "original_form": "여행조아하시나요",
          "speaker_id": "1",
          "time": "20210829 10:55"
        },
        {
          "id": "MDRW2100022200.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100022200.1.3",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210829 11:10"
        },
        {
          "id": "MDRW2100022200.1.4",
          "form": "네 좋아해요~!",
          "original_form": "네 좋아해요~!",
          "speaker_id": "1",
          "time": "20210829 11:19"
        },
        {
          "id": "MDRW2100022200.1.5",
          "form": "국내여행 기억에 남는곳 있으세요?",
          "original_form": "국내여행 기억에 남는곳 있으세요?",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100022200.1.6",
          "form": "역시 제주요!",
          "original_form": "역시 제주요!",
          "speaker_id": "1",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100022200.1.7",
          "form": "제주 갈만한곳 추천해주세요",
          "original_form": "제주 갈만한곳 추천해주세요",
          "speaker_id": "2",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100022200.1.8",
          "form": "저 안간지 너무 오래되어서요...",
          "original_form": "저 안간지 너무 오래되어서요...",
          "speaker_id": "2",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100022200.1.9",
          "form": "섭지코지요ㅎㅎ",
          "original_form": "섭지코지요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:25"
        },
        {
          "id": "MDRW2100022200.1.10",
          "form": "가보셨을때 어떠셨어요?",
          "original_form": "가보셨을때 어떠셨어요?",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022200.1.11",
          "form": "경치가 너무좋아요ㅎㅎ",
          "original_form": "경치가 너무좋아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022200.1.12",
          "form": "그러시구나",
          "original_form": "그러시구나",
          "speaker_id": "2",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022200.1.13",
          "form": "제주에서 식사는 어떤거로 하셨나요?",
          "original_form": "제주에서 식사는 어떤거로 하셨나요?",
          "speaker_id": "2",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022200.1.14",
          "form": "역시 흑돼지죠!",
          "original_form": "역시 흑돼지죠!",
          "speaker_id": "1",
          "time": "20210829 12:48"
        },
        {
          "id": "MDRW2100022200.1.15",
          "form": "제주는 역시 흑돼지인가봐요",
          "original_form": "제주는 역시 흑돼지인가봐요",
          "speaker_id": "2",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022200.1.16",
          "form": "꾸준한 인기메뉴네요",
          "original_form": "꾸준한 인기메뉴네요",
          "speaker_id": "2",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022200.1.17",
          "form": "딱새우도 유명하다던데 드셨어요?",
          "original_form": "딱새우도 유명하다던데 드셨어요?",
          "speaker_id": "2",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022200.1.18",
          "form": "딱새우회 못먹었어요ㅠㅠ",
          "original_form": "딱새우회 못먹었어요ㅠㅠ",
          "speaker_id": "1",
