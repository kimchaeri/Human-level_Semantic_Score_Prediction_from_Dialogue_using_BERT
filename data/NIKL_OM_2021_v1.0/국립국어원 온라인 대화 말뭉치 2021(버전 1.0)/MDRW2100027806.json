      "utterance": [
        {
          "id": "MDRW2100027806.1.1",
          "form": "나는 싸이가 없어",
          "original_form": "나는 싸이가 없어",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027806.1.2",
          "form": "정확히는 대학교 때 자료 올리는 용으로 쓴 거 같은데",
          "original_form": "정확히는 대학교 때 자료 올리는 용으로 쓴 거 같은데",
          "speaker_id": "1",
          "time": "20210902 15:16"
        },
        {
          "id": "MDRW2100027806.1.3",
          "form": "난 친구",
          "original_form": "난 친구",
          "speaker_id": "2",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027806.1.4",
          "form": "이무렵에 이미 가입방식이 이상해져서",
          "original_form": "이무렵에 이미 가입방식이 이상해져서",
          "speaker_id": "1",
          "time": "20210902 15:17"
        },
        {
          "id": "MDRW2100027806.1.5",
          "form": "난 싸이가 없고 / 대학교때 학과 사진 올리는 용도로만 썼는데",
          "original_form": "난 싸이가 없고 / 대학교때 학과 사진 올리는 용도로만 썼는데",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027806.1.6",
          "form": "그것도 싸이로 안한듯",
          "original_form": "그것도 싸이로 안한듯",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027806.1.7",
          "form": "네이트온의 기능이었던 거 같다ㅋㅋㅋ",
          "original_form": "네이트온의 기능이었던 거 같다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027806.1.8",
          "form": "넌 했어??",
          "original_form": "넌 했어??",
          "speaker_id": "1",
          "time": "20210902 15:18"
        },
        {
          "id": "MDRW2100027806.1.9",
          "form": "어휴 일하니까 바로 못 보내네 ㅋㅋㅋ",
          "original_form": "어휴 일하니까 바로 못 보내네 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027806.1.10",
          "form": "네이트온-싸이연동이었고",
          "original_form": "네이트온-싸이연동이었고",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027806.1.11",
          "form": "그냥 일기만 씀 ㅋㅋㅋㅋ",
          "original_form": "그냥 일기만 씀 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027806.1.12",
          "form": "천천히해 천천히",
          "original_form": "천천히해 천천히",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027806.1.13",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027806.1.14",
          "form": "안지워도 돼?",
          "original_form": "안지워도 돼?",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027806.1.15",
          "form": "주변에선 난리던데",
          "original_form": "주변에선 난리던데",
          "speaker_id": "1",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027806.1.16",
          "form": "웅웅",
          "original_form": "웅웅",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027806.1.17",
          "form": "사진도 암것도 없어",
          "original_form": "사진도 암것도 없어",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027806.1.18",
          "form": "근데 옛날 정보 글케 가지고 있는 것도 크리피한듯ㅋㅋㅋㅋㅋ",
          "original_form": "근데 옛날 정보 글케 가지고 있는 것도 크리피한듯ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027806.1.19",
          "form": "블로그가 그래서 지난번에 이벤트 할 때",
          "original_form": "블로그가 그래서 지난번에 이벤트 할 때",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027806.1.20",
          "form": "덕질 기억이나 교환일기는 있다만 ㅋㅋㅋ",
          "original_form": "덕질 기억이나 교환일기는 있다만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027806.1.21",
          "form": "옛 게시글 많이 삭제됐다던뎈ㅋㅋㅋㅋ",
          "original_form": "옛 게시글 많이 삭제됐다던뎈ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027806.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027806.1.23",
          "form": "네이버는 휴면으로 지워버릴애들을",
          "original_form": "네이버는 휴면으로 지워버릴애들을",
          "speaker_id": "2",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027806.1.24",
          "form": "싸이는 가지고있는거지...",
          "original_form": "싸이는 가지고있는거지...",
          "speaker_id": "2",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027806.1.25",
          "form": "근데 뭐 흑역사랄게있을까",
          "original_form": "근데 뭐 흑역사랄게있을까",
          "speaker_id": "2",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027806.1.26",
          "form": "사람들은 뭔 안 좋은 일을 했길래",
          "original_form": "사람들은 뭔 안 좋은 일을 했길래",
          "speaker_id": "2",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027806.1.27",
          "form": "다들 지레겁먹지?ㅎㅎ",
          "original_form": "다들 지레겁먹지?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027806.1.28",
          "form": "이건 나도 좀",
          "original_form": "이건 나도 좀",
          "speaker_id": "1",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027806.1.29",
          "form": "이해하는 게 옛날에 지식인에 모 물었는데",
          "original_form": "이해하는 게 옛날에 지식인에 모 물었는데",
          "speaker_id": "1",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100027806.1.30",
          "form": "그때 유행하던 (퍽 이런 거 썼더니?",
          "original_form": "그때 유행하던 (퍽 이런 거 썼더니?",
