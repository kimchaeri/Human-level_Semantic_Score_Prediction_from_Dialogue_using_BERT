      "utterance": [
        {
          "id": "MDRW2100032258.1.1",
          "form": "애기가 안좋아해요ㅎ",
          "original_form": "애기가 안좋아해요ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032258.1.2",
          "form": "도서관가서 노는것도 방법이네요ㅎ",
          "original_form": "도서관가서 노는것도 방법이네요ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032258.1.3",
          "form": "네 전 주로 도서관 자주가요 ㅎㅎ",
          "original_form": "네 전 주로 도서관 자주가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032258.1.4",
          "form": "저희 쪽에 전남도립미술관있는데 거기에 책보는곳도 있고",
          "original_form": "저희 쪽에 전남도립미술관있는데 거기에 책보는곳도 있고",
          "speaker_id": "2",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032258.1.5",
          "form": "순천에 아마 1호로 기적의 도서관 생겼을껄요?ㅎ 맞나 모르겟네요ㅎ",
          "original_form": "순천에 아마 1호로 기적의 도서관 생겼을껄요?ㅎ 맞나 모르겟네요ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:34"
        },
        {
          "id": "MDRW2100032258.1.6",
          "form": "ㅎㅎ 저희는 시에 8곳인가 잇어서오",
          "original_form": "ㅎㅎ 저희는 시에 8곳인가 잇어서오",
          "speaker_id": "1",
          "time": "20210905 14:34"
        },
        {
          "id": "MDRW2100032258.1.7",
          "form": "카페 같이 생긴곳도 잇다죠 ㅎㅎ",
          "original_form": "카페 같이 생긴곳도 잇다죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032258.1.8",
          "form": "그리고  어린이서점 (전집전문서점)이라고 있는데",
          "original_form": "그리고  어린이서점 (전집전문서점)이라고 있는데",
          "speaker_id": "1",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032258.1.9",
          "form": "오오ㅎ",
          "original_form": "오오ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032258.1.10",
          "form": "방문하면 샘플책을 줘요",
          "original_form": "방문하면 샘플책을 줘요",
          "speaker_id": "1",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032258.1.11",
          "form": "코로나만 아니더라도ㅎ",
          "original_form": "코로나만 아니더라도ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032258.1.12",
          "form": "아 그래요?ㅎ",
          "original_form": "아 그래요?ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032258.1.13",
          "form": "전집중에사 유명란책 두권정도",
          "original_form": "전집중에사 유명란책 두권정도",
          "speaker_id": "1",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032258.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032258.1.15",
          "form": "엄청 좋내요ㅎ",
          "original_form": "엄청 좋내요ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032258.1.16",
          "form": "저 카톡온게 이번에",
          "original_form": "저 카톡온게 이번에",
          "speaker_id": "1",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032258.1.17",
          "form": "name1 덕분에 정말 많은걸알아가네여ㅎ",
          "original_form": "&name1& 덕분에 정말 많은걸알아가네여ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032258.1.18",
          "form": "그레이트북스 사회공룡이라닌 전집 나왔는데",
          "original_form": "그레이트북스 사회공룡이라닌 전집 나왔는데",
          "speaker_id": "1",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100032258.1.19",
          "form": "방문해서 샘플책 받아 가라고 하더라구요",
          "original_form": "방문해서 샘플책 받아 가라고 하더라구요",
          "speaker_id": "1",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100032258.1.20",
          "form": "이야 그거 엄청 유명하잖아요ㅎ",
          "original_form": "이야 그거 엄청 유명하잖아요ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100032258.1.21",
          "form": "저도 가져가라고 문자왔는데",
          "original_form": "저도 가져가라고 문자왔는데",
          "speaker_id": "2",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100032258.1.22",
          "form": "네 전 과학공룡만 ㅎ 샀는데",
          "original_form": "네 전 과학공룡만 ㅎ 샀는데",
          "speaker_id": "1",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100032258.1.23",
          "form": "안가고있어요ㅠ 가면 계약할까봐ㅠ",
          "original_form": "안가고있어요ㅠ 가면 계약할까봐ㅠ",
          "speaker_id": "2",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100032258.1.24",
          "form": "나머진 도서관 가고",
          "original_form": "나머진 도서관 가고",
          "speaker_id": "1",
          "time": "20210905 14:36"
        },
        {
          "id": "MDRW2100032258.1.25",
          "form": "우와우와ㅎ",
          "original_form": "우와우와ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100032258.1.26",
          "form": "아 그리고  저렴하게 사는방법",
          "original_form": "아 그리고  저렴하게 사는방법",
          "speaker_id": "1",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100032258.1.27",
          "form": "어떻게요?",
          "original_form": "어떻게요?",
          "speaker_id": "2",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100032258.1.28",
          "form": "아 순천은 지역화페카드 없죠 ㅜㅠ",
          "original_form": "아 순천은 지역화페카드 없죠 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100032258.1.29",
          "form": "아ㅎ 있어요ㅎ",
          "original_form": "아ㅎ 있어요ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100032258.1.30",
          "form": "아 아니면 이번에 그거 나오자나요",
          "original_form": "아 아니면 이번에 그거 나오자나요",
          "speaker_id": "1",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100032258.1.31",
          "form": "정부에서 나오는거",
          "original_form": "정부에서 나오는거",
          "speaker_id": "1",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100032258.1.32",
          "form": "지역화폐는 있고 카드는없을꺼애요ㅎ",
          "original_form": "지역화폐는 있고 카드는없을꺼애요ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100032258.1.33",
          "form": "네네ㅎ",
          "original_form": "네네ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:37"
        },
        {
          "id": "MDRW2100032258.1.34",
          "form": "그걸고 사고  아이가  얌전히 보면   중고판매해도",
          "original_form": "그걸고 사고  아이가  얌전히 보면   중고판매해도",
          "speaker_id": "1",
          "time": "20210905 14:38"
        },
        {
          "id": "MDRW2100032258.1.35",
          "form": "이득이긴해요",
          "original_form": "이득이긴해요",
          "speaker_id": "1",
          "time": "20210905 14:38"
        },
        {
          "id": "MDRW2100032258.1.36",
          "form": "오오ㅎ 재판매ㅎㅎ",
          "original_form": "오오ㅎ 재판매ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 14:38"
        },
        {
          "id": "MDRW2100032258.1.37",
          "form": "저희 아이도 찢고 이런건 없어서",
          "original_form": "저희 아이도 찢고 이런건 없어서",
          "speaker_id": "1",
          "time": "20210905 14:38"
        },
        {
          "id": "MDRW2100032258.1.38",
          "form": "재판매 하려구용",
          "original_form": "재판매 하려구용",
          "speaker_id": "1",
          "time": "20210905 14:38"
        },
        {
          "id": "MDRW2100032258.1.39",
          "form": "안그래도 요즘 재판매가 하나의 수단이긴해요 ㅡㅎ",
          "original_form": "안그래도 요즘 재판매가 하나의 수단이긴해요 ㅡㅎ",
          "speaker_id": "2",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100032258.1.40",
          "form": "아람이낭 그레이트북스는 워낙 수요가 많아서",
          "original_form": "아람이낭 그레이트북스는 워낙 수요가 많아서",
          "speaker_id": "1",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100032258.1.41",
          "form": "지금 저희도 집에 너무 많아서ㅎ",
          "original_form": "지금 저희도 집에 너무 많아서ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100032258.1.42",
          "form": "중고판매해도 잘팔여요",
          "original_form": "중고판매해도 잘팔여요",
          "speaker_id": "1",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100032258.1.43",
          "form": "맞네여ㅎ 무조건 잘팔리는거 위주로 해야겟내여ㅎ",
          "original_form": "맞네여ㅎ 무조건 잘팔리는거 위주로 해야겟내여ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100032258.1.44",
          "form": "아 물론 아이가 좋아해야하니",
          "original_form": "아 물론 아이가 좋아해야하니",
          "speaker_id": "1",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100032258.1.45",
          "form": "아이가 보면 좋은거고 안보면 재판매해서 이득보는거고ㅎ",
          "original_form": "아이가 보면 좋은거고 안보면 재판매해서 이득보는거고ㅎ",
          "speaker_id": "2",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100032258.1.46",
          "form": "갈때 같이가셔요 ㅎㅎ",
          "original_form": "갈때 같이가셔요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100032258.1.47",
          "form": "전 아이한테 보라하고 삿어요",
          "original_form": "전 아이한테 보라하고 삿어요",
          "speaker_id": "1",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100032258.1.48",
          "form": "그래야겟습니당ㅎㅎㅈ",
          "original_form": "그래야겟습니당ㅎㅎㅈ",
          "speaker_id": "2",
          "time": "20210905 14:39"
        },
        {
          "id": "MDRW2100032258.1.49",
          "form": "항상 감사해요ㅎㅎ 맨날 도움만 받네요ㅠ",
          "original_form": "항상 감사해요ㅎㅎ 맨날 도움만 받네요ㅠ",
          "speaker_id": "2",
          "time": "20210905 14:40"
        }
      ]
