      "utterance": [
        {
          "id": "MDRW2100008562.1.1",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008562.1.2",
          "form": "쉬러가는거면 좋아요ㅎ힐링여행ㅋㅋ돌아다니는거 말구요ㅋ",
          "original_form": "쉬러가는거면 좋아요ㅎ힐링여행ㅋㅋ돌아다니는거 말구요ㅋ",
          "speaker_id": "2",
          "time": "20210822 16:56"
        },
        {
          "id": "MDRW2100008562.1.3",
          "form": "저도 쉬는거",
          "original_form": "저도 쉬는거",
          "speaker_id": "1",
          "time": "20210822 17:04"
        },
        {
          "id": "MDRW2100008562.1.4",
          "form": "ㅋㅋ 놀러가서도 땀흘리며 고생하고 싶진 않더라구요",
          "original_form": "ㅋㅋ 놀러가서도 땀흘리며 고생하고 싶진 않더라구요",
          "speaker_id": "2",
          "time": "20210822 17:04"
        },
        {
          "id": "MDRW2100008562.1.5",
          "form": "어릴땐 싸돌아다니는거 좋아했는데",
          "original_form": "어릴땐 싸돌아다니는거 좋아했는데",
          "speaker_id": "2",
          "time": "20210822 17:04"
        },
        {
          "id": "MDRW2100008562.1.6",
          "form": "마스크때문에 더 글런가봐요",
          "original_form": "마스크때문에 더 글런가봐요",
          "speaker_id": "1",
          "time": "20210822 17:07"
        },
        {
          "id": "MDRW2100008562.1.7",
          "form": "결혼하고. 애키우다보니 쉬고만 싶어지네요 ㅋ",
          "original_form": "결혼하고. 애키우다보니 쉬고만 싶어지네요 ㅋ",
          "speaker_id": "2",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008562.1.8",
          "form": "저도 게을러져서",
          "original_form": "저도 게을러져서",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008562.1.9",
          "form": "여행도 스파 호캉스 요런것만 찾아서 봐요",
          "original_form": "여행도 스파 호캉스 요런것만 찾아서 봐요",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008562.1.10",
          "form": "호캉스 좋죠",
          "original_form": "호캉스 좋죠",
          "speaker_id": "1",
          "time": "20210822 17:42"
        },
        {
          "id": "MDRW2100008562.1.11",
          "form": "돈이 문제지. 너무좋죠",
          "original_form": "돈이 문제지. 너무좋죠",
          "speaker_id": "2",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008562.1.12",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210822 18:17"
        },
        {
          "id": "MDRW2100008562.1.13",
          "form": "여름휴가 아직 ㅁ못갔는데 갈때되면 코로나도 제발 잠잠해졌음 좋겠네요ㅠ",
          "original_form": "여름휴가 아직 ㅁ못갔는데 갈때되면 코로나도 제발 잠잠해졌음 좋겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008562.1.14",
          "form": "네 제발요",
          "original_form": "네 제발요",
          "speaker_id": "1",
          "time": "20210822 18:54"
        }
      ]
