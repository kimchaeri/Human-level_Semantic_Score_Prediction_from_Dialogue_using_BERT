      "utterance": [
        {
          "id": "MDRW2100004451.1.1",
          "form": "혹시 화장품 뭐쓰세요?",
          "original_form": "혹시 화장품 뭐쓰세요?",
          "speaker_id": "1",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004451.1.2",
          "form": "기초요",
          "original_form": "기초요",
          "speaker_id": "1",
          "time": "20210821 16:36"
        },
        {
          "id": "MDRW2100004451.1.3",
          "form": "저는 프리메라 주로써요",
          "original_form": "저는 프리메라 주로써요",
          "speaker_id": "2",
          "time": "20210821 16:37"
        },
        {
          "id": "MDRW2100004451.1.4",
          "form": "프리메라 좋아요? 트러블이 많아서",
          "original_form": "프리메라 좋아요? 트러블이 많아서",
          "speaker_id": "1",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004451.1.5",
          "form": "건성용이죠?",
          "original_form": "건성용이죠?",
          "speaker_id": "1",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004451.1.6",
          "form": "근데 샘플 받은것들 많아서 그거 소진중이예요",
          "original_form": "근데 샘플 받은것들 많아서 그거 소진중이예요",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004451.1.7",
          "form": "프리메라는 순해서 좋긴해요",
          "original_form": "프리메라는 순해서 좋긴해요",
          "speaker_id": "2",
          "time": "20210821 16:38"
        },
        {
          "id": "MDRW2100004451.1.8",
          "form": "어떤거 쓰셨어요?",
          "original_form": "어떤거 쓰셨어요?",
          "speaker_id": "2",
          "time": "20210821 16:39"
        },
        {
          "id": "MDRW2100004451.1.9",
          "form": "요즘 샘플 쌓여있어서 동인비꺼랑 이니스프리 쓰고있어요",
          "original_form": "요즘 샘플 쌓여있어서 동인비꺼랑 이니스프리 쓰고있어요",
          "speaker_id": "1",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004451.1.10",
          "form": "그린티세럼이 저한테 맞나봐요 끈적이지도 않고",
          "original_form": "그린티세럼이 저한테 맞나봐요 끈적이지도 않고",
          "speaker_id": "1",
          "time": "20210821 16:40"
        },
        {
          "id": "MDRW2100004451.1.11",
          "form": "그린티도 기초 있지않아요?",
          "original_form": "그린티도 기초 있지않아요?",
          "speaker_id": "2",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004451.1.12",
          "form": "기초는 저도 안써봤어요 세럼만ㅎ",
          "original_form": "기초는 저도 안써봤어요 세럼만ㅎ",
          "speaker_id": "1",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004451.1.13",
          "form": "저도 샘플이 쌓였어요 안쓰니 자꾸 쌓여요",
          "original_form": "저도 샘플이 쌓였어요 안쓰니 자꾸 쌓여요",
          "speaker_id": "2",
          "time": "20210821 16:52"
        },
        {
          "id": "MDRW2100004451.1.14",
          "form": "그래서 전 비싼것도 막 바르고있어요ㅎ",
          "original_form": "그래서 전 비싼것도 막 바르고있어요ㅎ",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004451.1.15",
          "form": "몸에도 바르고",
          "original_form": "몸에도 바르고",
          "speaker_id": "1",
          "time": "20210821 16:54"
        },
        {
          "id": "MDRW2100004451.1.16",
          "form": "몸에도요?",
          "original_form": "몸에도요?",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004451.1.17",
          "form": "파우치 1회용은 걍 샤워하고 몸에발라요ㅋ",
          "original_form": "파우치 1회용은 걍 샤워하고 몸에발라요ㅋ",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004451.1.18",
          "form": "그것도 빠른소진용으로 좋네요",
          "original_form": "그것도 빠른소진용으로 좋네요",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004451.1.19",
          "form": "날짜지나기전에 빨리 써야겠어요",
          "original_form": "날짜지나기전에 빨리 써야겠어요",
          "speaker_id": "1",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004451.1.20",
          "form": "저도 그래야겠어요 빨리빨리 ㅎㅎ",
          "original_form": "저도 그래야겠어요 빨리빨리 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004451.1.21",
          "form": "정리한번해서 버릴껀버리고 써야겠어요",
          "original_form": "정리한번해서 버릴껀버리고 써야겠어요",
          "speaker_id": "2",
          "time": "20210821 17:03"
        }
