      "utterance": [
        {
          "id": "MDRW2100041655.1.1",
          "form": "반려동물 키우고 계세요?",
          "original_form": "반려동물 키우고 계세요?",
          "speaker_id": "1",
          "time": "20210906 13:57"
        },
        {
          "id": "MDRW2100041655.1.2",
          "form": "아뇽",
          "original_form": "아뇽",
          "speaker_id": "2",
          "time": "20210906 17:13"
        },
        {
          "id": "MDRW2100041655.1.3",
          "form": "친구들 고양이나 강아지 키우는거 보면 귀여운데 엄두가 안나요",
          "original_form": "친구들 고양이나 강아지 키우는거 보면 귀여운데 엄두가 안나요",
          "speaker_id": "1",
          "time": "20210907 21:05"
        },
        {
          "id": "MDRW2100041655.1.4",
          "form": "저는 고양이키우고싶아요 ㅠㅠ",
          "original_form": "저는 고양이키우고싶아요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100041655.1.5",
          "form": "정말 졸귀더라구요",
          "original_form": "정말 졸귀더라구요",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100041655.1.6",
          "form": "왜 엄두가안나세뇨?",
          "original_form": "왜 엄두가안나세뇨?",
          "speaker_id": "2",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100041655.1.7",
          "form": "밤에 눈이 안무서운가요 고양잌ㅋㅋㅋㅋ",
          "original_form": "밤에 눈이 안무서운가요 고양잌ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:06"
        },
        {
          "id": "MDRW2100041655.1.8",
          "form": "음 사랑주며 시간 써서 열심히 키워야할거 같은데 잘 못할거 같아서요",
          "original_form": "음 사랑주며 시간 써서 열심히 키워야할거 같은데 잘 못할거 같아서요",
          "speaker_id": "1",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100041655.1.9",
          "form": "ㅋㅋ아 어렵죠 그건 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ아 어렵죠 그건 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100041655.1.10",
          "form": "나 자신 하나 키우기도 어려우니",
          "original_form": "나 자신 하나 키우기도 어려우니",
          "speaker_id": "2",
          "time": "20210907 21:07"
        },
        {
          "id": "MDRW2100041655.1.11",
          "form": "고양이들은 그 허리가 쭈우욱 길게 늘어질때 엄청 귀여운거 같아요ㅋㅋ",
          "original_form": "고양이들은 그 허리가 쭈우욱 길게 늘어질때 엄청 귀여운거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:11"
        },
        {
          "id": "MDRW2100041655.1.12",
          "form": "허엉 ㅠㅠ그렇죠 ㅠㅠㅠ완전 사랑스러우너요",
          "original_form": "허엉 ㅠㅠ그렇죠 ㅠㅠㅠ완전 사랑스러우너요",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100041655.1.13",
          "form": "어렸을때는 병아리 햄스터 이런거 키웠는데",
          "original_form": "어렸을때는 병아리 햄스터 이런거 키웠는데",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100041655.1.14",
          "form": "아 어릴때그렇죠!",
          "original_form": "아 어릴때그렇죠!",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100041655.1.15",
          "form": "병아리가 그렇게 약할 줄도 모르고 속상해 하고..",
          "original_form": "병아리가 그렇게 약할 줄도 모르고 속상해 하고..",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100041655.1.16",
          "form": "아 ㅠㅠㅠㅠㅠ잘죽죠 ㅠㅠㅠㅠㅠㅠㅠ염색도하고 ㅠㅠㅠ",
          "original_form": "아 ㅠㅠㅠㅠㅠ잘죽죠 ㅠㅠㅠㅠㅠㅠㅠ염색도하고 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100041655.1.17",
          "form": "염색이요? 노란 염색?",
          "original_form": "염색이요? 노란 염색?",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100041655.1.18",
          "form": "?? 분홍 파란 염색하잖아요?",
          "original_form": "?? 분홍 파란 염색하잖아요?",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100041655.1.19",
          "form": "아 염색은 본적 없어요 노란 염색인줄 알았는데!",
          "original_form": "아 염색은 본적 없어요 노란 염색인줄 알았는데!",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100041655.1.20",
          "form": "그건 원래 색이에요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그건 원래 색이에요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100041655.1.21",
          "form": "제가 나이가 더 많나봐요 ㅋㅋㅋㅋㅋㅋㅋㅋ 제 어릴땐 염색도했어요",
          "original_form": "제가 나이가 더 많나봐요 ㅋㅋㅋㅋㅋㅋㅋㅋ 제 어릴땐 염색도했어요",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100041655.1.22",
          "form": "그죠 원래색ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그죠 원래색ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:21"
        }
      ]
    }
