      "utterance": [
        {
          "id": "MDRW2100045497.1.1",
          "form": "3위는 바나나다",
          "original_form": "3위는 바나나다",
          "speaker_id": "1",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045497.1.2",
          "form": "오 멜론? 의외",
          "original_form": "오 멜론? 의외",
          "speaker_id": "2",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045497.1.3",
          "form": "코코넛소보루 꼴찌",
          "original_form": "코코넛소보루 꼴찌",
          "speaker_id": "1",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045497.1.4",
          "form": "나 바나나랑 코코넛 안먹어봄!",
          "original_form": "나 바나나랑 코코넛 안먹어봄!",
          "speaker_id": "2",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045497.1.5",
          "form": "멜론맛있던데??",
          "original_form": "멜론맛있던데??",
          "speaker_id": "1",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045497.1.6",
          "form": "박빙이었는데 블베랑",
          "original_form": "박빙이었는데 블베랑",
          "speaker_id": "1",
          "time": "20210908 02:56"
        },
        {
          "id": "MDRW2100045497.1.7",
          "form": "로아커도 이제 질렸어ㅜㅜ",
          "original_form": "로아커도 이제 질렸어ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045497.1.8",
          "form": "다른 신박한거 먹고싶다",
          "original_form": "다른 신박한거 먹고싶다",
          "speaker_id": "2",
          "time": "20210908 02:58"
        },
        {
          "id": "MDRW2100045497.1.9",
          "form": "근데지금은 배고프니까 단백질음료먹을꺼야",
          "original_form": "근데지금은 배고프니까 단백질음료먹을꺼야",
          "speaker_id": "2",
          "time": "20210908 02:59"
        },
        {
          "id": "MDRW2100045497.1.10",
          "form": "그런것도먹어? ㅋㅋㅋㅋ",
          "original_form": "그런것도먹어? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100045497.1.11",
          "form": "어떤건데?",
          "original_form": "어떤건데?",
          "speaker_id": "1",
          "time": "20210908 03:01"
        },
        {
          "id": "MDRW2100045497.1.12",
          "form": "아빠꺼ㅋㅋㅋ",
          "original_form": "아빠꺼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:02"
        },
        {
          "id": "MDRW2100045497.1.13",
          "form": "두유같인거야ㅋㅋ",
          "original_form": "두유같인거야ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 03:03"
        },
        {
          "id": "MDRW2100045497.1.14",
          "form": "아빠가 고기같은거 잘 안드셔서",
          "original_form": "아빠가 고기같은거 잘 안드셔서",
          "speaker_id": "2",
          "time": "20210908 03:03"
        },
        {
          "id": "MDRW2100045497.1.15",
          "form": "왜 고기 안드셔 ㅜㅜ",
          "original_form": "왜 고기 안드셔 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210908 03:04"
        },
        {
          "id": "MDRW2100045497.1.16",
          "form": "고기별로안좋아하심ㅋㅋㅋ냄새난다고ㅜㅜ",
          "original_form": "고기별로안좋아하심ㅋㅋㅋ냄새난다고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 03:04"
        },
        {
          "id": "MDRW2100045497.1.17",
          "form": "생선은?",
          "original_form": "생선은?",
          "speaker_id": "1",
          "time": "20210908 03:04"
        },
        {
          "id": "MDRW2100045497.1.18",
          "form": "생선은 좋아하심!",
          "original_form": "생선은 좋아하심!",
          "speaker_id": "2",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045497.1.19",
          "form": "왜 고기가 싫을까",
          "original_form": "왜 고기가 싫을까",
          "speaker_id": "2",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045497.1.20",
          "form": "나도 요즘은 고기보단 생선",
          "original_form": "나도 요즘은 고기보단 생선",
          "speaker_id": "1",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045497.1.21",
          "form": "왜ㅜㅜ",
          "original_form": "왜ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045497.1.22",
          "form": "고기 점점 맛이없다",
          "original_form": "고기 점점 맛이없다",
          "speaker_id": "1",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045497.1.23",
          "form": "친구야왜ㅜㅜ",
          "original_form": "친구야왜ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045497.1.24",
          "form": "소화도잘안되고",
          "original_form": "소화도잘안되고",
          "speaker_id": "1",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045497.1.25",
          "form": "슬프다",
          "original_form": "슬프다",
          "speaker_id": "2",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045497.1.26",
          "form": "더부룩해 고기 ㅋㅋㅋㅋ",
          "original_form": "더부룩해 고기 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045497.1.27",
          "form": "ㅜㅜㅜ대패친구ㅜㅜ",
          "original_form": "ㅜㅜㅜ대패친구ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045497.1.28",
          "form": "생선먹고싶다 ㅜ 구이",
          "original_form": "생선먹고싶다 ㅜ 구이",
          "speaker_id": "1",
          "time": "20210908 03:05"
        },
        {
          "id": "MDRW2100045497.1.29",
          "form": "생선구이맛있지않냐",
          "original_form": "생선구이맛있지않냐",
          "speaker_id": "1",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100045497.1.30",
          "form": "생선구이......맛이야있지..",
          "original_form": "생선구이......맛이야있지..",
          "speaker_id": "2",
          "time": "20210908 03:06"
        },
        {
          "id": "MDRW2100045497.1.31",
          "form": "생선전이너무...먹고시ㅡ...",
          "original_form": "생선전이너무...먹고시ㅡ...",
          "speaker_id": "1",
          "time": "20210908 03:06"
        }
      ]
    }
  ]
