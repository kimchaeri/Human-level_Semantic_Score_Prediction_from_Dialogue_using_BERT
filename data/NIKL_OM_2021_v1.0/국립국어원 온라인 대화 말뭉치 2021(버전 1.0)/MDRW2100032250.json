      "utterance": [
        {
          "id": "MDRW2100032250.1.1",
          "form": "ㅋㅋㅋㅋㅋ지금 케톡 달리는데",
          "original_form": "ㅋㅋㅋㅋㅋ지금 케톡 달리는데",
          "speaker_id": "1",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032250.1.2",
          "form": "케톡 주제가 재난지원금이랑 근근히 나오는 루카스인데",
          "original_form": "케톡 주제가 재난지원금이랑 근근히 나오는 루카스인데",
          "speaker_id": "1",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032250.1.3",
          "form": "루카스는...과연 탈퇴를 할까...?",
          "original_form": "루카스는...과연 탈퇴를 할까...?",
          "speaker_id": "1",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032250.1.4",
          "form": "루카슼ㅋㅋㅋ 걔가 여자관계 문제있던가? 찬열의 후계자임?",
          "original_form": "루카슼ㅋㅋㅋ 걔가 여자관계 문제있던가? 찬열의 후계자임?",
          "speaker_id": "2",
          "time": "20210905 14:31"
        },
        {
          "id": "MDRW2100032250.1.5",
          "form": "중국멤?",
          "original_form": "중국멤?",
          "speaker_id": "2",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032250.1.6",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032250.1.7",
          "form": "찬열 후계자",
          "original_form": "찬열 후계자",
          "speaker_id": "1",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032250.1.8",
          "form": "루카스라이팅이라고 불리던뎈ㅋㅋㅋ",
          "original_form": "루카스라이팅이라고 불리던뎈ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032250.1.9",
          "form": "루카스라이팅ㅋㅋㅋㅋㅋ 문제 일으키면서 이름을 떨치네",
          "original_form": "루카스라이팅ㅋㅋㅋㅋㅋ 문제 일으키면서 이름을 떨치네",
          "speaker_id": "2",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032250.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그니까",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그니까",
          "speaker_id": "1",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032250.1.11",
          "form": "에스엠은 애들 교육을 어케 시키는거야--",
          "original_form": "에스엠은 애들 교육을 어케 시키는거야--",
          "speaker_id": "2",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032250.1.12",
          "form": "여자들 양다리 삼다리 걸치면서",
          "original_form": "여자들 양다리 삼다리 걸치면서",
          "speaker_id": "1",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032250.1.13",
          "form": "돈 열심히 빼먹었던데",
          "original_form": "돈 열심히 빼먹었던데",
          "speaker_id": "1",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032250.1.14",
          "form": "대단한듯",
          "original_form": "대단한듯",
          "speaker_id": "1",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032250.1.15",
          "form": "문어발이네, 아이돌이 돈이 없나",
          "original_form": "문어발이네, 아이돌이 돈이 없나",
          "speaker_id": "2",
          "time": "20210905 14:32"
        },
        {
          "id": "MDRW2100032250.1.16",
          "form": "제비짓하누",
          "original_form": "제비짓하누",
          "speaker_id": "2",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032250.1.17",
          "form": "ㄱㄴㄲㅋㅋㅋㅋ 에스엠 지금 최근에만 아이린 찬열 유노윤호 김종대디...는 좀 오래됐나",
          "original_form": "ㄱㄴㄲㅋㅋㅋㅋ 에스엠 지금 최근에만 아이린 찬열 유노윤호 김종대디...는 좀 오래됐나",
          "speaker_id": "1",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032250.1.18",
          "form": "여튼 출신 크리스도 그렇고",
          "original_form": "여튼 출신 크리스도 그렇고",
          "speaker_id": "1",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032250.1.19",
          "form": "인성 터져나가네",
          "original_form": "인성 터져나가네",
          "speaker_id": "2",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032250.1.20",
          "form": "찐은 와쥐 자잘구리는 스엠인듯",
          "original_form": "찐은 와쥐 자잘구리는 스엠인듯",
          "speaker_id": "1",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032250.1.21",
          "form": "그러게 ㅋㅋㅋ 돈이 없어서 그랫나...왜그런담",
          "original_form": "그러게 ㅋㅋㅋ 돈이 없어서 그랫나...왜그런담",
          "speaker_id": "1",
          "time": "20210905 14:33"
        },
        {
          "id": "MDRW2100032250.1.22",
          "form": "중고태용부터",
          "original_form": "중고태용부터",
          "speaker_id": "2",
          "time": "20210905 14:35"
        },
        {
          "id": "MDRW2100032250.1.23",
          "form": "엔시티는 개성시대네",
          "original_form": "엔시티는 개성시대네",
          "speaker_id": "2",
          "time": "20210905 14:54"
        },
        {
          "id": "MDRW2100032250.1.24",
          "form": "대중성도 없는것들이",
          "original_form": "대중성도 없는것들이",
          "speaker_id": "2",
          "time": "20210905 14:54"
        },
        {
          "id": "MDRW2100032250.1.25",
          "form": "ㅋㅋㅋㅋㅋ그니까",
          "original_form": "ㅋㅋㅋㅋㅋ그니까",
          "speaker_id": "1",
          "time": "20210905 14:55"
        },
        {
          "id": "MDRW2100032250.1.26",
          "form": "지금 스우파 흥하고있는데",
          "original_form": "지금 스우파 흥하고있는데",
          "speaker_id": "1",
          "time": "20210905 14:55"
        },
        {
          "id": "MDRW2100032250.1.27",
          "form": "중고태용 나와서",
          "original_form": "중고태용 나와서",
          "speaker_id": "1",
          "time": "20210905 14:55"
        },
        {
          "id": "MDRW2100032250.1.28",
          "form": "지 춤도 잘 못추면거",
          "original_form": "지 춤도 잘 못추면거",
          "speaker_id": "1",
          "time": "20210905 14:55"
        },
        {
          "id": "MDRW2100032250.1.29",
          "form": "서",
          "original_form": "서",
          "speaker_id": "1",
          "time": "20210905 14:55"
        },
        {
          "id": "MDRW2100032250.1.30",
          "form": "댄서들 평가하는것도 짱나고ㅡㅡ",
          "original_form": "댄서들 평가하는것도 짱나고ㅡㅡ",
          "speaker_id": "1",
          "time": "20210905 14:55"
        }
      ]
