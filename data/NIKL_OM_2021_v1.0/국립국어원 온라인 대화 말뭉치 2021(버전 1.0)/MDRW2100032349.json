      "utterance": [
        {
          "id": "MDRW2100032349.1.1",
          "form": "날씨 어떤가요",
          "original_form": "날씨 어떤가요",
          "speaker_id": "1",
          "time": "20210905 16:03"
        },
        {
          "id": "MDRW2100032349.1.2",
          "form": "지금 흐려요 거긴 어떤가요?",
          "original_form": "지금 흐려요 거긴 어떤가요?",
          "speaker_id": "2",
          "time": "20210905 16:04"
        },
        {
          "id": "MDRW2100032349.1.3",
          "form": "여기도 오늘 날씨가 흐리네요",
          "original_form": "여기도 오늘 날씨가 흐리네요",
          "speaker_id": "1",
          "time": "20210905 16:05"
        },
        {
          "id": "MDRW2100032349.1.4",
          "form": "날씨가 전국적으로 흐린 것 같아요",
          "original_form": "날씨가 전국적으로 흐린 것 같아요",
          "speaker_id": "2",
          "time": "20210905 16:07"
        },
        {
          "id": "MDRW2100032349.1.5",
          "form": "그렇네요 어제까진 좋았는데...",
          "original_form": "그렇네요 어제까진 좋았는데...",
          "speaker_id": "1",
          "time": "20210905 16:08"
        },
        {
          "id": "MDRW2100032349.1.6",
          "form": "금토 좋았죠 마치 초여름 날씨 같았어요",
          "original_form": "금토 좋았죠 마치 초여름 날씨 같았어요",
          "speaker_id": "2",
          "time": "20210905 16:11"
        },
        {
          "id": "MDRW2100032349.1.7",
          "form": "네 하늘도 맑구요 오늘은 흐려서 아쉽네요",
          "original_form": "네 하늘도 맑구요 오늘은 흐려서 아쉽네요",
          "speaker_id": "1",
          "time": "20210905 16:41"
        },
        {
          "id": "MDRW2100032349.1.8",
          "form": "다음주 또 비소식이 있네요",
          "original_form": "다음주 또 비소식이 있네요",
          "speaker_id": "2",
          "time": "20210905 18:12"
        },
        {
          "id": "MDRW2100032349.1.9",
          "form": "가을은 온 것 같은데 비가 계속 되네요",
          "original_form": "가을은 온 것 같은데 비가 계속 되네요",
          "speaker_id": "1",
          "time": "20210905 18:50"
        },
        {
          "id": "MDRW2100032349.1.10",
          "form": "가을장마리서 그런가봐요 얼른 끝나면 좋겠어요",
          "original_form": "가을장마리서 그런가봐요 얼른 끝나면 좋겠어요",
          "speaker_id": "2",
          "time": "20210905 19:12"
        },
        {
          "id": "MDRW2100032349.1.11",
          "form": "네 가을 장마라니.. 예전에도 있었을까요? 기억이 안나는게...",
          "original_form": "네 가을 장마라니.. 예전에도 있었을까요? 기억이 안나는게...",
          "speaker_id": "1",
          "time": "20210905 20:19"
        },
        {
          "id": "MDRW2100032349.1.12",
          "form": "자주 있는 건 아닌 것 같아요",
          "original_form": "자주 있는 건 아닌 것 같아요",
          "speaker_id": "2",
          "time": "20210905 20:46"
        },
        {
          "id": "MDRW2100032349.1.13",
          "form": "그렇군요 오늘 새벽은 역시 춥네요",
          "original_form": "그렇군요 오늘 새벽은 역시 춥네요",
