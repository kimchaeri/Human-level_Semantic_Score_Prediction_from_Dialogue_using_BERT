      "utterance": [
        {
          "id": "MDRW2100030723.1.1",
          "form": "쇼핑 얘기도 정말 할게 많네요",
          "original_form": "쇼핑 얘기도 정말 할게 많네요",
          "speaker_id": "2",
          "time": "20210904 11:53"
        },
        {
          "id": "MDRW2100030723.1.2",
          "form": "최근에 신발 마음에 드는 걸 샀었는데",
          "original_form": "최근에 신발 마음에 드는 걸 샀었는데",
          "speaker_id": "2",
          "time": "20210904 11:53"
        },
        {
          "id": "MDRW2100030723.1.3",
          "form": "어떤 신발이요",
          "original_form": "어떤 신발이요",
          "speaker_id": "1",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100030723.1.4",
          "form": "아푸 분이 살짝 밑바닥 부분가 떨어져 나가서 떨어졌어요",
          "original_form": "아푸 분이 살짝 밑바닥 부분가 떨어져 나가서 떨어졌어요",
          "speaker_id": "2",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100030723.1.5",
          "form": "순간접착제로 붙여야 할 것 같아요",
          "original_form": "순간접착제로 붙여야 할 것 같아요",
          "speaker_id": "2",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100030723.1.6",
          "form": "너무 저렴한 거 샀나요",
          "original_form": "너무 저렴한 거 샀나요",
          "speaker_id": "1",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100030723.1.7",
          "form": "실수한 건가요",
          "original_form": "실수한 건가요",
          "speaker_id": "1",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100030723.1.8",
          "form": "약간 레이스 망사처럼 되어 있고 9분 놨고 신으면 배가 예쁜 것이요",
          "original_form": "약간 레이스 망사처럼 되어 있고 9분 놨고 신으면 배가 예쁜 것이요",
          "speaker_id": "2",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100030723.1.9",
          "form": "제품 하자 아니에요",
          "original_form": "제품 하자 아니에요",
          "speaker_id": "1",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100030723.1.10",
          "form": "몇 만 원 줬어요 그렇게 싼 건 아니에요",
          "original_form": "몇 만 원 줬어요 그렇게 싼 건 아니에요",
          "speaker_id": "2",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100030723.1.11",
          "form": "as 안 돼요",
          "original_form": "as 안 돼요",
          "speaker_id": "1",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100030723.1.12",
          "form": "제 걸음걸이가 이상한가 봐요 걸을 때마다 계속 걸리더니 거기 부분이 떨어지더라고요",
          "original_form": "제 걸음걸이가 이상한가 봐요 걸을 때마다 계속 걸리더니 거기 부분이 떨어지더라고요",
          "speaker_id": "2",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100030723.1.13",
          "form": "신발은 좀 좋은 거 신어야 겠더라고요",
          "original_form": "신발은 좀 좋은 거 신어야 겠더라고요",
          "speaker_id": "1",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100030723.1.14",
          "form": "순간접착제로 붙이죠 뭐",
          "original_form": "순간접착제로 붙이죠 뭐",
          "speaker_id": "2",
          "time": "20210904 11:54"
        },
        {
          "id": "MDRW2100030723.1.15",
          "form": "쿨하네요",
          "original_form": "쿨하네요",
          "speaker_id": "1",
          "time": "20210904 11:55"
        },
        {
          "id": "MDRW2100030723.1.16",
          "form": "맞아요 신었을 때 편안한게 좋아요 저는 많이 걸어야 하기 때문에 굽이 높은 거는 못 신어요",
          "original_form": "맞아요 신었을 때 편안한게 좋아요 저는 많이 걸어야 하기 때문에 굽이 높은 거는 못 신어요",
          "speaker_id": "2",
          "time": "20210904 11:55"
        },
        {
          "id": "MDRW2100030723.1.17",
          "form": "저는 신발도 두 켤레 있어요",
          "original_form": "저는 신발도 두 켤레 있어요",
          "speaker_id": "1",
          "time": "20210904 11:55"
        },
        {
          "id": "MDRW2100030723.1.18",
          "form": "슬리퍼랑 운동화 두 개만 가져왔어요",
          "original_form": "슬리퍼랑 운동화 두 개만 가져왔어요",
          "speaker_id": "1",
          "time": "20210904 11:55"
        }
      ]
