      "utterance": [
        {
          "id": "MDRW2100041250.1.1",
          "form": "헉 에인절인가요",
          "original_form": "헉 에인절인가요",
          "speaker_id": "1",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100041250.1.2",
          "form": "설마 그 전설의 퍽퍽살파?",
          "original_form": "설마 그 전설의 퍽퍽살파?",
          "speaker_id": "1",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100041250.1.3",
          "form": "그렇습니다 퍽퍽살파입니다..!!!!!!",
          "original_form": "그렇습니다 퍽퍽살파입니다..!!!!!!",
          "speaker_id": "2",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100041250.1.4",
          "form": "당신은 내 밥상동지가 될 자격이 충분합니다!!!!",
          "original_form": "당신은 내 밥상동지가 될 자격이 충분합니다!!!!",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041250.1.5",
          "form": "오시오 동무!",
          "original_form": "오시오 동무!",
          "speaker_id": "1",
          "time": "20210905 21:02"
        },
        {
          "id": "MDRW2100041250.1.6",
          "form": "...근데",
          "original_form": "...근데",
          "speaker_id": "1",
          "time": "20210905 21:03"
        },
        {
          "id": "MDRW2100041250.1.7",
          "form": "이미 사라진 김치찜고기를 배신하는 것 같아 가슴 아프네요",
          "original_form": "이미 사라진 김치찜고기를 배신하는 것 같아 가슴 아프네요",
          "speaker_id": "1",
          "time": "20210905 21:05"
        },
        {
          "id": "MDRW2100041250.1.8",
          "form": "오시오 동무ㅋㅋㅋㄱ악ㅋㄱㅋ\n닭다리닭날개로 자격을 얻은거같아 기쁩니다ㅋㅋㅋㅋㄱㄲ",
          "original_form": "오시오 동무ㅋㅋㅋㄱ악ㅋㄱㅋ\n닭다리닭날개로 자격을 얻은거같아 기쁩니다ㅋㅋㅋㅋㄱㄲ",
          "speaker_id": "2",
          "time": "20210905 21:23"
        },
        {
          "id": "MDRW2100041250.1.9",
          "form": "김치찜고기는 잊으시라우.",
          "original_form": "김치찜고기는 잊으시라우.",
          "speaker_id": "2",
          "time": "20210905 21:24"
        },
        {
          "id": "MDRW2100041250.1.10",
          "form": "ㅋㅋ 지금 집 가는 길이니 치킨 시켜놓으라우ㅎㅎ",
          "original_form": "ㅋㅋ 지금 집 가는 길이니 치킨 시켜놓으라우ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100041250.1.11",
          "form": "일인일닭인 거 알지비?",
          "original_form": "일인일닭인 거 알지비?",
          "speaker_id": "1",
          "time": "20210905 21:27"
        },
        {
          "id": "MDRW2100041250.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ\n일인일닭..동무 배가 크지비...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ\n일인일닭..동무 배가 크지비...",
          "speaker_id": "2",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100041250.1.13",
          "form": "ㅋㅋㄱㅋㅋㅋ",
          "original_form": "ㅋㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:31"
        },
        {
          "id": "MDRW2100041250.1.14",
          "form": "남기는건 다 내가 조사버리갔어ㅎㅎ",
          "original_form": "남기는건 다 내가 조사버리갔어ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:41"
        },
        {
          "id": "MDRW2100041250.1.15",
          "form": "근데 정말 일인일닭 못하시남요?",
          "original_form": "근데 정말 일인일닭 못하시남요?",
          "speaker_id": "1",
          "time": "20210905 21:42"
        },
        {
          "id": "MDRW2100041250.1.16",
          "form": "다 조사버리갔어ㅋㅋㅋㅋ네 못합니다;-;  흑흑\n쉽지않더라구요ㅋㅋㅋ",
          "original_form": "다 조사버리갔어ㅋㅋㅋㅋ네 못합니다;-;  흑흑\n쉽지않더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100041250.1.17",
          "form": "쌉가능이신가요..",
          "original_form": "쌉가능이신가요..",
          "speaker_id": "2",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100041250.1.18",
          "form": "ㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210905 21:45"
        },
        {
          "id": "MDRW2100041250.1.19",
          "form": "왜 안되죠...",
          "original_form": "왜 안되죠...",
          "speaker_id": "1",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100041250.1.20",
          "form": "교촌은 가능하죠?!!",
          "original_form": "교촌은 가능하죠?!!",
          "speaker_id": "1",
          "time": "20210905 21:49"
        },
        {
          "id": "MDRW2100041250.1.21",
          "form": "교촌도 많긴많던데...!!!!!!!!;-; 흐잉",
          "original_form": "교촌도 많긴많던데...!!!!!!!!;-; 흐잉",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100041250.1.22",
          "form": "그 레드시리즈는 그래도 좀",
          "original_form": "그 레드시리즈는 그래도 좀",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100041250.1.23",
          "form": "많이들어가긴합니다!!!",
          "original_form": "많이들어가긴합니다!!!",
          "speaker_id": "2",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100041250.1.24",
          "form": "앗ㅎㅎ",
          "original_form": "앗ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100041250.1.25",
          "form": "저 레드오리지널 최애입니다",
          "original_form": "저 레드오리지널 최애입니다",
          "speaker_id": "1",
          "time": "20210905 21:52"
        },
        {
          "id": "MDRW2100041250.1.26",
          "form": "하...진짜 맛잘알....",
          "original_form": "하...진짜 맛잘알....",
          "speaker_id": "2",
          "time": "20210905 21:53"
        },
        {
          "id": "MDRW2100041250.1.27",
          "form": "레드가 젤좋습니다요...",
          "original_form": "레드가 젤좋습니다요...{emoji:♡}",
          "speaker_id": "2",
          "time": "20210905 21:54"
        },
        {
          "id": "MDRW2100041250.1.28",
          "form": "ㅎㅎ 레드에 퍽퍽살파 저와 조화가 완벽하시네요ㅎ",
          "original_form": "ㅎㅎ 레드에 퍽퍽살파 저와 조화가 완벽하시네요ㅎ",
          "speaker_id": "1",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100041250.1.29",
          "form": "목은 드시나요?",
          "original_form": "목은 드시나요?",
          "speaker_id": "1",
          "time": "20210905 21:56"
        },
        {
          "id": "MDRW2100041250.1.30",
          "form": "목 안먹습니다..!!  목이 맛있다는데 목에 먹을것있나요... 맛잘알님..!!!",
          "original_form": "목 안먹습니다..!!  목이 맛있다는데 목에 먹을것있나요... 맛잘알님..!!!",
          "speaker_id": "2",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100041250.1.31",
          "form": "제가 목을 사랑하니 더 좋군요ㅎㅎ",
          "original_form": "제가 목을 사랑하니 더 좋군요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 21:58"
        },
        {
          "id": "MDRW2100041250.1.32",
          "form": "저 목뼈 발라먹는것도 좋아하고",
          "original_form": "저 목뼈 발라먹는것도 좋아하고",
          "speaker_id": "1",
          "time": "20210905 21:59"
        },
        {
          "id": "MDRW2100041250.1.33",
          "form": "교촌이 좀 작잖아요ㅎ",
          "original_form": "교촌이 좀 작잖아요ㅎ",
