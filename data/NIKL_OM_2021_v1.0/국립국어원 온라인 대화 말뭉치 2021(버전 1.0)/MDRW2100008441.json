        {
          "id": "MDRW2100008441.1.1",
          "form": "슬의생 보시나요?",
          "original_form": "슬의생 보시나요?",
          "speaker_id": "2",
          "time": "20210822 15:48"
        },
        {
          "id": "MDRW2100008441.1.2",
          "form": "전 1기 다보곺2기는보다 못봤어요",
          "original_form": "전 1기 다보곺2기는보다 못봤어요",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008441.1.3",
          "form": "나중에 몰아 보려고요",
          "original_form": "나중에 몰아 보려고요",
          "speaker_id": "1",
          "time": "20210822 15:50"
        },
        {
          "id": "MDRW2100008441.1.4",
          "form": "저는 간간히  보는데 은근 감동적이네요",
          "original_form": "저는 간간히  보는데 은근 감동적이네요",
          "speaker_id": "2",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008441.1.5",
          "form": "맞아요 말투도 잔잔하고",
          "original_form": "맞아요 말투도 잔잔하고",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008441.1.6",
          "form": "시즌1보다는 지루한감은 있지만요 ㅎㅎ",
          "original_form": "시즌1보다는 지루한감은 있지만요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008441.1.7",
          "form": "몰입감 있어요",
          "original_form": "몰입감 있어요",
          "speaker_id": "1",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008441.1.8",
          "form": "스포하면 안되겠네요 ㅎ",
          "original_form": "스포하면 안되겠네요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008441.1.9",
          "form": "1이 잼났죠 ost도 넘좋았어요",
          "original_form": "1이 잼났죠 ost도 넘좋았어요",
          "speaker_id": "1",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008441.1.10",
          "form": "나중에 다보셔야하니카요 ㅋㅋ",
          "original_form": "나중에 다보셔야하니카요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008441.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008441.1.12",
          "form": "저 영화 스포보고 가요 ㅋㅋ",
          "original_form": "저 영화 스포보고 가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008441.1.13",
          "form": "OST도 넘 머쩌요",
          "original_form": "OST도 넘 머쩌요",
          "speaker_id": "2",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008441.1.14",
          "form": "괜찮아요",
          "original_form": "괜찮아요",
          "speaker_id": "1",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008441.1.15",
          "form": "친숙한 노래들이라 더 그런거같아요",
          "original_form": "친숙한 노래들이라 더 그런거같아요",
          "speaker_id": "2",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008441.1.16",
          "form": "네 목소리가 ㅋㅋ",
          "original_form": "네 목소리가 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008441.1.17",
          "form": "비전공자들이 부르니 친근감도 막 가고요",
          "original_form": "비전공자들이 부르니 친근감도 막 가고요",
          "speaker_id": "2",
          "time": "20210822 15:52"
        },
        {
          "id": "MDRW2100008441.1.18",
          "form": "전미도 너래 음치인척 하는거 웃겨요",
          "original_form": "전미도 너래 음치인척 하는거 웃겨요",
          "speaker_id": "1",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008441.1.19",
          "form": "진짜 잘하시는데",
          "original_form": "진짜 잘하시는데",
          "speaker_id": "1",
          "time": "20210822 15:53"
        },
        {
          "id": "MDRW2100008441.1.20",
