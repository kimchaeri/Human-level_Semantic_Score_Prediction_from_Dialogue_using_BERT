      "utterance": [
        {
          "id": "MDRW2100034584.1.1",
          "form": "아~양도 적고 비싸면 비싼거맞네요ㅠ",
          "original_form": "아~양도 적고 비싸면 비싼거맞네요ㅠ",
          "speaker_id": "2",
          "time": "20210828 15:45"
        },
        {
          "id": "MDRW2100034584.1.2",
          "form": "맞아여 ㅜㅜ",
          "original_form": "맞아여 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100034584.1.3",
          "form": "그래도 맛만좋으면",
          "original_form": "그래도 맛만좋으면",
          "speaker_id": "2",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100034584.1.4",
          "form": "그래도 너무 맛있어서 항상 먹어요",
          "original_form": "그래도 너무 맛있어서 항상 먹어요",
          "speaker_id": "1",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100034584.1.5",
          "form": "용서되죠ㅋㅋ",
          "original_form": "용서되죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100034584.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100034584.1.7",
          "form": "저는 제일 용서 안 되는게",
          "original_form": "저는 제일 용서 안 되는게",
          "speaker_id": "1",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100034584.1.8",
          "form": "맛없는데 비싸고 살찌는 음식이요 ㅠㅠ",
          "original_form": "맛없는데 비싸고 살찌는 음식이요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100034584.1.9",
          "form": "초코팝콘 꼭 먹어봐야겠어요",
          "original_form": "초코팝콘 꼭 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100034584.1.10",
          "form": "와ㅋㅋㅋㅋ",
          "original_form": "와ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100034584.1.11",
          "form": "최악의 조합이네요 진짜ㅋㅋ",
          "original_form": "최악의 조합이네요 진짜ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100034584.1.12",
          "form": "그쵸 ㅜ",
          "original_form": "그쵸 ㅜ",
          "speaker_id": "1",
          "time": "20210828 15:46"
        },
        {
          "id": "MDRW2100034584.1.13",
          "form": "차라리 맛있고 살찌고 비싸면 용서되는데 ㅋㅋㅋ",
          "original_form": "차라리 맛있고 살찌고 비싸면 용서되는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100034584.1.14",
          "form": "마자요 맛있으면 비싸든 살찌든",
          "original_form": "마자요 맛있으면 비싸든 살찌든",
          "speaker_id": "2",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100034584.1.15",
          "form": "다 용서되죠ㅋㅋ",
          "original_form": "다 용서되죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:47"
        },
        {
          "id": "MDRW2100034584.1.16",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100034584.1.17",
          "form": "맛있는건 0칼로리에요..!!",
          "original_form": "맛있는건 0칼로리에요..!!",
          "speaker_id": "1",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100034584.1.18",
          "form": "진짜 성인되고 돈벌어서 좋은게 비싸고 맛있는거 걱정많이 안하고 사먹을수있어서 좋아요",
          "original_form": "진짜 성인되고 돈벌어서 좋은게 비싸고 맛있는거 걱정많이 안하고 사먹을수있어서 좋아요",
          "speaker_id": "2",
          "time": "20210828 15:48"
        },
        {
          "id": "MDRW2100034584.1.19",
          "form": "0칼로리죠ㅋㅋ",
          "original_form": "0칼로리죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:48"
        },
        {
