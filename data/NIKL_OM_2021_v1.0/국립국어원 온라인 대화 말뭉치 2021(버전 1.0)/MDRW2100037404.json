      "utterance": [
        {
          "id": "MDRW2100037404.1.1",
          "form": "좋아 근데 너무 오래걸려",
          "original_form": "좋아 근데 너무 오래걸려",
          "speaker_id": "2",
          "time": "20210831 23:19"
        },
        {
          "id": "MDRW2100037404.1.2",
          "form": "파마 너무 오래걸리지 않아?",
          "original_form": "파마 너무 오래걸리지 않아?",
          "speaker_id": "2",
          "time": "20210831 23:19"
        },
        {
          "id": "MDRW2100037404.1.3",
          "form": "나는 사실 미용에는 인내심이 없어",
          "original_form": "나는 사실 미용에는 인내심이 없어",
          "speaker_id": "2",
          "time": "20210831 23:19"
        },
        {
          "id": "MDRW2100037404.1.4",
          "form": "귀찮음이 큰 편이야",
          "original_form": "귀찮음이 큰 편이야",
          "speaker_id": "2",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037404.1.5",
          "form": "맞아 미용실가면 기빨려",
          "original_form": "맞아 미용실가면 기빨려",
          "speaker_id": "1",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037404.1.6",
          "form": "시간이 너무 아까워",
          "original_form": "시간이 너무 아까워",
          "speaker_id": "2",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037404.1.7",
          "form": "잠오는데 잘수도 없잖아",
          "original_form": "잠오는데 잘수도 없잖아",
          "speaker_id": "1",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037404.1.8",
          "form": "너는 파마 좋아해?",
          "original_form": "너는 파마 좋아해?",
          "speaker_id": "2",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037404.1.9",
          "form": "응 나는 생머리하면",
          "original_form": "응 나는 생머리하면",
          "speaker_id": "1",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037404.1.10",
          "form": "맞아. 그리고 비싸",
          "original_form": "맞아. 그리고 비싸",
          "speaker_id": "2",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037404.1.11",
          "form": "머리숱이 너무 적어보여서 주기적으로 파마를해",
          "original_form": "머리숱이 너무 적어보여서 주기적으로 파마를해",
          "speaker_id": "1",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037404.1.12",
          "form": "너는 머리숱이 만ㄹ니?",
          "original_form": "너는 머리숱이 만ㄹ니?",
          "speaker_id": "1",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037404.1.13",
          "form": "그런 비결이 있었구나",
          "original_form": "그런 비결이 있었구나",
          "speaker_id": "2",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037404.1.14",
          "form": "나는 많아",
          "original_form": "나는 많아",
          "speaker_id": "2",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037404.1.15",
          "form": "좋겠다 나 좀 나눠두라",
          "original_form": "좋겠다 나 좀 나눠두라",
          "speaker_id": "1",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037404.1.16",
          "form": "앞으로도 많았으면 좋겠어",
          "original_form": "앞으로도 많았으면 좋겠어",
          "speaker_id": "2",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037404.1.17",
          "form": "어떻게 유지중인지 알려줘",
          "original_form": "어떻게 유지중인지 알려줘",
          "speaker_id": "1",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037404.1.18",
          "form": "근데ㅜ요즘 흰머리가 나",
          "original_form": "근데ㅜ요즘 흰머리가 나",
          "speaker_id": "2",
          "time": "20210831 23:20"
        },
        {
          "id": "MDRW2100037404.1.19",
          "form": "나중에 새치 염색 해야겠어",
          "original_form": "나중에 새치 염색 해야겠어",
          "speaker_id": "2",
          "time": "20210831 23:21"
        },
        {
          "id": "MDRW2100037404.1.20",
          "form": "요즘 염색삼퓨가 있대",
          "original_form": "요즘 염색삼퓨가 있대",
          "speaker_id": "1",
          "time": "20210831 23:21"
        },
        {
          "id": "MDRW2100037404.1.21",
          "form": "검은콩도 흰 머리 안나게 하는 데 좋대",
          "original_form": "검은콩도 흰 머리 안나게 하는 데 좋대",
          "speaker_id": "2",
          "time": "20210831 23:21"
        },
        {
          "id": "MDRW2100037404.1.22",
          "form": "같이 알아보자 새치를 예방해야해",
          "original_form": "같이 알아보자 새치를 예방해야해",
          "speaker_id": "1",
          "time": "20210831 23:21"
        },
        {
          "id": "MDRW2100037404.1.23",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210831 23:21"
        }
      ]
