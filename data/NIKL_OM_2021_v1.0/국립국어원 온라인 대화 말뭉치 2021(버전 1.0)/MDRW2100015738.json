      "utterance": [
        {
          "id": "MDRW2100015738.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 00:42"
        },
        {
          "id": "MDRW2100015738.1.2",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 00:49"
        },
        {
          "id": "MDRW2100015738.1.3",
          "form": "네~",
          "original_form": "네~",
          "speaker_id": "2",
          "time": "20210826 00:49"
        },
        {
          "id": "MDRW2100015738.1.4",
          "form": "잘 아는 편은 아닌데",
          "original_form": "잘 아는 편은 아닌데",
          "speaker_id": "2",
          "time": "20210826 00:49"
        },
        {
          "id": "MDRW2100015738.1.5",
          "form": "요즘 모바일 게임 즐겨 하고 있어요",
          "original_form": "요즘 모바일 게임 즐겨 하고 있어요",
          "speaker_id": "2",
          "time": "20210826 00:50"
        },
        {
          "id": "MDRW2100015738.1.6",
          "form": "쿠키런 킹덤이요",
          "original_form": "쿠키런 킹덤이요",
          "speaker_id": "2",
          "time": "20210826 00:50"
        },
        {
          "id": "MDRW2100015738.1.7",
          "form": "오 좋은 캐릭터 많이 있으세요?",
          "original_form": "오 좋은 캐릭터 많이 있으세요?",
          "speaker_id": "1",
          "time": "20210826 00:59"
        },
        {
          "id": "MDRW2100015738.1.8",
          "form": "3개 빼고 다 있는 것 같아요",
          "original_form": "3개 빼고 다 있는 것 같아요",
          "speaker_id": "2",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015738.1.9",
          "form": "근데 퓨어바닐라가 없어서...",
          "original_form": "근데 퓨어바닐라가 없어서...",
          "speaker_id": "2",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015738.1.10",
          "form": "ㅠㅠ ㅋㅋㅋ",
          "original_form": "ㅠㅠ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:00"
        },
        {
          "id": "MDRW2100015738.1.11",
          "form": "와 그럼 끝까지 깨셨나요? 스토리모드",
          "original_form": "와 그럼 끝까지 깨셨나요? 스토리모드",
          "speaker_id": "1",
          "time": "20210826 01:03"
        },
        {
          "id": "MDRW2100015738.1.12",
          "form": "10 스테이지가 끝이고 31까지 있는데",
          "original_form": "10 스테이지가 끝이고 31까지 있는데",
          "speaker_id": "2",
          "time": "20210826 01:07"
        },
        {
          "id": "MDRW2100015738.1.13",
          "form": "한 15? 하고 있어요",
          "original_form": "한 15? 하고 있어요",
          "speaker_id": "2",
          "time": "20210826 01:07"
        },
        {
          "id": "MDRW2100015738.1.14",
          "form": "아무래도 육성을 잘못해서..ㅋㅋ큐ㅠㅠ",
          "original_form": "아무래도 육성을 잘못해서..ㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 01:07"
        },
        {
          "id": "MDRW2100015738.1.15",
          "form": "바다요정맛 쿠키 있으세요??",
          "original_form": "바다요정맛 쿠키 있으세요??",
          "speaker_id": "1",
          "time": "20210826 01:08"
        },
        {
          "id": "MDRW2100015738.1.16",
          "form": "네 있어여 ㅋㅋㅋ",
          "original_form": "네 있어여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:09"
        },
        {
          "id": "MDRW2100015738.1.17",
          "form": "바요는 귀엽고 그래서 만렙 찍어주느라 힘들었어요",
          "original_form": "바요는 귀엽고 그래서 만렙 찍어주느라 힘들었어요",
          "speaker_id": "2",
          "time": "20210826 01:09"
        },
        {
          "id": "MDRW2100015738.1.18",
          "form": "오 현질도 해보셨나요?",
          "original_form": "오 현질도 해보셨나요?",
          "speaker_id": "1",
          "time": "20210826 01:10"
        },
        {
          "id": "MDRW2100015738.1.19",
          "form": "아니요 무과금러에요",
          "original_form": "아니요 무과금러에요",
          "speaker_id": "2",
          "time": "20210826 01:12"
        },
        {
          "id": "MDRW2100015738.1.20",
          "form": "뽑기운 좋으셨나봐요~",
          "original_form": "뽑기운 좋으셨나봐요~",
          "speaker_id": "1",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100015738.1.21",
          "form": "주변에 현질한 사람 많나요?",
          "original_form": "주변에 현질한 사람 많나요?",
          "speaker_id": "1",
          "time": "20210826 01:20"
        },
        {
          "id": "MDRW2100015738.1.22",
          "form": "주변에 현질하는 사람이 많이 없어서",
          "original_form": "주변에 현질하는 사람이 많이 없어서",
          "speaker_id": "2",
          "time": "20210826 01:22"
        },
        {
          "id": "MDRW2100015738.1.23",
          "form": "제가 영향을 받은 것 같아요..!",
          "original_form": "제가 영향을 받은 것 같아요..!",
          "speaker_id": "2",
          "time": "20210826 01:22"
        },
        {
          "id": "MDRW2100015738.1.24",
          "form": "리듬게임은 안좋아하세요?",
          "original_form": "리듬게임은 안좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 01:23"
        },
        {
          "id": "MDRW2100015738.1.25",
          "form": "아 리듬게임",
          "original_form": "아 리듬게임",
          "speaker_id": "2",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015738.1.26",
          "form": "그거 아세요? 유행 좀 지나긴 했는데",
          "original_form": "그거 아세요? 유행 좀 지나긴 했는데",
          "speaker_id": "2",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015738.1.27",
          "form": "불과 물의 춤??",
          "original_form": "불과 물의 춤??",
          "speaker_id": "2",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015738.1.28",
          "form": "한창 유튜버들이 하고 그랬던 게임이요",
          "original_form": "한창 유튜버들이 하고 그랬던 게임이요",
          "speaker_id": "2",
          "time": "20210826 01:24"
        },
        {
          "id": "MDRW2100015738.1.29",
          "form": "처음듣는거같아요!",
          "original_form": "처음듣는거같아요!",
          "speaker_id": "1",
          "time": "20210826 01:25"
        }
      ]
    }
