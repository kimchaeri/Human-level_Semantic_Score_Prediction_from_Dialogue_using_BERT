      "utterance": [
        {
          "id": "MDRW2100021994.1.1",
          "form": "안녕하세용 어제 뭐 드셨나용?",
          "original_form": "안녕하세용 어제 뭐 드셨나용?",
          "speaker_id": "1",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021994.1.2",
          "form": "저 마라탕 먹엇어요!",
          "original_form": "저 마라탕 먹엇어요!",
          "speaker_id": "2",
          "time": "20210829 00:32"
        },
        {
          "id": "MDRW2100021994.1.3",
          "form": "와 마라탕!! 넘 맛있죠ㅠㅠ",
          "original_form": "와 마라탕!! 넘 맛있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 00:33"
        },
        {
          "id": "MDRW2100021994.1.4",
          "form": "고기 넣어드셨나요?",
          "original_form": "고기 넣어드셨나요?",
          "speaker_id": "1",
          "time": "20210829 00:33"
        },
        {
          "id": "MDRW2100021994.1.5",
          "form": "당연하죠 ㅋㅋㅋㅋ",
          "original_form": "당연하죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:03"
        },
        {
          "id": "MDRW2100021994.1.6",
          "form": "마라탕에 고기는 필수입니당",
          "original_form": "마라탕에 고기는 필수입니당",
          "speaker_id": "2",
          "time": "20210829 01:03"
        },
        {
          "id": "MDRW2100021994.1.7",
          "form": "전 마라탕에 들어간 건두부가 그렇게 맛있더라구여ㅋㅋㅋ",
          "original_form": "전 마라탕에 들어간 건두부가 그렇게 맛있더라구여ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:06"
        },
        {
          "id": "MDRW2100021994.1.8",
          "form": "오 저는 팽이버섯 좋아해요 ㅋㅋㅋ",
          "original_form": "오 저는 팽이버섯 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:06"
        },
        {
          "id": "MDRW2100021994.1.9",
          "form": "마라탕 한동안 위생 난리나서 못갔는데",
          "original_form": "마라탕 한동안 위생 난리나서 못갔는데",
          "speaker_id": "1",
          "time": "20210829 01:06"
        },
        {
          "id": "MDRW2100021994.1.10",
          "form": "다시 가야겠어요ㅠㅠ",
          "original_form": "다시 가야겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 01:06"
        },
        {
          "id": "MDRW2100021994.1.11",
          "form": "위생이 그래도.. 마라탕은 중독이라 끊기 힘드네요 ㅋㅋㅋㅋ",
          "original_form": "위생이 그래도.. 마라탕은 중독이라 끊기 힘드네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100021994.1.12",
          "form": "맞아요 진짜 중독ㅋㅋㅋㅋ 저는 한동안",
          "original_form": "맞아요 진짜 중독ㅋㅋㅋㅋ 저는 한동안",
          "speaker_id": "1",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100021994.1.13",
          "form": "훠궈도 엄청 많이 먹었어요",
          "original_form": "훠궈도 엄청 많이 먹었어요",
          "speaker_id": "1",
          "time": "20210829 01:08"
        },
        {
          "id": "MDRW2100021994.1.14",
          "form": "오 저 훠궈는 한번도 안먹어봣는데 무슨 맛이에요?",
          "original_form": "오 저 훠궈는 한번도 안먹어봣는데 무슨 맛이에요?",
          "speaker_id": "2",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100021994.1.15",
          "form": "이게 홍탕 백탕이 있는데",
          "original_form": "이게 홍탕 백탕이 있는데",
          "speaker_id": "1",
          "time": "20210829 01:09"
        },
        {
          "id": "MDRW2100021994.1.16",
          "form": "홍탕은 약간 얼큰한 음...",
          "original_form": "홍탕은 약간 얼큰한 음...",
          "speaker_id": "1",
          "time": "20210829 01:10"
        },
        {
          "id": "MDRW2100021994.1.17",
          "form": "뭐라해여할까요ㅋㅋㅋㅋ",
          "original_form": "뭐라해여할까요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:10"
        },
        {
          "id": "MDRW2100021994.1.18",
          "form": "마라탕의 순한맛....?",
          "original_form": "마라탕의 순한맛....?",
          "speaker_id": "1",
          "time": "20210829 01:10"
        },
        {
          "id": "MDRW2100021994.1.19",
          "form": "근데 마라 특유의 향은 안나는...",
          "original_form": "근데 마라 특유의 향은 안나는...",
          "speaker_id": "1",
          "time": "20210829 01:10"
        },
        {
          "id": "MDRW2100021994.1.20",
          "form": "백탕은 약간 세제 푼맛나요ㅋㅋ",
          "original_form": "백탕은 약간 세제 푼맛나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 01:10"
        },
        {
          "id": "MDRW2100021994.1.21",
          "form": "오 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:11"
        },
        {
          "id": "MDRW2100021994.1.22",
          "form": "전 마라탕만 먹을랍니다..ㅋㅋㅋㅋㅋ",
          "original_form": "전 마라탕만 먹을랍니다..ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 01:11"
        },
        {
          "id": "MDRW2100021994.1.23",
          "form": "ㅋㅋㅋㅋ 그래도 맛있는집은 되게 맛있아요!",
          "original_form": "ㅋㅋㅋㅋ 그래도 맛있는집은 되게 맛있아요!",
