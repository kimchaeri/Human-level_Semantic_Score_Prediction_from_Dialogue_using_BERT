      "utterance": [
        {
          "id": "MDRW2100030870.1.1",
          "form": "국민지원금에 왜 스타벅스 이케아 제외야?",
          "original_form": "국민지원금에 왜 스타벅스 이케아 제외야?",
          "speaker_id": "1",
          "time": "20210904 13:40"
        },
        {
          "id": "MDRW2100030870.1.2",
          "form": "사실 난 오늘 처음 국민지원금 신청해야하는거 알았어",
          "original_form": "사실 난 오늘 처음 국민지원금 신청해야하는거 알았어",
          "speaker_id": "1",
          "time": "20210904 13:41"
        },
        {
          "id": "MDRW2100030870.1.3",
          "form": "부모님이 하는건줄 알았는데",
          "original_form": "부모님이 하는건줄 알았는데",
          "speaker_id": "1",
          "time": "20210904 13:41"
        },
        {
          "id": "MDRW2100030870.1.4",
          "form": "어디서하면 더 좋은가 싶기도 하구",
          "original_form": "어디서하면 더 좋은가 싶기도 하구",
          "speaker_id": "1",
          "time": "20210904 13:41"
        },
        {
          "id": "MDRW2100030870.1.5",
          "form": "스타벅스랑 이캐아가 전부 본사 직영이라 그럴거야",
          "original_form": "스타벅스랑 이캐아가 전부 본사 직영이라 그럴거야",
          "speaker_id": "2",
          "time": "20210904 13:41"
        },
        {
          "id": "MDRW2100030870.1.6",
          "form": "이미 대기업들이 하는거라 지역경제활성화엔 도움이 안된다 이런 취지겠지",
          "original_form": "이미 대기업들이 하는거라 지역경제활성화엔 도움이 안된다 이런 취지겠지",
          "speaker_id": "2",
          "time": "20210904 13:42"
        },
        {
          "id": "MDRW2100030870.1.7",
          "form": "그래도 더 좋지 않아?? 25만원 받는거니까",
          "original_form": "그래도 더 좋지 않아?? 25만원 받는거니까",
          "speaker_id": "2",
          "time": "20210904 13:42"
        },
        {
          "id": "MDRW2100030870.1.8",
          "form": "아아?!",
          "original_form": "아아?!",
          "speaker_id": "1",
          "time": "20210904 13:53"
        },
        {
          "id": "MDRW2100030870.1.9",
          "form": "본사직영이라 글쿠낭",
          "original_form": "본사직영이라 글쿠낭",
          "speaker_id": "1",
          "time": "20210904 13:53"
        },
        {
          "id": "MDRW2100030870.1.10",
          "form": "그럼 온라인에서는 못쓰것네?",
          "original_form": "그럼 온라인에서는 못쓰것네?",
          "speaker_id": "1",
          "time": "20210904 13:53"
        },
        {
          "id": "MDRW2100030870.1.11",
          "form": "응응 온라인 막는다는 것 같아",
          "original_form": "응응 온라인 막는다는 것 같아",
          "speaker_id": "2",
          "time": "20210904 13:54"
        },
        {
          "id": "MDRW2100030870.1.12",
          "form": "그리고 자기 주소지 있는 곳에서만 사용 가능하대",
          "original_form": "그리고 자기 주소지 있는 곳에서만 사용 가능하대",
          "speaker_id": "2",
          "time": "20210904 13:54"
        },
        {
          "id": "MDRW2100030870.1.13",
          "form": "헐 레알?",
          "original_form": "헐 레알?",
          "speaker_id": "1",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030870.1.14",
          "form": "어디? 동까지만인가?",
          "original_form": "어디? 동까지만인가?",
          "speaker_id": "1",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030870.1.15",
          "form": "구단위도 되나??",
          "original_form": "구단위도 되나??",
          "speaker_id": "1",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030870.1.16",
          "form": "서울은 구 단위인데",
          "original_form": "서울은 구 단위인데",
          "speaker_id": "2",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100030870.1.17",
          "form": "청주는 청주 자체로 나간다고 본 것 같아",
          "original_form": "청주는 청주 자체로 나간다고 본 것 같아",
          "speaker_id": "2",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030870.1.18",
          "form": "내가 문제지 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "내가 문제지 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030870.1.19",
          "form": "아하 구단위 시단위",
          "original_form": "아하 구단위 시단위",
          "speaker_id": "1",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030870.1.20",
          "form": "이정돈가보넹",
          "original_form": "이정돈가보넹",
          "speaker_id": "1",
          "time": "20210904 14:09"
        },
        {
          "id": "MDRW2100030870.1.21",
          "form": "스타벜스 슬프겟다",
          "original_form": "스타벜스 슬프겟다",
          "speaker_id": "1",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100030870.1.22",
          "form": "뭐 스타벅스는 안그래도 많이 팔리니까",
          "original_form": "뭐 스타벅스는 안그래도 많이 팔리니까",
          "speaker_id": "2",
          "time": "20210904 14:10"
        },
        {
          "id": "MDRW2100030870.1.23",
          "form": "그래도 포함돼야 쓸곳이 있긴한데",
          "original_form": "그래도 포함돼야 쓸곳이 있긴한데",
          "speaker_id": "2",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030870.1.24",
          "form": "특히 스타벅스",
          "original_form": "특히 스타벅스",
          "speaker_id": "2",
          "time": "20210904 14:11"
        },
        {
          "id": "MDRW2100030870.1.25",
          "form": "아근데 난저번에 스벅카드 충전해서",
          "original_form": "아근데 난저번에 스벅카드 충전해서",
          "speaker_id": "1",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100030870.1.26",
          "form": "어차피 카드쓰니까",
          "original_form": "어차피 카드쓰니까",
          "speaker_id": "1",
          "time": "20210904 16:49"
        },
        {
          "id": "MDRW2100030870.1.27",
          "form": "글킨하다",
          "original_form": "글킨하다",
          "speaker_id": "1",
          "time": "20210904 16:50"
        },
        {
          "id": "MDRW2100030870.1.28",
          "form": "근데  어디서신청하지",
          "original_form": "근데  어디서신청하지",
          "speaker_id": "1",
          "time": "20210904 16:50"
        },
        {
          "id": "MDRW2100030870.1.29",
          "form": "딱히 요즘 실적채우는 카드도 없구",
          "original_form": "딱히 요즘 실적채우는 카드도 없구",
          "speaker_id": "1",
          "time": "20210904 16:50"
        },
        {
          "id": "MDRW2100030870.1.30",
          "form": "카드 없으면 종이로도 받을 수 있고 선불 카드도 있고",
          "original_form": "카드 없으면 종이로도 받을 수 있고 선불 카드도 있고",
          "speaker_id": "2",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100030870.1.31",
          "form": "ㅇㅎㅇㅎ",
          "original_form": "ㅇㅎㅇㅎ",
          "speaker_id": "1",
          "time": "20210904 17:36"
        },
        {
          "id": "MDRW2100030870.1.32",
          "form": "근데 이디야는 되나?",
          "original_form": "근데 이디야는 되나?",
          "speaker_id": "1",
          "time": "20210904 17:37"
        },
        {
          "id": "MDRW2100030870.1.33",
          "form": "이디야는 가맹점 많아서",
          "original_form": "이디야는 가맹점 많아서",
          "speaker_id": "2",
          "time": "20210904 17:38"
        },
        {
          "id": "MDRW2100030870.1.34",
          "form": "될거야",
          "original_form": "될거야",
          "speaker_id": "2",
          "time": "20210904 17:38"
        },
        {
          "id": "MDRW2100030870.1.35",
          "form": "오홍 고맙",
          "original_form": "오홍 고맙",
          "speaker_id": "1",
