      "utterance": [
        {
          "id": "MDRW2100025701.1.1",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 16:09"
        },
        {
          "id": "MDRW2100025701.1.2",
          "form": "최근에 공연 전시회 가신게 있으신가요?",
          "original_form": "최근에 공연 전시회 가신게 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 16:10"
        },
        {
          "id": "MDRW2100025701.1.3",
          "form": "아뇨 요즘엔 많이 안가서요",
          "original_form": "아뇨 요즘엔 많이 안가서요",
          "speaker_id": "2",
          "time": "20210901 16:54"
        },
        {
          "id": "MDRW2100025701.1.4",
          "form": "저는 요즘 이건희 미술 전시회 가고 싶어요",
          "original_form": "저는 요즘 이건희 미술 전시회 가고 싶어요",
          "speaker_id": "1",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100025701.1.5",
          "form": "서울이라 아주 결심하고 가야하긴하지만 ㅠ",
          "original_form": "서울이라 아주 결심하고 가야하긴하지만 ㅠ",
          "speaker_id": "1",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100025701.1.6",
          "form": "지방사시나봐요 저도그런데..",
          "original_form": "지방사시나봐요 저도그런데..",
          "speaker_id": "2",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100025701.1.7",
          "form": "네 서울엔 안살아요 ㅠ",
          "original_form": "네 서울엔 안살아요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 16:55"
        },
        {
          "id": "MDRW2100025701.1.8",
          "form": "요즘 코로나라 또 입장제한도 있더라구요",
          "original_form": "요즘 코로나라 또 입장제한도 있더라구요",
          "speaker_id": "1",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100025701.1.9",
          "form": "인터넷 신청해야하던데",
          "original_form": "인터넷 신청해야하던데",
          "speaker_id": "1",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100025701.1.10",
          "form": "안해봤지만 그것도 빡실거같아요",
          "original_form": "안해봤지만 그것도 빡실거같아요",
          "speaker_id": "1",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100025701.1.11",
          "form": "네 번거롭겠어요",
          "original_form": "네 번거롭겠어요",
          "speaker_id": "2",
          "time": "20210901 16:56"
        },
        {
          "id": "MDRW2100025701.1.12",
          "form": "좋아하는 작가있으세요?",
          "original_form": "좋아하는 작가있으세요?",
          "speaker_id": "2",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100025701.1.13",
          "form": "요즘 작가는 잘 모르고 ㅠ 모네? 는 좋아요",
          "original_form": "요즘 작가는 잘 모르고 ㅠ 모네? 는 좋아요",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100025701.1.14",
          "form": "뭔가 그냥 그림이 맘에들어요",
          "original_form": "뭔가 그냥 그림이 맘에들어요",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100025701.1.15",
          "form": "name2님은 있으세요?",
          "original_form": "&name2&님은 있으세요?",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100025701.1.16",
          "form": "아뇨 할말이 별로 없어서요 ㅋㅋ",
          "original_form": "아뇨 할말이 별로 없어서요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025701.1.17",
          "form": "그쵸 ㅠㅠ",
          "original_form": "그쵸 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 16:58"
        },
        {
          "id": "MDRW2100025701.1.18",
          "form": "음 그럼 기억나는 아무거나 말해볼까요",
          "original_form": "음 그럼 기억나는 아무거나 말해볼까요",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025701.1.19",
          "form": "솔직히 저는 미술로는 이말저말 갖다붙이면서",
          "original_form": "솔직히 저는 미술로는 이말저말 갖다붙이면서",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025701.1.20",
          "form": "책보다가 알았는데 우리나라에서 가장 인기있는 화가가 빈센트 반 고흐래요 ㅋㅋ",
          "original_form": "책보다가 알았는데 우리나라에서 가장 인기있는 화가가 빈센트 반 고흐래요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025701.1.21",
          "form": "뻥튀기하는게 심한거같아요",
          "original_form": "뻥튀기하는게 심한거같아요",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025701.1.22",
          "form": "어 그런거같아요 저도",
          "original_form": "어 그런거같아요 저도",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100025701.1.23",
          "form": "예술성이 뭔지 잘 모르겠어요 ㅠ",
          "original_form": "예술성이 뭔지 잘 모르겠어요 ㅠ",
