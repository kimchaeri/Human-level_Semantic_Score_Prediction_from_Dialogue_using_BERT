      "utterance": [
        {
          "id": "MDRW2100006781.1.1",
          "form": "안녕하세요 반갑습니다",
          "original_form": "안녕하세요 반갑습니다",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100006781.1.2",
          "form": "날씨가 선선해지면서 가을이 성큼 다가왔습니다",
          "original_form": "날씨가 선선해지면서 가을이 성큼 다가왔습니다",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100006781.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100006781.1.4",
          "form": "가을이오면 어느 것이 가장 기대되시나요?",
          "original_form": "가을이오면 어느 것이 가장 기대되시나요?",
          "speaker_id": "1",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100006781.1.5",
          "form": "저도 오늘 긴팔 입고 출근했어요",
          "original_form": "저도 오늘 긴팔 입고 출근했어요",
          "speaker_id": "2",
          "time": "20210908 09:13"
        },
        {
          "id": "MDRW2100006781.1.6",
          "form": "긴팔 입어야 합니다",
          "original_form": "긴팔 입어야 합니다",
          "speaker_id": "1",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100006781.1.7",
          "form": "기대라?  그런건 전 없는데요ㅋ",
          "original_form": "기대라?  그런건 전 없는데요ㅋ",
          "speaker_id": "2",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100006781.1.8",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100006781.1.9",
          "form": "환절기에 감기 걸리기 쉽상이지요",
          "original_form": "환절기에 감기 걸리기 쉽상이지요",
          "speaker_id": "1",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100006781.1.10",
          "form": "저는 가을 먹거리가 기대됩니다",
          "original_form": "저는 가을 먹거리가 기대됩니다",
          "speaker_id": "1",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100006781.1.11",
          "form": "특히 감을 좋아하지요",
          "original_form": "특히 감을 좋아하지요",
          "speaker_id": "1",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100006781.1.12",
          "form": "아 그러시구나..  저는 그냥저냥ㅋ",
          "original_form": "아 그러시구나..  저는 그냥저냥ㅋ",
          "speaker_id": "2",
          "time": "20210908 09:15"
        },
        {
          "id": "MDRW2100006781.1.13",
          "form": "어느 계절을 가장 좋아하시나요?",
          "original_form": "어느 계절을 가장 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210908 09:15"
        },
        {
          "id": "MDRW2100006781.1.14",
          "form": "저도 봄이나 가을 좋아해요",
          "original_form": "저도 봄이나 가을 좋아해요",
          "speaker_id": "2",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100006781.1.15",
          "form": "날씨도 좋고 놀러가기도 좋고ㅋ",
          "original_form": "날씨도 좋고 놀러가기도 좋고ㅋ",
          "speaker_id": "2",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100006781.1.16",
          "form": "맞습니다",
          "original_form": "맞습니다",
          "speaker_id": "1",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100006781.1.17",
          "form": "다니기 좋은 계절이지요",
          "original_form": "다니기 좋은 계절이지요",
          "speaker_id": "1",
          "time": "20210908 09:16"
        },
        {
          "id": "MDRW2100006781.1.18",
          "form": "코로나만 아니면 많이 다녔을텐데요 아쉽네요",
          "original_form": "코로나만 아니면 많이 다녔을텐데요 아쉽네요",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100006781.1.19",
          "form": "그러게요 코로나 정말 싫다",
          "original_form": "그러게요 코로나 정말 싫다",
          "speaker_id": "2",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100006781.1.20",
          "form": "작년부터 여행을 못하고 있어요",
          "original_form": "작년부터 여행을 못하고 있어요",
          "speaker_id": "1",
          "time": "20210908 09:17"
        },
        {
          "id": "MDRW2100006781.1.21",
          "form": "동네만 좀 다니는 정도라서 많이 답답하네요",
          "original_form": "동네만 좀 다니는 정도라서 많이 답답하네요",
          "speaker_id": "1",
          "time": "20210908 09:17"
        }
      ]
