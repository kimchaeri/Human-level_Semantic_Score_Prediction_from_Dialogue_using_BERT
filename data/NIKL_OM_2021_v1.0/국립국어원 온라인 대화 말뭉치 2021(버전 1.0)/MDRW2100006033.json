      "utterance": [
        {
          "id": "MDRW2100006033.1.1",
          "form": "집놀이?해보신적 있나요",
          "original_form": "집놀이?해보신적 있나요",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100006033.1.2",
          "form": "집놀이가 머에요",
          "original_form": "집놀이가 머에요",
          "speaker_id": "2",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100006033.1.3",
          "form": "음..저도 첨들어보네요",
          "original_form": "음..저도 첨들어보네요",
          "speaker_id": "1",
          "time": "20210907 19:45"
        },
        {
          "id": "MDRW2100006033.1.4",
          "form": "홈루덴스족",
          "original_form": "홈루덴스족",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100006033.1.5",
          "form": "집에서 영화보고 안에서 노는건가봐요 밖에 안나가구",
          "original_form": "집에서 영화보고 안에서 노는건가봐요 밖에 안나가구",
          "speaker_id": "2",
          "time": "20210907 19:46"
        },
        {
          "id": "MDRW2100006033.1.6",
          "form": "아하ㅋㅋ 저거할려면 혹시",
          "original_form": "아하ㅋㅋ 저거할려면 혹시",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100006033.1.7",
          "form": "집에 음향시설같은거 빵빵하게해놔야할까요?",
          "original_form": "집에 음향시설같은거 빵빵하게해놔야할까요?",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100006033.1.8",
          "form": "이미 집콕은 코로나이후로 하고있는데",
          "original_form": "이미 집콕은 코로나이후로 하고있는데",
          "speaker_id": "1",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100006033.1.9",
          "form": "그긔랑은 다른거겠죠",
          "original_form": "그긔랑은 다른거겠죠",
          "speaker_id": "1",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100006033.1.10",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100006033.1.11",
          "form": "집에 빠방허게 채우는건가",
          "original_form": "집에 빠방허게 채우는건가",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100006033.1.12",
          "form": "저도 그래서 이번에 방 인테리어 했어요",
          "original_form": "저도 그래서 이번에 방 인테리어 했어요",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100006033.1.13",
          "form": "아무래도 그래야 집에서 보는맛이날텐데",
          "original_form": "아무래도 그래야 집에서 보는맛이날텐데",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100006033.1.14",
          "form": "그러게요 서라운드 음향 빵빵",
          "original_form": "그러게요 서라운드 음향 빵빵",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100006033.1.15",
          "form": "와와 어떤 인테리어요?",
          "original_form": "와와 어떤 인테리어요?",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100006033.1.16",
          "form": "이불침구 다 바꾸구",
          "original_form": "이불침구 다 바꾸구",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100006033.1.17",
          "form": "옷걸이 통일",
          "original_form": "옷걸이 통일",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100006033.1.18",
          "form": "이케아 조명과 서랍",
          "original_form": "이케아 조명과 서랍",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100006033.1.19",
          "form": "와 이케아면직접 조립하신건가요?",
          "original_form": "와 이케아면직접 조립하신건가요?",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100006033.1.20",
          "form": "네네 마자요 서랍은 철제인대",
          "original_form": "네네 마자요 서랍은 철제인대",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100006033.1.21",
          "form": "철판을 직접 구부려서 접어야하더라고요 힘들었어요",
          "original_form": "철판을 직접 구부려서 접어야하더라고요 힘들었어요",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100006033.1.22",
          "form": "우와 능력자시네요",
          "original_form": "우와 능력자시네요",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100006033.1.23",
          "form": "유툽같은거보고 하셨어요?",
          "original_form": "유툽같은거보고 하셨어요?",
          "speaker_id": "1",
          "time": "20210907 20:11"
        }
      ]
