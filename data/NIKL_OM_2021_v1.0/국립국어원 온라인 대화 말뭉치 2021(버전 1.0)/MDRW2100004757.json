      "utterance": [
        {
          "id": "MDRW2100004757.1.1",
          "form": "음료좋아하세요??",
          "original_form": "음료좋아하세요??",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004757.1.2",
          "form": "전 이제서야 깨달았는대 카페인먹으면 배탈나요",
          "original_form": "전 이제서야 깨달았는대 카페인먹으면 배탈나요",
          "speaker_id": "2",
          "time": "20210821 18:23"
        },
        {
          "id": "MDRW2100004757.1.3",
          "form": "음료 좋아요 너무 단건 요즘 잘못먹겠더라구요",
          "original_form": "음료 좋아요 너무 단건 요즘 잘못먹겠더라구요",
          "speaker_id": "1",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004757.1.4",
          "form": "저도 카페인 많이먹으면 어지럽고 소화가 안돼요",
          "original_form": "저도 카페인 많이먹으면 어지럽고 소화가 안돼요",
          "speaker_id": "1",
          "time": "20210821 18:28"
        },
        {
          "id": "MDRW2100004757.1.5",
          "form": "카페인 무서운자식~~~",
          "original_form": "카페인 무서운자식~~~",
          "speaker_id": "2",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004757.1.6",
          "form": "그래서 카페가면 그냥 에이드종류먹어요",
          "original_form": "그래서 카페가면 그냥 에이드종류먹어요",
          "speaker_id": "2",
          "time": "20210821 18:29"
        },
        {
          "id": "MDRW2100004757.1.7",
          "form": "에이드 요즘 같은 무덥고 습한 날씨에 상큼하고 조아요~~!",
          "original_form": "에이드 요즘 같은 무덥고 습한 날씨에 상큼하고 조아요~~!",
          "speaker_id": "1",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004757.1.8",
          "form": "저는 티음료를 마셔요",
          "original_form": "저는 티음료를 마셔요",
          "speaker_id": "1",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004757.1.9",
          "form": "티~~ 무슨티 자주 마시세용??",
          "original_form": "티~~ 무슨티 자주 마시세용??",
          "speaker_id": "2",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004757.1.10",
          "form": "프리티~^^",
          "original_form": "프리티~^^",
          "speaker_id": "2",
          "time": "20210821 18:31"
        },
        {
          "id": "MDRW2100004757.1.11",
          "form": "저는 큐티 마셔요ㅎㅎㅎ",
          "original_form": "저는 큐티 마셔요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:34"
        },
        {
          "id": "MDRW2100004757.1.12",
          "form": "홍차 조아해요",
          "original_form": "홍차 조아해요",
          "speaker_id": "1",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004757.1.13",
          "form": "홍차~^^저는 밀크티가조아요",
          "original_form": "홍차~^^저는 밀크티가조아요",
          "speaker_id": "2",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004757.1.14",
          "form": "달달하게만든밀크티",
          "original_form": "달달하게만든밀크티",
          "speaker_id": "2",
          "time": "20210821 18:35"
        },
        {
          "id": "MDRW2100004757.1.15",
          "form": "밀크티 짱이죠 집에서도 자주해먹는답니다",
          "original_form": "밀크티 짱이죠 집에서도 자주해먹는답니다",
          "speaker_id": "1",
          "time": "20210821 18:36"
        },
        {
          "id": "MDRW2100004757.1.16",
          "form": "음식은 어떤거 조아하시나요",
          "original_form": "음식은 어떤거 조아하시나요",
          "speaker_id": "1",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004757.1.17",
          "form": "음식은 삼겹살좋어해요",
          "original_form": "음식은 삼겹살좋어해요",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004757.1.18",
          "form": "삼겹살은 저의 인샹",
          "original_form": "삼겹살은 저의 인샹",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004757.1.19",
          "form": "삼겹살을 누가 안좋아할까요",
          "original_form": "삼겹살을 누가 안좋아할까요",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004757.1.20",
          "form": "야미한 삼겹살",
          "original_form": "야미한 삼겹살",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004757.1.21",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004757.1.22",
          "form": "음~~~~~~~",
          "original_form": "음~~~~~~~",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004757.1.23",
          "form": "구울때 후추가득뿌리면",
          "original_form": "구울때 후추가득뿌리면",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004757.1.24",
          "form": "진짜맛나요",
          "original_form": "진짜맛나요",
          "speaker_id": "2",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004757.1.25",
          "form": "캬악 상상만해도 행벅하네요",
          "original_form": "캬악 상상만해도 행벅하네요",
          "speaker_id": "1",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004757.1.26",
          "form": "고기 먹고싶어요ㅜㅜ",
          "original_form": "고기 먹고싶어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 18:46"
        },
        {
          "id": "MDRW2100004757.1.27",
          "form": "하....집근처마트에서돼지잡는날에는 삼겹살을 반값하더라고요....",
          "original_form": "하....집근처마트에서돼지잡는날에는 삼겹살을 반값하더라고요....",
          "speaker_id": "2",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004757.1.28",
          "form": "왕창사서 냉동실애 얼려뒀어요...",
          "original_form": "왕창사서 냉동실애 얼려뒀어요...",
          "speaker_id": "2",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004757.1.29",
          "form": "행복은 돈으로 살수있다...!!!",
          "original_form": "행복은 돈으로 살수있다...!!!",
          "speaker_id": "2",
          "time": "20210821 18:47"
        },
        {
          "id": "MDRW2100004757.1.30",
          "form": "와 정말 현명한 선택이세요",
          "original_form": "와 정말 현명한 선택이세요",
          "speaker_id": "1",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004757.1.31",
          "form": "행벅은 돈으로 살수있다!",
          "original_form": "행벅은 돈으로 살수있다!",
