        },
        {
          "id": "MDRW2100013394.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210825 14:25"
        },
        {
          "id": "MDRW2100013394.1.3",
          "form": "가고싶은 여행지 있으신가요",
          "original_form": "가고싶은 여행지 있으신가요",
          "speaker_id": "1",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013394.1.4",
          "form": "제주도 가고시퍼요",
          "original_form": "제주도 가고시퍼요",
          "speaker_id": "2",
          "time": "20210825 14:26"
        },
        {
          "id": "MDRW2100013394.1.5",
          "form": "제주도 너무 좋죠",
          "original_form": "제주도 너무 좋죠",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013394.1.6",
          "form": "맑은 날씨 예쁜 바다 생각하면 너무 좋은 곳인거같아요",
          "original_form": "맑은 날씨 예쁜 바다 생각하면 너무 좋은 곳인거같아요",
          "speaker_id": "1",
          "time": "20210825 14:27"
        },
        {
          "id": "MDRW2100013394.1.7",
          "form": "맞아요 못먹어본 음식들도 많구요",
          "original_form": "맞아요 못먹어본 음식들도 많구요",
          "speaker_id": "2",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013394.1.8",
          "form": "어떤음식을 먹고싶은가요",
          "original_form": "어떤음식을 먹고싶은가요",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013394.1.9",
          "form": "음..제주 유명한게 고기국수?",
          "original_form": "음..제주 유명한게 고기국수?",
          "speaker_id": "1",
          "time": "20210825 14:28"
        },
        {
          "id": "MDRW2100013394.1.10",
          "form": "사실은",
          "original_form": "사실은",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013394.1.11",
          "form": "사실은?",
          "original_form": "사실은?",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013394.1.12",
          "form": "연돈 치즈까츠가 너무 먹어보고싶어요",
          "original_form": "연돈 치즈까츠가 너무 먹어보고싶어요",
          "speaker_id": "2",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013394.1.13",
          "form": "저도 그거 너무 궁금해요",
          "original_form": "저도 그거 너무 궁금해요",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013394.1.14",
          "form": "이번생에는 먹어볼 수 있으려나.",
          "original_form": "이번생에는 먹어볼 수 있으려나.",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100013394.1.15",
          "form": "아직도 줄서서 먹는다더라구요",
          "original_form": "아직도 줄서서 먹는다더라구요",
          "speaker_id": "1",
          "time": "20210825 14:30"
        },
        {
          "id": "MDRW2100013394.1.16",
          "form": "정말 맛있어보이고 궁금하고 흑흑",
          "original_form": "정말 맛있어보이고 궁금하고 흑흑",
          "speaker_id": "2",
          "time": "20210825 14:31"
        },
        {
          "id": "MDRW2100013394.1.17",
          "form": "진짜 너무 궁금한 맛입니다",
          "original_form": "진짜 너무 궁금한 맛입니다",
