      "utterance": [
        {
          "id": "MDRW2100021792.1.1",
          "form": "아 맞다맞다 저 훠궈를 아주 좋아해요",
          "original_form": "아 맞다맞다 저 훠궈를 아주 좋아해요",
          "speaker_id": "1",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021792.1.2",
          "form": "그래ㅐ서 거기있는 재료는 하나씩 가져와서 다 맛을 봐요",
          "original_form": "그래ㅐ서 거기있는 재료는 하나씩 가져와서 다 맛을 봐요",
          "speaker_id": "1",
          "time": "20210828 22:36"
        },
        {
          "id": "MDRW2100021792.1.3",
          "form": "아맞다 훠궈였어요",
          "original_form": "아맞다 훠궈였어요",
          "speaker_id": "2",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100021792.1.4",
          "form": "저는 토마토탕을 엄청 좋아합니다",
          "original_form": "저는 토마토탕을 엄청 좋아합니다",
          "speaker_id": "2",
          "time": "20210828 22:38"
        },
        {
          "id": "MDRW2100021792.1.5",
          "form": "전 토마토탕을 좋아하는데 케챱에 물탄 탕은 싫어하고 하이디라오 토마토탕 정말 좋아해요",
          "original_form": "전 토마토탕을 좋아하는데 케챱에 물탄 탕은 싫어하고 하이디라오 토마토탕 정말 좋아해요",
          "speaker_id": "1",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021792.1.6",
          "form": "하이디라오 토마토탕 제대로죠",
          "original_form": "하이디라오 토마토탕 제대로죠",
          "speaker_id": "2",
          "time": "20210828 22:41"
        },
        {
          "id": "MDRW2100021792.1.7",
          "form": "하이디라오 재료 바도 너무 좋은데",
          "original_form": "하이디라오 재료 바도 너무 좋은데",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100021792.1.8",
          "form": "비싸서 자주는 못가요",
          "original_form": "비싸서 자주는 못가요",
          "speaker_id": "1",
          "time": "20210828 22:50"
        },
        {
          "id": "MDRW2100021792.1.9",
          "form": "어떤 가격대 훠궈를 자주가세요?",
          "original_form": "어떤 가격대 훠궈를 자주가세요?",
          "speaker_id": "1",
          "time": "20210828 22:55"
        },
        {
          "id": "MDRW2100021792.1.10",
          "form": "저도 하이디라오 자주 못가고 주로 무한리필집 가요...하하",
          "original_form": "저도 하이디라오 자주 못가고 주로 무한리필집 가요...하하",
          "speaker_id": "2",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021792.1.11",
          "form": "한 만오천우너 만팔천원 하는 집에 자주가요",
          "original_form": "한 만오천우너 만팔천원 하는 집에 자주가요",
          "speaker_id": "1",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021792.1.12",
          "form": "마음은 하이디라오에 있지만,,,",
          "original_form": "마음은 하이디라오에 있지만,,,",
          "speaker_id": "1",
          "time": "20210828 22:56"
        },
        {
          "id": "MDRW2100021792.1.13",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210828 22:57"
        },
        {
          "id": "MDRW2100021792.1.14",
          "form": "하이디라오 맨처음 갔을 때 가격에 놀라고 이제는 가끔 갑니다....",
          "original_form": "하이디라오 맨처음 갔을 때 가격에 놀라고 이제는 가끔 갑니다....",
          "speaker_id": "2",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100021792.1.15",
          "form": "비싼만큼 재료 질이 달라요 하아,,, 먹고 싶다",
          "original_form": "비싼만큼 재료 질이 달라요 하아,,, 먹고 싶다",
          "speaker_id": "1",
          "time": "20210828 22:58"
        },
        {
          "id": "MDRW2100021792.1.16",
          "form": "세일이벤트있을때 몇 번 갔어요",
          "original_form": "세일이벤트있을때 몇 번 갔어요",
          "speaker_id": "1",
          "time": "20210828 22:59"
        },
        {
          "id": "MDRW2100021792.1.17",
          "form": "완자같은것도 질차이 엄청나지 않나요",
          "original_form": "완자같은것도 질차이 엄청나지 않나요",
          "speaker_id": "2",
          "time": "20210828 23:02"
        },
        {
          "id": "MDRW2100021792.1.18",
          "form": "맞아요 좀 저가집은 어묵 같은데 하이디라오는 탱글 새우맛이 아주..",
          "original_form": "맞아요 좀 저가집은 어묵 같은데 하이디라오는 탱글 새우맛이 아주..",
          "speaker_id": "1",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100021792.1.19",
          "form": "고기질도 다르고 과일도 좋았어요",
          "original_form": "고기질도 다르고 과일도 좋았어요",
          "speaker_id": "1",
          "time": "20210828 23:04"
        }
      ]
