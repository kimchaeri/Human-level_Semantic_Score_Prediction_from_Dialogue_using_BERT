      "utterance": [
        {
          "id": "MDRW2100036047.1.1",
          "form": "당근마켓에 올린 게 연락이 안오네요ㅠ",
          "original_form": "당근마켓에 올린 게 연락이 안오네요ㅠ",
          "speaker_id": "1",
          "time": "20210830 15:08"
        },
        {
          "id": "MDRW2100036047.1.2",
          "form": "가격 내리게돼요..",
          "original_form": "가격 내리게돼요..",
          "speaker_id": "2",
          "time": "20210830 15:08"
        },
        {
          "id": "MDRW2100036047.1.3",
          "form": "ㅜㅗㅠ....",
          "original_form": "ㅜㅗㅠ....",
          "speaker_id": "2",
          "time": "20210830 15:09"
        },
        {
          "id": "MDRW2100036047.1.4",
          "form": "맞아요ㅜㅠ",
          "original_form": "맞아요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210830 15:24"
        },
        {
          "id": "MDRW2100036047.1.5",
          "form": "내리려다 여기서 만난분이 팁을 주셔서 조금 더 참아보기로 했어요",
          "original_form": "내리려다 여기서 만난분이 팁을 주셔서 조금 더 참아보기로 했어요",
          "speaker_id": "1",
          "time": "20210830 15:25"
        },
        {
          "id": "MDRW2100036047.1.6",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "2",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100036047.1.7",
          "form": "일부러 찜 해놓고",
          "original_form": "일부러 찜 해놓고",
          "speaker_id": "2",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100036047.1.8",
          "form": "가격내릴때까지 버티는 사람들 있대요",
          "original_form": "가격내릴때까지 버티는 사람들 있대요",
          "speaker_id": "2",
          "time": "20210830 15:51"
        },
        {
          "id": "MDRW2100036047.1.9",
          "form": "우와.. 그 하트가 그런거였나요..",
          "original_form": "우와.. 그 하트가 그런거였나요..",
          "speaker_id": "1",
          "time": "20210830 15:56"
        },
        {
          "id": "MDRW2100036047.1.10",
          "form": "하트 눌려서 좋아했는데!",
          "original_form": "하트 눌려서 좋아했는데!",
          "speaker_id": "1",
          "time": "20210830 15:56"
        },
        {
          "id": "MDRW2100036047.1.11",
          "form": "그냥 진짜 순수하게 찜하는 사람들도 있고",
          "original_form": "그냥 진짜 순수하게 찜하는 사람들도 있고",
          "speaker_id": "2",
          "time": "20210830 15:57"
        },
        {
          "id": "MDRW2100036047.1.12",
          "form": "간보려고 찜하는 사람들도 있대요! ㅠ",
          "original_form": "간보려고 찜하는 사람들도 있대요! ㅠ",
          "speaker_id": "2",
          "time": "20210830 15:57"
        },
        {
          "id": "MDRW2100036047.1.13",
          "form": "우와.. 그래서 연락이 안오는거군오ㅜㅠ",
          "original_form": "우와.. 그래서 연락이 안오는거군오ㅜㅠ",
          "speaker_id": "1",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100036047.1.14",
          "form": "거래도 그냥하는게 아니네요",
          "original_form": "거래도 그냥하는게 아니네요",
          "speaker_id": "1",
          "time": "20210830 16:40"
        },
        {
          "id": "MDRW2100036047.1.15",
          "form": "그냥 버텨보세요. !!!",
          "original_form": "그냥 버텨보세요. !!!",
          "speaker_id": "2",
          "time": "20210830 16:41"
        },
        {
          "id": "MDRW2100036047.1.16",
          "form": "넵!! 여유 가지고 기다려봐야겠어요!",
          "original_form": "넵!! 여유 가지고 기다려봐야겠어요!",
          "speaker_id": "1",
          "time": "20210830 18:06"
        },
        {
          "id": "MDRW2100036047.1.17",
          "form": "넨 좋아용 ㅎㅎ",
          "original_form": "넨 좋아용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 18:09"
        },
        {
          "id": "MDRW2100036047.1.18",
          "form": "급하게팔아야하는게 아니라면",
          "original_form": "급하게팔아야하는게 아니라면",
          "speaker_id": "2",
          "time": "20210830 18:09"
        },
        {
          "id": "MDRW2100036047.1.19",
          "form": "네!!! 그래야겠어요",
          "original_form": "네!!! 그래야겠어요",
          "speaker_id": "1",
          "time": "20210830 18:14"
        },
        {
          "id": "MDRW2100036047.1.20",
          "form": "물건 사보신적두 있으세요?",
          "original_form": "물건 사보신적두 있으세요?",
          "speaker_id": "2",
          "time": "20210830 18:38"
        },
        {
          "id": "MDRW2100036047.1.21",
          "form": "네!! 고양이 용품 샀어용ㅋㅋ",
          "original_form": "네!! 고양이 용품 샀어용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100036047.1.22",
          "form": "사보신거 있으세요??",
          "original_form": "사보신거 있으세요??",
          "speaker_id": "1",
          "time": "20210830 18:42"
        },
        {
          "id": "MDRW2100036047.1.23",
          "form": "저는 파는것만해봤어요!!",
          "original_form": "저는 파는것만해봤어요!!",
          "speaker_id": "2",
          "time": "20210830 18:43"
        }
      ]
