      "utterance": [
        {
          "id": "MDRW2100003943.1.1",
          "form": "좋아하는 연예인 있으신가요?",
          "original_form": "좋아하는 연예인 있으신가요?",
          "speaker_id": "1",
          "time": "20210821 14:05"
        },
        {
          "id": "MDRW2100003943.1.2",
          "form": "저 연예인은 다 그저 그런데 아이린ㅋㅋ얼굴만 좋아해요 몰래몰래",
          "original_form": "저 연예인은 다 그저 그런데 아이린ㅋㅋ얼굴만 좋아해요 몰래몰래",
          "speaker_id": "2",
          "time": "20210821 14:06"
        },
        {
          "id": "MDRW2100003943.1.3",
          "form": "아이린 예쁘죠",
          "original_form": "아이린 예쁘죠",
          "speaker_id": "1",
          "time": "20210821 14:06"
        },
        {
          "id": "MDRW2100003943.1.4",
          "form": "그쪽은 누구 좋아해요?",
          "original_form": "그쪽은 누구 좋아해요?",
          "speaker_id": "2",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003943.1.5",
          "form": "음 저는 한지민요ㅋㅋ",
          "original_form": "음 저는 한지민요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 14:12"
        },
        {
          "id": "MDRW2100003943.1.6",
          "form": "오ㅋㅋㅋ약간 어른스러운 느낌?",
          "original_form": "오ㅋㅋㅋ약간 어른스러운 느낌?",
          "speaker_id": "2",
          "time": "20210821 14:13"
        },
        {
          "id": "MDRW2100003943.1.7",
          "form": "음 아뇨 귀엽잖아요ㅋ",
          "original_form": "음 아뇨 귀엽잖아요ㅋ",
          "speaker_id": "1",
          "time": "20210821 14:17"
        },
        {
          "id": "MDRW2100003943.1.8",
          "form": "아하...그렇긴해요ㅎㅎ",
          "original_form": "아하...그렇긴해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003943.1.9",
          "form": "영화는뭐좋아하세용",
          "original_form": "영화는뭐좋아하세용",
          "speaker_id": "2",
          "time": "20210821 14:18"
        },
        {
          "id": "MDRW2100003943.1.10",
          "form": "음 요즘은 거의 안 봐서",
          "original_form": "음 요즘은 거의 안 봐서",
          "speaker_id": "1",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003943.1.11",
          "form": "추천좀 해주세요",
          "original_form": "추천좀 해주세요",
          "speaker_id": "1",
          "time": "20210821 14:20"
        },
        {
          "id": "MDRW2100003943.1.12",
          "form": "저 음 걍 옛날영화 좋아해요",
          "original_form": "저 음 걍 옛날영화 좋아해요",
          "speaker_id": "2",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003943.1.13",
          "form": "저수지의 개들 그런거",
          "original_form": "저수지의 개들 그런거",
          "speaker_id": "2",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003943.1.14",
          "form": "오 쿠엔틴 영화네요",
          "original_form": "오 쿠엔틴 영화네요",
          "speaker_id": "1",
          "time": "20210821 14:21"
        },
        {
          "id": "MDRW2100003943.1.15",
          "form": "작가주의!",
          "original_form": "작가주의!",
          "speaker_id": "1",
          "time": "20210821 14:22"
        },
        {
          "id": "MDRW2100003943.1.16",
          "form": "오 맞아요ㅋㅋ",
          "original_form": "오 맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003943.1.17",
          "form": "대부분 킬빌만 아시던 ㅜ",
          "original_form": "대부분 킬빌만 아시던 ㅜ",
          "speaker_id": "2",
          "time": "20210821 14:23"
        },
        {
          "id": "MDRW2100003943.1.18",
          "form": "음 좋은 작품 많죠ㅋ",
          "original_form": "음 좋은 작품 많죠ㅋ",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003943.1.19",
          "form": "저수지의 개들도 재밌구요. 대부분 잔인하지만ㅋ",
          "original_form": "저수지의 개들도 재밌구요. 대부분 잔인하지만ㅋ",
          "speaker_id": "1",
          "time": "20210821 14:29"
        },
        {
          "id": "MDRW2100003943.1.20",
          "form": "ㅋㅋㅋ오 그래두 쿠엔틴 좋아하시네요",
          "original_form": "ㅋㅋㅋ오 그래두 쿠엔틴 좋아하시네요",
          "speaker_id": "2",
          "time": "20210821 14:30"
        },
        {
          "id": "MDRW2100003943.1.21",
          "form": "장고도 진짜 재밌어요",
          "original_form": "장고도 진짜 재밌어요",
          "speaker_id": "2",
          "time": "20210821 14:30"
        }
      ]
