      "utterance": [
        {
          "id": "MDRW2100031078.1.1",
          "form": "백신 맞으셨어요?",
          "original_form": "백신 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210904 16:20"
        },
        {
          "id": "MDRW2100031078.1.2",
          "form": "아직이요! 14일에 맞아요",
          "original_form": "아직이요! 14일에 맞아요",
          "speaker_id": "2",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100031078.1.3",
          "form": "저는 일차만 맞았어요",
          "original_form": "저는 일차만 맞았어요",
          "speaker_id": "1",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100031078.1.4",
          "form": "어떤거 맞으세요?",
          "original_form": "어떤거 맞으세요?",
          "speaker_id": "1",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100031078.1.5",
          "form": "오, 벌써 맞으셨구나",
          "original_form": "오, 벌써 맞으셨구나",
          "speaker_id": "2",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100031078.1.6",
          "form": "저 20대니까 화이자 아닌가 싶어요",
          "original_form": "저 20대니까 화이자 아닌가 싶어요",
          "speaker_id": "2",
          "time": "20210904 16:22"
        },
        {
          "id": "MDRW2100031078.1.7",
          "form": "저도 화이자요 ㅎㅎ",
          "original_form": "저도 화이자요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 16:22"
        },
        {
          "id": "MDRW2100031078.1.8",
          "form": "name1님 주변분들은 백신 부작용 없던가요?",
          "original_form": "&name1&님 주변분들은 백신 부작용 없던가요?",
          "speaker_id": "2",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100031078.1.9",
          "form": "아직까지는 없었어요",
          "original_form": "아직까지는 없었어요",
          "speaker_id": "1",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100031078.1.10",
          "form": "다행히도",
          "original_form": "다행히도",
          "speaker_id": "1",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100031078.1.11",
          "form": "부작용 있으신분 계셨나요?",
          "original_form": "부작용 있으신분 계셨나요?",
          "speaker_id": "1",
          "time": "20210904 16:37"
        },
        {
          "id": "MDRW2100031078.1.12",
          "form": "진통제를 하루 복용 최대치까지 먹은 경우는 두명인가 있었어요",
          "original_form": "진통제를 하루 복용 최대치까지 먹은 경우는 두명인가 있었어요",
          "speaker_id": "2",
          "time": "20210904 16:39"
        },
        {
          "id": "MDRW2100031078.1.13",
          "form": "엄청 아프셨나봐요ㄷㄷㄷ",
          "original_form": "엄청 아프셨나봐요ㄷㄷㄷ",
          "speaker_id": "1",
          "time": "20210904 16:40"
        },
        {
          "id": "MDRW2100031078.1.14",
          "form": "그러니까요ㅜㅜ",
          "original_form": "그러니까요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210904 16:42"
        },
        {
          "id": "MDRW2100031078.1.15",
          "form": "백신이 건강한거랑은 상관없는거 같아요",
          "original_form": "백신이 건강한거랑은 상관없는거 같아요",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031078.1.16",
          "form": "전 안건강해서 걱정했는데 괜찮더라구요",
          "original_form": "전 안건강해서 걱정했는데 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210904 16:43"
        },
        {
          "id": "MDRW2100031078.1.17",
          "form": "오히려 건강하면 탈나는 것 같기도해요",
          "original_form": "오히려 건강하면 탈나는 것 같기도해요",
          "speaker_id": "2",
          "time": "20210904 16:44"
        },
        {
          "id": "MDRW2100031078.1.18",
          "form": "그런거 같기도 하네요",
          "original_form": "그런거 같기도 하네요",
          "speaker_id": "1",
          "time": "20210904 16:45"
        }
