      "utterance": [
        {
          "id": "MDRW2100024797.1.1",
          "form": "집안일 하시나요",
          "original_form": "집안일 하시나요",
          "speaker_id": "1",
          "time": "20210831 18:48"
        },
        {
          "id": "MDRW2100024797.1.2",
          "form": "저녁먹고 있었어요",
          "original_form": "저녁먹고 있었어요",
          "speaker_id": "2",
          "time": "20210831 18:50"
        },
        {
          "id": "MDRW2100024797.1.3",
          "form": "아항 직접 해드시나요",
          "original_form": "아항 직접 해드시나요",
          "speaker_id": "1",
          "time": "20210831 18:50"
        },
        {
          "id": "MDRW2100024797.1.4",
          "form": "오늘은 해먹었어요",
          "original_form": "오늘은 해먹었어요",
          "speaker_id": "2",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100024797.1.5",
          "form": "비가와서 빨래도 안하고 청소도 안하게되네요",
          "original_form": "비가와서 빨래도 안하고 청소도 안하게되네요",
          "speaker_id": "2",
          "time": "20210831 18:51"
        },
        {
          "id": "MDRW2100024797.1.6",
          "form": "ㅋㅋㅋ 넘 귀찮죵 ㅠㅡㅠ 뭐해드셨나요",
          "original_form": "ㅋㅋㅋ 넘 귀찮죵 ㅠㅡㅠ 뭐해드셨나요",
          "speaker_id": "1",
          "time": "20210831 18:52"
        },
        {
          "id": "MDRW2100024797.1.7",
          "form": "마끼라고해야하나?  김에 밥과야채와 날치알 싸먹는거요",
          "original_form": "마끼라고해야하나?  김에 밥과야채와 날치알 싸먹는거요",
          "speaker_id": "2",
          "time": "20210831 18:53"
        },
        {
          "id": "MDRW2100024797.1.8",
          "form": "오 맛있겟당 ㅋㅋ",
          "original_form": "오 맛있겟당 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:54"
        },
        {
          "id": "MDRW2100024797.1.9",
          "form": "집안일 꿀템 있으신가요",
          "original_form": "집안일 꿀템 있으신가요",
          "speaker_id": "1",
          "time": "20210831 18:54"
        },
        {
          "id": "MDRW2100024797.1.10",
          "form": "로봇청소기나",
          "original_form": "로봇청소기나",
          "speaker_id": "1",
          "time": "20210831 18:54"
        },
        {
          "id": "MDRW2100024797.1.11",
          "form": "식기세척기등",
          "original_form": "식기세척기등",
          "speaker_id": "1",
          "time": "20210831 18:54"
        },
        {
          "id": "MDRW2100024797.1.12",
          "form": "있어요",
          "original_form": "있어요",
          "speaker_id": "2",
          "time": "20210831 18:56"
        },
        {
          "id": "MDRW2100024797.1.13",
          "form": "ㅋㅋㅋ 자랑해주세요 !",
          "original_form": "ㅋㅋㅋ 자랑해주세요 !",
          "speaker_id": "1",
          "time": "20210831 18:56"
        },
        {
          "id": "MDRW2100024797.1.14",
          "form": "로봇청소기는 살림이 많아서 자꾸 부딪혀서 안쓰게되네요",
          "original_form": "로봇청소기는 살림이 많아서 자꾸 부딪혀서 안쓰게되네요",
          "speaker_id": "2",
          "time": "20210831 18:57"
        },
        {
          "id": "MDRW2100024797.1.15",
          "form": "아하 ㅠ 저도 그거때문에 살 엄두가 안나요",
          "original_form": "아하 ㅠ 저도 그거때문에 살 엄두가 안나요",
          "speaker_id": "1",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100024797.1.16",
          "form": "살림정리되면 쓰게 될것같아요",
          "original_form": "살림정리되면 쓰게 될것같아요",
          "speaker_id": "2",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100024797.1.17",
          "form": "제일 잘쓰는건 건조기네요",
          "original_form": "제일 잘쓰는건 건조기네요",
          "speaker_id": "2",
          "time": "20210831 18:59"
        },
        {
          "id": "MDRW2100024797.1.18",
          "form": "우오 건조기...부러워요 건조기",
          "original_form": "우오 건조기...부러워요 건조기",
          "speaker_id": "1",
          "time": "20210831 19:01"
        },
        {
          "id": "MDRW2100024797.1.19",
          "form": "꼭 추천해요",
          "original_form": "꼭 추천해요",
          "speaker_id": "2",
          "time": "20210831 19:01"
        },
        {
          "id": "MDRW2100024797.1.20",
          "form": "건조기1순위템.. ㅜㅜ에어드레서? 이런건 없으신가요",
          "original_form": "건조기1순위템.. ㅜㅜ에어드레서? 이런건 없으신가요",
          "speaker_id": "1",
          "time": "20210831 19:02"
        },
        {
          "id": "MDRW2100024797.1.21",
          "form": "사고싶은 템중 하나예요",
          "original_form": "사고싶은 템중 하나예요",
          "speaker_id": "2",
          "time": "20210831 19:03"
        },
        {
          "id": "MDRW2100024797.1.22",
          "form": "제일 힘든 집안일은 뭐가있나요?",
          "original_form": "제일 힘든 집안일은 뭐가있나요?",
          "speaker_id": "2",
          "time": "20210831 19:04"
        },
        {
          "id": "MDRW2100024797.1.23",
          "form": "그죠 궁금라더라구용",
          "original_form": "그죠 궁금라더라구용",
          "speaker_id": "1",
          "time": "20210831 19:04"
        },
        {
          "id": "MDRW2100024797.1.24",
          "form": "전 청소 ㅠㅠ 정리정돈이 안돼요 ㅋㅋㅋ",
          "original_form": "전 청소 ㅠㅠ 정리정돈이 안돼요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:04"
        }
      ]
