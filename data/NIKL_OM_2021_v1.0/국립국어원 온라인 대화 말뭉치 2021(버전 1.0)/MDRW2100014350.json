      "utterance": [
        {
          "id": "MDRW2100014350.1.1",
          "form": "보는 티비프로그램 있으신가요?",
          "original_form": "보는 티비프로그램 있으신가요?",
          "speaker_id": "1",
          "time": "20210825 18:32"
        },
        {
          "id": "MDRW2100014350.1.2",
          "form": "요새껀 안보고 혼밥할때 무한도전이랑 라디오스타 예전꺼",
          "original_form": "요새껀 안보고 혼밥할때 무한도전이랑 라디오스타 예전꺼",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014350.1.3",
          "form": "돌려봐요",
          "original_form": "돌려봐요",
          "speaker_id": "2",
          "time": "20210825 18:33"
        },
        {
          "id": "MDRW2100014350.1.4",
          "form": "그쵸 요즘 볼 거 없죠",
          "original_form": "그쵸 요즘 볼 거 없죠",
          "speaker_id": "1",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014350.1.5",
          "form": "저도 유튜브 더 만히 봐요",
          "original_form": "저도 유튜브 더 만히 봐요",
          "speaker_id": "1",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100014350.1.6",
          "form": "요새 티비를 잘안봐서",
          "original_form": "요새 티비를 잘안봐서",
          "speaker_id": "2",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014350.1.7",
          "form": "더 그런거같아요",
          "original_form": "더 그런거같아요",
          "speaker_id": "2",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014350.1.8",
          "form": "좋아하는 가수나 연예인 있으세요?",
          "original_form": "좋아하는 가수나 연예인 있으세요?",
          "speaker_id": "2",
          "time": "20210825 18:37"
        },
        {
          "id": "MDRW2100014350.1.9",
          "form": "예전에 아이돌 좋아했었는데",
          "original_form": "예전에 아이돌 좋아했었는데",
          "speaker_id": "1",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014350.1.10",
          "form": "지금은 없어요",
          "original_form": "지금은 없어요",
          "speaker_id": "1",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014350.1.11",
          "form": "좋아하는 연예인 있으세요?",
          "original_form": "좋아하는 연예인 있으세요?",
          "speaker_id": "1",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014350.1.12",
          "form": "저도 예전에 좋아했었는데",
          "original_form": "저도 예전에 좋아했었는데",
          "speaker_id": "2",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014350.1.13",
          "form": "논란이 터졌어서ㅋㅋㅋㅋ",
          "original_form": "논란이 터졌어서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:51"
        },
        {
          "id": "MDRW2100014350.1.14",
          "form": "연예인 사람자체를 좋아하진 않는것같아요",
          "original_form": "연예인 사람자체를 좋아하진 않는것같아요",
          "speaker_id": "2",
          "time": "20210825 18:52"
        },
        {
          "id": "MDRW2100014350.1.15",
          "form": "ㅋㅋㅋㅋㅋ저도 그래요",
          "original_form": "ㅋㅋㅋㅋㅋ저도 그래요",
          "speaker_id": "1",
          "time": "20210825 18:56"
        },
        {
          "id": "MDRW2100014350.1.16",
          "form": "어떻게 티비에 나온 모습만 보고 그 사람을 다 안다고 착각할 수 있는지",
          "original_form": "어떻게 티비에 나온 모습만 보고 그 사람을 다 안다고 착각할 수 있는지",
          "speaker_id": "1",
          "time": "20210825 18:56"
        },
        {
          "id": "MDRW2100014350.1.17",
          "form": "보여진 모습만 좋아하는 거죠",
          "original_form": "보여진 모습만 좋아하는 거죠",
          "speaker_id": "1",
          "time": "20210825 18:56"
        },
        {
          "id": "MDRW2100014350.1.18",
          "form": "결국에는 연예인도 콘텐츠와 이미지를 셀링하는 직업이다보니까",
          "original_form": "결국에는 연예인도 콘텐츠와 이미지를 셀링하는 직업이다보니까",
          "speaker_id": "2",
          "time": "20210825 18:58"
        },
        {
          "id": "MDRW2100014350.1.19",
          "form": "보는게 결국 제환상인것 같더라구요",
          "original_form": "보는게 결국 제환상인것 같더라구요",
          "speaker_id": "2",
          "time": "20210825 18:59"
        },
        {
          "id": "MDRW2100014350.1.20",
          "form": "맞아요 진짜 공감해요",
          "original_form": "맞아요 진짜 공감해요",
          "speaker_id": "1",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014350.1.21",
          "form": "우튜브로는 따로 구독해서 보는 거 없으세요?",
          "original_form": "우튜브로는 따로 구독해서 보는 거 없으세요?",
          "speaker_id": "1",
          "time": "20210825 19:07"
        },
        {
          "id": "MDRW2100014350.1.22",
          "form": "여행이나 일상 화장품",
          "original_form": "여행이나 일상 화장품",
          "speaker_id": "2",
          "time": "20210825 19:08"
        },
        {
          "id": "MDRW2100014350.1.23",
          "form": "이런 소소한주제들을 주로보는데 구독은 잘안하는거같아요",
          "original_form": "이런 소소한주제들을 주로보는데 구독은 잘안하는거같아요",
          "speaker_id": "2",
          "time": "20210825 19:09"
        },
        {
          "id": "MDRW2100014350.1.24",
          "form": "혹시 좋아하는 유투브채널이나 주제들 있으신가요?",
          "original_form": "혹시 좋아하는 유투브채널이나 주제들 있으신가요?",
          "speaker_id": "2",
          "time": "20210825 19:10"
        },
        {
          "id": "MDRW2100014350.1.25",
          "form": "저 asmr 같은 거 좋아해서",
          "original_form": "저 asmr 같은 거 좋아해서",
          "speaker_id": "1",
          "time": "20210825 19:12"
        },
        {
          "id": "MDRW2100014350.1.26",
          "form": "요리 asmr 찾아보고 그래요",
          "original_form": "요리 asmr 찾아보고 그래요",
          "speaker_id": "1",
          "time": "20210825 19:12"
        },
        {
          "id": "MDRW2100014350.1.27",
          "form": "한세님 유명해요",
          "original_form": "한세님 유명해요",
          "speaker_id": "1",
          "time": "20210825 19:12"
        },
        {
          "id": "MDRW2100014350.1.28",
          "form": "아 저 꿀 이랑 알로에 자르는 영상 본적있어요!",
          "original_form": "아 저 꿀 이랑 알로에 자르는 영상 본적있어요!",
          "speaker_id": "2",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014350.1.29",
          "form": "소리 너무 좋더라구요",
          "original_form": "소리 너무 좋더라구요",
          "speaker_id": "2",
          "time": "20210825 19:16"
        },
        {
          "id": "MDRW2100014350.1.30",
          "form": "그쵸 소리 다양하게 들을 수 있어서 좋아요",
          "original_form": "그쵸 소리 다양하게 들을 수 있어서 좋아요",
          "speaker_id": "1",
          "time": "20210825 19:52"
        }
      ]
    }
