      "utterance": [
        {
          "id": "MDRW2100047586.1.1",
          "form": "영화 최근에 보신거 있으셔요?",
          "original_form": "영화 최근에 보신거 있으셔요?",
          "speaker_id": "2",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100047586.1.2",
          "form": "영화관 가서 본 건 크루엘라요",
          "original_form": "영화관 가서 본 건 크루엘라요",
          "speaker_id": "1",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100047586.1.3",
          "form": "벌써 3개월 전이네요",
          "original_form": "벌써 3개월 전이네요",
          "speaker_id": "1",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100047586.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100047586.1.5",
          "form": "저는 더한참전ㅎㅎ 극한직업?",
          "original_form": "저는 더한참전ㅎㅎ 극한직업?",
          "speaker_id": "2",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100047586.1.6",
          "form": "아아 극한직업",
          "original_form": "아아 극한직업",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100047586.1.7",
          "form": "저도 영화관에서 봤는데 관람하는 내내 정말 많이 웃었어요ㅎㅎ",
          "original_form": "저도 영화관에서 봤는데 관람하는 내내 정말 많이 웃었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100047586.1.8",
          "form": "너무 재밌더라구요 요즘은 코시국도 그렇고 3살 아이 보느라 영화관 갈일이 없네요ㅠ",
          "original_form": "너무 재밌더라구요 요즘은 코시국도 그렇고 3살 아이 보느라 영화관 갈일이 없네요ㅠ",
          "speaker_id": "2",
          "time": "20210826 23:53"
        },
        {
          "id": "MDRW2100047586.1.9",
          "form": "네네 아무래도 영화관 가기가 쉽지 않죠",
          "original_form": "네네 아무래도 영화관 가기가 쉽지 않죠",
          "speaker_id": "1",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100047586.1.10",
          "form": "집에서 랜선 관람하는 것도 한 방법인 것 같아요",
          "original_form": "집에서 랜선 관람하는 것도 한 방법인 것 같아요",
          "speaker_id": "1",
          "time": "20210826 23:55"
        },
        {
          "id": "MDRW2100047586.1.11",
          "form": "맞아요 여건만 되면 요새는 왓챠나 그런것들고 잘 되어있으니까요 ㅎㅎ",
          "original_form": "맞아요 여건만 되면 요새는 왓챠나 그런것들고 잘 되어있으니까요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 23:58"
        },
        {
          "id": "MDRW2100047586.1.12",
          "form": "네네 영화도 그렇고",
          "original_form": "네네 영화도 그렇고",
          "speaker_id": "1",
          "time": "20210827 00:01"
        },
        {
          "id": "MDRW2100047586.1.13",
          "form": "요즘은 공연들도 온라인 서비스 많이 해주더라고요",
          "original_form": "요즘은 공연들도 온라인 서비스 많이 해주더라고요",
          "speaker_id": "1",
          "time": "20210827 00:01"
        },
        {
          "id": "MDRW2100047586.1.14",
          "form": "맞아요 근데 거의 ㅋㅋ 유료라 선뜻 손이 안가더라구요 ㅋㅋ",
          "original_form": "맞아요 근데 거의 ㅋㅋ 유료라 선뜻 손이 안가더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:03"
        },
        {
          "id": "MDRW2100047586.1.15",
          "form": "아 후원라이브가 많죠",
          "original_form": "아 후원라이브가 많죠",
          "speaker_id": "1",
          "time": "20210827 00:07"
        },
        {
          "id": "MDRW2100047586.1.16",
          "form": "가족들이랑 여럿이 보면 가성비 괜찮아요~",
          "original_form": "가족들이랑 여럿이 보면 가성비 괜찮아요~",
          "speaker_id": "1",
          "time": "20210827 00:07"
        },
        {
          "id": "MDRW2100047586.1.17",
          "form": "찾아보면 무료 관람 가능한 것도 있고요ㅎㅎ",
          "original_form": "찾아보면 무료 관람 가능한 것도 있고요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 00:07"
        },
        {
          "id": "MDRW2100047586.1.18",
          "form": "아 그런것도 있군요 !!",
          "original_form": "아 그런것도 있군요 !!",
          "speaker_id": "2",
          "time": "20210827 00:10"
        },
        {
          "id": "MDRW2100047586.1.19",
          "form": "네네 전시회들도 온라인 관람이 가능한 것이 있다니",
          "original_form": "네네 전시회들도 온라인 관람이 가능한 것이 있다니",
          "speaker_id": "1",
          "time": "20210827 00:13"
        },
        {
          "id": "MDRW2100047586.1.20",
          "form": "한번 찾아보셔요",
          "original_form": "한번 찾아보셔요",
          "speaker_id": "1",
          "time": "20210827 00:13"
        },
        {
          "id": "MDRW2100047586.1.21",
