        {
          "id": "MDRW2100045889.1.1",
          "form": "업무 스트레스는 없나요",
          "original_form": "업무 스트레스는 없나요",
          "speaker_id": "1",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100045889.1.2",
          "form": "있죠",
          "original_form": "있죠",
          "speaker_id": "2",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100045889.1.3",
          "form": "없을순없는거같아요",
          "original_form": "없을순없는거같아요",
          "speaker_id": "2",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100045889.1.4",
          "form": "일 스트레인가요 아님 사람 스트레스요",
          "original_form": "일 스트레인가요 아님 사람 스트레스요",
          "speaker_id": "1",
          "time": "20210908 08:30"
        },
        {
          "id": "MDRW2100045889.1.5",
          "form": "사람이기도하고 특허같은 먼가 내야하는 일ㄷᆢ",
          "original_form": "사람이기도하고 특허같은 먼가 내야하는 일ㄷᆢ",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045889.1.6",
          "form": "일도 스트레스구요",
          "original_form": "일도 스트레스구요",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045889.1.7",
          "form": "일하세요?",
          "original_form": "일하세요?",
          "speaker_id": "2",
          "time": "20210908 08:31"
        },
        {
          "id": "MDRW2100045889.1.8",
          "form": "네 직장인 입니다",
          "original_form": "네 직장인 입니다",
          "speaker_id": "1",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100045889.1.9",
          "form": "전 일 스트레스가 조금 있어요",
          "original_form": "전 일 스트레스가 조금 있어요",
          "speaker_id": "1",
          "time": "20210908 08:34"
        },
        {
          "id": "MDRW2100045889.1.10",
          "form": "사람들은 괜찮아요?",
          "original_form": "사람들은 괜찮아요?",
          "speaker_id": "2",
          "time": "20210908 08:35"
        },
        {
          "id": "MDRW2100045889.1.11",
          "form": "같이 일하는 언니 ,동생은 다 좋아요",
          "original_form": "같이 일하는 언니 ,동생은 다 좋아요",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045889.1.12",
          "form": "같이 일하는 사람은 어떤가요",
          "original_form": "같이 일하는 사람은 어떤가요",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045889.1.13",
          "form": "저는 같이 일하는동료가 나이차이가",
          "original_form": "저는 같이 일하는동료가 나이차이가",
          "speaker_id": "2",
          "time": "20210908 08:37"
        },
        {
          "id": "MDRW2100045889.1.14",
          "form": "많이 나니 좀 별루에요",
          "original_form": "많이 나니 좀 별루에요",
          "speaker_id": "2",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045889.1.15",
          "form": "지들끼리 이야기하고 처리하고 ㅋ",
          "original_form": "지들끼리 이야기하고 처리하고 ㅋ",
          "speaker_id": "2",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045889.1.16",
          "form": "제가 안편한가봐요",
          "original_form": "제가 안편한가봐요",
          "speaker_id": "2",
          "time": "20210908 08:38"
        },
        {
          "id": "MDRW2100045889.1.17",
          "form": "저는 04년도입사 후배는 16년도 17년도 입사 ㅋ",
          "original_form": "저는 04년도입사 후배는 16년도 17년도 입사 ㅋ",
          "speaker_id": "2",
          "time": "20210908 08:40"
        },
        {
          "id": "MDRW2100045889.1.18",
          "form": "차이가 너무나죠",
          "original_form": "차이가 너무나죠",
          "speaker_id": "2",
          "time": "20210908 08:40"
        },
        {
          "id": "MDRW2100045889.1.19",
          "form": "입사한지 오래되셨군요",
          "original_form": "입사한지 오래되셨군요",
          "speaker_id": "1",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045889.1.20",
          "form": "차이가 많이나니 좀 거리가 있겠군요",
          "original_form": "차이가 많이나니 좀 거리가 있겠군요",
          "speaker_id": "1",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045889.1.21",
          "form": "네 저는 아이엄마고하니 두분은 총각이고 ㅎ",
          "original_form": "네 저는 아이엄마고하니 두분은 총각이고 ㅎ",
          "speaker_id": "2",
          "time": "20210908 08:58"
        },
        {
          "id": "MDRW2100045889.1.22",
          "form": "아무래도 환경도 틀리니 저한테 머부탁하기 좀 그런가봐요",
          "original_form": "아무래도 환경도 틀리니 저한테 머부탁하기 좀 그런가봐요",
          "speaker_id": "2",
          "time": "20210908 09:00"
        },
        {
          "id": "MDRW2100045889.1.23",
          "form": "힘드시겠어요",
          "original_form": "힘드시겠어요",
          "speaker_id": "1",
          "time": "20210908 09:01"
        },
        {
          "id": "MDRW2100045889.1.24",
          "form": "네 제가 나가야할까봐요",
          "original_form": "네 제가 나가야할까봐요",
          "speaker_id": "2",
          "time": "20210908 09:05"
        },
        {
          "id": "MDRW2100045889.1.25",
          "form": "아니 그러진 마세요",
          "original_form": "아니 그러진 마세요",
          "speaker_id": "1",
          "time": "20210908 09:07"
        },
        {
          "id": "MDRW2100045889.1.26",
          "form": "오래 다신 직장인데 그분들과 공감대 형성을 해보시는건 어떨까요",
          "original_form": "오래 다신 직장인데 그분들과 공감대 형성을 해보시는건 어떨까요",
          "speaker_id": "1",
          "time": "20210908 09:08"
        },
        {
          "id": "MDRW2100045889.1.27",
          "form": "휴직생각해요 아이도 1학년되면할랬는데",
          "original_form": "휴직생각해요 아이도 1학년되면할랬는데",
          "speaker_id": "2",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100045889.1.28",
          "form": "1년 먼저할까싶기도해요 ㅎ",
          "original_form": "1년 먼저할까싶기도해요 ㅎ",
          "speaker_id": "2",
          "time": "20210908 09:09"
        },
        {
          "id": "MDRW2100045889.1.29",
          "form": "아 ?  초등 1학년이요",
          "original_form": "아 ?  초등 1학년이요",
          "speaker_id": "1",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100045889.1.30",
          "form": "손이 많이 필요하죠 챙겨야 할일도 많죠",
          "original_form": "손이 많이 필요하죠 챙겨야 할일도 많죠",
          "speaker_id": "1",
          "time": "20210908 09:14"
        },
        {
          "id": "MDRW2100045889.1.31",
          "form": "7살일때부터 먼저해서",
          "original_form": "7살일때부터 먼저해서",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045889.1.32",
          "form": "좀 놀까싶은맘이 ㅋ",
          "original_form": "좀 놀까싶은맘이 ㅋ",
          "speaker_id": "2",
          "time": "20210908 09:21"
        },
        {
          "id": "MDRW2100045889.1.33",
          "form": "많이 지치셨나봐요",
          "original_form": "많이 지치셨나봐요",
          "speaker_id": "1",
          "time": "20210908 09:22"
        },
        {
          "id": "MDRW2100045889.1.34",
          "form": "휴식이 필요한것 같아요",
          "original_form": "휴식이 필요한것 같아요",
          "speaker_id": "1",
          "time": "20210908 09:23"
        },
        {
          "id": "MDRW2100045889.1.35",
          "form": "좀 외로운듯한 느낌이요 ㅎ",
          "original_form": "좀 외로운듯한 느낌이요 ㅎ",
