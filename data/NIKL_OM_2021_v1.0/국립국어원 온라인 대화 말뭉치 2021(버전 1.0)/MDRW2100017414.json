      "utterance": [
        {
          "id": "MDRW2100017414.1.1",
          "form": "나는 강대병원앞 카페베이에서 카야토스트랑 포도망고스무벨라 사먹음ㅋㅋ",
          "original_form": "나는 강대병원앞 카페베이에서 카야토스트랑 포도망고스무벨라 사먹음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017414.1.2",
          "form": "나는 교정칫솔 이제 주말에 주문해야지..",
          "original_form": "나는 교정칫솔 이제 주말에 주문해야지..",
          "speaker_id": "2",
          "time": "20210826 19:22"
        },
        {
          "id": "MDRW2100017414.1.3",
          "form": "나 카야토스트 한번도 안먹어봤어",
          "original_form": "나 카야토스트 한번도 안먹어봤어",
          "speaker_id": "1",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017414.1.4",
          "form": "스무벨라는 뭐시여",
          "original_form": "스무벨라는 뭐시여",
          "speaker_id": "1",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017414.1.5",
          "form": "스무디야?",
          "original_form": "스무디야?",
          "speaker_id": "1",
          "time": "20210826 19:25"
        },
        {
          "id": "MDRW2100017414.1.6",
          "form": "웅웅ㅋㅋㅋㅋ",
          "original_form": "웅웅ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017414.1.7",
          "form": "카야토스트 개존맛이야",
          "original_form": "카야토스트 개존맛이야",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017414.1.8",
          "form": "카야잼 사면 크으",
          "original_form": "카야잼 사면 크으",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100017414.1.9",
          "form": "나는 카야토스트 파는곳을 갈일이 없어서",
          "original_form": "나는 카야토스트 파는곳을 갈일이 없어서",
          "speaker_id": "1",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100017414.1.10",
          "form": "나도 먹어보고싶다",
          "original_form": "나도 먹어보고싶다",
          "speaker_id": "1",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100017414.1.11",
          "form": "나는 적립금 쓰려고 저번에 벨트산거 왔는데",
          "original_form": "나는 적립금 쓰려고 저번에 벨트산거 왔는데",
          "speaker_id": "1",
          "time": "20210827 14:06"
        },
        {
          "id": "MDRW2100017414.1.12",
          "form": "무난무난",
          "original_form": "무난무난",
          "speaker_id": "1",
          "time": "20210827 14:06"
        },
        {
          "id": "MDRW2100017414.1.13",
          "form": "오 그래??",
          "original_form": "오 그래??",
          "speaker_id": "2",
          "time": "20210827 17:21"
        },
        {
          "id": "MDRW2100017414.1.14",
          "form": "카야토스트 그 카페베이에 팔아!!",
          "original_form": "카야토스트 그 카페베이에 팔아!!",
          "speaker_id": "2",
          "time": "20210827 17:21"
        },
        {
          "id": "MDRW2100017414.1.15",
          "form": "나는 가을옷 사려고하는데 금방추울거같으니까 많이는 못사겠고..",
          "original_form": "나는 가을옷 사려고하는데 금방추울거같으니까 많이는 못사겠고..",
          "speaker_id": "2",
          "time": "20210827 17:21"
        },
        {
          "id": "MDRW2100017414.1.16",
          "form": "뭔가 카페베이는 갈생각을 안해ㅋㅋㅋ스벅이나 투썸 할리스 정도 가는듯",
          "original_form": "뭔가 카페베이는 갈생각을 안해ㅋㅋㅋ스벅이나 투썸 할리스 정도 가는듯",
          "speaker_id": "1",
          "time": "20210827 17:36"
        },
        {
          "id": "MDRW2100017414.1.17",
          "form": "가을옷ㅋㅋ올해는 그냥 입던옷으로 버틴다",
          "original_form": "가을옷ㅋㅋ올해는 그냥 입던옷으로 버틴다",
          "speaker_id": "1",
          "time": "20210827 17:36"
        },
        {
          "id": "MDRW2100017414.1.18",
          "form": "ㅋㅋㅋㅋㅋ나는 일단 바지는 말고",
          "original_form": "ㅋㅋㅋㅋㅋ나는 일단 바지는 말고",
          "speaker_id": "2",
          "time": "20210827 17:56"
        },
        {
          "id": "MDRW2100017414.1.19",
          "form": "티만 일단...",
          "original_form": "티만 일단...",
          "speaker_id": "2",
          "time": "20210827 17:57"
        },
        {
          "id": "MDRW2100017414.1.20",
          "form": "어차피 겨울되면 기모로된거 후드랑 맨투맨 살거같음ㅋㅋ",
          "original_form": "어차피 겨울되면 기모로된거 후드랑 맨투맨 살거같음ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:57"
        },
        {
          "id": "MDRW2100017414.1.21",
          "form": "맨투맨도 올초에 사서 겨울옷도 안살듯ㅋㅋㅋ",
          "original_form": "맨투맨도 올초에 사서 겨울옷도 안살듯ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:07"
        },
        {
          "id": "MDRW2100017414.1.22",
          "form": "ㅋㅋㅋㅋ근데 나는 사실 동생이 사는거 입엌ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ근데 나는 사실 동생이 사는거 입엌ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100017414.1.23",
          "form": "약간 나는 먹는데 돈쓰는타입이고 동생은 옷이랑 신발 가방 이런거에 돈씀ㅋㅋㅋ",
          "original_form": "약간 나는 먹는데 돈쓰는타입이고 동생은 옷이랑 신발 가방 이런거에 돈씀ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:10"
        },
        {
          "id": "MDRW2100017414.1.24",
          "form": "역시 자매가 좋다니까",
          "original_form": "역시 자매가 좋다니까",
          "speaker_id": "1",
          "time": "20210827 18:16"
        },
        {
          "id": "MDRW2100017414.1.25",
          "form": "아니 근데 옷장보면 다 츄리닝 맨투맨 이런것만있어",
          "original_form": "아니 근데 옷장보면 다 츄리닝 맨투맨 이런것만있어",
          "speaker_id": "1",
          "time": "20210827 18:16"
        },
        {
          "id": "MDRW2100017414.1.26",
          "form": "나도 멋쟁이 옷을 입고싶다",
          "original_form": "나도 멋쟁이 옷을 입고싶다",
          "speaker_id": "1",
          "time": "20210827 18:16"
        },
        {
          "id": "MDRW2100017414.1.27",
          "form": "뭐지 나 답장한줄알았다ㅠㅠㅋㅋㅋㅋ",
          "original_form": "뭐지 나 답장한줄알았다ㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:16"
        },
        {
          "id": "MDRW2100017414.1.28",
          "form": "근데 나도 동생없으면 옷 살일이없어ㅋㅋㅋ",
          "original_form": "근데 나도 동생없으면 옷 살일이없어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:16"
        },
        {
          "id": "MDRW2100017414.1.29",
          "form": "오늘 홈플 뿌시고왔다",
          "original_form": "오늘 홈플 뿌시고왔다",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100017414.1.30",
          "form": "두유한팩사고 샐러드사고 라면사고 파스타소스사고",
          "original_form": "두유한팩사고 샐러드사고 라면사고 파스타소스사고",
          "speaker_id": "1",
          "time": "20210830 23:52"
        },
        {
          "id": "MDRW2100017414.1.31",
          "form": "세일하는 여름잠옷바지까지 샀다",
          "original_form": "세일하는 여름잠옷바지까지 샀다",
          "speaker_id": "1",
          "time": "20210830 23:53"
        },
        {
          "id": "MDRW2100017414.1.32",
          "form": "이야 엄청난 쇼핑을 했넼ㅋㅋㅋㅋㅋ",
          "original_form": "이야 엄청난 쇼핑을 했넼ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 07:42"
        },
        {
          "id": "MDRW2100017414.1.33",
          "form": "나도 월급들어오면 이제 생리대도 사야됌ㅋㅋㅋ",
          "original_form": "나도 월급들어오면 이제 생리대도 사야됌ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 07:42"
        },
        {
          "id": "MDRW2100017414.1.34",
          "form": "아 생리대도 비싸서 사면 텅장됨",
          "original_form": "아 생리대도 비싸서 사면 텅장됨",
