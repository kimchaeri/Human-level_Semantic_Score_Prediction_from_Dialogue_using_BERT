      "utterance": [
        {
          "id": "MDRW2100013314.1.1",
          "form": "안녕하세요! 재밌는 게임 알고 계신가요??",
          "original_form": "안녕하세요! 재밌는 게임 알고 계신가요??",
          "speaker_id": "1",
          "time": "20210825 14:00"
        },
        {
          "id": "MDRW2100013314.1.2",
          "form": "저는 게임을 잘 몰라서ㅋㅋ",
          "original_form": "저는 게임을 잘 몰라서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013314.1.3",
          "form": "ㅋㅋㅋㅋ사실 저두 게임에 대해 잘 몰라요",
          "original_form": "ㅋㅋㅋㅋ사실 저두 게임에 대해 잘 몰라요",
          "speaker_id": "1",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013314.1.4",
          "form": "제가 하는 게임은 한가지인데",
          "original_form": "제가 하는 게임은 한가지인데",
          "speaker_id": "1",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013314.1.5",
          "form": "혹시 스타듀밸리 아시나요??",
          "original_form": "혹시 스타듀밸리 아시나요??",
          "speaker_id": "1",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013314.1.6",
          "form": "헉 저 그거 알아요",
          "original_form": "헉 저 그거 알아요",
          "speaker_id": "2",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013314.1.7",
          "form": "헐!!!!!!!!!!!",
          "original_form": "헐!!!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013314.1.8",
          "form": "대박이당 이거이거",
          "original_form": "대박이당 이거이거",
          "speaker_id": "1",
          "time": "20210825 14:12"
        },
        {
          "id": "MDRW2100013314.1.9",
          "form": "넘 재밌어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "넘 재밌어요 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:13"
        },
        {
          "id": "MDRW2100013314.1.10",
          "form": "그쵸ㅋㅋ 저는 모바일 PC 둘 다 샀어요!",
          "original_form": "그쵸ㅋㅋ 저는 모바일 PC 둘 다 샀어요!",
          "speaker_id": "2",
          "time": "20210825 14:13"
        },
        {
          "id": "MDRW2100013314.1.11",
          "form": "저는 게임 잘 안하는 사람인데 이거에만 빠져서.... RPG게임입문용으로 딱인듯해요",
          "original_form": "저는 게임 잘 안하는 사람인데 이거에만 빠져서.... RPG게임입문용으로 딱인듯해요",
          "speaker_id": "1",
          "time": "20210825 14:13"
        },
        {
          "id": "MDRW2100013314.1.12",
          "form": "헉!!!!!! 저랑",
          "original_form": "헉!!!!!! 저랑",
          "speaker_id": "1",
          "time": "20210825 14:13"
        },
        {
          "id": "MDRW2100013314.1.13",
          "form": "똑같으시네요",
          "original_form": "똑같으시네요",
          "speaker_id": "1",
          "time": "20210825 14:13"
        },
        {
          "id": "MDRW2100013314.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:13"
        },
        {
          "id": "MDRW2100013314.1.15",
          "form": "진짜 게임 하느라 시간 다 잡아먹어서 겨우 지웠어요",
          "original_form": "진짜 게임 하느라 시간 다 잡아먹어서 겨우 지웠어요",
          "speaker_id": "2",
          "time": "20210825 14:15"
        },
        {
          "id": "MDRW2100013314.1.16",
          "form": "저는 얼마전에 폰을 바꿔서 폰에는 스타듀밸리가 없어요",
          "original_form": "저는 얼마전에 폰을 바꿔서 폰에는 스타듀밸리가 없어요",
          "speaker_id": "1",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100013314.1.17",
          "form": "그래서 안하게되었죠ㅜ ㅋㅋㅋㅋㅋ",
          "original_form": "그래서 안하게되었죠ㅜ ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100013314.1.18",
          "form": "아 정말요",
          "original_form": "아 정말요",
          "speaker_id": "2",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013314.1.19",
          "form": "하긴 새로 하시려면 막막하긴 하겠네요",
          "original_form": "하긴 새로 하시려면 막막하긴 하겠네요",
          "speaker_id": "2",
          "time": "20210825 15:23"
        },
        {
          "id": "MDRW2100013314.1.20",
          "form": "맞아요 이미 다 했는데 새로 또 하려면....",
          "original_form": "맞아요 이미 다 했는데 새로 또 하려면....",
          "speaker_id": "1",
          "time": "20210825 15:36"
        },
        {
          "id": "MDRW2100013314.1.21",
          "form": "그래서 다른 게임 찾아보고 잇어요",
          "original_form": "그래서 다른 게임 찾아보고 잇어요",
