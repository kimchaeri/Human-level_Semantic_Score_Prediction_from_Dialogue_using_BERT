      "utterance": [
        {
          "id": "MDRW2100020392.1.1",
          "form": "결혼작사 이혼작곡 보셨어요?",
          "original_form": "결혼작사 이혼작곡 보셨어요?",
          "speaker_id": "1",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020392.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020392.1.3",
          "form": "드디어 저도 본 거네요 ㅎ",
          "original_form": "드디어 저도 본 거네요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020392.1.4",
          "form": "그러게요 ㅋㅋㅋ",
          "original_form": "그러게요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020392.1.5",
          "form": "재밌죠!!",
          "original_form": "재밌죠!!",
          "speaker_id": "1",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020392.1.6",
          "form": "네 결말 너무 충격적..",
          "original_form": "네 결말 너무 충격적..",
          "speaker_id": "2",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020392.1.7",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020392.1.8",
          "form": "그래서 재밌었어요",
          "original_form": "그래서 재밌었어요",
          "speaker_id": "1",
          "time": "20210828 02:40"
        },
        {
          "id": "MDRW2100020392.1.9",
          "form": "네 시즌3 기대돼요",
          "original_form": "네 시즌3 기대돼요",
          "speaker_id": "2",
          "time": "20210828 02:41"
        },
        {
          "id": "MDRW2100020392.1.10",
          "form": "시즌 1은 엄청 지루했는데",
          "original_form": "시즌 1은 엄청 지루했는데",
          "speaker_id": "1",
          "time": "20210828 02:41"
        },
        {
          "id": "MDRW2100020392.1.11",
          "form": "2는 재밌었어요",
          "original_form": "2는 재밌었어요",
          "speaker_id": "1",
          "time": "20210828 02:41"
        },
        {
          "id": "MDRW2100020392.1.12",
          "form": "네 저도 시즌1은 대충봤어요",
          "original_form": "네 저도 시즌1은 대충봤어요",
          "speaker_id": "2",
          "time": "20210828 02:41"
        },
        {
          "id": "MDRW2100020392.1.13",
          "form": "역시 임성한 ㅋㅋ",
          "original_form": "역시 임성한 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 02:41"
        },
        {
          "id": "MDRW2100020392.1.14",
          "form": "글빨은 인정..",
          "original_form": "글빨은 인정..",
          "speaker_id": "2",
          "time": "20210828 02:41"
        },
        {
          "id": "MDRW2100020392.1.15",
          "form": "펜하보다 더 재밌지않아요?",
          "original_form": "펜하보다 더 재밌지않아요?",
          "speaker_id": "1",
          "time": "20210828 02:41"
        },
        {
          "id": "MDRW2100020392.1.16",
          "form": "네 ㅎ 흥미진진.. ㅎㅎ",
          "original_form": "네 ㅎ 흥미진진.. ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 02:41"
        },
        {
          "id": "MDRW2100020392.1.17",
          "form": "빨리 시즌3 나왔으면 좋겠어요",
          "original_form": "빨리 시즌3 나왔으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210828 02:42"
        },
        {
          "id": "MDRW2100020392.1.18",
          "form": "네 도대체 어떻게 풀지..",
          "original_form": "네 도대체 어떻게 풀지..",
