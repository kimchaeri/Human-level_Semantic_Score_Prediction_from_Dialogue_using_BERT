      "utterance": [
        {
          "id": "MDRW2100031183.1.1",
          "form": "ㅋㅋㅋㅋㅋ언젠가 직관도 꼭 가봐",
          "original_form": "ㅋㅋㅋㅋㅋ언젠가 직관도 꼭 가봐",
          "speaker_id": "2",
          "time": "20210904 17:57"
        },
        {
          "id": "MDRW2100031183.1.2",
          "form": "좋아하겠다",
          "original_form": "좋아하겠다",
          "speaker_id": "2",
          "time": "20210904 17:57"
        },
        {
          "id": "MDRW2100031183.1.3",
          "form": "ㅋㅋㅋㅋㅋ집수니라 집에서보는게 더좋을것같긴한데",
          "original_form": "ㅋㅋㅋㅋㅋ집수니라 집에서보는게 더좋을것같긴한데",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100031183.1.4",
          "form": "갈생갑두있지",
          "original_form": "갈생갑두있지",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100031183.1.5",
          "form": "심판씨 어디맞았대",
          "original_form": "심판씨 어디맞았대",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100031183.1.6",
          "form": "공인가",
          "original_form": "공인가",
          "speaker_id": "1",
          "time": "20210904 17:58"
        },
        {
          "id": "MDRW2100031183.1.7",
          "form": "아프겄다",
          "original_form": "아프겄다",
          "speaker_id": "1",
          "time": "20210904 17:59"
        },
        {
          "id": "MDRW2100031183.1.8",
          "form": "지금 점수났으면조켔는뎅",
          "original_form": "지금 점수났으면조켔는뎅",
          "speaker_id": "1",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100031183.1.9",
          "form": "심판도 공에 맞아??ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "심판도 공에 맞아??ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100031183.1.10",
          "form": "아직 1점 차?",
          "original_form": "아직 1점 차?",
          "speaker_id": "2",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100031183.1.11",
          "form": "파울같은데 응응 기아가 1점",
          "original_form": "파울같은데 응응 기아가 1점",
          "speaker_id": "1",
          "time": "20210904 18:00"
        },
        {
          "id": "MDRW2100031183.1.12",
          "form": "답장치다가 놓치니까",
          "original_form": "답장치다가 놓치니까",
          "speaker_id": "1",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100031183.1.13",
          "form": "아프다고 들어가셧는데",
          "original_form": "아프다고 들어가셧는데",
          "speaker_id": "1",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100031183.1.14",
          "form": "심판다시 나왓다",
          "original_form": "심판다시 나왓다",
          "speaker_id": "1",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100031183.1.15",
          "form": "파울맞네ㅠ",
          "original_form": "파울맞네ㅠ",
          "speaker_id": "1",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100031183.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ1점차면 금방 따라잡지 뭐",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ1점차면 금방 따라잡지 뭐",
          "speaker_id": "2",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100031183.1.17",
          "form": "괜찮으시대ㅠ",
          "original_form": "괜찮으시대ㅠ",
          "speaker_id": "1",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100031183.1.18",
          "form": "진짜야구 맞으면 엄청 아프겠지?",
          "original_form": "진짜야구 맞으면 엄청 아프겠지?",
          "speaker_id": "1",
          "time": "20210904 18:01"
        },
        {
          "id": "MDRW2100031183.1.19",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "2",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100031183.1.20",
          "form": "선수들 맨날맞는선수 또 맞구",
          "original_form": "선수들 맨날맞는선수 또 맞구",
          "speaker_id": "1",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100031183.1.21",
          "form": "야구공으로 살짝 쳐도 아프잖아",
          "original_form": "야구공으로 살짝 쳐도 아프잖아",
          "speaker_id": "2",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100031183.1.22",
          "form": "살짝 안쳐봤으",
          "original_form": "살짝 안쳐봤으",
          "speaker_id": "1",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100031183.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100031183.1.24",
          "form": "몰라난,,야구공 본적없는거같은뎅",
          "original_form": "몰라난,,야구공 본적없는거같은뎅",
          "speaker_id": "1",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100031183.1.25",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "2",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100031183.1.26",
          "form": "엄청 딱딱해",
          "original_form": "엄청 딱딱해",
          "speaker_id": "2",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100031183.1.27",
          "form": "응 야구공을 어디서봐 ?",
          "original_form": "응 야구공을 어디서봐 ?",
          "speaker_id": "1",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100031183.1.28",
          "form": "어렸을 때 야구하고 놀았었어",
          "original_form": "어렸을 때 야구하고 놀았었어",
          "speaker_id": "2",
          "time": "20210904 18:04"
        },
        {
          "id": "MDRW2100031183.1.29",
          "form": "근데 너무 딱딱해서 거의 테니스공 가지고 해",
          "original_form": "근데 너무 딱딱해서 거의 테니스공 가지고 해",
          "speaker_id": "2",
          "time": "20210904 18:04"
        },
        {
          "id": "MDRW2100031183.1.30",
          "form": "어릴때?  야구선수였어?",
          "original_form": "어릴때?  야구선수였어?",
          "speaker_id": "1",
          "time": "20210904 18:04"
        },
        {
          "id": "MDRW2100031183.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋ그럴리가",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그럴리가",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031183.1.32",
          "form": "야구가 인기였지 아주 잠깐",
          "original_form": "야구가 인기였지 아주 잠깐",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031183.1.33",
          "form": "아시안 시리즈 우승했을 때인가",
          "original_form": "아시안 시리즈 우승했을 때인가",
          "speaker_id": "2",
          "time": "20210904 18:07"
        },
        {
          "id": "MDRW2100031183.1.34",
          "form": "엥그래??",
          "original_form": "엥그래??",
          "speaker_id": "1",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031183.1.35",
          "form": "난첨들어",
          "original_form": "난첨들어",
          "speaker_id": "1",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031183.1.36",
          "form": "응응 그랬었어",
          "original_form": "응응 그랬었어",
          "speaker_id": "2",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031183.1.37",
          "form": "지금은 쉬는 시간?",
          "original_form": "지금은 쉬는 시간?",
          "speaker_id": "2",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031183.1.38",
          "form": "지금 1점추가해서",
          "original_form": "지금 1점추가해서",
          "speaker_id": "1",
          "time": "20210904 18:08"
        },
        {
          "id": "MDRW2100031183.1.39",
          "form": "동점!",
          "original_form": "동점!",
          "speaker_id": "1",
          "time": "20210904 18:09"
        },
        {
          "id": "MDRW2100031183.1.40",
          "form": "야구 하는거 동점이야",
          "original_form": "야구 하는거 동점이야",
          "speaker_id": "1",
          "time": "20210904 18:09"
        }
      ]
    }
