      "utterance": [
        {
          "id": "MDRW2100009587.1.1",
          "form": "펜트 하우스 잘 보셨나요?",
          "original_form": "펜트 하우스 잘 보셨나요?",
          "speaker_id": "1",
          "time": "20210823 23:48"
        },
        {
          "id": "MDRW2100009587.1.2",
          "form": "네네 ㅎ 천서진죽은거예요?",
          "original_form": "네네 ㅎ 천서진죽은거예요?",
          "speaker_id": "2",
          "time": "20210823 23:48"
        },
        {
          "id": "MDRW2100009587.1.3",
          "form": "저도 궁금해요 ㅋㅋㅋ 근데 죽기엔 너무 낮은 높인데..ㅋㅋ 피가 ..ㅋㅋ 쿨럭",
          "original_form": "저도 궁금해요 ㅋㅋㅋ 근데 죽기엔 너무 낮은 높인데..ㅋㅋ 피가 ..ㅋㅋ 쿨럭",
          "speaker_id": "1",
          "time": "20210823 23:50"
        },
        {
          "id": "MDRW2100009587.1.4",
          "form": "폭발해도 사는데 그정도로 죽을거같진 않고 ㅎ",
          "original_form": "폭발해도 사는데 그정도로 죽을거같진 않고 ㅎ",
          "speaker_id": "2",
          "time": "20210823 23:50"
        },
        {
          "id": "MDRW2100009587.1.5",
          "form": "맞죠 ~ 뭐 그 떨어진게 뾰족한것도 아니었고...ㅋㅋㅋ 다 부활하니",
          "original_form": "맞죠 ~ 뭐 그 떨어진게 뾰족한것도 아니었고...ㅋㅋㅋ 다 부활하니",
          "speaker_id": "1",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100009587.1.6",
          "form": "ㅎㅎ 담주는 챙겨봐야겠어요",
          "original_form": "ㅎㅎ 담주는 챙겨봐야겠어요",
          "speaker_id": "2",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100009587.1.7",
          "form": "지난주꺼는 그래도 볼만 했죠??",
          "original_form": "지난주꺼는 그래도 볼만 했죠??",
          "speaker_id": "1",
          "time": "20210823 23:51"
        },
        {
          "id": "MDRW2100009587.1.8",
          "form": "네네 슬슬 악역들 죄받고있는거같아서 답답하진 않더라구요",
          "original_form": "네네 슬슬 악역들 죄받고있는거같아서 답답하진 않더라구요",
          "speaker_id": "2",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100009587.1.9",
          "form": "근데 주단태가 다음주에 처키로...ㅋㅋ",
          "original_form": "근데 주단태가 다음주에 처키로...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:53"
        },
        {
          "id": "MDRW2100009587.1.10",
          "form": "빨리  심수련이 다 복수해주길",
          "original_form": "빨리  심수련이 다 복수해주길",
          "speaker_id": "2",
          "time": "20210823 23:54"
        },
        {
          "id": "MDRW2100009587.1.11",
          "form": "이번주에 뭔가 큰일이 벌어질것 같아요ㅠㅠ",
          "original_form": "이번주에 뭔가 큰일이 벌어질것 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 23:54"
        },
        {
          "id": "MDRW2100009587.1.12",
          "form": "탈출해서 한국올거같긴 하더라구요",
          "original_form": "탈출해서 한국올거같긴 하더라구요",
          "speaker_id": "2",
          "time": "20210823 23:54"
        },
        {
          "id": "MDRW2100009587.1.13",
          "form": "근데 주단태도 반전이 있긴했었어서 쫌 불쌍하지만 하도 나쁜짓을 많이 해서ㅠㅠ",
