          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 10:31"
        },
        {
          "id": "MDRW2100013014.1.8",
          "form": "급하기싫어졌음ㅋ",
          "original_form": "급하기싫어졌음ㅋ",
          "speaker_id": "1",
          "time": "20210825 10:31"
        },
        {
          "id": "MDRW2100013014.1.9",
          "form": "해서 숨겨놓자 ㅋㅋㅋㅋ",
          "original_form": "해서 숨겨놓자 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 10:31"
        },
        {
          "id": "MDRW2100013014.1.10",
          "form": "ㅋ숨겨놓을때도없다ㅋㅋ",
          "original_form": "ㅋ숨겨놓을때도없다ㅋㅋ",
          "speaker_id": "1",
