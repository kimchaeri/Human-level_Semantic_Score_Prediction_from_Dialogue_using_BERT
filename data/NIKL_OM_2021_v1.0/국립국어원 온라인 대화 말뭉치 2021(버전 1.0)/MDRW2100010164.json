      "utterance": [
        {
          "id": "MDRW2100010164.1.1",
          "form": "진로두꺼비 너무 좋아해요ㅠㅠ",
          "original_form": "진로두꺼비 너무 좋아해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 14:59"
        },
        {
          "id": "MDRW2100010164.1.2",
          "form": "네 귀엽고 먹을때 깔끔하더라구요ㅎㅎ",
          "original_form": "네 귀엽고 먹을때 깔끔하더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 14:59"
        },
        {
          "id": "MDRW2100010164.1.3",
          "form": "그쵸 숙취도 없고 좋더라구요",
          "original_form": "그쵸 숙취도 없고 좋더라구요",
          "speaker_id": "2",
          "time": "20210823 15:02"
        },
        {
          "id": "MDRW2100010164.1.4",
          "form": "너무 귀여워서 편의점에서 파는 굿즈도 샀어요ㅋㅋ",
          "original_form": "너무 귀여워서 편의점에서 파는 굿즈도 샀어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:02"
        },
        {
          "id": "MDRW2100010164.1.5",
          "form": "광고고 업청 귀엽던데ㅎㅎ 두꺼빈데 징그럽지 않구",
          "original_form": "광고고 업청 귀엽던데ㅎㅎ 두꺼빈데 징그럽지 않구",
          "speaker_id": "1",
          "time": "20210823 15:03"
        },
        {
          "id": "MDRW2100010164.1.6",
          "form": "아이유님 따라하는 거 너무 귀엽지않나요ㅠ?",
          "original_form": "아이유님 따라하는 거 너무 귀엽지않나요ㅠ?",
          "speaker_id": "2",
          "time": "20210823 15:04"
        },
        {
          "id": "MDRW2100010164.1.7",
          "form": "네 디자인이 잘 뽑힌거같아요ㅋㅋ",
          "original_form": "네 디자인이 잘 뽑힌거같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 15:04"
        },
        {
          "id": "MDRW2100010164.1.8",
          "form": "전 진로 분홍색보다 파란색이 더 귀엽더라구요",
          "original_form": "전 진로 분홍색보다 파란색이 더 귀엽더라구요",
          "speaker_id": "2",
          "time": "20210823 15:05"
        },
        {
          "id": "MDRW2100010164.1.9",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210823 15:06"
        },
        {
          "id": "MDRW2100010164.1.10",
          "form": "저도 파란색이 훨씬 귀엽던데 분홍색도 있나요?ㅎㅎ",
          "original_form": "저도 파란색이 훨씬 귀엽던데 분홍색도 있나요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 15:06"
        },
        {
          "id": "MDRW2100010164.1.11",
          "form": "네ㅋㅋ 커플인가봐요ㅋㅋ",
          "original_form": "네ㅋㅋ 커플인가봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 15:09"
        },
        {
          "id": "MDRW2100010164.1.12",
          "form": "그건 몰랐네요ㅎㅎ 분홍색ㅎㅎ",
          "original_form": "그건 몰랐네요ㅎㅎ 분홍색ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 15:09"
        },
        {
          "id": "MDRW2100010164.1.13",
          "form": "곰표맥주 드셔보셨어요?",
          "original_form": "곰표맥주 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210823 15:10"
        },
        {
          "id": "MDRW2100010164.1.14",
          "form": "네 곰표맥주 좋아해요ㅎㅎ",
          "original_form": "네 곰표맥주 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 15:10"
        },
        {
          "id": "MDRW2100010164.1.15",
          "form": "저두 초반엔 없어서 찾으러다녔는데 요즘은 물량이 많아서 좋아요ㅋㅋ",
          "original_form": "저두 초반엔 없어서 찾으러다녔는데 요즘은 물량이 많아서 좋아요ㅋㅋ",
