      "utterance": [
        {
          "id": "MDRW2100019757.1.1",
          "form": "잘 만드는 요리가 있나요",
          "original_form": "잘 만드는 요리가 있나요",
          "speaker_id": "2",
          "time": "20210827 22:28"
        },
        {
          "id": "MDRW2100019757.1.2",
          "form": "똥손임다",
          "original_form": "똥손임다",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019757.1.3",
          "form": "주로 사먹거나 시켜먹어요",
          "original_form": "주로 사먹거나 시켜먹어요",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100019757.1.4",
          "form": "똥손 ㅋㅋㅋㅋ",
          "original_form": "똥손 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019757.1.5",
          "form": "저두 그래요",
          "original_form": "저두 그래요",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019757.1.6",
          "form": "좋아하는 메뉴는 뭔가요",
          "original_form": "좋아하는 메뉴는 뭔가요",
          "speaker_id": "1",
          "time": "20210827 23:29"
        },
        {
          "id": "MDRW2100019757.1.7",
          "form": "저 마라샹궈랑 해물칼국수",
          "original_form": "저 마라샹궈랑 해물칼국수",
          "speaker_id": "2",
          "time": "20210827 23:47"
        },
        {
          "id": "MDRW2100019757.1.8",
          "form": "저는 마라탕이랑 면종류싹다!",
          "original_form": "저는 마라탕이랑 면종류싹다!",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100019757.1.9",
          "form": "우와 저랑 비슷해요",
          "original_form": "우와 저랑 비슷해요",
          "speaker_id": "2",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100019757.1.10",
          "form": "훠궈가 상궈보다 좋나요?",
          "original_form": "훠궈가 상궈보다 좋나요?",
          "speaker_id": "2",
          "time": "20210828 10:05"
        },
        {
          "id": "MDRW2100019757.1.11",
          "form": "네ㅎㅎ 저는 젤 매운맛으로",
          "original_form": "네ㅎㅎ 저는 젤 매운맛으로",
          "speaker_id": "1",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100019757.1.12",
          "form": "국물잇는게 좋더라구욤",
          "original_form": "국물잇는게 좋더라구욤",
          "speaker_id": "1",
          "time": "20210828 10:06"
        },
        {
          "id": "MDRW2100019757.1.13",
          "form": "우와 그렇군요 저는 볶음이 더좋아용 헤헤",
          "original_form": "우와 그렇군요 저는 볶음이 더좋아용 헤헤",
          "speaker_id": "2",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100019757.1.14",
          "form": "부산에 라라관 마라탕 정말 좋아하는데",
          "original_form": "부산에 라라관 마라탕 정말 좋아하는데",
          "speaker_id": "2",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100019757.1.15",
          "form": "맛집 있으신가요",
          "original_form": "맛집 있으신가요",
          "speaker_id": "2",
          "time": "20210828 11:34"
        },
        {
          "id": "MDRW2100019757.1.16",
          "form": "저도 거기 젤 좋아해요ㅋ",
          "original_form": "저도 거기 젤 좋아해요ㅋ",
          "speaker_id": "1",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100019757.1.17",
          "form": "부산마라탕은 서면 라라관임다",
          "original_form": "부산마라탕은 서면 라라관임다",
          "speaker_id": "1",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100019757.1.18",
          "form": "우와 아시능구나",
          "original_form": "우와 아시능구나",
          "speaker_id": "2",
          "time": "20210828 11:36"
        },
        {
          "id": "MDRW2100019757.1.19",
          "form": "넵 ㅎㅎ",
          "original_form": "넵 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100019757.1.20",
          "form": "지금은 트레이더스 가는길이에요",
          "original_form": "지금은 트레이더스 가는길이에요",
          "speaker_id": "1",
          "time": "20210828 11:38"
        },
        {
          "id": "MDRW2100019757.1.21",
          "form": "우와 뭐사러가세여",
          "original_form": "우와 뭐사러가세여",
          "speaker_id": "2",
          "time": "20210828 11:53"
        },
        {
          "id": "MDRW2100019757.1.22",
          "form": "간장게장 대박인데욤ㅎㅎㅎㅎ",
          "original_form": "간장게장 대박인데욤ㅎㅎㅎㅎ",
