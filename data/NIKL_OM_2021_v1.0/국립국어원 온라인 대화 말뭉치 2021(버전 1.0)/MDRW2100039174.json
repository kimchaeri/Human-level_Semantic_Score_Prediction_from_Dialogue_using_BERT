          "speaker_id": "2",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100039174.1.28",
          "form": "아근데",
          "original_form": "아근데",
          "speaker_id": "2",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100039174.1.29",
          "form": "에잇 몰랑 ㅎ.ㅎ.",
          "original_form": "에잇 몰랑 ㅎ.ㅎ.",
          "speaker_id": "2",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100039174.1.30",
          "form": "잘말안해",
          "original_form": "잘말안해",
          "speaker_id": "2",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100039174.1.31",
          "form": "그런건",
          "original_form": "그런건",
          "speaker_id": "2",
          "time": "20210903 12:51"
        },
        {
          "id": "MDRW2100039174.1.32",
          "form": "ㅋㅋ그럼 덩생들과는\r\n주로 어떤 대화를 나누시나염",
          "original_form": "ㅋㅋ그럼 덩생들과는\r\n주로 어떤 대화를 나누시나염",
          "speaker_id": "1",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100039174.1.33",
          "form": "먹을거요",
          "original_form": "먹을거요",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100039174.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100039174.1.35",
          "form": "맨날 메뉴정행",
          "original_form": "맨날 메뉴정행",
          "speaker_id": "2",
          "time": "20210903 12:53"
        },
        {
          "id": "MDRW2100039174.1.36",
          "form": "그거아니믄 영상같이보기 ?ㅋㅋ",
          "original_form": "그거아니믄 영상같이보기 ?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:54"
        },
        {
          "id": "MDRW2100039174.1.37",
          "form": "먹는걸로 대동단결되는 자매ㅋㅋ",
          "original_form": "먹는걸로 대동단결되는 자매ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:14"
        },
        {
          "id": "MDRW2100039174.1.38",
