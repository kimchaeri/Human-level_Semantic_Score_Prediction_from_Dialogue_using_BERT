          "speaker_id": "2",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100039174.1.3",
          "form": "어랏",
          "original_form": "어랏",
          "speaker_id": "2",
          "time": "20210903 12:41"
        },
        {
          "id": "MDRW2100039174.1.4",
          "form": "ㅋㅋㅋㅋㅋ아니 name3님도",
          "original_form": "ㅋㅋㅋㅋㅋ아니 &name3&님도",
          "speaker_id": "1",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100039174.1.5",
          "form": "지금 나가서 지내는거 아니에요?",
          "original_form": "지금 나가서 지내는거 아니에요?",
          "speaker_id": "1",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100039174.1.6",
          "form": "name3이",
          "original_form": "&name3&이",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100039174.1.7",
          "form": "맞아여",
          "original_form": "맞아여",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100039174.1.8",
          "form": "걔",
          "original_form": "걔",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100039174.1.9",
          "form": "일산쪽에",
          "original_form": "일산쪽에",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100039174.1.10",
          "form": "직장다니는데",
          "original_form": "직장다니는데",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100039174.1.11",
          "form": "거기서 기숙사 제공되가지고..ㅎㅎㅎ",
          "original_form": "거기서 기숙사 제공되가지고..ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100039174.1.12",
          "form": "거서 살쥬",
          "original_form": "거서 살쥬",
          "speaker_id": "2",
          "time": "20210903 12:42"
        },
        {
          "id": "MDRW2100039174.1.13",
          "form": "일산이면\r\n다닐만하지 않낭? ㅜㅜ",
          "original_form": "일산이면\r\n다닐만하지 않낭? ㅜㅜ",
          "speaker_id": "1",
          "time": "20210903 12:44"
        },
        {
          "id": "MDRW2100039174.1.14",
          "form": "대중교통으로는\r\n오래 걸리낭",
          "original_form": "대중교통으로는\r\n오래 걸리낭",
          "speaker_id": "1",
          "time": "20210903 12:44"
        },
        {
          "id": "MDRW2100039174.1.15",
          "form": "name4이는 원래\r\n직장 부평 아니었나영?",
          "original_form": "&name4&이는 원래\r\n직장 부평 아니었나영?",
          "speaker_id": "1",
          "time": "20210903 12:44"
        },
        {
          "id": "MDRW2100039174.1.16",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210903 12:46"
        },
        {
          "id": "MDRW2100039174.1.17",
          "form": "name4이 그만두고",
          "original_form": "&name4&이 그만두고",
          "speaker_id": "2",
          "time": "20210903 12:46"
        },
        {
          "id": "MDRW2100039174.1.18",
          "form": "대학다녀용",
          "original_form": "대학다녀용",
          "speaker_id": "2",
          "time": "20210903 12:46"
        },
        {
          "id": "MDRW2100039174.1.19",
          "form": "우왕ㅋㅋ 대학생이에영?",
          "original_form": "우왕ㅋㅋ 대학생이에영?",
          "speaker_id": "1",
          "time": "20210903 12:46"
        },
        {
          "id": "MDRW2100039174.1.20",
          "form": "뭐 전공해여?",
          "original_form": "뭐 전공해여?",
          "speaker_id": "1",
          "time": "20210903 12:47"
        },
        {
          "id": "MDRW2100039174.1.21",
          "form": "멩 ㅎ.ㅎ",
          "original_form": "멩 ㅎ.ㅎ",
          "speaker_id": "2",
          "time": "20210903 12:47"
        },
        {
          "id": "MDRW2100039174.1.22",
          "form": "간호학과였나",
          "original_form": "간호학과였나",
          "speaker_id": "2",
          "time": "20210903 12:47"
        },
        {
          "id": "MDRW2100039174.1.23",
          "form": "...기억이안나네 ㅎ",
          "original_form": "...기억이안나네 ㅎ",
          "speaker_id": "2",
          "time": "20210903 12:47"
        },
        {
          "id": "MDRW2100039174.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 12:47"
        },
        {
          "id": "MDRW2100039174.1.25",
          "form": "name3이도 간호학과 아니었나영?",
          "original_form": "&name3&이도 간호학과 아니었나영?",
          "speaker_id": "1",
          "time": "20210903 12:47"
        },
        {
          "id": "MDRW2100039174.1.26",
          "form": "가끔 님 보면\r\n덩생들이랑 동물들\r\n이름 아는것만도 신기ㅋㅋㅋㅋㅋ",
          "original_form": "가끔 님 보면\r\n덩생들이랑 동물들\r\n이름 아는것만도 신기ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 12:48"
        },
        {
          "id": "MDRW2100039174.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
