      "utterance": [
        {
          "id": "MDRW2100016718.1.1",
          "form": "유튭 보시나요",
          "original_form": "유튭 보시나요",
          "speaker_id": "1",
          "time": "20210826 15:06"
        },
        {
          "id": "MDRW2100016718.1.2",
          "form": "네~ 주식 공모주관련 영상 봐요",
          "original_form": "네~ 주식 공모주관련 영상 봐요",
          "speaker_id": "2",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100016718.1.3",
          "form": "추천채널있나요",
          "original_form": "추천채널있나요",
          "speaker_id": "1",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100016718.1.4",
          "form": "공모주린이 실시간 청약률이랑 평가관련요",
          "original_form": "공모주린이 실시간 청약률이랑 평가관련요",
          "speaker_id": "2",
          "time": "20210826 23:37"
        },
        {
          "id": "MDRW2100016718.1.5",
          "form": "오오 다른거 또뭐 보시나요?",
          "original_form": "오오 다른거 또뭐 보시나요?",
          "speaker_id": "1",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100016718.1.6",
          "form": "다른거는 빨간조끼 아저씨?ㅋㅋ",
          "original_form": "다른거는 빨간조끼 아저씨?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:23"
        },
        {
          "id": "MDRW2100016718.1.7",
          "form": "그게뭐죠??",
          "original_form": "그게뭐죠??",
          "speaker_id": "1",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100016718.1.8",
          "form": "그것도 공모주관련ㅎㅎ",
          "original_form": "그것도 공모주관련ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:31"
        },
        {
          "id": "MDRW2100016718.1.9",
          "form": "아하ㅋㅋㄱㄲ 주식에 관심이 많으시군요",
          "original_form": "아하ㅋㅋㄱㄲ 주식에 관심이 많으시군요",
          "speaker_id": "1",
          "time": "20210829 11:05"
        },
        {
          "id": "MDRW2100016718.1.10",
          "form": "아니요~ 그냥 과자값 벌려고 ㅋㅋ 진짜주식은 못 하구요",
          "original_form": "아니요~ 그냥 과자값 벌려고 ㅋㅋ 진짜주식은 못 하구요",
          "speaker_id": "2",
          "time": "20210829 11:39"
        },
        {
          "id": "MDRW2100016718.1.11",
          "form": "빨간조끼 함 봐야겠어요ㅎㅎ",
          "original_form": "빨간조끼 함 봐야겠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100016718.1.12",
          "form": "ㅋㅋ동네아저씨 같은 느낌",
          "original_form": "ㅋㅋ동네아저씨 같은 느낌",
          "speaker_id": "2",
          "time": "20210829 12:52"
        },
        {
          "id": "MDRW2100016718.1.13",
          "form": "이에요",
          "original_form": "이에요",
          "speaker_id": "2",
          "time": "20210829 12:52"
        },
        {
          "id": "MDRW2100016718.1.14",
          "form": "오구독자도꽤많네요",
          "original_form": "오구독자도꽤많네요",
