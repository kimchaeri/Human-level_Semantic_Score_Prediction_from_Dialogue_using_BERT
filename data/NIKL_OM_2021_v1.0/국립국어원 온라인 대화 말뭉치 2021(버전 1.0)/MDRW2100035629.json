      "utterance": [
        {
          "id": "MDRW2100035629.1.1",
          "form": "그래서 교토갔나봐요 그때ㅋㅋ",
          "original_form": "그래서 교토갔나봐요 그때ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100035629.1.2",
          "form": "교토가 가장 역사가 깊은 도시라고 들었던것같아요",
          "original_form": "교토가 가장 역사가 깊은 도시라고 들었던것같아요",
          "speaker_id": "1",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100035629.1.3",
          "form": "돈이 많이 없어서ㅋㅋ",
          "original_form": "돈이 많이 없어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100035629.1.4",
          "form": "한국으로치면 경주 느낌이라고",
          "original_form": "한국으로치면 경주 느낌이라고",
          "speaker_id": "1",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100035629.1.5",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100035629.1.6",
          "form": "저는 오사카랑 후쿠오카 갔어용",
          "original_form": "저는 오사카랑 후쿠오카 갔어용",
          "speaker_id": "1",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100035629.1.7",
          "form": "오 어떠셨나요",
          "original_form": "오 어떠셨나요",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100035629.1.8",
          "form": "유명한 이치란 라멘?집을 갔는데",
          "original_form": "유명한 이치란 라멘?집을 갔는데",
          "speaker_id": "1",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100035629.1.9",
          "form": "오사카는 좀 우리나라랑 성격이 비슷하다던데",
          "original_form": "오사카는 좀 우리나라랑 성격이 비슷하다던데",
          "speaker_id": "2",
          "time": "20210829 21:32"
        },
        {
          "id": "MDRW2100035629.1.10",
          "form": "직원분이 돈 받을때마다",
          "original_form": "직원분이 돈 받을때마다",
          "speaker_id": "1",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100035629.1.11",
          "form": "손을 쨕짝! 두번 치고",
          "original_form": "손을 쨕짝! 두번 치고",
          "speaker_id": "1",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100035629.1.12",
          "form": "이랏샤이마세",
          "original_form": "이랏샤이마세",
          "speaker_id": "2",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100035629.1.13",
          "form": "했나요ㅋㅋ",
          "original_form": "했나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100035629.1.14",
          "form": "되게 정중하고 90도 인사하시고ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "되게 정중하고 90도 인사하시고ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 21:33"
        },
        {
          "id": "MDRW2100035629.1.15",
          "form": "부담스러웠어요ㅠㅠㅠ",
          "original_form": "부담스러웠어요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 21:33"
        },
        {
