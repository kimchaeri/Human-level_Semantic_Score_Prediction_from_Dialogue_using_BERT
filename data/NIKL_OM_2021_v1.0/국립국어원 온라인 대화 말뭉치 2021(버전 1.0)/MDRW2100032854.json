      "utterance": [
        {
          "id": "MDRW2100032854.1.1",
          "form": "이게뭔데?",
          "original_form": "이게뭔데?",
          "speaker_id": "1",
          "time": "20210906 02:09"
        },
        {
          "id": "MDRW2100032854.1.2",
          "form": "뭐 아는 거 있어??",
          "original_form": "뭐 아는 거 있어??",
          "speaker_id": "1",
          "time": "20210906 02:10"
        },
        {
          "id": "MDRW2100032854.1.3",
          "form": "자세하게 알지는 못하고 예산을 증액했다는 사실정도만 알고 있어요",
          "original_form": "자세하게 알지는 못하고 예산을 증액했다는 사실정도만 알고 있어요",
          "speaker_id": "2",
          "time": "20210906 02:13"
        },
        {
          "id": "MDRW2100032854.1.4",
          "form": "흠미",
          "original_form": "흠미",
          "speaker_id": "1",
          "time": "20210906 02:14"
        },
        {
          "id": "MDRW2100032854.1.5",
          "form": "아는 게 있어야 대화를 하는데 ",
          "original_form": "아는 게 있어야 대화를 하는데 ",
          "speaker_id": "1",
          "time": "20210906 02:14"
        },
        {
          "id": "MDRW2100032854.1.6",
          "form": "난 좋은 것같애",
          "original_form": "난 좋은 것같애",
          "speaker_id": "1",
          "time": "20210906 02:14"
        },
        {
          "id": "MDRW2100032854.1.7",
          "form": "요즘 틱톡 개인정보 논란도 그렇구 ㅜ 불안",
          "original_form": "요즘 틱톡 개인정보 논란도 그렇구 ㅜ 불안",
          "speaker_id": "1",
          "time": "20210906 02:15"
        },
        {
          "id": "MDRW2100032854.1.8",
          "form": "여튼 그런 여론들이 있어서 예산이 증//액되었다고 생각해요",
          "original_form": "여튼 그런 여론들이 있어서 예산이 증//액되었다고 생각해요",
          "speaker_id": "2",
          "time": "20210906 02:20"
        },
        {
          "id": "MDRW2100032854.1.9",
          "form": "저도 최근 뉴스를 보면 그런 개인정보 팔리는 게 많아서 찬성해요",
          "original_form": "저도 최근 뉴스를 보면 그런 개인정보 팔리는 게 많아서 찬성해요",
          "speaker_id": "2",
          "time": "20210906 02:21"
        },
        {
          "id": "MDRW2100032854.1.10",
          "form": "회의나 학교 강의 같은 곳에서 줌을 많이 사용하는데 줌이 중국으로 개인정보를 넘긴다는 이야기도 있고",
          "original_form": "회의나 학교 강의 같은 곳에서 줌을 많이 사용하는데 줌이 중국으로 개인정보를 넘긴다는 이야기도 있고",
          "speaker_id": "2",
          "time": "20210906 02:22"
        },
        {
          "id": "MDRW2100032854.1.11",
          "form": "아 맞아요 줌!!!진짜 걱정 많이 되더라구요",
          "original_form": "아 맞아요 줌!!!진짜 걱정 많이 되더라구요",
          "speaker_id": "1",
          "time": "20210906 02:22"
        },
        {
          "id": "MDRW2100032854.1.12",
          "form": "ㅠㅠ이런 데에 사용되면 좋겠네요",
          "original_form": "ㅠㅠ이런 데에 사용되면 좋겠네요",
          "speaker_id": "1",
          "time": "20210906 02:22"
        },
        {
          "id": "MDRW2100032854.1.13",
          "form": "아니면 마케팅 전화들;;이요",
          "original_form": "아니면 마케팅 전화들;;이요",
          "speaker_id": "1",
          "time": "20210906 02:22"
        },
        {
          "id": "MDRW2100032854.1.14",
          "form": "마케팅 전화들은 진짜...",
          "original_form": "마케팅 전화들은 진짜...",
          "speaker_id": "2",
          "time": "20210906 02:23"
        },
        {
          "id": "MDRW2100032854.1.15",
          "form": "10년 전이나 지금이나 나아진 게 하나도 없어요",
          "original_form": "10년 전이나 지금이나 나아진 게 하나도 없어요",
          "speaker_id": "2",
          "time": "20210906 02:24"
        },
        {
          "id": "MDRW2100032854.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210906 02:24"
        },
        {
          "id": "MDRW2100032854.1.17",
          "form": "보험광고랑 휴대폰 광고들",
          "original_form": "보험광고랑 휴대폰 광고들",
          "speaker_id": "1",
          "time": "20210906 02:24"
        },
        {
          "id": "MDRW2100032854.1.18",
          "form": "애도 없는데 자녀교육을 위해 투자하라는 문자를 받지를 않나...",
          "original_form": "애도 없는데 자녀교육을 위해 투자하라는 문자를 받지를 않나...",
          "speaker_id": "2",
          "time": "20210906 02:24"
        },
        {
          "id": "MDRW2100032854.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ그런 문자도 와요? 황당하다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ그런 문자도 와요? 황당하다",
          "speaker_id": "1",
          "time": "20210906 02:25"
        },
        {
          "id": "MDRW2100032854.1.20",
          "form": "아마 그런 문자 오는 경로다 마케팅 제3자동의하면 오는 거라 하더라구요",
          "original_form": "아마 그런 문자 오는 경로다 마케팅 제3자동의하면 오는 거라 하더라구요",
          "speaker_id": "1",
          "time": "20210906 02:25"
        },
        {
          "id": "MDRW2100032854.1.21",
          "form": "전화번호 모아서 업체에 판대요ㅋㅋ 불법인데 안 걸리니까",
          "original_form": "전화번호 모아서 업체에 판대요ㅋㅋ 불법인데 안 걸리니까",
          "speaker_id": "1",
          "time": "20210906 02:25"
        },
        {
          "id": "MDRW2100032854.1.22",
          "form": "오... 귀찮아서 묶어서 전체동의했는데 이젠 안 해야겠네요",
          "original_form": "오... 귀찮아서 묶어서 전체동의했는데 이젠 안 해야겠네요",
          "speaker_id": "2",
          "time": "20210906 02:26"
        },
        {
          "id": "MDRW2100032854.1.23",
          "form": "네네 그거 하지 마세요ㅜ",
          "original_form": "네네 그거 하지 마세요ㅜ",
          "speaker_id": "1",
          "time": "20210906 02:27"
        },
        {
          "id": "MDRW2100032854.1.24",
          "form": "제 3자동의가 중요해용",
          "original_form": "제 3자동의가 중요해용",
          "speaker_id": "1",
          "time": "20210906 02:27"
        },
        {
          "id": "MDRW2100032854.1.25",
          "form": "침해방지에는 5억 증액했다니까 그런거 이젠 잡히려나",
          "original_form": "침해방지에는 5억 증액했다니까 그런거 이젠 잡히려나",
          "speaker_id": "2",
          "time": "20210906 02:27"
        },
        {
          "id": "MDRW2100032854.1.26",
          "form": "ㅋ... 꼭 단속했음 좋겠습니다",
          "original_form": "ㅋ... 꼭 단속했음 좋겠습니다",
