      "utterance": [
        {
          "id": "MDRW2100030007.1.1",
          "form": "좋아하는 스포츠가 있나요",
          "original_form": "좋아하는 스포츠가 있나요",
          "speaker_id": "1",
          "time": "20210903 18:24"
        },
        {
          "id": "MDRW2100030007.1.2",
          "form": "직접 하는건 탁구 좋아하고",
          "original_form": "직접 하는건 탁구 좋아하고",
          "speaker_id": "2",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100030007.1.3",
          "form": "보는건 배구, 농구 좋아해요",
          "original_form": "보는건 배구, 농구 좋아해요",
          "speaker_id": "2",
          "time": "20210903 20:42"
        },
        {
          "id": "MDRW2100030007.1.4",
          "form": "전 배구 보단 농구가 재미있어요",
          "original_form": "전 배구 보단 농구가 재미있어요",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100030007.1.5",
          "form": "그보단 야구가 잼있네요",
          "original_form": "그보단 야구가 잼있네요",
          "speaker_id": "1",
          "time": "20210903 20:46"
        },
        {
          "id": "MDRW2100030007.1.6",
          "form": "저는 배구보다 야구보면 조금 지루해서...",
          "original_form": "저는 배구보다 야구보면 조금 지루해서...",
          "speaker_id": "2",
          "time": "20210903 20:47"
        },
        {
          "id": "MDRW2100030007.1.7",
          "form": "야구장에서 치킨 먹는건 해보고 싶긴 합니다",
          "original_form": "야구장에서 치킨 먹는건 해보고 싶긴 합니다",
          "speaker_id": "2",
          "time": "20210903 20:47"
        },
        {
          "id": "MDRW2100030007.1.8",
          "form": "저도 오래전에 해보고 못했네요",
          "original_form": "저도 오래전에 해보고 못했네요",
          "speaker_id": "1",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100030007.1.9",
          "form": "그전이 그립네요",
          "original_form": "그전이 그립네요",
          "speaker_id": "1",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100030007.1.10",
          "form": "지금은 함성도 금지죠?",
          "original_form": "지금은 함성도 금지죠?",
          "speaker_id": "2",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100030007.1.11",
          "form": "치킨도 금지죠",
          "original_form": "치킨도 금지죠",
          "speaker_id": "1",
          "time": "20210903 21:57"
        },
        {
          "id": "MDRW2100030007.1.12",
          "form": "아쉬우시겠어요",
          "original_form": "아쉬우시겠어요",
          "speaker_id": "2",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100030007.1.13",
          "form": "너무나 아쉽고 코로나 이전이 그립죠",
          "original_form": "너무나 아쉽고 코로나 이전이 그립죠",
          "speaker_id": "1",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030007.1.14",
          "form": "코로나 끝나면 바로 가실건가요",
          "original_form": "코로나 끝나면 바로 가실건가요",
          "speaker_id": "2",
          "time": "20210903 23:03"
        },
        {
          "id": "MDRW2100030007.1.15",
          "form": "완전 종식되면 그때 가야죠",
          "original_form": "완전 종식되면 그때 가야죠",
          "speaker_id": "1",
          "time": "20210903 23:04"
        },
        {
          "id": "MDRW2100030007.1.16",
          "form": "ㅎㅎ좋아요",
          "original_form": "ㅎㅎ좋아요",
          "speaker_id": "2",
          "time": "20210903 23:04"
        }
