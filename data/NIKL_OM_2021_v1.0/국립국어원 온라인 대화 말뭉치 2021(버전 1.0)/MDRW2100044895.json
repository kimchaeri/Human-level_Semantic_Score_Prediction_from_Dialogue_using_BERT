      "utterance": [
        {
          "id": "MDRW2100044895.1.1",
          "form": "아니 원조격 하나 있는데",
          "original_form": "아니 원조격 하나 있는데",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044895.1.2",
          "form": "여기가 특허? 그런거 내서",
          "original_form": "여기가 특허? 그런거 내서",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044895.1.3",
          "form": "그 회사 말고는 인형을 못 만들어",
          "original_form": "그 회사 말고는 인형을 못 만들어",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044895.1.4",
          "form": "구관인형 한정 ㅇㅇ",
          "original_form": "구관인형 한정 ㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044895.1.5",
          "form": "무서운 곳이야.. 그러면서",
          "original_form": "무서운 곳이야.. 그러면서",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044895.1.6",
          "form": "다른 나라 인형 수입도 지들이 해",
          "original_form": "다른 나라 인형 수입도 지들이 해",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044895.1.7",
          "form": "그리고 더 비싸게 팔아(?)",
          "original_form": "그리고 더 비싸게 팔아(?)",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044895.1.8",
          "form": "와 ㅋㅋㅋㅋㅋ",
          "original_form": "와 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044895.1.9",
          "form": "완전 악덕기업이네",
          "original_form": "완전 악덕기업이네",
          "speaker_id": "1",
          "time": "20210908 00:09"
        },
        {
          "id": "MDRW2100044895.1.10",
          "form": "ㅋㅋㅋㅋㅋ 유명하지...",
          "original_form": "ㅋㅋㅋㅋㅋ 유명하지...",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044895.1.11",
          "form": "거기다 우리나라엔 우익 논란도 있어서;;",
          "original_form": "거기다 우리나라엔 우익 논란도 있어서;;",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044895.1.12",
          "form": "아하....",
          "original_form": "아하....",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044895.1.13",
          "form": "일본기업 상품이면",
          "original_form": "일본기업 상품이면",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044895.1.14",
          "form": "머 예쁜 얼굴도 많은데 나는.. 눈길을 주지 않으려고 한다..",
          "original_form": "머 예쁜 얼굴도 많은데 나는.. 눈길을 주지 않으려고 한다..",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044895.1.15",
          "form": "피할 수 없는 그것...",
          "original_form": "피할 수 없는 그것...",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044895.1.16",
          "form": "ㅇㅇ...",
          "original_form": "ㅇㅇ...",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044895.1.17",
          "form": "우익..",
          "original_form": "우익..",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044895.1.18",
          "form": "중국은 동북공정",
          "original_form": "중국은 동북공정",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044895.1.19",
          "form": "환장함",
          "original_form": "환장함",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044895.1.20",
          "form": "덕질 좀 편하게..합시다..",
          "original_form": "덕질 좀 편하게..합시다..",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044895.1.21",
          "form": "한국이라고 다른가? 그렇지 않다 어디든 박살이다",
          "original_form": "한국이라고 다른가? 그렇지 않다 어디든 박살이다",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044895.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그치 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그치 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044895.1.23",
          "form": "한국은 국내 업체라 아는",
          "original_form": "한국은 국내 업체라 아는",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044895.1.24",
          "form": "자잘한 판매처의 헛소리들..",
          "original_form": "자잘한 판매처의 헛소리들..",
          "speaker_id": "1",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044895.1.25",
          "form": "ㅋㅋㅋㅋ 그렇다...",
          "original_form": "ㅋㅋㅋㅋ 그렇다...",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044895.1.26",
          "form": "어디든...다 그렇다 하지만 국가적인 것만 아니라면..눈을 적당히 감고 사는거지....ㅠ",
          "original_form": "어디든...다 그렇다 하지만 국가적인 것만 아니라면..눈을 적당히 감고 사는거지....ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044895.1.27",
          "form": "ㅇㅇ 그치...",
          "original_form": "ㅇㅇ 그치...",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044895.1.28",
          "form": "물건 사면서 판매처 깨끗한데 찾기 젤 힘듦...",
          "original_form": "물건 사면서 판매처 깨끗한데 찾기 젤 힘듦...",
          "speaker_id": "1",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044895.1.29",
          "form": "ㅇㅇ.. 다들 헛소리 안하면 안되는 병이 있나봄...",
          "original_form": "ㅇㅇ.. 다들 헛소리 안하면 안되는 병이 있나봄...",
          "speaker_id": "2",
          "time": "20210908 00:17"
        },
        {
          "id": "MDRW2100044895.1.30",
          "form": "제발 맘껏 쇼핑하게 이미지 관리 좀 해달라...",
          "original_form": "제발 맘껏 쇼핑하게 이미지 관리 좀 해달라...",
          "speaker_id": "1",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044895.1.31",
          "form": "이미지가 니들의 전부라는 생각을 하고 살아라",
          "original_form": "이미지가 니들의 전부라는 생각을 하고 살아라",
          "speaker_id": "2",
          "time": "20210908 00:18"
        },
        {
          "id": "MDRW2100044895.1.32",
          "form": "그래도 우리 물건 살거잖아 하는 자신감은 할말 없지만 ㅠ",
          "original_form": "그래도 우리 물건 살거잖아 하는 자신감은 할말 없지만 ㅠ",
          "speaker_id": "2",
          "time": "20210908 00:19"
        }
      ]
