      "utterance": [
        {
          "id": "MDRW2100033543.1.1",
          "form": "주로 보는 영상 있으세요?",
          "original_form": "주로 보는 영상 있으세요?",
          "speaker_id": "2",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100033543.1.2",
          "form": "저는 요리영상 자주봐요!",
          "original_form": "저는 요리영상 자주봐요!",
          "speaker_id": "1",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100033543.1.3",
          "form": "오 누구꺼 자주 보세요?",
          "original_form": "오 누구꺼 자주 보세요?",
          "speaker_id": "2",
          "time": "20210824 13:25"
        },
        {
          "id": "MDRW2100033543.1.4",
          "form": "하루한끼님 자주 보는 편이에요 디저트류는 다른 분을 많이 보지만요",
          "original_form": "하루한끼님 자주 보는 편이에요 디저트류는 다른 분을 많이 보지만요",
          "speaker_id": "1",
          "time": "20210824 13:26"
        },
        {
          "id": "MDRW2100033543.1.5",
          "form": "우와.. 요리를 잘하시나봐요",
          "original_form": "우와.. 요리를 잘하시나봐요",
          "speaker_id": "2",
          "time": "20210824 13:30"
        },
        {
          "id": "MDRW2100033543.1.6",
          "form": "아뇨ㅋㅋㅋ 그냥 보는 거 예요 대리만족 느낌으로요",
          "original_form": "아뇨ㅋㅋㅋ 그냥 보는 거 예요 대리만족 느낌으로요",
          "speaker_id": "1",
          "time": "20210824 13:32"
        },
        {
          "id": "MDRW2100033543.1.7",
          "form": "ㅋㅋㅋㅋ아 뭔지 알죠",
          "original_form": "ㅋㅋㅋㅋ아 뭔지 알죠",
          "speaker_id": "2",
          "time": "20210824 13:43"
        },
        {
          "id": "MDRW2100033543.1.8",
          "form": "전 브이로그 자주 본답니다 ㅎ",
          "original_form": "전 브이로그 자주 본답니다 ㅎ",
          "speaker_id": "2",
          "time": "20210824 13:43"
        },
        {
          "id": "MDRW2100033543.1.9",
          "form": "브이로그는 누구 거 자주보세요?",
          "original_form": "브이로그는 누구 거 자주보세요?",
          "speaker_id": "1",
          "time": "20210824 14:03"
        },
        {
          "id": "MDRW2100033543.1.10",
          "form": "저 요새 트위티 이분이랑 유니크룡이요",
          "original_form": "저 요새 트위티 이분이랑 유니크룡이요",
          "speaker_id": "2",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100033543.1.11",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100033543.1.12",
          "form": "전 브이로그를 잘 안봐요...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 브이로그를 잘 안봐요...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100033543.1.13",
          "form": "그분들은 브이로그에서 주로 뭘 하시나요??",
          "original_form": "그분들은 브이로그에서 주로 뭘 하시나요??",
          "speaker_id": "1",
          "time": "20210824 14:16"
        },
        {
          "id": "MDRW2100033543.1.14",
          "form": "ㅋㅋ ㅋㅋㅋ 그냥 일상 보여줘요",
          "original_form": "ㅋㅋ ㅋㅋㅋ 그냥 일상 보여줘요",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100033543.1.15",
          "form": "요리하고 스킨케어하고 등",
          "original_form": "요리하고 스킨케어하고 등",
          "speaker_id": "2",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100033543.1.16",
          "form": "아ㅋㅋㅋㅋ 그렇군요 name2님이 알려주신 분들의 브이로그 저도 한 번 봐야겠어요!",
          "original_form": "아ㅋㅋㅋㅋ 그렇군요 &name2&님이 알려주신 분들의 브이로그 저도 한 번 봐야겠어요!",
          "speaker_id": "1",
          "time": "20210824 14:54"
        },
        {
          "id": "MDRW2100033543.1.17",
          "form": "ㅋㅋㅋㅋ 취향이 맞을지 모르겠지만",
          "original_form": "ㅋㅋㅋㅋ 취향이 맞을지 모르겠지만",
