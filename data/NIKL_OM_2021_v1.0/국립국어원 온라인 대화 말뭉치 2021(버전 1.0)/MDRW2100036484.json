      "utterance": [
        {
          "id": "MDRW2100036484.1.1",
          "form": "맞아요 안경 !!",
          "original_form": "맞아요 안경 !!",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036484.1.2",
          "form": "또 그 드레스 입은 무대",
          "original_form": "또 그 드레스 입은 무대",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036484.1.3",
          "form": "아시나요??!",
          "original_form": "아시나요??!",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036484.1.4",
          "form": "아 아 알아요 ㅠ ㅠ!!! 다 다른 색깔 드레스 입은!!",
          "original_form": "아 아 알아요 ㅠ ㅠ!!! 다 다른 색깔 드레스 입은!!",
          "speaker_id": "1",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036484.1.5",
          "form": "저 그 뮤대도 진짜 좋아해요",
          "original_form": "저 그 뮤대도 진짜 좋아해요",
          "speaker_id": "1",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036484.1.6",
          "form": "네네 !!",
          "original_form": "네네 !!",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036484.1.7",
          "form": "지효 엄청 이쁜 무대ㅜㅜ",
          "original_form": "지효 엄청 이쁜 무대ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036484.1.8",
          "form": "나연이 교복은 엠카였던거 같구 ㅋㅋ",
          "original_form": "나연이 교복은 엠카였던거 같구 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:43"
        },
        {
          "id": "MDRW2100036484.1.9",
          "form": "맞아요 ㅠ 지효 엄청 예뻤어요",
          "original_form": "맞아요 ㅠ 지효 엄청 예뻤어요",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036484.1.10",
          "form": "저 그 댄스 더 나닛 어웨이도 좋아해요",
          "original_form": "저 그 댄스 더 나닛 어웨이도 좋아해요",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036484.1.11",
          "form": "정연 예쁘게 나온!",
          "original_form": "정연 예쁘게 나온!",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036484.1.12",
          "form": "아 맞아요!",
          "original_form": "아 맞아요!",
          "speaker_id": "2",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100036484.1.13",
          "form": "여름에 꼭 들어요 ㅎㅎ",
          "original_form": "여름에 꼭 들어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100036484.1.14",
          "form": "정연이 ㅜㅜ 아프잖아요",
          "original_form": "정연이 ㅜㅜ 아프잖아요",
          "speaker_id": "2",
          "time": "20210830 23:47"
        },
        {
          "id": "MDRW2100036484.1.15",
          "form": "아니 그거 넘 맘 아파요 ㅠ.ㅠ 아파서 몸 부은 애한테",
          "original_form": "아니 그거 넘 맘 아파요 ㅠ.ㅠ 아파서 몸 부은 애한테",
          "speaker_id": "1",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036484.1.16",
          "form": "악플 달아서 공황꺼지 오게 만들고",
          "original_form": "악플 달아서 공황꺼지 오게 만들고",
          "speaker_id": "1",
          "time": "20210830 23:48"
        },
        {
          "id": "MDRW2100036484.1.17",
          "form": "아 맞아요ㅜㅜ",
          "original_form": "아 맞아요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036484.1.18",
          "form": "직캠도 안올라왔잖아요 그쯤에ㅜㅜ",
          "original_form": "직캠도 안올라왔잖아요 그쯤에ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 23:49"
        },
        {
          "id": "MDRW2100036484.1.19",
          "form": "그니까요... ㅠ 너무 맴 아파요 실력파 멤버 중 하나인데 ㅠㅠ",
          "original_form": "그니까요... ㅠ 너무 맴 아파요 실력파 멤버 중 하나인데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036484.1.20",
          "form": "얼른 나았우면 ㅠ.ㅠ",
          "original_form": "얼른 나았우면 ㅠ.ㅠ",
          "speaker_id": "1",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036484.1.21",
          "form": "정연이가 없으니까 ㅜㅜ",
          "original_form": "정연이가 없으니까 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036484.1.22",
          "form": "보컬들이 더 힘든 느낌이에요",
          "original_form": "보컬들이 더 힘든 느낌이에요",
          "speaker_id": "2",
          "time": "20210830 23:51"
        },
        {
          "id": "MDRW2100036484.1.23",
          "form": "그니께요 특히.그 아이 캔트 스탑 미였나",
          "original_form": "그니께요 특히.그 아이 캔트 스탑 미였나",
          "speaker_id": "1",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100036484.1.24",
          "form": "그건 ㅠ 음도 엄청 높대요",
          "original_form": "그건 ㅠ 음도 엄청 높대요",
          "speaker_id": "1",
          "time": "20210830 23:54"
        },
        {
          "id": "MDRW2100036484.1.25",
          "form": "ㅠ ㅠ 너무 혹사당하는 거 아닌지",
          "original_form": "ㅠ ㅠ 너무 혹사당하는 거 아닌지",
          "speaker_id": "1",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036484.1.26",
          "form": "와 저 그거보고",
          "original_form": "와 저 그거보고",
          "speaker_id": "2",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036484.1.27",
          "form": "ㅜㅜ 안타까웠어요 진짜ㅜㅜ",
          "original_form": "ㅜㅜ 안타까웠어요 진짜ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 23:55"
        },
        {
          "id": "MDRW2100036484.1.28",
          "form": "그니까요 ㅠㅠ 욕도 엄청 먹었잖아요",
          "original_form": "그니까요 ㅠㅠ 욕도 엄청 먹었잖아요",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036484.1.29",
          "form": "누가보면 범죄저인줄... ^.ㅠ ;",
          "original_form": "누가보면 범죄저인줄... ^.ㅠ ;",
          "speaker_id": "1",
          "time": "20210830 23:56"
        },
        {
          "id": "MDRW2100036484.1.30",
          "form": "아 맞아요 그 앵콜 ㅜㅜ",
          "original_form": "아 맞아요 그 앵콜 ㅜㅜ",
