      "utterance": [
        {
          "id": "MDRW2100011992.1.1",
          "form": "안녕하세요 혹시 종교가 있으세요? 저는 불자입니다",
          "original_form": "안녕하세요 혹시 종교가 있으세요? 저는 불자입니다",
          "speaker_id": "1",
          "time": "20210824 18:14"
        },
        {
          "id": "MDRW2100011992.1.2",
          "form": "저는 딱히 없는데",
          "original_form": "저는 딱히 없는데",
          "speaker_id": "2",
          "time": "20210824 18:14"
        },
        {
          "id": "MDRW2100011992.1.3",
          "form": "양쪽집안이 불교긴해요^^;",
          "original_form": "양쪽집안이 불교긴해요^^;",
          "speaker_id": "2",
          "time": "20210824 18:14"
        },
        {
          "id": "MDRW2100011992.1.4",
          "form": "아이들도 불교재단 유치원에 다니고있답니다!ㅋ",
          "original_form": "아이들도 불교재단 유치원에 다니고있답니다!ㅋ",
          "speaker_id": "2",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011992.1.5",
          "form": "그렇군요 반갑습니다",
          "original_form": "그렇군요 반갑습니다",
          "speaker_id": "1",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011992.1.6",
          "form": "네 반가워요^^",
          "original_form": "네 반가워요^^",
          "speaker_id": "2",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011992.1.7",
          "form": "불교유치원에 보내니",
          "original_form": "불교유치원에 보내니",
          "speaker_id": "2",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011992.1.8",
          "form": "스님이 선물을 주셔서 좋네요ㅋㅋㅋ",
          "original_form": "스님이 선물을 주셔서 좋네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011992.1.9",
          "form": "정말  좋네요",
          "original_form": "정말  좋네요",
          "speaker_id": "1",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011992.1.10",
          "form": "근데 종교에 크게 관심이없었는데",
          "original_form": "근데 종교에 크게 관심이없었는데",
          "speaker_id": "2",
          "time": "20210824 18:15"
        },
        {
          "id": "MDRW2100011992.1.11",
          "form": "코로나가 심하게 터지고 나서는",
          "original_form": "코로나가 심하게 터지고 나서는",
          "speaker_id": "2",
          "time": "20210824 18:16"
        },
        {
          "id": "MDRW2100011992.1.12",
          "form": "이단은 정말 싫더라구요",
          "original_form": "이단은 정말 싫더라구요",
          "speaker_id": "2",
          "time": "20210824 18:16"
        },
        {
          "id": "MDRW2100011992.1.13",
          "form": "종교 활동은 좋지만 모임행사는 자제해야할 시국인거같아요!",
          "original_form": "종교 활동은 좋지만 모임행사는 자제해야할 시국인거같아요!",
          "speaker_id": "2",
          "time": "20210824 18:16"
        },
        {
          "id": "MDRW2100011992.1.14",
          "form": "저도  그래요 이단들 너무 싫어요",
          "original_form": "저도  그래요 이단들 너무 싫어요",
          "speaker_id": "1",
          "time": "20210824 18:16"
        },
        {
          "id": "MDRW2100011992.1.15",
          "form": "굳이 모이지 말라는데 모여서는..",
          "original_form": "굳이 모이지 말라는데 모여서는..",
          "speaker_id": "2",
          "time": "20210824 18:16"
        },
        {
          "id": "MDRW2100011992.1.16",
          "form": "저도 불교인데 코로나이후 한번도 참석 안했어요",
          "original_form": "저도 불교인데 코로나이후 한번도 참석 안했어요",
          "speaker_id": "1",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100011992.1.17",
          "form": "불교는  그런부분 철저해요",
          "original_form": "불교는  그런부분 철저해요",
          "speaker_id": "1",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100011992.1.18",
          "form": "그쵸...다들 하고싶지만 못하는건데..ㅠㅠ",
          "original_form": "그쵸...다들 하고싶지만 못하는건데..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100011992.1.19",
          "form": "교회가 너무 심각하게 터졌어서  참 안타까웠어요",
          "original_form": "교회가 너무 심각하게 터졌어서  참 안타까웠어요",
          "speaker_id": "2",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100011992.1.20",
          "form": "종교활동은 이제 변화가 필요한거같아요!",
          "original_form": "종교활동은 이제 변화가 필요한거같아요!",
          "speaker_id": "2",
          "time": "20210824 18:17"
        },
        {
          "id": "MDRW2100011992.1.21",
          "form": "신천지부터  전광훈 무리수로  코로나  무서웠어요",
          "original_form": "신천지부터  전광훈 무리수로  코로나  무서웠어요",
          "speaker_id": "1",
          "time": "20210824 18:18"
        },
        {
          "id": "MDRW2100011992.1.22",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 18:18"
        },
        {
          "id": "MDRW2100011992.1.23",
          "form": "종교도  변화가  필요한  시기같아요",
          "original_form": "종교도  변화가  필요한  시기같아요",
          "speaker_id": "1",
          "time": "20210824 18:19"
        },
        {
          "id": "MDRW2100011992.1.24",
          "form": "그렇죠? 코로나 이전의 삶은 없다고들 하잖아요 슬프지만",
          "original_form": "그렇죠? 코로나 이전의 삶은 없다고들 하잖아요 슬프지만",
          "speaker_id": "2",
          "time": "20210824 18:19"
        },
        {
          "id": "MDRW2100011992.1.25",
          "form": "그게맞게 종교단체도 모임,행사에 변화가 필요하죠",
          "original_form": "그게맞게 종교단체도 모임,행사에 변화가 필요하죠",
          "speaker_id": "2",
          "time": "20210824 18:19"
        },
        {
          "id": "MDRW2100011992.1.26",
          "form": "저희아이들 유치원에",
          "original_form": "저희아이들 유치원에",
          "speaker_id": "2",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011992.1.27",
          "form": "부처님오신날",
          "original_form": "부처님오신날",
          "speaker_id": "2",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011992.1.28",
          "form": "현수막이 걸렸더라구요",
          "original_form": "현수막이 걸렸더라구요",
          "speaker_id": "2",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011992.1.29",
          "form": "성당에서 부처님오신날을 축하한다고~",
          "original_form": "성당에서 부처님오신날을 축하한다고~",
          "speaker_id": "2",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011992.1.30",
          "form": "너무 보기 좋았답니다^^",
          "original_form": "너무 보기 좋았답니다^^",
          "speaker_id": "2",
          "time": "20210824 18:20"
        },
        {
          "id": "MDRW2100011992.1.31",
          "form": "예전부터   성당과 불교는  종교의 이념을 뛰어넘는  행보를 많이  한걸로  알아요",
          "original_form": "예전부터   성당과 불교는  종교의 이념을 뛰어넘는  행보를 많이  한걸로  알아요",
          "speaker_id": "1",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100011992.1.32",
          "form": "맞아요  넓은 포용력? 이런것도 보기좋더라구요",
          "original_form": "맞아요  넓은 포용력? 이런것도 보기좋더라구요",
          "speaker_id": "2",
          "time": "20210824 18:21"
        },
        {
          "id": "MDRW2100011992.1.33",
          "form": "너무 강하게 배척하는것도 보기좋지않아요ㅜ",
          "original_form": "너무 강하게 배척하는것도 보기좋지않아요ㅜ",
          "speaker_id": "2",
          "time": "20210824 18:22"
        }
      ]
