        {
          "id": "MDRW2100003607.1.2",
          "form": "네 안녕하세요~",
          "original_form": "네 안녕하세요~",
          "speaker_id": "2",
          "time": "20210821 08:15"
        },
        {
          "id": "MDRW2100003607.1.3",
          "form": "지금 계신곳 비오나요?",
          "original_form": "지금 계신곳 비오나요?",
          "speaker_id": "2",
          "time": "20210821 08:16"
        },
        {
          "id": "MDRW2100003607.1.4",
          "form": "네 비가오고있어요",
          "original_form": "네 비가오고있어요",
          "speaker_id": "1",
          "time": "20210821 08:16"
        },
        {
          "id": "MDRW2100003607.1.5",
          "form": "name2님이 사는 곳도 비가오나요?",
          "original_form": "&name2&님이 사는 곳도 비가오나요?",
          "speaker_id": "1",
          "time": "20210821 08:16"
        },
        {
          "id": "MDRW2100003607.1.6",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 08:16"
        },
        {
          "id": "MDRW2100003607.1.7",
          "form": "가을장마래요ㅜ",
          "original_form": "가을장마래요ㅜ",
          "speaker_id": "2",
          "time": "20210821 08:16"
        },
        {
          "id": "MDRW2100003607.1.8",
          "form": "아 장마구나ㅠ",
          "original_form": "아 장마구나ㅠ",
          "speaker_id": "1",
          "time": "20210821 08:16"
        },
        {
          "id": "MDRW2100003607.1.9",
          "form": "비오는 날 어떠세요?",
          "original_form": "비오는 날 어떠세요?",
          "speaker_id": "1",
          "time": "20210821 08:17"
        },
        {
          "id": "MDRW2100003607.1.10",
          "form": "다음주 내내 비온다던데요",
          "original_form": "다음주 내내 비온다던데요",
          "speaker_id": "2",
          "time": "20210821 08:17"
        },
        {
          "id": "MDRW2100003607.1.11",
          "form": "다음주도요?",
          "original_form": "다음주도요?",
          "speaker_id": "1",
          "time": "20210821 08:17"
        },
        {
          "id": "MDRW2100003607.1.12",
          "form": "저는 눈 비 둘다시러해요...ㅋㅋㅋ",
          "original_form": "저는 눈 비 둘다시러해요...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:17"
        },
        {
          "id": "MDRW2100003607.1.13",
          "form": "저두요ㅎㅎ",
          "original_form": "저두요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 08:17"
        },
        {
          "id": "MDRW2100003607.1.14",
          "form": "비는 습해서 싫고 눈은 미끄러워서 싫어요",
          "original_form": "비는 습해서 싫고 눈은 미끄러워서 싫어요",
          "speaker_id": "1",
          "time": "20210821 08:17"
        },
        {
          "id": "MDRW2100003607.1.15",
          "form": "비는 좋아하시는분 많던데",
          "original_form": "비는 좋아하시는분 많던데",
          "speaker_id": "2",
          "time": "20210821 08:18"
        },
        {
          "id": "MDRW2100003607.1.16",
          "form": "눈은 남자라면 거의 싫어할듯하고 ㅋㅋ",
          "original_form": "눈은 남자라면 거의 싫어할듯하고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 08:18"
        },
        {
          "id": "MDRW2100003607.1.17",
          "form": "군대때문에요?",
          "original_form": "군대때문에요?",
          "speaker_id": "1",
          "time": "20210821 08:18"
        },
        {
          "id": "MDRW2100003607.1.18",
          "form": "제설작업 자주했다하더라구요",
          "original_form": "제설작업 자주했다하더라구요",
          "speaker_id": "1",
          "time": "20210821 08:18"
        },
        {
          "id": "MDRW2100003607.1.19",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 08:19"
        },
        {
          "id": "MDRW2100003607.1.20",
