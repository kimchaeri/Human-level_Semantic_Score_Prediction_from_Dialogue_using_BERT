        {
          "id": "MDRW2100046068.1.1",
          "form": "너 그거 아니",
          "original_form": "너 그거 아니",
          "speaker_id": "2",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100046068.1.2",
          "form": "어떤 거",
          "original_form": "어떤 거",
          "speaker_id": "1",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100046068.1.3",
          "form": "사실 바나나 우유는 하예",
          "original_form": "사실 바나나 우유는 하예",
          "speaker_id": "2",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100046068.1.4",
          "form": "거짓말 하지마",
          "original_form": "거짓말 하지마",
          "speaker_id": "1",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100046068.1.5",
          "form": "너가 이제까지 먹은 게 노랗다고 생각했지",
          "original_form": "너가 이제까지 먹은 게 노랗다고 생각했지",
          "speaker_id": "2",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100046068.1.6",
          "form": "바나나는 노라니까",
          "original_form": "바나나는 노라니까",
          "speaker_id": "1",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100046068.1.7",
          "form": "하지만 바나나 우유는 하얀색이야",
          "original_form": "하지만 바나나 우유는 하얀색이야",
          "speaker_id": "2",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100046068.1.8",
          "form": "그럼 색소를 넣어서 노랗게 만드나 보다",
          "original_form": "그럼 색소를 넣어서 노랗게 만드나 보다",
          "speaker_id": "1",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100046068.1.9",
          "form": "괜찮아",
          "original_form": "괜찮아",
          "speaker_id": "2",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100046068.1.10",
          "form": "바나나 우유는 맛있어",
          "original_form": "바나나 우유는 맛있어",
          "speaker_id": "1",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100046068.1.11",
          "form": "어렸을 때부터 목욕하고 하나씩 꼭 먹었는데",
          "original_form": "어렸을 때부터 목욕하고 하나씩 꼭 먹었는데",
          "speaker_id": "2",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100046068.1.12",
          "form": "맞아 추억이지",
          "original_form": "맞아 추억이지",
          "speaker_id": "1",
          "time": "20210908 10:01"
        },
        {
          "id": "MDRW2100046068.1.13",
          "form": "오늘도 하나 사 먹어야겠다",
          "original_form": "오늘도 하나 사 먹어야겠다",
