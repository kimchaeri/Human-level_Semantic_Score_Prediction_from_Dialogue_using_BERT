      "utterance": [
        {
          "id": "MDRW2100008985.1.1",
          "form": "반려동물키우세요?",
          "original_form": "반려동물키우세요?",
          "speaker_id": "2",
          "time": "20210822 20:11"
        },
        {
          "id": "MDRW2100008985.1.2",
          "form": "아뇨..보는것만 좋아해요ㅎㅎ 키우세요?",
          "original_form": "아뇨..보는것만 좋아해요ㅎㅎ 키우세요?",
          "speaker_id": "1",
          "time": "20210822 20:11"
        },
        {
          "id": "MDRW2100008985.1.3",
          "form": "아뇨 저도 보는것만요!",
          "original_form": "아뇨 저도 보는것만요!",
          "speaker_id": "2",
          "time": "20210822 20:12"
        },
        {
          "id": "MDRW2100008985.1.4",
          "form": "열심히 보다보니까 오히려 키우기 겁나더라구요",
          "original_form": "열심히 보다보니까 오히려 키우기 겁나더라구요",
          "speaker_id": "2",
          "time": "20210822 20:13"
        },
        {
          "id": "MDRW2100008985.1.5",
          "form": "저두요 유투브로 키우는 동물들보는데 귀엽고 또 한편으론 힘들겠다 싶어요",
          "original_form": "저두요 유투브로 키우는 동물들보는데 귀엽고 또 한편으론 힘들겠다 싶어요",
          "speaker_id": "1",
          "time": "20210822 20:13"
        },
        {
          "id": "MDRW2100008985.1.6",
          "form": "케어해 줘야하는게 엄청나더라고요",
          "original_form": "케어해 줘야하는게 엄청나더라고요",
          "speaker_id": "2",
          "time": "20210822 20:14"
        },
        {
          "id": "MDRW2100008985.1.7",
          "form": "맞아요 ㅠㅜ귀여운 면만 있는게 아니었던ㅜㅜ",
          "original_form": "맞아요 ㅠㅜ귀여운 면만 있는게 아니었던ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 20:15"
        },
        {
          "id": "MDRW2100008985.1.8",
          "form": "또 나이들면 할머니같이 돌봐줘야하고요",
          "original_form": "또 나이들면 할머니같이 돌봐줘야하고요",
          "speaker_id": "2",
          "time": "20210822 20:15"
        },
        {
          "id": "MDRW2100008985.1.9",
          "form": "여러모로 책임감이 있어야하는데 저는 그정도 마음이 아닌거같아서ㅠㅠㅠ",
          "original_form": "여러모로 책임감이 있어야하는데 저는 그정도 마음이 아닌거같아서ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 20:16"
        },
        {
          "id": "MDRW2100008985.1.10",
          "form": "맞아요 막상 보면 진짜 내아기같고 그렇다던데요",
          "original_form": "맞아요 막상 보면 진짜 내아기같고 그렇다던데요",
          "speaker_id": "2",
          "time": "20210822 20:17"
        },
        {
          "id": "MDRW2100008985.1.11",
          "form": "할수있을까? 생각하면자신없어요",
          "original_form": "할수있을까? 생각하면자신없어요",
          "speaker_id": "2",
          "time": "20210822 20:17"
        },
        {
          "id": "MDRW2100008985.1.12",
          "form": "맞아요 제자신이 안믿겨지고ㅎㅎ",
          "original_form": "맞아요 제자신이 안믿겨지고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 20:17"
        },
        {
          "id": "MDRW2100008985.1.13",
          "form": "만약 키운다면 어떤동물 키우고싶으세요?",
          "original_form": "만약 키운다면 어떤동물 키우고싶으세요?",
          "speaker_id": "2",
          "time": "20210822 20:18"
        },
        {
          "id": "MDRW2100008985.1.14",
          "form": "저는 고양이.ㅎㅎ",
          "original_form": "저는 고양이.ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 20:19"
        },
        {
          "id": "MDRW2100008985.1.15",
          "form": "저도 고양이 넘 좋아해요ㅠ 저는 치즈냥이가 넘 귀여워요",
          "original_form": "저도 고양이 넘 좋아해요ㅠ 저는 치즈냥이가 넘 귀여워요",
          "speaker_id": "2",
          "time": "20210822 20:19"
        },
        {
          "id": "MDRW2100008985.1.16",
          "form": "저도 그래요",
          "original_form": "저도 그래요",
          "speaker_id": "1",
          "time": "20210822 20:20"
        }
      ]
