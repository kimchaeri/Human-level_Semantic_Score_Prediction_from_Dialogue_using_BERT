      "utterance": [
        {
          "id": "MDRW2100028894.1.1",
          "form": "name1 여행 나보다 많이 가니까 얘기좀 풀어봐요",
          "original_form": "&name1& 여행 나보다 많이 가니까 얘기좀 풀어봐요",
          "speaker_id": "2",
          "time": "20210902 22:41"
        },
        {
          "id": "MDRW2100028894.1.2",
          "form": "얘기해준 게 없나?",
          "original_form": "얘기해준 게 없나?",
          "speaker_id": "1",
          "time": "20210902 22:42"
        },
        {
          "id": "MDRW2100028894.1.3",
          "form": "일본 네번 중국 한번 북한 한번 유럽을 가기 위해 카타르에도 경유하고 독일 프라하 오스트리아 스위스 이탈리아 바티칸 간듯",
          "original_form": "일본 네번 중국 한번 북한 한번 유럽을 가기 위해 카타르에도 경유하고 독일 프라하 오스트리아 스위스 이탈리아 바티칸 간듯",
          "speaker_id": "1",
          "time": "20210902 22:44"
        },
        {
          "id": "MDRW2100028894.1.4",
          "form": "많이갔군...",
          "original_form": "많이갔군...",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100028894.1.5",
          "form": "와....",
          "original_form": "와....",
          "speaker_id": "2",
          "time": "20210902 22:52"
        },
        {
          "id": "MDRW2100028894.1.6",
          "form": "아니... 생각보다 더 많이가심...",
          "original_form": "아니... 생각보다 더 많이가심...",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028894.1.7",
          "form": "name1 넘나 활동적...",
          "original_form": "&name1& 넘나 활동적...",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028894.1.8",
          "form": "유럽갈때 나라를 많이 찍었지",
          "original_form": "유럽갈때 나라를 많이 찍었지",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028894.1.9",
          "form": "정작 제주도는 안감",
          "original_form": "정작 제주도는 안감",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028894.1.10",
          "form": "한국여행도 좀 해요 ㅋㅋㅋ",
          "original_form": "한국여행도 좀 해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100028894.1.11",
          "form": "그래두 제주도만 안갔지 한반도는",
          "original_form": "그래두 제주도만 안갔지 한반도는",
          "speaker_id": "1",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100028894.1.12",
          "form": "많이 돌아다녔어!",
          "original_form": "많이 돌아다녔어!",
          "speaker_id": "1",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100028894.1.13",
          "form": "속초랑 강릉도 많이 갔고",
          "original_form": "속초랑 강릉도 많이 갔고",
          "speaker_id": "1",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100028894.1.14",
          "form": "동쪽 남쪽 종횡무진했다",
          "original_form": "동쪽 남쪽 종횡무진했다",
          "speaker_id": "1",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100028894.1.15",
          "form": "근데 진짜 많이갔다",
          "original_form": "근데 진짜 많이갔다",
          "speaker_id": "2",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100028894.1.16",
          "form": "나는 집이 전부에요",
          "original_form": "나는 집이 전부에요",
          "speaker_id": "2",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100028894.1.17",
          "form": "아드님은 돌아다니는 거 안좋아하지 않나?¿",
          "original_form": "아드님은 돌아다니는 거 안좋아하지 않나?¿",
          "speaker_id": "1",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100028894.1.18",
          "form": "나는 여행은 좋아해서..",
          "original_form": "나는 여행은 좋아해서..",
          "speaker_id": "1",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100028894.1.19",
          "form": "안조아해....",
          "original_form": "안조아해....",
          "speaker_id": "2",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100028894.1.20",
          "form": "집이 여행지야",
          "original_form": "집이 여행지야",
          "speaker_id": "2",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100028894.1.21",
          "form": "영상으로 즐기겠어요",
          "original_form": "영상으로 즐기겠어요",
          "speaker_id": "2",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100028894.1.22",
          "form": "내가 텔레포트를 쓸수잇다면",
          "original_form": "내가 텔레포트를 쓸수잇다면",
          "speaker_id": "2",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100028894.1.23",
          "form": "감",
          "original_form": "감",
          "speaker_id": "2",
          "time": "20210902 23:02"
        },
        {
          "id": "MDRW2100028894.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 차로 이동하는 걸 제일 안좋아하나보다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 차로 이동하는 걸 제일 안좋아하나보다",
          "speaker_id": "1",
          "time": "20210902 23:18"
        },
        {
          "id": "MDRW2100028894.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ약간 그거",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ약간 그거",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028894.1.26",
          "form": "내가 일본갔을때",
          "original_form": "내가 일본갔을때",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028894.1.27",
          "form": "딱 휴가철이라...",
          "original_form": "딱 휴가철이라...",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028894.1.28",
          "form": "입국심사 오래걸렿거든",
          "original_form": "입국심사 오래걸렿거든",
          "speaker_id": "2",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100028894.1.29",
          "form": "그런거 넘 싫어서....",
          "original_form": "그런거 넘 싫어서....",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100028894.1.30",
          "form": "텔레포트로 불법입국할래",
          "original_form": "텔레포트로 불법입국할래",
          "speaker_id": "2",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100028894.1.31",
          "form": "난 입국심사는 오래걸린 적은 없는데... 이게 뭐야했던게 독일 첨 갔을 때 였던 거 같기도 하고ㅋㅋㅋㅋㅋ",
          "original_form": "난 입국심사는 오래걸린 적은 없는데... 이게 뭐야했던게 독일 첨 갔을 때 였던 거 같기도 하고ㅋㅋㅋㅋㅋ",
