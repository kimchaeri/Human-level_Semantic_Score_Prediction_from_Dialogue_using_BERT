      "utterance": [
        {
          "id": "MDRW2100005395.1.1",
          "form": "여행 어디어디 가보셨어요?",
          "original_form": "여행 어디어디 가보셨어요?",
          "speaker_id": "1",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100005395.1.2",
          "form": "갑자기 궁금해졌어요",
          "original_form": "갑자기 궁금해졌어요",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005395.1.3",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005395.1.4",
          "form": "되게 많이 다니신 것 같아요",
          "original_form": "되게 많이 다니신 것 같아요",
          "speaker_id": "1",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005395.1.5",
          "form": "저요 저 막엄청다니진않고요 \r\n미국 라스베가스 시애틀 캐나다 밴쿠버 빅토리아 록키 남미저루트 대만 타이페이랑 베트남 다낭 일케갔다왔어요",
          "original_form": "저요 저 막엄청다니진않고요 \r\n미국 라스베가스 시애틀 캐나다 밴쿠버 빅토리아 록키 남미저루트 대만 타이페이랑 베트남 다낭 일케갔다왔어요",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005395.1.6",
          "form": "name1님은여 name1님 여행많이다니신거같은데!",
          "original_form": "&name1&님은여 &name1&님 여행많이다니신거같은데!",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005395.1.7",
          "form": "으와",
          "original_form": "으와",
          "speaker_id": "1",
          "time": "20210821 23:00"
        },
        {
          "id": "MDRW2100005395.1.8",
          "form": "저 캐나다 진짜 가고 싶었는데ㅜㅜ",
          "original_form": "저 캐나다 진짜 가고 싶었는데ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 23:00"
        },
        {
          "id": "MDRW2100005395.1.9",
          "form": "코로나 끝나면 꼭 갈 거예요,,,",
          "original_form": "코로나 끝나면 꼭 갈 거예요,,,",
          "speaker_id": "1",
          "time": "20210821 23:00"
        },
        {
          "id": "MDRW2100005395.1.10",
          "form": "제가 학기중에 워홀갔다와서 캐나다 밴쿠버에서 1년있었어여 ㅋㅋㅋㅋ",
          "original_form": "제가 학기중에 워홀갔다와서 캐나다 밴쿠버에서 1년있었어여 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005395.1.11",
          "form": "그때모은돈으로 즉흥적으로 남미갔다왔었네여",
          "original_form": "그때모은돈으로 즉흥적으로 남미갔다왔었네여",
          "speaker_id": "2",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005395.1.12",
          "form": "헉 워홀 너무 너무 제 이상이에요ㅜ",
          "original_form": "헉 워홀 너무 너무 제 이상이에요ㅜ",
          "speaker_id": "1",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005395.1.13",
          "form": "진짜 멋진 삶이네요...",
          "original_form": "진짜 멋진 삶이네요...",
          "speaker_id": "1",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005395.1.14",
          "form": "외노자의삶이었어여...ㅋㅋㅋㅋ",
          "original_form": "외노자의삶이었어여...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:06"
        },
        {
          "id": "MDRW2100005395.1.15",
          "form": "가게앞이 해변이었는데 브레이크타임마다 돗자리들고나가서 쉬었던게 젤기억에남아요ㅜ",
          "original_form": "가게앞이 해변이었는데 브레이크타임마다 돗자리들고나가서 쉬었던게 젤기억에남아요ㅜ",
          "speaker_id": "2",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005395.1.16",
          "form": "외노자ㅋㅋㅋㅋ큐ㅠㅠ",
          "original_form": "외노자ㅋㅋㅋㅋ큐ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005395.1.17",
          "form": "그래도 하나같이 너무 멋있어요",
          "original_form": "그래도 하나같이 너무 멋있어요",
          "speaker_id": "1",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005395.1.18",
          "form": "저는 여행 빼고는",
          "original_form": "저는 여행 빼고는",
          "speaker_id": "1",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005395.1.19",
          "form": "외국에서 살거나 이런 게 앖어수",
          "original_form": "외국에서 살거나 이런 게 앖어수",
          "speaker_id": "1",
          "time": "20210821 23:08"
        },
        {
          "id": "MDRW2100005395.1.20",
          "form": "저 어렸을때 가졌던 유일한 목표였어서 ㅋㅋㅋㅋ",
