      "utterance": [
        {
          "id": "MDRW2100046371.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211019 17:45"
        },
        {
          "id": "MDRW2100046371.1.2",
          "form": "안녕하세요! 저녁메뉴 정하셨어요?",
          "original_form": "안녕하세요! 저녁메뉴 정하셨어요?",
          "speaker_id": "2",
          "time": "20211019 17:45"
        },
        {
          "id": "MDRW2100046371.1.3",
          "form": "떡볶이 먹으려구요",
          "original_form": "떡볶이 먹으려구요",
          "speaker_id": "1",
          "time": "20211019 17:45"
        },
        {
          "id": "MDRW2100046371.1.4",
          "form": "요즘 추워서 어묵탕에",
          "original_form": "요즘 추워서 어묵탕에",
          "speaker_id": "1",
          "time": "20211019 17:46"
        },
        {
          "id": "MDRW2100046371.1.5",
          "form": "찰진 떡볶이 먹으면 맛있더라구요",
          "original_form": "찰진 떡볶이 먹으면 맛있더라구요",
          "speaker_id": "1",
          "time": "20211019 17:46"
        },
        {
          "id": "MDRW2100046371.1.6",
          "form": "와 대박 진짜 땡기네요 떡볶이랑 어묵탕..",
          "original_form": "와 대박 진짜 땡기네요 떡볶이랑 어묵탕..",
          "speaker_id": "2",
          "time": "20211019 17:47"
        },
        {
          "id": "MDRW2100046371.1.7",
          "form": "날이 추워지니까 뜨거운 음식이 잘 들어가는 것 같아요 ㅋㅋ",
          "original_form": "날이 추워지니까 뜨거운 음식이 잘 들어가는 것 같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 17:47"
        },
        {
          "id": "MDRW2100046371.1.8",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20211019 17:53"
        },
        {
          "id": "MDRW2100046371.1.9",
          "form": "뭐드세요?",
          "original_form": "뭐드세요?",
          "speaker_id": "1",
          "time": "20211019 17:53"
        },
        {
          "id": "MDRW2100046371.1.10",
          "form": "저는 라면에 삼김먹을 생각이었어요",
          "original_form": "저는 라면에 삼김먹을 생각이었어요",
          "speaker_id": "2",
          "time": "20211019 17:54"
        },
        {
          "id": "MDRW2100046371.1.11",
          "form": "국물이 땡겨서요 ㅎ",
          "original_form": "국물이 땡겨서요 ㅎ",
          "speaker_id": "2",
          "time": "20211019 17:54"
        },
        {
          "id": "MDRW2100046371.1.12",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20211019 17:54"
        },
        {
          "id": "MDRW2100046371.1.13",
          "form": "야채는",
          "original_form": "야채는",
          "speaker_id": "1",
          "time": "20211019 17:54"
        },
        {
          "id": "MDRW2100046371.1.14",
          "form": "안드시나요?",
          "original_form": "안드시나요?",
          "speaker_id": "1",
          "time": "20211019 17:54"
        },
        {
          "id": "MDRW2100046371.1.15",
          "form": "식이섬유가 풍부해야 소화가 잘되는데",
          "original_form": "식이섬유가 풍부해야 소화가 잘되는데",
          "speaker_id": "1",
          "time": "20211019 17:54"
        },
        {
          "id": "MDRW2100046371.1.16",
          "form": "야채요...? 먹고나서 과일은 먹지만...",
          "original_form": "야채요...? 먹고나서 과일은 먹지만...",
          "speaker_id": "2",
          "time": "20211019 17:54"
        },
        {
          "id": "MDRW2100046371.1.17",
          "form": "저는 야채 잘 안먹어요 ㅋㅋ",
          "original_form": "저는 야채 잘 안먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20211019 17:54"
        },
        {
          "id": "MDRW2100046371.1.18",
          "form": "야채를 잘 챙겨드시나요?",
          "original_form": "야채를 잘 챙겨드시나요?",
          "speaker_id": "2",
          "time": "20211019 17:55"
        },
        {
          "id": "MDRW2100046371.1.19",
          "form": "먹고나서 과일먹으면 안돼요",
          "original_form": "먹고나서 과일먹으면 안돼요",
          "speaker_id": "1",
          "time": "20211019 17:55"
        },
        {
          "id": "MDRW2100046371.1.20",
          "form": "아침에 공복에드세여",
          "original_form": "아침에 공복에드세여",
          "speaker_id": "1",
          "time": "20211019 17:55"
        },
        {
          "id": "MDRW2100046371.1.21",
          "form": "야채 매일 챙겨먹어요",
          "original_form": "야채 매일 챙겨먹어요",
          "speaker_id": "1",
          "time": "20211019 17:55"
        },
        {
          "id": "MDRW2100046371.1.22",
          "form": "와 진짜 건강한 식습관이네요..",
          "original_form": "와 진짜 건강한 식습관이네요..",
          "speaker_id": "2",
          "time": "20211019 17:56"
        },
        {
          "id": "MDRW2100046371.1.23",
          "form": "밥먹고 과일 먹으면 안좋나요??",
          "original_form": "밥먹고 과일 먹으면 안좋나요??",
          "speaker_id": "2",
          "time": "20211019 17:56"
        },
        {
          "id": "MDRW2100046371.1.24",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20211019 17:56"
        },
        {
          "id": "MDRW2100046371.1.25",
          "form": "포도당 때문에",
          "original_form": "포도당 때문에",
          "speaker_id": "1",
          "time": "20211019 17:56"
        },
        {
          "id": "MDRW2100046371.1.26",
          "form": "과일이 설탕보다 빠르게 흡수되요",
          "original_form": "과일이 설탕보다 빠르게 흡수되요",
          "speaker_id": "1",
          "time": "20211019 17:57"
        },
        {
          "id": "MDRW2100046371.1.27",
          "form": "과당이 몸을 보호하려고 만든건데",
          "original_form": "과당이 몸을 보호하려고 만든건데",
          "speaker_id": "1",
          "time": "20211019 17:57"
        },
        {
          "id": "MDRW2100046371.1.28",
          "form": "밥먹고 바로 먹으면 오히려 안좋아요",
          "original_form": "밥먹고 바로 먹으면 오히려 안좋아요",
          "speaker_id": "1",
          "time": "20211019 17:57"
        },
        {
          "id": "MDRW2100046371.1.29",
          "form": "오.... 건강한 팁 감사합니다.",
          "original_form": "오.... 건강한 팁 감사합니다.",
          "speaker_id": "2",
          "time": "20211019 17:58"
        },
        {
          "id": "MDRW2100046371.1.30",
          "form": "이제 과일은 공복에 먹을게요 ㅎㅎ",
          "original_form": "이제 과일은 공복에 먹을게요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 17:58"
        }
      ]
    }
