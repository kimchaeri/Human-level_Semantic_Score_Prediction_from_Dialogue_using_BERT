      "utterance": [
        {
          "id": "MDRW2100028253.1.1",
          "form": "여행간지 진짜 오래됐다",
          "original_form": "여행간지 진짜 오래됐다",
          "speaker_id": "1",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100028253.1.2",
          "form": "맞아 수영장이나 바다 가고 싶어",
          "original_form": "맞아 수영장이나 바다 가고 싶어",
          "speaker_id": "2",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100028253.1.3",
          "form": "여름마다 에버랜드로 퍼레이드 가는거 좋았는데",
          "original_form": "여름마다 에버랜드로 퍼레이드 가는거 좋았는데",
          "speaker_id": "1",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100028253.1.4",
          "form": "아쉬워",
          "original_form": "아쉬워",
          "speaker_id": "1",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100028253.1.5",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100028253.1.6",
          "form": "내가 여태 즐겼던 최고의 퍼레이드였는데",
          "original_form": "내가 여태 즐겼던 최고의 퍼레이드였는데",
          "speaker_id": "2",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100028253.1.7",
          "form": "난 부산도 가고싶어",
          "original_form": "난 부산도 가고싶어",
          "speaker_id": "2",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100028253.1.8",
          "form": "부산도 진짜 좋은데",
          "original_form": "부산도 진짜 좋은데",
          "speaker_id": "1",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100028253.1.9",
          "form": "먹을거 엄청 많잖아",
          "original_form": "먹을거 엄청 많잖아",
          "speaker_id": "1",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100028253.1.10",
          "form": "돼지국밥 먹고싶다 ><",
          "original_form": "돼지국밥 먹고싶다 ><",
          "speaker_id": "2",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100028253.1.11",
          "form": "나는 낙곱새도 먹고싶어",
          "original_form": "나는 낙곱새도 먹고싶어",
          "speaker_id": "1",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100028253.1.12",
          "form": "바다도 구경하고",
          "original_form": "바다도 구경하고",
          "speaker_id": "1",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100028253.1.13",
          "form": "우리 지난번에 갔을 때 완전 잘 먹고 잘 놀다 왔잖아",
          "original_form": "우리 지난번에 갔을 때 완전 잘 먹고 잘 놀다 왔잖아",
          "speaker_id": "2",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100028253.1.14",
          "form": "내 최애 음식도 찾고",
          "original_form": "내 최애 음식도 찾고",
          "speaker_id": "2",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100028253.1.15",
          "form": "우리가 수영했던 곳이",
          "original_form": "우리가 수영했던 곳이",
          "speaker_id": "1",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100028253.1.16",
          "form": "해운대였나 광안리였나?",
          "original_form": "해운대였나 광안리였나?",
          "speaker_id": "1",
          "time": "20210902 18:41"
        },
        {
          "id": "MDRW2100028253.1.17",
          "form": "해운대를 구경하고 광안리에서 수영한걸로 기억하는데",
          "original_form": "해운대를 구경하고 광안리에서 수영한걸로 기억하는데",
          "speaker_id": "2",
          "time": "20210902 18:42"
        },
        {
          "id": "MDRW2100028253.1.18",
          "form": "맞다",
          "original_form": "맞다",
          "speaker_id": "1",
          "time": "20210902 18:42"
        },
        {
          "id": "MDRW2100028253.1.19",
          "form": "광안리 야경도 좋았는데",
          "original_form": "광안리 야경도 좋았는데",
          "speaker_id": "1",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028253.1.20",
          "form": "거기서 먹은 물회도!",
          "original_form": "거기서 먹은 물회도!",
          "speaker_id": "2",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028253.1.21",
          "form": "먹은 기억만 많다",
          "original_form": "먹은 기억만 많다",
          "speaker_id": "2",
          "time": "20210902 18:43"
        },
        {
          "id": "MDRW2100028253.1.22",
          "form": "원래 여행은 먹는게 최고지 ㅋㅋㅋ",
          "original_form": "원래 여행은 먹는게 최고지 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:43"
        }
      ]
