      "utterance": [
        {
          "id": "MDRW2100011236.1.1",
          "form": "요새 뭐사노??",
          "original_form": "요새 뭐사노??",
          "speaker_id": "1",
          "time": "20210824 09:36"
        },
        {
          "id": "MDRW2100011236.1.2",
          "form": "오늘 쌀 괜찮더나??",
          "original_form": "오늘 쌀 괜찮더나??",
          "speaker_id": "1",
          "time": "20210824 09:36"
        },
        {
          "id": "MDRW2100011236.1.3",
          "form": "얼마전에 쌀통샀음 ㅎㅎ",
          "original_form": "얼마전에 쌀통샀음 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 09:37"
        },
        {
          "id": "MDRW2100011236.1.4",
          "form": "오늘 쌀샀고ㅋㅋ",
          "original_form": "오늘 쌀샀고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:37"
        },
        {
          "id": "MDRW2100011236.1.5",
          "form": "오오 ㅋㅋ잘샀다ㅋㅋㅋ",
          "original_form": "오오 ㅋㅋ잘샀다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:37"
        },
        {
          "id": "MDRW2100011236.1.6",
          "form": "요즘엔 근데 핫딜이 잘 안떠서",
          "original_form": "요즘엔 근데 핫딜이 잘 안떠서",
          "speaker_id": "2",
          "time": "20210824 09:37"
        },
        {
          "id": "MDRW2100011236.1.7",
          "form": "사재기를 자제하고있다ㅋㅋ",
          "original_form": "사재기를 자제하고있다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:37"
        },
        {
          "id": "MDRW2100011236.1.8",
          "form": "아 ㅋㅋㅋㅋㄱ 그래 아 뭐 살거있어서",
          "original_form": "아 ㅋㅋㅋㅋㄱ 그래 아 뭐 살거있어서",
          "speaker_id": "1",
          "time": "20210824 09:37"
        },
        {
          "id": "MDRW2100011236.1.9",
          "form": "사고싶은건 많음ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "사고싶은건 많음ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:38"
        },
        {
          "id": "MDRW2100011236.1.10",
          "form": "니한테 얘기할랬는데",
          "original_form": "니한테 얘기할랬는데",
          "speaker_id": "1",
          "time": "20210824 09:38"
        },
        {
          "id": "MDRW2100011236.1.11",
          "form": "기억이 안나노",
          "original_form": "기억이 안나노",
          "speaker_id": "1",
          "time": "20210824 09:38"
        },
        {
          "id": "MDRW2100011236.1.12",
          "form": "뭐 사려고?",
          "original_form": "뭐 사려고?",
          "speaker_id": "2",
          "time": "20210824 09:38"
        },
        {
          "id": "MDRW2100011236.1.13",
          "form": "기억이 안남 ㅋㅋㅋㄱ",
          "original_form": "기억이 안남 ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210824 09:38"
        },
        {
          "id": "MDRW2100011236.1.14",
          "form": "아아ㅋㅋㅋㅋ웃겨ㅋㅋ",
          "original_form": "아아ㅋㅋㅋㅋ웃겨ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:38"
        },
        {
          "id": "MDRW2100011236.1.15",
          "form": "요새 화장품이 다 떨어져서 사야되는데",
          "original_form": "요새 화장품이 다 떨어져서 사야되는데",
          "speaker_id": "1",
          "time": "20210824 09:38"
        },
        {
          "id": "MDRW2100011236.1.16",
          "form": "애들 오일사고 바디로션사고",
          "original_form": "애들 오일사고 바디로션사고",
          "speaker_id": "1",
          "time": "20210824 09:38"
        },
        {
          "id": "MDRW2100011236.1.17",
          "form": "응 ㅋㅋ 화장품 뭐쓰는데?",
          "original_form": "응 ㅋㅋ 화장품 뭐쓰는데?",
          "speaker_id": "2",
          "time": "20210824 09:38"
        },
        {
          "id": "MDRW2100011236.1.18",
          "form": "내 크림 사고",
          "original_form": "내 크림 사고",
          "speaker_id": "1",
          "time": "20210824 09:38"
        },
        {
          "id": "MDRW2100011236.1.19",
          "form": "헤어에센스 사고",
          "original_form": "헤어에센스 사고",
          "speaker_id": "1",
          "time": "20210824 09:39"
        },
        {
          "id": "MDRW2100011236.1.20",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210824 09:39"
        },
        {
          "id": "MDRW2100011236.1.21",
          "form": "편강율?? 거기",
          "original_form": "편강율?? 거기",
          "speaker_id": "1",
          "time": "20210824 09:39"
        },
        {
          "id": "MDRW2100011236.1.22",
          "form": "싸고 괜찮더라고",
          "original_form": "싸고 괜찮더라고",
          "speaker_id": "1",
          "time": "20210824 09:39"
        },
        {
          "id": "MDRW2100011236.1.23",
          "form": "살거많네 다 떨어졌나보당ㅋㅋ",
          "original_form": "살거많네 다 떨어졌나보당ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 09:39"
        },
        {
          "id": "MDRW2100011236.1.24",
          "form": "편강율?",
          "original_form": "편강율?",
          "speaker_id": "2",
          "time": "20210824 09:39"
        },
        {
          "id": "MDRW2100011236.1.25",
          "form": "엉 갑자기 후두둑 떨어짐",
          "original_form": "엉 갑자기 후두둑 떨어짐",
          "speaker_id": "1",
          "time": "20210824 09:39"
        },
        {
          "id": "MDRW2100011236.1.26",
          "form": "샴푸도 사고",
          "original_form": "샴푸도 사고",
          "speaker_id": "1",
          "time": "20210824 09:39"
        },
        {
          "id": "MDRW2100011236.1.27",
          "form": "그게뭐지? 검색해봐야겠구만",
          "original_form": "그게뭐지? 검색해봐야겠구만",
