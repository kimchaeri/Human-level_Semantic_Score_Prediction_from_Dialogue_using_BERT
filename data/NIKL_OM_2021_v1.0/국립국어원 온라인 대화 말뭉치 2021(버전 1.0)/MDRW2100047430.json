      "utterance": [
        {
          "id": "MDRW2100047430.1.1",
          "form": "혹시 좋아하는 맛집 있으신가용",
          "original_form": "혹시 좋아하는 맛집 있으신가용",
          "speaker_id": "1",
          "time": "20210826 15:33"
        },
        {
          "id": "MDRW2100047430.1.2",
          "form": "낙 곱 새 좋아해요 ㅎㅎ 맛집 어디 좋아하세요?",
          "original_form": "낙 곱 새 좋아해요 ㅎㅎ 맛집 어디 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 15:33"
        },
        {
          "id": "MDRW2100047430.1.3",
          "form": "헐 저도 좋아해요 ㅎㅎ 부산 사람이라 !",
          "original_form": "헐 저도 좋아해요 ㅎㅎ 부산 사람이라 !",
          "speaker_id": "1",
          "time": "20210826 15:34"
        },
        {
          "id": "MDRW2100047430.1.4",
          "form": "오 낙 곱 새의 도시ㅋㅋ저는 원래 몰랐는데 부산가서 먹구 반했어요",
          "original_form": "오 낙 곱 새의 도시ㅋㅋ저는 원래 몰랐는데 부산가서 먹구 반했어요",
          "speaker_id": "2",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100047430.1.5",
          "form": "저는 피자집도 좋아하구 ㅎㅎ 국밥도 좋아해요",
          "original_form": "저는 피자집도 좋아하구 ㅎㅎ 국밥도 좋아해요",
          "speaker_id": "1",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100047430.1.6",
          "form": "아 저두요ㅎㅎ 국밥먹고싶네요",
          "original_form": "아 저두요ㅎㅎ 국밥먹고싶네요",
          "speaker_id": "2",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100047430.1.7",
          "form": "낙 곱 새 엄청 맛있죠 ㅎㅎ 위쪽에도 체인 있더라구요",
          "original_form": "낙 곱 새 엄청 맛있죠 ㅎㅎ 위쪽에도 체인 있더라구요",
          "speaker_id": "1",
          "time": "20210826 15:36"
        },
        {
          "id": "MDRW2100047430.1.8",
          "form": "네 근데 여행가서 먹은 그맛이랑 좀 달라요ㅠㅠ",
          "original_form": "네 근데 여행가서 먹은 그맛이랑 좀 달라요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100047430.1.9",
          "form": "아 체인이라서 그 맛이 안나나보네요 ㅠㅠ",
          "original_form": "아 체인이라서 그 맛이 안나나보네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100047430.1.10",
          "form": "어디 가서 드셨었어요???",
          "original_form": "어디 가서 드셨었어요???",
          "speaker_id": "1",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100047430.1.11",
          "form": "저 자세히 기억안나는데 해운대근처였어요!",
          "original_form": "저 자세히 기억안나는데 해운대근처였어요!",
          "speaker_id": "2",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100047430.1.12",
          "form": "저도 안먹은지 오래되서 먹고싶네요 ㅎㅎㅅ",
          "original_form": "저도 안먹은지 오래되서 먹고싶네요 ㅎㅎㅅ",
          "speaker_id": "1",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100047430.1.13",
          "form": "아마 개미집에서 드셨나봐요 ㅎㅎ",
          "original_form": "아마 개미집에서 드셨나봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100047430.1.14",
          "form": "아 그런거같아요ㅋㅋ그런 이름이었어요ㅋㅋㅋ",
          "original_form": "아 그런거같아요ㅋㅋ그런 이름이었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100047430.1.15",
          "form": "ㅋㅋㅋㅋ 다 여행오면 거기 가더라구요 !",
          "original_form": "ㅋㅋㅋㅋ 다 여행오면 거기 가더라구요 !",
          "speaker_id": "1",
          "time": "20210826 15:43"
        },
        {
          "id": "MDRW2100047430.1.16",
          "form": "맛집이라구 꼭 가보라해서 갔는데 맛있어요ㅋㅋ",
          "original_form": "맛집이라구 꼭 가보라해서 갔는데 맛있어요ㅋㅋ",
          "speaker_id": "2",
