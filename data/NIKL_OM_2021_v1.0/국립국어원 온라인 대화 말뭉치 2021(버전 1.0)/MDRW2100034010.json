      "utterance": [
        {
          "id": "MDRW2100034010.1.1",
          "form": "게임은 해보신거 있으세요?",
          "original_form": "게임은 해보신거 있으세요?",
          "speaker_id": "1",
          "time": "20210825 22:33"
        },
        {
          "id": "MDRW2100034010.1.2",
          "form": "게임 음 이것저것 해봤어요",
          "original_form": "게임 음 이것저것 해봤어요",
          "speaker_id": "2",
          "time": "20210825 22:36"
        },
        {
          "id": "MDRW2100034010.1.3",
          "form": "어떤건지 여쭤봐도 되나요?",
          "original_form": "어떤건지 여쭤봐도 되나요?",
          "speaker_id": "1",
          "time": "20210825 22:37"
        },
        {
          "id": "MDRW2100034010.1.4",
          "form": "음... 막 진득하게 하는건 잘 없어가지고...",
          "original_form": "음... 막 진득하게 하는건 잘 없어가지고...",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100034010.1.5",
          "form": "스팀게임쪽으로 몇몇개랑...",
          "original_form": "스팀게임쪽으로 몇몇개랑...",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100034010.1.6",
          "form": "모바일은 쿠키런이랑 쿠키런킹덤도 하고 그랬어요",
          "original_form": "모바일은 쿠키런이랑 쿠키런킹덤도 하고 그랬어요",
          "speaker_id": "2",
          "time": "20210825 22:39"
        },
        {
          "id": "MDRW2100034010.1.7",
          "form": "오 쿠키런킹덤 저두 했는데",
          "original_form": "오 쿠키런킹덤 저두 했는데",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100034010.1.8",
          "form": "생산품을 계속 확인해야해서 ㅋㅋ 잠을 설쳐서...지웠습니다..",
          "original_form": "생산품을 계속 확인해야해서 ㅋㅋ 잠을 설쳐서...지웠습니다..",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100034010.1.9",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ저는",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ저는",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100034010.1.10",
          "form": "하루에...한 번...겨우...접속해요...",
          "original_form": "하루에...한 번...겨우...접속해요...",
          "speaker_id": "2",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100034010.1.11",
          "form": "아..절케했어야했는데..",
          "original_form": "아..절케했어야했는데..",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100034010.1.12",
          "form": "어리석은저는 중독이 심해서 적당히 하지못했네요 ..ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "어리석은저는 중독이 심해서 적당히 하지못했네요 ..ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100034010.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ다른 게임은 뭐하셔요??",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ다른 게임은 뭐하셔요??",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100034010.1.14",
          "form": "앗... 아 저는 ㅠㅠ 제가 게임을 좀 진득하게는 못하는 성격이라 ㅋㅋㅋㅋㅋ",
          "original_form": "앗... 아 저는 ㅠㅠ 제가 게임을 좀 진득하게는 못하는 성격이라 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100034010.1.15",
          "form": "로드오브히어로즈랑 포켓몬고 하는데 아시나요",
          "original_form": "로드오브히어로즈랑 포켓몬고 하는데 아시나요",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100034010.1.16",
          "form": "아 로오히 저도 했었어욬ㅋㅋㅋㅋㅋ",
          "original_form": "아 로오히 저도 했었어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100034010.1.17",
          "form": "포켓몬고는 진~~짜 초창기에 깔았었는데",
          "original_form": "포켓몬고는 진~~짜 초창기에 깔았었는데",
          "speaker_id": "2",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100034010.1.18",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210825 22:45"
        },
        {
          "id": "MDRW2100034010.1.19",
          "form": "집근처에 아무것도 없어서",
          "original_form": "집근처에 아무것도 없어서",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100034010.1.20",
          "form": "정말 한 3일만에 지웠어요",
          "original_form": "정말 한 3일만에 지웠어요",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100034010.1.21",
          "form": "ㅜㅜㅜㅜ그게 지역차가 크더라구요",
          "original_form": "ㅜㅜㅜㅜ그게 지역차가 크더라구요",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100034010.1.22",
          "form": "ㅎㅎ...",
          "original_form": "ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100034010.1.23",
          "form": "3일간은 멀리...일부러 산책도 가서 해봤는데",
          "original_form": "3일간은 멀리...일부러 산책도 가서 해봤는데",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100034010.1.24",
          "form": "그렇게까지 부지런하긴 힘들어서...",
          "original_form": "그렇게까지 부지런하긴 힘들어서...",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100034010.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100034010.1.26",
          "form": "슬퍼요..시골이라도 유적지나 절이 교회있으면",
          "original_form": "슬퍼요..시골이라도 유적지나 절이 교회있으면",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100034010.1.27",
          "form": "포켓스탑있는뎅",
          "original_form": "포켓스탑있는뎅",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100034010.1.28",
          "form": "집 옆에...유적지가 있진 않으니까욬ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "집 옆에...유적지가 있진 않으니까욬ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100034010.1.29",
          "form": "또르르...",
          "original_form": "또르르...",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100034010.1.30",
          "form": "제 친구들도 걷기싫어해서",
          "original_form": "제 친구들도 걷기싫어해서",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100034010.1.31",
          "form": "같이 안해주더라구요 또르르...",
          "original_form": "같이 안해주더라구요 또르르...",
          "speaker_id": "1",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100034010.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ제 친구 중에서는 폰 한 3개씩 들고가서",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ제 친구 중에서는 폰 한 3개씩 들고가서",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100034010.1.33",
          "form": "하는 친구도 있긴해욬ㅋㅋㅋㅋ",
          "original_form": "하는 친구도 있긴해욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100034010.1.34",
          "form": "폰 3개가 왜필요한진 모르겠지만...",
          "original_form": "폰 3개가 왜필요한진 모르겠지만...",
          "speaker_id": "2",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100034010.1.35",
          "form": "저는 두갠데",
          "original_form": "저는 두갠데",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100034010.1.36",
          "form": "친구가 대단하시네요 ㅋㅋ",
          "original_form": "친구가 대단하시네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100034010.1.37",
          "form": "두개도 대단해요...",
          "original_form": "두개도 대단해요...",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100034010.1.38",
          "form": "지금 옆에 겜돌리고있어요",
          "original_form": "지금 옆에 겜돌리고있어요",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100034010.1.39",
          "form": "워치랑은 연동 안되나요??",
          "original_form": "워치랑은 연동 안되나요??",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100034010.1.40",
          "form": "워치는 안써요",
          "original_form": "워치는 안써요",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100034010.1.41",
          "form": "이쯤되면 워치랑 연동되게 만들어줘야하는데",
          "original_form": "이쯤되면 워치랑 연동되게 만들어줘야하는데",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100034010.1.42",
          "form": "아 안쓰시는구나",
          "original_form": "아 안쓰시는구나",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100034010.1.43",
          "form": "아 연동 되는거 같긴한데",
          "original_form": "아 연동 되는거 같긴한데",
          "speaker_id": "1",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100034010.1.44",
          "form": "안써봤어요ㅋㅋㅋㅋ",
          "original_form": "안써봤어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100034010.1.45",
          "form": "연동된대요?? 오  ...",
          "original_form": "연동된대요?? 오  ...",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100034010.1.46",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100034010.1.47",
          "form": "외국인들이 이겜을 많이해서 같이 댕기곤 했는데",
          "original_form": "외국인들이 이겜을 많이해서 같이 댕기곤 했는데",
          "speaker_id": "1",
          "time": "20210825 22:50"
