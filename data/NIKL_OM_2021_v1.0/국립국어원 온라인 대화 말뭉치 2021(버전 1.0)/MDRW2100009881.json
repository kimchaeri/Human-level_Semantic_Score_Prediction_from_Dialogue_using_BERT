      "utterance": [
        {
          "id": "MDRW2100009881.1.1",
          "form": "마우스를 좀 바꿔야 할 것 같아",
          "original_form": "마우스를 좀 바꿔야 할 것 같아",
          "speaker_id": "2",
          "time": "20210823 11:29"
        },
        {
          "id": "MDRW2100009881.1.2",
          "form": "마우스는 왜?",
          "original_form": "마우스는 왜?",
          "speaker_id": "1",
          "time": "20210823 11:29"
        },
        {
          "id": "MDRW2100009881.1.3",
          "form": "클릭이 두번씩 돼서 엄청 신경쓰이네",
          "original_form": "클릭이 두번씩 돼서 엄청 신경쓰이네",
          "speaker_id": "2",
          "time": "20210823 11:29"
        },
        {
          "id": "MDRW2100009881.1.4",
          "form": "한번만 누르는데도 두번씩 돼?",
          "original_form": "한번만 누르는데도 두번씩 돼?",
          "speaker_id": "1",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100009881.1.5",
          "form": "ㅇㅇ 한 번 누르는데 그래 ㅠㅠ 너무 불편해",
          "original_form": "ㅇㅇ 한 번 누르는데 그래 ㅠㅠ 너무 불편해",
          "speaker_id": "2",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100009881.1.6",
          "form": "그럼 사야지 ㅎㅎ 신랑 편한 걸로 알아봐",
          "original_form": "그럼 사야지 ㅎㅎ 신랑 편한 걸로 알아봐",
          "speaker_id": "1",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100009881.1.7",
          "form": "나 예전에 쓰던 마우스 있는데 그걸로 알아봐야겠다",
          "original_form": "나 예전에 쓰던 마우스 있는데 그걸로 알아봐야겠다",
          "speaker_id": "2",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100009881.1.8",
          "form": "ㅇㅇ 그랴요",
          "original_form": "ㅇㅇ 그랴요",
          "speaker_id": "1",
          "time": "20210823 11:30"
        },
        {
          "id": "MDRW2100009881.1.9",
          "form": "요즘도 10만원 넘는 거라 리퍼 제품으로 사면 괜찮을겨",
          "original_form": "요즘도 10만원 넘는 거라 리퍼 제품으로 사면 괜찮을겨",
          "speaker_id": "2",
          "time": "20210823 11:31"
        },
        {
          "id": "MDRW2100009881.1.10",
          "form": "기왕이면 새 거로 사지",
          "original_form": "기왕이면 새 거로 사지",
          "speaker_id": "1",
          "time": "20210823 11:31"
        },
        {
          "id": "MDRW2100009881.1.11",
          "form": "아냐 리퍼는 3만원인데 작동만 잘 되면 상관없어 ㅎㅎ",
          "original_form": "아냐 리퍼는 3만원인데 작동만 잘 되면 상관없어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 11:32"
        },
        {
          "id": "MDRW2100009881.1.12",
          "form": "에궁 맨날 중고 사서 써 ㅠ 오래 쓸거면 새거 사요 ㅎㅎ",
          "original_form": "에궁 맨날 중고 사서 써 ㅠ 오래 쓸거면 새거 사요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 11:32"
        },
        {
          "id": "MDRW2100009881.1.13",
          "form": "상품평 보니 겉에만 좀 중고같고 괜찮은 것 같아",
          "original_form": "상품평 보니 겉에만 좀 중고같고 괜찮은 것 같아",
          "speaker_id": "2",
          "time": "20210823 11:32"
        },
        {
          "id": "MDRW2100009881.1.14",
          "form": "괜찮을라나..",
          "original_form": "괜찮을라나..",
          "speaker_id": "1",
          "time": "20210823 11:32"
        },
        {
          "id": "MDRW2100009881.1.15",
          "form": "새것도 사자마자 중고 되는건데 뭘 ㅎㅎ",
          "original_form": "새것도 사자마자 중고 되는건데 뭘 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 11:33"
        },
        {
          "id": "MDRW2100009881.1.16",
          "form": "생수 때문에 분리수거가 너무 많이 나오던데 정수기 놓는 게 낫지 않을까",
          "original_form": "생수 때문에 분리수거가 너무 많이 나오던데 정수기 놓는 게 낫지 않을까",
          "speaker_id": "2",
          "time": "20210823 14:46"
        }
      ]
