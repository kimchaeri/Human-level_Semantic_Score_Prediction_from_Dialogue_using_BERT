      "utterance": [
        {
          "id": "MDRW2100022224.1.1",
          "form": "점심 뭐 드실거에요?",
          "original_form": "점심 뭐 드실거에요?",
          "speaker_id": "2",
          "time": "20210829 11:17"
        },
        {
          "id": "MDRW2100022224.1.2",
          "form": "전 어제 빵 사놓은게있어서",
          "original_form": "전 어제 빵 사놓은게있어서",
          "speaker_id": "1",
          "time": "20210829 11:17"
        },
        {
          "id": "MDRW2100022224.1.3",
          "form": "그거먹으려구요",
          "original_form": "그거먹으려구요",
          "speaker_id": "1",
          "time": "20210829 11:17"
        },
        {
          "id": "MDRW2100022224.1.4",
          "form": "name2님은 정하셨나요",
          "original_form": "&name2&님은 정하셨나요",
          "speaker_id": "1",
          "time": "20210829 11:17"
        },
        {
          "id": "MDRW2100022224.1.5",
          "form": "ㅠㅠ 저 참치뽈찜 먹으려구요!",
          "original_form": "ㅠㅠ 저 참치뽈찜 먹으려구요!",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100022224.1.6",
          "form": "먹어봤어요 이거?",
          "original_form": "먹어봤어요 이거?",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100022224.1.7",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100022224.1.8",
          "form": "뽈찜이 뭐에요",
          "original_form": "뽈찜이 뭐에요",
          "speaker_id": "1",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100022224.1.9",
          "form": "생선머리찜인가요",
          "original_form": "생선머리찜인가요",
          "speaker_id": "1",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100022224.1.10",
          "form": "어멐ㅋㅋㅋ 생선머리는 아니구요",
          "original_form": "어멐ㅋㅋㅋ 생선머리는 아니구요",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022224.1.11",
          "form": "참치 부위 중에서 특수부위라고 하더라구요?",
          "original_form": "참치 부위 중에서 특수부위라고 하더라구요?",
          "speaker_id": "2",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022224.1.12",
          "form": "아.. 처음들어봐요",
          "original_form": "아.. 처음들어봐요",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022224.1.13",
          "form": "뽈이라는 부위가 있구나",
          "original_form": "뽈이라는 부위가 있구나",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022224.1.14",
          "form": "이전에 드셔보신적은 있으세요?",
          "original_form": "이전에 드셔보신적은 있으세요?",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022224.1.15",
          "form": "저 이거 처음 먹어봐요 ㅜㅜ",
          "original_form": "저 이거 처음 먹어봐요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 11:29"
        },
        {
          "id": "MDRW2100022224.1.16",
          "form": "그래서 좀 기대되는데 기대반 걱정반이기도 해요 ㅋㅋ",
          "original_form": "그래서 좀 기대되는데 기대반 걱정반이기도 해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022224.1.17",
          "form": "헉 그럼 더 기대되시겠어요",
          "original_form": "헉 그럼 더 기대되시겠어요",
          "speaker_id": "1",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022224.1.18",
          "form": "어딴점이 걱정되시나요",
          "original_form": "어딴점이 걱정되시나요",
          "speaker_id": "1",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022224.1.19",
          "form": "네네 근데 맛이름이 독특하더라구요",
          "original_form": "네네 근데 맛이름이 독특하더라구요",
          "speaker_id": "2",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022224.1.20",
          "form": "들어볼래요?",
          "original_form": "들어볼래요?",
          "speaker_id": "2",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022224.1.21",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022224.1.22",
          "form": "수원왕갈비맛이래요 ㅋㅋㅋㅋ",
          "original_form": "수원왕갈비맛이래요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022224.1.23",
          "form": "왕갈비요?",
          "original_form": "왕갈비요?",
          "speaker_id": "1",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022224.1.24",
          "form": "생선에서요?",
          "original_form": "생선에서요?",
          "speaker_id": "1",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022224.1.25",
          "form": "네 ㅋㅋㅋ 상상되나요 이게..?",
          "original_form": "네 ㅋㅋㅋ 상상되나요 이게..?",
          "speaker_id": "2",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022224.1.26",
          "form": "네 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022224.1.27",
          "form": "아니요ㅋㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022224.1.28",
          "form": "그냥 간장맛 아닐까싶어요",
          "original_form": "그냥 간장맛 아닐까싶어요",
          "speaker_id": "2",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022224.1.29",
          "form": "ㄴㅇㄱ 예상치 못한",
          "original_form": "ㄴㅇㄱ 예상치 못한",
          "speaker_id": "1",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022224.1.30",
          "form": "다른 것도 맛 특이하더라구요",
          "original_form": "다른 것도 맛 특이하더라구요",
          "speaker_id": "2",
          "time": "20210829 11:30"
        },
        {
          "id": "MDRW2100022224.1.31",
          "form": "회기동닭볶음탕맛 ㅋㅋㅋㅋ",
          "original_form": "회기동닭볶음탕맛 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:31"
        },
        {
          "id": "MDRW2100022224.1.32",
          "form": "너무 신기해요",
          "original_form": "너무 신기해요",
          "speaker_id": "1",
          "time": "20210829 11:31"
        },
        {
          "id": "MDRW2100022224.1.33",
          "form": "닭볶음탕요,",
          "original_form": "닭볶음탕요,",
          "speaker_id": "1",
          "time": "20210829 11:31"
        },
        {
          "id": "MDRW2100022224.1.34",
          "form": "이건 상상이 되긴해요.. 빨간양념맛이겠죠 뭐",
          "original_form": "이건 상상이 되긴해요.. 빨간양념맛이겠죠 뭐",
          "speaker_id": "2",
          "time": "20210829 11:31"
        },
        {
          "id": "MDRW2100022224.1.35",
          "form": "진짜 예상이 안가네요",
          "original_form": "진짜 예상이 안가네요",
          "speaker_id": "1",
          "time": "20210829 11:31"
        }
      ]
