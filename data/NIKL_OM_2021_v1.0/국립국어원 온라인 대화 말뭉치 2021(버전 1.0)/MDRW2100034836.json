      "utterance": [
        {
          "id": "MDRW2100034836.1.1",
          "form": "네네! 장르물 좋아해요",
          "original_form": "네네! 장르물 좋아해요",
          "speaker_id": "2",
          "time": "20210828 20:35"
        },
        {
          "id": "MDRW2100034836.1.2",
          "form": "주로 시간이 소재인 드라마는 다 잘보는것같아용",
          "original_form": "주로 시간이 소재인 드라마는 다 잘보는것같아용",
          "speaker_id": "2",
          "time": "20210828 20:35"
        },
        {
          "id": "MDRW2100034836.1.3",
          "form": "시그널같은ㅋㅋㅋ",
          "original_form": "시그널같은ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:35"
        },
        {
          "id": "MDRW2100034836.1.4",
          "form": "아 시그널 제 인생작이에요ㅋㅋㅋ",
          "original_form": "아 시그널 제 인생작이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 20:36"
        },
        {
          "id": "MDRW2100034836.1.5",
          "form": "너무 재밌게봤어요 ㅜㅜ",
          "original_form": "너무 재밌게봤어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 20:37"
        },
        {
          "id": "MDRW2100034836.1.6",
          "form": "저랑 취향이 겹치셔서 넘 즐거워요ㅋㅋㅋ",
          "original_form": "저랑 취향이 겹치셔서 넘 즐거워요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:39"
        },
        {
          "id": "MDRW2100034836.1.7",
          "form": "시그널 2 언제하는걸까요 대체ㅠㅠ",
          "original_form": "시그널 2 언제하는걸까요 대체ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 20:39"
        },
        {
          "id": "MDRW2100034836.1.8",
          "form": "맞죠 ㅠㅠㅠ 제작은 한다던데ㅣ",
          "original_form": "맞죠 ㅠㅠㅠ 제작은 한다던데ㅣ",
          "speaker_id": "1",
          "time": "20210828 20:40"
        },
        {
          "id": "MDRW2100034836.1.9",
          "form": "방영 그 날짜?를 못정했나보더라구요",
          "original_form": "방영 그 날짜?를 못정했나보더라구요",
          "speaker_id": "1",
          "time": "20210828 20:40"
        },
        {
          "id": "MDRW2100034836.1.10",
          "form": "헐헐 다행이네요 ㅋㅋㅋㅋ",
          "original_form": "헐헐 다행이네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:40"
        },
        {
          "id": "MDRW2100034836.1.11",
          "form": "라인업 그대로였으면ㅠㅠ",
          "original_form": "라인업 그대로였으면ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 20:41"
        },
        {
          "id": "MDRW2100034836.1.12",
          "form": "맞아요 저는",
          "original_form": "맞아요 저는",
          "speaker_id": "1",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100034836.1.13",
          "form": "시즌 바뀌면 라인업 바뀌는걸",
          "original_form": "시즌 바뀌면 라인업 바뀌는걸",
          "speaker_id": "1",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100034836.1.14",
          "form": "안좋아하거든요 ㅜㅜ",
          "original_form": "안좋아하거든요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100034836.1.15",
          "form": "몰입이 안된달까나 ...",
          "original_form": "몰입이 안된달까나 ...",
          "speaker_id": "1",
          "time": "20210828 20:56"
        },
        {
          "id": "MDRW2100034836.1.16",
          "form": "특히 같은 캐릭터인데 배우만 바뀌면 제일 거부감 들더라구요ㅋㅋㅋㅋ",
          "original_form": "특히 같은 캐릭터인데 배우만 바뀌면 제일 거부감 들더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:16"
        },
        {
          "id": "MDRW2100034836.1.17",
          "form": "맞아욬ㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:22"
        },
        {
          "id": "MDRW2100034836.1.18",
          "form": "조진웅 배우님이 연기한 캐릭터는 살아있겠죠?",
          "original_form": "조진웅 배우님이 연기한 캐릭터는 살아있겠죠?",
          "speaker_id": "2",
          "time": "20210828 21:23"
        },
        {
          "id": "MDRW2100034836.1.19",
          "form": "백퍼 살아있을거같아요 !!",
          "original_form": "백퍼 살아있을거같아요 !!",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100034836.1.20",
          "form": "시즌 2까지 만들려면 ㅋㅋ 살아있어야해요 ㅜㅜ",
          "original_form": "시즌 2까지 만들려면 ㅋㅋ 살아있어야해요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100034836.1.21",
          "form": "마지막 장면에 암시되긴 한것같은데",
          "original_form": "마지막 장면에 암시되긴 한것같은데",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100034836.1.22",
          "form": "은퇴한다거나 그러지 안ㅅ았으면ㅠㅠ",
          "original_form": "은퇴한다거나 그러지 안ㅅ았으면ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100034836.1.23",
          "form": "맞아요 ㅠㅠㅠ 뭔가 스케줄이 안맞으면",
          "original_form": "맞아요 ㅠㅠㅠ 뭔가 스케줄이 안맞으면",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100034836.1.24",
          "form": "그대로 안나올듯한...느낌이죠?",
          "original_form": "그대로 안나올듯한...느낌이죠?",
          "speaker_id": "1",
          "time": "20210828 21:29"
        },
        {
          "id": "MDRW2100034836.1.25",
          "form": "네네... 뭐 극중 사정이 생겨서 캐릭터를 하차시킨다던지",
          "original_form": "네네... 뭐 극중 사정이 생겨서 캐릭터를 하차시킨다던지",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100034836.1.26",
          "form": "그럴것같기도해요ㅠ",
          "original_form": "그럴것같기도해요ㅠ",
          "speaker_id": "2",
          "time": "20210828 21:30"
        }
      ]
    }
