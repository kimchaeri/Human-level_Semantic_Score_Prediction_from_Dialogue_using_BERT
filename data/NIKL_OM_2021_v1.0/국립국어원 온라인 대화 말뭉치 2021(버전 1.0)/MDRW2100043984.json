        {
          "id": "MDRW2100043984.1.1",
          "form": "야식좋아하세요",
          "original_form": "야식좋아하세요",
          "speaker_id": "2",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043984.1.2",
          "form": "네 좋아합니다@@",
          "original_form": "네 좋아합니다@@",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043984.1.3",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 21:24"
        },
        {
          "id": "MDRW2100043984.1.4",
          "form": "ㅎㅎ넹~~",
          "original_form": "ㅎㅎ넹~~",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100043984.1.5",
          "form": "주로 어떤거 드세요?",
          "original_form": "주로 어떤거 드세요?",
          "speaker_id": "1",
          "time": "20210907 21:29"
        },
        {
          "id": "MDRW2100043984.1.6",
          "form": "방금과자먹었어요ㅜ",
          "original_form": "방금과자먹었어요ㅜ",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100043984.1.7",
          "form": "오 어떤 과자 드셨나요?",
          "original_form": "오 어떤 과자 드셨나요?",
          "speaker_id": "1",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100043984.1.8",
          "form": "오레오요",
          "original_form": "오레오요{emoji:❤}",
          "speaker_id": "2",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100043984.1.9",
          "form": "헉 오레오 맛있죠ㅠㅠ",
          "original_form": "헉 오레오 맛있죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100043984.1.10",
          "form": "기본 드셨나요?",
          "original_form": "기본 드셨나요?",
          "speaker_id": "1",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100043984.1.11",
          "form": "ㅎㅎ넵",
          "original_form": "ㅎㅎ넵",
          "speaker_id": "2",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100043984.1.12",
          "form": "오레오 가루도 별로 안떨어지고 깔끔하게 먹기 좋은거 같아요ㅎㅎ",
          "original_form": "오레오 가루도 별로 안떨어지고 깔끔하게 먹기 좋은거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100043984.1.13",
          "form": "꼬북칩 드셔보셨나요?",
          "original_form": "꼬북칩 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100043984.1.14",
          "form": "콘스프맛좋아해요",
          "original_form": "콘스프맛좋아해요",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100043984.1.15",
          "form": "오 초코맛 드셔보셨어요?",
          "original_form": "오 초코맛 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100043984.1.16",
          "form": "앗 아뇨",
          "original_form": "앗 아뇨",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100043984.1.17",
          "form": "신상인가요",
          "original_form": "신상인가요",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100043984.1.18",
          "form": "나온지 좀 되긴 했어요!",
          "original_form": "나온지 좀 되긴 했어요!",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100043984.1.19",
          "form": "초코츄러스맛!!",
          "original_form": "초코츄러스맛!!",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100043984.1.20",
          "form": "살짝 얼려서 먹으면 엄청 맛있더라구요",
          "original_form": "살짝 얼려서 먹으면 엄청 맛있더라구요",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100043984.1.21",
          "form": "츄러스 좋아하시면 드셔보세요ㅎㅎ",
          "original_form": "츄러스 좋아하시면 드셔보세요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
