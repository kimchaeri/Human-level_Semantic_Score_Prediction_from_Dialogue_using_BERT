      "utterance": [
        {
          "id": "MDRW2100004480.1.1",
          "form": "게임게임",
          "original_form": "게임게임",
          "speaker_id": "1",
          "time": "20210821 16:45"
        },
        {
          "id": "MDRW2100004480.1.2",
          "form": "게임.... 역시 게임은 가챠 요소가 있는게 재밌죠",
          "original_form": "게임.... 역시 게임은 가챠 요소가 있는게 재밌죠",
          "speaker_id": "2",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004480.1.3",
          "form": "운을 시험해보는거지... 오늘의 운세 같은 느낌으로",
          "original_form": "운을 시험해보는거지... 오늘의 운세 같은 느낌으로",
          "speaker_id": "2",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004480.1.4",
          "form": "그게 도파민이 많이 나온대잖아",
          "original_form": "그게 도파민이 많이 나온대잖아",
          "speaker_id": "1",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004480.1.5",
          "form": "재밌을수밖에없어 뽑기겜..",
          "original_form": "재밌을수밖에없어 뽑기겜..",
          "speaker_id": "1",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004480.1.6",
          "form": "맞아ㅋㅋ 오기로라도 뽑고 싶은 마음만 잘 다스리면 최고의 재미라구",
          "original_form": "맞아ㅋㅋ 오기로라도 뽑고 싶은 마음만 잘 다스리면 최고의 재미라구",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004480.1.7",
          "form": "근데 조절이 안된다..",
          "original_form": "근데 조절이 안된다..",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004480.1.8",
          "form": "일단 재화를 모으는거부터 인내심의 시작이라구",
          "original_form": "일단 재화를 모으는거부터 인내심의 시작이라구",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004480.1.9",
          "form": "도닦는 기분으로... 한개씩..",
          "original_form": "도닦는 기분으로... 한개씩..",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004480.1.10",
          "form": "에잇 내가 도닦으려고 게임하는줄 알어!!!",
          "original_form": "에잇 내가 도닦으려고 게임하는줄 알어!!!",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004480.1.11",
          "form": "블럭쌓기처럼 다 만들어놓고... 무너트리는 심리지 뭐",
          "original_form": "블럭쌓기처럼 다 만들어놓고... 무너트리는 심리지 뭐",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004480.1.12",
          "form": "지금 하나하나 모으고 있는데 정말... 애간장이 탄다",
          "original_form": "지금 하나하나 모으고 있는데 정말... 애간장이 탄다",
          "speaker_id": "2",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004480.1.13",
          "form": "뭐 노리는거 있어?",
          "original_form": "뭐 노리는거 있어?",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004480.1.14",
          "form": "포케마스 2주년 N가챠...",
          "original_form": "포케마스 2주년 N가챠...",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004480.1.15",
          "form": "저번 생방 공개 때 보고 너무너무인거야",
          "original_form": "저번 생방 공개 때 보고 너무너무인거야",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004480.1.16",
          "form": "그래서 다시 깔았지",
          "original_form": "그래서 다시 깔았지",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004480.1.17",
          "form": "내 마음 속 왕이야....",
          "original_form": "내 마음 속 왕이야....",
          "speaker_id": "2",
          "time": "20210821 17:02"
        },
        {
          "id": "MDRW2100004480.1.18",
          "form": "캐릭터 콘텐츠의 선두주자 포켓몬 인정합니다..",
          "original_form": "캐릭터 콘텐츠의 선두주자 포켓몬 인정합니다..",
          "speaker_id": "2",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004480.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ너무 진정성넘쳐..",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ너무 진정성넘쳐..",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004480.1.20",
          "form": "심지어 옷도 멋있음",
          "original_form": "심지어 옷도 멋있음",
          "speaker_id": "1",
          "time": "20210821 17:03"
        },
        {
          "id": "MDRW2100004480.1.21",
          "form": "그렇지!!",
          "original_form": "그렇지!!",
          "speaker_id": "2",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004480.1.22",
          "form": "꼭 뽑길 바랄게 ㅎㅇㅌ",
          "original_form": "꼭 뽑길 바랄게 ㅎㅇㅌ",
          "speaker_id": "1",
          "time": "20210821 17:04"
        },
        {
          "id": "MDRW2100004480.1.23",
          "form": "이상과 현실의 흑과 백을 모두 가진 남자를 우리집에 넣어놔야만",
          "original_form": "이상과 현실의 흑과 백을 모두 가진 남자를 우리집에 넣어놔야만",
          "speaker_id": "2",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004480.1.24",
          "form": "나도 할까했는데 내폰에 안깔림ㅋㅋ",
          "original_form": "나도 할까했는데 내폰에 안깔림ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004480.1.25",
          "form": "ㅋㅋㅋㅋ디테일한 앓이..",
          "original_form": "ㅋㅋㅋㅋ디테일한 앓이..",
          "speaker_id": "1",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004480.1.26",
          "form": "엥 그렇게 앱이 무겁진 않을텐데!",
          "original_form": "엥 그렇게 앱이 무겁진 않을텐데!",
          "speaker_id": "2",
          "time": "20210821 17:05"
        },
        {
          "id": "MDRW2100004480.1.27",
          "form": "기기가 호환 안된다던데?",
          "original_form": "기기가 호환 안된다던데?",
          "speaker_id": "1",
