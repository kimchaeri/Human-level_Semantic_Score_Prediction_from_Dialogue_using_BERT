      "utterance": [
        {
          "id": "MDRW2100004493.1.1",
          "form": "제 주변에 군대 간 사람이 없어서 딱히 아는 게  없네요ㅎㅎ",
          "original_form": "제 주변에 군대 간 사람이 없어서 딱히 아는 게  없네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100004493.1.2",
          "form": "저도요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100004493.1.3",
          "form": "요즘 군대가서도 연애 잘한다는것만..",
          "original_form": "요즘 군대가서도 연애 잘한다는것만..",
          "speaker_id": "1",
          "time": "20210824 20:41"
        },
        {
          "id": "MDRW2100004493.1.4",
          "form": "아 요즘에는 저녁부터 핸드폰 사용이 가능하대요",
          "original_form": "아 요즘에는 저녁부터 핸드폰 사용이 가능하대요",
          "speaker_id": "2",
          "time": "20210824 20:42"
        },
        {
          "id": "MDRW2100004493.1.5",
          "form": "6시부턴가",
          "original_form": "6시부턴가",
          "speaker_id": "2",
          "time": "20210824 20:42"
        },
        {
          "id": "MDRW2100004493.1.6",
          "form": "아 맞아 그럴거예요",
          "original_form": "아 맞아 그럴거예요",
          "speaker_id": "1",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100004493.1.7",
          "form": "친구들중에 군대간애랑 다시 연락해서",
          "original_form": "친구들중에 군대간애랑 다시 연락해서",
          "speaker_id": "1",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100004493.1.8",
          "form": "사귀는경우도 좀 봐서",
          "original_form": "사귀는경우도 좀 봐서",
          "speaker_id": "1",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100004493.1.9",
          "form": "신기하더라고요 ㅋㅋㅋㅌㅋ",
          "original_form": "신기하더라고요 ㅋㅋㅋㅌㅋ",
          "speaker_id": "1",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100004493.1.10",
          "form": "군대 가서 사귀는 건 처도 처음 들어본 거 같아요ㅋㅋㅋㅋ",
          "original_form": "군대 가서 사귀는 건 처도 처음 들어본 거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100004493.1.11",
          "form": "남동생이나 오빠 있으세요?",
          "original_form": "남동생이나 오빠 있으세요?",
          "speaker_id": "2",
          "time": "20210824 20:52"
        },
        {
          "id": "MDRW2100004493.1.12",
          "form": "남동생은 있어요 !",
          "original_form": "남동생은 있어요 !",
          "speaker_id": "1",
          "time": "20210824 20:54"
        },
        {
          "id": "MDRW2100004493.1.13",
          "form": "아 저도 5살 차이나는 남동생만 있어요ㅋㅋㅋ",
          "original_form": "아 저도 5살 차이나는 남동생만 있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 20:56"
        },
        {
          "id": "MDRW2100004493.1.14",
          "form": "군대는 동생이 가면 좀 알게 되지 않을까 싶어요",
          "original_form": "군대는 동생이 가면 좀 알게 되지 않을까 싶어요",
          "speaker_id": "2",
          "time": "20210824 20:58"
        },
        {
          "id": "MDRW2100004493.1.15",
          "form": "아직 한참이네요 ㅋㅋㅋㅋㅋㄱ",
          "original_form": "아직 한참이네요 ㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100004493.1.16",
          "form": "근데 제 동생은 늦둥이라 10년은 더 남아서 ㅋㅋㅋㅋㅋ",
          "original_form": "근데 제 동생은 늦둥이라 10년은 더 남아서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 22:49"
        },
        {
          "id": "MDRW2100004493.1.17",
          "form": "군대.. 그때는 어떨런지",
          "original_form": "군대.. 그때는 어떨런지",
          "speaker_id": "1",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100004493.1.18",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100004493.1.19",
          "form": "그래도 안 좋은 군대 문화는 거의 없어진 거 같아서 다행이에요",
          "original_form": "그래도 안 좋은 군대 문화는 거의 없어진 거 같아서 다행이에요",
          "speaker_id": "2",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100004493.1.20",
          "form": "기간도 짧아졌고",
          "original_form": "기간도 짧아졌고",
          "speaker_id": "2",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100004493.1.21",
          "form": "맞아요 ..",
          "original_form": "맞아요 ..",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100004493.1.22",
          "form": "저는 사실 좀 가고싶어요",
          "original_form": "저는 사실 좀 가고싶어요",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100004493.1.23",
          "form": "취업하려니까 이력서에 뭐라도 한줄 적는게 부럽더라고요..",
          "original_form": "취업하려니까 이력서에 뭐라도 한줄 적는게 부럽더라고요..",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100004493.1.24",
          "form": "저도 이번에 여자도 군대 가라고 할 때 가라면 갈 수 있을 거 같아요",
          "original_form": "저도 이번에 여자도 군대 가라고 할 때 가라면 갈 수 있을 거 같아요",
          "speaker_id": "2",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100004493.1.25",
          "form": "국가에서 허락한 자기계발시간..",
          "original_form": "국가에서 허락한 자기계발시간..",
          "speaker_id": "1",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100004493.1.26",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 22:54"
        },
        {
          "id": "MDRW2100004493.1.27",
          "form": "가서 공부하는 사람도 많고 학점인증도 된다 그러고",
          "original_form": "가서 공부하는 사람도 많고 학점인증도 된다 그러고",
          "speaker_id": "2",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100004493.1.28",
          "form": "가산점도 꽤 받지 않나요",
          "original_form": "가산점도 꽤 받지 않나요",
          "speaker_id": "2",
          "time": "20210824 22:55"
        },
        {
          "id": "MDRW2100004493.1.29",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210824 22:57"
        },
        {
          "id": "MDRW2100004493.1.30",
