      "utterance": [
        {
          "id": "MDRW2100024750.1.1",
          "form": "오 점심특선이 얼마였어?",
          "original_form": "오 점심특선이 얼마였어?",
          "speaker_id": "2",
          "time": "20210831 18:08"
        },
        {
          "id": "MDRW2100024750.1.2",
          "form": "14000원",
          "original_form": "14000원",
          "speaker_id": "1",
          "time": "20210831 18:09"
        },
        {
          "id": "MDRW2100024750.1.3",
          "form": "샤브샤브랑 뷔페이용으로",
          "original_form": "샤브샤브랑 뷔페이용으로",
          "speaker_id": "1",
          "time": "20210831 18:09"
        },
        {
          "id": "MDRW2100024750.1.4",
          "form": "기분 내기에 괜찮더라",
          "original_form": "기분 내기에 괜찮더라",
          "speaker_id": "1",
          "time": "20210831 18:09"
        },
        {
          "id": "MDRW2100024750.1.5",
          "form": "이야 양껏 먹었겠네",
          "original_form": "이야 양껏 먹었겠네",
          "speaker_id": "2",
          "time": "20210831 18:09"
        },
        {
          "id": "MDRW2100024750.1.6",
          "form": "뷔페에는 뭐뭐 있었어?",
          "original_form": "뷔페에는 뭐뭐 있었어?",
          "speaker_id": "2",
          "time": "20210831 18:09"
        },
        {
          "id": "MDRW2100024750.1.7",
          "form": "뷔페는 사실 샐러드만 먹고",
          "original_form": "뷔페는 사실 샐러드만 먹고",
          "speaker_id": "1",
          "time": "20210831 18:09"
        },
        {
          "id": "MDRW2100024750.1.8",
          "form": "일부러 샤브샤브 많이먹었어",
          "original_form": "일부러 샤브샤브 많이먹었어",
          "speaker_id": "1",
          "time": "20210831 18:09"
        },
        {
          "id": "MDRW2100024750.1.9",
          "form": "스파게티나 탕수육같은거 있떠라",
          "original_form": "스파게티나 탕수육같은거 있떠라",
          "speaker_id": "1",
          "time": "20210831 18:10"
        },
        {
          "id": "MDRW2100024750.1.10",
          "form": "샤브샤브 고기도 리필이 됐었어?",
          "original_form": "샤브샤브 고기도 리필이 됐었어?",
          "speaker_id": "2",
          "time": "20210831 18:10"
        },
        {
          "id": "MDRW2100024750.1.11",
          "form": "스파게티 탕수육 둘 다 맛있겠다 쩝",
          "original_form": "스파게티 탕수육 둘 다 맛있겠다 쩝",
          "speaker_id": "2",
          "time": "20210831 18:10"
        },
        {
          "id": "MDRW2100024750.1.12",
          "form": "고기는 리필안됐는데 혼자먹기에 괜찮터라",
          "original_form": "고기는 리필안됐는데 혼자먹기에 괜찮터라",
          "speaker_id": "1",
          "time": "20210831 18:10"
        },
        {
          "id": "MDRW2100024750.1.13",
          "form": "넌 저녁 뭐먹냐잉",
          "original_form": "넌 저녁 뭐먹냐잉",
          "speaker_id": "1",
          "time": "20210831 18:10"
        },
        {
          "id": "MDRW2100024750.1.14",
          "form": "난 된장국이랑 토마토달걀볶음 먹을 것 같아ㅋㅋㅋㅋ",
          "original_form": "난 된장국이랑 토마토달걀볶음 먹을 것 같아ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:10"
        },
        {
          "id": "MDRW2100024750.1.15",
          "form": "요즘 계란 달걀이랑 토마토에 빠졌어",
          "original_form": "요즘 계란 달걀이랑 토마토에 빠졌어",
          "speaker_id": "2",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024750.1.16",
          "form": "세상에 토달볶이라니",
          "original_form": "세상에 토달볶이라니",
          "speaker_id": "1",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024750.1.17",
          "form": "토달볶 먹어봤니?",
          "original_form": "토달볶 먹어봤니?",
          "speaker_id": "2",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024750.1.18",
          "form": "용납할수없다",
          "original_form": "용납할수없다",
          "speaker_id": "1",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024750.1.19",
          "form": "응...중2때",
          "original_form": "응...중2때",
          "speaker_id": "1",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024750.1.20",
          "form": "ㅋㅋㅌㅌㅋㅋㅋㅋㅋ아 싫어서 그런 거였냐",
          "original_form": "ㅋㅋㅌㅌㅋㅋㅋㅋㅋ아 싫어서 그런 거였냐",
          "speaker_id": "2",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024750.1.21",
          "form": "짱웃겨ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "짱웃겨ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:11"
        },
        {
          "id": "MDRW2100024750.1.22",
          "form": "니가 탕수육 하니까",
          "original_form": "니가 탕수육 하니까",
          "speaker_id": "2",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100024750.1.23",
          "form": "생각나는데",
          "original_form": "생각나는데",
          "speaker_id": "2",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100024750.1.24",
          "form": "나 오늘 탕수육 먹는 꿈도 꿨어",
          "original_form": "나 오늘 탕수육 먹는 꿈도 꿨어",
          "speaker_id": "2",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100024750.1.25",
          "form": "요 몇달 간 간간히 생각났는데",
          "original_form": "요 몇달 간 간간히 생각났는데",
          "speaker_id": "2",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100024750.1.26",
          "form": "언제 먹지",
          "original_form": "언제 먹지",
          "speaker_id": "2",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100024750.1.27",
          "form": "탕수육이 땡길땐",
          "original_form": "탕수육이 땡길땐",
          "speaker_id": "1",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100024750.1.28",
          "form": "먹어야지",
          "original_form": "먹어야지",
          "speaker_id": "1",
          "time": "20210831 18:12"
        },
        {
          "id": "MDRW2100024750.1.29",
          "form": "탕수육만 시켜먹기도 되려나",
          "original_form": "탕수육만 시켜먹기도 되려나",
          "speaker_id": "2",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100024750.1.30",
          "form": "혼자 짜장면 탕수육 둘 다는 못 먹는데..",
          "original_form": "혼자 짜장면 탕수육 둘 다는 못 먹는데..",
          "speaker_id": "2",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100024750.1.31",
          "form": "응 될걸 나도 종종시켜먹어",
          "original_form": "응 될걸 나도 종종시켜먹어",
          "speaker_id": "1",
          "time": "20210831 18:13"
        },
        {
          "id": "MDRW2100024750.1.32",
          "form": "옛날엔 군만두서비스였는데",
          "original_form": "옛날엔 군만두서비스였는데",
          "speaker_id": "1",
          "time": "20210831 18:14"
        }
      ]
