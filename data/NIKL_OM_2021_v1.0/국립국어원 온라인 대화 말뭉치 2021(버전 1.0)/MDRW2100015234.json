      "utterance": [
        {
          "id": "MDRW2100015234.1.1",
          "form": "좋아하는 뮤지컬 있어?",
          "original_form": "좋아하는 뮤지컬 있어?",
          "speaker_id": "1",
          "time": "20210825 22:19"
        },
        {
          "id": "MDRW2100015234.1.2",
          "form": "제일 좋아하는 건 오페라의 유령!!",
          "original_form": "제일 좋아하는 건 오페라의 유령!!",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015234.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015234.1.4",
          "form": "오 나도",
          "original_form": "오 나도",
          "speaker_id": "1",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100015234.1.5",
          "form": "그거 2탄도 봤었어",
          "original_form": "그거 2탄도 봤었어",
          "speaker_id": "1",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100015234.1.6",
          "form": "2탄? 1탄 2탄이 따로 있어?",
          "original_form": "2탄? 1탄 2탄이 따로 있어?",
          "speaker_id": "2",
          "time": "20210826 19:11"
        },
        {
          "id": "MDRW2100015234.1.7",
          "form": "후속작인데 결혼한 크리스틴 앞으로 오페라 유령이 나타나는 이야기야",
          "original_form": "후속작인데 결혼한 크리스틴 앞으로 오페라 유령이 나타나는 이야기야",
          "speaker_id": "1",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100015234.1.8",
          "form": "근데 그냥 1탄이 나아",
          "original_form": "근데 그냥 1탄이 나아",
          "speaker_id": "1",
          "time": "20210826 20:11"
        },
        {
          "id": "MDRW2100015234.1.9",
          "form": "와 몰랐어..",
          "original_form": "와 몰랐어..",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100015234.1.10",
          "form": "근데 뭐든 처음 나온 편이 제일 재밌는 거 같아",
          "original_form": "근데 뭐든 처음 나온 편이 제일 재밌는 거 같아",
          "speaker_id": "2",
          "time": "20210826 20:13"
        },
        {
          "id": "MDRW2100015234.1.11",
          "form": "맞아 1탄 따라잡는 작품이 드문 듯ㅋㅋㅋ",
          "original_form": "맞아 1탄 따라잡는 작품이 드문 듯ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100015234.1.12",
          "form": "영화도 그런 거 같아",
          "original_form": "영화도 그런 거 같아",
          "speaker_id": "1",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100015234.1.13",
          "form": "트랜스포머를 좋아한 거까진 아니지만",
          "original_form": "트랜스포머를 좋아한 거까진 아니지만",
          "speaker_id": "1",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100015234.1.14",
          "form": "1이 최고였다는 사람들이 많은 것처럼",
          "original_form": "1이 최고였다는 사람들이 많은 것처럼",
          "speaker_id": "1",
          "time": "20210826 20:15"
        },
        {
          "id": "MDRW2100015234.1.15",
          "form": "오 맞아",
          "original_form": "오 맞아",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100015234.1.16",
          "form": "나도 영화 생각하고 말했어",
          "original_form": "나도 영화 생각하고 말했어",
          "speaker_id": "2",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100015234.1.17",
          "form": "영화 자주 봐?",
          "original_form": "영화 자주 봐?",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100015234.1.18",
          "form": "친구랑 놀 때 영화보러 많이 가",
          "original_form": "친구랑 놀 때 영화보러 많이 가",
          "speaker_id": "2",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100015234.1.19",
          "form": "코로나 터지고는 거의 안 봤어",
          "original_form": "코로나 터지고는 거의 안 봤어",
          "speaker_id": "2",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100015234.1.20",
          "form": "나도 코로나 터지기 전엔 한달에 한번은 봣는데",
          "original_form": "나도 코로나 터지기 전엔 한달에 한번은 봣는데",
          "speaker_id": "1",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100015234.1.21",
          "form": "그 이후론 별로 안 가네",
          "original_form": "그 이후론 별로 안 가네",
          "speaker_id": "1",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100015234.1.22",
          "form": "그치ㅠㅠ 영화관 가는 거 좋은데 아쉬워",
          "original_form": "그치ㅠㅠ 영화관 가는 거 좋은데 아쉬워",
          "speaker_id": "2",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100015234.1.23",
          "form": "영화 어떤 종류 좋아해?",
          "original_form": "영화 어떤 종류 좋아해?",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100015234.1.24",
          "form": "난 뮤지컬영화",
          "original_form": "난 뮤지컬영화",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100015234.1.25",
          "form": "나는 소설 기반 영화 좋아해",
          "original_form": "나는 소설 기반 영화 좋아해",
          "speaker_id": "2",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100015234.1.26",
          "form": "오 뮤지컬 영화",
          "original_form": "오 뮤지컬 영화",
          "speaker_id": "2",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100015234.1.27",
          "form": "얼마전에 위대한 쇼맨 추천 받았는데",
          "original_form": "얼마전에 위대한 쇼맨 추천 받았는데",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100015234.1.28",
          "form": "그거 안봤는데 ost는 알아ㅋㅋㅋㅋㅋ",
          "original_form": "그거 안봤는데 ost는 알아ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100015234.1.29",
          "form": "오 소설기반이면 해리포터 같은?",
          "original_form": "오 소설기반이면 해리포터 같은?",
          "speaker_id": "1",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100015234.1.30",
          "form": "노래 너무 좋더라구",
          "original_form": "노래 너무 좋더라구",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100015234.1.31",
          "form": "응 해리포터ㅠㅜㅜㅜㅠㅡ 내 인생영화",
          "original_form": "응 해리포터ㅠㅜㅜㅜㅠㅡ 내 인생영화",
          "speaker_id": "2",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100015234.1.32",
          "form": "해리포터.. 인기 많지 나도 그거 마지막편(?)은 보러갔었어",
          "original_form": "해리포터.. 인기 많지 나도 그거 마지막편(?)은 보러갔었어",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100015234.1.33",
          "form": "즉ㆍ음의성물인가",
          "original_form": "즉ㆍ음의성물인가",
          "speaker_id": "1",
          "time": "20210826 20:35"
        },
        {
          "id": "MDRW2100015234.1.34",
          "form": "마지막만 봤구나ㅠㅜㅠ",
          "original_form": "마지막만 봤구나ㅠㅜㅠ",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100015234.1.35",
          "form": "다른 편은 아예 봤어?",
          "original_form": "다른 편은 아예 봤어?",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100015234.1.36",
          "form": "안 봤어?",
          "original_form": "안 봤어?",
          "speaker_id": "2",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100015234.1.37",
          "form": "책으로 마법사의 돌은 봤었어",
          "original_form": "책으로 마법사의 돌은 봤었어",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100015234.1.38",
          "form": "해리포터 찐뉴비",
          "original_form": "해리포터 찐뉴비",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100015234.1.39",
          "form": "암것도몰라",
          "original_form": "암것도몰라",
          "speaker_id": "1",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100015234.1.40",
          "form": "1편이 진짜 그 특유의 분위기가 있어..",
          "original_form": "1편이 진짜 그 특유의 분위기가 있어..",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100015234.1.41",
          "form": "크리스마스에 꼭 봐줘",
          "original_form": "크리스마스에 꼭 봐줘",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100015234.1.42",
          "form": "ㅠㅜㅠ",
          "original_form": "ㅠㅜㅠ",
          "speaker_id": "2",
          "time": "20210826 20:38"
        },
        {
          "id": "MDRW2100015234.1.43",
          "form": "좋아 이번 크리스마스엔 해리포터 볼게",
          "original_form": "좋아 이번 크리스마스엔 해리포터 볼게",
          "speaker_id": "1",
          "time": "20210826 20:39"
        },
        {
          "id": "MDRW2100015234.1.44",
          "form": "고마워 기억나면 봐줘ㅎㅎ",
          "original_form": "고마워 기억나면 봐줘ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:41"
        }
      ]
