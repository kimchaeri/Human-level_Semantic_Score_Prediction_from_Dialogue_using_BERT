      "utterance": [
        {
          "id": "MDRW2100025784.1.1",
          "form": "시상에...",
          "original_form": "시상에...",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100025784.1.2",
          "form": "한다는거듣고 욕하긴했는데",
          "original_form": "한다는거듣고 욕하긴했는데",
          "speaker_id": "1",
          "time": "20210901 16:57"
        },
        {
          "id": "MDRW2100025784.1.3",
          "form": "거긴 의료붕괴여서 코로나 걸린 산모가 자택요양하다 애기 낳아서 애기 죽고",
          "original_form": "거긴 의료붕괴여서 코로나 걸린 산모가 자택요양하다 애기 낳아서 애기 죽고",
          "speaker_id": "2",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025784.1.4",
          "form": "헐........",
          "original_form": "헐........",
          "speaker_id": "1",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025784.1.5",
          "form": "젊은이도 코로나로 집에서 죽고 난리인데",
          "original_form": "젊은이도 코로나로 집에서 죽고 난리인데",
          "speaker_id": "2",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025784.1.6",
          "form": "진짜정신못차려ㅜㅜ",
          "original_form": "진짜정신못차려ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025784.1.7",
          "form": "으아아아 그정도로심각한줄 몰랐어ㅇ",
          "original_form": "으아아아 그정도로심각한줄 몰랐어ㅇ",
          "speaker_id": "1",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025784.1.8",
          "form": "올림픽때 연일 난리였다는데",
          "original_form": "올림픽때 연일 난리였다는데",
          "speaker_id": "1",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100025784.1.9",
          "form": "진짜 올림픽 강행한게.... 노답 절정이었지",
          "original_form": "진짜 올림픽 강행한게.... 노답 절정이었지",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025784.1.10",
          "form": "근데 페스티벌 가서 노는거 보니깐",
          "original_form": "근데 페스티벌 가서 노는거 보니깐",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025784.1.11",
          "form": "진짜....",
          "original_form": "진짜....",
          "speaker_id": "1",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025784.1.12",
          "form": "노 답도 안되니",
          "original_form": "노 답도 안되니",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025784.1.13",
          "form": "비상사태 이미 무소용이라던데",
          "original_form": "비상사태 이미 무소용이라던데",
          "speaker_id": "1",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025784.1.14",
          "form": "우리도 위드코로나해도",
          "original_form": "우리도 위드코로나해도",
          "speaker_id": "1",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100025784.1.15",
          "form": "저런러함부로 하면 안돼....",
          "original_form": "저런러함부로 하면 안돼....",
          "speaker_id": "1",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100025784.1.16",
          "form": "콘서트 하더라도 스탠딩에 좌석 깔고",
          "original_form": "콘서트 하더라도 스탠딩에 좌석 깔고",
          "speaker_id": "2",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100025784.1.17",
          "form": "걍 좌석으로...",
          "original_form": "걍 좌석으로...",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100025784.1.18",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100025784.1.19",
          "form": "절대",
          "original_form": "절대",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100025784.1.20",
          "form": "좌석에서 함성금지.....",
          "original_form": "좌석에서 함성금지.....",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100025784.1.21",
          "form": "아니 이거 다맞아도 돌파감염되는데",
          "original_form": "아니 이거 다맞아도 돌파감염되는데",
          "speaker_id": "1",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100025784.1.22",
          "form": "인도처럼될까봐 무서움",
          "original_form": "인도처럼될까봐 무서움",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100025784.1.23",
          "form": "이미 우리나라 돌파감염 추이 증가 중이라며ㅜㅜ",
          "original_form": "이미 우리나라 돌파감염 추이 증가 중이라며ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100025784.1.24",
          "form": "그런거보면 위드코로나 힘들어 진짜",
          "original_form": "그런거보면 위드코로나 힘들어 진짜",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100025784.1.25",
          "form": "ㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100025784.1.26",
          "form": "아제돌파감염",
          "original_form": "아제돌파감염",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100025784.1.27",
          "form": "별로없는이유가",
          "original_form": "별로없는이유가",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100025784.1.28",
          "form": "나이드신분들이많아서잖아",
          "original_form": "나이드신분들이많아서잖아",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100025784.1.29",
          "form": "그분들은 막 많이 돌아다니지않으시니까",
          "original_form": "그분들은 막 많이 돌아다니지않으시니까",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100025784.1.30",
          "form": "근데 사회활돔활발하게 하는 인구들이 계속걸리면",
          "original_form": "근데 사회활돔활발하게 하는 인구들이 계속걸리면",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025784.1.31",
          "form": "ㅜㅜ그게제일심각할듯",
          "original_form": "ㅜㅜ그게제일심각할듯",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100025784.1.32",
          "form": "맞어ㅜㅜㅜㅜ진짜 집 회사만 하는데도 걸리는 경우도 있으니깐",
          "original_form": "맞어ㅜㅜㅜㅜ진짜 집 회사만 하는데도 걸리는 경우도 있으니깐",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025784.1.33",
          "form": "언제어디서걸릴지도모르고",
          "original_form": "언제어디서걸릴지도모르고",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025784.1.34",
          "form": "그렇다고 경제활동을 안할수는없고",
          "original_form": "그렇다고 경제활동을 안할수는없고",
          "speaker_id": "2",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100025784.1.35",
          "form": "그니까 ㅠㅕ",
          "original_form": "그니까 ㅠㅕ",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100025784.1.36",
          "form": "우리회사도 다들 안걸리는게 신기할지경",
          "original_form": "우리회사도 다들 안걸리는게 신기할지경",
