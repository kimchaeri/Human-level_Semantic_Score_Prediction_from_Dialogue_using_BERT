      "utterance": [
        {
          "id": "MDRW2100032763.1.1",
          "form": "어떤음식 좋아하세여",
          "original_form": "어떤음식 좋아하세여",
          "speaker_id": "2",
          "time": "20210905 23:14"
        },
        {
          "id": "MDRW2100032763.1.2",
          "form": "저는 단거요ㅎㅎ",
          "original_form": "저는 단거요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100032763.1.3",
          "form": "빵순이에요",
          "original_form": "빵순이에요",
          "speaker_id": "1",
          "time": "20210906 16:18"
        },
        {
          "id": "MDRW2100032763.1.4",
          "form": "빵맛잇죠 저도 빵 면 좋아해여",
          "original_form": "빵맛잇죠 저도 빵 면 좋아해여",
          "speaker_id": "2",
          "time": "20210906 16:20"
        },
        {
          "id": "MDRW2100032763.1.5",
          "form": "탄수화물좋죠",
          "original_form": "탄수화물좋죠",
          "speaker_id": "1",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100032763.1.6",
          "form": "어제도쌀국수먹었어요",
          "original_form": "어제도쌀국수먹었어요",
          "speaker_id": "1",
          "time": "20210906 16:23"
        },
        {
          "id": "MDRW2100032763.1.7",
          "form": "전 라면이요 근데오늘도 먹고싶네요",
          "original_form": "전 라면이요 근데오늘도 먹고싶네요",
          "speaker_id": "2",
          "time": "20210906 16:28"
        },
        {
          "id": "MDRW2100032763.1.8",
          "form": "ㅎㅎㅎ국물당기는날씨에요",
          "original_form": "ㅎㅎㅎ국물당기는날씨에요",
          "speaker_id": "1",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100032763.1.9",
          "form": "커피도드세요?",
          "original_form": "커피도드세요?",
          "speaker_id": "1",
          "time": "20210906 16:37"
        },
        {
          "id": "MDRW2100032763.1.10",
          "form": "커피는 안먹어요\r\n비가오니 그렿네요",
          "original_form": "커피는 안먹어요\r\n비가오니 그렿네요",
          "speaker_id": "2",
          "time": "20210906 16:38"
        },
        {
          "id": "MDRW2100032763.1.11",
          "form": "그럼 라면은 어떤거좋아하세요?",
          "original_form": "그럼 라면은 어떤거좋아하세요?",
          "speaker_id": "1",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100032763.1.12",
          "form": "진라면 잘먹어요",
          "original_form": "진라면 잘먹어요",
          "speaker_id": "2",
          "time": "20210906 16:39"
        },
        {
          "id": "MDRW2100032763.1.13",
          "form": "순한맛인가요",
          "original_form": "순한맛인가요",
          "speaker_id": "1",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100032763.1.14",
          "form": "매운맛 좋아해요ㅋㅋ",
          "original_form": "매운맛 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:40"
        },
        {
          "id": "MDRW2100032763.1.15",
          "form": "ㅎㅎㅎ매운맛맛있죠",
          "original_form": "ㅎㅎㅎ매운맛맛있죠",
          "speaker_id": "1",
          "time": "20210906 16:41"
        },
        {
          "id": "MDRW2100032763.1.16",
          "form": "네 어제 거기에 숙주 넣어서 먹엇는더 맛잇더라고요",
          "original_form": "네 어제 거기에 숙주 넣어서 먹엇는더 맛잇더라고요",
          "speaker_id": "2",
          "time": "20210906 16:42"
        },
        {
          "id": "MDRW2100032763.1.17",
          "form": "앗 좋은레시피네요 저는 계란넣어 먹는거 좋아해요",
          "original_form": "앗 좋은레시피네요 저는 계란넣어 먹는거 좋아해요",
          "speaker_id": "1",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100032763.1.18",
          "form": "그것도 맛있져\r\n파 넣어도 맛나요ㅋ",
          "original_form": "그것도 맛있져\r\n파 넣어도 맛나요ㅋ",
          "speaker_id": "2",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100032763.1.19",
          "form": "앗그것도좋네요",
          "original_form": "앗그것도좋네요",
          "speaker_id": "1",
          "time": "20210906 16:58"
        }
      ]
