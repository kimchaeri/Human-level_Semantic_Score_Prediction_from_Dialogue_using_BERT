      "utterance": [
        {
          "id": "MDRW2100036585.1.1",
          "form": "예쁜옷입고 바다가고싶네욥",
          "original_form": "예쁜옷입고 바다가고싶네욥",
          "speaker_id": "2",
          "time": "20210831 01:15"
        },
        {
          "id": "MDRW2100036585.1.2",
          "form": "네 저도 바다 너무 가고싶어요ㅠㅠ",
          "original_form": "네 저도 바다 너무 가고싶어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 01:27"
        },
        {
          "id": "MDRW2100036585.1.3",
          "form": "어디바다 젤좋아하세요",
          "original_form": "어디바다 젤좋아하세요",
          "speaker_id": "1",
          "time": "20210831 01:28"
        },
        {
          "id": "MDRW2100036585.1.4",
          "form": "제주 바다가 생각나네요",
          "original_form": "제주 바다가 생각나네요",
          "speaker_id": "2",
          "time": "20210831 06:38"
        },
        {
          "id": "MDRW2100036585.1.5",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210831 06:38"
        },
        {
          "id": "MDRW2100036585.1.6",
          "form": "네 저두요 제주바다랑 포항바다생각나요",
          "original_form": "네 저두요 제주바다랑 포항바다생각나요",
          "speaker_id": "1",
          "time": "20210831 13:37"
        },
        {
          "id": "MDRW2100036585.1.7",
          "form": "포항가보셨요?",
          "original_form": "포항가보셨요?",
          "speaker_id": "1",
          "time": "20210831 13:37"
        },
        {
          "id": "MDRW2100036585.1.8",
          "form": "안가봤어요ㅠ 포항바다는 어떤가요?",
          "original_form": "안가봤어요ㅠ 포항바다는 어떤가요?",
          "speaker_id": "2",
          "time": "20210831 15:24"
        },
        {
          "id": "MDRW2100036585.1.9",
          "form": "물진짜 맑고 좋아보였어요ㅜㅜㅜㅜ",
          "original_form": "물진짜 맑고 좋아보였어요ㅜㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100036585.1.10",
          "form": "카페구경갔다가 바다봤는데",
          "original_form": "카페구경갔다가 바다봤는데",
          "speaker_id": "1",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100036585.1.11",
          "form": "보는것만으로도 시원하고 좋더라구여",
          "original_form": "보는것만으로도 시원하고 좋더라구여",
          "speaker_id": "1",
          "time": "20210831 15:25"
        },
        {
          "id": "MDRW2100036585.1.12",
          "form": "우아.. ㅠ 진짜 바다 근처에 사시는 분들 부러워요",
          "original_form": "우아.. ㅠ 진짜 바다 근처에 사시는 분들 부러워요",
          "speaker_id": "2",
          "time": "20210831 15:29"
        },
        {
          "id": "MDRW2100036585.1.13",
          "form": "맞아요 나중에 노년에 바다근처에서 ㄹ살고싶어요",
          "original_form": "맞아요 나중에 노년에 바다근처에서 ㄹ살고싶어요",
          "speaker_id": "1",
          "time": "20210831 15:30"
        },
        {
          "id": "MDRW2100036585.1.14",
          "form": "산책도하고ㅜㅜ바다도보고",
          "original_form": "산책도하고ㅜㅜ바다도보고",
          "speaker_id": "1",
          "time": "20210831 15:30"
        },
        {
          "id": "MDRW2100036585.1.15",
          "form": "크아 진짜요!!",
          "original_form": "크아 진짜요!!",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100036585.1.16",
          "form": "제주도서 살고싶다~ 생각했는데 포항도 좋구 여수도 좋을거같아요",
          "original_form": "제주도서 살고싶다~ 생각했는데 포항도 좋구 여수도 좋을거같아요",
          "speaker_id": "2",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100036585.1.17",
          "form": "여수도 넘좋던데요ㅜㅜ",
          "original_form": "여수도 넘좋던데요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 16:26"
        },
        {
          "id": "MDRW2100036585.1.18",
          "form": "진짜 바닷가 도시들 너무좋아요",
          "original_form": "진짜 바닷가 도시들 너무좋아요",
          "speaker_id": "1",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100036585.1.19",
          "form": "해외는 어디가보고싶으셍6",
          "original_form": "해외는 어디가보고싶으셍6",
          "speaker_id": "1",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100036585.1.20",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20210831 16:27"
        },
        {
          "id": "MDRW2100036585.1.21",
          "form": "진짜 좋아요!ㅠ",
          "original_form": "진짜 좋아요!ㅠ",
          "speaker_id": "2",
          "time": "20210831 16:32"
        },
        {
          "id": "MDRW2100036585.1.22",
          "form": "네덜란드 가보고 싶어요!",
          "original_form": "네덜란드 가보고 싶어요!",
