      "utterance": [
        {
          "id": "MDRW2100001853.1.1",
          "form": "맥도날드 알바 생각난다..",
          "original_form": "맥도날드 알바 생각난다..",
          "speaker_id": "2",
          "time": "20210819 21:45"
        },
        {
          "id": "MDRW2100001853.1.2",
          "form": "나도 기억난다..",
          "original_form": "나도 기억난다..",
          "speaker_id": "1",
          "time": "20210819 21:46"
        },
        {
          "id": "MDRW2100001853.1.3",
          "form": "야구장 알바도 잊지말라구",
          "original_form": "야구장 알바도 잊지말라구",
          "speaker_id": "1",
          "time": "20210819 21:46"
        },
        {
          "id": "MDRW2100001853.1.4",
          "form": "ㅎ...그건 별로 기억하고 싶지 않네..",
          "original_form": "ㅎ...그건 별로 기억하고 싶지 않네..",
          "speaker_id": "2",
          "time": "20210819 21:46"
        },
        {
          "id": "MDRW2100001853.1.5",
          "form": "요새 최저시급 얼마지",
          "original_form": "요새 최저시급 얼마지",
          "speaker_id": "2",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001853.1.6",
          "form": "9천 얼만가?",
          "original_form": "9천 얼만가?",
          "speaker_id": "1",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001853.1.7",
          "form": "내년에 많이 오르던데",
          "original_form": "내년에 많이 오르던데",
          "speaker_id": "1",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001853.1.8",
          "form": "8720원 이야",
          "original_form": "8720원 이야",
          "speaker_id": "2",
          "time": "20210819 21:47"
        },
        {
          "id": "MDRW2100001853.1.9",
          "form": "헐 9천원이 안되다니",
          "original_form": "헐 9천원이 안되다니",
          "speaker_id": "1",
          "time": "20210819 21:48"
        },
        {
          "id": "MDRW2100001853.1.10",
          "form": "뭔가 몇년전에 확 오르고 쪼금씩 오르는 느낌이다",
          "original_form": "뭔가 몇년전에 확 오르고 쪼금씩 오르는 느낌이다",
          "speaker_id": "2",
          "time": "20210819 21:48"
        },
        {
          "id": "MDRW2100001853.1.11",
          "form": "그치 나도 생각보다 낮아서 놀람",
          "original_form": "그치 나도 생각보다 낮아서 놀람",
          "speaker_id": "2",
          "time": "20210819 21:48"
        },
        {
          "id": "MDRW2100001853.1.12",
          "form": "내년이 9천원 넘나보다 그러면",
          "original_form": "내년이 9천원 넘나보다 그러면",
          "speaker_id": "1",
          "time": "20210819 21:52"
        },
        {
          "id": "MDRW2100001853.1.13",
          "form": "5퍼센트였나 오른다고 ㄱ했던거같아",
          "original_form": "5퍼센트였나 오른다고 ㄱ했던거같아",
          "speaker_id": "1",
          "time": "20210819 21:52"
        },
        {
          "id": "MDRW2100001853.1.14",
          "form": "할 글쿠나",
          "original_form": "할 글쿠나",
          "speaker_id": "2",
          "time": "20210819 22:30"
        },
        {
          "id": "MDRW2100001853.1.15",
          "form": "주휴수당 받고하면 만원 좀 안되겠다",
          "original_form": "주휴수당 받고하면 만원 좀 안되겠다",
          "speaker_id": "2",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001853.1.16",
          "form": "오늘 일 많이했니..ㅠ",
          "original_form": "오늘 일 많이했니..ㅠ",
          "speaker_id": "2",
          "time": "20210819 22:31"
        },
        {
          "id": "MDRW2100001853.1.17",
          "form": "응 일 많았지..",
          "original_form": "응 일 많았지..",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001853.1.18",
          "form": "아직 다 못끝내서 내일 아침에 가서 해야돼",
          "original_form": "아직 다 못끝내서 내일 아침에 가서 해야돼",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001853.1.19",
          "form": "출근하자마자..",
          "original_form": "출근하자마자..",
          "speaker_id": "1",
          "time": "20210819 22:34"
        },
        {
          "id": "MDRW2100001853.1.20",
          "form": "업무량 많아서 스트레스지..ㅠ",
          "original_form": "업무량 많아서 스트레스지..ㅠ",
          "speaker_id": "2",
          "time": "20210826 10:51"
        },
        {
          "id": "MDRW2100001853.1.21",
          "form": "일 적고 돈만 만히 줬으면 좋겠네요..",
          "original_form": "일 적고 돈만 만히 줬으면 좋겠네요..",
          "speaker_id": "2",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100001853.1.22",
          "form": "제발",
          "original_form": "제발",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100001853.1.23",
          "form": "적게 일하고",
          "original_form": "적게 일하고",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100001853.1.24",
          "form": "돈 많이..",
          "original_form": "돈 많이..",
          "speaker_id": "1",
          "time": "20210826 21:24"
        },
        {
          "id": "MDRW2100001853.1.25",
          "form": "이번년도안에 제발요..^^",
          "original_form": "이번년도안에 제발요..^^",
