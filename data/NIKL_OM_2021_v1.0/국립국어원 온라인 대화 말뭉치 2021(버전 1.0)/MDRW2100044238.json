      "utterance": [
        {
          "id": "MDRW2100044238.1.1",
          "form": "잘해내실꺼에요",
          "original_form": "잘해내실꺼에요",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044238.1.2",
          "form": "자신감이다죠",
          "original_form": "자신감이다죠",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100044238.1.3",
          "form": "공감합니다ㅋㅋㅋㅋㅋ",
          "original_form": "공감합니다ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044238.1.4",
          "form": "아르바이트 같은건 공무원에서는 못하시겠네요",
          "original_form": "아르바이트 같은건 공무원에서는 못하시겠네요",
          "speaker_id": "2",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044238.1.5",
          "form": "ㅎㅎㅎ아르바이트하시나요",
          "original_form": "ㅎㅎㅎ아르바이트하시나요",
          "speaker_id": "1",
          "time": "20210907 22:02"
        },
        {
          "id": "MDRW2100044238.1.6",
          "form": "저는 주말에 잠깐잠깐 하고 있어요",
          "original_form": "저는 주말에 잠깐잠깐 하고 있어요",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044238.1.7",
          "form": "ㅋㅋㅋㅋ물론 투잡 금지는 동일하지만",
          "original_form": "ㅋㅋㅋㅋ물론 투잡 금지는 동일하지만",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044238.1.8",
          "form": "조용히 하고있죠",
          "original_form": "조용히 하고있죠",
          "speaker_id": "2",
          "time": "20210907 22:03"
        },
        {
          "id": "MDRW2100044238.1.9",
          "form": "오 어떤거요?",
          "original_form": "오 어떤거요?",
          "speaker_id": "1",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044238.1.10",
          "form": "저는 병원에서 몇 시간씩 일 돕고 있어요 주말에!",
          "original_form": "저는 병원에서 몇 시간씩 일 돕고 있어요 주말에!",
          "speaker_id": "2",
          "time": "20210907 22:04"
        },
        {
          "id": "MDRW2100044238.1.11",
          "form": "조무사세요?",
          "original_form": "조무사세요?",
          "speaker_id": "1",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044238.1.12",
          "form": "약국에서 근무 중이에요~",
          "original_form": "약국에서 근무 중이에요~",
          "speaker_id": "2",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044238.1.13",
          "form": "오 그러시군요",
          "original_form": "오 그러시군요",
          "speaker_id": "1",
          "time": "20210907 22:17"
        },
        {
          "id": "MDRW2100044238.1.14",
          "form": "뭔가 알바는 시적하고 나니까 플러스 알파 같은 느낌이라서",
          "original_form": "뭔가 알바는 시적하고 나니까 플러스 알파 같은 느낌이라서",
          "speaker_id": "2",
          "time": "20210907 22:20"
        },
        {
          "id": "MDRW2100044238.1.15",
          "form": "중간에 끊지를 못하겠더라구요",
          "original_form": "중간에 끊지를 못하겠더라구요",
          "speaker_id": "2",
          "time": "20210907 22:21"
        },
        {
          "id": "MDRW2100044238.1.16",
          "form": "오 근데주말에안피곤하세요",
          "original_form": "오 근데주말에안피곤하세요",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044238.1.17",
          "form": "근데 일이 엄청 빡세지는 않아요",
          "original_form": "근데 일이 엄청 빡세지는 않아요",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044238.1.18",
          "form": "제 할일도 좀 하며 시간 보내다 오는 느낌이라서",
          "original_form": "제 할일도 좀 하며 시간 보내다 오는 느낌이라서",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044238.1.19",
          "form": "대신 주말 약속 잡기가 좀 어렵죠",
          "original_form": "대신 주말 약속 잡기가 좀 어렵죠",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044238.1.20",
          "form": "긍정적이시네요",
          "original_form": "긍정적이시네요",
          "speaker_id": "1",
          "time": "20210907 22:25"
        }
      ]
