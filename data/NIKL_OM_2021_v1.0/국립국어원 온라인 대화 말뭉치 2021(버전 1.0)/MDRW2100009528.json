        },
        {
          "id": "MDRW2100009528.1.36",
          "form": "남편 뻘쭘",
          "original_form": "남편 뻘쭘",
          "speaker_id": "2",
          "time": "20210823 00:18"
        },
        {
          "id": "MDRW2100009528.1.37",
          "form": "뻗어계셨다며 ㅋㅋㅋ 왜 깨어나셨나",
          "original_form": "뻗어계셨다며 ㅋㅋㅋ 왜 깨어나셨나",
          "speaker_id": "1",
