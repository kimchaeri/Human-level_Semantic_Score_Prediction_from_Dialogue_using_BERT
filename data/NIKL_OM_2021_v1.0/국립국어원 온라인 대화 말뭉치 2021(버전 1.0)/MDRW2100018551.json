      "utterance": [
        {
          "id": "MDRW2100018551.1.1",
          "form": "부산 가래떡 오뎅 드셔보셨나요",
          "original_form": "부산 가래떡 오뎅 드셔보셨나요",
          "speaker_id": "1",
          "time": "20210827 08:08"
        },
        {
          "id": "MDRW2100018551.1.2",
          "form": "아니요ㅜㅜ",
          "original_form": "아니요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 08:11"
        },
        {
          "id": "MDRW2100018551.1.3",
          "form": "먹어보고 싶었는데ㅜㅜ",
          "original_form": "먹어보고 싶었는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 08:11"
        },
        {
          "id": "MDRW2100018551.1.4",
          "form": "엄청 맛있겠죠?(꿀꺽)",
          "original_form": "엄청 맛있겠죠?(꿀꺽)",
          "speaker_id": "2",
          "time": "20210827 08:11"
        },
        {
          "id": "MDRW2100018551.1.5",
          "form": "name1는 드셔보셨나요?",
          "original_form": "&name1&는 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210827 08:11"
        },
        {
          "id": "MDRW2100018551.1.6",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210827 08:12"
        },
        {
          "id": "MDRW2100018551.1.7",
          "form": "오뎅국물에 가래떡이라 맛이 궁금해요",
          "original_form": "오뎅국물에 가래떡이라 맛이 궁금해요",
          "speaker_id": "1",
          "time": "20210827 08:12"
        },
        {
          "id": "MDRW2100018551.1.8",
          "form": "간이 잘 배었을까.",
          "original_form": "간이 잘 배었을까.",
          "speaker_id": "1",
          "time": "20210827 08:12"
        },
        {
          "id": "MDRW2100018551.1.9",
          "form": "둘 다 맛있는거니까 맛있겠죠?ㅎㅎ",
          "original_form": "둘 다 맛있는거니까 맛있겠죠?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 08:12"
        },
        {
          "id": "MDRW2100018551.1.10",
          "form": "부산여행서 뭐 드셔보셨어요?",
          "original_form": "부산여행서 뭐 드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210827 08:12"
        },
        {
          "id": "MDRW2100018551.1.11",
          "form": "꼼장어 돼지국밥 밀면 만두 씨앗호떡  족발",
          "original_form": "꼼장어 돼지국밥 밀면 만두 씨앗호떡  족발",
          "speaker_id": "2",
          "time": "20210827 08:13"
        },
        {
          "id": "MDRW2100018551.1.12",
          "form": "회",
          "original_form": "회",
          "speaker_id": "2",
          "time": "20210827 08:13"
        },
        {
          "id": "MDRW2100018551.1.13",
          "form": "저 회 안 좋아하는데 광안리에서 먹은 회가 진심 맛있었어요",
          "original_form": "저 회 안 좋아하는데 광안리에서 먹은 회가 진심 맛있었어요",
          "speaker_id": "2",
          "time": "20210827 08:14"
        },
        {
          "id": "MDRW2100018551.1.14",
          "form": "엄청 다양하게 드셨네요ㅎ",
          "original_form": "엄청 다양하게 드셨네요ㅎ",
          "speaker_id": "1",
          "time": "20210827 08:14"
        },
        {
          "id": "MDRW2100018551.1.15",
          "form": "회센터에서 드셨나요",
          "original_form": "회센터에서 드셨나요",
          "speaker_id": "1",
          "time": "20210827 08:14"
        },
        {
          "id": "MDRW2100018551.1.16",
          "form": "한번은 회센터, 한번은 칠성회관이요ㅎㅅㄴ",
          "original_form": "한번은 회센터, 한번은 칠성회관이요ㅎㅅㄴ",
          "speaker_id": "2",
          "time": "20210827 08:14"
        },
        {
          "id": "MDRW2100018551.1.17",
          "form": "칠성회관이 더 맛있었어요",
          "original_form": "칠성회관이 더 맛있었어요",
          "speaker_id": "2",
          "time": "20210827 08:15"
        },
        {
          "id": "MDRW2100018551.1.18",
          "form": "아 정말요? 저는 줄창 회센터만 갔네요",
          "original_form": "아 정말요? 저는 줄창 회센터만 갔네요",
          "speaker_id": "1",
          "time": "20210827 08:15"
        },
        {
          "id": "MDRW2100018551.1.19",
          "form": "담에는 칠성회관 가야겠어요",
          "original_form": "담에는 칠성회관 가야겠어요",
          "speaker_id": "1",
          "time": "20210827 08:15"
        },
        {
          "id": "MDRW2100018551.1.20",
          "form": "무슨 회로 드셨어요?",
          "original_form": "무슨 회로 드셨어요?",
          "speaker_id": "1",
          "time": "20210827 08:16"
        },
        {
          "id": "MDRW2100018551.1.21",
          "form": "거기 종업원이 잡어 추천해서 먹었는데 진심 맛있었어요ㅜㅜ",
          "original_form": "거기 종업원이 잡어 추천해서 먹었는데 진심 맛있었어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 08:16"
        },
        {
          "id": "MDRW2100018551.1.22",
          "form": "막장이랑 너무 잘 어울려요",
          "original_form": "막장이랑 너무 잘 어울려요",
          "speaker_id": "2",
          "time": "20210827 08:17"
        },
        {
          "id": "MDRW2100018551.1.23",
          "form": "오오 막장이랑은 안 먹어봤는데",
          "original_form": "오오 막장이랑은 안 먹어봤는데",
          "speaker_id": "1",
          "time": "20210827 08:18"
        },
        {
          "id": "MDRW2100018551.1.24",
          "form": "맛이 궁금해요",
          "original_form": "맛이 궁금해요",
          "speaker_id": "1",
          "time": "20210827 08:18"
        },
        {
          "id": "MDRW2100018551.1.25",
          "form": "초장과 많이 다른가요",
          "original_form": "초장과 많이 다른가요",
          "speaker_id": "1",
          "time": "20210827 08:19"
        },
        {
          "id": "MDRW2100018551.1.26",
          "form": "네 완전 달라요. 아마 된장베이스일 듯",
          "original_form": "네 완전 달라요. 아마 된장베이스일 듯",
          "speaker_id": "2",
          "time": "20210827 08:21"
        },
        {
          "id": "MDRW2100018551.1.27",
          "form": "근데 수원 와서 막장에 회 먹으려니까 그 맛이 안 나더라구요",
          "original_form": "근데 수원 와서 막장에 회 먹으려니까 그 맛이 안 나더라구요",
          "speaker_id": "2",
          "time": "20210827 08:21"
        },
        {
          "id": "MDRW2100018551.1.28",
          "form": "ㅜ장이 다른가요",
          "original_form": "ㅜ장이 다른가요",
          "speaker_id": "1",
          "time": "20210827 08:22"
        },
        {
          "id": "MDRW2100018551.1.29",
          "form": "아니면 회가?",
          "original_form": "아니면 회가?",
          "speaker_id": "1",
          "time": "20210827 08:22"
        }
      ]
