      "utterance": [
        {
          "id": "MDRW2100004469.1.1",
          "form": "다이어트 해본신적 있으세요?",
          "original_form": "다이어트 해본신적 있으세요?",
          "speaker_id": "1",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004469.1.2",
          "form": "매번 작심3일이라 제대호 해본적은 없네요;",
          "original_form": "매번 작심3일이라 제대호 해본적은 없네요;",
          "speaker_id": "2",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004469.1.3",
          "form": "제대로",
          "original_form": "제대로",
          "speaker_id": "2",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004469.1.4",
          "form": "먹는게 행복해서 저도 안해봤어요",
          "original_form": "먹는게 행복해서 저도 안해봤어요",
          "speaker_id": "1",
          "time": "20210821 16:46"
        },
        {
          "id": "MDRW2100004469.1.5",
          "form": "운동은 하시는거있으세요?",
          "original_form": "운동은 하시는거있으세요?",
          "speaker_id": "1",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004469.1.6",
          "form": "그쵸 먹는거 짱! 스트레칭 가끔하는거 외에는...잘 살고 있습니다!name1님은요?",
          "original_form": "그쵸 먹는거 짱! 스트레칭 가끔하는거 외에는...잘 살고 있습니다!&name1&님은요?",
          "speaker_id": "2",
          "time": "20210821 16:47"
        },
        {
          "id": "MDRW2100004469.1.7",
          "form": "스피닝이랑 줌바 해봤는데 요즘 코로나로 숨쉬기 운동 하고있어요ㅎ",
          "original_form": "스피닝이랑 줌바 해봤는데 요즘 코로나로 숨쉬기 운동 하고있어요ㅎ",
          "speaker_id": "1",
          "time": "20210821 16:48"
        },
        {
          "id": "MDRW2100004469.1.8",
          "form": "저도 숨쉬기 운동 매니아입니다..",
          "original_form": "저도 숨쉬기 운동 매니아입니다..",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004469.1.9",
          "form": "ㅎㅎ 요즘 넘 집콕했더니 살만쪄요",
          "original_form": "ㅎㅎ 요즘 넘 집콕했더니 살만쪄요",
          "speaker_id": "1",
          "time": "20210821 16:55"
        },
        {
          "id": "MDRW2100004469.1.10",
          "form": "엄청 많이요...",
          "original_form": "엄청 많이요...",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004469.1.11",
          "form": "그런데 귀찮아서 운동을 안하는..",
          "original_form": "그런데 귀찮아서 운동을 안하는..",
          "speaker_id": "2",
          "time": "20210821 16:56"
        },
        {
          "id": "MDRW2100004469.1.12",
          "form": "배워보고싶은 운동은 있는데 비싸고 코로나도 있고",
          "original_form": "배워보고싶은 운동은 있는데 비싸고 코로나도 있고",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004469.1.13",
          "form": "생각만 하다 끝나요",
          "original_form": "생각만 하다 끝나요",
          "speaker_id": "1",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004469.1.14",
          "form": "그쵸 운동 제대로 배울려면 돈이 많이 깨져요",
          "original_form": "그쵸 운동 제대로 배울려면 돈이 많이 깨져요",
          "speaker_id": "2",
          "time": "20210821 16:57"
        },
        {
          "id": "MDRW2100004469.1.15",
          "form": "골프도 재밌대요",
          "original_form": "골프도 재밌대요",
          "speaker_id": "1",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004469.1.16",
          "form": "돈이 제일 많이 들 것 같은;;;스포츠",
          "original_form": "돈이 제일 많이 들 것 같은;;;스포츠",
          "speaker_id": "2",
          "time": "20210821 16:58"
        },
        {
          "id": "MDRW2100004469.1.17",
          "form": "예전에 골프 장난감으로는 해보았어요",
          "original_form": "예전에 골프 장난감으로는 해보았어요",
          "speaker_id": "2",
          "time": "20210821 16:59"
        },
        {
          "id": "MDRW2100004469.1.18",
          "form": "골프는 아직 대중화가 안됐죠",
          "original_form": "골프는 아직 대중화가 안됐죠",
          "speaker_id": "1",
          "time": "20210821 17:00"
        },
        {
          "id": "MDRW2100004469.1.19",
          "form": "돈있는 자들의 스포츠",
          "original_form": "돈있는 자들의 스포츠",
          "speaker_id": "1",
          "time": "20210821 17:01"
        },
        {
          "id": "MDRW2100004469.1.20",
          "form": "나중에 기회면 배워보려구요 돈많이 벌어서 ㅋ",
          "original_form": "나중에 기회면 배워보려구요 돈많이 벌어서 ㅋ",
          "speaker_id": "1",
          "time": "20210821 21:52"
        }
      ]
