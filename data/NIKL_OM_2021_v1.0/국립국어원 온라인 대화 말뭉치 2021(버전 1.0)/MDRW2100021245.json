      "utterance": [
        {
          "id": "MDRW2100021245.1.1",
          "form": "그치만... 너가 좀 할수도 있는거자나....",
          "original_form": "그치만... 너가 좀 할수도 있는거자나....",
          "speaker_id": "2",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021245.1.2",
          "form": "아악!!!!",
          "original_form": "아악!!!!",
          "speaker_id": "1",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021245.1.3",
          "form": "야 나 요즘 민초 가루 사서",
          "original_form": "야 나 요즘 민초 가루 사서",
          "speaker_id": "2",
          "time": "20210828 17:01"
        },
        {
          "id": "MDRW2100021245.1.4",
          "form": "집에서 민초 프라페 해먹어",
          "original_form": "집에서 민초 프라페 해먹어",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021245.1.5",
          "form": "난 빨리 와플기계 당근에 처분하고싶다구",
          "original_form": "난 빨리 와플기계 당근에 처분하고싶다구",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021245.1.6",
          "form": "맨날맨날 4천원씩 버는 기분이야",
          "original_form": "맨날맨날 4천원씩 버는 기분이야",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021245.1.7",
          "form": "개이득!",
          "original_form": "개이득!",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021245.1.8",
          "form": "프라페해먹는단얘기 전에도하셨어요",
          "original_form": "프라페해먹는단얘기 전에도하셨어요",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021245.1.9",
          "form": "어르신",
          "original_form": "어르신",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021245.1.10",
          "form": "기억력 감퇴가 왓나보지ㅡㅡ",
          "original_form": "기억력 감퇴가 왓나보지ㅡㅡ",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021245.1.11",
          "form": "와플기계 팔지마",
          "original_form": "와플기계 팔지마",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021245.1.12",
          "form": "프라페 +5000\n재료값 -1000\n지흡비용 -50000 (잔당)",
          "original_form": "프라페 +5000\n재료값 -1000\n지흡비용 -50000 (잔당)",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021245.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021245.1.14",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021245.1.15",
          "form": "미췻나",
          "original_form": "미췻나",
          "speaker_id": "2",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021245.1.16",
          "form": "분명이이득이긴하나...",
          "original_form": "분명이이득이긴하나...",
          "speaker_id": "1",
          "time": "20210828 17:02"
        },
        {
          "id": "MDRW2100021245.1.17",
          "form": "잔당 지흡 오만원은 ㅇ너무하잖아",
          "original_form": "잔당 지흡 오만원은 ㅇ너무하잖아",
          "speaker_id": "2",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021245.1.18",
          "form": "얼마인지잘몰라",
          "original_form": "얼마인지잘몰라",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021245.1.19",
          "form": "지흡 그러캐 비싼나",
          "original_form": "지흡 그러캐 비싼나",
          "speaker_id": "2",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021245.1.20",
          "form": "그럼피티비용 (회당) 50000",
          "original_form": "그럼피티비용 (회당) 50000",
          "speaker_id": "1",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021245.1.21",
          "form": "글고 지흡 안하고",
          "original_form": "글고 지흡 안하고",
          "speaker_id": "2",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021245.1.22",
          "form": "홈트 할수도 있지",
          "original_form": "홈트 할수도 있지",
          "speaker_id": "2",
          "time": "20210828 17:03"
        },
        {
          "id": "MDRW2100021245.1.23",
          "form": "샐러드 같은거 먹으면서",
          "original_form": "샐러드 같은거 먹으면서",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021245.1.24",
          "form": "얼추맞을듯 민초프라페=피티한번감소칼로리",
          "original_form": "얼추맞을듯 민초프라페=피티한번감소칼로리",
          "speaker_id": "1",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021245.1.25",
          "form": "그치그치 독학재수로 서울대에 간사람이잇는것처럼",
          "original_form": "그치그치 독학재수로 서울대에 간사람이잇는것처럼",
          "speaker_id": "1",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021245.1.26",
          "form": "근데 민초 먹는다고 피티 한번 안한 효과면",
          "original_form": "근데 민초 먹는다고 피티 한번 안한 효과면",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021245.1.27",
          "form": "피티가 좀 문제가 있는거같다",
          "original_form": "피티가 좀 문제가 있는거같다",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021245.1.28",
          "form": "근본적우로",
          "original_form": "근본적우로",
          "speaker_id": "2",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021245.1.29",
          "form": "민초칼로리가 500인데",
          "original_form": "민초칼로리가 500인데",
          "speaker_id": "1",
          "time": "20210828 17:04"
        },
        {
          "id": "MDRW2100021245.1.30",
          "form": "피티 쌔빠지게해야",
          "original_form": "피티 쌔빠지게해야",
          "speaker_id": "2",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100021245.1.31",
          "form": "500인거구나",
          "original_form": "500인거구나",
          "speaker_id": "2",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100021245.1.32",
          "form": "런닝1시간달리고 근육50분하면 되겠네",
          "original_form": "런닝1시간달리고 근육50분하면 되겠네",
          "speaker_id": "1",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100021245.1.33",
          "form": "근육은더생기겠네 쁠라스알파로",
          "original_form": "근육은더생기겠네 쁠라스알파로",
          "speaker_id": "1",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100021245.1.34",
          "form": "걍 지흡할게",
          "original_form": "걍 지흡할게",
          "speaker_id": "2",
          "time": "20210828 17:05"
        },
        {
          "id": "MDRW2100021245.1.35",
