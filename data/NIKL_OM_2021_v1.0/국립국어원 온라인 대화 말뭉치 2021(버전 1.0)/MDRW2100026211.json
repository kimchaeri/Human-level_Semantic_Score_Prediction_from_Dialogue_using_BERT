      "utterance": [
        {
          "id": "MDRW2100026211.1.1",
          "form": "쇼핑 좋아하세요?",
          "original_form": "쇼핑 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 20:51"
        },
        {
          "id": "MDRW2100026211.1.2",
          "form": "돈쓰는거최고죠ㅋㅋ",
          "original_form": "돈쓰는거최고죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026211.1.3",
          "form": "며칠전에도 운동화 샀습니다",
          "original_form": "며칠전에도 운동화 샀습니다",
          "speaker_id": "2",
          "time": "20210901 21:01"
        },
        {
          "id": "MDRW2100026211.1.4",
          "form": "오 어디꺼요?",
          "original_form": "오 어디꺼요?",
          "speaker_id": "1",
          "time": "20210902 13:39"
        },
        {
          "id": "MDRW2100026211.1.5",
          "form": "아식스요!",
          "original_form": "아식스요!",
          "speaker_id": "2",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100026211.1.6",
          "form": "편하고 좋더라구요",
          "original_form": "편하고 좋더라구요",
          "speaker_id": "2",
          "time": "20210902 13:40"
        },
        {
          "id": "MDRW2100026211.1.7",
          "form": "아식스 가볍죠!",
          "original_form": "아식스 가볍죠!",
          "speaker_id": "1",
          "time": "20210902 13:41"
        },
        {
          "id": "MDRW2100026211.1.8",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210902 13:48"
        },
        {
          "id": "MDRW2100026211.1.9",
          "form": "엄청가벼워요",
          "original_form": "엄청가벼워요",
          "speaker_id": "2",
          "time": "20210902 13:48"
        },
        {
          "id": "MDRW2100026211.1.10",
          "form": "저도 내년엔 러닝화?스타일로 사려구욥!",
          "original_form": "저도 내년엔 러닝화?스타일로 사려구욥!",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100026211.1.11",
          "form": "편한게 최고에요",
          "original_form": "편한게 최고에요",
          "speaker_id": "1",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100026211.1.12",
          "form": "진짜요ㅇㅇ",
          "original_form": "진짜요ㅇㅇ",
          "speaker_id": "2",
          "time": "20210902 15:34"
        },
        {
          "id": "MDRW2100026211.1.13",
          "form": "평소에 나이키만 샀는데",
          "original_form": "평소에 나이키만 샀는데",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100026211.1.14",
          "form": "아식스 추천입니당ㅋㅋ",
          "original_form": "아식스 추천입니당ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 15:35"
        },
        {
          "id": "MDRW2100026211.1.15",
          "form": "네넹 저도 원래 나이키 애용이요 ㅎㅎ",
          "original_form": "네넹 저도 원래 나이키 애용이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 16:14"
        },
        {
          "id": "MDRW2100026211.1.16",
          "form": "최근에 만족하신 쇼핑 있으신가요?",
          "original_form": "최근에 만족하신 쇼핑 있으신가요?",
          "speaker_id": "2",
          "time": "20210902 16:20"
        },
        {
          "id": "MDRW2100026211.1.17",
          "form": "귀걸이 구매했어요 ~",
          "original_form": "귀걸이 구매했어요 ~",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100026211.1.18",
          "form": "오 귀걸이",
          "original_form": "오 귀걸이",
          "speaker_id": "2",
          "time": "20210903 09:15"
        },
        {
          "id": "MDRW2100026211.1.19",
          "form": "저도 귀뚫어보고싶은데 무섭네요ㅜㅜ",
          "original_form": "저도 귀뚫어보고싶은데 무섭네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210903 09:15"
        },
        {
          "id": "MDRW2100026211.1.20",
          "form": "아 안뚫으셨어욘 ?",
          "original_form": "아 안뚫으셨어욘 ?",
          "speaker_id": "1",
          "time": "20210903 09:28"
        },
        {
          "id": "MDRW2100026211.1.21",
          "form": "네ㅠㅠ",
          "original_form": "네ㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 09:29"
        },
        {
          "id": "MDRW2100026211.1.22",
          "form": "그래서 귀찌만 엄청 사들였네요",
          "original_form": "그래서 귀찌만 엄청 사들였네요",
          "speaker_id": "2",
          "time": "20210903 09:29"
        },
        {
          "id": "MDRW2100026211.1.23",
          "form": "요즘도 귀찌 자연스럽게 잘나오더라구용",
          "original_form": "요즘도 귀찌 자연스럽게 잘나오더라구용",
          "speaker_id": "1",
          "time": "20210903 09:33"
