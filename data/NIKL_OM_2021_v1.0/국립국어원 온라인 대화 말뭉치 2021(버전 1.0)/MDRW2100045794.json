      "utterance": [
        {
          "id": "MDRW2100045794.1.1",
          "form": "네 편해서 자주 시켜요",
          "original_form": "네 편해서 자주 시켜요",
          "speaker_id": "2",
          "time": "20210908 07:18"
        },
        {
          "id": "MDRW2100045794.1.2",
          "form": "뭐 좋아하세여?",
          "original_form": "뭐 좋아하세여?",
          "speaker_id": "1",
          "time": "20210908 07:20"
        },
        {
          "id": "MDRW2100045794.1.3",
          "form": "치킨이죠!",
          "original_form": "치킨이죠!",
          "speaker_id": "2",
          "time": "20210908 07:20"
        },
        {
          "id": "MDRW2100045794.1.4",
          "form": "최애 치킨은 뭔가요?",
          "original_form": "최애 치킨은 뭔가요?",
          "speaker_id": "1",
          "time": "20210908 07:22"
        },
        {
          "id": "MDRW2100045794.1.5",
          "form": "지코바 양념이요!",
          "original_form": "지코바 양념이요!",
          "speaker_id": "2",
          "time": "20210908 07:23"
        },
        {
          "id": "MDRW2100045794.1.6",
          "form": "치밥하면 맛잇죠 ㅎㅎ",
          "original_form": "치밥하면 맛잇죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045794.1.7",
          "form": "맛잘알이시네요!ㅋㅋㅋ name1님 최애치킨은 뭔가요",
          "original_form": "맛잘알이시네요!ㅋㅋㅋ &name1&님 최애치킨은 뭔가요",
          "speaker_id": "2",
          "time": "20210908 07:25"
        },
        {
          "id": "MDRW2100045794.1.8",
          "form": "전 비비큐 황금올리브가 최애예요",
          "original_form": "전 비비큐 황금올리브가 최애예요",
          "speaker_id": "1",
          "time": "20210908 07:27"
        },
        {
          "id": "MDRW2100045794.1.9",
          "form": "그것도 맛있죠ㅠㅠㅠ",
          "original_form": "그것도 맛있죠ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100045794.1.10",
          "form": "돌고돌아 돌아가는 맛",
          "original_form": "돌고돌아 돌아가는 맛",
          "speaker_id": "2",
          "time": "20210908 07:28"
        },
        {
          "id": "MDRW2100045794.1.11",
          "form": "후라이드의 교과서 ㅋㅋㅋㅋㅋ",
          "original_form": "후라이드의 교과서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 07:30"
        },
        {
          "id": "MDRW2100045794.1.12",
          "form": "맞아요ㅋㅋㅋㅋ 교과서!",
          "original_form": "맞아요ㅋㅋㅋㅋ 교과서!",
          "speaker_id": "2",
          "time": "20210908 07:31"
        },
        {
          "id": "MDRW2100045794.1.13",
          "form": "아 후참잘 후라이드도 종종 먹어요",
          "original_form": "아 후참잘 후라이드도 종종 먹어요",
          "speaker_id": "1",
          "time": "20210908 07:33"
        },
        {
          "id": "MDRW2100045794.1.14",
          "form": "오 후참잘이 회사 이름이에요?",
          "original_form": "오 후참잘이 회사 이름이에요?",
          "speaker_id": "2",
          "time": "20210908 07:34"
        },
        {
          "id": "MDRW2100045794.1.15",
          "form": "후라이드참잘하는집 이에요",
          "original_form": "후라이드참잘하는집 이에요",
