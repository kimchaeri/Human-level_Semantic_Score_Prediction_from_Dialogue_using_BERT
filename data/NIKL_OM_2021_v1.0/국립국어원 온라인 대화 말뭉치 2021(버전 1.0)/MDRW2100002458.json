      "utterance": [
        {
          "id": "MDRW2100002458.1.1",
          "form": "나는 그 햄튜브를 먼저알앗는데",
          "original_form": "나는 그 햄튜브를 먼저알앗는데",
          "speaker_id": "2",
          "time": "20210820 12:29"
        },
        {
          "id": "MDRW2100002458.1.2",
          "form": "해쭈가 햄튜브 친구인데",
          "original_form": "해쭈가 햄튜브 친구인데",
          "speaker_id": "2",
          "time": "20210820 12:30"
        },
        {
          "id": "MDRW2100002458.1.3",
          "form": "유투브 한다그래가지고 알앗어",
          "original_form": "유투브 한다그래가지고 알앗어",
          "speaker_id": "2",
          "time": "20210820 12:30"
        },
        {
          "id": "MDRW2100002458.1.4",
          "form": "아 글쿠만",
          "original_form": "아 글쿠만",
          "speaker_id": "1",
          "time": "20210820 12:30"
        },
        {
          "id": "MDRW2100002458.1.5",
          "form": "햄튜브 친구엿구나",
          "original_form": "햄튜브 친구엿구나",
          "speaker_id": "1",
          "time": "20210820 12:30"
        },
        {
          "id": "MDRW2100002458.1.6",
          "form": "근데 해쭈 가족 몇명이야??",
          "original_form": "근데 해쭈 가족 몇명이야??",
          "speaker_id": "1",
          "time": "20210820 12:30"
        },
        {
          "id": "MDRW2100002458.1.7",
          "form": "4남매야",
          "original_form": "4남매야",
          "speaker_id": "2",
          "time": "20210820 12:30"
        },
        {
          "id": "MDRW2100002458.1.8",
          "form": "일케4명",
          "original_form": "일케4명",
          "speaker_id": "2",
          "time": "20210820 12:30"
        },
        {
          "id": "MDRW2100002458.1.9",
          "form": "오빠 해쭈 해수 해은",
          "original_form": "오빠 해쭈 해수 해은",
          "speaker_id": "2",
          "time": "20210820 12:30"
        },
        {
          "id": "MDRW2100002458.1.10",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002458.1.11",
          "form": "난 3자맨줄ㅋㅌㅌㅋ",
          "original_form": "난 3자맨줄ㅋㅌㅌㅋ",
          "speaker_id": "1",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002458.1.12",
          "form": "근데 카야 넘귀여워",
          "original_form": "근데 카야 넘귀여워",
          "speaker_id": "1",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002458.1.13",
          "form": "진짜 애기중에 젤기여움",
          "original_form": "진짜 애기중에 젤기여움",
          "speaker_id": "1",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002458.1.14",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "2",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002458.1.15",
          "form": "카야 사랑스러워",
          "original_form": "카야 사랑스러워",
          "speaker_id": "2",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002458.1.16",
          "form": "벌써 4살이야 애기가",
          "original_form": "벌써 4살이야 애기가",
          "speaker_id": "2",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002458.1.17",
          "form": "애기들은 진짜 빨리커",
          "original_form": "애기들은 진짜 빨리커",
          "speaker_id": "1",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002458.1.18",
          "form": "특히 남의 애기들은",
          "original_form": "특히 남의 애기들은",
          "speaker_id": "1",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002458.1.19",
          "form": "나도 호주가서 살구싶다...",
          "original_form": "나도 호주가서 살구싶다...",
          "speaker_id": "1",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002458.1.20",
          "form": "아그니까",
          "original_form": "아그니까",
          "speaker_id": "2",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002458.1.21",
          "form": "호즈 하늘더 진짜 맑고",
          "original_form": "호즈 하늘더 진짜 맑고",
          "speaker_id": "2",
          "time": "20210820 12:31"
        },
        {
          "id": "MDRW2100002458.1.22",
          "form": "자연 풍경이 너무조아",
          "original_form": "자연 풍경이 너무조아",
          "speaker_id": "2",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002458.1.23",
          "form": "풍경이 너무좋아",
          "original_form": "풍경이 너무좋아",
          "speaker_id": "1",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002458.1.24",
          "form": "해쭈 브이로그",
          "original_form": "해쭈 브이로그",
          "speaker_id": "1",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002458.1.25",
          "form": "볼때마다",
          "original_form": "볼때마다",
          "speaker_id": "1",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002458.1.26",
          "form": "해쯔 고딩때 이민갔자나",
          "original_form": "해쯔 고딩때 이민갔자나",
          "speaker_id": "2",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002458.1.27",
          "form": "근데 아직 한국음식 먹는게 신기해",
          "original_form": "근데 아직 한국음식 먹는게 신기해",
          "speaker_id": "2",
          "time": "20210820 12:32"
        },
        {
          "id": "MDRW2100002458.1.28",
          "form": "고런가??ㅋㅋㅋ",
          "original_form": "고런가??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002458.1.29",
          "form": "그래도 한국음식을 잊을수는 없으니",
          "original_form": "그래도 한국음식을 잊을수는 없으니",
          "speaker_id": "1",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002458.1.30",
          "form": "계속 먹는거아닐까",
          "original_form": "계속 먹는거아닐까",
          "speaker_id": "1",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002458.1.31",
          "form": "그건 인정",
          "original_form": "그건 인정",
          "speaker_id": "2",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002458.1.32",
          "form": "한국음식은 못잊어",
          "original_form": "한국음식은 못잊어",
          "speaker_id": "2",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002458.1.33",
          "form": "넘 맛있거든",
          "original_form": "넘 맛있거든",
          "speaker_id": "2",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002458.1.34",
          "form": "거기 한인 음식집도 되게많던데",
          "original_form": "거기 한인 음식집도 되게많던데",
          "speaker_id": "1",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002458.1.35",
          "form": "코로나 끝나면 해쭈가 가본 음식점 가보고싶어",
          "original_form": "코로나 끝나면 해쭈가 가본 음식점 가보고싶어",
          "speaker_id": "1",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002458.1.36",
          "form": "나도 호주갈랴~~~",
          "original_form": "나도 호주갈랴~~~",
          "speaker_id": "1",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002458.1.37",
          "form": "나도 해쭈따라서",
          "original_form": "나도 해쭈따라서",
          "speaker_id": "2",
          "time": "20210820 12:33"
        },
        {
          "id": "MDRW2100002458.1.38",
          "form": "호주여행갈래~~~",
          "original_form": "호주여행갈래~~~",
          "speaker_id": "2",
          "time": "20210820 12:34"
        }
      ]
    }
