      "utterance": [
        {
          "id": "MDRW2100005249.1.1",
          "form": "해외 여행 가보신곳 중에 기억남는곳 있으세요?",
          "original_form": "해외 여행 가보신곳 중에 기억남는곳 있으세요?",
          "speaker_id": "1",
          "time": "20210821 21:27"
        },
        {
          "id": "MDRW2100005249.1.2",
          "form": "저는 고생한기억이 오래가는거같아요",
          "original_form": "저는 고생한기억이 오래가는거같아요",
          "speaker_id": "2",
          "time": "20210821 21:28"
        },
        {
          "id": "MDRW2100005249.1.3",
          "form": "일본 도쿄여행이요 ㅎㅎ",
          "original_form": "일본 도쿄여행이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:28"
        },
        {
          "id": "MDRW2100005249.1.4",
          "form": "맞아요 저도그래요",
          "original_form": "맞아요 저도그래요",
          "speaker_id": "1",
          "time": "20210821 21:28"
        },
        {
          "id": "MDRW2100005249.1.5",
          "form": "캐리어 찾아 삼만리했거든요 ㅎㅎ",
          "original_form": "캐리어 찾아 삼만리했거든요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:28"
        },
        {
          "id": "MDRW2100005249.1.6",
          "form": "앗..! ㅠㅠㅠ어쩌다가",
          "original_form": "앗..! ㅠㅠㅠ어쩌다가",
          "speaker_id": "1",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005249.1.7",
          "form": "공항에서 도쿄시내로가는 기차를 탔는데 캐리어를 다른칸에 두고 자리에 앉았더니",
          "original_form": "공항에서 도쿄시내로가는 기차를 탔는데 캐리어를 다른칸에 두고 자리에 앉았더니",
          "speaker_id": "2",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005249.1.8",
          "form": "기차가 어느지점에서 분리되는거였더라구요",
          "original_form": "기차가 어느지점에서 분리되는거였더라구요",
          "speaker_id": "2",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005249.1.9",
          "form": "헉..! 그럴수가",
          "original_form": "헉..! 그럴수가",
          "speaker_id": "1",
          "time": "20210821 21:29"
        },
        {
          "id": "MDRW2100005249.1.10",
          "form": "ㅠ넘 놀라셨겠어요",
          "original_form": "ㅠ넘 놀라셨겠어요",
          "speaker_id": "1",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005249.1.11",
          "form": "캐리어는 다른 목적지로 가고 저는 도쿄시내로가고 ㅋ",
          "original_form": "캐리어는 다른 목적지로 가고 저는 도쿄시내로가고 ㅋ",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005249.1.12",
          "form": "진짜 넘황당했어요",
          "original_form": "진짜 넘황당했어요",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005249.1.13",
          "form": "이런 ㅠ금방 알게되셨어요? 찾느라 넘힘드셨겠어요",
          "original_form": "이런 ㅠ금방 알게되셨어요? 찾느라 넘힘드셨겠어요",
          "speaker_id": "1",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005249.1.14",
          "form": "내려서 캐리어 있는칸으로 가려는데 뒷줄이 없더라구요 ㅋㅋ",
          "original_form": "내려서 캐리어 있는칸으로 가려는데 뒷줄이 없더라구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:30"
        },
        {
          "id": "MDRW2100005249.1.15",
          "form": "목적지 도착해서 알았어요 ㅎ",
          "original_form": "목적지 도착해서 알았어요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005249.1.16",
          "form": "아..ㅠㅋ 끔찍해옄ㅋㅋ",
          "original_form": "아..ㅠㅋ 끔찍해옄ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005249.1.17",
          "form": "일본어도 할줄모르고 혼자고",
          "original_form": "일본어도 할줄모르고 혼자고",
          "speaker_id": "2",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005249.1.18",
          "form": "진짜 암담 ㅋㅋ 그자체였어요",
          "original_form": "진짜 암담 ㅋㅋ 그자체였어요",
          "speaker_id": "2",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005249.1.19",
          "form": "역무원한테 손짓발짓 영어반섞어가며 말하니",
          "original_form": "역무원한테 손짓발짓 영어반섞어가며 말하니",
          "speaker_id": "2",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005249.1.20",
          "form": "너의 가방은 어디로가고있다 이런말을하더라구요 ㅋㅋㅋ",
          "original_form": "너의 가방은 어디로가고있다 이런말을하더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005249.1.21",
          "form": "그러면서 도착해있을역에 전화해주고 나보고 어디서 어디로가라고 친절히 안내해줘서 결국 찾아서 다시 목적지로 왔죠",
          "original_form": "그러면서 도착해있을역에 전화해주고 나보고 어디서 어디로가라고 친절히 안내해줘서 결국 찾아서 다시 목적지로 왔죠",
          "speaker_id": "2",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005249.1.22",
          "form": "앗 ㅠㅠㅠ",
          "original_form": "앗 ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005249.1.23",
          "form": "덕분에 하루 다 지나갔어요 ㅎㅎ",
          "original_form": "덕분에 하루 다 지나갔어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005249.1.24",
          "form": "ㅋㅋㅋ 너의 가방은 어디로가고있다 ㅠ넘 무섭",
          "original_form": "ㅋㅋㅋ 너의 가방은 어디로가고있다 ㅠ넘 무섭",
          "speaker_id": "1",
          "time": "20210821 21:33"
        },
        {
          "id": "MDRW2100005249.1.25",
          "form": "진짜 일정 다 망치셨겠어요 ㅠㅠ",
          "original_form": "진짜 일정 다 망치셨겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005249.1.26",
          "form": "진짜 도쿄는 악몽이였어요",
          "original_form": "진짜 도쿄는 악몽이였어요",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005249.1.27",
          "form": "우산도 없었는데 비가오고",
          "original_form": "우산도 없었는데 비가오고",
          "speaker_id": "2",
          "time": "20210821 21:34"
        },
        {
          "id": "MDRW2100005249.1.28",
          "form": "이런 ㅠㅠ",
          "original_form": "이런 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 21:35"
        }
      ]
