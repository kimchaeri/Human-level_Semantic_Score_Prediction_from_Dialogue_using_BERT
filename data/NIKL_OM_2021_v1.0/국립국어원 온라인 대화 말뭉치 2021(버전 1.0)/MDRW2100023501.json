      "utterance": [
        {
          "id": "MDRW2100023501.1.1",
          "form": "일한지 얼마나됐어요?",
          "original_form": "일한지 얼마나됐어요?",
          "speaker_id": "1",
          "time": "20210830 10:01"
        },
        {
          "id": "MDRW2100023501.1.2",
          "form": "역시 월욜은 출근하면 집에 가고 싶어요",
          "original_form": "역시 월욜은 출근하면 집에 가고 싶어요",
          "speaker_id": "2",
          "time": "20210830 10:01"
        },
        {
          "id": "MDRW2100023501.1.3",
          "form": "오늘저도 일하는척만할거에요 ㅋㅋㅋㅋ",
          "original_form": "오늘저도 일하는척만할거에요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:05"
        },
        {
          "id": "MDRW2100023501.1.4",
          "form": "저 8년..ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 8년..ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:33"
        },
        {
          "id": "MDRW2100023501.1.5",
          "form": "얼마나 되셨어요?",
          "original_form": "얼마나 되셨어요?",
          "speaker_id": "2",
          "time": "20210830 11:33"
        },
        {
          "id": "MDRW2100023501.1.6",
          "form": "일하는 척만 하고 싶었는데 일이 자꾸 들어와요",
          "original_form": "일하는 척만 하고 싶었는데 일이 자꾸 들어와요",
          "speaker_id": "2",
          "time": "20210830 11:35"
        },
        {
          "id": "MDRW2100023501.1.7",
          "form": "5년 차 입미다요",
          "original_form": "5년 차 입미다요",
          "speaker_id": "1",
          "time": "20210830 12:15"
        },
        {
          "id": "MDRW2100023501.1.8",
          "form": "나이가잇으시니 그정도될거라 예상은햇어요 ㅎ",
          "original_form": "나이가잇으시니 그정도될거라 예상은햇어요 ㅎ",
          "speaker_id": "1",
          "time": "20210830 12:15"
        },
        {
          "id": "MDRW2100023501.1.9",
          "form": "드뎌 점심시간~!",
          "original_form": "드뎌 점심시간~!",
          "speaker_id": "1",
          "time": "20210830 12:15"
        },
        {
          "id": "MDRW2100023501.1.10",
          "form": "와 오래되셨네요",
          "original_form": "와 오래되셨네요",
          "speaker_id": "2",
          "time": "20210830 12:16"
        },
        {
          "id": "MDRW2100023501.1.11",
          "form": "여기는 제육 된찌 나왔습니다",
          "original_form": "여기는 제육 된찌 나왔습니다",
          "speaker_id": "2",
          "time": "20210830 12:16"
        },
        {
          "id": "MDRW2100023501.1.12",
          "form": "5년차에 엉덩이",
          "original_form": "5년차에 엉덩이",
          "speaker_id": "2",
          "time": "20210830 12:17"
        },
        {
          "id": "MDRW2100023501.1.13",
          "form": "들썩일텐데",
          "original_form": "들썩일텐데",
          "speaker_id": "2",
          "time": "20210830 12:17"
        },
        {
          "id": "MDRW2100023501.1.14",
          "form": "3 5 7로 오잖아요 퇴사 or 이직욕구",
          "original_form": "3 5 7로 오잖아요 퇴사 or 이직욕구",
          "speaker_id": "2",
          "time": "20210830 12:17"
        },
        {
          "id": "MDRW2100023501.1.15",
          "form": "지금들썩입니다 후",
          "original_form": "지금들썩입니다 후",
          "speaker_id": "1",
          "time": "20210830 12:18"
        },
        {
          "id": "MDRW2100023501.1.16",
          "form": "저는 불닭이랑 참마먹어요 ㅎ",
          "original_form": "저는 불닭이랑 참마먹어요 ㅎ",
          "speaker_id": "1",
          "time": "20210830 12:18"
        },
        {
          "id": "MDRW2100023501.1.17",
          "form": "맵다매워",
          "original_form": "맵다매워",
          "speaker_id": "1",
          "time": "20210830 12:18"
        },
        {
          "id": "MDRW2100023501.1.18",
          "form": "불닭 살면서 세번? 먹어봤어요",
          "original_form": "불닭 살면서 세번? 먹어봤어요",
          "speaker_id": "2",
          "time": "20210830 12:19"
        },
        {
          "id": "MDRW2100023501.1.19",
          "form": "대체 왜 먹나요?",
          "original_form": "대체 왜 먹나요?",
          "speaker_id": "2",
          "time": "20210830 12:19"
        },
        {
          "id": "MDRW2100023501.1.20",
          "form": "그쪽 계열은 이직하기 쉬워요?",
          "original_form": "그쪽 계열은 이직하기 쉬워요?",
          "speaker_id": "2",
          "time": "20210830 12:20"
        },
        {
          "id": "MDRW2100023501.1.21",
          "form": "음 아무래도 스트레스가많다보니 자리는금방생겨요",
          "original_form": "음 아무래도 스트레스가많다보니 자리는금방생겨요",
          "speaker_id": "1",
          "time": "20210830 12:20"
        },
        {
          "id": "MDRW2100023501.1.22",
          "form": "대체왜먹냐뇨 ㅋㅋㅋㅋㅋㅋ맛잇으니먹죠ㅠ",
          "original_form": "대체왜먹냐뇨 ㅋㅋㅋㅋㅋㅋ맛잇으니먹죠ㅠ",
          "speaker_id": "1",
          "time": "20210830 12:21"
        },
        {
          "id": "MDRW2100023501.1.23",
          "form": "저게 왜 맛있지",
          "original_form": "저게 왜 맛있지",
          "speaker_id": "2",
          "time": "20210830 12:23"
        },
        {
          "id": "MDRW2100023501.1.24",
          "form": "지금 제가먹고잇는데",
          "original_form": "지금 제가먹고잇는데",
          "speaker_id": "1",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100023501.1.25",
          "form": "왜맛잇냐뮬으면",
          "original_form": "왜맛잇냐뮬으면",
          "speaker_id": "1",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100023501.1.26",
          "form": "매운게 맛있으면",
          "original_form": "매운게 맛있으면",
          "speaker_id": "2",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100023501.1.27",
          "form": "아씨",
          "original_form": "아씨",
          "speaker_id": "2",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100023501.1.28",
          "form": "변Eㅐ라는 말",
          "original_form": "변Eㅐ라는 말",
          "speaker_id": "2",
          "time": "20210830 12:25"
        },
        {
          "id": "MDRW2100023501.1.29",
          "form": "매운거좋아하는사람들한테",
          "original_form": "매운거좋아하는사람들한테",
          "speaker_id": "1",
          "time": "20210830 12:25"
        },
        {
          "id": "MDRW2100023501.1.30",
          "form": "들어보셨죠??",
          "original_form": "들어보셨죠??",
          "speaker_id": "2",
          "time": "20210830 12:25"
        },
        {
          "id": "MDRW2100023501.1.31",
          "form": "뺨맞아요",
          "original_form": "뺨맞아요",
          "speaker_id": "1",
          "time": "20210830 12:25"
        },
        {
          "id": "MDRW2100023501.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 12:25"
        },
        {
          "id": "MDRW2100023501.1.33",
          "form": "엥?",
          "original_form": "엥?",
          "speaker_id": "1",
          "time": "20210830 12:25"
        },
        {
          "id": "MDRW2100023501.1.34",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:25"
        },
        {
          "id": "MDRW2100023501.1.35",
          "form": "왕전처음듣는데",
          "original_form": "왕전처음듣는데",
          "speaker_id": "1",
          "time": "20210830 12:25"
        },
        {
          "id": "MDRW2100023501.1.36",
          "form": "매운건 통각이라 변Eㅐ끼 있다던 썰",
          "original_form": "매운건 통각이라 변Eㅐ끼 있다던 썰",
          "speaker_id": "2",
          "time": "20210830 12:25"
        },
        {
          "id": "MDRW2100023501.1.37",
          "form": "아픈거 조아하는",
          "original_form": "아픈거 조아하는",
          "speaker_id": "2",
          "time": "20210830 12:25"
        },
        {
          "id": "MDRW2100023501.1.38",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 12:26"
        },
        {
          "id": "MDRW2100023501.1.39",
          "form": "어디서 그런 ㅡㅡ",
          "original_form": "어디서 그런 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210830 12:26"
        },
        {
          "id": "MDRW2100023501.1.40",
          "form": "이상한말을",
          "original_form": "이상한말을",
          "speaker_id": "1",
          "time": "20210830 12:26"
        },
        {
          "id": "MDRW2100023501.1.41",
          "form": "아 맵다",
          "original_form": "아 맵다",
          "speaker_id": "1",
          "time": "20210830 12:26"
        },
        {
          "id": "MDRW2100023501.1.42",
          "form": "후식 아이스크림",
          "original_form": "후식 아이스크림",
          "speaker_id": "1",
          "time": "20210830 12:26"
        },
        {
          "id": "MDRW2100023501.1.43",
          "form": "이상하다뇨 과학적이죠",
          "original_form": "이상하다뇨 과학적이죠",
          "speaker_id": "2",
          "time": "20210830 12:27"
        },
        {
          "id": "MDRW2100023501.1.44",
          "form": "아스크림 뭐에요?",
          "original_form": "아스크림 뭐에요?",
          "speaker_id": "2",
          "time": "20210830 12:27"
        },
        {
          "id": "MDRW2100023501.1.45",
          "form": "아니 근데 편의점이세요?",
          "original_form": "아니 근데 편의점이세요?",
          "speaker_id": "2",
          "time": "20210830 12:28"
        },
        {
          "id": "MDRW2100023501.1.46",
          "form": "컵라면에 삼김에 아스크림",
          "original_form": "컵라면에 삼김에 아스크림",
          "speaker_id": "2",
          "time": "20210830 12:28"
        },
        {
          "id": "MDRW2100023501.1.47",
          "form": "편의점 국룰조합인데",
          "original_form": "편의점 국룰조합인데",
          "speaker_id": "2",
          "time": "20210830 12:28"
        },
        {
          "id": "MDRW2100023501.1.48",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023501.1.49",
          "form": "사왓어요 출근길에",
          "original_form": "사왓어요 출근길에",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023501.1.50",
          "form": "오늘부터 사먹어야대가지구",
          "original_form": "오늘부터 사먹어야대가지구",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023501.1.51",
          "form": "부라보머것금다",
          "original_form": "부라보머것금다",
          "speaker_id": "1",
          "time": "20210830 13:00"
        },
        {
          "id": "MDRW2100023501.1.52",
          "form": "매운거 잘드시는구나 점심에 불닭이라니",
          "original_form": "매운거 잘드시는구나 점심에 불닭이라니",
          "speaker_id": "2",
          "time": "20210830 13:50"
        },
        {
          "id": "MDRW2100023501.1.53",
          "form": "제가 점심에 먹으면 일 못할듯요",
          "original_form": "제가 점심에 먹으면 일 못할듯요",
          "speaker_id": "2",
          "time": "20210830 13:51"
        },
        {
          "id": "MDRW2100023501.1.54",
          "form": "화장실가느라",
          "original_form": "화장실가느라",
          "speaker_id": "2",
          "time": "20210830 13:51"
        },
        {
          "id": "MDRW2100023501.1.55",
          "form": "맵찔이신가봐요",
          "original_form": "맵찔이신가봐요",
          "speaker_id": "1",
          "time": "20210830 13:51"
        }
      ]
    }
  ]
