      "utterance": [
        {
          "id": "MDRW2100037912.1.1",
          "form": "배달...",
          "original_form": "배달...",
          "speaker_id": "1",
          "time": "20210901 20:52"
        },
        {
          "id": "MDRW2100037912.1.2",
          "form": "맞아요ㅠㅜ 저 맛있어서 또 시켜먹은 음료 있어요",
          "original_form": "맞아요ㅠㅜ 저 맛있어서 또 시켜먹은 음료 있어요",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100037912.1.3",
          "form": "근데 사라진거예요!!",
          "original_form": "근데 사라진거예요!!",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100037912.1.4",
          "form": "어찌나 눈물이 차오르던지",
          "original_form": "어찌나 눈물이 차오르던지",
          "speaker_id": "2",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100037912.1.5",
          "form": "아아ㅠㅠ",
          "original_form": "아아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 23:42"
        },
        {
          "id": "MDRW2100037912.1.6",
          "form": "저는 정말 마시던 거 사라지면 메뉴 고르는 데만 한참 걸릴 거예요",
          "original_form": "저는 정말 마시던 거 사라지면 메뉴 고르는 데만 한참 걸릴 거예요",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100037912.1.7",
          "form": "음로는 멘날 기본으로 마셔서 사라질 일은 거의 없지만요ㅋㅋㅋㅋ",
          "original_form": "음로는 멘날 기본으로 마셔서 사라질 일은 거의 없지만요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:43"
        },
        {
          "id": "MDRW2100037912.1.8",
          "form": "오 뭐 주로 마시세요? 아메리카노?",
          "original_form": "오 뭐 주로 마시세요? 아메리카노?",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100037912.1.9",
          "form": "저는 거의 커피류 말고 뭐 잔뜩 들어간 신메뉴 먹는거 같아요!",
          "original_form": "저는 거의 커피류 말고 뭐 잔뜩 들어간 신메뉴 먹는거 같아요!",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100037912.1.10",
          "form": "아메리카노랑 얼그레이 많이 마셔요",
          "original_form": "아메리카노랑 얼그레이 많이 마셔요",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100037912.1.11",
          "form": "저 졸려가지고 일어나서 답장할게요!! 헐 얼그레이 저도 좋아하는뎅",
          "original_form": "저 졸려가지고 일어나서 답장할게요!! 헐 얼그레이 저도 좋아하는뎅",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100037912.1.12",
          "form": "내일 봬요~!",
          "original_form": "내일 봬요~!",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100037912.1.13",
          "form": "넹~ 오늘은 물 한잔 마시고 시작했어요!",
          "original_form": "넹~ 오늘은 물 한잔 마시고 시작했어요!",
          "speaker_id": "2",
          "time": "20210902 09:54"
        },
        {
          "id": "MDRW2100037912.1.14",
          "form": "일어나셨어요?",
          "original_form": "일어나셨어요?",
          "speaker_id": "2",
          "time": "20210902 09:55"
        },
        {
          "id": "MDRW2100037912.1.15",
          "form": "네네~",
          "original_form": "네네~",
          "speaker_id": "1",
          "time": "20210902 09:58"
        },
        {
          "id": "MDRW2100037912.1.16",
          "form": "저도 보리차 한 잔 마셨어요",
          "original_form": "저도 보리차 한 잔 마셨어요",
          "speaker_id": "1",
          "time": "20210902 09:59"
        },
        {
          "id": "MDRW2100037912.1.17",
          "form": "지금은 토스트 먹고 있어요 ㅋㅋ 에그박스거!",
          "original_form": "지금은 토스트 먹고 있어요 ㅋㅋ 에그박스거!",
          "speaker_id": "2",
          "time": "20210902 10:06"
        },
        {
          "id": "MDRW2100037912.1.18",
          "form": "와 맛있겠어요!! 저는 조금 이따 야채 볶아 먹으려고요",
          "original_form": "와 맛있겠어요!! 저는 조금 이따 야채 볶아 먹으려고요",
          "speaker_id": "1",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100037912.1.19",
          "form": "우와 야채볶음 좋죠!",
          "original_form": "우와 야채볶음 좋죠!",
          "speaker_id": "2",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100037912.1.20",
          "form": "건강에도 좋구",
          "original_form": "건강에도 좋구",
          "speaker_id": "2",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100037912.1.21",
          "form": "놀고 있는 버섯이랑 같이 볶으려고요",
          "original_form": "놀고 있는 버섯이랑 같이 볶으려고요",
          "speaker_id": "1",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100037912.1.22",
          "form": "헐 저도 어제 버섯 볶아먹었는데 ㅋㅋㅋㅋ 싱기",
          "original_form": "헐 저도 어제 버섯 볶아먹었는데 ㅋㅋㅋㅋ 싱기",
          "speaker_id": "2",
          "time": "20210902 10:26"
        }
