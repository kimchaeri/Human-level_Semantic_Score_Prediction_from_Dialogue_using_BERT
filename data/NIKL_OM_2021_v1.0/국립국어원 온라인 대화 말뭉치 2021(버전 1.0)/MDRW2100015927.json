      "utterance": [
        {
          "id": "MDRW2100015927.1.1",
          "form": "흰머리좀 뽑아줘",
          "original_form": "흰머리좀 뽑아줘",
          "speaker_id": "2",
          "time": "20210826 03:04"
        },
        {
          "id": "MDRW2100015927.1.2",
          "form": "벌써 났어?",
          "original_form": "벌써 났어?",
          "speaker_id": "1",
          "time": "20210826 03:04"
        },
        {
          "id": "MDRW2100015927.1.3",
          "form": "그럼",
          "original_form": "그럼",
          "speaker_id": "2",
          "time": "20210826 03:04"
        },
        {
          "id": "MDRW2100015927.1.4",
          "form": "금방나지",
          "original_form": "금방나지",
          "speaker_id": "2",
          "time": "20210826 03:04"
        },
        {
          "id": "MDRW2100015927.1.5",
          "form": "얼마전에 잘라줬잖아",
          "original_form": "얼마전에 잘라줬잖아",
          "speaker_id": "1",
          "time": "20210826 03:04"
        },
        {
          "id": "MDRW2100015927.1.6",
          "form": "벌써 몇개월됐어",
          "original_form": "벌써 몇개월됐어",
          "speaker_id": "2",
          "time": "20210826 03:04"
        },
        {
          "id": "MDRW2100015927.1.7",
          "form": "그래?",
          "original_form": "그래?",
          "speaker_id": "1",
          "time": "20210826 03:04"
        },
        {
          "id": "MDRW2100015927.1.8",
          "form": "염색은 왜 안해?",
          "original_form": "염색은 왜 안해?",
          "speaker_id": "1",
          "time": "20210826 03:05"
        },
        {
          "id": "MDRW2100015927.1.9",
          "form": "염색 많이 하면 안좋아",
          "original_form": "염색 많이 하면 안좋아",
          "speaker_id": "2",
          "time": "20210826 03:05"
        },
        {
          "id": "MDRW2100015927.1.10",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "1",
          "time": "20210826 03:05"
        },
        {
          "id": "MDRW2100015927.1.11",
          "form": "머리 상하잖아",
          "original_form": "머리 상하잖아",
          "speaker_id": "2",
          "time": "20210826 03:05"
        },
        {
          "id": "MDRW2100015927.1.12",
          "form": "그럼 흰머리 뽑는건?",
          "original_form": "그럼 흰머리 뽑는건?",
          "speaker_id": "1",
          "time": "20210826 03:05"
        },
        {
          "id": "MDRW2100015927.1.13",
          "form": "뽑는것도 안좋아",
          "original_form": "뽑는것도 안좋아",
          "speaker_id": "2",
          "time": "20210826 03:05"
        },
        {
          "id": "MDRW2100015927.1.14",
          "form": "그럼 잘라야해?",
          "original_form": "그럼 잘라야해?",
          "speaker_id": "1",
          "time": "20210826 03:06"
        },
        {
          "id": "MDRW2100015927.1.15",
          "form": "짧게 자르는게 제일 좋아",
          "original_form": "짧게 자르는게 제일 좋아",
          "speaker_id": "2",
          "time": "20210826 03:06"
        },
        {
          "id": "MDRW2100015927.1.16",
          "form": "엄마 머리가 너무 풍성해서 한가닥 잡아서 짧게 자르는게 어려워",
          "original_form": "엄마 머리가 너무 풍성해서 한가닥 잡아서 짧게 자르는게 어려워",
          "speaker_id": "1",
          "time": "20210826 03:06"
        },
        {
          "id": "MDRW2100015927.1.17",
          "form": "많이 하면 늘어",
          "original_form": "많이 하면 늘어",
          "speaker_id": "2",
          "time": "20210826 03:06"
        },
        {
          "id": "MDRW2100015927.1.18",
          "form": "흰머리 많아?",
          "original_form": "흰머리 많아?",
          "speaker_id": "1",
          "time": "20210826 03:06"
        },
        {
          "id": "MDRW2100015927.1.19",
          "form": "여러가닥 났지",
          "original_form": "여러가닥 났지",
          "speaker_id": "2",
          "time": "20210826 03:07"
        },
        {
          "id": "MDRW2100015927.1.20",
          "form": "미용 가위로 잘 잘라봐",
          "original_form": "미용 가위로 잘 잘라봐",
          "speaker_id": "2",
          "time": "20210826 03:07"
        },
        {
          "id": "MDRW2100015927.1.21",
          "form": "우리집에 미용가위있어?",
          "original_form": "우리집에 미용가위있어?",
          "speaker_id": "1",
          "time": "20210826 03:07"
        },
        {
          "id": "MDRW2100015927.1.22",
          "form": "얼마전에 샀어",
          "original_form": "얼마전에 샀어",
          "speaker_id": "2",
          "time": "20210826 03:07"
        },
        {
          "id": "MDRW2100015927.1.23",
          "form": "왜??",
          "original_form": "왜??",
          "speaker_id": "1",
          "time": "20210826 03:07"
        },
        {
          "id": "MDRW2100015927.1.24",
          "form": "그냥 가위는?",
          "original_form": "그냥 가위는?",
          "speaker_id": "1",
          "time": "20210826 03:07"
        },
        {
          "id": "MDRW2100015927.1.25",
          "form": "그냥 가위보다 미용가위로 자르는게 너도 편할거야",
          "original_form": "그냥 가위보다 미용가위로 자르는게 너도 편할거야",
          "speaker_id": "2",
          "time": "20210826 03:07"
        },
        {
          "id": "MDRW2100015927.1.26",
          "form": "그래? 그럼 그래야지",
          "original_form": "그래? 그럼 그래야지",
          "speaker_id": "1",
          "time": "20210826 03:08"
        },
        {
          "id": "MDRW2100015927.1.27",
          "form": "그래 빨리 잘라줘",
          "original_form": "그래 빨리 잘라줘",
          "speaker_id": "2",
          "time": "20210826 03:08"
        },
        {
          "id": "MDRW2100015927.1.28",
          "form": "알겠어",
          "original_form": "알겠어",
          "speaker_id": "1",
          "time": "20210826 03:08"
        }
      ]
    }
