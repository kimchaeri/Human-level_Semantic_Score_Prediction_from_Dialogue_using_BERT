        {
          "id": "MDRW2100025355.1.2",
          "form": "여행좋아해요?",
          "original_form": "여행좋아해요?",
          "speaker_id": "1",
          "time": "20210901 10:18"
        },
        {
          "id": "MDRW2100025355.1.3",
          "form": "저도 좋죠",
          "original_form": "저도 좋죠",
          "speaker_id": "1",
          "time": "20210901 10:18"
        },
        {
          "id": "MDRW2100025355.1.4",
          "form": "제주가야되는데 여건이 안되네요ㅎ",
          "original_form": "제주가야되는데 여건이 안되네요ㅎ",
          "speaker_id": "2",
          "time": "20210901 10:35"
        },
        {
          "id": "MDRW2100025355.1.5",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210901 11:47"
        },
        {
          "id": "MDRW2100025355.1.6",
          "form": "제주에 가보셨나요",
