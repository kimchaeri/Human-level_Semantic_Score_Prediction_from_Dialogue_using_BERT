      "utterance": [
        {
          "id": "MDRW2100004544.1.1",
          "form": "그럼 드림독에 대해 이야기해보죠",
          "original_form": "그럼 드림독에 대해 이야기해보죠",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004544.1.2",
          "form": "당신의 드림독",
          "original_form": "당신의 드림독",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004544.1.3",
          "form": "저도 개를 키우고 싶을 때가 있어요",
          "original_form": "저도 개를 키우고 싶을 때가 있어요",
          "speaker_id": "2",
          "time": "20210821 17:06"
        },
        {
          "id": "MDRW2100004544.1.4",
          "form": "산책하는 강강쥐들을 봤을 때이죠",
          "original_form": "산책하는 강강쥐들을 봤을 때이죠",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004544.1.5",
          "form": "저의 드림독은 털길고 큰걸 키우고싶어요",
          "original_form": "저의 드림독은 털길고 큰걸 키우고싶어요",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004544.1.6",
          "form": "마자요  저도 거기에 끼고싶어요",
          "original_form": "마자요  저도 거기에 끼고싶어요",
          "speaker_id": "1",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004544.1.7",
          "form": "ㅋㅋㅋ맞아요 털 길고 큰 거?",
          "original_form": "ㅋㅋㅋ맞아요 털 길고 큰 거?",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004544.1.8",
          "form": "색은요?",
          "original_form": "색은요?",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004544.1.9",
          "form": "생각해둔 종이 있나요?",
          "original_form": "생각해둔 종이 있나요?",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004544.1.10",
          "form": "저는 리트리버나... 삼색이용",
          "original_form": "저는 리트리버나... 삼색이용",
          "speaker_id": "2",
          "time": "20210821 17:07"
        },
        {
          "id": "MDRW2100004544.1.11",
          "form": "저는 세틀랜드쉽독도 좋고 유기견센터에서 데리고올까싶네요",
          "original_form": "저는 세틀랜드쉽독도 좋고 유기견센터에서 데리고올까싶네요",
          "speaker_id": "1",
          "time": "20210821 17:08"
        },
        {
          "id": "MDRW2100004544.1.12",
          "form": "그냥 절 좀 좋아한다는걸 표현해주는 애기면좋겠어요",
          "original_form": "그냥 절 좀 좋아한다는걸 표현해주는 애기면좋겠어요",
          "speaker_id": "1",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004544.1.13",
          "form": "하아 고양이누 그게 좀 부족하긴 하죠",
          "original_form": "하아 고양이누 그게 좀 부족하긴 하죠",
          "speaker_id": "2",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004544.1.14",
          "form": "새침해서",
          "original_form": "새침해서",
          "speaker_id": "2",
          "time": "20210821 17:09"
        },
        {
          "id": "MDRW2100004544.1.15",
          "form": "마자요! 날  좋아하는거? 제가 집에있는걸 허락했다는건알겠는데 표현좀해줬으면좋겠어요!",
          "original_form": "마자요! 날  좋아하는거? 제가 집에있는걸 허락했다는건알겠는데 표현좀해줬으면좋겠어요!",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004544.1.16",
          "form": "당신은 애정이 필요하군요",
          "original_form": "당신은 애정이 필요하군요",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004544.1.17",
          "form": "애정표현이",
          "original_form": "애정표현이",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004544.1.18",
          "form": "정말요",
          "original_form": "정말요",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004544.1.19",
          "form": "강강쥐들은 그런면에서 최고의 프렌드이죠",
          "original_form": "강강쥐들은 그런면에서 최고의 프렌드이죠",
          "speaker_id": "2",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004544.1.20",
          "form": "꼬리로 다 드러남",
          "original_form": "꼬리로 다 드러남",
          "speaker_id": "2",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004544.1.21",
          "form": "마자여 강쥐들은 침대에거 걑이 자주기도햐던데",
          "original_form": "마자여 강쥐들은 침대에거 걑이 자주기도햐던데",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004544.1.22",
          "form": "우리집고양이는 그런게없어요",
          "original_form": "우리집고양이는 그런게없어요",
          "speaker_id": "1",
          "time": "20210821 17:11"
        },
        {
          "id": "MDRW2100004544.1.23",
          "form": "나중이 가끔 옆구리에 껴서 자줬었는데",
          "original_form": "나중이 가끔 옆구리에 껴서 자줬었는데",
          "speaker_id": "2",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004544.1.24",
          "form": "요즘엔 그런것도 없어요",
          "original_form": "요즘엔 그런것도 없어요",
          "speaker_id": "2",
          "time": "20210821 17:12"
        },
        {
          "id": "MDRW2100004544.1.25",
          "form": "독립심이강해졌군요",
          "original_form": "독립심이강해졌군요",
          "speaker_id": "1",
          "time": "20210821 17:14"
        },
        {
          "id": "MDRW2100004544.1.26",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004544.1.27",
          "form": "싫어하는 표현이 강해졌어요",
          "original_form": "싫어하는 표현이 강해졌어요",
          "speaker_id": "2",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004544.1.28",
          "form": "단호해졌군요",
          "original_form": "단호해졌군요",
          "speaker_id": "1",
          "time": "20210821 17:23"
        },
        {
          "id": "MDRW2100004544.1.29",
          "form": "넹.....,",
          "original_form": "넹.....,",
          "speaker_id": "2",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004544.1.30",
          "form": "name3 똥간은 뭐써요",
          "original_form": "&name3& 똥간은 뭐써요",
          "speaker_id": "2",
          "time": "20210821 17:24"
        },
        {
          "id": "MDRW2100004544.1.31",
          "form": "똥간은 그때 지마켓에서 산 XL쓰고 모래는... 뭐쓰는지 까먹었는데 집에가서보내드릴께요",
          "original_form": "똥간은 그때 지마켓에서 산 XL쓰고 모래는... 뭐쓰는지 까먹었는데 집에가서보내드릴께요",
          "speaker_id": "1",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004544.1.32",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 17:41"
        },
        {
          "id": "MDRW2100004544.1.33",
          "form": "name3 알러지없나요",
          "original_form": "&name3& 알러지없나요",
          "speaker_id": "2",
          "time": "20210821 17:41"
        },
        {
          "id": "MDRW2100004544.1.34",
          "form": "네 딱히 없는거같아요!  지마음에드는거면 다 잘먹어요...",
          "original_form": "네 딱히 없는거같아요!  지마음에드는거면 다 잘먹어요...",
          "speaker_id": "1",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004544.1.35",
          "form": "name4이와 name5이는 알러지있나요",
          "original_form": "&name4&이와 &name5&이는 알러지있나요",
          "speaker_id": "1",
          "time": "20210821 17:49"
        },
        {
          "id": "MDRW2100004544.1.36",
          "form": "음 제가 고양이 알러지가 있죠",
          "original_form": "음 제가 고양이 알러지가 있죠",
          "speaker_id": "2",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004544.1.37",
          "form": "고양이들은 이제까지 딱히 모르겠어요",
          "original_form": "고양이들은 이제까지 딱히 모르겠어요",
          "speaker_id": "2",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004544.1.38",
          "form": "편식이 심함",
          "original_form": "편식이 심함",
          "speaker_id": "2",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004544.1.39",
          "form": "알러지가있는데 키우는건가요??",
          "original_form": "알러지가있는데 키우는건가요??",
          "speaker_id": "1",
          "time": "20210821 18:08"
        },
        {
          "id": "MDRW2100004544.1.40",
          "form": "참사랑이네요",
          "original_form": "참사랑이네요",
          "speaker_id": "1",
          "time": "20210821 18:08"
        },
        {
          "id": "MDRW2100004544.1.41",
          "form": "마일드해서요",
          "original_form": "마일드해서요",
          "speaker_id": "2",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004544.1.42",
          "form": "재채기콧물 살짝간지러움",
          "original_form": "재채기콧물 살짝간지러움",
          "speaker_id": "2",
          "time": "20210821 18:11"
        },
        {
          "id": "MDRW2100004544.1.43",
          "form": "그살짝간지러움이 얼마나큰건데요!",
          "original_form": "그살짝간지러움이 얼마나큰건데요!",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004544.1.44",
          "form": "저 고양이를위해 쇼핑갔다가 귀리사왔습니다",
          "original_form": "저 고양이를위해 쇼핑갔다가 귀리사왔습니다",
          "speaker_id": "1",
          "time": "20210821 18:17"
        },
        {
          "id": "MDRW2100004544.1.45",
          "form": "귀리 키워서 먹이게요?",
          "original_form": "귀리 키워서 먹이게요?",
          "speaker_id": "2",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004544.1.46",
          "form": "넹 이게 고양이에게좋대여",
          "original_form": "넹 이게 고양이에게좋대여",
          "speaker_id": "1",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004544.1.47",
          "form": "저도 키워서 먹여봣는데 다 자라기도전에 다먹어요",
          "original_form": "저도 키워서 먹여봣는데 다 자라기도전에 다먹어요",
          "speaker_id": "2",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100004544.1.48",
          "form": "그쵸? 식이섬유 좀 먹이려고 샀읍니다",
          "original_form": "그쵸? 식이섬유 좀 먹이려고 샀읍니다",
          "speaker_id": "1",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004544.1.49",
          "form": "쟨 너무안먹더라구여",
          "original_form": "쟨 너무안먹더라구여",
          "speaker_id": "1",
          "time": "20210821 18:30"
        },
        {
          "id": "MDRW2100004544.1.50",
          "form": "식이섬유ㅋㅋㅋㅋㅋ",
          "original_form": "식이섬유ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 18:58"
        },
        {
          "id": "MDRW2100004544.1.51",
          "form": "캣닢 좋아해요 평소에?",
          "original_form": "캣닢 좋아해요 평소에?",
          "speaker_id": "2",
          "time": "20210821 18:58"
        },
        {
          "id": "MDRW2100004544.1.52",
          "form": "가루줬을땐 그릉그릉하고 부버적거렸어요",
          "original_form": "가루줬을땐 그릉그릉하고 부버적거렸어요",
          "speaker_id": "1",
          "time": "20210821 19:03"
        },
        {
          "id": "MDRW2100004544.1.53",
          "form": "격해지지는 않네요 저희집애들은 너무 좋아서 사나웢ㅁ",
          "original_form": "격해지지는 않네요 저희집애들은 너무 좋아서 사나웢ㅁ",
          "speaker_id": "2",
          "time": "20210821 19:34"
        },
        {
          "id": "MDRW2100004544.1.54",
          "form": "세상에 리액션최고네요",
          "original_form": "세상에 리액션최고네요",
          "speaker_id": "1",
          "time": "20210821 19:38"
        },
        {
          "id": "MDRW2100004544.1.55",
          "form": "그래서 또 둘이 싸워요 갑자기 흥분해서",
          "original_form": "그래서 또 둘이 싸워요 갑자기 흥분해서",
          "speaker_id": "2",
          "time": "20210821 19:42"
        },
        {
          "id": "MDRW2100004544.1.56",
          "form": "세상에...정말손이많이가는군여",
          "original_form": "세상에...정말손이많이가는군여",
