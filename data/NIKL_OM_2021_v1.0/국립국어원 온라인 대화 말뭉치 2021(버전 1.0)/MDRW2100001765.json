      "utterance": [
        {
          "id": "MDRW2100001765.1.1",
          "form": "오늘 롯데한화 야구한거 봤어?",
          "original_form": "오늘 롯데한화 야구한거 봤어?",
          "speaker_id": "2",
          "time": "20210903 22:40"
        },
        {
          "id": "MDRW2100001765.1.2",
          "form": "결과 어떻게 됐어?? 롯데가 이겼나?",
          "original_form": "결과 어떻게 됐어?? 롯데가 이겼나?",
          "speaker_id": "1",
          "time": "20210903 22:52"
        },
        {
          "id": "MDRW2100001765.1.3",
          "form": "오늘 더블헤더라고 두게임했는데",
          "original_form": "오늘 더블헤더라고 두게임했는데",
          "speaker_id": "2",
          "time": "20210904 00:41"
        },
        {
          "id": "MDRW2100001765.1.4",
          "form": "두게임 다 이겼어",
          "original_form": "두게임 다 이겼어",
          "speaker_id": "2",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100001765.1.5",
          "form": "멋지네! 롯데 요즘 전력 좋은것 같아",
          "original_form": "멋지네! 롯데 요즘 전력 좋은것 같아",
          "speaker_id": "1",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100001765.1.6",
          "form": "계속 잘 가라~",
          "original_form": "계속 잘 가라~",
          "speaker_id": "2",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100001765.1.7",
          "form": "사실 요새는 게임 자체보다도ㅋㅋ그냥 출첵하러 들어간 김에 게임 구경도 하고 그러는거 같아ㅋㅋㅋ",
          "original_form": "사실 요새는 게임 자체보다도ㅋㅋ그냥 출첵하러 들어간 김에 게임 구경도 하고 그러는거 같아ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100001765.1.8",
          "form": "이번 시즌 우승도 노려볼 수 있을려나",
          "original_form": "이번 시즌 우승도 노려볼 수 있을려나",
          "speaker_id": "1",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100001765.1.9",
          "form": "엔씨 요즘 못 해서 아쉬움",
          "original_form": "엔씨 요즘 못 해서 아쉬움",
          "speaker_id": "1",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100001765.1.10",
          "form": "글쎄ㅋㅋㅋ가능할까...?",
          "original_form": "글쎄ㅋㅋㅋ가능할까...?",
          "speaker_id": "2",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100001765.1.11",
          "form": "아 넌 엔씨 팬이야?",
          "original_form": "아 넌 엔씨 팬이야?",
          "speaker_id": "2",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100001765.1.12",
          "form": "아ㅣ",
          "original_form": "아ㅣ",
          "speaker_id": "1",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100001765.1.13",
          "form": "내 적금",
          "original_form": "내 적금",
          "speaker_id": "1",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100001765.1.14",
          "form": "ㅠㅠ 추가금리 받아야하는데 엔씨 이 녀석들 작년 시즌 우승이었다면서",
          "original_form": "ㅠㅠ 추가금리 받아야하는데 엔씨 이 녀석들 작년 시즌 우승이었다면서",
          "speaker_id": "1",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100001765.1.15",
          "form": "아아 나는 그 신한적금은 두산으로 들었는데",
          "original_form": "아아 나는 그 신한적금은 두산으로 들었는데",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100001765.1.16",
          "form": "(롯데미안..)",
          "original_form": "(롯데미안..)",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100001765.1.17",
          "form": "두산 요새 되게 못하드라...?",
          "original_form": "두산 요새 되게 못하드라...?",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100001765.1.18",
          "form": "두산팬들 울어요",
          "original_form": "두산팬들 울어요",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100001765.1.19",
          "form": "내 주변에 두산팬들 많아서 다들 분위기가 초상이더라",
          "original_form": "내 주변에 두산팬들 많아서 다들 분위기가 초상이더라",
          "speaker_id": "1",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100001765.1.20",
          "form": "등수도 엄청 낮나?",
          "original_form": "등수도 엄청 낮나?",
          "speaker_id": "2",
          "time": "20210904 00:51"
        },
        {
          "id": "MDRW2100001765.1.21",
          "form": "이 정도면 거의 하위권 아닌가 싶을 정도로",
          "original_form": "이 정도면 거의 하위권 아닌가 싶을 정도로",
          "speaker_id": "2",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100001765.1.22",
          "form": "위닝포인트를 못받아",
          "original_form": "위닝포인트를 못받아",
          "speaker_id": "2",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100001765.1.23",
          "form": "ㅋㅋ 근데 여기 팬들은 의리잖아 잘해서 응원하는게 아니라",
          "original_form": "ㅋㅋ 근데 여기 팬들은 의리잖아 잘해서 응원하는게 아니라",
          "speaker_id": "1",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100001765.1.24",
          "form": "ㅋㅋㅋ 맞아 이거 연속으로 우승해야되는데 작년시즌 강력 우승후보들이 쓱에 밀려나고 있어",
          "original_form": "ㅋㅋㅋ 맞아 이거 연속으로 우승해야되는데 작년시즌 강력 우승후보들이 쓱에 밀려나고 있어",
          "speaker_id": "1",
          "time": "20210904 00:52"
        },
        {
          "id": "MDRW2100001765.1.25",
          "form": "ㅋㅋㅋㅋ맞아ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ맞아ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100001765.1.26",
          "form": "한화랑 롯뎈ㅋㅋ",
          "original_form": "한화랑 롯뎈ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100001765.1.27",
          "form": "예전에 롯기엘? 슬퍼하고 그랬는데",
          "original_form": "예전에 롯기엘? 슬퍼하고 그랬는데",
          "speaker_id": "2",
          "time": "20210904 00:54"
        },
        {
          "id": "MDRW2100001765.1.28",
          "form": "응 예선전인가 그때 쓱이 다 먹는거 보고 결정했어야했는데 왜 엔씨를 믿은걸까",
          "original_form": "응 예선전인가 그때 쓱이 다 먹는거 보고 결정했어야했는데 왜 엔씨를 믿은걸까",
          "speaker_id": "1",
          "time": "20210904 00:56"
        }
      ]
    }
