      "utterance": [
        {
          "id": "MDRW2100018732.1.1",
          "form": "대학생이신가요?",
          "original_form": "대학생이신가요?",
          "speaker_id": "1",
          "time": "20210827 11:34"
        },
        {
          "id": "MDRW2100018732.1.2",
          "form": "네 신입생이에요",
          "original_form": "네 신입생이에요",
          "speaker_id": "2",
          "time": "20210827 11:42"
        },
        {
          "id": "MDRW2100018732.1.3",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018732.1.4",
          "form": "21학번이요???",
          "original_form": "21학번이요???",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018732.1.5",
          "form": "전 18학번...",
          "original_form": "전 18학번...",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018732.1.6",
          "form": "이모 채팅 잘 치죠...",
          "original_form": "이모 채팅 잘 치죠...",
          "speaker_id": "1",
          "time": "20210827 11:45"
        },
        {
          "id": "MDRW2100018732.1.7",
          "form": "히익?... 18학번 처음봐요!!!",
          "original_form": "히익?... 18학번 처음봐요!!!",
          "speaker_id": "2",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018732.1.8",
          "form": "애니콜 쓰시나요(?)",
          "original_form": "애니콜 쓰시나요(?)",
          "speaker_id": "2",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018732.1.9",
          "form": "삐삐도 씁니다 (?)",
          "original_form": "삐삐도 씁니다 (?)",
          "speaker_id": "1",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100018732.1.10",
          "form": "ㅋㅋㅋㅋㅋ 수강신청 성공하셨어요?",
          "original_form": "ㅋㅋㅋㅋㅋ 수강신청 성공하셨어요?",
          "speaker_id": "2",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100018732.1.11",
          "form": "실은 전 휴학생이랍니다.",
          "original_form": "실은 전 휴학생이랍니다.",
          "speaker_id": "1",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100018732.1.12",
          "form": "휴하학",
          "original_form": "휴하학",
          "speaker_id": "1",
          "time": "20210827 11:49"
        },
        {
          "id": "MDRW2100018732.1.13",
          "form": "왜 휴학하셨어여?",
          "original_form": "왜 휴학하셨어여?",
          "speaker_id": "2",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100018732.1.14",
          "form": "재학생은 웃을 수 없습니다",
          "original_form": "재학생은 웃을 수 없습니다",
          "speaker_id": "1",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100018732.1.15",
          "form": "아직 진로도 못 정했는데ㅜ 4학년이라고 졸업할 순 없더라고요...",
          "original_form": "아직 진로도 못 정했는데ㅜ 4학년이라고 졸업할 순 없더라고요...",
          "speaker_id": "1",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100018732.1.16",
          "form": "제 미래같아서 무섭네요",
          "original_form": "제 미래같아서 무섭네요",
          "speaker_id": "2",
          "time": "20210827 11:50"
        },
        {
          "id": "MDRW2100018732.1.17",
          "form": "과제 좋아하세요?",
          "original_form": "과제 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100018732.1.18",
          "form": "례? 과제는 음 시험보단 좋지만 귀찮은 정도...?",
          "original_form": "례? 과제는 음 시험보단 좋지만 귀찮은 정도...?",
          "speaker_id": "1",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100018732.1.19",
          "form": "과제 중이신가요? 벌써 개강한 대학도 있다길래...",
          "original_form": "과제 중이신가요? 벌써 개강한 대학도 있다길래...",
          "speaker_id": "1",
          "time": "20210827 11:51"
        },
        {
          "id": "MDRW2100018732.1.20",
          "form": "네 동기거 베끼고있어요!",
          "original_form": "네 동기거 베끼고있어요!",
          "speaker_id": "2",
          "time": "20210827 11:52"
        },
        {
          "id": "MDRW2100018732.1.21",
          "form": "우와...",
          "original_form": "우와...",
          "speaker_id": "1",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100018732.1.22",
          "form": "진짜 과제를 학기 초부터",
          "original_form": "진짜 과제를 학기 초부터",
          "speaker_id": "1",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100018732.1.23",
          "form": "어메이징 k대학",
          "original_form": "어메이징 k대학",
          "speaker_id": "1",
          "time": "20210827 12:05"
        },
        {
          "id": "MDRW2100018732.1.24",
          "form": "무슨 학과에요?",
          "original_form": "무슨 학과에요?",
          "speaker_id": "1",
          "time": "20210827 12:05"
        }
      ]
