      "utterance": [
        {
          "id": "MDRW2100014932.1.1",
          "form": "맛집은 자주 찾아가시는 편인가요??",
          "original_form": "맛집은 자주 찾아가시는 편인가요??",
          "speaker_id": "2",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014932.1.2",
          "form": "네 가끔 주말에 친구볼때는",
          "original_form": "네 가끔 주말에 친구볼때는",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014932.1.3",
          "form": "일부러 맛집검색해서가요 ㅋㅋ",
          "original_form": "일부러 맛집검색해서가요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:14"
        },
        {
          "id": "MDRW2100014932.1.4",
          "form": "맛집검색은 주로 어디서 하나요??",
          "original_form": "맛집검색은 주로 어디서 하나요??",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014932.1.5",
          "form": "네이버 그냥 리뷰순이요 ㅋㅋ",
          "original_form": "네이버 그냥 리뷰순이요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014932.1.6",
          "form": "맛집 찾아가는편인가요??",
          "original_form": "맛집 찾아가는편인가요??",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014932.1.7",
          "form": "저는 친구들이 가자는곳",
          "original_form": "저는 친구들이 가자는곳",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014932.1.8",
          "form": "따라가는 편이에요",
          "original_form": "따라가는 편이에요",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014932.1.9",
          "form": "아하 뭐좋아하세요?",
          "original_form": "아하 뭐좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014932.1.10",
          "form": "잘모르는 상태에서 가는데 나중에 보니 맛집이래요 ㅎㅎ",
          "original_form": "잘모르는 상태에서 가는데 나중에 보니 맛집이래요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:16"
        },
        {
          "id": "MDRW2100014932.1.11",
          "form": "음..저는 가리지않고 잘먹지만",
          "original_form": "음..저는 가리지않고 잘먹지만",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014932.1.12",
          "form": "스시좋아해요!!",
          "original_form": "스시좋아해요!!",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014932.1.13",
          "form": "아 ㅋㅋㅋㅋ스시맛있죠",
          "original_form": "아 ㅋㅋㅋㅋ스시맛있죠",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014932.1.14",
          "form": "일식 저도 좋아해요",
          "original_form": "일식 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014932.1.15",
          "form": "어떤음식 좋아해요?",
          "original_form": "어떤음식 좋아해요?",
          "speaker_id": "2",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014932.1.16",
          "form": "전 떡볶이요",
          "original_form": "전 떡볶이요",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014932.1.17",
          "form": "요즘 로제에 꽂혀서",
          "original_form": "요즘 로제에 꽂혀서",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014932.1.18",
          "form": "그것만 먹어요",
          "original_form": "그것만 먹어요",
          "speaker_id": "1",
          "time": "20210825 21:17"
        },
        {
          "id": "MDRW2100014932.1.19",
          "form": "신전로제떡볶이가",
          "original_form": "신전로제떡볶이가",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014932.1.20",
          "form": "후추맛이 좀 강하다던데",
          "original_form": "후추맛이 좀 강하다던데",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014932.1.21",
          "form": "맞나요??",
          "original_form": "맞나요??",
          "speaker_id": "2",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014932.1.22",
          "form": "저는 못느꼈어요",
          "original_form": "저는 못느꼈어요",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014932.1.23",
          "form": "엄청 달아서 신전을 제일 좋아해요 ㅋㅋ",
          "original_form": "엄청 달아서 신전을 제일 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:22"
        },
        {
          "id": "MDRW2100014932.1.24",
          "form": "배떡도 맛있는데",
          "original_form": "배떡도 맛있는데",
          "speaker_id": "1",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014932.1.25",
          "form": "ㅋㅋ헉 배떡 저 한번도 안먹어봤어요",
          "original_form": "ㅋㅋ헉 배떡 저 한번도 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210825 21:23"
        },
        {
          "id": "MDRW2100014932.1.26",
          "form": "로제의 원조 아닌가요!!",
          "original_form": "로제의 원조 아닌가요!!",
          "speaker_id": "2",
          "time": "20210825 21:23"
        }
      ]
    }
