      "utterance": [
        {
          "id": "MDRW2100014967.1.1",
          "form": "요즘 보시는 콘텐츠 있으신가요?",
          "original_form": "요즘 보시는 콘텐츠 있으신가요?",
          "speaker_id": "2",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100014967.1.2",
          "form": "전 예능 자주 봐요",
          "original_form": "전 예능 자주 봐요",
          "speaker_id": "1",
          "time": "20210828 13:15"
        },
        {
          "id": "MDRW2100014967.1.3",
          "form": "앗 그렇군요  최근에 어떤 예능 보셨어요?",
          "original_form": "앗 그렇군요  최근에 어떤 예능 보셨어요?",
          "speaker_id": "2",
          "time": "20210828 13:16"
        },
        {
          "id": "MDRW2100014967.1.4",
          "form": "런닝맨이요",
          "original_form": "런닝맨이요",
          "speaker_id": "1",
          "time": "20210828 13:27"
        },
        {
          "id": "MDRW2100014967.1.5",
          "form": "런닝맨 재미있죠 ㅎㅎ",
          "original_form": "런닝맨 재미있죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:05"
        },
        {
          "id": "MDRW2100014967.1.6",
          "form": "네 유튜브도 잘 올려주고요",
          "original_form": "네 유튜브도 잘 올려주고요",
          "speaker_id": "1",
          "time": "20210828 21:06"
        },
        {
          "id": "MDRW2100014967.1.7",
          "form": "맞아요 옛날꺼 클립  보는것도 재미있더라구요",
          "original_form": "맞아요 옛날꺼 클립  보는것도 재미있더라구요",
          "speaker_id": "2",
          "time": "20210828 21:07"
        },
        {
          "id": "MDRW2100014967.1.8",
          "form": "런닝맨 클립은 제목도 대충인게 웃겨요",
          "original_form": "런닝맨 클립은 제목도 대충인게 웃겨요",
          "speaker_id": "1",
          "time": "20210828 21:08"
        },
        {
          "id": "MDRW2100014967.1.9",
          "form": "맞아요 웃겨요",
          "original_form": "맞아요 웃겨요",
          "speaker_id": "2",
          "time": "20210828 21:10"
        },
        {
          "id": "MDRW2100014967.1.10",
          "form": "요즘 보는거 있나요?",
          "original_form": "요즘 보는거 있나요?",
          "speaker_id": "1",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100014967.1.11",
          "form": "슬기로운의사생활2 요!",
          "original_form": "슬기로운의사생활2 요!",
          "speaker_id": "2",
          "time": "20210828 21:15"
        },
        {
          "id": "MDRW2100014967.1.12",
          "form": "그거 인기 많죠ㅋㅋㅋ",
          "original_form": "그거 인기 많죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:20"
        },
        {
          "id": "MDRW2100014967.1.13",
          "form": "저도 몇 편은 봤어요",
          "original_form": "저도 몇 편은 봤어요",
          "speaker_id": "1",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100014967.1.14",
          "form": "앗 그러시군요",
          "original_form": "앗 그러시군요",
          "speaker_id": "2",
          "time": "20210828 21:21"
        },
        {
          "id": "MDRW2100014967.1.15",
          "form": "이번주거 재밌던데요ㅋㅋ",
          "original_form": "이번주거 재밌던데요ㅋㅋ",
