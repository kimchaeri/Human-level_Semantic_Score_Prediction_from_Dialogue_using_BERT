      "utterance": [
        {
          "id": "MDRW2100036413.1.1",
          "form": "저도 그거 사먹었는데",
          "original_form": "저도 그거 사먹었는데",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100036413.1.2",
          "form": "네 ㅋㅋㅋ 약국에서파는데 인터넷이 더 싸요",
          "original_form": "네 ㅋㅋㅋ 약국에서파는데 인터넷이 더 싸요",
          "speaker_id": "2",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100036413.1.3",
          "form": "달달하니 맛이 좋아서",
          "original_form": "달달하니 맛이 좋아서",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100036413.1.4",
          "form": "삼일만에 다 먹었어요 ㅋㅋㅋ",
          "original_form": "삼일만에 다 먹었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100036413.1.5",
          "form": "맞아요 맛잇어요 ㅋㅋㅋ",
          "original_form": "맞아요 맛잇어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100036413.1.6",
          "form": "남용하게되어요",
          "original_form": "남용하게되어요",
          "speaker_id": "2",
          "time": "20210830 22:37"
        },
        {
          "id": "MDRW2100036413.1.7",
          "form": "name2님은 혹시",
          "original_form": "&name2&님은 혹시",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100036413.1.8",
          "form": "술 잘 드세요?",
          "original_form": "술 잘 드세요?",
          "speaker_id": "1",
          "time": "20210830 22:39"
        },
        {
          "id": "MDRW2100036413.1.9",
          "form": "저 과음하다가",
          "original_form": "저 과음하다가",
          "speaker_id": "2",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100036413.1.10",
          "form": "위염앓게되어서",
          "original_form": "위염앓게되어서",
          "speaker_id": "2",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100036413.1.11",
          "form": "술을 끊은지일년되엇어요 하지만 술에대한 좋은기억들도 남아잇어요",
          "original_form": "술을 끊은지일년되엇어요 하지만 술에대한 좋은기억들도 남아잇어요",
          "speaker_id": "2",
          "time": "20210830 22:40"
        },
        {
          "id": "MDRW2100036413.1.12",
          "form": "아이구ㅠㅠ 가끔 간단한 맥주한잔 시원하게 드시고싶으실텐데",
          "original_form": "아이구ㅠㅠ 가끔 간단한 맥주한잔 시원하게 드시고싶으실텐데",
          "speaker_id": "1",
          "time": "20210830 22:41"
        },
        {
          "id": "MDRW2100036413.1.13",
          "form": "맞아요!! 가끔 맥주한잔 먹긴해요 ㅋㅋㅋㅋ",
          "original_form": "맞아요!! 가끔 맥주한잔 먹긴해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100036413.1.14",
          "form": "예전엔 부어라마셔라엿는데",
          "original_form": "예전엔 부어라마셔라엿는데",
          "speaker_id": "2",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100036413.1.15",
          "form": "요즘은 튀김같은거잇으면 간단히 한잔정도..두달에한번?정도 먹는거같아요",
          "original_form": "요즘은 튀김같은거잇으면 간단히 한잔정도..두달에한번?정도 먹는거같아요",
          "speaker_id": "2",
          "time": "20210830 22:42"
        },
        {
          "id": "MDRW2100036413.1.16",
          "form": "ㅎㅎ 선호하시는 맥주 있으신가용?",
          "original_form": "ㅎㅎ 선호하시는 맥주 있으신가용?",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100036413.1.17",
          "form": "저 흑맥주를 좋아햇어요",
          "original_form": "저 흑맥주를 좋아햇어요",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100036413.1.18",
          "form": "튀김에 맥주 크으 궁합 좋네요",
          "original_form": "튀김에 맥주 크으 궁합 좋네요",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100036413.1.19",
          "form": "스타우트류..",
          "original_form": "스타우트류..",
          "speaker_id": "2",
          "time": "20210830 22:43"
        },
        {
          "id": "MDRW2100036413.1.20",
          "form": "오 기네스요?",
          "original_form": "오 기네스요?",
          "speaker_id": "1",
          "time": "20210830 22:43"
        },
        {
