        },
        {
          "id": "MDRW2100043867.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043867.1.3",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "2",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100043867.1.4",
          "form": "네넹 닭볶음탕 먹었엉ㅎ",
          "original_form": "네넹 닭볶음탕 먹었엉ㅎ",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043867.1.5",
          "form": "저녁드셨나요?",
          "original_form": "저녁드셨나요?",
          "speaker_id": "1",
          "time": "20210907 20:41"
        },
        {
          "id": "MDRW2100043867.1.6",
          "form": "네 불닭크림리조또 해먹었어용",
          "original_form": "네 불닭크림리조또 해먹었어용",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043867.1.7",
          "form": "닭볶음탕 저 엄청 좋아하는데 맛있었겠어요",
          "original_form": "닭볶음탕 저 엄청 좋아하는데 맛있었겠어요",
          "speaker_id": "2",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043867.1.8",
          "form": "네 사촌동생이 해줬어요!",
          "original_form": "네 사촌동생이 해줬어요!",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043867.1.9",
          "form": "오우 리조또도 너무 맛있겠는걸요..",
          "original_form": "오우 리조또도 너무 맛있겠는걸요..",
          "speaker_id": "1",
          "time": "20210907 20:42"
        },
        {
          "id": "MDRW2100043867.1.10",
          "form": "오 좋은 사촌동생을 두셨군요!!",
          "original_form": "오 좋은 사촌동생을 두셨군요!!",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043867.1.11",
          "form": "당면도 들어갔나요?",
          "original_form": "당면도 들어갔나요?",
          "speaker_id": "2",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043867.1.12",
          "form": "헉 어떻게 아셨죠.....",
          "original_form": "헉 어떻게 아셨죠.....",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043867.1.13",
          "form": "당면이 진짜 맛있드라고요..",
          "original_form": "당면이 진짜 맛있드라고요..",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043867.1.14",
          "form": "무조건 필수죠... ㅠㅠ 당면 넘 맛있어요 !!",
          "original_form": "무조건 필수죠... ㅠㅠ 당면 넘 맛있어요 !!",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043867.1.15",
          "form": "전 닭볶음탕에 당면 들어간 건 처음 먹어봤는데",
          "original_form": "전 닭볶음탕에 당면 들어간 건 처음 먹어봤는데",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043867.1.16",
          "form": "넘 맛있드라거요..",
          "original_form": "넘 맛있드라거요..",
          "speaker_id": "1",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043867.1.17",
          "form": "양념을 아주그냥 ㅠㅠ",
          "original_form": "양념을 아주그냥 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043867.1.18",
          "form": "쏙 빨",
          "original_form": "쏙 빨",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043867.1.19",
          "form": "아들여서ㅠㅠㅠㅠ",
          "original_form": "아들여서ㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043867.1.20",
          "form": "그니까요 .. 양념 쫙 배어가지고 넘 맛있어용 ㅋㅋ",
          "original_form": "그니까요 .. 양념 쫙 배어가지고 넘 맛있어용 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043867.1.21",
          "form": "당면만 또 먹고 싶네요",
          "original_form": "당면만 또 먹고 싶네요",
          "speaker_id": "1",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043867.1.22",
          "form": "닭은 어떤 부위 제일 좋아하시나요?",
          "original_form": "닭은 어떤 부위 제일 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043867.1.23",
          "form": "저는 다리는 안 좋아하고",
          "original_form": "저는 다리는 안 좋아하고",
          "speaker_id": "1",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043867.1.24",
          "form": "날개부분을 좋아해요",
          "original_form": "날개부분을 좋아해요",
          "speaker_id": "1",
          "time": "20210907 20:51"
        }
      ]
