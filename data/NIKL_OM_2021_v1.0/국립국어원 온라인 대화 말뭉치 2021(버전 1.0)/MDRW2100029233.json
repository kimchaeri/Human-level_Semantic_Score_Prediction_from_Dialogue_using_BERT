      "utterance": [
        {
          "id": "MDRW2100029233.1.1",
          "form": "학교에서",
          "original_form": "학교에서",
          "speaker_id": "2",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029233.1.2",
          "form": "과활동은",
          "original_form": "과활동은",
          "speaker_id": "2",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029233.1.3",
          "form": "어쩔수없이했는데",
          "original_form": "어쩔수없이했는데",
          "speaker_id": "2",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029233.1.4",
          "form": "숙제가 넘어럽고",
          "original_form": "숙제가 넘어럽고",
          "speaker_id": "2",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029233.1.5",
          "form": "others는 필수가 아니었는데",
          "original_form": "&others&는 필수가 아니었는데",
          "speaker_id": "1",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029233.1.6",
          "form": "시간많이 걸려서",
          "original_form": "시간많이 걸려서",
          "speaker_id": "2",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029233.1.7",
          "form": "다같이모여서 했기때문이지",
          "original_form": "다같이모여서 했기때문이지",
          "speaker_id": "2",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029233.1.8",
          "form": "그래도 기억은 많이 남았겠네요",
          "original_form": "그래도 기억은 많이 남았겠네요",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029233.1.9",
          "form": "홍보도 많이 되고",
          "original_form": "홍보도 많이 되고",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029233.1.10",
          "form": "공부도 많이 되고",
          "original_form": "공부도 많이 되고",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029233.1.11",
          "form": "몰라서 물어본다고 ㅋㅋㅋ",
          "original_form": "몰라서 물어본다고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029233.1.12",
          "form": "빵집에 모여서 맨날 과제함",
          "original_form": "빵집에 모여서 맨날 과제함",
          "speaker_id": "2",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029233.1.13",
          "form": "동아리에서 연애도 많이 했겠는데요",
          "original_form": "동아리에서 연애도 많이 했겠는데요",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029233.1.14",
          "form": "시험도 자주첬음",
          "original_form": "시험도 자주첬음",
          "speaker_id": "2",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029233.1.15",
          "form": "물어보다가 정들었겠다",
          "original_form": "물어보다가 정들었겠다",
          "speaker_id": "1",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029233.1.16",
          "form": "난씨씨아니어서",
          "original_form": "난씨씨아니어서",
          "speaker_id": "2",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029233.1.17",
          "form": "다른 친구랑 후배가",
          "original_form": "다른 친구랑 후배가",
          "speaker_id": "2",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029233.1.18",
          "form": "아우 저는 그런 동아리 시 렸을 거 같애요",
          "original_form": "아우 저는 그런 동아리 시 렸을 거 같애요",
          "speaker_id": "1",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029233.1.19",
          "form": "씨씨돌아가면서했지",
          "original_form": "씨씨돌아가면서했지",
          "speaker_id": "2",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029233.1.20",
          "form": "씨씨는 하는 게 아니에요",
          "original_form": "씨씨는 하는 게 아니에요",
          "speaker_id": "1",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029233.1.21",
          "form": "동물의 왕국이네요",
          "original_form": "동물의 왕국이네요",
          "speaker_id": "1",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029233.1.22",
          "form": "맞아 하는게 아니지",
          "original_form": "맞아 하는게 아니지",
          "speaker_id": "2",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029233.1.23",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:32"
