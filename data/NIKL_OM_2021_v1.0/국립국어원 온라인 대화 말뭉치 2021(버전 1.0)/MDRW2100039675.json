      "utterance": [
        {
          "id": "MDRW2100039675.1.1",
          "form": "학교...다니시나요...? 이게 무슨 질문이지",
          "original_form": "학교...다니시나요...? 이게 무슨 질문이지",
          "speaker_id": "1",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039675.1.2",
          "form": "저는 다 졸업했답니다...",
          "original_form": "저는 다 졸업했답니다...",
          "speaker_id": "1",
          "time": "20210903 21:28"
        },
        {
          "id": "MDRW2100039675.1.3",
          "form": "저도요ㅋㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039675.1.4",
          "form": "그냥 학교다녔을때 얘기해도 될 것 같은데",
          "original_form": "그냥 학교다녔을때 얘기해도 될 것 같은데",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039675.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ네네ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ네네ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039675.1.6",
          "form": "조금 까마득하네요ㅋㅋㅋ",
          "original_form": "조금 까마득하네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039675.1.7",
          "form": "무슨과셨는지 여쭤봐도 되나요ㅋㅋㅋㅋㅋ",
          "original_form": "무슨과셨는지 여쭤봐도 되나요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100039675.1.8",
          "form": "저는 경영학과요!",
          "original_form": "저는 경영학과요!",
          "speaker_id": "2",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100039675.1.9",
          "form": "우와~ 경영학과 멋있네요 저는 사범대였어요",
          "original_form": "우와~ 경영학과 멋있네요 저는 사범대였어요",
          "speaker_id": "1",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039675.1.10",
          "form": "경영학과는 사람 엄청 많잖아요",
          "original_form": "경영학과는 사람 엄청 많잖아요",
          "speaker_id": "1",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039675.1.11",
          "form": "복수전공하러 오는 사람도 많아서",
          "original_form": "복수전공하러 오는 사람도 많아서",
          "speaker_id": "2",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039675.1.12",
          "form": "과생활안하고 사막의 모래먼지처럼 묻어가긴 좋은 것 같아요ㅋㅋㅋ",
          "original_form": "과생활안하고 사막의 모래먼지처럼 묻어가긴 좋은 것 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039675.1.13",
          "form": "전 사범대가 더 멋있는 것 같아요ㅋㅋㅋㅋ",
          "original_form": "전 사범대가 더 멋있는 것 같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039675.1.14",
          "form": "교생실습도 다녀오셨죠?",
          "original_form": "교생실습도 다녀오셨죠?",
          "speaker_id": "2",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039675.1.15",
          "form": "네넵 다녀왔죠ㅋㅋㅋㅋ... 근데 저는 모교로 안가서",
          "original_form": "네넵 다녀왔죠ㅋㅋㅋㅋ... 근데 저는 모교로 안가서",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039675.1.16",
          "form": "100명 넘는 교생쌤들 중 하나로 흐릿하게 묻혀갔었어요ㅋㅋㅋㅋ",
          "original_form": "100명 넘는 교생쌤들 중 하나로 흐릿하게 묻혀갔었어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039675.1.17",
          "form": "혹시 동아리같은 거 하셨나요?",
          "original_form": "혹시 동아리같은 거 하셨나요?",
          "speaker_id": "1",
          "time": "20210903 21:35"
        },
        {
          "id": "MDRW2100039675.1.18",
          "form": "동아리 이것저것 해보고 싶었는데 발은 잘 못붙였어요ㅋㅋㅋ",
          "original_form": "동아리 이것저것 해보고 싶었는데 발은 잘 못붙였어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039675.1.19",
          "form": "학생때로 돌아가면 악기연주하는 동아리? 하나쯤 들어서 악기배워보고 싶어요",
          "original_form": "학생때로 돌아가면 악기연주하는 동아리? 하나쯤 들어서 악기배워보고 싶어요",
          "speaker_id": "2",
          "time": "20210903 21:36"
        },
        {
          "id": "MDRW2100039675.1.20",
          "form": "헐 저두요!",
          "original_form": "헐 저두요!",
          "speaker_id": "1",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039675.1.21",
          "form": "전 드럼 배워보고 싶었는데... 뭔가 무서워서ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 드럼 배워보고 싶었는데... 뭔가 무서워서ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039675.1.22",
          "form": "학생때는 겁이 많았죠...",
          "original_form": "학생때는 겁이 많았죠...",
          "speaker_id": "1",
          "time": "20210903 21:37"
        },
        {
          "id": "MDRW2100039675.1.23",
          "form": "맞아요 드럼 너무 멋있는데ㅋㅋㅋㅋ",
          "original_form": "맞아요 드럼 너무 멋있는데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100039675.1.24",
          "form": "악기도 크고 압도적이니까",
          "original_form": "악기도 크고 압도적이니까",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039675.1.25",
          "form": "용기내기가 쉽지 않았을 것 같아요",
          "original_form": "용기내기가 쉽지 않았을 것 같아요",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039675.1.26",
          "form": "공연하는 동아리들이 멋있기도하고",
          "original_form": "공연하는 동아리들이 멋있기도하고",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039675.1.27",
          "form": "맞아요ㅋㅋㅋㅋㅋ 그리고 뭔가 무대에 설 용기도 안 나고",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ 그리고 뭔가 무대에 설 용기도 안 나고",
          "speaker_id": "1",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039675.1.28",
          "form": "학창시절 지나면 그렇게 나와서 공연할 수 있는 기회도 적어서",
          "original_form": "학창시절 지나면 그렇게 나와서 공연할 수 있는 기회도 적어서",
          "speaker_id": "2",
          "time": "20210903 21:39"
        },
        {
          "id": "MDRW2100039675.1.29",
          "form": "마자요ㅋㅋㅋㅋ배울 기회도 적구요",
          "original_form": "마자요ㅋㅋㅋㅋ배울 기회도 적구요",
          "speaker_id": "1",
          "time": "20210903 21:40"
        },
        {
          "id": "MDRW2100039675.1.30",
          "form": "학교를 나와서 뭔가 새롭게 배우려면 엄청 발품팔아야하는 것 같아요ㅋㅋㅋㅠ",
          "original_form": "학교를 나와서 뭔가 새롭게 배우려면 엄청 발품팔아야하는 것 같아요ㅋㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210903 21:42"
        }
      ]
