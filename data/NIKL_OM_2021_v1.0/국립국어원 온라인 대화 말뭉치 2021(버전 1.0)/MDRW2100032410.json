      "utterance": [
        {
          "id": "MDRW2100032410.1.1",
          "form": "오늘 점심은 뭐 먹었어?",
          "original_form": "오늘 점심은 뭐 먹었어?",
          "speaker_id": "2",
          "time": "20210905 17:05"
        },
        {
          "id": "MDRW2100032410.1.2",
          "form": "불짬뽕 먹었어",
          "original_form": "불짬뽕 먹었어",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032410.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "1",
          "time": "20210905 17:06"
        },
        {
          "id": "MDRW2100032410.1.4",
          "form": "라면 끓여 먹었어",
          "original_form": "라면 끓여 먹었어",
          "speaker_id": "2",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100032410.1.5",
          "form": "우리 둘 다 면요리 먹었네 ㅋㅋ",
          "original_form": "우리 둘 다 면요리 먹었네 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100032410.1.6",
          "form": "주말이라 요리하기 귀찮아서 ㅋㄴ",
          "original_form": "주말이라 요리하기 귀찮아서 ㅋㄴ",
          "speaker_id": "2",
          "time": "20210905 17:08"
        },
        {
          "id": "MDRW2100032410.1.7",
          "form": "맞아 ㅋㅋㅋ 저녁엔 짬뽕 국물에 밥 말아먹으려고",
          "original_form": "맞아 ㅋㅋㅋ 저녁엔 짬뽕 국물에 밥 말아먹으려고",
          "speaker_id": "1",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032410.1.8",
          "form": "핫도그 먹고 싶다",
          "original_form": "핫도그 먹고 싶다",
          "speaker_id": "2",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032410.1.9",
          "form": "핫도그 맛있지",
          "original_form": "핫도그 맛있지",
          "speaker_id": "1",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032410.1.10",
          "form": "떡볶이랑 같이 먹으면 맛있어",
          "original_form": "떡볶이랑 같이 먹으면 맛있어",
          "speaker_id": "2",
          "time": "20210905 17:09"
        },
        {
          "id": "MDRW2100032410.1.11",
          "form": "아 알지알지",
          "original_form": "아 알지알지",
          "speaker_id": "1",
          "time": "20210905 17:10"
        },
        {
          "id": "MDRW2100032410.1.12",
          "form": "만득이핫도그 먹고 싶다",
          "original_form": "만득이핫도그 먹고 싶다",
          "speaker_id": "2",
          "time": "20210905 17:10"
        },
        {
          "id": "MDRW2100032410.1.13",
          "form": "만득이 핫도그가 뭐야?",
          "original_form": "만득이 핫도그가 뭐야?",
          "speaker_id": "1",
          "time": "20210905 17:10"
        },
        {
          "id": "MDRW2100032410.1.14",
          "form": "감자 큐브 덕지덕지 붙은 핫도그 ㅋㅋ",
          "original_form": "감자 큐브 덕지덕지 붙은 핫도그 ㅋㅋ",
          "speaker_id": "2",
