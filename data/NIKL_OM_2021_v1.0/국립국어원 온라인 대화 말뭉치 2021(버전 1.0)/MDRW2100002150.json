      "utterance": [
        {
          "id": "MDRW2100002150.1.1",
          "form": "게임 하는거 있어?",
          "original_form": "게임 하는거 있어?",
          "speaker_id": "2",
          "time": "20210820 00:06"
        },
        {
          "id": "MDRW2100002150.1.2",
          "form": "게임 어릴땐 많이 했는데 회사다니면서 하기어려운거같아 ㅠㅠ 너는 게임 해?",
          "original_form": "게임 어릴땐 많이 했는데 회사다니면서 하기어려운거같아 ㅠㅠ 너는 게임 해?",
          "speaker_id": "1",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002150.1.3",
          "form": "나는 닌텐도스위치 있어서 그거만 가끔 해",
          "original_form": "나는 닌텐도스위치 있어서 그거만 가끔 해",
          "speaker_id": "2",
          "time": "20210820 00:07"
        },
        {
          "id": "MDRW2100002150.1.4",
          "form": "아 스위치 있구나 나는 사두고 못켠지좀됐다",
          "original_form": "아 스위치 있구나 나는 사두고 못켠지좀됐다",
          "speaker_id": "1",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100002150.1.5",
          "form": "어떤게임해?",
          "original_form": "어떤게임해?",
          "speaker_id": "1",
          "time": "20210820 00:08"
        },
        {
          "id": "MDRW2100002150.1.6",
          "form": "동물의숲이랑 젤다!",
          "original_form": "동물의숲이랑 젤다!",
          "speaker_id": "2",
          "time": "20210820 00:09"
        },
        {
          "id": "MDRW2100002150.1.7",
          "form": "동숲 젤다! 둘다 재밌게했다 나두 ㅠㅠㅋㅋㅋ",
          "original_form": "동숲 젤다! 둘다 재밌게했다 나두 ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:09"
        },
        {
          "id": "MDRW2100002150.1.8",
          "form": "그치 젤다 2 나온다길래 기다리고있어",
          "original_form": "그치 젤다 2 나온다길래 기다리고있어",
          "speaker_id": "2",
          "time": "20210820 00:10"
        },
        {
          "id": "MDRW2100002150.1.9",
          "form": "젤다 2 나오는구나",
          "original_form": "젤다 2 나오는구나",
          "speaker_id": "1",
          "time": "20210820 00:13"
        },
        {
          "id": "MDRW2100002150.1.10",
          "form": "기대된다!!",
          "original_form": "기대된다!!",
          "speaker_id": "1",
          "time": "20210820 00:13"
        },
        {
          "id": "MDRW2100002150.1.11",
          "form": "그치! 스위치 재밌는 게임 더 있어?",
          "original_form": "그치! 스위치 재밌는 게임 더 있어?",
          "speaker_id": "2",
          "time": "20210820 00:13"
        },
        {
          "id": "MDRW2100002150.1.12",
          "form": "나는 그나마?최근에 젤다무쌍 했었는데",
          "original_form": "나는 그나마?최근에 젤다무쌍 했었는데",
          "speaker_id": "1",
          "time": "20210820 00:14"
        },
        {
          "id": "MDRW2100002150.1.13",
          "form": "약간 젤다의 단순한버전? 재밌더라구",
          "original_form": "약간 젤다의 단순한버전? 재밌더라구",
          "speaker_id": "1",
          "time": "20210820 00:14"
        },
        {
          "id": "MDRW2100002150.1.14",
          "form": "중고로 구매해서 해봐야겠다",
          "original_form": "중고로 구매해서 해봐야겠다",
          "speaker_id": "2",
          "time": "20210820 00:15"
        },
        {
          "id": "MDRW2100002150.1.15",
          "form": "난 약간 유치한게 취향인지 마리오 오딧세이 넘 재밌게 했어ㅋㅋ",
          "original_form": "난 약간 유치한게 취향인지 마리오 오딧세이 넘 재밌게 했어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:15"
        },
        {
          "id": "MDRW2100002150.1.16",
          "form": "ㅋㅋㅋㅋㅋ나두 포켓몬도 재밌게 했다",
          "original_form": "ㅋㅋㅋㅋㅋ나두 포켓몬도 재밌게 했다",
          "speaker_id": "1",
          "time": "20210820 00:15"
        },
        {
          "id": "MDRW2100002150.1.17",
          "form": "포켓몬도 내년에 또 새로운버전 나온다더라",
          "original_form": "포켓몬도 내년에 또 새로운버전 나온다더라",
          "speaker_id": "1",
          "time": "20210820 00:15"
        },
        {
          "id": "MDRW2100002150.1.18",
          "form": "아 포켓몬도 재밌겠다ㅋㅋㅋ",
          "original_form": "아 포켓몬도 재밌겠다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002150.1.19",
          "form": "그 무슨 포켓몬도 젤다 야숨처럼 막 돌아다닐수있게 나온다더라",
          "original_form": "그 무슨 포켓몬도 젤다 야숨처럼 막 돌아다닐수있게 나온다더라",
          "speaker_id": "1",
          "time": "20210820 00:18"
        },
        {
          "id": "MDRW2100002150.1.20",
          "form": "사람들이 그래서 포숨이래 ㅋㅋ",
          "original_form": "사람들이 그래서 포숨이래 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:18"
        },
        {
          "id": "MDRW2100002150.1.21",
          "form": "헐 짱재밌겠다",
          "original_form": "헐 짱재밌겠다",
          "speaker_id": "2",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002150.1.22",
          "form": "나오면 바로 사서 할듯",
          "original_form": "나오면 바로 사서 할듯",
          "speaker_id": "2",
          "time": "20210820 00:20"
        },
        {
          "id": "MDRW2100002150.1.23",
          "form": "그치 내년 1월이래 ㅋㅋㅋ",
          "original_form": "그치 내년 1월이래 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:22"
        },
        {
          "id": "MDRW2100002150.1.24",
          "form": "완전 기대된다",
          "original_form": "완전 기대된다",
          "speaker_id": "2",
          "time": "20210820 00:22"
        }
      ]
    }
