      "utterance": [
        {
          "id": "MDRW2100025580.1.1",
          "form": "최근에 뭐 사셨어요?",
          "original_form": "최근에 뭐 사셨어요?",
          "speaker_id": "2",
          "time": "20210901 13:57"
        },
        {
          "id": "MDRW2100025580.1.2",
          "form": "저는! 최근에 비비고 왕교자를 샀습니다",
          "original_form": "저는! 최근에 비비고 왕교자를 샀습니다",
          "speaker_id": "1",
          "time": "20210901 16:17"
        },
        {
          "id": "MDRW2100025580.1.3",
          "form": "ㅋㅋ 만두 너무 맛있죠!!",
          "original_form": "ㅋㅋ 만두 너무 맛있죠!!",
          "speaker_id": "2",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025580.1.4",
          "form": "전 영양제 샀어요?",
          "original_form": "전 영양제 샀어요?",
          "speaker_id": "2",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025580.1.5",
          "form": "!",
          "original_form": "!",
          "speaker_id": "2",
          "time": "20210901 16:23"
        },
        {
          "id": "MDRW2100025580.1.6",
          "form": "오 뭐사셨어요?",
          "original_form": "오 뭐사셨어요?",
          "speaker_id": "1",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100025580.1.7",
          "form": "마그네슘비타민디 함께 들어있는거요!",
          "original_form": "마그네슘비타민디 함께 들어있는거요!",
          "speaker_id": "2",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100025580.1.8",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100025580.1.9",
          "form": "저도 먹는데 따로 먹고있어요",
          "original_form": "저도 먹는데 따로 먹고있어요",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100025580.1.10",
          "form": "두개만 드세용?",
          "original_form": "두개만 드세용?",
          "speaker_id": "1",
          "time": "20210901 20:41"
        },
        {
          "id": "MDRW2100025580.1.11",
          "form": "같이든거 하나랑 유산균먹어요!",
          "original_form": "같이든거 하나랑 유산균먹어요!",
          "speaker_id": "2",
          "time": "20210901 20:43"
        },
        {
          "id": "MDRW2100025580.1.12",
          "form": "오~ 저두 유산균 먹고있는데",
          "original_form": "오~ 저두 유산균 먹고있는데",
          "speaker_id": "1",
          "time": "20210901 20:46"
        },
        {
          "id": "MDRW2100025580.1.13",
          "form": "최근에 비타민씨도 구입했어요",
          "original_form": "최근에 비타민씨도 구입했어요",
          "speaker_id": "1",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100025580.1.14",
          "form": "참 그러고보면 먹을게 너무 많아요 약만으로도 배부를 지경이죠ㅋㅋ",
          "original_form": "참 그러고보면 먹을게 너무 많아요 약만으로도 배부를 지경이죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:48"
        },
        {
          "id": "MDRW2100025580.1.15",
          "form": "전 약과 함께 초컬릿과 냉동만두를ㅋㅋ",
          "original_form": "전 약과 함께 초컬릿과 냉동만두를ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 20:49"
        },
        {
          "id": "MDRW2100025580.1.16",
          "form": "맞아요ㅋㅋ처음엔 한두개만 먹다가 지금 5개는 먹는거같아요",
          "original_form": "맞아요ㅋㅋ처음엔 한두개만 먹다가 지금 5개는 먹는거같아요",
          "speaker_id": "1",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100025580.1.17",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100025580.1.18",
          "form": "저 만두 먹고싶었는데!!!!",
          "original_form": "저 만두 먹고싶었는데!!!!",
          "speaker_id": "1",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100025580.1.19",
          "form": "맛있었겠네용",
          "original_form": "맛있었겠네용",
          "speaker_id": "1",
          "time": "20210901 20:58"
        },
        {
          "id": "MDRW2100025580.1.20",
          "form": "ㅋㅋ만둣국용 사서 끓여먹어요!",
          "original_form": "ㅋㅋ만둣국용 사서 끓여먹어요!",
          "speaker_id": "2",
          "time": "20210901 20:59"
        },
        {
          "id": "MDRW2100025580.1.21",
          "form": "식품도 온라인구매하세요?",
          "original_form": "식품도 온라인구매하세요?",
          "speaker_id": "2",
          "time": "20210901 21:00"
        },
        {
          "id": "MDRW2100025580.1.22",
          "form": "네 요즘은 더더욱 온라인으로 시키는거 같아요 ㅎ",
          "original_form": "네 요즘은 더더욱 온라인으로 시키는거 같아요 ㅎ",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100025580.1.23",
          "form": "어제 칫솔 샀는데 벌써 도착했더라고요",
          "original_form": "어제 칫솔 샀는데 벌써 도착했더라고요",
          "speaker_id": "1",
          "time": "20210901 21:06"
        },
        {
          "id": "MDRW2100025580.1.24",
          "form": "아 요즘 진짜 빨라요",
          "original_form": "아 요즘 진짜 빨라요",
