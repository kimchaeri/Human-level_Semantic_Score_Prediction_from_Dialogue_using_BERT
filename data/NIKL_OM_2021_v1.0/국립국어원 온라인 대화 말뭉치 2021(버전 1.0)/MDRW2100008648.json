      "utterance": [
        {
          "id": "MDRW2100008648.1.1",
          "form": "싸이월드 하셨었어요?",
          "original_form": "싸이월드 하셨었어요?",
          "speaker_id": "1",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008648.1.2",
          "form": "네 한 때 했었네요 ㅎㅎ",
          "original_form": "네 한 때 했었네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008648.1.3",
          "form": "저두요 ㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008648.1.4",
          "form": "그땐 거의 다했었어서 ㅋㅋ",
          "original_form": "그땐 거의 다했었어서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:09"
        },
        {
          "id": "MDRW2100008648.1.5",
          "form": "네 또 부활된다면서요",
          "original_form": "네 또 부활된다면서요",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008648.1.6",
          "form": "네 저번에 찾아봤더니 아직 안나오도라구요",
          "original_form": "네 저번에 찾아봤더니 아직 안나오도라구요",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008648.1.7",
          "form": "거기에 사진 많이 올렸었는데",
          "original_form": "거기에 사진 많이 올렸었는데",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008648.1.8",
          "form": "네 흑역사도 엄청 많을 듯 ㅋㅋ",
          "original_form": "네 흑역사도 엄청 많을 듯 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008648.1.9",
          "form": "아이구 흑역사",
          "original_form": "아이구 흑역사",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008648.1.10",
          "form": "그렇겠네요",
          "original_form": "그렇겠네요",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008648.1.11",
          "form": "네 다이어리에 일기들도.. ㅋㅋㅋ",
          "original_form": "네 다이어리에 일기들도.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008648.1.12",
          "form": "조금 두렵네요",
          "original_form": "조금 두렵네요",
          "speaker_id": "2",
          "time": "20210822 17:10"
        },
        {
          "id": "MDRW2100008648.1.13",
          "form": "네 ㅋㅋ 그래도 추억도 많겠죠?",
          "original_form": "네 ㅋㅋ 그래도 추억도 많겠죠?",
          "speaker_id": "1",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008648.1.14",
          "form": "네 기다려지네요",
          "original_form": "네 기다려지네요",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008648.1.15",
          "form": "네 저도요",
          "original_form": "네 저도요",
          "speaker_id": "1",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008648.1.16",
          "form": "일단 풀리면 접속해 볼 것 같긴해요",
          "original_form": "일단 풀리면 접속해 볼 것 같긴해요",
          "speaker_id": "1",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008648.1.17",
          "form": "저는 좀 귀찮아서",
          "original_form": "저는 좀 귀찮아서",
          "speaker_id": "2",
          "time": "20210822 17:11"
        },
        {
          "id": "MDRW2100008648.1.18",
          "form": "일단은 미뤄질 것 같아요",
          "original_form": "일단은 미뤄질 것 같아요",
          "speaker_id": "2",
