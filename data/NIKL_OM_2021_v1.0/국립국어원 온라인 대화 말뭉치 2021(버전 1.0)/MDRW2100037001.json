      "utterance": [
        {
          "id": "MDRW2100037001.1.1",
          "form": "최근에 구매하신제품있나요",
          "original_form": "최근에 구매하신제품있나요",
          "speaker_id": "1",
          "time": "20210831 16:47"
        },
        {
          "id": "MDRW2100037001.1.2",
          "form": "소소하게는 어제 푸딩을 샀어요",
          "original_form": "소소하게는 어제 푸딩을 샀어요",
          "speaker_id": "2",
          "time": "20210831 16:48"
        },
        {
          "id": "MDRW2100037001.1.3",
          "form": "그쳐 초코푸딩",
          "original_form": "그쳐 초코푸딩",
          "speaker_id": "1",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100037001.1.4",
          "form": "구매하고싶은 물건있으시나여",
          "original_form": "구매하고싶은 물건있으시나여",
          "speaker_id": "1",
          "time": "20210831 16:49"
        },
        {
          "id": "MDRW2100037001.1.5",
          "form": "ㅋㅋㅋ 사고 싶은 건.. 운동화도 하나 새로 사고 싶네요",
          "original_form": "ㅋㅋㅋ 사고 싶은 건.. 운동화도 하나 새로 사고 싶네요",
          "speaker_id": "2",
          "time": "20210831 16:50"
        },
        {
          "id": "MDRW2100037001.1.6",
          "form": "저도 운동화하나사고싶은데",
          "original_form": "저도 운동화하나사고싶은데",
          "speaker_id": "1",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100037001.1.7",
          "form": "어떤운동화 사고싶으세요",
          "original_form": "어떤운동화 사고싶으세요",
          "speaker_id": "1",
          "time": "20210831 16:51"
        },
        {
          "id": "MDRW2100037001.1.8",
          "form": "한동안 흰운동화 신고다녀서 이번엔 어두운 색으로 사고 싶어요",
          "original_form": "한동안 흰운동화 신고다녀서 이번엔 어두운 색으로 사고 싶어요",
          "speaker_id": "2",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100037001.1.9",
          "form": "전 러닝화사고싶어요 편한걸루",
          "original_form": "전 러닝화사고싶어요 편한걸루",
          "speaker_id": "1",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100037001.1.10",
          "form": "그쳐 여름엔흰거신기좋은데 날쌀쌍해지니까 어두운거끌리네요",
          "original_form": "그쳐 여름엔흰거신기좋은데 날쌀쌍해지니까 어두운거끌리네요",
          "speaker_id": "1",
          "time": "20210831 16:52"
        },
        {
          "id": "MDRW2100037001.1.11",
          "form": "편한 신발이 최고더라구요",
          "original_form": "편한 신발이 최고더라구요",
          "speaker_id": "2",
          "time": "20210831 16:53"
        },
        {
          "id": "MDRW2100037001.1.12",
          "form": "네 아무래도 옷도 편한스타일많이입우니까 편한운동화를 자주구매하게되는둣여",
          "original_form": "네 아무래도 옷도 편한스타일많이입우니까 편한운동화를 자주구매하게되는둣여",
          "speaker_id": "1",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100037001.1.13",
          "form": "지마켓 스마일데이던데",
          "original_form": "지마켓 스마일데이던데",
          "speaker_id": "1",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100037001.1.14",
          "form": "지마켓은 잘 안 써봤는데 스마일클럽인가? 그거 쓰면 좋나요?",
          "original_form": "지마켓은 잘 안 써봤는데 스마일클럽인가? 그거 쓰면 좋나요?",
          "speaker_id": "2",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100037001.1.15",
          "form": "예전에썼었는데",
          "original_form": "예전에썼었는데",
          "speaker_id": "1",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100037001.1.16",
          "form": "이런빅세일기간에 좋아여",
          "original_form": "이런빅세일기간에 좋아여",
          "speaker_id": "1",
          "time": "20210831 16:57"
        },
        {
          "id": "MDRW2100037001.1.17",
          "form": "예쁜 거 있나 좀 찾아봐야겠어요",
          "original_form": "예쁜 거 있나 좀 찾아봐야겠어요",
          "speaker_id": "2",
          "time": "20210831 16:58"
        },
        {
          "id": "MDRW2100037001.1.18",
          "form": "네 그래도 가격일부러 올리는판매자들도있어서 가격비교하시는거 필수입니당",
          "original_form": "네 그래도 가격일부러 올리는판매자들도있어서 가격비교하시는거 필수입니당",
          "speaker_id": "1",
          "time": "20210831 16:59"
        }
