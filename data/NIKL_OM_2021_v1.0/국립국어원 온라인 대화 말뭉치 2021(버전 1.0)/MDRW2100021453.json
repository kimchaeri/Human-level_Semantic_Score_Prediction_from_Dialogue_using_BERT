      "utterance": [
        {
          "id": "MDRW2100021453.1.1",
          "form": "네 지금은 집에서 있어요",
          "original_form": "네 지금은 집에서 있어요",
          "speaker_id": "2",
          "time": "20210828 19:15"
        },
        {
          "id": "MDRW2100021453.1.2",
          "form": "집에선 주로 누가 집안일을 하시나요",
          "original_form": "집에선 주로 누가 집안일을 하시나요",
          "speaker_id": "1",
          "time": "20210828 19:17"
        },
        {
          "id": "MDRW2100021453.1.3",
          "form": "혼자 살아서 그 때는 제가 다 하고",
          "original_form": "혼자 살아서 그 때는 제가 다 하고",
          "speaker_id": "2",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100021453.1.4",
          "form": "주말에 본가오면 누워만있어요 ㅋㅋ",
          "original_form": "주말에 본가오면 누워만있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100021453.1.5",
          "form": "name1은 어떠신가요?",
          "original_form": "&name1&은 어떠신가요?",
          "speaker_id": "2",
          "time": "20210828 19:18"
        },
        {
          "id": "MDRW2100021453.1.6",
          "form": "저두요.. 불효자식이네요",
          "original_form": "저두요.. 불효자식이네요",
          "speaker_id": "1",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021453.1.7",
          "form": "집에서 설거지는 누가하시나요",
          "original_form": "집에서 설거지는 누가하시나요",
          "speaker_id": "1",
          "time": "20210828 19:19"
        },
        {
          "id": "MDRW2100021453.1.8",
          "form": "당연히 엄빠죠.. 전 손님인걸요...!",
          "original_form": "당연히 엄빠죠.. 전 손님인걸요...!",
          "speaker_id": "2",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021453.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ당 단",
          "original_form": "ㅋㅋㅋㅋㅋㅋ당 단",
          "speaker_id": "2",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021453.1.10",
          "form": "당 당ㅋㅋㅋㅋ",
          "original_form": "당 당ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:26"
        },
        {
          "id": "MDRW2100021453.1.11",
          "form": "zzzzzzzzz",
          "original_form": "zzzzzzzzz",
          "speaker_id": "1",
          "time": "20210828 19:29"
        },
        {
          "id": "MDRW2100021453.1.12",
          "form": "무료 하숙집",
          "original_form": "무료 하숙집",
          "speaker_id": "1",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021453.1.13",
          "form": "저도 엄마가 밥해주고 설거지하는곳으로  가고싶어요... 넘 머네요 집이",
          "original_form": "저도 엄마가 밥해주고 설거지하는곳으로  가고싶어요... 넘 머네요 집이",
          "speaker_id": "1",
          "time": "20210828 19:30"
        },
        {
          "id": "MDRW2100021453.1.14",
          "form": "맞아요 집이 정말 최고죠..",
          "original_form": "맞아요 집이 정말 최고죠..",
          "speaker_id": "2",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100021453.1.15",
          "form": "근데 무료는 아니에용..",
          "original_form": "근데 무료는 아니에용..",
          "speaker_id": "2",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100021453.1.16",
          "form": "요금은 잔소리로 받아요 ㅠ ㅠ",
          "original_form": "요금은 잔소리로 받아요 ㅠ ㅠ",
          "speaker_id": "2",
          "time": "20210828 20:09"
        },
        {
          "id": "MDRW2100021453.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ 그정도면 저렴하네요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그정도면 저렴하네요!",
          "speaker_id": "1",
          "time": "20210828 20:27"
        },
        {
          "id": "MDRW2100021453.1.18",
          "form": "흑흑 강력잔소리라면..? ㅋㅋㅋㅋㅋ",
          "original_form": "흑흑 강력잔소리라면..? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:57"
        },
        {
          "id": "MDRW2100021453.1.19",
          "form": "ㅋㅋㅋㅋㅋ 귀를 막읍시다",
          "original_form": "ㅋㅋㅋㅋㅋ 귀를 막읍시다",
          "speaker_id": "1",
          "time": "20210828 20:59"
        },
        {
          "id": "MDRW2100021453.1.20",
          "form": "맞아요.. 그게 답입니다 ㅋㅋㅋ 근데그래도 잔소리 싫긴한데",
          "original_form": "맞아요.. 그게 답입니다 ㅋㅋㅋ 근데그래도 잔소리 싫긴한데",
          "speaker_id": "2",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021453.1.21",
          "form": "본가 있다가 혼자사는 집 돌아가면",
          "original_form": "본가 있다가 혼자사는 집 돌아가면",
          "speaker_id": "2",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021453.1.22",
          "form": "너무조용해서 그게 또 슬프기도해요",
          "original_form": "너무조용해서 그게 또 슬프기도해요",
          "speaker_id": "2",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021453.1.23",
          "form": "그치만 또 조용해서 좋기도 한 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그치만 또 조용해서 좋기도 한 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021453.1.24",
          "form": "자취의 양면",
          "original_form": "자취의 양면",
          "speaker_id": "2",
          "time": "20210828 21:12"
        },
        {
          "id": "MDRW2100021453.1.25",
          "form": "마자요 조용하고 혼자인느낌 좋음..",
          "original_form": "마자요 조용하고 혼자인느낌 좋음..",
          "speaker_id": "1",
          "time": "20210828 21:17"
        },
        {
          "id": "MDRW2100021453.1.26",
          "form": "뭔가 근데 친구들 놀러와서 집에 와글와글하다가",
          "original_form": "뭔가 근데 친구들 놀러와서 집에 와글와글하다가",
          "speaker_id": "2",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021453.1.27",
          "form": "조용해지면 슬프지않아요? 그때뭔가 갑자기 훅 슬퍼요 ㅋㅋ",
          "original_form": "조용해지면 슬프지않아요? 그때뭔가 갑자기 훅 슬퍼요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100021453.1.28",
          "form": "같이잇던공간에 나만남을때 ㅋㅋㅋ ㅠㅠ",
