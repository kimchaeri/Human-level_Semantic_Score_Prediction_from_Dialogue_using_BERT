      "utterance": [
        {
          "id": "MDRW2100005503.1.1",
          "form": "우리집 name3",
          "original_form": "우리집 &name3&",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005503.1.2",
          "form": "어떻게하면",
          "original_form": "어떻게하면",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005503.1.3",
          "form": "배변훈련을 잘 시킬수있을까",
          "original_form": "배변훈련을 잘 시킬수있을까",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005503.1.4",
          "form": "이미 늦었다",
          "original_form": "이미 늦었다",
          "speaker_id": "2",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005503.1.5",
          "form": "저 개는 이미 우리 말을 개말로 알아들어서",
          "original_form": "저 개는 이미 우리 말을 개말로 알아들어서",
          "speaker_id": "2",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005503.1.6",
          "form": "패야 알아들을텐데",
          "original_form": "패야 알아들을텐데",
          "speaker_id": "2",
          "time": "20210821 22:43"
        },
        {
          "id": "MDRW2100005503.1.7",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005503.1.8",
          "form": "우리는 개가 아니고 인간인네",
          "original_form": "우리는 개가 아니고 인간인네",
          "speaker_id": "1",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005503.1.9",
          "form": "인데",
          "original_form": "인데",
          "speaker_id": "1",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005503.1.10",
          "form": "우리말을 왜 개말로 알아듣노",
          "original_form": "우리말을 왜 개말로 알아듣노",
          "speaker_id": "1",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005503.1.11",
          "form": "오줌 아무데나 안쌀거다",
          "original_form": "오줌 아무데나 안쌀거다",
          "speaker_id": "1",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005503.1.12",
          "form": "저 개는 이미 우리말을 한귀로 듣고 한귀로 흘리는 경지에 도달했다",
          "original_form": "저 개는 이미 우리말을 한귀로 듣고 한귀로 흘리는 경지에 도달했다",
          "speaker_id": "2",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005503.1.13",
          "form": "몽둥이로 교정하는게 최선이다",
          "original_form": "몽둥이로 교정하는게 최선이다",
          "speaker_id": "2",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005503.1.14",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005503.1.15",
          "form": "그럼 일본 훈련사가",
          "original_form": "그럼 일본 훈련사가",
          "speaker_id": "1",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005503.1.16",
          "form": "시바견 때린거처럼",
          "original_form": "시바견 때린거처럼",
          "speaker_id": "1",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005503.1.17",
          "form": "때려야 말을들으려나",
          "original_form": "때려야 말을들으려나",
          "speaker_id": "1",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005503.1.18",
          "form": "그러하다",
          "original_form": "그러하다",
          "speaker_id": "2",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005503.1.19",
          "form": "저 개를 교정하는 방법은 물리치료가 최선이다",
          "original_form": "저 개를 교정하는 방법은 물리치료가 최선이다",
          "speaker_id": "2",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005503.1.20",
          "form": "역시....",
          "original_form": "역시....",
          "speaker_id": "1",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005503.1.21",
          "form": "고대부터 이어온",
          "original_form": "고대부터 이어온",
          "speaker_id": "1",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005503.1.22",
          "form": "유구한 치료법은",
          "original_form": "유구한 치료법은",
          "speaker_id": "1",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005503.1.23",
          "form": "물리치료다",
          "original_form": "물리치료다",
          "speaker_id": "1",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005503.1.24",
          "form": "그렇다",
          "original_form": "그렇다",
          "speaker_id": "2",
          "time": "20210821 22:46"
        },
        {
          "id": "MDRW2100005503.1.25",
          "form": "선조들의 지혜다",
          "original_form": "선조들의 지혜다",
          "speaker_id": "2",
          "time": "20210821 22:47"
        },
        {
          "id": "MDRW2100005503.1.26",
          "form": "그렇군",
          "original_form": "그렇군",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005503.1.27",
          "form": "역시 말 안듣는 개는",
          "original_form": "역시 말 안듣는 개는",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005503.1.28",
          "form": "몽둥이찜질이 제일이다",
          "original_form": "몽둥이찜질이 제일이다",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005503.1.29",
          "form": "간식도 안주고",
          "original_form": "간식도 안주고",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005503.1.30",
          "form": "사료만 먹여야한다!",
          "original_form": "사료만 먹여야한다!",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005503.1.31",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005503.1.32",
          "form": "그게 최선이다",
          "original_form": "그게 최선이다",
          "speaker_id": "2",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005503.1.33",
          "form": "name3",
          "original_form": "&name3&",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005503.1.34",
          "form": "심장사상충 치료하느라",
          "original_form": "심장사상충 치료하느라",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005503.1.35",
          "form": "스케일링 하느라",
          "original_form": "스케일링 하느라",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005503.1.36",
          "form": "20name3 정도 들었다",
          "original_form": "20&name3& 정도 들었다",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005503.1.37",
          "form": "내돈~~~~!!",
          "original_form": "내돈~~~~!!",
          "speaker_id": "1",
          "time": "20210821 22:49"
        },
        {
          "id": "MDRW2100005503.1.38",
          "form": "ㅠㅠㅠㅠㅠㅠㅠ",
          "original_form": "ㅠㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005503.1.39",
          "form": "저 개는 돈먹는 하마임이 틀림없다",
          "original_form": "저 개는 돈먹는 하마임이 틀림없다",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005503.1.40",
          "form": "지금도 해맑게 웃고있다",
          "original_form": "지금도 해맑게 웃고있다",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005503.1.41",
          "form": "그렇다",
          "original_form": "그렇다",
          "speaker_id": "1",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005503.1.42",
          "form": "사실 개의 탈을 쓴",
          "original_form": "사실 개의 탈을 쓴",
          "speaker_id": "1",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005503.1.43",
          "form": "돈먹는 하마이다",
          "original_form": "돈먹는 하마이다",
          "speaker_id": "1",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005503.1.44",
          "form": "그러하다",
          "original_form": "그러하다",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005503.1.45",
          "form": "방금도 눈에 불을켜고",
          "original_form": "방금도 눈에 불을켜고",
          "speaker_id": "1",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005503.1.46",
          "form": "나의 치즈볼을 빼앗아먹으려했다",
          "original_form": "나의 치즈볼을 빼앗아먹으려했다",
          "speaker_id": "1",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005503.1.47",
          "form": "저렇게 악독한 개는 본적이 없다",
          "original_form": "저렇게 악독한 개는 본적이 없다",
          "speaker_id": "2",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005503.1.48",
          "form": "저 개한테 먹이는 사료도 비싼 사료다",
          "original_form": "저 개한테 먹이는 사료도 비싼 사료다",
          "speaker_id": "2",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005503.1.49",
          "form": "저 개는 비싼 사료가 아니면 안쳐먹는다",
          "original_form": "저 개는 비싼 사료가 아니면 안쳐먹는다",
          "speaker_id": "2",
          "time": "20210821 22:53"
        }
