      "utterance": [
        {
          "id": "MDRW2100004093.1.1",
          "form": "날이 시원하네요",
          "original_form": "날이 시원하네요",
          "speaker_id": "1",
          "time": "20210821 14:54"
        },
        {
          "id": "MDRW2100004093.1.2",
          "form": "아침에 비가 오더니 많이 시원해졌어요",
          "original_form": "아침에 비가 오더니 많이 시원해졌어요",
          "speaker_id": "2",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004093.1.3",
          "form": "비오는 거 보셨나요?",
          "original_form": "비오는 거 보셨나요?",
          "speaker_id": "2",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004093.1.4",
          "form": "네 퍼붓더라고요",
          "original_form": "네 퍼붓더라고요",
          "speaker_id": "1",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004093.1.5",
          "form": "지금은 좀 갰네요",
          "original_form": "지금은 좀 갰네요",
          "speaker_id": "1",
          "time": "20210821 14:55"
        },
        {
          "id": "MDRW2100004093.1.6",
          "form": "그리고 확실히 시원해졌어요",
          "original_form": "그리고 확실히 시원해졌어요",
          "speaker_id": "2",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004093.1.7",
          "form": "이제 여름도 끝나가네요",
          "original_form": "이제 여름도 끝나가네요",
          "speaker_id": "2",
          "time": "20210821 14:57"
        },
        {
          "id": "MDRW2100004093.1.8",
          "form": "그러게요 벌써 가을이라니",
          "original_form": "그러게요 벌써 가을이라니",
          "speaker_id": "1",
          "time": "20210821 14:58"
        },
        {
          "id": "MDRW2100004093.1.9",
          "form": "내일도 비가 올 수도 있다네요",
          "original_form": "내일도 비가 올 수도 있다네요",
          "speaker_id": "2",
          "time": "20210821 15:02"
        },
        {
          "id": "MDRW2100004093.1.10",
          "form": "이번 여름엔 그래도 비가 꽤 온 거 같아요",
          "original_form": "이번 여름엔 그래도 비가 꽤 온 거 같아요",
          "speaker_id": "2",
          "time": "20210821 15:03"
        },
        {
          "id": "MDRW2100004093.1.11",
          "form": "이제 장마시작인거같아요",
          "original_form": "이제 장마시작인거같아요",
          "speaker_id": "1",
          "time": "20210821 15:04"
        },
        {
          "id": "MDRW2100004093.1.12",
          "form": "주말엔 집에 있어야겠어요",
          "original_form": "주말엔 집에 있어야겠어요",
          "speaker_id": "2",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004093.1.13",
          "form": "그래야겠어요",
          "original_form": "그래야겠어요",
          "speaker_id": "1",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004093.1.14",
          "form": "그래도 바람 시원하니 좋네요",
          "original_form": "그래도 바람 시원하니 좋네요",
          "speaker_id": "1",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100004093.1.15",
          "form": "정말 더울 때는 창문 닫고 에어컨 틀어야했는데 요즘에는 창문만 열어놓으면 시원해서 좋아요",
          "original_form": "정말 더울 때는 창문 닫고 에어컨 틀어야했는데 요즘에는 창문만 열어놓으면 시원해서 좋아요",
          "speaker_id": "2",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004093.1.16",
          "form": "이번 여름에 정말 더웠죠",
          "original_form": "이번 여름에 정말 더웠죠",
          "speaker_id": "2",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004093.1.17",
          "form": "맞아요 지금은 창문에서 에어컨바람이 솔솔",
          "original_form": "맞아요 지금은 창문에서 에어컨바람이 솔솔",
          "speaker_id": "1",
          "time": "20210821 15:15"
        },
        {
          "id": "MDRW2100004093.1.18",
          "form": "더울땐 싫었는데 이젠 추위걱정하게생겼어요ㅋㅋ",
          "original_form": "더울땐 싫었는데 이젠 추위걱정하게생겼어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:16"
        },
        {
          "id": "MDRW2100004093.1.19",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004093.1.20",
          "form": "그래도 이번 여름이 너무 더워서 겨울도 별로 안 추울 거라고 하는데",
          "original_form": "그래도 이번 여름이 너무 더워서 겨울도 별로 안 추울 거라고 하는데",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004093.1.21",
          "form": "그래도 매번 덜춥다해도 춥게 느껴지더라고요",
          "original_form": "그래도 매번 덜춥다해도 춥게 느껴지더라고요",
          "speaker_id": "2",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004093.1.22",
          "form": "겨울은 겨울이라..ㅎㅎ",
          "original_form": "겨울은 겨울이라..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:22"
        }
      ]
