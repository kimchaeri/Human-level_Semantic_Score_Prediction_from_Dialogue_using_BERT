      "utterance": [
        {
          "id": "MDRW2100019212.1.1",
          "form": "반항기인가봐...",
          "original_form": "반항기인가봐...",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019212.1.2",
          "form": "어쩌려는 건지 모르겠음",
          "original_form": "어쩌려는 건지 모르겠음",
          "speaker_id": "1",
          "time": "20210827 16:18"
        },
        {
          "id": "MDRW2100019212.1.3",
          "form": "아니... 공부 안하면",
          "original_form": "아니... 공부 안하면",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019212.1.4",
          "form": "내 동생놈처럼 될텐데ㅔ",
          "original_form": "내 동생놈처럼 될텐데ㅔ",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019212.1.5",
          "form": "저놈 여전히 공부 안해",
          "original_form": "저놈 여전히 공부 안해",
          "speaker_id": "2",
          "time": "20210827 16:20"
        },
        {
          "id": "MDRW2100019212.1.6",
          "form": "하 조금 화나",
          "original_form": "하 조금 화나",
          "speaker_id": "1",
          "time": "20210827 16:21"
        },
        {
          "id": "MDRW2100019212.1.7",
          "form": "연락도 없이 그냥 안 왔음...",
          "original_form": "연락도 없이 그냥 안 왔음...",
          "speaker_id": "1",
          "time": "20210827 16:22"
        },
        {
          "id": "MDRW2100019212.1.8",
          "form": "이건 진짜 예의가 아니지 않아..?",
          "original_form": "이건 진짜 예의가 아니지 않아..?",
          "speaker_id": "1",
          "time": "20210827 16:22"
        },
        {
          "id": "MDRW2100019212.1.9",
          "form": "???",
          "original_form": "???",
          "speaker_id": "2",
          "time": "20210827 16:22"
        },
        {
          "id": "MDRW2100019212.1.10",
          "form": "그건 좀 에바",
          "original_form": "그건 좀 에바",
          "speaker_id": "2",
          "time": "20210827 16:22"
        },
        {
          "id": "MDRW2100019212.1.11",
          "form": "어차피 같은 아파트라",
          "original_form": "어차피 같은 아파트라",
          "speaker_id": "1",
          "time": "20210827 16:22"
        },
        {
          "id": "MDRW2100019212.1.12",
          "form": "연락 다 씹길래 문 따고 들어감",
          "original_form": "연락 다 씹길래 문 따고 들어감",
          "speaker_id": "1",
          "time": "20210827 16:23"
        },
        {
          "id": "MDRW2100019212.1.13",
          "form": "잘했네",
          "original_form": "잘했네",
          "speaker_id": "2",
          "time": "20210827 16:23"
        },
        {
          "id": "MDRW2100019212.1.14",
          "form": "방 문 잠그고 침대에서 자고 있더라고",
          "original_form": "방 문 잠그고 침대에서 자고 있더라고",
          "speaker_id": "1",
          "time": "20210827 16:23"
        },
        {
          "id": "MDRW2100019212.1.15",
          "form": "내가 볼 때는 자는 척이긴 한데...",
          "original_form": "내가 볼 때는 자는 척이긴 한데...",
          "speaker_id": "1",
          "time": "20210827 16:23"
        },
        {
          "id": "MDRW2100019212.1.16",
          "form": "왜그래",
          "original_form": "왜그래",
          "speaker_id": "2",
          "time": "20210827 16:24"
        },
        {
          "id": "MDRW2100019212.1.17",
          "form": "솔직히 2시 반에 줌 수업 끝나고 3시에 과외 수업인데 그 사이에 잘 생각을 하는 거부터가 말이 안 되지 않아? ㅋㅋㅋ",
          "original_form": "솔직히 2시 반에 줌 수업 끝나고 3시에 과외 수업인데 그 사이에 잘 생각을 하는 거부터가 말이 안 되지 않아? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:24"
        },
        {
          "id": "MDRW2100019212.1.18",
          "form": "그지",
          "original_form": "그지",
          "speaker_id": "2",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019212.1.19",
          "form": "진짜 어이없어...",
          "original_form": "진짜 어이없어...",
          "speaker_id": "1",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019212.1.20",
          "form": "정말 왜 그러는거야",
          "original_form": "정말 왜 그러는거야",
          "speaker_id": "2",
          "time": "20210827 16:25"
        },
        {
          "id": "MDRW2100019212.1.21",
          "form": "그냥 무음으로 해놓고 침대에 누운 거부터가 과외  할 생각이 없었던 거 같아서",
          "original_form": "그냥 무음으로 해놓고 침대에 누운 거부터가 과외  할 생각이 없었던 거 같아서",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019212.1.22",
          "form": "그냥 피곤하면 더 자라고 내일 보충하자고 하고 집에 왔음ㅠㅠ",
          "original_form": "그냥 피곤하면 더 자라고 내일 보충하자고 하고 집에 왔음ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019212.1.23",
          "form": "에구...",
          "original_form": "에구...",
          "speaker_id": "2",
          "time": "20210827 16:26"
        },
        {
          "id": "MDRW2100019212.1.24",
          "form": "이러다가 과외 잘릴까봐 무섭다",
          "original_form": "이러다가 과외 잘릴까봐 무섭다",
          "speaker_id": "1",
