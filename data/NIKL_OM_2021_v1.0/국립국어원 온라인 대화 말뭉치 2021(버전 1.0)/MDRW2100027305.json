      "utterance": [
        {
          "id": "MDRW2100027305.1.1",
          "form": "정모해본적있음?",
          "original_form": "정모해본적있음?",
          "speaker_id": "1",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100027305.1.2",
          "form": "오늘 커뮤니티서 매점남 짤 줍줍함",
          "original_form": "오늘 커뮤니티서 매점남 짤 줍줍함",
          "speaker_id": "1",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100027305.1.3",
          "form": "한 번도 없어요",
          "original_form": "한 번도 없어요",
          "speaker_id": "2",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100027305.1.4",
          "form": "풀었다 그 커뮤니티 최고다",
          "original_form": "풀었다 그 커뮤니티 최고다",
          "speaker_id": "2",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100027305.1.5",
          "form": "며칠전엔 망내짤 주움",
          "original_form": "며칠전엔 망내짤 주움",
          "speaker_id": "1",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100027305.1.6",
          "form": "없다 부럽다 부럽다 부럽다",
          "original_form": "없다 부럽다 부럽다 부럽다",
          "speaker_id": "2",
          "time": "20210902 23:54"
        },
        {
          "id": "MDRW2100027305.1.7",
          "form": "우리 꼬리는 왜",
          "original_form": "우리 꼬리는 왜",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100027305.1.8",
          "form": "고양이 강아지 사진 모으는 목적",
          "original_form": "고양이 강아지 사진 모으는 목적",
          "speaker_id": "1",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100027305.1.9",
          "form": "우리  무니도 좀",
          "original_form": "우리  무니도 좀",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100027305.1.10",
          "form": "커뮤니티 좋음",
          "original_form": "커뮤니티 좋음",
          "speaker_id": "1",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100027305.1.11",
          "form": "요새 무늬가 기운이 없어서 마음이 아파요",
          "original_form": "요새 무늬가 기운이 없어서 마음이 아파요",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100027305.1.12",
          "form": "검색해봐야함",
          "original_form": "검색해봐야함",
          "speaker_id": "1",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100027305.1.13",
          "form": "커뮤니티는 가입하는 게 까다로 와요",
          "original_form": "커뮤니티는 가입하는 게 까다로 와요",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100027305.1.14",
          "form": "랜선집사는 커뮤니티 필수",
          "original_form": "랜선집사는 커뮤니티 필수",
          "speaker_id": "1",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100027305.1.15",
          "form": "정회원 업로드",
          "original_form": "정회원 업로드",
          "speaker_id": "2",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100027305.1.16",
          "form": "가입보다는 등업이 헬",
          "original_form": "가입보다는 등업이 헬",
          "speaker_id": "1",
          "time": "20210902 23:55"
        },
        {
          "id": "MDRW2100027305.1.17",
          "form": "그래서 대부분 커뮤니티는 포기하는 것 같아요",
          "original_form": "그래서 대부분 커뮤니티는 포기하는 것 같아요",
          "speaker_id": "2",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100027305.1.18",
          "form": "좋은 커뮤 추천",
          "original_form": "좋은 커뮤 추천",
          "speaker_id": "1",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100027305.1.19",
          "form": "등급 법이 쉬워요",
          "original_form": "등급 법이 쉬워요",
          "speaker_id": "2",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100027305.1.20",
          "form": "나도 힘들게 한데있음",
          "original_form": "나도 힘들게 한데있음",
          "speaker_id": "1",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100027305.1.21",
          "form": "근데유용",
          "original_form": "근데유용",
          "speaker_id": "1",
          "time": "20210902 23:56"
        },
        {
          "id": "MDRW2100027305.1.22",
          "form": "거기만 감",
          "original_form": "거기만 감",
          "speaker_id": "1",
          "time": "20210902 23:56"
        },
        {
