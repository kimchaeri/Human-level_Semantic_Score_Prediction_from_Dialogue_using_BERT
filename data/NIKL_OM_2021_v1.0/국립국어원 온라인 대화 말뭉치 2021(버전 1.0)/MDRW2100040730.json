      "utterance": [
        {
          "id": "MDRW2100040730.1.1",
          "form": "반려동물은 키우세요?",
          "original_form": "반려동물은 키우세요?",
          "speaker_id": "1",
          "time": "20210905 01:04"
        },
        {
          "id": "MDRW2100040730.1.2",
          "form": "아뇨 name1님은요?",
          "original_form": "아뇨 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 01:05"
        },
        {
          "id": "MDRW2100040730.1.3",
          "form": "저는 구피키우고있어요",
          "original_form": "저는 구피키우고있어요",
          "speaker_id": "1",
          "time": "20210905 19:09"
        },
        {
          "id": "MDRW2100040730.1.4",
          "form": "강아지 키우고싶은데 아직은 자신이없네요ㅋ",
          "original_form": "강아지 키우고싶은데 아직은 자신이없네요ㅋ",
          "speaker_id": "1",
          "time": "20210905 19:09"
        },
        {
          "id": "MDRW2100040730.1.5",
          "form": "구피는 몇마리나 키우세요?",
          "original_form": "구피는 몇마리나 키우세요?",
          "speaker_id": "2",
          "time": "20210905 19:12"
        },
        {
          "id": "MDRW2100040730.1.6",
          "form": "5마리키웠다가 지금은 4마리있어요ㅋ",
          "original_form": "5마리키웠다가 지금은 4마리있어요ㅋ",
          "speaker_id": "1",
          "time": "20210905 19:12"
        },
        {
          "id": "MDRW2100040730.1.7",
          "form": "원래는 수십마리키웠는데",
          "original_form": "원래는 수십마리키웠는데",
          "speaker_id": "1",
          "time": "20210905 19:13"
        },
        {
          "id": "MDRW2100040730.1.8",
          "form": "감당이 안되서요ㅋㅋ다 분양하고",
          "original_form": "감당이 안되서요ㅋㅋ다 분양하고",
          "speaker_id": "1",
          "time": "20210905 19:13"
        },
        {
          "id": "MDRW2100040730.1.9",
          "form": "지금4마리있어요ㅋ",
          "original_form": "지금4마리있어요ㅋ",
          "speaker_id": "1",
          "time": "20210905 19:13"
        },
        {
          "id": "MDRW2100040730.1.10",
          "form": "수십마리라니ㅋㅋ 근데 암수구별이 딱 되나요?",
          "original_form": "수십마리라니ㅋㅋ 근데 암수구별이 딱 되나요?",
          "speaker_id": "2",
          "time": "20210905 19:18"
        },
        {
          "id": "MDRW2100040730.1.11",
          "form": "네네ㅋㅋ구분되더라구요ㅋ",
          "original_form": "네네ㅋㅋ구분되더라구요ㅋ",
          "speaker_id": "1",
          "time": "20210905 19:40"
        },
        {
          "id": "MDRW2100040730.1.12",
          "form": "색깔이달랐어요ㅋ",
          "original_form": "색깔이달랐어요ㅋ",
          "speaker_id": "1",
          "time": "20210905 19:40"
        },
        {
          "id": "MDRW2100040730.1.13",
          "form": "예전에 새 키웠었는데 걔들은 암수구분이 힘들더라구요 ㅋㅋ 알 낳으면 암컷..ㅋㅋ",
          "original_form": "예전에 새 키웠었는데 걔들은 암수구분이 힘들더라구요 ㅋㅋ 알 낳으면 암컷..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:55"
        },
        {
          "id": "MDRW2100040730.1.14",
          "form": "오오 새..ㅋㅋ대박인데요 ㅋㅋ",
          "original_form": "오오 새..ㅋㅋ대박인데요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100040730.1.15",
          "form": "제 지인도 앵무새 키우는데 이것 저것 신경쓰이는게 많더라구요",
          "original_form": "제 지인도 앵무새 키우는데 이것 저것 신경쓰이는게 많더라구요",
          "speaker_id": "1",
          "time": "20210905 19:56"
        },
        {
          "id": "MDRW2100040730.1.16",
          "form": "반려동물 키우려면 진짜 신경 쓸 것 많은 것 같아요",
          "original_form": "반려동물 키우려면 진짜 신경 쓸 것 많은 것 같아요",
          "speaker_id": "2",
          "time": "20210905 19:58"
        },
        {
          "id": "MDRW2100040730.1.17",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100040730.1.18",
          "form": "앵무새가 한번 아프니깐 자주 아프더라구요 ㅠ",
          "original_form": "앵무새가 한번 아프니깐 자주 아프더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100040730.1.19",
          "form": "동물병원 가기도 어려울 거구요ㅠ",
          "original_form": "동물병원 가기도 어려울 거구요ㅠ",
          "speaker_id": "2",
          "time": "20210905 20:04"
        },
        {
          "id": "MDRW2100040730.1.20",
          "form": "맞아요 ㅠ동물병원도 근처에 없어서 난감해하더라구요.",
          "original_form": "맞아요 ㅠ동물병원도 근처에 없어서 난감해하더라구요.",
          "speaker_id": "1",
          "time": "20210905 20:07"
        },
        {
          "id": "MDRW2100040730.1.21",
          "form": "안 아프고 오래오래 함께 할 수 있었으면 좋겠어요 ㅠ",
          "original_form": "안 아프고 오래오래 함께 할 수 있었으면 좋겠어요 ㅠ",
          "speaker_id": "2",
          "time": "20210905 20:09"
        }
      ]
    }
