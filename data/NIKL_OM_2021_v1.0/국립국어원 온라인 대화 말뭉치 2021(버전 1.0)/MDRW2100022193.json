      "utterance": [
        {
          "id": "MDRW2100022193.1.1",
          "form": "직장 생활은 어떠신지요",
          "original_form": "직장 생활은 어떠신지요",
          "speaker_id": "1",
          "time": "20210829 10:50"
        },
        {
          "id": "MDRW2100022193.1.2",
          "form": "음 루즈합니다",
          "original_form": "음 루즈합니다",
          "speaker_id": "2",
          "time": "20210829 10:50"
        },
        {
          "id": "MDRW2100022193.1.3",
          "form": "특별이 성과를 요구하는 직종이 아니라서",
          "original_form": "특별이 성과를 요구하는 직종이 아니라서",
          "speaker_id": "2",
          "time": "20210829 10:50"
        },
        {
          "id": "MDRW2100022193.1.4",
          "form": "주부이신가요?",
          "original_form": "주부이신가요?",
          "speaker_id": "2",
          "time": "20210829 10:51"
        },
        {
          "id": "MDRW2100022193.1.5",
          "form": "네 네 그렇습니다",
          "original_form": "네 네 그렇습니다",
          "speaker_id": "1",
          "time": "20210829 10:51"
        },
        {
          "id": "MDRW2100022193.1.6",
          "form": "그러면 회사에 가서 쉬면 되겠군요",
          "original_form": "그러면 회사에 가서 쉬면 되겠군요",
          "speaker_id": "1",
          "time": "20210829 10:51"
        },
        {
          "id": "MDRW2100022193.1.7",
          "form": "회사가면 애기 안봐서 쉰다는게 맞는거 같아요",
          "original_form": "회사가면 애기 안봐서 쉰다는게 맞는거 같아요",
          "speaker_id": "2",
          "time": "20210829 10:51"
        },
        {
          "id": "MDRW2100022193.1.8",
          "form": "회사가서 쉽니다",
          "original_form": "회사가서 쉽니다",
          "speaker_id": "2",
          "time": "20210829 10:51"
        },
        {
          "id": "MDRW2100022193.1.9",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 10:51"
        },
        {
          "id": "MDRW2100022193.1.10",
          "form": "최근에",
          "original_form": "최근에",
          "speaker_id": "2",
          "time": "20210829 10:51"
        },
        {
          "id": "MDRW2100022193.1.11",
          "form": "맞습니다 애 보는 게 쉽진 않죠ㅋㅋㅋㅋ",
          "original_form": "맞습니다 애 보는 게 쉽진 않죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:52"
        },
        {
          "id": "MDRW2100022193.1.12",
          "form": "드라마 미치지않고서야 라는",
          "original_form": "드라마 미치지않고서야 라는",
          "speaker_id": "2",
          "time": "20210829 10:52"
        },
        {
          "id": "MDRW2100022193.1.13",
          "form": "거기 촬영지가 저희 연구원이었습니다",
          "original_form": "거기 촬영지가 저희 연구원이었습니다",
          "speaker_id": "2",
          "time": "20210829 10:52"
        },
        {
          "id": "MDRW2100022193.1.14",
          "form": "연애인도 보고 신기했습니다",
          "original_form": "연애인도 보고 신기했습니다",
          "speaker_id": "2",
          "time": "20210829 10:52"
        },
        {
          "id": "MDRW2100022193.1.15",
          "form": "애보는거 대단하더라구요",
          "original_form": "애보는거 대단하더라구요",
          "speaker_id": "2",
          "time": "20210829 10:52"
        },
        {
          "id": "MDRW2100022193.1.16",
          "form": "주부님들 정말 대단합니다",
          "original_form": "주부님들 정말 대단합니다",
          "speaker_id": "2",
          "time": "20210829 10:52"
        },
        {
          "id": "MDRW2100022193.1.17",
          "form": "응원하구요",
          "original_form": "응원하구요",
          "speaker_id": "2",
          "time": "20210829 10:52"
        },
        {
          "id": "MDRW2100022193.1.18",
          "form": "그래서 드라마를 못봤습니다ㅠ 어떤 연예인이 왔던가요",
          "original_form": "그래서 드라마를 못봤습니다ㅠ 어떤 연예인이 왔던가요",
          "speaker_id": "1",
          "time": "20210829 10:52"
        },
        {
          "id": "MDRW2100022193.1.19",
          "form": "문소리 이상엽 안내상",
          "original_form": "문소리 이상엽 안내상",
          "speaker_id": "2",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100022193.1.20",
          "form": "아 네 그렇군요",
          "original_form": "아 네 그렇군요",
          "speaker_id": "1",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100022193.1.21",
          "form": "저는 문소리 배우를 좋아해서",
          "original_form": "저는 문소리 배우를 좋아해서",
          "speaker_id": "2",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100022193.1.22",
          "form": "암튼 그렇습니다 ㅎ",
          "original_form": "암튼 그렇습니다 ㅎ",
          "speaker_id": "2",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100022193.1.23",
          "form": "육아에 참여를 많이 하니",
          "original_form": "육아에 참여를 많이 하니",
          "speaker_id": "1",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100022193.1.24",
          "form": "힘든 것도 아는 것 같습니다",
          "original_form": "힘든 것도 아는 것 같습니다",
          "speaker_id": "1",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100022193.1.25",
          "form": "애 보고 논다고 생각하는 남자들이 많습니다",
          "original_form": "애 보고 논다고 생각하는 남자들이 많습니다",
          "speaker_id": "1",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100022193.1.26",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100022193.1.27",
          "form": "혼 꾸녕이 나야 합니다",
          "original_form": "혼 꾸녕이 나야 합니다",
          "speaker_id": "1",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100022193.1.28",
          "form": "정신나간 사람들입니다",
          "original_form": "정신나간 사람들입니다",
          "speaker_id": "2",
          "time": "20210829 10:53"
        },
        {
          "id": "MDRW2100022193.1.29",
          "form": "해봐야 됩니다",
          "original_form": "해봐야 됩니다",
          "speaker_id": "2",
          "time": "20210829 10:54"
        },
        {
          "id": "MDRW2100022193.1.30",
          "form": "금토는 제가 데리고 자는데",
          "original_form": "금토는 제가 데리고 자는데",
          "speaker_id": "2",
          "time": "20210829 10:54"
        },
        {
          "id": "MDRW2100022193.1.31",
          "form": "아직 통잠을 안자서",
          "original_form": "아직 통잠을 안자서",
          "speaker_id": "2",
          "time": "20210829 10:54"
        },
        {
          "id": "MDRW2100022193.1.32",
          "form": "맞습니다 잘하고 계심 잘 하고 계십니다",
          "original_form": "맞습니다 잘하고 계심 잘 하고 계십니다",
          "speaker_id": "1",
          "time": "20210829 10:54"
        }
      ]
    }
  ]
