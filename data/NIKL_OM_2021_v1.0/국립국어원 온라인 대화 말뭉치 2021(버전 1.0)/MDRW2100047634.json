      "utterance": [
        {
          "id": "MDRW2100047634.1.1",
          "form": "도마뱀은 오랫동안 기르셧나요?",
          "original_form": "도마뱀은 오랫동안 기르셧나요?",
          "speaker_id": "2",
          "time": "20210826 22:57"
        },
        {
          "id": "MDRW2100047634.1.2",
          "form": "2년요",
          "original_form": "2년요",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100047634.1.3",
          "form": "강아지 말고 키우고 싶은거 있으세요",
          "original_form": "강아지 말고 키우고 싶은거 있으세요",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100047634.1.4",
          "form": "키우고 있는 식물이나요",
          "original_form": "키우고 있는 식물이나요",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100047634.1.5",
          "form": "저도 초보입니다",
          "original_form": "저도 초보입니다",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100047634.1.6",
          "form": "아이들이 키우자 했는데 이젠 제가 키워요",
          "original_form": "아이들이 키우자 했는데 이젠 제가 키워요",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100047634.1.7",
          "form": "고양이도 좋아해요 ㅋㅋㅋㅋㅋ",
          "original_form": "고양이도 좋아해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100047634.1.8",
          "form": "오 저희딸도요",
          "original_form": "오 저희딸도요",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100047634.1.9",
          "form": "너무 귀여워요",
          "original_form": "너무 귀여워요",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100047634.1.10",
          "form": "맞아요 전 털알레르기가 있어서",
          "original_form": "맞아요 전 털알레르기가 있어서",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100047634.1.11",
          "form": "못길러요",
          "original_form": "못길러요",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100047634.1.12",
          "form": "아이가 좋아해서 강아지 카페",
          "original_form": "아이가 좋아해서 강아지 카페",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100047634.1.13",
          "form": "잘갔는데 코로나로",
          "original_form": "잘갔는데 코로나로",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100047634.1.14",
          "form": "다 사라졌어요",
          "original_form": "다 사라졌어요",
          "speaker_id": "1",
          "time": "20210826 23:10"
        },
        {
          "id": "MDRW2100047634.1.15",
          "form": "코로나 때문에 사라진게 많죠 ㅠㅠ",
          "original_form": "코로나 때문에 사라진게 많죠 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100047634.1.16",
          "form": "털 알러지가 있으시구나 ㅠㅠ",
          "original_form": "털 알러지가 있으시구나 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100047634.1.17",
          "form": "네 가면 약먹소 가고 그랬어요",
          "original_form": "네 가면 약먹소 가고 그랬어요",
          "speaker_id": "1",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100047634.1.18",
          "form": "네 전부 사라졌어요",
          "original_form": "네 전부 사라졌어요",
          "speaker_id": "1",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100047634.1.19",
          "form": "걱정인개 거기 있던 강라지들 어찌된지 모르겠어요",
          "original_form": "걱정인개 거기 있던 강라지들 어찌된지 모르겠어요",
          "speaker_id": "1",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100047634.1.20",
          "form": "ㅠㅠ 그러게요...",
          "original_form": "ㅠㅠ 그러게요...",
          "speaker_id": "2",
          "time": "20210826 23:16"
        },
        {
          "id": "MDRW2100047634.1.21",
          "form": "책임질 줄 아는 주인이어야 할텐데 ㅠㅠㅠ",
          "original_form": "책임질 줄 아는 주인이어야 할텐데 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100047634.1.22",
          "form": "키우고 있는지 어쩐지",
          "original_form": "키우고 있는지 어쩐지",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100047634.1.23",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100047634.1.24",
          "form": "궁금해요 한상",
          "original_form": "궁금해요 한상",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100047634.1.25",
          "form": "항상",
          "original_form": "항상",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100047634.1.26",
          "form": "나중에 깔끔하게 관리하고 잘키우실것 같아요",
          "original_form": "나중에 깔끔하게 관리하고 잘키우실것 같아요",
          "speaker_id": "1",
          "time": "20210826 23:17"
        },
        {
          "id": "MDRW2100047634.1.27",
          "form": "도마뱀 집이 전 매장에 냄새 나길해",
          "original_form": "도마뱀 집이 전 매장에 냄새 나길해",
          "speaker_id": "1",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100047634.1.28",
          "form": "원래 그런가 했는데",
          "original_form": "원래 그런가 했는데",
          "speaker_id": "1",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100047634.1.29",
          "form": "그개 주인이 잘관리하면 안나요",
          "original_form": "그개 주인이 잘관리하면 안나요",
          "speaker_id": "1",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100047634.1.30",
          "form": "뭐든 깔끔히 해주면 아이들도 좋고요",
          "original_form": "뭐든 깔끔히 해주면 아이들도 좋고요",
          "speaker_id": "1",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100047634.1.31",
          "form": "잘하실것 같아요",
          "original_form": "잘하실것 같아요",
          "speaker_id": "1",
          "time": "20210826 23:18"
        },
        {
          "id": "MDRW2100047634.1.32",
          "form": "전 항상 바로바로 똥치우고 관리해요",
          "original_form": "전 항상 바로바로 똥치우고 관리해요",
          "speaker_id": "1",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100047634.1.33",
          "form": "엄청 부지런해야 할 것 같아요",
          "original_form": "엄청 부지런해야 할 것 같아요",
          "speaker_id": "2",
          "time": "20210826 23:19"
        },
        {
          "id": "MDRW2100047634.1.34",
          "form": "저는 도마뱀 귀여운데 무서워요 ㅋㅋㅋㅋ",
          "original_form": "저는 도마뱀 귀여운데 무서워요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100047634.1.35",
          "form": "네 그렇죠",
          "original_form": "네 그렇죠",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100047634.1.36",
          "form": "좀무섭긴해요",
          "original_form": "좀무섭긴해요",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100047634.1.37",
          "form": "발톱이 아파요 ㅜㅜ",
          "original_form": "발톱이 아파요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 23:22"
        },
        {
          "id": "MDRW2100047634.1.38",
          "form": "발톱이 많이 날카로워요?",
          "original_form": "발톱이 많이 날카로워요?",
          "speaker_id": "2",
          "time": "20210826 23:23"
        },
        {
          "id": "MDRW2100047634.1.39",
          "form": "네 자국나요",
          "original_form": "네 자국나요",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100047634.1.40",
          "form": "깍아주는데도 그래요 ㅋㅋ",
          "original_form": "깍아주는데도 그래요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100047634.1.41",
          "form": "용발톱 ㅋㅋ",
          "original_form": "용발톱 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:24"
        },
        {
          "id": "MDRW2100047634.1.42",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "2",
          "time": "20210826 23:28"
        }
      ]
