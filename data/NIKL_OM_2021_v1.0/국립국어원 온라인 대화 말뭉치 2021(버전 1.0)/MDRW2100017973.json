      "utterance": [
        {
          "id": "MDRW2100017973.1.1",
          "form": "오늘 저녁 뭐드셨어요?",
          "original_form": "오늘 저녁 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017973.1.2",
          "form": "파리바게트 빵 먹었네요 ㅎㅎ",
          "original_form": "파리바게트 빵 먹었네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017973.1.3",
          "form": "도넛이랑 치즈케잌도 같이 먹었어요~",
          "original_form": "도넛이랑 치즈케잌도 같이 먹었어요~",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017973.1.4",
          "form": "파리바게트 치즈케잌 맛있어요!",
          "original_form": "파리바게트 치즈케잌 맛있어요!",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017973.1.5",
          "form": "전보다 맛있어진 것 같아요",
          "original_form": "전보다 맛있어진 것 같아요",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017973.1.6",
          "form": "전 작은거 동그란거 샀는데..",
          "original_form": "전 작은거 동그란거 샀는데..",
          "speaker_id": "1",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100017973.1.7",
          "form": "예전보다 별로더라구요ㅜㅜ",
          "original_form": "예전보다 별로더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100017973.1.8",
          "form": "아..ㅠ 그거 진짜 맛있었는데 말이죠",
          "original_form": "아..ㅠ 그거 진짜 맛있었는데 말이죠",
          "speaker_id": "2",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100017973.1.9",
          "form": "빵 좋아하세요?",
          "original_form": "빵 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100017973.1.10",
          "form": "네 빵 미쳐요",
          "original_form": "네 빵 미쳐요",
          "speaker_id": "1",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017973.1.11",
          "form": "근데 빵값 너무 비싸요 진짜..",
          "original_form": "근데 빵값 너무 비싸요 진짜..",
          "speaker_id": "1",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017973.1.12",
          "form": "맞아요.. 저번에 몇 개 안골랐는데 밥값을 뛰어넘더라구요",
          "original_form": "맞아요.. 저번에 몇 개 안골랐는데 밥값을 뛰어넘더라구요",
          "speaker_id": "2",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017973.1.13",
          "form": "그런거 보면 차라리 국밥 먹는 게 낫겠다 싶어요",
          "original_form": "그런거 보면 차라리 국밥 먹는 게 낫겠다 싶어요",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017973.1.14",
          "form": "갑자기 국밥이 확 당기네 ㅜㅜ",
          "original_form": "갑자기 국밥이 확 당기네 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017973.1.15",
          "form": "아 말하시니 저도..ㅋㅋ",
          "original_form": "아 말하시니 저도..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017973.1.16",
          "form": "국밥에 보쌈 먹고싶어요",
          "original_form": "국밥에 보쌈 먹고싶어요",
          "speaker_id": "2",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017973.1.17",
          "form": "어떤 국밥 좋아하세요?",
          "original_form": "어떤 국밥 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017973.1.18",
          "form": "수육 국밥 최고!",
          "original_form": "수육 국밥 최고!",
          "speaker_id": "1",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017973.1.19",
          "form": "저도 수육이요!",
          "original_form": "저도 수육이요!",
          "speaker_id": "2",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017973.1.20",
          "form": "전에는 순대국밥이 좋았는데 깔끔한게 좋아지네요",
          "original_form": "전에는 순대국밥이 좋았는데 깔끔한게 좋아지네요",
          "speaker_id": "2",
          "time": "20210826 22:05"
        },
        {
          "id": "MDRW2100017973.1.21",
          "form": "순대국밥도 종류가 은근 여러가지에요",
          "original_form": "순대국밥도 종류가 은근 여러가지에요",
          "speaker_id": "1",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100017973.1.22",
          "form": "찰순대 나오면 그 식당은 두번 다시 안갑니다..ㅋㅋ",
          "original_form": "찰순대 나오면 그 식당은 두번 다시 안갑니다..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:07"
        },
        {
          "id": "MDRW2100017973.1.23",
          "form": "오..? 여러가지군요",
          "original_form": "오..? 여러가지군요",
          "speaker_id": "2",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100017973.1.24",
          "form": "국밥에 찰순대요?!?",
          "original_form": "국밥에 찰순대요?!?",
          "speaker_id": "2",
          "time": "20210826 22:09"
        },
        {
          "id": "MDRW2100017973.1.25",
          "form": "순대국밥 안에 찰순대가..",
          "original_form": "순대국밥 안에 찰순대가..",
          "speaker_id": "1",
          "time": "20210826 22:10"
        }
      ]
