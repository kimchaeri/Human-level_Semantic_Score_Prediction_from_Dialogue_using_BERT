      "utterance": [
        {
          "id": "MDRW2100038754.1.1",
          "form": "티비는 자주 보시나요?",
          "original_form": "티비는 자주 보시나요?",
          "speaker_id": "1",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100038754.1.2",
          "form": "그냥 백색소음용으로 틀어놔요",
          "original_form": "그냥 백색소음용으로 틀어놔요",
          "speaker_id": "2",
          "time": "20210902 20:48"
        },
        {
          "id": "MDRW2100038754.1.3",
          "form": "ㅋㅋ예전에 무도가 최고였죠",
          "original_form": "ㅋㅋ예전에 무도가 최고였죠",
          "speaker_id": "1",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100038754.1.4",
          "form": "그럼 주로 유투브보시나요?",
          "original_form": "그럼 주로 유투브보시나요?",
          "speaker_id": "1",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100038754.1.5",
          "form": "네네 유투브 자주봐요 ㅎㅎ",
          "original_form": "네네 유투브 자주봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100038754.1.6",
          "form": "유튜브 자주보세요?",
          "original_form": "유튜브 자주보세요?",
          "speaker_id": "2",
          "time": "20210902 20:49"
        },
        {
          "id": "MDRW2100038754.1.7",
          "form": "좋아하는 컨텐츠 골라서요",
          "original_form": "좋아하는 컨텐츠 골라서요",
          "speaker_id": "1",
          "time": "20210902 20:50"
        },
        {
          "id": "MDRW2100038754.1.8",
          "form": "어떤 컨텐츠좋아하세요?",
          "original_form": "어떤 컨텐츠좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 20:51"
        },
        {
          "id": "MDRW2100038754.1.9",
          "form": "요리랑 음악이랑 홈트ㅋㅋ요런거요",
          "original_form": "요리랑 음악이랑 홈트ㅋㅋ요런거요",
          "speaker_id": "1",
          "time": "20210902 20:52"
        },
        {
          "id": "MDRW2100038754.1.10",
          "form": "오 홈트",
          "original_form": "오 홈트",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100038754.1.11",
          "form": "맞아요 그런 컨텐츠도 잘되오있더라거요",
          "original_form": "맞아요 그런 컨텐츠도 잘되오있더라거요",
          "speaker_id": "2",
          "time": "20210902 20:55"
        },
        {
          "id": "MDRW2100038754.1.12",
          "form": "주로 어떤 거 보세요?",
          "original_form": "주로 어떤 거 보세요?",
          "speaker_id": "1",
          "time": "20210902 20:56"
        },
        {
          "id": "MDRW2100038754.1.13",
          "form": "저는 방송 클립 같은거 자주 봐요",
          "original_form": "저는 방송 클립 같은거 자주 봐요",
          "speaker_id": "2",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100038754.1.14",
          "form": "요리도 종종 보고",
          "original_form": "요리도 종종 보고",
          "speaker_id": "2",
          "time": "20210902 20:58"
        },
        {
          "id": "MDRW2100038754.1.15",
          "form": "저도 방송 편집본 같은것도 좋아해요!",
          "original_form": "저도 방송 편집본 같은것도 좋아해요!",
          "speaker_id": "1",
          "time": "20210902 20:59"
        },
        {
          "id": "MDRW2100038754.1.16",
          "form": "드라마는 하이라이트로 보는게 제일 재밌는것같아요",
          "original_form": "드라마는 하이라이트로 보는게 제일 재밌는것같아요",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100038754.1.17",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:00"
        },
        {
          "id": "MDRW2100038754.1.18",
          "form": "그러니까요ㅋㅋ 엑기스니까 재밌어요ㅜㅜ 영화도 해설본 보면 새롭고 좋더라고요",
          "original_form": "그러니까요ㅋㅋ 엑기스니까 재밌어요ㅜㅜ 영화도 해설본 보면 새롭고 좋더라고요",
          "speaker_id": "1",
          "time": "20210902 21:02"
        },
        {
          "id": "MDRW2100038754.1.19",
          "form": "맞아요 새롭고 좋더라구요",
          "original_form": "맞아요 새롭고 좋더라구요",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100038754.1.20",
          "form": "넷플릭스 그 장기용 정수정 영화는 유투브에서 설명해주는 사람으로 봤어욬ㅋㅋ",
          "original_form": "넷플릭스 그 장기용 정수정 영화는 유투브에서 설명해주는 사람으로 봤어욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:09"
        },
        {
          "id": "MDRW2100038754.1.21",
          "form": "아 저는 책 내용 풀어주는거랑 역사 설명해주는 것도 즐겨봐요!",
          "original_form": "아 저는 책 내용 풀어주는거랑 역사 설명해주는 것도 즐겨봐요!",
          "speaker_id": "1",
          "time": "20210902 21:16"
        }
      ]
    }
