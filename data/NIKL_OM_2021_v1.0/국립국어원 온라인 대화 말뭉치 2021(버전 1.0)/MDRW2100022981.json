      "utterance": [
        {
          "id": "MDRW2100022981.1.1",
          "form": "헐 왜 과일만 먹어 밥 안 먹고",
          "original_form": "헐 왜 과일만 먹어 밥 안 먹고",
          "speaker_id": "2",
          "time": "20210829 20:00"
        },
        {
          "id": "MDRW2100022981.1.2",
          "form": "속이 안 좋니?",
          "original_form": "속이 안 좋니?",
          "speaker_id": "2",
          "time": "20210829 20:00"
        },
        {
          "id": "MDRW2100022981.1.3",
          "form": "어제 저녁에 엄청 먹었잖아",
          "original_form": "어제 저녁에 엄청 먹었잖아",
          "speaker_id": "1",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100022981.1.4",
          "form": "그게 오늘까치 배부른거있지",
          "original_form": "그게 오늘까치 배부른거있지",
          "speaker_id": "1",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100022981.1.5",
          "form": "오 어제 저녁은 뭐 먹었니",
          "original_form": "오 어제 저녁은 뭐 먹었니",
          "speaker_id": "2",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100022981.1.6",
          "form": "어제 저녁에 무려 광어회랑",
          "original_form": "어제 저녁에 무려 광어회랑",
          "speaker_id": "1",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100022981.1.7",
          "form": "김치찌개",
          "original_form": "김치찌개",
          "speaker_id": "1",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100022981.1.8",
          "form": "우와 신기하게 먹었다",
          "original_form": "우와 신기하게 먹었다",
          "speaker_id": "2",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100022981.1.9",
          "form": "넌 회 좋아해?",
          "original_form": "넌 회 좋아해?",
          "speaker_id": "2",
          "time": "20210829 20:03"
        },
        {
          "id": "MDRW2100022981.1.10",
          "form": "응 회 엄청 좋아해",
          "original_form": "응 회 엄청 좋아해",
          "speaker_id": "1",
          "time": "20210829 20:05"
        },
        {
          "id": "MDRW2100022981.1.11",
          "form": "와사비간장 찍어먹는걸로",
          "original_form": "와사비간장 찍어먹는걸로",
          "speaker_id": "1",
          "time": "20210829 20:05"
        },
        {
          "id": "MDRW2100022981.1.12",
          "form": "헉 와사비간장은 찍어먹을 생각 해본 적 없는데 특이하다",
          "original_form": "헉 와사비간장은 찍어먹을 생각 해본 적 없는데 특이하다",
          "speaker_id": "2",
          "time": "20210829 20:05"
        },
        {
          "id": "MDRW2100022981.1.13",
          "form": "난 초고추장파거든",
          "original_form": "난 초고추장파거든",
          "speaker_id": "2",
          "time": "20210829 20:05"
        },
        {
          "id": "MDRW2100022981.1.14",
          "form": "김치도 좋아해?",
          "original_form": "김치도 좋아해?",
          "speaker_id": "2",
          "time": "20210829 20:07"
        },
        {
          "id": "MDRW2100022981.1.15",
          "form": "초밥먹는 맛이랑 비슷해서",
          "original_form": "초밥먹는 맛이랑 비슷해서",
          "speaker_id": "1",
          "time": "20210829 20:07"
        },
        {
          "id": "MDRW2100022981.1.16",
          "form": "맛있어",
          "original_form": "맛있어",
          "speaker_id": "1",
          "time": "20210829 20:08"
        },
        {
          "id": "MDRW2100022981.1.17",
          "form": "회랑 김치?!",
          "original_form": "회랑 김치?!",
          "speaker_id": "1",
          "time": "20210829 20:08"
        },
        {
          "id": "MDRW2100022981.1.18",
          "form": "오 그렇구나 초밥 맛있지",
          "original_form": "오 그렇구나 초밥 맛있지",
          "speaker_id": "2",
          "time": "20210829 20:08"
        },
        {
          "id": "MDRW2100022981.1.19",
          "form": "ㅋㅋㅋㅋㅋㅌ아니아니",
          "original_form": "ㅋㅋㅋㅋㅋㅌ아니아니",
          "speaker_id": "2",
          "time": "20210829 20:08"
        },
        {
          "id": "MDRW2100022981.1.20",
          "form": "김치 따로 좋아해 ㅋㅋㅋㅋㅋ?",
          "original_form": "김치 따로 좋아해 ㅋㅋㅋㅋㅋ?",
          "speaker_id": "2",
          "time": "20210829 20:08"
        },
        {
          "id": "MDRW2100022981.1.21",
          "form": "응 김치  짱좋아해",
          "original_form": "응 김치  짱좋아해",
          "speaker_id": "1",
          "time": "20210829 20:09"
        },
        {
          "id": "MDRW2100022981.1.22",
          "form": "어떤 김치를 제일 좋아하니",
          "original_form": "어떤 김치를 제일 좋아하니",
          "speaker_id": "2",
          "time": "20210829 20:10"
        },
        {
          "id": "MDRW2100022981.1.23",
          "form": "난 갓담은 김장김치",
          "original_form": "난 갓담은 김장김치",
          "speaker_id": "1",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100022981.1.24",
          "form": "쌀밥도둑이야",
          "original_form": "쌀밥도둑이야",
          "speaker_id": "1",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100022981.1.25",
          "form": "헐 맛있겠다 나도 갓 담은 거 좋아해",
          "original_form": "헐 맛있겠다 나도 갓 담은 거 좋아해",
          "speaker_id": "2",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100022981.1.26",
          "form": "겉절이 정말 맛있더라",
          "original_form": "겉절이 정말 맛있더라",
          "speaker_id": "2",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100022981.1.27",
          "form": "반대로 싫어하는 김치 있어?",
          "original_form": "반대로 싫어하는 김치 있어?",
          "speaker_id": "2",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100022981.1.28",
          "form": "응 오이소박이 싫어해",
          "original_form": "응 오이소박이 싫어해",
          "speaker_id": "1",
          "time": "20210829 20:13"
        },
        {
          "id": "MDRW2100022981.1.29",
          "form": "오이소박이의 오이아삭함이 싫더라",
          "original_form": "오이소박이의 오이아삭함이 싫더라",
          "speaker_id": "1",
          "time": "20210829 20:14"
        }
      ]
    }
  ]
