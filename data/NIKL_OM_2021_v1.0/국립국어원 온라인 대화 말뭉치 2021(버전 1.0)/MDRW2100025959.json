        {
          "id": "MDRW2100025959.1.1",
          "form": "비와요?",
          "original_form": "비와요?",
          "speaker_id": "1",
          "time": "20210901 18:21"
        },
        {
          "id": "MDRW2100025959.1.2",
          "form": "아니요 비가 올거같아요",
          "original_form": "아니요 비가 올거같아요",
          "speaker_id": "2",
          "time": "20210901 18:34"
        },
        {
          "id": "MDRW2100025959.1.3",
          "form": "오락가락해요",
          "original_form": "오락가락해요",
          "speaker_id": "1",
          "time": "20210901 19:20"
        },
        {
          "id": "MDRW2100025959.1.4",
          "form": "비와요 ㅠㅠㅠㅠㅠ",
          "original_form": "비와요 ㅠㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 19:28"
        },
        {
          "id": "MDRW2100025959.1.5",
          "form": "여기도",
          "original_form": "여기도",
          "speaker_id": "1",
          "time": "20210901 19:31"
        },
        {
          "id": "MDRW2100025959.1.6",
          "form": "비와서 다이소에서 우비 사서 왔는데 집오니 그쳤네요 ㅠ",
          "original_form": "비와서 다이소에서 우비 사서 왔는데 집오니 그쳤네요 ㅠ",
          "speaker_id": "2",
          "time": "20210901 20:47"
        },
        {
          "id": "MDRW2100025959.1.7",
          "form": "비오는 거 너무 싫어요",
          "original_form": "비오는 거 너무 싫어요",
          "speaker_id": "2",
          "time": "20210901 21:04"
        },
        {
          "id": "MDRW2100025959.1.8",
          "form": "저도",
          "original_form": "저도",
          "speaker_id": "1",
          "time": "20210901 22:15"
        },
        {
          "id": "MDRW2100025959.1.9",
          "form": "우산은 가져가셨나여",
          "original_form": "우산은 가져가셨나여",
          "speaker_id": "2",
          "time": "20210901 22:30"
        },
        {
          "id": "MDRW2100025959.1.10",
          "form": "항상 챙기죠ㅠ요ㄱ즘",
          "original_form": "항상 챙기죠ㅠ요ㄱ즘",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100025959.1.11",
          "form": "안무거우신가요..",
          "original_form": "안무거우신가요..",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100025959.1.12",
          "form": "전 그렇게 안챙기다가 맞고와요 ㅋㅋ",
          "original_form": "전 그렇게 안챙기다가 맞고와요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:42"
        },
        {
          "id": "MDRW2100025959.1.13",
          "form": "3단 정도면",
          "original_form": "3단 정도면",
          "speaker_id": "1",
          "time": "20210901 22:49"
        },
        {
          "id": "MDRW2100025959.1.14",
          "form": "생각보다는 무겁죠 ㅠ",
          "original_form": "생각보다는 무겁죠 ㅠ",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100025959.1.15",
          "form": "가벼우면 우산 날라가고",
          "original_form": "가벼우면 우산 날라가고",
          "speaker_id": "2",
          "time": "20210901 23:39"
        },
        {
          "id": "MDRW2100025959.1.16",
          "form": "아쉬운게 많아요 ㅠ",
