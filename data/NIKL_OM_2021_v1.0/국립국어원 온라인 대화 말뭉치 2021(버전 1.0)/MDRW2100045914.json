      "utterance": [
        {
          "id": "MDRW2100045914.1.1",
          "form": "매일우유에서 나온건데",
          "original_form": "매일우유에서 나온건데",
          "speaker_id": "1",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045914.1.2",
          "form": "팩이라 먹기 좋아",
          "original_form": "팩이라 먹기 좋아",
          "speaker_id": "1",
          "time": "20210908 08:39"
        },
        {
          "id": "MDRW2100045914.1.3",
          "form": "오홍",
          "original_form": "오홍",
          "speaker_id": "2",
          "time": "20210908 08:42"
        },
        {
          "id": "MDRW2100045914.1.4",
          "form": "다음에 장볼때 그걸 사볼까",
          "original_form": "다음에 장볼때 그걸 사볼까",
          "speaker_id": "2",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100045914.1.5",
          "form": "있으면...()",
          "original_form": "있으면...()",
          "speaker_id": "2",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100045914.1.6",
          "form": "ㅇㅇ 함 찾아봐",
          "original_form": "ㅇㅇ 함 찾아봐",
          "speaker_id": "1",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100045914.1.7",
          "form": "글고 나 충격적인 기사를 봣는데",
          "original_form": "글고 나 충격적인 기사를 봣는데",
          "speaker_id": "1",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100045914.1.8",
          "form": "유당분해효소는",
          "original_form": "유당분해효소는",
          "speaker_id": "1",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100045914.1.9",
          "form": "일반우유를 계속 먹으면",
          "original_form": "일반우유를 계속 먹으면",
          "speaker_id": "1",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100045914.1.10",
          "form": "몸이 적응해서",
          "original_form": "몸이 적응해서",
          "speaker_id": "1",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045914.1.11",
          "form": "생긴대 ㅋㅋㅋㅋ",
          "original_form": "생긴대 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045914.1.12",
          "form": "머라구",
          "original_form": "머라구",
          "speaker_id": "2",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045914.1.13",
          "form": "그럼 계속 설사하다가 적응한다는건가",
          "original_form": "그럼 계속 설사하다가 적응한다는건가",
          "speaker_id": "2",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045914.1.14",
          "form": "인간의 진화",
          "original_form": "인간의 진화",
          "speaker_id": "2",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045914.1.15",
          "form": "그런가봐 ㅋㅋㅋㅋㅋ",
          "original_form": "그런가봐 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100045914.1.16",
          "form": "충격적이지",
          "original_form": "충격적이지",
          "speaker_id": "1",
          "time": "20210908 08:45"
        },
        {
          "id": "MDRW2100045914.1.17",
          "form": "되게 충격이다",
          "original_form": "되게 충격이다",
          "speaker_id": "2",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045914.1.18",
          "form": "점심 구내식당 갈거야?",
          "original_form": "점심 구내식당 갈거야?",
          "speaker_id": "1",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045914.1.19",
          "form": "매일?에서 그래서 유당불내증 있는 애기들인가 때문에 하루는 전부 비우고 다른 우유만든댔는데(?",
          "original_form": "매일?에서 그래서 유당불내증 있는 애기들인가 때문에 하루는 전부 비우고 다른 우유만든댔는데(?",
          "speaker_id": "2",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045914.1.20",
          "form": "앗 고민중이야...",
          "original_form": "앗 고민중이야...",
          "speaker_id": "2",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045914.1.21",
          "form": "식당 메뉴 뭔지 알아?",
          "original_form": "식당 메뉴 뭔지 알아?",
          "speaker_id": "1",
          "time": "20210908 08:53"
        },
        {
          "id": "MDRW2100045914.1.22",
          "form": "아니.. 거기 적혀있어서 봐야 아는데 늘 직접 가야 알지...",
          "original_form": "아니.. 거기 적혀있어서 봐야 아는데 늘 직접 가야 알지...",
          "speaker_id": "2",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100045914.1.23",
          "form": "ㅋㅋㅋㅋㅋ 그렇군...",
          "original_form": "ㅋㅋㅋㅋㅋ 그렇군...",
          "speaker_id": "1",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100045914.1.24",
          "form": "그냥 4천원 백반이야(?",
          "original_form": "그냥 4천원 백반이야(?",
          "speaker_id": "2",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100045914.1.25",
          "form": "근처에 식당 많아?",
          "original_form": "근처에 식당 많아?",
          "speaker_id": "1",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100045914.1.26",
          "form": "ㅋㅋ큐ㅠㅠ",
          "original_form": "ㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:54"
        },
        {
          "id": "MDRW2100045914.1.27",
          "form": "머.. 없지는 않아 그래도 직장인들이 많아서",
          "original_form": "머.. 없지는 않아 그래도 직장인들이 많아서",
          "speaker_id": "2",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100045914.1.28",
          "form": "버거킹이랑 맘터는 얘기 들었어",
          "original_form": "버거킹이랑 맘터는 얘기 들었어",
          "speaker_id": "1",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100045914.1.29",
          "form": "가격대가 싸진 않겠지만",
          "original_form": "가격대가 싸진 않겠지만",
          "speaker_id": "2",
          "time": "20210908 08:55"
        },
        {
          "id": "MDRW2100045914.1.30",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
