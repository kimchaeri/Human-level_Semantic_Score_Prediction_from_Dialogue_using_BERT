      "utterance": [
        {
          "id": "MDRW2100025579.1.1",
          "form": "오오 메모해두고 꼭 가볼게요",
          "original_form": "오오 메모해두고 꼭 가볼게요",
          "speaker_id": "2",
          "time": "20210901 13:47"
        },
        {
          "id": "MDRW2100025579.1.2",
          "form": "거기가 명장이 하시는곳인데",
          "original_form": "거기가 명장이 하시는곳인데",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100025579.1.3",
          "form": "그래도 체인점이있어요",
          "original_form": "그래도 체인점이있어요",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100025579.1.4",
          "form": "다른것도 맛있는데",
          "original_form": "다른것도 맛있는데",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100025579.1.5",
          "form": "바게트가최고에용",
          "original_form": "바게트가최고에용",
          "speaker_id": "1",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100025579.1.6",
          "form": "오오 명장이 전국에서 얼마 안되는거 맞죠?",
          "original_form": "오오 명장이 전국에서 얼마 안되는거 맞죠?",
          "speaker_id": "2",
          "time": "20210901 13:48"
        },
        {
          "id": "MDRW2100025579.1.7",
          "form": "네네 아마",
          "original_form": "네네 아마",
          "speaker_id": "1",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025579.1.8",
          "form": "열명좀 넘으시는걸로 알고있어요",
          "original_form": "열명좀 넘으시는걸로 알고있어요",
          "speaker_id": "1",
          "time": "20210901 13:49"
        },
        {
          "id": "MDRW2100025579.1.9",
          "form": "그쵸 담에 함 가봐야겠어요 ㅎㅎ",
          "original_form": "그쵸 담에 함 가봐야겠어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025579.1.10",
          "form": "커피도 좋아하시나요?",
          "original_form": "커피도 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025579.1.11",
          "form": "오 네 좋아해요",
          "original_form": "오 네 좋아해요",
          "speaker_id": "1",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025579.1.12",
          "form": "단걸안마셔서",
          "original_form": "단걸안마셔서",
          "speaker_id": "1",
          "time": "20210901 13:50"
        },
        {
          "id": "MDRW2100025579.1.13",
          "form": "커피맛있는데 찾는것도 쉽지않아요 ㅎㅎㅎ",
          "original_form": "커피맛있는데 찾는것도 쉽지않아요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100025579.1.14",
          "form": "담백한걸 좋아하시나봐요 고소하고 ㅋㅋ",
          "original_form": "담백한걸 좋아하시나봐요 고소하고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:51"
        },
        {
          "id": "MDRW2100025579.1.15",
          "form": "네네 ㅋㄱㅋㅋ단걸안좋아해서 ㅠ",
          "original_form": "네네 ㅋㄱㅋㅋ단걸안좋아해서 ㅠ",
          "speaker_id": "1",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100025579.1.16",
          "form": "name2님은 어떤거좋아하시나요",
          "original_form": "&name2&님은 어떤거좋아하시나요",
          "speaker_id": "1",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100025579.1.17",
          "form": "카페가시면",
          "original_form": "카페가시면",
          "speaker_id": "1",
          "time": "20210901 13:52"
        },
        {
          "id": "MDRW2100025579.1.18",
          "form": "저는 아메 라떼 주로 먹어요 ㅋㅋㅋ",
          "original_form": "저는 아메 라떼 주로 먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:54"
        },
        {
          "id": "MDRW2100025579.1.19",
          "form": "오 라떼도 맛있죠",
          "original_form": "오 라떼도 맛있죠",
          "speaker_id": "1",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025579.1.20",
          "form": "우유의맛...!",
          "original_form": "우유의맛...!",
          "speaker_id": "1",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025579.1.21",
          "form": "폴바셋 라떼 고소한데 지점이 얼마 없어서 잘 못 가네요 ㅎㅎ",
          "original_form": "폴바셋 라떼 고소한데 지점이 얼마 없어서 잘 못 가네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 13:55"
        },
        {
          "id": "MDRW2100025579.1.22",
          "form": "오 저도 폴바셋 좋아해요",
          "original_form": "오 저도 폴바셋 좋아해요",
          "speaker_id": "1",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100025579.1.23",
          "form": "시즌아이스크림 유일하게 먹는 맛이 밀크티 아이스크림...",
          "original_form": "시즌아이스크림 유일하게 먹는 맛이 밀크티 아이스크림...",
          "speaker_id": "1",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100025579.1.24",
          "form": "고소하죠!!! 아이스크림도 맛있는데",
          "original_form": "고소하죠!!! 아이스크림도 맛있는데",
          "speaker_id": "2",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100025579.1.25",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "2",
          "time": "20210901 14:01"
        },
        {
          "id": "MDRW2100025579.1.26",
          "form": "밀크티아이스크림은 꼭",
          "original_form": "밀크티아이스크림은 꼭",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025579.1.27",
          "form": "일반메뉴로 넣어줬으면...",
          "original_form": "일반메뉴로 넣어줬으면...",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025579.1.28",
          "form": "그러게요 그런건 꼭 한정이네요 ㅋㅋㅋ",
          "original_form": "그러게요 그런건 꼭 한정이네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025579.1.29",
          "form": "저희도 동네에 있던 폴바셋이 없어져서",
          "original_form": "저희도 동네에 있던 폴바셋이 없어져서",
          "speaker_id": "1",
          "time": "20210901 14:02"
        },
        {
          "id": "MDRW2100025579.1.30",
          "form": "아쉽더라구요...",
          "original_form": "아쉽더라구요...",
          "speaker_id": "1",
          "time": "20210901 14:03"
        }
      ]
    }
