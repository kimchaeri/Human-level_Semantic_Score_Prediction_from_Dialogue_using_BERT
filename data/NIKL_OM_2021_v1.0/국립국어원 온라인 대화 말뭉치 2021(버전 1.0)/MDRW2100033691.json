      "utterance": [
        {
          "id": "MDRW2100033691.1.1",
          "form": "진짜 저도 확진자가 있어서",
          "original_form": "진짜 저도 확진자가 있어서",
          "speaker_id": "2",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100033691.1.2",
          "form": "자가격리해봤는데요",
          "original_form": "자가격리해봤는데요",
          "speaker_id": "2",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100033691.1.3",
          "form": "아니 직장있는 놈들이 왜 코로나 걸려서 오는겨",
          "original_form": "아니 직장있는 놈들이 왜 코로나 걸려서 오는겨",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100033691.1.4",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100033691.1.5",
          "form": "하 씁 진짜 그거때문에 불안할바에는 백신이 낫다...",
          "original_form": "하 씁 진짜 그거때문에 불안할바에는 백신이 낫다...",
          "speaker_id": "2",
          "time": "20210824 23:00"
        },
        {
          "id": "MDRW2100033691.1.6",
          "form": "저는 가족이 확진자라...",
          "original_form": "저는 가족이 확진자라...",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100033691.1.7",
          "form": "근데 돌아가셨어요 이런얘기 처음 해봅니다....",
          "original_form": "근데 돌아가셨어요 이런얘기 처음 해봅니다....",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100033691.1.8",
          "form": "약간 백신맞기싫다는 사람한테 이 얘기하면 싹 들어간다는 오빠의 얘기를 들었어요",
          "original_form": "약간 백신맞기싫다는 사람한테 이 얘기하면 싹 들어간다는 오빠의 얘기를 들었어요",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100033691.1.9",
          "form": "님 위로안해도된다고요!!!!!!!! 평소같은...농담....오케이라고요....",
          "original_form": "님 위로안해도된다고요!!!!!!!! 평소같은...농담....오케이라고요....",
          "speaker_id": "2",
          "time": "20210824 23:01"
        },
        {
          "id": "MDRW2100033691.1.10",
          "form": "!!!!!!!!!",
          "original_form": "!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210824 23:02"
        },
        {
          "id": "MDRW2100033691.1.11",
          "form": "ㅇ...오케이..",
          "original_form": "ㅇ...오케이..",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100033691.1.12",
          "form": "하지만 그래도 위로의 말 전합니다",
          "original_form": "하지만 그래도 위로의 말 전합니다",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100033691.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ괜찮습니다....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ괜찮습니다....",
          "speaker_id": "2",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100033691.1.14",
          "form": "그..할머니 할아버지 쪽이셨나요",
          "original_form": "그..할머니 할아버지 쪽이셨나요",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100033691.1.15",
          "form": "근데 그 경로가 좀 화나더라고요!!!! 네 할아버지!",
          "original_form": "근데 그 경로가 좀 화나더라고요!!!! 네 할아버지!",
          "speaker_id": "2",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100033691.1.16",
          "form": "쓰읍..",
          "original_form": "쓰읍..",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100033691.1.17",
          "form": "할아버지가 폐렴으로 입원중이셨는데 같은 병동에 있던.....사람의 가족이...",
          "original_form": "할아버지가 폐렴으로 입원중이셨는데 같은 병동에 있던.....사람의 가족이...",
          "speaker_id": "2",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100033691.1.18",
          "form": "그럼 돌아댕기는 애들 더 싫을것같어요",
          "original_form": "그럼 돌아댕기는 애들 더 싫을것같어요",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100033691.1.19",
          "form": "짱나",
          "original_form": "짱나",
          "speaker_id": "1",
          "time": "20210824 23:03"
        },
        {
          "id": "MDRW2100033691.1.20",
          "form": "타지역에서 병문안을 왔는데 확진자였던거예요",
          "original_form": "타지역에서 병문안을 왔는데 확진자였던거예요",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100033691.1.21",
          "form": "헐 하필",
          "original_form": "헐 하필",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100033691.1.22",
          "form": "아 짱나",
          "original_form": "아 짱나",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100033691.1.23",
          "form": "뭐하는 놈인겨",
          "original_form": "뭐하는 놈인겨",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100033691.1.24",
          "form": "아니..하..ㅋ",
          "original_form": "아니..하..ㅋ",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100033691.1.25",
          "form": "아 참나",
          "original_form": "아 참나",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100033691.1.26",
          "form": "근데 아직도 기억나거든요 할아버지가 그때 같은 병실환자들 전부 60대이상 노인이었다고",
          "original_form": "근데 아직도 기억나거든요 할아버지가 그때 같은 병실환자들 전부 60대이상 노인이었다고",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100033691.1.27",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100033691.1.28",
          "form": "아니 경각심이 없어도 글치",
          "original_form": "아니 경각심이 없어도 글치",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100033691.1.29",
          "form": "확진자 그놈은 살아있을거 아녀요",
          "original_form": "확진자 그놈은 살아있을거 아녀요",
          "speaker_id": "1",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100033691.1.30",
          "form": "나중에 지인통해 들었는데 그 확진자가 와서 격리 된 노인분들 반이 돌아가셨다고 하더라고요",
          "original_form": "나중에 지인통해 들었는데 그 확진자가 와서 격리 된 노인분들 반이 돌아가셨다고 하더라고요",
          "speaker_id": "2",
          "time": "20210824 23:04"
        },
        {
          "id": "MDRW2100033691.1.31",
          "form": "허얼",
          "original_form": "허얼",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100033691.1.32",
          "form": "너무하다 진짜",
          "original_form": "너무하다 진짜",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100033691.1.33",
          "form": "다 편찮아서 입원하셨던 분들이니까",
          "original_form": "다 편찮아서 입원하셨던 분들이니까",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100033691.1.34",
          "form": "심지어 코로나가 폐 석화시켜서",
          "original_form": "심지어 코로나가 폐 석화시켜서",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100033691.1.35",
          "form": "하필 진짜..",
          "original_form": "하필 진짜..",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100033691.1.36",
          "form": "저는 같이 안살아서",
          "original_form": "저는 같이 안살아서",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100033691.1.37",
          "form": "그나마 낫긴한데...씁..",
          "original_form": "그나마 낫긴한데...씁..",
          "speaker_id": "2",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100033691.1.38",
          "form": "아이고..",
          "original_form": "아이고..",
          "speaker_id": "1",
          "time": "20210824 23:05"
        },
        {
          "id": "MDRW2100033691.1.39",
          "form": "아이고...아이고...ㅠㅠㅠ",
          "original_form": "아이고...아이고...ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100033691.1.40",
          "form": "할머니도 할아버지께 옮아서 그 폐한쪽이 기능을 못한다고 하더라고요",
          "original_form": "할머니도 할아버지께 옮아서 그 폐한쪽이 기능을 못한다고 하더라고요",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100033691.1.41",
          "form": "!!!!! 아이고",
          "original_form": "!!!!! 아이고",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100033691.1.42",
          "form": "할머니 건강하세요 제발",
          "original_form": "할머니 건강하세요 제발",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100033691.1.43",
          "form": "진짜 돌아다니는 사람 너무하지않나....싶어요 갑자기 넘 진지했다",
          "original_form": "진짜 돌아다니는 사람 너무하지않나....싶어요 갑자기 넘 진지했다",
          "speaker_id": "2",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100033691.1.44",
          "form": "아녀유",
          "original_form": "아녀유",
          "speaker_id": "1",
          "time": "20210824 23:06"
        },
        {
          "id": "MDRW2100033691.1.45",
          "form": "우리가 그동안 넘 발랄했던거같아유",
          "original_form": "우리가 그동안 넘 발랄했던거같아유",
          "speaker_id": "1",
          "time": "20210824 23:06"
        }
