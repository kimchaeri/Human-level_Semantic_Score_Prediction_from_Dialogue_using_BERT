      "utterance": [
        {
          "id": "MDRW2100018569.1.1",
          "form": "일 가기 싫어요",
          "original_form": "일 가기 싫어요",
          "speaker_id": "2",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100018569.1.2",
          "form": "지금 부산역이에요",
          "original_form": "지금 부산역이에요",
          "speaker_id": "2",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100018569.1.3",
          "form": "악저도요\n엄청빨리가셧네요",
          "original_form": "악저도요\n엄청빨리가셧네요",
          "speaker_id": "1",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100018569.1.4",
          "form": "그러네요 환승하고하면 거의 9시에요 너무싫어요 ㅋㅋ",
          "original_form": "그러네요 환승하고하면 거의 9시에요 너무싫어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 08:37"
        },
        {
          "id": "MDRW2100018569.1.5",
          "form": "진짜일찍출근해요 피곤하겠어요 힝",
          "original_form": "진짜일찍출근해요 피곤하겠어요 힝",
          "speaker_id": "1",
          "time": "20210827 10:41"
        },
        {
          "id": "MDRW2100018569.1.6",
          "form": "맞아요 너무 졸려요",
          "original_form": "맞아요 너무 졸려요",
          "speaker_id": "2",
          "time": "20210827 11:00"
        },
        {
          "id": "MDRW2100018569.1.7",
          "form": "커피는마셨나요",
          "original_form": "커피는마셨나요",
          "speaker_id": "1",
          "time": "20210827 11:15"
        },
        {
          "id": "MDRW2100018569.1.8",
          "form": "네 아침에 콜드브루요",
          "original_form": "네 아침에 콜드브루요",
          "speaker_id": "2",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018569.1.9",
          "form": "카페인이 노화의 지름길이라는데 큰일이에요",
          "original_form": "카페인이 노화의 지름길이라는데 큰일이에요",
          "speaker_id": "2",
          "time": "20210827 11:26"
        },
        {
          "id": "MDRW2100018569.1.10",
          "form": "콜드브루맛있겠다흐흐",
          "original_form": "콜드브루맛있겠다흐흐",
          "speaker_id": "1",
          "time": "20210827 12:50"
        },
        {
          "id": "MDRW2100018569.1.11",
          "form": "그렇게빨리노화안될거예요괜찮아요",
          "original_form": "그렇게빨리노화안될거예요괜찮아요",
          "speaker_id": "1",
          "time": "20210827 12:50"
        },
        {
          "id": "MDRW2100018569.1.12",
          "form": "일할때도움되고좋아요오오으히",
          "original_form": "일할때도움되고좋아요오오으히",
          "speaker_id": "1",
          "time": "20210827 12:50"
        },
        {
          "id": "MDRW2100018569.1.13",
          "form": "맞아요오히히",
          "original_form": "맞아요오히히",
          "speaker_id": "2",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100018569.1.14",
          "form": "우엉차가좋대요",
          "original_form": "우엉차가좋대요",
          "speaker_id": "2",
          "time": "20210827 13:11"
        },
        {
          "id": "MDRW2100018569.1.15",
          "form": "오 우엉차맛있어요",
          "original_form": "오 우엉차맛있어요",
          "speaker_id": "1",
          "time": "20210827 13:15"
        },
        {
          "id": "MDRW2100018569.1.16",
          "form": "한번사먹어봣습죠 회사에두고먹었었는데",
          "original_form": "한번사먹어봣습죠 회사에두고먹었었는데",
          "speaker_id": "1",
          "time": "20210827 13:15"
        },
        {
          "id": "MDRW2100018569.1.17",
          "form": "오 다른 음료로 대체해야할것같아요 커피노노",
          "original_form": "오 다른 음료로 대체해야할것같아요 커피노노",
          "speaker_id": "2",
          "time": "20210827 14:01"
        },
        {
          "id": "MDRW2100018569.1.18",
          "form": "냉우엉차",
          "original_form": "냉우엉차",
          "speaker_id": "1",
          "time": "20210827 14:08"
        },
        {
          "id": "MDRW2100018569.1.19",
          "form": "도전!",
          "original_form": "도전!",
          "speaker_id": "1",
          "time": "20210827 14:08"
        },
        {
          "id": "MDRW2100018569.1.20",
          "form": "오늘은짜증나게하는직장동료없어여?",
          "original_form": "오늘은짜증나게하는직장동료없어여?",
          "speaker_id": "1",
          "time": "20210827 14:08"
        },
        {
          "id": "MDRW2100018569.1.21",
          "form": "항상있죠",
          "original_form": "항상있죠",
          "speaker_id": "2",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100018569.1.22",
          "form": "진짜 타인에의해 스트레스받기싫은데ㅠㅗ",
          "original_form": "진짜 타인에의해 스트레스받기싫은데ㅠㅗ",
          "speaker_id": "2",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100018569.1.23",
          "form": "정말 괴롭지괴로워",
          "original_form": "정말 괴롭지괴로워",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100018569.1.24",
          "form": "회사그만두고싶어요",
          "original_form": "회사그만두고싶어요",
