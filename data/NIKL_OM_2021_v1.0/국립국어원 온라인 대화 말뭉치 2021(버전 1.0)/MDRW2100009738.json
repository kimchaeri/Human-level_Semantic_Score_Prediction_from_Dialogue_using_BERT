      "utterance": [
        {
          "id": "MDRW2100009738.1.1",
          "form": "재밌게 본 영화가있나요",
          "original_form": "재밌게 본 영화가있나요",
          "speaker_id": "1",
          "time": "20210823 09:00"
        },
        {
          "id": "MDRW2100009738.1.2",
          "form": "저 최근에 블랙위도우 재밌게 봤어요! 보셨나요",
          "original_form": "저 최근에 블랙위도우 재밌게 봤어요! 보셨나요",
          "speaker_id": "2",
          "time": "20210823 09:01"
        },
        {
          "id": "MDRW2100009738.1.3",
          "form": "그거 재밌다고 하더라구요. 저는 시간이 안되서 보지는 못했어요ㅠ",
          "original_form": "그거 재밌다고 하더라구요. 저는 시간이 안되서 보지는 못했어요ㅠ",
          "speaker_id": "1",
          "time": "20210823 09:01"
        },
        {
          "id": "MDRW2100009738.1.4",
          "form": "기억에 남는 장면이 있나요?",
          "original_form": "기억에 남는 장면이 있나요?",
          "speaker_id": "1",
          "time": "20210823 09:02"
        },
        {
          "id": "MDRW2100009738.1.5",
          "form": "육탄 액션씬이 멋져요!",
          "original_form": "육탄 액션씬이 멋져요!",
          "speaker_id": "2",
          "time": "20210823 09:02"
        },
        {
          "id": "MDRW2100009738.1.6",
          "form": "치고박고 싸우거든요",
          "original_form": "치고박고 싸우거든요",
          "speaker_id": "2",
          "time": "20210823 09:02"
        },
        {
          "id": "MDRW2100009738.1.7",
          "form": "액션영화 좋아하시나요?",
          "original_form": "액션영화 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210823 09:02"
        },
        {
          "id": "MDRW2100009738.1.8",
          "form": "네 저도 좋아해요",
          "original_form": "네 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210823 09:02"
        },
        {
          "id": "MDRW2100009738.1.9",
          "form": "약간 액션을 보면 쾌감이 있는 거 같아요",
          "original_form": "약간 액션을 보면 쾌감이 있는 거 같아요",
          "speaker_id": "1",
          "time": "20210823 09:02"
        },
        {
          "id": "MDRW2100009738.1.10",
          "form": "맞아요! 쾌감 쩔어요",
          "original_form": "맞아요! 쾌감 쩔어요",
          "speaker_id": "2",
          "time": "20210823 09:03"
        },
        {
          "id": "MDRW2100009738.1.11",
          "form": "그 맛에 더 찾아보게 되는 것도 있구요",
          "original_form": "그 맛에 더 찾아보게 되는 것도 있구요",
          "speaker_id": "1",
          "time": "20210823 09:03"
        },
        {
          "id": "MDRW2100009738.1.12",
          "form": "그리고 가볍게 보기도 좋아요!",
          "original_form": "그리고 가볍게 보기도 좋아요!",
          "speaker_id": "2",
          "time": "20210823 09:03"
        },
        {
          "id": "MDRW2100009738.1.13",
          "form": "무거운 영화 갈수록 잘 안보게 되더라구요 삶이 무거우니 ㅋㅋㅋ",
          "original_form": "무거운 영화 갈수록 잘 안보게 되더라구요 삶이 무거우니 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 09:03"
        },
        {
          "id": "MDRW2100009738.1.14",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 09:03"
        },
        {
          "id": "MDRW2100009738.1.15",
          "form": "저는 액션도 좋고 코미디도 요즘 좋더라구요",
          "original_form": "저는 액션도 좋고 코미디도 요즘 좋더라구요",
          "speaker_id": "1",
          "time": "20210823 09:04"
        },
        {
          "id": "MDRW2100009738.1.16",
          "form": "최근에 집에서 정직한 후보 봤는데 가볍게 보기 좋았어요",
          "original_form": "최근에 집에서 정직한 후보 봤는데 가볍게 보기 좋았어요",
          "speaker_id": "1",
          "time": "20210823 09:05"
        },
        {
          "id": "MDRW2100009738.1.17",
          "form": "저도 그거 되게 재밌게 봤어요!",
          "original_form": "저도 그거 되게 재밌게 봤어요!",
          "speaker_id": "2",
          "time": "20210823 09:08"
        },
        {
          "id": "MDRW2100009738.1.18",
          "form": "라미란이 시어머니랑 싸우는 씬 되게 웃겼어요",
          "original_form": "라미란이 시어머니랑 싸우는 씬 되게 웃겼어요",
          "speaker_id": "2",
          "time": "20210823 09:08"
        },
        {
          "id": "MDRW2100009738.1.19",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 09:08"
        },
        {
          "id": "MDRW2100009738.1.20",
          "form": "그리고 거짓말을 못하는 설정도 재밌더라구요",
          "original_form": "그리고 거짓말을 못하는 설정도 재밌더라구요",
          "speaker_id": "1",
          "time": "20210823 09:09"
        },
        {
          "id": "MDRW2100009738.1.21",
          "form": "정말 정직한 후보가 되더라구요ㅋㅋ",
          "original_form": "정말 정직한 후보가 되더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 09:09"
        },
        {
          "id": "MDRW2100009738.1.22",
          "form": "세계최초 정직한 후보 ㅋㅋㅋ",
          "original_form": "세계최초 정직한 후보 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 09:09"
        },
        {
          "id": "MDRW2100009738.1.23",
          "form": "너무 웃겨요ㅋㅣㅣ",
          "original_form": "너무 웃겨요ㅋㅣㅣ",
          "speaker_id": "1",
          "time": "20210823 09:10"
        },
        {
          "id": "MDRW2100009738.1.24",
          "form": "라미란이 아들이랑 남편이랑 싸우는것도 웃기고",
          "original_form": "라미란이 아들이랑 남편이랑 싸우는것도 웃기고",
          "speaker_id": "2",
          "time": "20210823 09:10"
        }
      ]
    }
