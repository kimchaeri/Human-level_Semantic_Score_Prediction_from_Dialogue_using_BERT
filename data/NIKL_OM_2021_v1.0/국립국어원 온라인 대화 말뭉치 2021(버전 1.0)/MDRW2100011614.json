      "utterance": [
        {
          "id": "MDRW2100011614.1.1",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "1",
          "time": "20210824 14:34"
        },
        {
          "id": "MDRW2100011614.1.2",
          "form": "점심 식사는 하셨나요?",
          "original_form": "점심 식사는 하셨나요?",
          "speaker_id": "1",
          "time": "20210824 14:34"
        },
        {
          "id": "MDRW2100011614.1.3",
          "form": "안녕하세요~~",
          "original_form": "안녕하세요~~",
          "speaker_id": "2",
          "time": "20210824 14:34"
        },
        {
          "id": "MDRW2100011614.1.4",
          "form": "네 먹었어요.  아점으로요",
          "original_form": "네 먹었어요.  아점으로요",
          "speaker_id": "2",
          "time": "20210824 14:34"
        },
        {
          "id": "MDRW2100011614.1.5",
          "form": "아점으로 드셨군요:)",
          "original_form": "아점으로 드셨군요:)",
          "speaker_id": "1",
          "time": "20210824 14:35"
        },
        {
          "id": "MDRW2100011614.1.6",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "1",
          "time": "20210824 14:35"
        },
        {
          "id": "MDRW2100011614.1.7",
          "form": "된장찌개요^^",
          "original_form": "된장찌개요^^",
          "speaker_id": "2",
          "time": "20210824 14:35"
        },
        {
          "id": "MDRW2100011614.1.8",
          "form": "맛있었겠네요^^",
          "original_form": "맛있었겠네요^^",
          "speaker_id": "1",
          "time": "20210824 14:36"
        },
        {
          "id": "MDRW2100011614.1.9",
          "form": "저는 저녁에 된장찌개 하려구요:)",
          "original_form": "저는 저녁에 된장찌개 하려구요:)",
          "speaker_id": "1",
          "time": "20210824 14:36"
        },
        {
          "id": "MDRW2100011614.1.10",
          "form": "오랫만에 먹었더니 좋더라구요",
          "original_form": "오랫만에 먹었더니 좋더라구요",
          "speaker_id": "2",
          "time": "20210824 14:40"
        },
        {
          "id": "MDRW2100011614.1.11",
          "form": "한동안 더워서 잘 밥도안하고시켜먹고 그랬거든요",
          "original_form": "한동안 더워서 잘 밥도안하고시켜먹고 그랬거든요",
          "speaker_id": "2",
          "time": "20210824 14:41"
        },
        {
          "id": "MDRW2100011614.1.12",
          "form": "그쵸ㅠ 올 여름 유난히도 더웠어가지고",
          "original_form": "그쵸ㅠ 올 여름 유난히도 더웠어가지고",
          "speaker_id": "1",
          "time": "20210824 14:42"
        },
        {
          "id": "MDRW2100011614.1.13",
          "form": "저도 뭐 해먹기도 싫더라구요",
          "original_form": "저도 뭐 해먹기도 싫더라구요",
          "speaker_id": "1",
          "time": "20210824 14:42"
        },
        {
          "id": "MDRW2100011614.1.14",
          "form": "오늘 저녁은 뭐드실거에요? ^^",
          "original_form": "오늘 저녁은 뭐드실거에요? ^^",
          "speaker_id": "1",
          "time": "20210824 14:42"
        },
        {
          "id": "MDRW2100011614.1.15",
          "form": "대강 빵으로~~~",
          "original_form": "대강 빵으로~~~",
          "speaker_id": "2",
          "time": "20210824 14:42"
        },
        {
          "id": "MDRW2100011614.1.16",
          "form": "잘챙겨드셔야죠ㅠㅠ",
          "original_form": "잘챙겨드셔야죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 14:42"
        },
        {
          "id": "MDRW2100011614.1.17",
          "form": "요기요 파바 할인해서 빵먹을까 생각중이에요",
          "original_form": "요기요 파바 할인해서 빵먹을까 생각중이에요",
          "speaker_id": "2",
          "time": "20210824 14:42"
        },
        {
          "id": "MDRW2100011614.1.18",
          "form": "앗.... 사실 저는 점심으로",
          "original_form": "앗.... 사실 저는 점심으로",
          "speaker_id": "1",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100011614.1.19",
          "form": "빠바 빵 먹었습니다ㅋㅋㅋㅋㅋ",
          "original_form": "빠바 빵 먹었습니다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100011614.1.20",
          "form": "빵좋아해서^^",
          "original_form": "빵좋아해서^^",
          "speaker_id": "2",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100011614.1.21",
          "form": "할인은 또 못 참쥬",
          "original_form": "할인은 또 못 참쥬{emoji:♡}",
          "speaker_id": "1",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100011614.1.22",
          "form": "빠바 어떤 빵 젤 좋아하세요? ㅋㅋ",
          "original_form": "빠바 어떤 빵 젤 좋아하세요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100011614.1.23",
          "form": "ㅋㅋ 그니깐요~~~할인은 써줘야죠",
          "original_form": "ㅋㅋ 그니깐요~~~할인은 써줘야죠",
          "speaker_id": "2",
          "time": "20210824 14:43"
        },
        {
          "id": "MDRW2100011614.1.24",
          "form": "모카크림 이나 팥도너츠^^",
          "original_form": "모카크림 이나 팥도너츠^^",
          "speaker_id": "2",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011614.1.25",
          "form": "소시지빵",
          "original_form": "소시지빵",
          "speaker_id": "2",
          "time": "20210824 14:44"
        },
        {
          "id": "MDRW2100011614.1.26",
          "form": "ㅋㅋ다 맛있는 빵이네요",
          "original_form": "ㅋㅋ다 맛있는 빵이네요",
          "speaker_id": "1",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011614.1.27",
          "form": "혹시양파치즈브레드도 드셔보셨어용?",
          "original_form": "혹시양파치즈브레드도 드셔보셨어용?",
          "speaker_id": "1",
          "time": "20210824 14:45"
        },
        {
          "id": "MDRW2100011614.1.28",
          "form": "네~  그것도 맛있어요.  가족이 잘안먹어서 자주먹는건 아니에요~",
          "original_form": "네~  그것도 맛있어요.  가족이 잘안먹어서 자주먹는건 아니에요~",
          "speaker_id": "2",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011614.1.29",
          "form": "아ㅠㅠ 아쉽네요",
          "original_form": "아ㅠㅠ 아쉽네요",
          "speaker_id": "1",
          "time": "20210824 14:47"
        },
        {
          "id": "MDRW2100011614.1.30",
          "form": "전자렌지에 살짝 돌려먹음\r\n완전 맛있는뎅...  ㅋㅋㅋㅋㅋ",
          "original_form": "전자렌지에 살짝 돌려먹음\r\n완전 맛있는뎅...  ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:48"
        },
        {
          "id": "MDRW2100011614.1.31",
          "form": "애들이 양파를 싫어해서 안먹더라구요",
          "original_form": "애들이 양파를 싫어해서 안먹더라구요",
          "speaker_id": "2",
          "time": "20210824 14:48"
        },
        {
          "id": "MDRW2100011614.1.32",
          "form": "아 양파를 안좋아하는군용",
          "original_form": "아 양파를 안좋아하는군용",
          "speaker_id": "1",
          "time": "20210824 14:49"
        },
        {
          "id": "MDRW2100011614.1.33",
          "form": "그럼 싫어할 수 있겠네요!",
          "original_form": "그럼 싫어할 수 있겠네요!",
          "speaker_id": "1",
          "time": "20210824 14:49"
        }
      ]
