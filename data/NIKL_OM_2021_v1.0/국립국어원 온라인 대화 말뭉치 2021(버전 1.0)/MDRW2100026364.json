      "utterance": [
        {
          "id": "MDRW2100026364.1.1",
          "form": "기사 찾아보니까 둘다 종영아니고 사콜은 나중에 또 한대",
          "original_form": "기사 찾아보니까 둘다 종영아니고 사콜은 나중에 또 한대",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026364.1.2",
          "form": "맞나 임영웅이도 나오나",
          "original_form": "맞나 임영웅이도 나오나",
          "speaker_id": "1",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026364.1.3",
          "form": "임영웅도 나온다는데",
          "original_form": "임영웅도 나온다는데",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026364.1.4",
          "form": "어디서 안 나온다고 본겨",
          "original_form": "어디서 안 나온다고 본겨",
          "speaker_id": "2",
          "time": "20210901 21:49"
        },
        {
          "id": "MDRW2100026364.1.5",
          "form": "뽕숭아는?",
          "original_form": "뽕숭아는?",
          "speaker_id": "1",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026364.1.6",
          "form": "뽕숭아는 쉬는것도 아니고 그냥 계속 한다는데?",
          "original_form": "뽕숭아는 쉬는것도 아니고 그냥 계속 한다는데?",
          "speaker_id": "2",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026364.1.7",
          "form": "계속해서 좋겠네? 매주 보는건데",
          "original_form": "계속해서 좋겠네? 매주 보는건데",
          "speaker_id": "2",
          "time": "20210901 21:50"
        },
        {
          "id": "MDRW2100026364.1.8",
          "form": "그래 재미나게 보던거 계속 보면좋지!",
          "original_form": "그래 재미나게 보던거 계속 보면좋지!",
          "speaker_id": "1",
          "time": "20210901 21:51"
        },
        {
          "id": "MDRW2100026364.1.9",
          "form": "거기 나오는 사람들 다 지쳐 보이던데 신기하네 난 계약 종료되고 프로그램 안 할줄",
          "original_form": "거기 나오는 사람들 다 지쳐 보이던데 신기하네 난 계약 종료되고 프로그램 안 할줄",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026364.1.10",
          "form": "같이 하는게 이득인갑지",
          "original_form": "같이 하는게 이득인갑지",
          "speaker_id": "1",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026364.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:52"
        },
        {
          "id": "MDRW2100026364.1.12",
          "form": "또 요즘 하는거 잘 보는거 뭐더라?",
          "original_form": "또 요즘 하는거 잘 보는거 뭐더라?",
          "speaker_id": "2",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026364.1.13",
          "form": "와카남?",
          "original_form": "와카남?",
          "speaker_id": "1",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026364.1.14",
          "form": "그거말고 진짜 엄청 재밌게보는거 있드만 ㅋㅋ",
          "original_form": "그거말고 진짜 엄청 재밌게보는거 있드만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:53"
        },
        {
          "id": "MDRW2100026364.1.15",
          "form": "여자들 축구하는거 재밌더라",
          "original_form": "여자들 축구하는거 재밌더라",
          "speaker_id": "1",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026364.1.16",
          "form": "아 그것도 열심히 보더라 ㅎㅎ",
          "original_form": "아 그것도 열심히 보더라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026364.1.17",
          "form": "아 허재 좋아하잖아 허재 나오는것도 많이 보더만",
          "original_form": "아 허재 좋아하잖아 허재 나오는것도 많이 보더만",
          "speaker_id": "2",
          "time": "20210901 21:54"
        },
        {
          "id": "MDRW2100026364.1.18",
          "form": "허재가 나와야 웃기다 그 혼자사는프로에도 허재가 안 나오면 안 웃기다",
          "original_form": "허재가 나와야 웃기다 그 혼자사는프로에도 허재가 안 나오면 안 웃기다",
          "speaker_id": "1",
          "time": "20210901 21:55"
        },
        {
          "id": "MDRW2100026364.1.19",
          "form": "아 ㅋㅋㅋ 그렇구만 그 아저씨 엄청 허당인가보더만 ㅋㅋ",
          "original_form": "아 ㅋㅋㅋ 그렇구만 그 아저씨 엄청 허당인가보더만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 21:55"
        }
      ]
