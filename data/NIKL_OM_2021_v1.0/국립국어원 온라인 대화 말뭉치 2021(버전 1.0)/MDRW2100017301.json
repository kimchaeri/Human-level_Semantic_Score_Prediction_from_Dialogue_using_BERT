      "utterance": [
        {
          "id": "MDRW2100017301.1.1",
          "form": "무슨 일을 하고 먹고 살아야 할까",
          "original_form": "무슨 일을 하고 먹고 살아야 할까",
          "speaker_id": "2",
          "time": "20210827 17:31"
        },
        {
          "id": "MDRW2100017301.1.2",
          "form": "극ㄹ게",
          "original_form": "극ㄹ게",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100017301.1.3",
          "form": "긁적극적",
          "original_form": "긁적극적",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100017301.1.4",
          "form": "막막...",
          "original_form": "막막...",
          "speaker_id": "1",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100017301.1.5",
          "form": "진짜 막막하다",
          "original_form": "진짜 막막하다",
          "speaker_id": "2",
          "time": "20210827 17:32"
        },
        {
          "id": "MDRW2100017301.1.6",
          "form": "뭔가 앞으로 갈수록",
          "original_form": "뭔가 앞으로 갈수록",
          "speaker_id": "1",
          "time": "20210827 17:33"
        },
        {
          "id": "MDRW2100017301.1.7",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210827 17:33"
        },
        {
          "id": "MDRW2100017301.1.8",
          "form": "it나 공학쪽 엔지니어 이런직업처럼",
          "original_form": "it나 공학쪽 엔지니어 이런직업처럼",
          "speaker_id": "1",
          "time": "20210827 17:33"
        },
        {
          "id": "MDRW2100017301.1.9",
          "form": "완전 고학력&지식인 직업아니면",
          "original_form": "완전 고학력&지식인 직업아니면",
          "speaker_id": "1",
          "time": "20210827 17:33"
        },
        {
          "id": "MDRW2100017301.1.10",
          "form": "맞아..",
          "original_form": "맞아..",
          "speaker_id": "2",
          "time": "20210827 17:33"
        },
        {
          "id": "MDRW2100017301.1.11",
          "form": "못 살아남을거같아서 걱정됨",
          "original_form": "못 살아남을거같아서 걱정됨",
          "speaker_id": "1",
          "time": "20210827 17:33"
        },
        {
          "id": "MDRW2100017301.1.12",
          "form": "ㅇㅈ....",
          "original_form": "ㅇㅈ....",
          "speaker_id": "2",
          "time": "20210827 17:33"
        },
        {
          "id": "MDRW2100017301.1.13",
          "form": "아니면 완전 예체능쪽으로 가던가",
          "original_form": "아니면 완전 예체능쪽으로 가던가",
          "speaker_id": "2",
          "time": "20210827 17:34"
        },
        {
          "id": "MDRW2100017301.1.14",
          "form": "맞아..",
          "original_form": "맞아..",
          "speaker_id": "1",
          "time": "20210827 17:34"
        },
        {
          "id": "MDRW2100017301.1.15",
          "form": "어중간하면 다 죽을듯",
          "original_form": "어중간하면 다 죽을듯",
          "speaker_id": "2",
          "time": "20210827 17:34"
        },
        {
          "id": "MDRW2100017301.1.16",
          "form": "대충 평범하게 무능한사람은 머하고사냐",
          "original_form": "대충 평범하게 무능한사람은 머하고사냐",
          "speaker_id": "1",
          "time": "20210827 17:34"
        },
        {
          "id": "MDRW2100017301.1.17",
          "form": "내말이",
          "original_form": "내말이",
          "speaker_id": "2",
          "time": "20210827 17:34"
        },
        {
          "id": "MDRW2100017301.1.18",
          "form": "주식만 주구장장 해야하나",
          "original_form": "주식만 주구장장 해야하나",
          "speaker_id": "2",
          "time": "20210827 17:34"
        },
        {
          "id": "MDRW2100017301.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:34"
        },
        {
          "id": "MDRW2100017301.1.20",
          "form": "아니면",
          "original_form": "아니면",
          "speaker_id": "2",
          "time": "20210827 17:34"
        },
        {
          "id": "MDRW2100017301.1.21",
          "form": "인도네시아어 태국어 이런거 배워서",
          "original_form": "인도네시아어 태국어 이런거 배워서",
          "speaker_id": "2",
          "time": "20210827 17:34"
        },
        {
          "id": "MDRW2100017301.1.22",
          "form": "그 나라 가거나...",
          "original_form": "그 나라 가거나...",
          "speaker_id": "2",
          "time": "20210827 17:34"
        },
        {
          "id": "MDRW2100017301.1.23",
          "form": "글게ㅠ",
          "original_form": "글게ㅠ",
          "speaker_id": "1",
          "time": "20210827 17:35"
        },
        {
          "id": "MDRW2100017301.1.24",
          "form": "그래서 내목표는",
          "original_form": "그래서 내목표는",
          "speaker_id": "1",
          "time": "20210827 17:35"
        },
        {
          "id": "MDRW2100017301.1.25",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210827 17:35"
        },
        {
          "id": "MDRW2100017301.1.26",
          "form": "본 직장말고도",
          "original_form": "본 직장말고도",
          "speaker_id": "1",
          "time": "20210827 17:35"
        },
        {
          "id": "MDRW2100017301.1.27",
          "form": "100만원~200만원정도",
          "original_form": "100만원~200만원정도",
          "speaker_id": "1",
          "time": "20210827 17:35"
        },
        {
          "id": "MDRW2100017301.1.28",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210827 17:35"
        },
        {
          "id": "MDRW2100017301.1.29",
          "form": "부업 파이프라인 만드는거임",
          "original_form": "부업 파이프라인 만드는거임",
          "speaker_id": "1",
          "time": "20210827 17:35"
        },
        {
          "id": "MDRW2100017301.1.30",
          "form": "근데 진짜 그게 좋을것 같음",
          "original_form": "근데 진짜 그게 좋을것 같음",
          "speaker_id": "2",
          "time": "20210827 17:35"
        },
        {
          "id": "MDRW2100017301.1.31",
          "form": "근데 난 공무원 하고 싶어서",
          "original_form": "근데 난 공무원 하고 싶어서",
          "speaker_id": "2",
          "time": "20210827 17:36"
        },
        {
          "id": "MDRW2100017301.1.32",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210827 17:36"
        },
        {
          "id": "MDRW2100017301.1.33",
          "form": "돈 나올 구석이 많아야함",
          "original_form": "돈 나올 구석이 많아야함",
          "speaker_id": "1",
          "time": "20210827 17:36"
        },
        {
          "id": "MDRW2100017301.1.34",
          "form": "부업은 절대 안되고",
          "original_form": "부업은 절대 안되고",
          "speaker_id": "2",
          "time": "20210827 17:36"
        },
        {
          "id": "MDRW2100017301.1.35",
          "form": "아..까비",
          "original_form": "아..까비",
          "speaker_id": "1",
          "time": "20210827 17:36"
        },
        {
          "id": "MDRW2100017301.1.36",
          "form": "믿을건 주식뿐...",
          "original_form": "믿을건 주식뿐...",
          "speaker_id": "2",
          "time": "20210827 17:36"
        },
        {
          "id": "MDRW2100017301.1.37",
          "form": "전자책이라도 쓰자",
          "original_form": "전자책이라도 쓰자",
          "speaker_id": "1",
          "time": "20210827 17:36"
        },
        {
          "id": "MDRW2100017301.1.38",
          "form": "전자책이요..?",
          "original_form": "전자책이요..?",
          "speaker_id": "2",
          "time": "20210827 17:36"
        },
        {
          "id": "MDRW2100017301.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:36"
        },
        {
          "id": "MDRW2100017301.1.40",
          "form": "ㅇㅇ그런거도 안되나?",
          "original_form": "ㅇㅇ그런거도 안되나?",
          "speaker_id": "1",
          "time": "20210827 17:36"
        },
