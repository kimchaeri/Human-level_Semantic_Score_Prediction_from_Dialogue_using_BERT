        },
        {
          "id": "MDRW2100022167.1.2",
          "form": "집들이 ㅎㅎ 맛난거먹어라ㅋ",
          "original_form": "집들이 ㅎㅎ 맛난거먹어라ㅋ",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022167.1.3",
          "form": "난 장보고 들어가야지ㅋ",
          "original_form": "난 장보고 들어가야지ㅋ",
          "speaker_id": "2",
          "time": "20210829 12:39"
        },
        {
          "id": "MDRW2100022167.1.4",
          "form": "스테끼 묵었는데..지는 짬뽕이 더 맛나네여 ㅋㅋㅋㅋ",
          "original_form": "스테끼 묵었는데..지는 짬뽕이 더 맛나네여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100022167.1.5",
          "form": "저려미 입맛 ㅋㅋㅋㅋ",
          "original_form": "저려미 입맛 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100022167.1.6",
          "form": "오호 소코끼ㅋㅋ",
          "original_form": "오호 소코끼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100022167.1.7",
          "form": "부릅다ㅋㅋ",
          "original_form": "부릅다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100022167.1.8",
          "form": "지는 라면이 더 맛있네여 ㅋㅋㅋㅋ",
          "original_form": "지는 라면이 더 맛있네여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 08:01"
        },
        {
          "id": "MDRW2100022167.1.9",
          "form": "이제 준비끝~언넝 다들 출근시키고 청소해야되네여;;",
          "original_form": "이제 준비끝~언넝 다들 출근시키고 청소해야되네여;;",
          "speaker_id": "1",
          "time": "20210830 08:02"
        },
        {
          "id": "MDRW2100022167.1.10",
          "form": "첫찌.방 또 분리해달라해서ㅠ",
          "original_form": "첫찌.방 또 분리해달라해서ㅠ",
          "speaker_id": "1",
          "time": "20210830 08:02"
        },
        {
          "id": "MDRW2100022167.1.11",
          "form": "오호 그렇네ㅋㅋ",
          "original_form": "오호 그렇네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100022167.1.12",
          "form": "난 집에서나왔다ㅋ",
          "original_form": "난 집에서나왔다ㅋ",
          "speaker_id": "2",
          "time": "20210830 13:38"
        },
        {
          "id": "MDRW2100022167.1.13",
          "form": "그렇네요 언니 오늘",
          "original_form": "그렇네요 언니 오늘",
          "speaker_id": "1",
          "time": "20210830 13:39"
        },
        {
          "id": "MDRW2100022167.1.14",
          "form": "난 집안일이 바쁜가 했죠",
          "original_form": "난 집안일이 바쁜가 했죠",
          "speaker_id": "1",
          "time": "20210830 13:39"
        },
        {
          "id": "MDRW2100022167.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 13:39"
        },
        {
          "id": "MDRW2100022167.1.16",
          "form": "어디 가요",
          "original_form": "어디 가요",
          "speaker_id": "1",
          "time": "20210830 13:40"
        },
        {
          "id": "MDRW2100022167.1.17",
          "form": "ㅋㅋ 집안일드ㅡ했다 빨래도하고",
          "original_form": "ㅋㅋ 집안일드ㅡ했다 빨래도하고",
          "speaker_id": "2",
          "time": "20210830 13:40"
        },
        {
          "id": "MDRW2100022167.1.18",
          "form": "널고",
          "original_form": "널고",
          "speaker_id": "2",
          "time": "20210830 13:40"
        },
        {
          "id": "MDRW2100022167.1.19",
          "form": "지금 네일샵ㅋ",
          "original_form": "지금 네일샵ㅋ",
          "speaker_id": "2",
          "time": "20210830 13:40"
        },
        {
          "id": "MDRW2100022167.1.20",
          "form": "아이고 언니 먼 걸음했네요",
          "original_form": "아이고 언니 먼 걸음했네요",
          "speaker_id": "1",
          "time": "20210830 13:41"
        },
        {
          "id": "MDRW2100022167.1.21",
          "form": "네일하고 집에 갑니까??",
          "original_form": "네일하고 집에 갑니까??",
          "speaker_id": "1",
          "time": "20210830 13:41"
        },
        {
          "id": "MDRW2100022167.1.22",
          "form": "네일구경하로옴  ㅋ",
