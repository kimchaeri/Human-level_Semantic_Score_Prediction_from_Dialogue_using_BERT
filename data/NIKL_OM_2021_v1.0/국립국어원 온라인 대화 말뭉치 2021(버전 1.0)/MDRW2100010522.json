      "utterance": [
        {
          "id": "MDRW2100010522.1.1",
          "form": "건강을 위해서",
          "original_form": "건강을 위해서",
          "speaker_id": "2",
          "time": "20210823 19:13"
        },
        {
          "id": "MDRW2100010522.1.2",
          "form": "드시는 영양제 있으세요?",
          "original_form": "드시는 영양제 있으세요?",
          "speaker_id": "2",
          "time": "20210823 19:13"
        },
        {
          "id": "MDRW2100010522.1.3",
          "form": "저 비타민디랑 유산균먹고있어요",
          "original_form": "저 비타민디랑 유산균먹고있어요",
          "speaker_id": "1",
          "time": "20210823 19:13"
        },
        {
          "id": "MDRW2100010522.1.4",
          "form": "드시는거 있으세요?",
          "original_form": "드시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210823 19:14"
        },
        {
          "id": "MDRW2100010522.1.5",
          "form": "저는 프로폴리스랑 유산균이요 !",
          "original_form": "저는 프로폴리스랑 유산균이요 !",
          "speaker_id": "2",
          "time": "20210823 19:16"
        },
        {
          "id": "MDRW2100010522.1.6",
          "form": "유산균 어디거 드세요?",
          "original_form": "유산균 어디거 드세요?",
          "speaker_id": "2",
          "time": "20210823 19:16"
        },
        {
          "id": "MDRW2100010522.1.7",
          "form": "저 재로우꺼 먹는데 효과가 있는건지ㅎㅎ 어디서 드세요?",
          "original_form": "저 재로우꺼 먹는데 효과가 있는건지ㅎㅎ 어디서 드세요?",
          "speaker_id": "1",
          "time": "20210823 19:17"
        },
        {
          "id": "MDRW2100010522.1.8",
          "form": "저는 한국야쿠르트,,? 여기꺼 먹고있어요",
          "original_form": "저는 한국야쿠르트,,? 여기꺼 먹고있어요",
          "speaker_id": "2",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100010522.1.9",
          "form": "재로우거 유명하잔항요!!",
          "original_form": "재로우거 유명하잔항요!!",
          "speaker_id": "2",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100010522.1.10",
          "form": "네 그래서 먹는중인데 별다른 효과는 모르겠어요효",
          "original_form": "네 그래서 먹는중인데 별다른 효과는 모르겠어요효",
          "speaker_id": "1",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100010522.1.11",
          "form": "프로폴리스 괜찮아여?",
          "original_form": "프로폴리스 괜찮아여?",
          "speaker_id": "1",
          "time": "20210823 19:26"
        },
        {
          "id": "MDRW2100010522.1.12",
          "form": "네 그냥저냥..",
          "original_form": "네 그냥저냥..",
          "speaker_id": "2",
          "time": "20210823 19:29"
        },
        {
          "id": "MDRW2100010522.1.13",
          "form": "근데 사실 저도 모르겠어요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 사실 저도 모르겠어요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:29"
        },
        {
          "id": "MDRW2100010522.1.14",
          "form": "ㅋㅋ그래도 안먹을수도 없구ㅎㅎ 오메가3도 먹으라던데 냄새가 ㅜㅜ",
          "original_form": "ㅋㅋ그래도 안먹을수도 없구ㅎㅎ 오메가3도 먹으라던데 냄새가 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 19:31"
        },
        {
          "id": "MDRW2100010522.1.15",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "2",
          "time": "20210823 19:36"
        },
        {
          "id": "MDRW2100010522.1.16",
          "form": "오메가3는 거의 필수라고 하더라구요",
          "original_form": "오메가3는 거의 필수라고 하더라구요",
          "speaker_id": "2",
          "time": "20210823 19:37"
        },
        {
          "id": "MDRW2100010522.1.17",
          "form": "나이들수록 챙겨먹을게 많아지네요ㅎㅎ",
          "original_form": "나이들수록 챙겨먹을게 많아지네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 19:37"
        },
        {
          "id": "MDRW2100010522.1.18",
          "form": "맞아요 ㅎ 나중에는",
          "original_form": "맞아요 ㅎ 나중에는",
          "speaker_id": "2",
          "time": "20210823 19:41"
        },
        {
          "id": "MDRW2100010522.1.19",
          "form": "제 몸이 알아서 잘 하겠죠",
          "original_form": "제 몸이 알아서 잘 하겠죠",
          "speaker_id": "2",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010522.1.20",
          "form": "많이 먹여놨으니깐...!",
          "original_form": "많이 먹여놨으니깐...!",
          "speaker_id": "2",
          "time": "20210823 19:42"
        },
        {
          "id": "MDRW2100010522.1.21",
          "form": "ㅎㅎ맞아요 나중에 좋으라고 챙겨먹는거니까요ㅎㅎ",
          "original_form": "ㅎㅎ맞아요 나중에 좋으라고 챙겨먹는거니까요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 19:43"
        },
        {
          "id": "MDRW2100010522.1.22",
          "form": "맞아요 효과 잘보면 좋겠어요",
          "original_form": "맞아요 효과 잘보면 좋겠어요",
