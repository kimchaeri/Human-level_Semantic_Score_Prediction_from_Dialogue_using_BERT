      "utterance": [
        {
          "id": "MDRW2100024765.1.1",
          "form": "언니 저 가연이에요ㅋㅋㅋ",
          "original_form": "언니 저 가연이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:14"
        },
        {
          "id": "MDRW2100024765.1.2",
          "form": "저 유튜브빼고 안봐서 사실 잘 몰라요",
          "original_form": "저 유튜브빼고 안봐서 사실 잘 몰라요",
          "speaker_id": "1",
          "time": "20210831 18:14"
        },
        {
          "id": "MDRW2100024765.1.3",
          "form": "유튜브만 보시는군요",
          "original_form": "유튜브만 보시는군요",
          "speaker_id": "2",
          "time": "20210831 18:15"
        },
        {
          "id": "MDRW2100024765.1.4",
          "form": "구독하는 사람 있나요?",
          "original_form": "구독하는 사람 있나요?",
          "speaker_id": "2",
          "time": "20210831 18:15"
        },
        {
          "id": "MDRW2100024765.1.5",
          "form": "김메주와 고양이들이요!",
          "original_form": "김메주와 고양이들이요!",
          "speaker_id": "1",
          "time": "20210831 18:16"
        },
        {
          "id": "MDRW2100024765.1.6",
          "form": "우왕",
          "original_form": "우왕",
          "speaker_id": "2",
          "time": "20210831 18:16"
        },
        {
          "id": "MDRW2100024765.1.7",
          "form": "고양이 귀엽죠",
          "original_form": "고양이 귀엽죠",
          "speaker_id": "2",
          "time": "20210831 18:16"
        },
        {
          "id": "MDRW2100024765.1.8",
          "form": "고양이 asmr ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "고양이 asmr ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:17"
        },
        {
          "id": "MDRW2100024765.1.9",
          "form": "채널명 곰돌이 숭늉이  에요",
          "original_form": "채널명 곰돌이 숭늉이  에요",
          "speaker_id": "2",
          "time": "20210831 18:18"
        },
        {
          "id": "MDRW2100024765.1.10",
          "form": "고양인데 곰돌이네요ㅋㅋ",
          "original_form": "고양인데 곰돌이네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:19"
        },
        {
          "id": "MDRW2100024765.1.11",
          "form": "그러네요",
          "original_form": "그러네요",
          "speaker_id": "2",
          "time": "20210831 18:22"
        },
        {
          "id": "MDRW2100024765.1.12",
          "form": "근데 귀여워요",
          "original_form": "근데 귀여워요",
          "speaker_id": "2",
          "time": "20210831 18:22"
        },
        {
          "id": "MDRW2100024765.1.13",
          "form": "그래요 어쩜 그렇게 가만히 있는지 모르겠어요",
          "original_form": "그래요 어쩜 그렇게 가만히 있는지 모르겠어요",
          "speaker_id": "1",
          "time": "20210831 18:24"
        },
        {
          "id": "MDRW2100024765.1.14",
          "form": "그만큼 집사를 믿고 따른다는거겠죠 ㅋㅋ",
          "original_form": "그만큼 집사를 믿고 따른다는거겠죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:26"
        },
        {
          "id": "MDRW2100024765.1.15",
          "form": "완전요 그러기 쉽지 않을거같은데요ㅋㅋㅋㅋ",
          "original_form": "완전요 그러기 쉽지 않을거같은데요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:28"
        },
        {
          "id": "MDRW2100024765.1.16",
          "form": "저는 알러지땜에 고양이를 못기르지만",
          "original_form": "저는 알러지땜에 고양이를 못기르지만",
          "speaker_id": "2",
          "time": "20210831 18:33"
        },
        {
          "id": "MDRW2100024765.1.17",
          "form": "저런 분들을 보면 참 사랑으로 기른다는 생각이 들어요",
          "original_form": "저런 분들을 보면 참 사랑으로 기른다는 생각이 들어요",
          "speaker_id": "2",
          "time": "20210831 18:34"
        },
        {
          "id": "MDRW2100024765.1.18",
          "form": "와 언니 제가 답장 안했었네요",
          "original_form": "와 언니 제가 답장 안했었네요",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100024765.1.19",
          "form": "언니 좋아하는 노래있나요??",
          "original_form": "언니 좋아하는 노래있나요??",
          "speaker_id": "1",
          "time": "20210901 23:14"
        },
        {
          "id": "MDRW2100024765.1.20",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100024765.1.21",
          "form": "전 뮤지컬 좋아해서 듣는 노래도 그쪽 노래를 자주듣게 되더라구요",
          "original_form": "전 뮤지컬 좋아해서 듣는 노래도 그쪽 노래를 자주듣게 되더라구요",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100024765.1.22",
          "form": "근데 요즘 안예은 노래를 좀 들어요 ㅋㅋ",
          "original_form": "근데 요즘 안예은 노래를 좀 들어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100024765.1.23",
          "form": "우와 뮤지컬노래ㅋㅋㅋㅋ",
          "original_form": "우와 뮤지컬노래ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100024765.1.24",
          "form": "언니 출퇴근하면서 노래들어요??",
          "original_form": "언니 출퇴근하면서 노래들어요??",
          "speaker_id": "1",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100024765.1.25",
          "form": "가끔요. 루틴은 아니에요",
          "original_form": "가끔요. 루틴은 아니에요",
          "speaker_id": "2",
