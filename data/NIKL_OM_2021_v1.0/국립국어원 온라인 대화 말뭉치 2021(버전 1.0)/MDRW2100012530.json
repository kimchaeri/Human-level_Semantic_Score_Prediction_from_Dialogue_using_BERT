      "utterance": [
        {
          "id": "MDRW2100012530.1.1",
          "form": "맞아.. 금리 많이 오르는 것 같더라",
          "original_form": "맞아.. 금리 많이 오르는 것 같더라",
          "speaker_id": "2",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100012530.1.2",
          "form": "금리 많이 오르고 해서 돈 생길 때마다 갚아야겠어",
          "original_form": "금리 많이 오르고 해서 돈 생길 때마다 갚아야겠어",
          "speaker_id": "1",
          "time": "20210824 22:51"
        },
        {
          "id": "MDRW2100012530.1.3",
          "form": "우린 진짜 많이 갚았잖아 ㅎㅎ 지금처럼만 하면 돼",
          "original_form": "우린 진짜 많이 갚았잖아 ㅎㅎ 지금처럼만 하면 돼",
          "speaker_id": "2",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012530.1.4",
          "form": "이자로 나가는 거 너무 아까워서",
          "original_form": "이자로 나가는 거 너무 아까워서",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012530.1.5",
          "form": "그래봤자 몇만원인데",
          "original_form": "그래봤자 몇만원인데",
          "speaker_id": "2",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012530.1.6",
          "form": "고정으로 매달 몇만원도 크지 ㅠ",
          "original_form": "고정으로 매달 몇만원도 크지 ㅠ",
          "speaker_id": "1",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012530.1.7",
          "form": "다른 사람들은 매달 몇십만원도 내",
          "original_form": "다른 사람들은 매달 몇십만원도 내",
          "speaker_id": "2",
          "time": "20210824 22:52"
        },
        {
          "id": "MDRW2100012530.1.8",
          "form": "ㅎㅎ 그건 다른 사람 얘기고 난 만원도 아까움",
          "original_form": "ㅎㅎ 그건 다른 사람 얘기고 난 만원도 아까움",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012530.1.9",
          "form": "ㅋㅋ 짠순이",
          "original_form": "ㅋㅋ 짠순이",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012530.1.10",
          "form": "지금 대출금리가 얼마지?",
          "original_form": "지금 대출금리가 얼마지?",
          "speaker_id": "1",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012530.1.11",
          "form": "2.21 근데 곧 많이 오를 것 같아",
          "original_form": "2.21 근데 곧 많이 오를 것 같아",
          "speaker_id": "2",
          "time": "20210824 22:53"
        },
        {
          "id": "MDRW2100012530.1.12",
          "form": "맞아~ 요즘에 한 번 오르면 1프로는 기본이라던데",
          "original_form": "맞아~ 요즘에 한 번 오르면 1프로는 기본이라던데",
