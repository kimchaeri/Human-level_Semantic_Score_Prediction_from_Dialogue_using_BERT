      "utterance": [
        {
          "id": "MDRW2100009400.1.1",
          "form": "마스크는 있는지 치료는되는지",
          "original_form": "마스크는 있는지 치료는되는지",
          "speaker_id": "2",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009400.1.2",
          "form": "당장 쌀이며 의약품 지원해야 할텐데",
          "original_form": "당장 쌀이며 의약품 지원해야 할텐데",
          "speaker_id": "2",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009400.1.3",
          "form": "북한은 코로나걸리면 다 처벌한다는데요",
          "original_form": "북한은 코로나걸리면 다 처벌한다는데요",
          "speaker_id": "1",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009400.1.4",
          "form": "그렇더라구요",
          "original_form": "그렇더라구요",
          "speaker_id": "2",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009400.1.5",
          "form": "참나",
          "original_form": "참나",
          "speaker_id": "2",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009400.1.6",
          "form": "알려진바가 없어서 규모도 알기어렵네요",
          "original_form": "알려진바가 없어서 규모도 알기어렵네요",
          "speaker_id": "1",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009400.1.7",
          "form": "인도적지원하면 또 퍼준다고욕하고",
          "original_form": "인도적지원하면 또 퍼준다고욕하고",
          "speaker_id": "1",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009400.1.8",
          "form": "국제기구에서 지원하고 감시해주면좋겠어요",
          "original_form": "국제기구에서 지원하고 감시해주면좋겠어요",
          "speaker_id": "2",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009400.1.9",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009400.1.10",
          "form": "다른나라가 주는만큼도 안주는데",
          "original_form": "다른나라가 주는만큼도 안주는데",
          "speaker_id": "1",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009400.1.11",
          "form": "아니 우리나라 사람인데",
          "original_form": "아니 우리나라 사람인데",
          "speaker_id": "2",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009400.1.12",
          "form": "당연히 도와줘야죠",
          "original_form": "당연히 도와줘야죠",
          "speaker_id": "2",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009400.1.13",
          "form": "정재에 많이 쓰여서 긍가봐요",
          "original_form": "정재에 많이 쓰여서 긍가봐요",
          "speaker_id": "1",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009400.1.14",
          "form": "정쟁",
          "original_form": "정쟁",
          "speaker_id": "1",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009400.1.15",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009400.1.16",
          "form": "그사이에 다죽어가고",
          "original_form": "그사이에 다죽어가고",
          "speaker_id": "2",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009400.1.17",
          "form": "다른나라 지원봉사보다 십해요",
          "original_form": "다른나라 지원봉사보다 십해요",
          "speaker_id": "2",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009400.1.18",
          "form": "통일되야하는데 가치를따질수없는건데",
          "original_form": "통일되야하는데 가치를따질수없는건데",
          "speaker_id": "1",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009400.1.19",
          "form": "시급해요",
          "original_form": "시급해요",
          "speaker_id": "2",
          "time": "20210822 23:14"
        },
        {
          "id": "MDRW2100009400.1.20",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "2",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009400.1.21",
          "form": "사람들이 세금많이낼까봐 두려워해요",
          "original_form": "사람들이 세금많이낼까봐 두려워해요",
          "speaker_id": "1",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009400.1.22",
          "form": "안타깝네요  ㅎㅎ",
          "original_form": "안타깝네요  ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009400.1.23",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210822 23:15"
        },
        {
          "id": "MDRW2100009400.1.24",
          "form": "중국으로 북한여성들 인권유린이 심하게 일어나고있거든요",
          "original_form": "중국으로 북한여성들 인권유린이 심하게 일어나고있거든요",
          "speaker_id": "1",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009400.1.25",
          "form": "근데 통일은 될꺼고 서로도와서 어느정도 끌어 올려야죠",
          "original_form": "근데 통일은 될꺼고 서로도와서 어느정도 끌어 올려야죠",
          "speaker_id": "2",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009400.1.26",
          "form": "안타까워요",
          "original_form": "안타까워요",
          "speaker_id": "2",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009400.1.27",
          "form": "일자리준다고 탈북하고 성착취하고있거든요",
          "original_form": "일자리준다고 탈북하고 성착취하고있거든요",
          "speaker_id": "1",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009400.1.28",
          "form": "지원에 정치적 이용 안함좋겠어요",
          "original_form": "지원에 정치적 이용 안함좋겠어요",
          "speaker_id": "2",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009400.1.29",
          "form": "통일되면 그런일이 적어지겠죠",
          "original_form": "통일되면 그런일이 적어지겠죠",
          "speaker_id": "1",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009400.1.30",
          "form": "네ㅠㅠ저번에 도와주는 봉사하는 목사님보니",
          "original_form": "네ㅠㅠ저번에 도와주는 봉사하는 목사님보니",
          "speaker_id": "2",
          "time": "20210822 23:16"
        },
        {
          "id": "MDRW2100009400.1.31",
          "form": "인권이 말이 아니예요",
          "original_form": "인권이 말이 아니예요",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009400.1.32",
          "form": "근데 name2님같은 생각을 가진분들이 적어요ㅜ",
          "original_form": "근데 &name2&님같은 생각을 가진분들이 적어요ㅜ",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009400.1.33",
          "form": "그분은 참 탈북하는분 직접가서 데려오시고",
          "original_form": "그분은 참 탈북하는분 직접가서 데려오시고",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009400.1.34",
          "form": "네 저도 티비조선으로 봤어요",
          "original_form": "네 저도 티비조선으로 봤어요",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009400.1.35",
          "form": "전 진짜 그분 감동했어요",
          "original_form": "전 진짜 그분 감동했어요",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009400.1.36",
          "form": "질질울면서 봤어요",
          "original_form": "질질울면서 봤어요",
          "speaker_id": "1",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009400.1.37",
          "form": "저도요ㅠㅜㅜ",
          "original_form": "저도요ㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 23:17"
        },
        {
          "id": "MDRW2100009400.1.38",
