        },
        {
          "id": "MDRW2100019020.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019020.1.3",
          "form": "점심드셨나요?",
          "original_form": "점심드셨나요?",
          "speaker_id": "2",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019020.1.4",
          "form": "전오늘 육개장국밥 먹었어요~",
          "original_form": "전오늘 육개장국밥 먹었어요~",
          "speaker_id": "1",
          "time": "20210827 14:43"
        },
        {
          "id": "MDRW2100019020.1.5",
          "form": "전 돈까스 먹었어요",
          "original_form": "전 돈까스 먹었어요",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019020.1.6",
          "form": "날씨가 흐려서 칼칼하고 좋았을거같아요!",
          "original_form": "날씨가 흐려서 칼칼하고 좋았을거같아요!",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019020.1.7",
          "form": "오 어느부위 드셨나요?",
          "original_form": "오 어느부위 드셨나요?",
          "speaker_id": "1",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019020.1.8",
          "form": "등심 먹었어요!",
          "original_form": "등심 먹었어요!",
          "speaker_id": "2",
          "time": "20210827 14:45"
        },
        {
          "id": "MDRW2100019020.1.9",
          "form": "아하! 등심좋아하시나봐요! 전 안심좋아해요",
          "original_form": "아하! 등심좋아하시나봐요! 전 안심좋아해요",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019020.1.10",
          "form": "안심도 부드럽고 좋죠!",
          "original_form": "안심도 부드럽고 좋죠!",
          "speaker_id": "2",
          "time": "20210827 14:49"
        },
        {
          "id": "MDRW2100019020.1.11",
          "form": "맞아요 ㅎㅎ 돈가스 뭐에찍어드시나요?",
          "original_form": "맞아요 ㅎㅎ 돈가스 뭐에찍어드시나요?",
          "speaker_id": "1",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019020.1.12",
          "form": "여기는 일반 소스랑 와사비 주는데 생각보다 와사비가 잘어울리더라구요",
          "original_form": "여기는 일반 소스랑 와사비 주는데 생각보다 와사비가 잘어울리더라구요",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019020.1.13",
          "form": "아하~ 전 소금에 찍어먹어요 ㅋㅋ",
          "original_form": "아하~ 전 소금에 찍어먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019020.1.14",
          "form": "소금에 찍어먹으면 담백하겠네요",
          "original_form": "소금에 찍어먹으면 담백하겠네요",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019020.1.15",
          "form": "맞아요 ㅋㅋ 그래서 쉽게 안질리는거같아요",
          "original_form": "맞아요 ㅋㅋ 그래서 쉽게 안질리는거같아요",
          "speaker_id": "1",
          "time": "20210827 14:54"
        }
