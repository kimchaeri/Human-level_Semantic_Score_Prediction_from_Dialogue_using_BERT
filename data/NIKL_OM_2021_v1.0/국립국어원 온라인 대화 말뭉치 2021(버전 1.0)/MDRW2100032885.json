      "utterance": [
        {
          "id": "MDRW2100032885.1.1",
          "form": "그쵸 뭔가 특별한 날에",
          "original_form": "그쵸 뭔가 특별한 날에",
          "speaker_id": "1",
          "time": "20210906 03:52"
        },
        {
          "id": "MDRW2100032885.1.2",
          "form": "취직이나 그런 이벤트로 해보고싶조 레터링케이크!",
          "original_form": "취직이나 그런 이벤트로 해보고싶조 레터링케이크!",
          "speaker_id": "1",
          "time": "20210906 03:53"
        },
        {
          "id": "MDRW2100032885.1.3",
          "form": "어버이날이나?",
          "original_form": "어버이날이나?",
          "speaker_id": "1",
          "time": "20210906 03:53"
        },
        {
          "id": "MDRW2100032885.1.4",
          "form": "완전 좋을 것 같아요ㅠㅠ",
          "original_form": "완전 좋을 것 같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 03:54"
        },
        {
          "id": "MDRW2100032885.1.5",
          "form": "생일이라든지 환갑이나 누구 결혼한다고 했을때라든지ㅠㅠ",
          "original_form": "생일이라든지 환갑이나 누구 결혼한다고 했을때라든지ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 03:55"
        },
        {
          "id": "MDRW2100032885.1.6",
          "form": "맞아요ㅋㅋㅋ감동받아줬으면...!",
          "original_form": "맞아요ㅋㅋㅋ감동받아줬으면...!",
          "speaker_id": "1",
          "time": "20210906 03:55"
        },
        {
          "id": "MDRW2100032885.1.7",
          "form": "저도한번 미니케이크 받아봤는데 감동 완전받앗엇어요",
          "original_form": "저도한번 미니케이크 받아봤는데 감동 완전받앗엇어요",
          "speaker_id": "1",
          "time": "20210906 03:56"
        },
        {
          "id": "MDRW2100032885.1.8",
          "form": "대신 맛은 쏘쏘했어요",
          "original_form": "대신 맛은 쏘쏘했어요",
          "speaker_id": "1",
          "time": "20210906 03:56"
        },
        {
          "id": "MDRW2100032885.1.9",
          "form": "헉 진짜요? 누가 생각해서 이렇게 해주면 정말 감동이죠ㅠㅠ",
          "original_form": "헉 진짜요? 누가 생각해서 이렇게 해주면 정말 감동이죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 03:57"
        },
        {
          "id": "MDRW2100032885.1.10",
          "form": "생일이었는데 괜히 감동받아서",
          "original_form": "생일이었는데 괜히 감동받아서",
          "speaker_id": "1",
          "time": "20210906 03:58"
        },
        {
          "id": "MDRW2100032885.1.11",
          "form": "또 인스타에 올렸죠...",
          "original_form": "또 인스타에 올렸죠...",
          "speaker_id": "1",
          "time": "20210906 03:58"
        },
        {
          "id": "MDRW2100032885.1.12",
          "form": "다음엔 제가 주문해보려구요1",
          "original_form": "다음엔 제가 주문해보려구요1",
          "speaker_id": "1",
          "time": "20210906 03:58"
        },
        {
          "id": "MDRW2100032885.1.13",
          "form": "그런걸 받으면 당연 자랑하셔야해요!",
          "original_form": "그런걸 받으면 당연 자랑하셔야해요!",
          "speaker_id": "2",
          "time": "20210906 03:58"
        },
        {
          "id": "MDRW2100032885.1.14",
          "form": "ㅎㅎ그렇게 말해줘서 고마워요",
          "original_form": "ㅎㅎ그렇게 말해줘서 고마워요",
          "speaker_id": "1",
          "time": "20210906 03:59"
        },
        {
          "id": "MDRW2100032885.1.15",
          "form": "레터링 케이크는",
          "original_form": "레터링 케이크는",
          "speaker_id": "1",
          "time": "20210906 03:59"
        },
        {
          "id": "MDRW2100032885.1.16",
          "form": "한 십만원하죠? 케이크치고도ㅠ",
          "original_form": "한 십만원하죠? 케이크치고도ㅠ",
          "speaker_id": "1",
          "time": "20210906 03:59"
        },
        {
          "id": "MDRW2100032885.1.17",
          "form": "좀 비싸더라구여ㅠㅠ",
          "original_form": "좀 비싸더라구여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 03:59"
        },
        {
          "id": "MDRW2100032885.1.18",
          "form": "잘하는 집은 5만원부터 시작하는 것 같더라구요ㅠㅠ",
          "original_form": "잘하는 집은 5만원부터 시작하는 것 같더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 03:59"
        },
        {
          "id": "MDRW2100032885.1.19",
          "form": "아고ㅠ그래도 알아보셨나봐요",
          "original_form": "아고ㅠ그래도 알아보셨나봐요",
          "speaker_id": "1",
          "time": "20210906 04:00"
        },
        {
          "id": "MDRW2100032885.1.20",
          "form": "미니는 3만원선에서 케이크 되던데",
          "original_form": "미니는 3만원선에서 케이크 되던데",
          "speaker_id": "1",
          "time": "20210906 04:00"
        },
        {
          "id": "MDRW2100032885.1.21",
          "form": "네ㅠㅠ 저두 해보고 싶어서 알아봤는데 마음에 드는거 하려면 한 8만원돈은 깨지겠더라구요ㅠㅠ",
          "original_form": "네ㅠㅠ 저두 해보고 싶어서 알아봤는데 마음에 드는거 하려면 한 8만원돈은 깨지겠더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 04:02"
        },
        {
          "id": "MDRW2100032885.1.22",
          "form": "ㅋㅋㅋ혹시 어떤 케이크인지 물어봐도 되나요!?",
          "original_form": "ㅋㅋㅋ혹시 어떤 케이크인지 물어봐도 되나요!?",
          "speaker_id": "1",
          "time": "20210906 04:02"
        },
        {
          "id": "MDRW2100032885.1.23",
          "form": "찾아보고 왔어요",
          "original_form": "찾아보고 왔어요",
          "speaker_id": "2",
          "time": "20210906 04:08"
        },
        {
          "id": "MDRW2100032885.1.24",
          "form": "다정하시네요...",
          "original_form": "다정하시네요...",
          "speaker_id": "1",
          "time": "20210906 04:09"
        }
      ]
    }
