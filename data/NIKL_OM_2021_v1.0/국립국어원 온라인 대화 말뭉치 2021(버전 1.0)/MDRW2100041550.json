        },
        {
          "id": "MDRW2100041550.1.2",
          "form": "니는 4시까지아니가?",
          "original_form": "니는 4시까지아니가?",
          "speaker_id": "2",
          "time": "20210906 11:29"
        },
        {
          "id": "MDRW2100041550.1.3",
          "form": "애들하원전까지?",
          "original_form": "애들하원전까지?",
          "speaker_id": "2",
          "time": "20210906 11:29"
        },
        {
          "id": "MDRW2100041550.1.4",
          "form": "엉 그렇지",
          "original_form": "엉 그렇지",
          "speaker_id": "1",
          "time": "20210906 11:36"
        },
        {
          "id": "MDRW2100041550.1.5",
          "form": "난 4시\nㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "난 4시\nㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210906 11:36"
        },
        {
          "id": "MDRW2100041550.1.6",
          "form": "12시부터 4시까지",
          "original_form": "12시부터 4시까지",
          "speaker_id": "1",
          "time": "20210906 11:36"
        },
        {
          "id": "MDRW2100041550.1.7",
          "form": "놀수있음",
          "original_form": "놀수있음",
          "speaker_id": "1",
          "time": "20210906 11:36"
        },
        {
          "id": "MDRW2100041550.1.8",
          "form": "그래 112동에서ㅈ본다네ㅋ",
          "original_form": "그래 112동에서ㅈ본다네ㅋ",
          "speaker_id": "2",
          "time": "20210906 12:50"
        },
        {
          "id": "MDRW2100041550.1.9",
          "form": "난 이제점심먹을려고ㅋ",
          "original_form": "난 이제점심먹을려고ㅋ",
          "speaker_id": "2",
          "time": "20210906 12:50"
        },
        {
          "id": "MDRW2100041550.1.10",
          "form": "엉 112동으로 오슈",
          "original_form": "엉 112동으로 오슈",
          "speaker_id": "1",
          "time": "20210906 13:01"
        },
        {
          "id": "MDRW2100041550.1.11",
          "form": "커피마실때 날라오슈",
          "original_form": "커피마실때 날라오슈",
          "speaker_id": "1",
          "time": "20210906 13:01"
        },
        {
          "id": "MDRW2100041550.1.12",
          "form": "그래ㅋㅋ",
          "original_form": "그래ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100041550.1.13",
          "form": "난 커피 지금마시고있음",
          "original_form": "난 커피 지금마시고있음",
          "speaker_id": "2",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100041550.1.14",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100041550.1.15",
          "form": "아아 좋지",
          "original_form": "아아 좋지",
          "speaker_id": "1",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100041550.1.16",
          "form": "점심 뭐 먹었어요??",
          "original_form": "점심 뭐 먹었어요??",
          "speaker_id": "1",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100041550.1.17",
          "form": "오늘 떡ㅋㅋ",
          "original_form": "오늘 떡ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100041550.1.18",
          "form": "배부르다ㅋ",
          "original_form": "배부르다ㅋ",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100041550.1.19",
          "form": "좀이따집에가야지",
          "original_form": "좀이따집에가야지",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100041550.1.20",
          "form": "ㅋㅋㅋㅋㅋ 떡은 간식이지",
          "original_form": "ㅋㅋㅋㅋㅋ 떡은 간식이지",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041550.1.21",
          "form": "언니 왜 소식하노 ㅎㅎ",
          "original_form": "언니 왜 소식하노 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100041550.1.22",
          "form": "아니 떡이 양이ㅡㅡ",
          "original_form": "아니 떡이 양이ㅡㅡ",
          "speaker_id": "2",
          "time": "20210906 15:33"
        },
        {
          "id": "MDRW2100041550.1.23",
          "form": "거의오늘하루칼로리넘어섬",
          "original_form": "거의오늘하루칼로리넘어섬",
          "speaker_id": "2",
          "time": "20210906 15:34"
        },
        {
          "id": "MDRW2100041550.1.24",
          "form": "아칼로리로 계산하는구만",
          "original_form": "아칼로리로 계산하는구만",
          "speaker_id": "1",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100041550.1.25",
          "form": "칼로리는 어렵다",
          "original_form": "칼로리는 어렵다",
          "speaker_id": "1",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100041550.1.26",
          "form": "배가계속나오네ㅋㅋ",
          "original_form": "배가계속나오네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:17"
        },
        {
          "id": "MDRW2100041550.1.27",
          "form": "오늘 저녁은 분식할까한다ㅋㅋ",
          "original_form": "오늘 저녁은 분식할까한다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 16:17"
        },
        {
          "id": "MDRW2100041550.1.28",
          "form": "내맘대로 저녁",
          "original_form": "내맘대로 저녁",
          "speaker_id": "2",
          "time": "20210906 16:17"
        },
        {
          "id": "MDRW2100041550.1.29",
          "form": "분식 좋지",
          "original_form": "분식 좋지",
          "speaker_id": "1",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100041550.1.30",
          "form": "시켜 먹는 거겠지 당연히??ㅋㅋㅋ",
          "original_form": "시켜 먹는 거겠지 당연히??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 16:45"
        },
        {
          "id": "MDRW2100041550.1.31",
          "form": "어제먹은 김밥 이랑 떡볶이만해서ㅋ",
          "original_form": "어제먹은 김밥 이랑 떡볶이만해서ㅋ",
          "speaker_id": "2",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100041550.1.32",
          "form": "만두랑",
          "original_form": "만두랑",
          "speaker_id": "2",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100041550.1.33",
          "form": "니는 뭐먹노",
          "original_form": "니는 뭐먹노",
          "speaker_id": "2",
          "time": "20210906 16:51"
        },
        {
          "id": "MDRW2100041550.1.34",
          "form": "나 오늘은",
          "original_form": "나 오늘은",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041550.1.35",
          "form": "반찬 나무 숟가락에서",
          "original_form": "반찬 나무 숟가락에서",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041550.1.36",
          "form": "국이랑 반찬 사왔어",
          "original_form": "국이랑 반찬 사왔어",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041550.1.37",
          "form": "월요일은 아무것도 하기 싫음",
          "original_form": "월요일은 아무것도 하기 싫음",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041550.1.38",
          "form": "화요일도 하기 싫지만 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "화요일도 하기 싫지만 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 17:35"
        },
        {
          "id": "MDRW2100041550.1.39",
          "form": "수요일도 싫을껄?ㅋㅋ",
          "original_form": "수요일도 싫을껄?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041550.1.40",
          "form": "냉파중이다 나는",
          "original_form": "냉파중이다 나는",
          "speaker_id": "2",
          "time": "20210906 17:55"
        },
        {
          "id": "MDRW2100041550.1.41",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋ",
