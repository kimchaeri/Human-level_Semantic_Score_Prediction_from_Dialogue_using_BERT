        {
          "id": "MDRW2100027121.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100027121.1.2",
          "form": "저는 귀찮아서 잘안해요",
          "original_form": "저는 귀찮아서 잘안해요",
          "speaker_id": "2",
          "time": "20210902 09:51"
        },
        {
          "id": "MDRW2100027121.1.3",
          "form": "당근마켓해보셨나요?",
          "original_form": "당근마켓해보셨나요?",
          "speaker_id": "2",
          "time": "20210902 09:51"
        },
        {
          "id": "MDRW2100027121.1.4",
          "form": "네 당근이요",
          "original_form": "네 당근이요",
          "speaker_id": "1",
          "time": "20210902 09:52"
        },
        {
          "id": "MDRW2100027121.1.5",
          "form": "어떤것 팔아보셨나요",
          "original_form": "어떤것 팔아보셨나요",
          "speaker_id": "2",
          "time": "20210902 09:53"
        },
        {
          "id": "MDRW2100027121.1.6",
          "form": "공기청정기요",
          "original_form": "공기청정기요",
          "speaker_id": "1",
          "time": "20210902 09:56"
        },
        {
          "id": "MDRW2100027121.1.7",
          "form": "오 왜 파셨어요??",
          "original_form": "오 왜 파셨어요??",
          "speaker_id": "2",
          "time": "20210902 09:58"
        },
        {
          "id": "MDRW2100027121.1.8",
          "form": "2개라서요",
          "original_form": "2개라서요",
          "speaker_id": "1",
          "time": "20210902 09:59"
        },
        {
          "id": "MDRW2100027121.1.9",
          "form": "아 ㅋㅋ왜 2개나 사셨었어요?",
          "original_form": "아 ㅋㅋ왜 2개나 사셨었어요?",
          "speaker_id": "2",
          "time": "20210902 10:08"
        },
        {
          "id": "MDRW2100027121.1.10",
          "form": "1개는 선물이여",
          "original_form": "1개는 선물이여",
          "speaker_id": "1",
          "time": "20210902 10:21"
        },
        {
          "id": "MDRW2100027121.1.11",
          "form": "아항 공기청정기 있으면 확실히 좋나요?",
          "original_form": "아항 공기청정기 있으면 확실히 좋나요?",
          "speaker_id": "2",
          "time": "20210902 10:46"
        },
        {
          "id": "MDRW2100027121.1.12",
          "form": "느낌상 ㅎㅎ",
          "original_form": "느낌상 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 10:51"
        },
        {
          "id": "MDRW2100027121.1.13",
          "form": "제습기도 써보셨나요?",
