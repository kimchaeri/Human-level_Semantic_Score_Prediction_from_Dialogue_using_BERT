      "utterance": [
        {
          "id": "MDRW2100026305.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026305.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026305.1.3",
          "form": "저녁식사 하셨어요?",
          "original_form": "저녁식사 하셨어요?",
          "speaker_id": "2",
          "time": "20210901 21:25"
        },
        {
          "id": "MDRW2100026305.1.4",
          "form": "네 name2님도 저녁 드셨나요?",
          "original_form": "네 &name2&님도 저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 21:27"
        },
        {
          "id": "MDRW2100026305.1.5",
          "form": "네!  김밥싸서 먹었어요.  숙주나물 넣은 라면하구요",
          "original_form": "네!  김밥싸서 먹었어요.  숙주나물 넣은 라면하구요",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026305.1.6",
          "form": "뭐 드셨어요?",
          "original_form": "뭐 드셨어요?",
          "speaker_id": "2",
          "time": "20210901 21:28"
        },
        {
          "id": "MDRW2100026305.1.7",
          "form": "저는 비빔냉면 먹었어요",
          "original_form": "저는 비빔냉면 먹었어요",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026305.1.8",
          "form": "숙주나물에 라면 괜찮나요??",
          "original_form": "숙주나물에 라면 괜찮나요??",
          "speaker_id": "1",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026305.1.9",
          "form": "네!  맛있어요.",
          "original_form": "네!  맛있어요.",
          "speaker_id": "2",
          "time": "20210901 21:29"
        },
        {
          "id": "MDRW2100026305.1.10",
          "form": "숙주나물은 어떤요리에도 잘 어울리는것같아여",
          "original_form": "숙주나물은 어떤요리에도 잘 어울리는것같아여",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026305.1.11",
          "form": "비냉도 맛있겠어요",
          "original_form": "비냉도 맛있겠어요",
          "speaker_id": "2",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026305.1.12",
          "form": "비냉은 그냥저냥이었답니다.\r\n그럼 숙주나물 좋아하시나요?",
          "original_form": "비냉은 그냥저냥이었답니다.\r\n그럼 숙주나물 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 21:30"
        },
        {
          "id": "MDRW2100026305.1.13",
          "form": "배홍동 비빔면 드셔보셨어요?  전 이거 맛있던데ㅎㅎ",
          "original_form": "배홍동 비빔면 드셔보셨어요?  전 이거 맛있던데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026305.1.14",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210901 21:31"
        },
        {
          "id": "MDRW2100026305.1.15",
          "form": "어떤 음식좋아해요?",
          "original_form": "어떤 음식좋아해요?",
          "speaker_id": "2",
          "time": "20210901 21:33"
        },
        {
          "id": "MDRW2100026305.1.16",
          "form": "배홍동 한번 찾아봐야겠네요\r\n저는 떡볶이 좋아해요",
          "original_form": "배홍동 한번 찾아봐야겠네요\r\n저는 떡볶이 좋아해요",
          "speaker_id": "1",
          "time": "20210901 21:34"
        },
        {
          "id": "MDRW2100026305.1.17",
          "form": "저도 떡볶이 좋아요!",
          "original_form": "저도 떡볶이 좋아요!",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100026305.1.18",
          "form": "좋아하는 떡볶이 브랜드 있으신가요?",
          "original_form": "좋아하는 떡볶이 브랜드 있으신가요?",
          "speaker_id": "1",
          "time": "20210901 21:41"
        },
        {
          "id": "MDRW2100026305.1.19",
          "form": "주로 마트에서 사서 집에서 해먹어요",
          "original_form": "주로 마트에서 사서 집에서 해먹어요",
          "speaker_id": "2",
          "time": "20210901 21:42"
        },
        {
          "id": "MDRW2100026305.1.20",
          "form": "쌀떡 좋아하세요?  밀떡 좋아하세요?",
          "original_form": "쌀떡 좋아하세요?  밀떡 좋아하세요?",
          "speaker_id": "2",
          "time": "20210901 21:44"
        },
        {
          "id": "MDRW2100026305.1.21",
          "form": "저는 밀떡이요",
          "original_form": "저는 밀떡이요",
          "speaker_id": "1",
          "time": "20210901 21:48"
        }
      ]
