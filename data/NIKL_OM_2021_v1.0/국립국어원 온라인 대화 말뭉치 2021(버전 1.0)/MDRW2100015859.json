      "utterance": [
        {
          "id": "MDRW2100015859.1.1",
          "form": "학생이세요?",
          "original_form": "학생이세요?",
          "speaker_id": "1",
          "time": "20210826 01:54"
        },
        {
          "id": "MDRW2100015859.1.2",
          "form": "아니요ㅋㅋ쥐어짜볼게요ㅋㅋㅋㅋ",
          "original_form": "아니요ㅋㅋ쥐어짜볼게요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:57"
        },
        {
          "id": "MDRW2100015859.1.3",
          "form": "혹시 학생이세여?",
          "original_form": "혹시 학생이세여?",
          "speaker_id": "2",
          "time": "20210826 01:57"
        },
        {
          "id": "MDRW2100015859.1.4",
          "form": "ㅋㅋㅋ 저도 학생은 아니예요ㅋㄱ",
          "original_form": "ㅋㅋㅋ 저도 학생은 아니예요ㅋㄱ",
          "speaker_id": "1",
          "time": "20210826 01:57"
        },
        {
          "id": "MDRW2100015859.1.5",
          "form": "학창시절 어떻게 보내셨나요?",
          "original_form": "학창시절 어떻게 보내셨나요?",
          "speaker_id": "1",
          "time": "20210826 01:57"
        },
        {
          "id": "MDRW2100015859.1.6",
          "form": "재밌게 놀면서 보냈어요ㅋㅋ공부좀할껄ㅜㅜ",
          "original_form": "재밌게 놀면서 보냈어요ㅋㅋ공부좀할껄ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 01:58"
        },
        {
          "id": "MDRW2100015859.1.7",
          "form": "모범생은 아니었어요..",
          "original_form": "모범생은 아니었어요..",
          "speaker_id": "2",
          "time": "20210826 01:58"
        },
        {
          "id": "MDRW2100015859.1.8",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 01:58"
        },
        {
          "id": "MDRW2100015859.1.9",
          "form": "저도요ㅋㅋ",
          "original_form": "저도요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:00"
        },
        {
          "id": "MDRW2100015859.1.10",
          "form": "하라는 공부는 안하고",
          "original_form": "하라는 공부는 안하고",
          "speaker_id": "1",
          "time": "20210826 02:00"
        },
        {
          "id": "MDRW2100015859.1.11",
          "form": "전자사전에 영화 다운 받아서",
          "original_form": "전자사전에 영화 다운 받아서",
          "speaker_id": "1",
          "time": "20210826 02:00"
        },
        {
          "id": "MDRW2100015859.1.12",
          "form": "몰래보고 그랬어요",
          "original_form": "몰래보고 그랬어요",
          "speaker_id": "1",
          "time": "20210826 02:00"
        },
        {
          "id": "MDRW2100015859.1.13",
          "form": "저두요ㅋㅋㅋ걸리기도 잘걸려서 책상도 빼고ㅠㅠ",
          "original_form": "저두요ㅋㅋㅋ걸리기도 잘걸려서 책상도 빼고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 02:01"
        },
        {
          "id": "MDRW2100015859.1.14",
          "form": "지각도 자주했어요ㅋㅋㅋ",
          "original_form": "지각도 자주했어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:02"
        },
        {
          "id": "MDRW2100015859.1.15",
          "form": "아침에 일어나기 힘들더라고요",
          "original_form": "아침에 일어나기 힘들더라고요",
          "speaker_id": "1",
          "time": "20210826 02:02"
        },
        {
          "id": "MDRW2100015859.1.16",
          "form": "전 지각은 안했습니다",
          "original_form": "전 지각은 안했습니다",
          "speaker_id": "2",
          "time": "20210826 02:02"
        },
        {
          "id": "MDRW2100015859.1.17",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 02:02"
        },
        {
          "id": "MDRW2100015859.1.18",
          "form": "동아리해보셨나요?",
          "original_form": "동아리해보셨나요?",
          "speaker_id": "1",
          "time": "20210826 02:02"
        },
        {
          "id": "MDRW2100015859.1.19",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "2",
          "time": "20210826 02:03"
        },
        {
          "id": "MDRW2100015859.1.20",
          "form": "천문학동아리",
          "original_form": "천문학동아리",
          "speaker_id": "2",
          "time": "20210826 02:03"
        },
        {
          "id": "MDRW2100015859.1.21",
          "form": "와우 미래의 과학자?",
          "original_form": "와우 미래의 과학자?",
          "speaker_id": "1",
          "time": "20210826 02:04"
        },
        {
          "id": "MDRW2100015859.1.22",
          "form": "저는 십자수 동아리했어요",
          "original_form": "저는 십자수 동아리했어요",
          "speaker_id": "1",
          "time": "20210826 02:04"
        },
        {
          "id": "MDRW2100015859.1.23",
          "form": "짜장면준다고해서ㅋㅋ",
          "original_form": "짜장면준다고해서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:04"
        },
        {
          "id": "MDRW2100015859.1.24",
          "form": "오오ㅋㅋㅋ십자수 잘하세요?!",
          "original_form": "오오ㅋㅋㅋ십자수 잘하세요?!",
