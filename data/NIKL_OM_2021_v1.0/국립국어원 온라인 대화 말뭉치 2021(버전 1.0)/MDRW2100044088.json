      "utterance": [
        {
          "id": "MDRW2100044088.1.1",
          "form": "아 역시 배우신분!",
          "original_form": "아 역시 배우신분!",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044088.1.2",
          "form": "당연한 코스에요!!",
          "original_form": "당연한 코스에요!!",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044088.1.3",
          "form": "안먹으면 허전한거죠",
          "original_form": "안먹으면 허전한거죠",
          "speaker_id": "2",
          "time": "20210907 21:32"
        },
        {
          "id": "MDRW2100044088.1.4",
          "form": "ㅋㅋㅋㅋㅋ근데 남자들은 그걸 이해못해요!!!",
          "original_form": "ㅋㅋㅋㅋㅋ근데 남자들은 그걸 이해못해요!!!",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044088.1.5",
          "form": "밥 배 따로 디저트 배 따로 있건만!!!",
          "original_form": "밥 배 따로 디저트 배 따로 있건만!!!",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044088.1.6",
          "form": "아니.. 그건.. 당연한건데!!",
          "original_form": "아니.. 그건.. 당연한건데!!",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100044088.1.7",
          "form": "어떻게 그걸.. 같은 배라고 생각을..!!!",
          "original_form": "어떻게 그걸.. 같은 배라고 생각을..!!!",
          "speaker_id": "2",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100044088.1.8",
          "form": "아 맞다 카이막이라고 혹시 들어보셨나요?",
          "original_form": "아 맞다 카이막이라고 혹시 들어보셨나요?",
          "speaker_id": "2",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100044088.1.9",
          "form": "ㅋㅋㅋㅋㅋ아뇨?",
          "original_form": "ㅋㅋㅋㅋㅋ아뇨?",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044088.1.10",
          "form": "카이막이 뭔가용",
          "original_form": "카이막이 뭔가용",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044088.1.11",
          "form": "터키 음식인데 백종원이 음식 소개해주는 영상을 보고 저도 알게 된 음식인데요",
          "original_form": "터키 음식인데 백종원이 음식 소개해주는 영상을 보고 저도 알게 된 음식인데요",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044088.1.12",
          "form": "오...찾아봐야겠는데오?",
          "original_form": "오...찾아봐야겠는데오?",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044088.1.13",
          "form": "우유를 오랫동안 끓여서 분리되는 특별한 층을 걷어내서 만들어낸 음식이에요",
          "original_form": "우유를 오랫동안 끓여서 분리되는 특별한 층을 걷어내서 만들어낸 음식이에요",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044088.1.14",
          "form": "그게 백종원 선생님이 만드려고 하셨는데 가성비가 안나오신다고",
          "original_form": "그게 백종원 선생님이 만드려고 하셨는데 가성비가 안나오신다고",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044088.1.15",
          "form": "포기한 음식이기도 한데",
          "original_form": "포기한 음식이기도 한데",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044088.1.16",
          "form": "그게 천상에 맛이래요 ㅋㅋ",
          "original_form": "그게 천상에 맛이래요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044088.1.17",
          "form": "우와....!!!",
          "original_form": "우와....!!!",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044088.1.18",
          "form": "혹시 드셔보셨나요?",
          "original_form": "혹시 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044088.1.19",
          "form": "그게 홍대에서 팔고 있어요...",
          "original_form": "그게 홍대에서 팔고 있어요...",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044088.1.20",
          "form": "저는 모센즈 스위츠 라는 곳에거 먹었는데",
          "original_form": "저는 모센즈 스위츠 라는 곳에거 먹었는데",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044088.1.21",
          "form": "거기 발르슈트라고하는 우유에 꿀을 넣은 음료도 꼭 같이 드셔봐야해요!",
          "original_form": "거기 발르슈트라고하는 우유에 꿀을 넣은 음료도 꼭 같이 드셔봐야해요!",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044088.1.22",
          "form": "아니...!!!홍대에요????",
          "original_form": "아니...!!!홍대에요????",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044088.1.23",
          "form": "헐...!!!!",
          "original_form": "헐...!!!!",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044088.1.24",
          "form": "ㅜㅜ올해 꼭갑니다",
          "original_form": "ㅜㅜ올해 꼭갑니다",
          "speaker_id": "1",
          "time": "20210907 21:42"
        },
        {
          "id": "MDRW2100044088.1.25",
          "form": "홍대 자주가세요?",
          "original_form": "홍대 자주가세요?",
          "speaker_id": "1",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044088.1.26",
          "form": "네!! 홍대에 있어요",
          "original_form": "네!! 홍대에 있어요",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044088.1.27",
          "form": "꽤 자주 놀러가는것 같아요",
          "original_form": "꽤 자주 놀러가는것 같아요",
          "speaker_id": "2",
          "time": "20210907 21:43"
        },
        {
          "id": "MDRW2100044088.1.28",
          "form": "name1님도 자주 가시나요?",
          "original_form": "&name1&님도 자주 가시나요?",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044088.1.29",
          "form": "저는 자주 갔었죠ㅋㅋㅋ요즘에는 못간지 꽤 되었어요ㅜㅜ",
          "original_form": "저는 자주 갔었죠ㅋㅋㅋ요즘에는 못간지 꽤 되었어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044088.1.30",
          "form": "혹시 그 양있는 카페도 가셨나요?",
          "original_form": "혹시 그 양있는 카페도 가셨나요?",
          "speaker_id": "1",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044088.1.31",
          "form": "거기 와플이 맛있는데",
          "original_form": "거기 와플이 맛있는데",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044088.1.32",
          "form": "아 양이 있는 카페!",
          "original_form": "아 양이 있는 카페!",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044088.1.33",
          "form": "매번 지나가면서 양이 있네 하고 지나만 갔는데",
          "original_form": "매번 지나가면서 양이 있네 하고 지나만 갔는데",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044088.1.34",
          "form": "와플맛집이라니...",
          "original_form": "와플맛집이라니...",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100044088.1.35",
          "form": "와플위에 아이스크림있는 메뉴가있는데",
          "original_form": "와플위에 아이스크림있는 메뉴가있는데",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044088.1.36",
          "form": "와플이 따끈하게 나와서 아이스크림에 와플을 딱 찍어서먹으면",
          "original_form": "와플이 따끈하게 나와서 아이스크림에 와플을 딱 찍어서먹으면",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044088.1.37",
          "form": "하아.....",
          "original_form": "하아.....",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044088.1.38",
          "form": "진짜 진짜 맛있어요",
          "original_form": "진짜 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210907 22:01"
        },
        {
          "id": "MDRW2100044088.1.39",
          "form": "와플대학의 와플이랑은 또 다른 느낌인거죠?",
          "original_form": "와플대학의 와플이랑은 또 다른 느낌인거죠?",
          "speaker_id": "2",
          "time": "20210907 22:05"
        },
        {
          "id": "MDRW2100044088.1.40",
          "form": "네!! 그런 와플과는 다른스타일이에요",
          "original_form": "네!! 그런 와플과는 다른스타일이에요",
          "speaker_id": "1",
          "time": "20210907 22:06"
        },
        {
          "id": "MDRW2100044088.1.41",
          "form": "약간 더 고급진느낌의 와플???",
          "original_form": "약간 더 고급진느낌의 와플???",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044088.1.42",
          "form": "와플대학 와플은 식감이 과자같다면 저기는.. 진짜 빵의 느낌...?",
          "original_form": "와플대학 와플은 식감이 과자같다면 저기는.. 진짜 빵의 느낌...?",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044088.1.43",
          "form": "흠.. 벨기에 와플에 가깝나요??",
          "original_form": "흠.. 벨기에 와플에 가깝나요??",
          "speaker_id": "2",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044088.1.44",
          "form": "벨기에와플과 비슷하긴한데",
          "original_form": "벨기에와플과 비슷하긴한데",
          "speaker_id": "1",
          "time": "20210907 22:07"
        },
        {
          "id": "MDRW2100044088.1.45",
          "form": "저는 벨기에와플을 안좋아하거든요.. ? 근데저거는 맛있어요",
          "original_form": "저는 벨기에와플을 안좋아하거든요.. ? 근데저거는 맛있어요",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044088.1.46",
          "form": "뭐라고 설명해야하지...어렵네요 ㅜ",
          "original_form": "뭐라고 설명해야하지...어렵네요 ㅜ",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100044088.1.47",
          "form": "옹.. 역시 직접 가서 먹어보는게 확실하겠군요",
          "original_form": "옹.. 역시 직접 가서 먹어보는게 확실하겠군요",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100044088.1.48",
          "form": "양이 귀엽네로 끝내다니... 내려가볼 생각을 안하다니이ㅣ ㅠ",
          "original_form": "양이 귀엽네로 끝내다니... 내려가볼 생각을 안하다니이ㅣ ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:11"
        }
