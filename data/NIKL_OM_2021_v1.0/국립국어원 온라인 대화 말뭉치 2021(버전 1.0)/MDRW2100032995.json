      "utterance": [
        {
          "id": "MDRW2100032995.1.1",
          "form": "아침에 보니까 치약 다 떨어졌더라",
          "original_form": "아침에 보니까 치약 다 떨어졌더라",
          "speaker_id": "1",
          "time": "20210906 12:27"
        },
        {
          "id": "MDRW2100032995.1.2",
          "form": "ㅇㅇ 새로 사야지",
          "original_form": "ㅇㅇ 새로 사야지",
          "speaker_id": "2",
          "time": "20210906 12:27"
        },
        {
          "id": "MDRW2100032995.1.3",
          "form": "이번에 지마켓에서 쿠폰 많이 뿌리던데 활용 좀 해야겠네 ㅎㅎ",
          "original_form": "이번에 지마켓에서 쿠폰 많이 뿌리던데 활용 좀 해야겠네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 12:27"
        },
        {
          "id": "MDRW2100032995.1.4",
          "form": "한 번에 좀 많이 사둬",
          "original_form": "한 번에 좀 많이 사둬",
          "speaker_id": "2",
          "time": "20210906 12:28"
        },
        {
          "id": "MDRW2100032995.1.5",
          "form": "치약은 유통기한 상관 없나?",
          "original_form": "치약은 유통기한 상관 없나?",
          "speaker_id": "1",
          "time": "20210906 12:28"
        },
        {
          "id": "MDRW2100032995.1.6",
          "form": "유통기한 별로 상관없지 않을까?",
          "original_form": "유통기한 별로 상관없지 않을까?",
          "speaker_id": "2",
          "time": "20210906 12:28"
        },
        {
          "id": "MDRW2100032995.1.7",
          "form": "매일 쓰는 거니 좀 넉넉하게 사둬야겠네",
          "original_form": "매일 쓰는 거니 좀 넉넉하게 사둬야겠네",
          "speaker_id": "1",
          "time": "20210906 12:28"
        },
        {
          "id": "MDRW2100032995.1.8",
          "form": "ㅇㅇ 한달 2개 정도 잡고 일년치 사놔",
          "original_form": "ㅇㅇ 한달 2개 정도 잡고 일년치 사놔",
          "speaker_id": "2",
          "time": "20210906 12:29"
        },
        {
          "id": "MDRW2100032995.1.9",
          "form": "그럼 25개~30개 정도 사야겠네 ㅎㅎ",
          "original_form": "그럼 25개~30개 정도 사야겠네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 12:29"
        },
        {
          "id": "MDRW2100032995.1.10",
          "form": "죽염 이런거 말고 2080이나 메디안으로",
          "original_form": "죽염 이런거 말고 2080이나 메디안으로",
          "speaker_id": "2",
          "time": "20210906 12:29"
        },
        {
          "id": "MDRW2100032995.1.11",
          "form": "ㅎㅎ 죽염치약은 나도 짜서 별로야",
          "original_form": "ㅎㅎ 죽염치약은 나도 짜서 별로야",
          "speaker_id": "1",
          "time": "20210906 12:29"
        },
        {
          "id": "MDRW2100032995.1.12",
          "form": "우리도 우리지만 애들이 싫어하니까 ㅋㅋ",
          "original_form": "우리도 우리지만 애들이 싫어하니까 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:30"
        },
        {
          "id": "MDRW2100032995.1.13",
          "form": "그럼 치약은 그냥 평소에 쓰던 걸로 살께",
          "original_form": "그럼 치약은 그냥 평소에 쓰던 걸로 살께",
          "speaker_id": "1",
          "time": "20210906 12:31"
        },
        {
          "id": "MDRW2100032995.1.14",
          "form": "ㅇㅇ 향 강하지 않고 평범한 게 제일 좋아",
          "original_form": "ㅇㅇ 향 강하지 않고 평범한 게 제일 좋아",
          "speaker_id": "2",
          "time": "20210906 12:31"
        },
        {
          "id": "MDRW2100032995.1.15",
          "form": "맞아~ 무난한 게 제일 좋지",
          "original_form": "맞아~ 무난한 게 제일 좋지",
          "speaker_id": "1",
          "time": "20210906 12:32"
        },
        {
          "id": "MDRW2100032995.1.16",
          "form": "지마켓에 잘 찾아봐~ 할인 상품 있을거야",
          "original_form": "지마켓에 잘 찾아봐~ 할인 상품 있을거야",
          "speaker_id": "2",
          "time": "20210906 12:32"
        },
        {
          "id": "MDRW2100032995.1.17",
