      "utterance": [
        {
          "id": "MDRW2100016200.1.1",
          "form": "점심은 뭐 드실 예정인가요",
          "original_form": "점심은 뭐 드실 예정인가요",
          "speaker_id": "1",
          "time": "20210826 10:48"
        },
        {
          "id": "MDRW2100016200.1.2",
          "form": "샐러드 먹을거에요",
          "original_form": "샐러드 먹을거에요",
          "speaker_id": "2",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100016200.1.3",
          "form": "다이어트 중이라",
          "original_form": "다이어트 중이라",
          "speaker_id": "2",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100016200.1.4",
          "form": "아.. 다이어트 중이시구나",
          "original_form": "아.. 다이어트 중이시구나",
          "speaker_id": "1",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100016200.1.5",
          "form": "name1은 뭐 드실거에요?",
          "original_form": "&name1&은 뭐 드실거에요?",
          "speaker_id": "2",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100016200.1.6",
          "form": "전 아마 라면 먹을 거 같아요",
          "original_form": "전 아마 라면 먹을 거 같아요",
          "speaker_id": "1",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100016200.1.7",
          "form": "좀 간단하게 먹고싶어서ㅎ",
          "original_form": "좀 간단하게 먹고싶어서ㅎ",
          "speaker_id": "1",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100016200.1.8",
          "form": "라면 좋아하시나요",
          "original_form": "라면 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 10:49"
        },
        {
          "id": "MDRW2100016200.1.9",
          "form": "그럼요 좋아하죠",
          "original_form": "그럼요 좋아하죠",
          "speaker_id": "2",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016200.1.10",
          "form": "어떤라면 가장 좋아하시나요",
          "original_form": "어떤라면 가장 좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 10:50"
        },
        {
          "id": "MDRW2100016200.1.11",
          "form": "저는 신라면 너구리 진라면 비빔면",
          "original_form": "저는 신라면 너구리 진라면 비빔면",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016200.1.12",
          "form": "너무 많네요",
          "original_form": "너무 많네요",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016200.1.13",
          "form": "너무 맛있어요 약간 농심라면 좋아하시는거같아요",
          "original_form": "너무 맛있어요 약간 농심라면 좋아하시는거같아요",
          "speaker_id": "1",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016200.1.14",
          "form": "라면말고 좋아하는 음식은 뭐가있어요?",
          "original_form": "라면말고 좋아하는 음식은 뭐가있어요?",
          "speaker_id": "2",
          "time": "20210826 10:57"
        },
        {
          "id": "MDRW2100016200.1.15",
          "form": "저 감자로 만든 요리 좋아해요",
          "original_form": "저 감자로 만든 요리 좋아해요",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016200.1.16",
          "form": "감자전이나 감자튀김같은",
          "original_form": "감자전이나 감자튀김같은",
          "speaker_id": "1",
          "time": "20210826 10:58"
        },
        {
          "id": "MDRW2100016200.1.17",
          "form": "오 저도",
          "original_form": "오 저도",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016200.1.18",
          "form": "얼마전에 감자수프 만들었는데",
          "original_form": "얼마전에 감자수프 만들었는데",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016200.1.19",
          "form": "감자 소진하려고",
          "original_form": "감자 소진하려고",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016200.1.20",
          "form": "맛있었어요",
          "original_form": "맛있었어요",
          "speaker_id": "2",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016200.1.21",
          "form": "감자수프는 버터 녹여서 루 만들어서 만드셨나요",
          "original_form": "감자수프는 버터 녹여서 루 만들어서 만드셨나요",
          "speaker_id": "1",
          "time": "20210826 10:59"
        },
        {
          "id": "MDRW2100016200.1.22",
          "form": "양파 버터에 볶은 후 갈고",
          "original_form": "양파 버터에 볶은 후 갈고",
          "speaker_id": "2",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016200.1.23",
          "form": "감자 삶은 후 갈고",
          "original_form": "감자 삶은 후 갈고",
          "speaker_id": "2",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016200.1.24",
          "form": "합쳐서 잘 저어주며 끓이다 치즈한장 넣고 완성",
