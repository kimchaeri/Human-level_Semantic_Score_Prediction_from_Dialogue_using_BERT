        },
        {
          "id": "MDRW2100005900.1.5",
          "form": "껄껄",
          "original_form": "껄껄",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100005900.1.6",
          "form": "?????",
          "original_form": "?????",
          "speaker_id": "2",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100005900.1.7",
          "form": "모태신앙은아닌데",
          "original_form": "모태신앙은아닌데",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100005900.1.8",
          "form": "와기때는",
          "original_form": "와기때는",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100005900.1.9",
          "form": "천주교였음",
          "original_form": "천주교였음",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100005900.1.10",
          "form": "그랬군",
          "original_form": "그랬군",
          "speaker_id": "2",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100005900.1.11",
          "form": "세례명도있는걸",
          "original_form": "세례명도있는걸",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100005900.1.12",
          "form": "근데이사오면서",
          "original_form": "근데이사오면서",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100005900.1.13",
          "form": "아까 말했듯이...",
          "original_form": "아까 말했듯이...",
          "speaker_id": "2",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100005900.1.14",
          "form": "교회다녔당",
          "original_form": "교회다녔당",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100005900.1.15",
          "form": "그래서 뉴욕에서도 교회다니기시작해서",
          "original_form": "그래서 뉴욕에서도 교회다니기시작해서",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100005900.1.16",
          "form": "천주교는 기독교에 포함되는 것 ㅋㅋㅋ",
          "original_form": "천주교는 기독교에 포함되는 것 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100005900.1.17",
          "form": "거기서도 세례를 받앗다",
          "original_form": "거기서도 세례를 받앗다",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100005900.1.18",
          "form": "예아",
          "original_form": "예아",
          "speaker_id": "1",
          "time": "20210907 15:45"
        },
        {
          "id": "MDRW2100005900.1.19",
          "form": "암튼 성당과 교회를 번갈아가면서",
          "original_form": "암튼 성당과 교회를 번갈아가면서",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005900.1.20",
          "form": "다닌기억",
          "original_form": "다닌기억",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005900.1.21",
          "form": "뉴욕에서도 성당한번갔었거든",
          "original_form": "뉴욕에서도 성당한번갔었거든",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005900.1.22",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005900.1.23",
          "form": "뭔가 내부의 그 스테인글라스와",
          "original_form": "뭔가 내부의 그 스테인글라스와",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005900.1.24",
          "form": "그런거 구경겸",
          "original_form": "그런거 구경겸",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005900.1.25",
          "form": "너무이쁘더라구",
          "original_form": "너무이쁘더라구",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005900.1.26",
          "form": "신기하네...",
          "original_form": "신기하네...",
          "speaker_id": "2",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005900.1.27",
          "form": "근데미사드릴때",
          "original_form": "근데미사드릴때",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005900.1.28",
          "form": "너무 어려웠어",
          "original_form": "너무 어려웠어",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005900.1.29",
          "form": "무릎꿇었다가",
          "original_form": "무릎꿇었다가",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005900.1.30",
          "form": "앉았다가",
          "original_form": "앉았다가",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005900.1.31",
          "form": "일어났다가...",
          "original_form": "일어났다가...",
          "speaker_id": "1",
          "time": "20210907 15:46"
        },
        {
          "id": "MDRW2100005900.1.32",
          "form": "템플스테이가서도",
          "original_form": "템플스테이가서도",
          "speaker_id": "1",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100005900.1.33",
          "form": "뭐라해야하지",
          "original_form": "뭐라해야하지",
          "speaker_id": "1",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100005900.1.34",
          "form": "불당안에",
          "original_form": "불당안에",
          "speaker_id": "1",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100005900.1.35",
          "form": "들어가서",
          "original_form": "들어가서",
          "speaker_id": "1",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100005900.1.36",
          "form": "불교체험을했었는데",
          "original_form": "불교체험을했었는데",
          "speaker_id": "1",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100005900.1.37",
          "form": "사방에 3번씩 절하고",
          "original_form": "사방에 3번씩 절하고",
          "speaker_id": "1",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100005900.1.38",
          "form": "하는게 너무 헷갈렸고",
          "original_form": "하는게 너무 헷갈렸고",
          "speaker_id": "1",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100005900.1.39",
          "form": "냅다 불전 해석본?",
          "original_form": "냅다 불전 해석본?",
          "speaker_id": "1",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100005900.1.40",
          "form": "그런것도있어서",
          "original_form": "그런것도있어서",
          "speaker_id": "1",
          "time": "20210907 15:47"
        },
        {
          "id": "MDRW2100005900.1.41",
          "form": "같이읽는데 순서도헷갈리고",
          "original_form": "같이읽는데 순서도헷갈리고",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100005900.1.42",
          "form": "뭔가 무소유 그런기",
          "original_form": "뭔가 무소유 그런기",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100005900.1.43",
          "form": "무소유라는걸 딱히 보이지도않아서",
          "original_form": "무소유라는걸 딱히 보이지도않아서",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100005900.1.44",
          "form": "웃겨 ㅋㅋㅋㅋㅋ",
          "original_form": "웃겨 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100005900.1.45",
          "form": "생각보다 실망스러웠던..",
          "original_form": "생각보다 실망스러웠던..",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100005900.1.46",
          "form": "뭔가 목사님이",
          "original_form": "뭔가 목사님이",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100005900.1.47",
          "form": "신도들 기도해주는것처럼",
          "original_form": "신도들 기도해주는것처럼",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100005900.1.48",
          "form": "스님도",
          "original_form": "스님도",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100005900.1.49",
          "form": "다른사람들이 부탁한",
          "original_form": "다른사람들이 부탁한",
          "speaker_id": "1",
          "time": "20210907 15:48"
        },
        {
          "id": "MDRW2100005900.1.50",
          "form": "그런것들을 외우시더라고",
          "original_form": "그런것들을 외우시더라고",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100005900.1.51",
          "form": "사업이나 성적 건강같은걸",
          "original_form": "사업이나 성적 건강같은걸",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100005900.1.52",
          "form": "엥 진짜??",
          "original_form": "엥 진짜??",
          "speaker_id": "2",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100005900.1.53",
          "form": "ㅇㅇㅇㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100005900.1.54",
          "form": "신기하네",
          "original_form": "신기하네",
          "speaker_id": "2",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100005900.1.55",
          "form": "그래서 오옹 이렇게 대신해주시기도하구나",
          "original_form": "그래서 오옹 이렇게 대신해주시기도하구나",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100005900.1.56",
          "form": "그걸 누구한테 비는거지??",
          "original_form": "그걸 누구한테 비는거지??",
          "speaker_id": "2",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100005900.1.57",
          "form": "부처님한테",
          "original_form": "부처님한테",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100005900.1.58",
          "form": "비는거아닐까",
          "original_form": "비는거아닐까",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100005900.1.59",
          "form": "개큰불상",
          "original_form": "개큰불상",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100005900.1.60",
          "form": "있던데",
          "original_form": "있던데",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100005900.1.61",
          "form": "불당안이엄청 웅장하고화려하고",
          "original_form": "불당안이엄청 웅장하고화려하고",
          "speaker_id": "1",
          "time": "20210907 15:49"
        },
        {
          "id": "MDRW2100005900.1.62",
          "form": "좋더라고",
          "original_form": "좋더라고",
          "speaker_id": "1",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100005900.1.63",
          "form": "미적으로 보면",
          "original_form": "미적으로 보면",
          "speaker_id": "1",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100005900.1.64",
          "form": "성당이가장화려하고",
          "original_form": "성당이가장화려하고",
          "speaker_id": "1",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100005900.1.65",
          "form": "그다음이 불당",
          "original_form": "그다음이 불당",
          "speaker_id": "1",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100005900.1.66",
          "form": "그다음이 교회같아",
          "original_form": "그다음이 교회같아",
          "speaker_id": "1",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100005900.1.67",
          "form": "내가뉴욕에서다니던교회는 그 배터리 파크",
          "original_form": "내가뉴욕에서다니던교회는 그 배터리 파크",
          "speaker_id": "1",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100005900.1.68",
          "form": "그쪽에있었거든",
          "original_form": "그쪽에있었거든",
          "speaker_id": "1",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100005900.1.69",
          "form": "경치는짱좋았는데",
          "original_form": "경치는짱좋았는데",
          "speaker_id": "1",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100005900.1.70",
          "form": "헐 ㅋㅋㅋ",
          "original_form": "헐 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100005900.1.71",
          "form": "배터리파크",
          "original_form": "배터리파크",
          "speaker_id": "2",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100005900.1.72",
          "form": "교회가 너무 작아서",
          "original_form": "교회가 너무 작아서",
          "speaker_id": "1",
          "time": "20210907 15:50"
        },
        {
          "id": "MDRW2100005900.1.73",
          "form": "학교를 빌렸었던...",
          "original_form": "학교를 빌렸었던...",
          "speaker_id": "1",
          "time": "20210907 15:51"
