        {
          "id": "MDRW2100015383.1.1",
          "form": "보스베이비요",
          "original_form": "보스베이비요",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015383.1.2",
          "form": "아시나요",
          "original_form": "아시나요",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015383.1.3",
          "form": "그거 봤어쇼",
          "original_form": "그거 봤어쇼",
          "speaker_id": "2",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015383.1.4",
          "form": "1편만",
          "original_form": "1편만",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015383.1.5",
          "form": "제가 보스베이비때문에 연애를 시작했었어요",
          "original_form": "제가 보스베이비때문에 연애를 시작했었어요",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015383.1.6",
          "form": "애기가ㅋㅋㅋㅋㅋ",
          "original_form": "애기가ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015383.1.7",
          "form": "쓰읍..썰풀기",
          "original_form": "쓰읍..썰풀기",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015383.1.8",
          "form": "진짜요.?",
          "original_form": "진짜요.?",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015383.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015383.1.10",
          "form": "보스베이비로요.?",
          "original_form": "보스베이비로요.?",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015383.1.11",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015383.1.12",
          "form": "어떻게요",
          "original_form": "어떻게요",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015383.1.13",
          "form": "같이 간 남사친이 거기 대사중에 형이 애기한테",
          "original_form": "같이 간 남사친이 거기 대사중에 형이 애기한테",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015383.1.14",
          "form": "열받아서 '너같은건 태어나지 말았어야했어!' 이러잖어요",
          "original_form": "열받아서 '너같은건 태어나지 말았어야했어!' 이러잖어요",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015383.1.15",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015383.1.16",
          "form": "근데 걔가 허어어어억!!!!! 이러고 리액션 하는거에요",
          "original_form": "근데 걔가 허어어어억!!!!! 이러고 리액션 하는거에요",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015383.1.17",
          "form": "그래가지고 그거에 반해서",
          "original_form": "그래가지고 그거에 반해서",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015383.1.18",
          "form": "사겼어요",
          "original_form": "사겼어요",
          "speaker_id": "1",
          "time": "20210825 23:08"
        },
        {
          "id": "MDRW2100015383.1.19",
          "form": "ㅋㅋㅋ 보스베이비가 이어준 만남이네요",
