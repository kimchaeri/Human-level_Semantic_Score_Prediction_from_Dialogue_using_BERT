      "utterance": [
        {
          "id": "MDRW2100022662.1.1",
          "form": "아 보기만 해도 맛있을거 같아요ㅠㅠ",
          "original_form": "아 보기만 해도 맛있을거 같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022662.1.2",
          "form": "오랜만에 어묵국 땡기네요!",
          "original_form": "오랜만에 어묵국 땡기네요!",
          "speaker_id": "1",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022662.1.3",
          "form": "어묵국 좋아하시나요?",
          "original_form": "어묵국 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210829 16:48"
        },
        {
          "id": "MDRW2100022662.1.4",
          "form": "막좋아하능건아닌데 ㅋㅋㅋ국물 좋아해여 ㅋㅋㅋ",
          "original_form": "막좋아하능건아닌데 ㅋㅋㅋ국물 좋아해여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 17:22"
        },
        {
          "id": "MDRW2100022662.1.5",
          "form": "어제 저녁 드셨나요?",
          "original_form": "어제 저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210901 00:14"
        },
        {
          "id": "MDRW2100022662.1.6",
          "form": "네에~ 비빔국수랑 부추전먹엇네요!",
          "original_form": "네에~ 비빔국수랑 부추전먹엇네요!",
          "speaker_id": "2",
          "time": "20210901 14:30"
        },
        {
          "id": "MDRW2100022662.1.7",
          "form": "맛있는거 드셨네요!!",
          "original_form": "맛있는거 드셨네요!!",
          "speaker_id": "1",
          "time": "20210901 20:24"
        },
        {
          "id": "MDRW2100022662.1.8",
          "form": "부추전 직접 해드셨나요?",
          "original_form": "부추전 직접 해드셨나요?",
          "speaker_id": "1",
          "time": "20210901 20:25"
        },
        {
          "id": "MDRW2100022662.1.9",
          "form": "아뇨ㅠㅠ 국수랑 전은 사먹엇어여ㅠㅗ",
          "original_form": "아뇨ㅠㅠ 국수랑 전은 사먹엇어여ㅠㅗ",
          "speaker_id": "2",
          "time": "20210901 22:54"
        },
        {
          "id": "MDRW2100022662.1.10",
          "form": "포장해오셨나요?",
          "original_form": "포장해오셨나요?",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100022662.1.11",
          "form": "배달??",
          "original_form": "배달??",
          "speaker_id": "1",
          "time": "20210902 00:39"
        },
        {
          "id": "MDRW2100022662.1.12",
          "form": "포장주문해놓고 ㅋㅋ 퇴근길에 픽업해오라 시켰네요",
          "original_form": "포장주문해놓고 ㅋㅋ 퇴근길에 픽업해오라 시켰네요",
          "speaker_id": "2",
          "time": "20210902 15:11"
        },
        {
          "id": "MDRW2100022662.1.13",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210902 17:03"
        },
        {
          "id": "MDRW2100022662.1.14",
          "form": "컵라면먹었네요~",
          "original_form": "컵라면먹었네요~",
          "speaker_id": "2",
          "time": "20210902 17:20"
        },
        {
          "id": "MDRW2100022662.1.15",
          "form": "오 무슨 라면 드셨나요?",
          "original_form": "오 무슨 라면 드셨나요?",
          "speaker_id": "1",
          "time": "20210902 19:34"
        },
        {
          "id": "MDRW2100022662.1.16",
          "form": "육개장사발면이요 ㅎㅎ 면얇은거!!",
          "original_form": "육개장사발면이요 ㅎㅎ 면얇은거!!",
          "speaker_id": "2",
          "time": "20210902 19:42"
        },
        {
          "id": "MDRW2100022662.1.17",
          "form": "제가 젤 좋아하는 컵라면 드셨네요!!",
          "original_form": "제가 젤 좋아하는 컵라면 드셨네요!!",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100022662.1.18",
          "form": "라면 좋아하시나요?",
          "original_form": "라면 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100022662.1.19",
          "form": "정말요?ㅎ",
          "original_form": "정말요?ㅎ",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100022662.1.20",
          "form": "라면보단 다른면들을 더 좋아해요ㅠㅋㅋㅋ 뭐 라면도좋아하지만 ㅠ",
          "original_form": "라면보단 다른면들을 더 좋아해요ㅠㅋㅋㅋ 뭐 라면도좋아하지만 ㅠ",
          "speaker_id": "2",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100022662.1.21",
          "form": "어떤 면류 좋아하세요??",
          "original_form": "어떤 면류 좋아하세요??",
