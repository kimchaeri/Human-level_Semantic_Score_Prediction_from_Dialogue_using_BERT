      "utterance": [
        {
          "id": "MDRW2100039972.1.1",
          "form": "좋아하는 가수 있으세요",
          "original_form": "좋아하는 가수 있으세요",
          "speaker_id": "2",
          "time": "20210903 23:43"
        },
        {
          "id": "MDRW2100039972.1.2",
          "form": "흠 뮤직..",
          "original_form": "흠 뮤직..",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100039972.1.3",
          "form": "전 인디계열이라",
          "original_form": "전 인디계열이라",
          "speaker_id": "1",
          "time": "20210903 23:44"
        },
        {
          "id": "MDRW2100039972.1.4",
          "form": "새소년 루시드폴 좋아하고요",
          "original_form": "새소년 루시드폴 좋아하고요",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039972.1.5",
          "form": "루시드폴에 나긋나긋한 목소리는 너무 좋아요",
          "original_form": "루시드폴에 나긋나긋한 목소리는 너무 좋아요",
          "speaker_id": "2",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039972.1.6",
          "form": "장얼도 괜찮았어요",
          "original_form": "장얼도 괜찮았어요",
          "speaker_id": "1",
          "time": "20210903 23:45"
        },
        {
          "id": "MDRW2100039972.1.7",
          "form": "루시드폴님도 제주 어딘가에서 귤농사와 음악작업을 하고 계시겠네요 ㅎㅎ",
          "original_form": "루시드폴님도 제주 어딘가에서 귤농사와 음악작업을 하고 계시겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 23:46"
        },
        {
          "id": "MDRW2100039972.1.8",
          "form": "워낙 취미로 음악하시는 분이셔서ㅋㅋㅋ",
          "original_form": "워낙 취미로 음악하시는 분이셔서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039972.1.9",
          "form": "진짜 취미중에 하나가 음악이신듯ㅋㅋ",
          "original_form": "진짜 취미중에 하나가 음악이신듯ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:47"
        },
        {
          "id": "MDRW2100039972.1.10",
          "form": "아 최근에 신재평님 가나다라마바사랑",
          "original_form": "아 최근에 신재평님 가나다라마바사랑",
          "speaker_id": "1",
          "time": "20210903 23:48"
        },
        {
          "id": "MDRW2100039972.1.11",
          "form": "음반내셨나요?",
          "original_form": "음반내셨나요?",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039972.1.12",
          "form": "이장원님 쉬즈어베이비 보고 ㅋㅋㅋ 정말 많이 웃었어요",
          "original_form": "이장원님 쉬즈어베이비 보고 ㅋㅋㅋ 정말 많이 웃었어요",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039972.1.13",
          "form": "아뇨 진지하게 부르시는데",
          "original_form": "아뇨 진지하게 부르시는데",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039972.1.14",
          "form": "웃겨죽어요",
          "original_form": "웃겨죽어요",
          "speaker_id": "1",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039972.1.15",
          "form": "앗...보고 싶어졌다ㅋㅋㅋㅋ",
          "original_form": "앗...보고 싶어졌다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:49"
        },
        {
          "id": "MDRW2100039972.1.16",
          "form": "유튭에 있겠죠?ㅋㄱㅋ",
          "original_form": "유튭에 있겠죠?ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039972.1.17",
          "form": "네 함 보세요",
          "original_form": "네 함 보세요",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039972.1.18",
          "form": "우울할때 보심 더 좋은데",
          "original_form": "우울할때 보심 더 좋은데",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039972.1.19",
          "form": "정형돈의 처음엔사랑이란게에 맞먹습니다",
          "original_form": "정형돈의 처음엔사랑이란게에 맞먹습니다",
          "speaker_id": "1",
          "time": "20210903 23:50"
        },
        {
          "id": "MDRW2100039972.1.20",
          "form": "앗 그럼 아껴두겠습니다!!",
          "original_form": "앗 그럼 아껴두겠습니다!!",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100039972.1.21",
          "form": "ㅋㅋ너무기대되네요ㅋㅋ",
          "original_form": "ㅋㅋ너무기대되네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 23:51"
        },
        {
          "id": "MDRW2100039972.1.22",
          "form": "보고 오세요 채팅에 동기부여될듯요 ㅋㅋ",
