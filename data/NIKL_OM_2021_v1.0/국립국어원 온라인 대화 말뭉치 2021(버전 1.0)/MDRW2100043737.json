      "utterance": [
        {
          "id": "MDRW2100043737.1.1",
          "form": "종류가 많더라구요",
          "original_form": "종류가 많더라구요",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043737.1.2",
          "form": "저는 오늘 던킨 갔다 왔어요",
          "original_form": "저는 오늘 던킨 갔다 왔어요",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043737.1.3",
          "form": "감귤 인절미 딸기 녹차 등등",
          "original_form": "감귤 인절미 딸기 녹차 등등",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043737.1.4",
          "form": "인절미 감귤 크림치즈 말고도 더 있어요?",
          "original_form": "인절미 감귤 크림치즈 말고도 더 있어요?",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043737.1.5",
          "form": "와 종류가 많군요",
          "original_form": "와 종류가 많군요",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043737.1.6",
          "form": "초코도있고",
          "original_form": "초코도있고",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043737.1.7",
          "form": "3개뿐인줄 알았어요",
          "original_form": "3개뿐인줄 알았어요",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043737.1.8",
          "form": "던킨에 미니도넛팩 1000원 할인권 주길래",
          "original_form": "던킨에 미니도넛팩 1000원 할인권 주길래",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043737.1.9",
          "form": "맛은어때요?",
          "original_form": "맛은어때요?",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043737.1.10",
          "form": "그래도 비싸곘지 하고 생각도 안햇는데 ㅎㅎ",
          "original_form": "그래도 비싸곘지 하고 생각도 안햇는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043737.1.11",
          "form": "찾아보니 3300원이더라구요 ㅎㅎ",
          "original_form": "찾아보니 3300원이더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043737.1.12",
          "form": "괜찮네요",
          "original_form": "괜찮네요",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043737.1.13",
          "form": "2300원에 미니도넛 6개 받아왔어요",
          "original_form": "2300원에 미니도넛 6개 받아왔어요",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043737.1.14",
          "form": "쬐끄만하죠?",
          "original_form": "쬐끄만하죠?",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043737.1.15",
          "form": "미니인데 손바닥만해요",
          "original_form": "미니인데 손바닥만해요",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100043737.1.16",
          "form": "애들이 2개씩 다 먹었어요 ㅎㅎ",
          "original_form": "애들이 2개씩 다 먹었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043737.1.17",
          "form": "역시 초코발린 도넛부터 가져가더라구요",
          "original_form": "역시 초코발린 도넛부터 가져가더라구요",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043737.1.18",
          "form": "애들은 좋아할듯요",
          "original_form": "애들은 좋아할듯요",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043737.1.19",
          "form": "사이즈도 딱 맞아요",
          "original_form": "사이즈도 딱 맞아요",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043737.1.20",
          "form": "그러게요 ㅎ",
          "original_form": "그러게요 ㅎ",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043737.1.21",
          "form": "크리스피 먹을땐 커보이더니",
          "original_form": "크리스피 먹을땐 커보이더니",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100043737.1.22",
          "form": "이건 먹는데 안커보이더라구요",
          "original_form": "이건 먹는데 안커보이더라구요",
          "speaker_id": "1",
          "time": "20210907 20:11"
