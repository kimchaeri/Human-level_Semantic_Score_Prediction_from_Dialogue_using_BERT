      "utterance": [
        {
          "id": "MDRW2100000630.1.1",
          "form": "오늘 저녁은 뭐 먹나?",
          "original_form": "오늘 저녁은 뭐 먹나?",
          "speaker_id": "2",
          "time": "20210819 17:15"
        },
        {
          "id": "MDRW2100000630.1.2",
          "form": "오늘 저녁 뭐 먹지?",
          "original_form": "오늘 저녁 뭐 먹지?",
          "speaker_id": "1",
          "time": "20210819 17:15"
        },
        {
          "id": "MDRW2100000630.1.3",
          "form": "같은 고민을 하고 있었네",
          "original_form": "같은 고민을 하고 있었네",
          "speaker_id": "1",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000630.1.4",
          "form": "먹고 사는게 제일 큰 문제여",
          "original_form": "먹고 사는게 제일 큰 문제여",
          "speaker_id": "2",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000630.1.5",
          "form": "과자만 엄청 많고 주식이 없어",
          "original_form": "과자만 엄청 많고 주식이 없어",
          "speaker_id": "1",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000630.1.6",
          "form": "맞아 하루에 3번 먹어야 하니까 그게 가장 문제야",
          "original_form": "맞아 하루에 3번 먹어야 하니까 그게 가장 문제야",
          "speaker_id": "1",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000630.1.7",
          "form": "오늘 수확한 표고버섯이 좀 있는데",
          "original_form": "오늘 수확한 표고버섯이 좀 있는데",
          "speaker_id": "2",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000630.1.8",
          "form": "매일 라면을 3번 먹을 수도 없고",
          "original_form": "매일 라면을 3번 먹을 수도 없고",
          "speaker_id": "1",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000630.1.9",
          "form": "그걸로 뭘 해먹어야 하나",
          "original_form": "그걸로 뭘 해먹어야 하나",
          "speaker_id": "2",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000630.1.10",
          "form": "라면도 계속 먹으니까 물리더라고",
          "original_form": "라면도 계속 먹으니까 물리더라고",
          "speaker_id": "1",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000630.1.11",
          "form": "버섯라면",
          "original_form": "버섯라면",
          "speaker_id": "1",
          "time": "20210819 17:16"
        },
        {
          "id": "MDRW2100000630.1.12",
          "form": "해물라면",
          "original_form": "해물라면",
          "speaker_id": "1",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000630.1.13",
          "form": "헛~ 라면밖에 먹을 것이 없나?",
          "original_form": "헛~ 라면밖에 먹을 것이 없나?",
          "speaker_id": "2",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000630.1.14",
          "form": "산에 사는 사람들은 맨날 버섯라면 해먹겠다",
          "original_form": "산에 사는 사람들은 맨날 버섯라면 해먹겠다",
          "speaker_id": "1",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000630.1.15",
          "form": "과자와 라면이 많아",
          "original_form": "과자와 라면이 많아",
          "speaker_id": "1",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000630.1.16",
          "form": "진라면 신라면 안성탕면 해물탕면",
          "original_form": "진라면 신라면 안성탕면 해물탕면",
          "speaker_id": "1",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000630.1.17",
          "form": "표고버섯멸치볶음 해먹어야겠다",
          "original_form": "표고버섯멸치볶음 해먹어야겠다",
          "speaker_id": "2",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000630.1.18",
          "form": "멸치가 없을걸",
          "original_form": "멸치가 없을걸",
          "speaker_id": "1",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000630.1.19",
          "form": "갓 따온 거라 향이 좋음",
          "original_form": "갓 따온 거라 향이 좋음",
          "speaker_id": "2",
          "time": "20210819 17:17"
        },
        {
          "id": "MDRW2100000630.1.20",
          "form": "표고버섯라면",
          "original_form": "표고버섯라면",
          "speaker_id": "1",
          "time": "20210819 17:18"
        },
        {
          "id": "MDRW2100000630.1.21",
          "form": "라면에 넣으면 다 라면이 이김",
          "original_form": "라면에 넣으면 다 라면이 이김",
          "speaker_id": "1",
          "time": "20210819 17:18"
        }
