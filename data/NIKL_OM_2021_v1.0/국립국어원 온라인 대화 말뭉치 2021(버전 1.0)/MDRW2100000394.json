      "utterance": [
        {
          "id": "MDRW2100000394.1.1",
          "form": "가을이 오나 했는데 이것은 장마인가",
          "original_form": "가을이 오나 했는데 이것은 장마인가",
          "speaker_id": "1",
          "time": "20210819 15:49"
        },
        {
          "id": "MDRW2100000394.1.2",
          "form": "가을이 왔다 할 수 있을까",
          "original_form": "가을이 왔다 할 수 있을까",
          "speaker_id": "1",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000394.1.3",
          "form": "진짜 나 가을비 이렇게 내리는거 첨 봄ㅋㅋ",
          "original_form": "진짜 나 가을비 이렇게 내리는거 첨 봄ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000394.1.4",
          "form": "하루종일 비다ㅋㅋ",
          "original_form": "하루종일 비다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000394.1.5",
          "form": "곧 있음 태풍도 올 시기 같은데",
          "original_form": "곧 있음 태풍도 올 시기 같은데",
          "speaker_id": "1",
          "time": "20210819 15:51"
        },
        {
          "id": "MDRW2100000394.1.6",
          "form": "구름 걷어내고 파란하늘 보고싶다ㅠ",
          "original_form": "구름 걷어내고 파란하늘 보고싶다ㅠ",
          "speaker_id": "2",
          "time": "20210819 15:52"
        },
        {
          "id": "MDRW2100000394.1.7",
          "form": "방울토마토 선드라이 토마토 만들려고 했는데 해가 없어서 못하네",
          "original_form": "방울토마토 선드라이 토마토 만들려고 했는데 해가 없어서 못하네",
          "speaker_id": "1",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000394.1.8",
          "form": "우리집 고추는 아직도",
          "original_form": "우리집 고추는 아직도",
          "speaker_id": "2",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000394.1.9",
          "form": "건조되지 못했다ㅋㅋ",
          "original_form": "건조되지 못했다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000394.1.10",
          "form": "날 계속 흐리니 기운도 없고 우울해지는 듯",
          "original_form": "날 계속 흐리니 기운도 없고 우울해지는 듯",
          "speaker_id": "2",
          "time": "20210819 15:55"
        },
        {
          "id": "MDRW2100000394.1.11",
          "form": "방울토마토는 선드라이말고 에어프라이어 드라이 해야할듯",
          "original_form": "방울토마토는 선드라이말고 에어프라이어 드라이 해야할듯",
          "speaker_id": "1",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000394.1.12",
          "form": "시간 오래 걸리겠다",
          "original_form": "시간 오래 걸리겠다",
          "speaker_id": "2",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000394.1.13",
          "form": "날씨가 이 모양이니 어쩔수가 없네",
          "original_form": "날씨가 이 모양이니 어쩔수가 없네",
          "speaker_id": "1",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000394.1.14",
          "form": "선선해져서 좋긴한데",
          "original_form": "선선해져서 좋긴한데",
          "speaker_id": "1",
          "time": "20210819 15:57"
        },
        {
          "id": "MDRW2100000394.1.15",
          "form": "날씨 찾아보니 다음주까지 계속 비ㅋㅋㅋ",
          "original_form": "날씨 찾아보니 다음주까지 계속 비ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 15:58"
        },
        {
          "id": "MDRW2100000394.1.16",
          "form": "지난달에 갔던 장마 죽지도 않고 또 왔네",
          "original_form": "지난달에 갔던 장마 죽지도 않고 또 왔네",
          "speaker_id": "1",
          "time": "20210819 15:59"
        },
        {
          "id": "MDRW2100000394.1.17",
          "form": "지금 이 시기에 이렇게 비 많이 오면 농작물에도 영향있지않나?",
          "original_form": "지금 이 시기에 이렇게 비 많이 오면 농작물에도 영향있지않나?",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000394.1.18",
          "form": "모릅니다...",
          "original_form": "모릅니다...",
          "speaker_id": "1",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000394.1.19",
          "form": "...그렇구나ㅋㅋ",
          "original_form": "...그렇구나ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:00"
        },
        {
          "id": "MDRW2100000394.1.20",
          "form": "아 근데 오늘인가 뭐 또 31도 된다는걸 얼핏 본듯",
          "original_form": "아 근데 오늘인가 뭐 또 31도 된다는걸 얼핏 본듯",
          "speaker_id": "1",
          "time": "20210819 16:01"
        },
        {
          "id": "MDRW2100000394.1.21",
          "form": "더우면서 습해서 뭍에나온 오징어처럼 늘어져 있다",
          "original_form": "더우면서 습해서 뭍에나온 오징어처럼 늘어져 있다",
          "speaker_id": "2",
          "time": "20210819 16:02"
        },
        {
          "id": "MDRW2100000394.1.22",
          "form": "빵 먹고싶은데 비와서 갈수가 없네",
          "original_form": "빵 먹고싶은데 비와서 갈수가 없네",
          "speaker_id": "1",
          "time": "20210819 16:02"
        },
        {
          "id": "MDRW2100000394.1.23",
          "form": "빵 어디서 사게",
          "original_form": "빵 어디서 사게",
          "speaker_id": "2",
          "time": "20210819 16:03"
        },
        {
          "id": "MDRW2100000394.1.24",
          "form": "우산쓰고 다녀오징",
          "original_form": "우산쓰고 다녀오징",
          "speaker_id": "2",
          "time": "20210819 16:03"
        },
        {
          "id": "MDRW2100000394.1.25",
          "form": "파바 기프티콘이 있어서 내일 가던지 해야지",
          "original_form": "파바 기프티콘이 있어서 내일 가던지 해야지",
          "speaker_id": "1",
          "time": "20210819 16:03"
        },
        {
          "id": "MDRW2100000394.1.26",
          "form": "부디 비가 안 오기를",
          "original_form": "부디 비가 안 오기를",
          "speaker_id": "1",
          "time": "20210819 16:03"
        },
        {
          "id": "MDRW2100000394.1.27",
          "form": "아...파바는 멀다",
          "original_form": "아...파바는 멀다",
          "speaker_id": "2",
          "time": "20210819 16:03"
        },
        {
          "id": "MDRW2100000394.1.28",
          "form": "낼은 그래도 비 안온다 하는거같던디",
          "original_form": "낼은 그래도 비 안온다 하는거같던디",
          "speaker_id": "2",
          "time": "20210819 16:04"
        },
        {
          "id": "MDRW2100000394.1.29",
          "form": "오오 좋다",
          "original_form": "오오 좋다",
          "speaker_id": "1",
          "time": "20210819 16:04"
        },
        {
          "id": "MDRW2100000394.1.30",
          "form": "동네 빵집은 그저 식빵정도 사는 ㅋㅋ",
          "original_form": "동네 빵집은 그저 식빵정도 사는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:04"
        },
        {
          "id": "MDRW2100000394.1.31",
          "form": "낼 날씨 좋으면 엄마차 끌고 도서관 갔다와야겠다",
          "original_form": "낼 날씨 좋으면 엄마차 끌고 도서관 갔다와야겠다",
          "speaker_id": "2",
          "time": "20210819 16:05"
        },
        {
          "id": "MDRW2100000394.1.32",
          "form": "비 징글징글",
          "original_form": "비 징글징글",
          "speaker_id": "1",
          "time": "20210821 14:49"
        }
      ]
