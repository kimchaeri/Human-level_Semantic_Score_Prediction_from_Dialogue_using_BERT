      "utterance": [
        {
          "id": "MDRW2100005975.1.1",
          "form": "엥?",
          "original_form": "엥?",
          "speaker_id": "1",
          "time": "20210907 18:06"
        },
        {
          "id": "MDRW2100005975.1.2",
          "form": "학교 요즘 나가고있어?",
          "original_form": "학교 요즘 나가고있어?",
          "speaker_id": "1",
          "time": "20210907 18:07"
        },
        {
          "id": "MDRW2100005975.1.3",
          "form": "학교...",
          "original_form": "학교...",
          "speaker_id": "2",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100005975.1.4",
          "form": "오늘 근로때문에!",
          "original_form": "오늘 근로때문에!",
          "speaker_id": "2",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100005975.1.5",
          "form": "시급 9000받고",
          "original_form": "시급 9000받고",
          "speaker_id": "2",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100005975.1.6",
          "form": "4시간했는데",
          "original_form": "4시간했는데",
          "speaker_id": "2",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100005975.1.7",
          "form": "전화4통받은게끝이얔ㅋㅋㅋ",
          "original_form": "전화4통받은게끝이얔ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:33"
        },
        {
          "id": "MDRW2100005975.1.8",
          "form": "헐? 뭐야? 개꿀인데...??",
          "original_form": "헐? 뭐야? 개꿀인데...??",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100005975.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100005975.1.10",
          "form": "동아리 너 하고싶은거 있어?",
          "original_form": "동아리 너 하고싶은거 있어?",
          "speaker_id": "2",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100005975.1.11",
          "form": "난 여행다니는 동아리 하고싶었는데 아마 이번생에는 힘들지 않을까",
          "original_form": "난 여행다니는 동아리 하고싶었는데 아마 이번생에는 힘들지 않을까",
          "speaker_id": "1",
          "time": "20210907 20:08"
        },
        {
          "id": "MDRW2100005975.1.12",
          "form": "왜 수업중에 답장이온지는 모르겠지만",
          "original_form": "왜 수업중에 답장이온지는 모르겠지만",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100005975.1.13",
          "form": "오오 이번생에 가능하세요",
          "original_form": "오오 이번생에 가능하세요",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100005975.1.14",
          "form": "굼대...다녕오시몬",
          "original_form": "굼대...다녕오시몬",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100005975.1.15",
          "form": "코로나없을걸요..",
          "original_form": "코로나없을걸요..",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100005975.1.16",
          "form": "수업진행을 못하구있어요...",
          "original_form": "수업진행을 못하구있어요...",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100005975.1.17",
          "form": "선생님 마이크가 나가신거같은데",
          "original_form": "선생님 마이크가 나가신거같은데",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100005975.1.18",
          "form": "선생님이 돌아오질않으세요ㅠ",
          "original_form": "선생님이 돌아오질않으세요ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100005975.1.19",
          "form": "헉 눈물나네요..",
          "original_form": "헉 눈물나네요..",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100005975.1.20",
          "form": "수업 빡세요?",
          "original_form": "수업 빡세요?",
          "speaker_id": "2",
          "time": "20210907 20:10"
        },
        {
          "id": "MDRW2100005975.1.21",
          "form": "아뇨 안빡세요",
          "original_form": "아뇨 안빡세요",
          "speaker_id": "1",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100005975.1.22",
          "form": "오오 그럼 씹어먹어요?",
          "original_form": "오오 그럼 씹어먹어요?",
          "speaker_id": "2",
          "time": "20210907 20:11"
        },
        {
          "id": "MDRW2100005975.1.23",
          "form": "그럴리가요!",
          "original_form": "그럴리가요!",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100005975.1.24",
          "form": "과제있나요?",
          "original_form": "과제있나요?",
          "speaker_id": "2",
          "time": "20210907 20:56"
        },
        {
          "id": "MDRW2100005975.1.25",
          "form": "네...쌓여있습니다",
          "original_form": "네...쌓여있습니다",
          "speaker_id": "1",
          "time": "20210907 20:57"
        },
        {
          "id": "MDRW2100005975.1.26",
          "form": "언제하시려구요ㅠㅠ",
          "original_form": "언제하시려구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 20:59"
        }
      ]
