      "utterance": [
        {
          "id": "MDRW2100010186.1.1",
          "form": "프랑켄슈타인도 원래 남편인가 암튼 남자 이름으로 나왔었잖아",
          "original_form": "프랑켄슈타인도 원래 남편인가 암튼 남자 이름으로 나왔었잖아",
          "speaker_id": "2",
          "time": "20210823 15:21"
        },
        {
          "id": "MDRW2100010186.1.2",
          "form": "참 여러의미로 대단한듯",
          "original_form": "참 여러의미로 대단한듯",
          "speaker_id": "1",
          "time": "20210823 15:21"
        },
        {
          "id": "MDRW2100010186.1.3",
          "form": "그냥",
          "original_form": "그냥",
          "speaker_id": "1",
          "time": "20210823 15:21"
        },
        {
          "id": "MDRW2100010186.1.4",
          "form": "소설훔친거랑뭐가달라",
          "original_form": "소설훔친거랑뭐가달라",
          "speaker_id": "1",
          "time": "20210823 15:21"
        },
        {
          "id": "MDRW2100010186.1.5",
          "form": "그니까 그리고 남편 이름으로 나왔을때는 찬사를 받다가",
          "original_form": "그니까 그리고 남편 이름으로 나왔을때는 찬사를 받다가",
          "speaker_id": "2",
          "time": "20210823 15:22"
        },
        {
          "id": "MDRW2100010186.1.6",
          "form": "이름 돌려놓으니까 혹평 받았다는게 참",
          "original_form": "이름 돌려놓으니까 혹평 받았다는게 참",
          "speaker_id": "2",
          "time": "20210823 15:23"
        },
        {
          "id": "MDRW2100010186.1.7",
          "form": "그런식으로혹평받아서사라진여성작가님들",
          "original_form": "그런식으로혹평받아서사라진여성작가님들",
          "speaker_id": "1",
          "time": "20210823 15:23"
        },
        {
          "id": "MDRW2100010186.1.8",
          "form": "많았을거같아",
          "original_form": "많았을거같아",
          "speaker_id": "1",
          "time": "20210823 15:23"
        },
        {
          "id": "MDRW2100010186.1.9",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010186.1.10",
          "form": "그래서 아까워",
          "original_form": "그래서 아까워",
          "speaker_id": "2",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010186.1.11",
          "form": "유명한작품중에서도여자가쓴작품",
          "original_form": "유명한작품중에서도여자가쓴작품",
          "speaker_id": "1",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010186.1.12",
          "form": "많았을듯",
          "original_form": "많았을듯",
          "speaker_id": "1",
          "time": "20210823 15:25"
        },
        {
          "id": "MDRW2100010186.1.13",
          "form": "그냥안밝혀졌을뿐",
          "original_form": "그냥안밝혀졌을뿐",
          "speaker_id": "1",
          "time": "20210823 15:26"
        },
        {
          "id": "MDRW2100010186.1.14",
          "form": "맞아 하도 뺏긴 이름이 많았어서",
          "original_form": "맞아 하도 뺏긴 이름이 많았어서",
          "speaker_id": "2",
          "time": "20210823 15:26"
        },
        {
          "id": "MDRW2100010186.1.15",
          "form": "휴 그래도 요즘 여자 작가들 많아져서 좋음",
          "original_form": "휴 그래도 요즘 여자 작가들 많아져서 좋음",
          "speaker_id": "1",
          "time": "20210823 15:29"
        },
        {
          "id": "MDRW2100010186.1.16",
          "form": "외국에서상도받았다녀",
          "original_form": "외국에서상도받았다녀",
          "speaker_id": "1",
          "time": "20210823 15:29"
        },
        {
          "id": "MDRW2100010186.1.17",
          "form": "며",
          "original_form": "며",
          "speaker_id": "1",
          "time": "20210823 15:29"
        },
        {
          "id": "MDRW2100010186.1.18",
          "form": "한국인최초로",
          "original_form": "한국인최초로",
          "speaker_id": "1",
          "time": "20210823 15:29"
        },
        {
          "id": "MDRW2100010186.1.19",
          "form": "아 받았지",
          "original_form": "아 받았지",
          "speaker_id": "2",
          "time": "20210823 15:30"
        },
        {
          "id": "MDRW2100010186.1.20",
          "form": "맨부커상 받았잖아",
          "original_form": "맨부커상 받았잖아",
          "speaker_id": "2",
          "time": "20210823 15:30"
        },
        {
          "id": "MDRW2100010186.1.21",
          "form": "대박이야",
          "original_form": "대박이야",
          "speaker_id": "2",
          "time": "20210823 15:30"
        },
        {
          "id": "MDRW2100010186.1.22",
          "form": "번역때문에남작가들이상못받았다는건",
          "original_form": "번역때문에남작가들이상못받았다는건",
          "speaker_id": "1",
          "time": "20210823 15:30"
        },
        {
          "id": "MDRW2100010186.1.23",
          "form": "다거짓말이라니까",
          "original_form": "다거짓말이라니까",
          "speaker_id": "1",
          "time": "20210823 15:30"
        },
        {
          "id": "MDRW2100010186.1.24",
          "form": "그냥작품성이없었을뿐",
          "original_form": "그냥작품성이없었을뿐",
          "speaker_id": "1",
          "time": "20210823 15:31"
        },
        {
          "id": "MDRW2100010186.1.25",
          "form": "아 맞지맞지",
          "original_form": "아 맞지맞지",
          "speaker_id": "2",
          "time": "20210823 15:31"
        },
        {
          "id": "MDRW2100010186.1.26",
          "form": "아니 다른 나라가 출판사가 없는 것도 아니고",
          "original_form": "아니 다른 나라가 출판사가 없는 것도 아니고",
          "speaker_id": "2",
          "time": "20210823 15:31"
        },
        {
          "id": "MDRW2100010186.1.27",
          "form": "거기서 검수해서 하는건데",
          "original_form": "거기서 검수해서 하는건데",
          "speaker_id": "2",
          "time": "20210823 15:31"
        },
        {
          "id": "MDRW2100010186.1.28",
          "form": "그니까. 과도한 애국심은 독인듯",
          "original_form": "그니까. 과도한 애국심은 독인듯",
          "speaker_id": "1",
          "time": "20210823 15:31"
        },
        {
          "id": "MDRW2100010186.1.29",
          "form": "그니까 으",
          "original_form": "그니까 으",
          "speaker_id": "2",
          "time": "20210823 15:32"
        },
        {
          "id": "MDRW2100010186.1.30",
          "form": "가정폭력 성희롱 작품을 찬양하기나하고",
          "original_form": "가정폭력 성희롱 작품을 찬양하기나하고",
          "speaker_id": "1",
          "time": "20210823 15:32"
        },
        {
          "id": "MDRW2100010186.1.31",
          "form": "나 어제 책 읽는데 약간 사기당한 기분임",
          "original_form": "나 어제 책 읽는데 약간 사기당한 기분임",
