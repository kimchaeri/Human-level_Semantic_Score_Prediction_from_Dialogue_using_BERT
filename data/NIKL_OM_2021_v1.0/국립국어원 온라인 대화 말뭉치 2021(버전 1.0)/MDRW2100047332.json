      "utterance": [
        {
          "id": "MDRW2100047332.1.1",
          "form": "조개 최고죠",
          "original_form": "조개 최고죠",
          "speaker_id": "1",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100047332.1.2",
          "form": "뭐 제일 좋아하시나요?",
          "original_form": "뭐 제일 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 13:05"
        },
        {
          "id": "MDRW2100047332.1.3",
          "form": "회좋아해요",
          "original_form": "회좋아해요",
          "speaker_id": "1",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100047332.1.4",
          "form": "아 조개이야기신가요?",
          "original_form": "아 조개이야기신가요?",
          "speaker_id": "1",
          "time": "20210826 13:08"
        },
        {
          "id": "MDRW2100047332.1.5",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:10"
        },
        {
          "id": "MDRW2100047332.1.6",
          "form": "조개 무난하게는 가리비좋아하구요",
          "original_form": "조개 무난하게는 가리비좋아하구요",
          "speaker_id": "1",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100047332.1.7",
          "form": "자주못먹지만 새조개를 제일좋아해요",
          "original_form": "자주못먹지만 새조개를 제일좋아해요",
          "speaker_id": "1",
          "time": "20210826 13:11"
        },
        {
          "id": "MDRW2100047332.1.8",
          "form": "아 저도 가리비 너무 좋아해요",
          "original_form": "아 저도 가리비 너무 좋아해요",
          "speaker_id": "2",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100047332.1.9",
          "form": "치즈 살짝 올려서 먹음 캬 ㅠ",
          "original_form": "치즈 살짝 올려서 먹음 캬 ㅠ",
          "speaker_id": "2",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100047332.1.10",
          "form": "너무 최고죠 저는 사촌이 조개구이집을 해서",
          "original_form": "너무 최고죠 저는 사촌이 조개구이집을 해서",
          "speaker_id": "1",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100047332.1.11",
          "form": "가끔 먹으러 간답니다!",
          "original_form": "가끔 먹으러 간답니다!",
          "speaker_id": "1",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100047332.1.12",
          "form": "네 자랑이예요",
          "original_form": "네 자랑이예요",
          "speaker_id": "1",
          "time": "20210826 13:12"
        },
        {
          "id": "MDRW2100047332.1.13",
          "form": "대박",
          "original_form": "대박",
          "speaker_id": "2",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100047332.1.14",
          "form": "자랑맞내요 부러워요 ㅠ",
          "original_form": "자랑맞내요 부러워요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 13:15"
        },
        {
          "id": "MDRW2100047332.1.15",
          "form": "조개구이 먹기 힘든대",
          "original_form": "조개구이 먹기 힘든대",
          "speaker_id": "2",
          "time": "20210826 13:16"
        },
        {
          "id": "MDRW2100047332.1.16",
          "form": "제가 부산출신이라 가격만 아니면 접근성은 좋았어요",
          "original_form": "제가 부산출신이라 가격만 아니면 접근성은 좋았어요",
          "speaker_id": "1",
          "time": "20210826 13:46"
        },
        {
          "id": "MDRW2100047332.1.17",
          "form": "부산오셔요!",
          "original_form": "부산오셔요!",
          "speaker_id": "1",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100047332.1.18",
          "form": "안그래도 이번에 부산가고싶었는데",
          "original_form": "안그래도 이번에 부산가고싶었는데",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100047332.1.19",
          "form": "돈이 모잘라서 못갔네요 ㅋㅋㅋㅋ",
          "original_form": "돈이 모잘라서 못갔네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100047332.1.20",
          "form": "부산에는 확실히 맛난개 많겠죠?",
          "original_form": "부산에는 확실히 맛난개 많겠죠?",
          "speaker_id": "2",
          "time": "20210826 15:38"
        },
        {
          "id": "MDRW2100047332.1.21",
          "form": "해산물은 확실히 많아요",
          "original_form": "해산물은 확실히 많아요",
          "speaker_id": "1",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100047332.1.22",
          "form": "맛집 추천해드릴까요?",
          "original_form": "맛집 추천해드릴까요?",
          "speaker_id": "1",
          "time": "20210826 16:11"
        },
        {
          "id": "MDRW2100047332.1.23",
          "form": "네!!!!!",
          "original_form": "네!!!!!",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100047332.1.24",
          "form": "해주새요 ㅎㅎㅎ",
          "original_form": "해주새요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:38"
        },
        {
          "id": "MDRW2100047332.1.25",
          "form": "일단 캐주얼하게 갈만한곳은 노르웨이식당이라고 서면에 연어덮밥식당이 있는데 연어좋아하시면 추천이요",
          "original_form": "일단 캐주얼하게 갈만한곳은 노르웨이식당이라고 서면에 연어덮밥식당이 있는데 연어좋아하시면 추천이요",
          "speaker_id": "1",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100047332.1.26",
          "form": "아 저 부산을 1년이상 안가고있어서... 추천하는 가게들 폐업했을수도 있어요",
          "original_form": "아 저 부산을 1년이상 안가고있어서... 추천하는 가게들 폐업했을수도 있어요",
          "speaker_id": "1",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100047332.1.27",
          "form": "향미공간이라고 서면에서 밥드시고 가면 좋은 카펜데요 케이크가 진짜 맛있어요",
          "original_form": "향미공간이라고 서면에서 밥드시고 가면 좋은 카펜데요 케이크가 진짜 맛있어요",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100047332.1.28",
          "form": "우와 저 케이크도 완전 좋아해요",
          "original_form": "우와 저 케이크도 완전 좋아해요",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100047332.1.29",
          "form": "추천 감사합니다.. 부산을 꼬옥.. 가야겠어요",
          "original_form": "추천 감사합니다.. 부산을 꼬옥.. 가야겠어요",
          "speaker_id": "2",
          "time": "20210826 21:48"
        },
        {
          "id": "MDRW2100047332.1.30",
          "form": "타이밍맞으면 한번 오세요 관광도시니까 볼만하실거예요",
          "original_form": "타이밍맞으면 한번 오세요 관광도시니까 볼만하실거예요",
          "speaker_id": "1",
          "time": "20210826 22:19"
        }
      ]
    }
