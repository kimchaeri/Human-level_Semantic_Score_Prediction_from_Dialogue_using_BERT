      "utterance": [
        {
          "id": "MDRW2100027340.1.1",
          "form": "앗 제가 뭔가 잘못 누른것같기도 하고요ㅜㅜ",
          "original_form": "앗 제가 뭔가 잘못 누른것같기도 하고요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 11:37"
        },
        {
          "id": "MDRW2100027340.1.2",
          "form": "저는 학생 때 야구를 보러갔었는데",
          "original_form": "저는 학생 때 야구를 보러갔었는데",
          "speaker_id": "2",
          "time": "20210902 11:38"
        },
        {
          "id": "MDRW2100027340.1.3",
          "form": "치킨만 맛있게 먹었던 기억이 납니다",
          "original_form": "치킨만 맛있게 먹었던 기억이 납니다",
          "speaker_id": "2",
          "time": "20210902 11:38"
        },
        {
          "id": "MDRW2100027340.1.4",
          "form": "야구장이 치킨을 참 잘하더라고요",
          "original_form": "야구장이 치킨을 참 잘하더라고요",
          "speaker_id": "2",
          "time": "20210902 11:38"
        },
        {
          "id": "MDRW2100027340.1.5",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027340.1.6",
          "form": "제 주변에 야구덕후인 친구들도 야구장은 먹으러간다고 하더라고요",
          "original_form": "제 주변에 야구덕후인 친구들도 야구장은 먹으러간다고 하더라고요",
          "speaker_id": "1",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027340.1.7",
          "form": "야구장에서 먹는 음식이 그렇ㄱㅔ 맛있다고..",
          "original_form": "야구장에서 먹는 음식이 그렇ㄱㅔ 맛있다고..",
          "speaker_id": "1",
          "time": "20210902 11:39"
        },
        {
          "id": "MDRW2100027340.1.8",
          "form": "경기 내용은 기억안나는데 그날 치킨의 식감은 기억이 나요ㅋㅋ",
          "original_form": "경기 내용은 기억안나는데 그날 치킨의 식감은 기억이 나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:41"
        },
        {
          "id": "MDRW2100027340.1.9",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ 식감ㅋㅋㅋㅋ까지 났을 정도면 진짜 맛있었나봐요",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ 식감ㅋㅋㅋㅋ까지 났을 정도면 진짜 맛있었나봐요",
          "speaker_id": "1",
          "time": "20210902 11:41"
        },
        {
          "id": "MDRW2100027340.1.10",
          "form": "저는 야구는 전혀 몰라서 아버지가 티비로 보고있는데 계속 물어봤다가",
          "original_form": "저는 야구는 전혀 몰라서 아버지가 티비로 보고있는데 계속 물어봤다가",
          "speaker_id": "1",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100027340.1.11",
          "form": "직접 보라고 한소리 듣고 싸웠었어요...  ㅋㅋㅋㅋㅋ",
          "original_form": "직접 보라고 한소리 듣고 싸웠었어요...  ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100027340.1.12",
          "form": "저는 축구 경기를 잘 못봐요ㅋㅋ",
          "original_form": "저는 축구 경기를 잘 못봐요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027340.1.13",
          "form": "제가 경기 실시간으로 보면 우리나라가 지는 징크스가 있어요",
          "original_form": "제가 경기 실시간으로 보면 우리나라가 지는 징크스가 있어요",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027340.1.14",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027340.1.15",
          "form": "한일전은 무조건 경기 끝나고 봅니다",
          "original_form": "한일전은 무조건 경기 끝나고 봅니다",
          "speaker_id": "2",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027340.1.16",
          "form": "안돼요... 배구 보시면 안되겠어요..",
          "original_form": "안돼요... 배구 보시면 안되겠어요..",
          "speaker_id": "1",
          "time": "20210902 11:43"
        },
        {
          "id": "MDRW2100027340.1.17",
          "form": "아앗... 희진언니 스크린으로만 만나요",
          "original_form": "아앗... 희진언니 스크린으로만 만나요",
          "speaker_id": "2",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027340.1.18",
          "form": "사실 언니가 아니지만",
          "original_form": "사실 언니가 아니지만",
          "speaker_id": "2",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027340.1.19",
          "form": "나보다 잘생겼으니까 언니하려고요",
          "original_form": "나보다 잘생겼으니까 언니하려고요",
          "speaker_id": "2",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027340.1.20",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맞죠맞죠 한일전은 무조건 끝나고 보세요ㅠㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 맞죠맞죠 한일전은 무조건 끝나고 보세요ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027340.1.21",
          "form": "희진언니....눈물줄줄",
          "original_form": "희진언니....눈물줄줄",
          "speaker_id": "1",
          "time": "20210902 11:44"
        },
        {
          "id": "MDRW2100027340.1.22",
          "form": "여자배구 예능 많이 잡혔더라구요",
          "original_form": "여자배구 예능 많이 잡혔더라구요",
          "speaker_id": "1",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027340.1.23",
          "form": "오 어디어디 나오나요?",
          "original_form": "오 어디어디 나오나요?",
          "speaker_id": "2",
          "time": "20210902 11:45"
        },
        {
          "id": "MDRW2100027340.1.24",
          "form": "런닝맨 전참시 나혼산 나온대요!",
          "original_form": "런닝맨 전참시 나혼산 나온대요!",
          "speaker_id": "1",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027340.1.25",
          "form": "오오오 런닝맨",
          "original_form": "오오오 런닝맨",
          "speaker_id": "2",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027340.1.26",
          "form": "완전 다 이길 것 같은데요",
          "original_form": "완전 다 이길 것 같은데요",
          "speaker_id": "2",
          "time": "20210902 11:46"
        },
        {
          "id": "MDRW2100027340.1.27",
          "form": "진짜 너무 기대돼요",
          "original_form": "진짜 너무 기대돼요",
          "speaker_id": "1",
          "time": "20210902 11:47"
        }
      ]
