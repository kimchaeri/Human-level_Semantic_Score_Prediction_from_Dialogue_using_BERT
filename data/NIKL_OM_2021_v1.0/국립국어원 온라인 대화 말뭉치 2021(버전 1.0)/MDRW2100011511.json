      "utterance": [
        {
          "id": "MDRW2100011511.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210824 13:24"
        },
        {
          "id": "MDRW2100011511.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011511.1.3",
          "form": "여기는 비가와요",
          "original_form": "여기는 비가와요",
          "speaker_id": "1",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011511.1.4",
          "form": "여기는 그쳤네요",
          "original_form": "여기는 그쳤네요",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011511.1.5",
          "form": "아직오나요?",
          "original_form": "아직오나요?",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011511.1.6",
          "form": "네 많이 오네요",
          "original_form": "네 많이 오네요",
          "speaker_id": "1",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011511.1.7",
          "form": "벌써 가을 이네요",
          "original_form": "벌써 가을 이네요",
          "speaker_id": "1",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100011511.1.8",
          "form": "그러게요 온도가",
          "original_form": "그러게요 온도가",
          "speaker_id": "2",
          "time": "20210824 13:34"
        },
        {
          "id": "MDRW2100011511.1.9",
          "form": "확낮아졌어요!",
          "original_form": "확낮아졌어요!",
          "speaker_id": "2",
          "time": "20210824 13:35"
        },
        {
          "id": "MDRW2100011511.1.10",
          "form": "환절기인데 감기 조심하세요~",
          "original_form": "환절기인데 감기 조심하세요~",
          "speaker_id": "1",
          "time": "20210824 13:38"
        },
        {
          "id": "MDRW2100011511.1.11",
          "form": "님도요!",
          "original_form": "님도요!",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100011511.1.12",
          "form": "미세먼지는 많이 사라진것같아요",
          "original_form": "미세먼지는 많이 사라진것같아요",
          "speaker_id": "2",
          "time": "20210824 23:11"
        },
        {
          "id": "MDRW2100011511.1.13",
          "form": "미세먼지 너무 싫네요",
          "original_form": "미세먼지 너무 싫네요",
          "speaker_id": "1",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100011511.1.14",
          "form": "중국이 사막에 나무좀 심었으면 좋겠어요",
          "original_form": "중국이 사막에 나무좀 심었으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100011511.1.15",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100011511.1.16",
          "form": "요즘은 많이나아진거래요ㅠ",
          "original_form": "요즘은 많이나아진거래요ㅠ",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100011511.1.17",
          "form": "지구온난화가 걱정돼요",
          "original_form": "지구온난화가 걱정돼요",
          "speaker_id": "1",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100011511.1.18",
          "form": "10년뒤면 부산이 잠긴대요",
          "original_form": "10년뒤면 부산이 잠긴대요",
          "speaker_id": "1",
          "time": "20210824 23:35"
        },
        {
          "id": "MDRW2100011511.1.19",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100011511.1.20",
          "form": "진짜그럴까요?",
          "original_form": "진짜그럴까요?",
          "speaker_id": "2",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100011511.1.21",
          "form": "학자들이 얘기하던데",
          "original_form": "학자들이 얘기하던데",
          "speaker_id": "1",
          "time": "20210825 00:19"
        },
        {
          "id": "MDRW2100011511.1.22",
          "form": "신빙성 있더라고요",
          "original_form": "신빙성 있더라고요",
          "speaker_id": "1",
          "time": "20210825 00:19"
        }
      ]
