      "utterance": [
        {
          "id": "MDRW2100015883.1.1",
          "form": "이제 추석이라 선물사야겠네?!@",
          "original_form": "이제 추석이라 선물사야겠네?!@",
          "speaker_id": "2",
          "time": "20210826 02:21"
        },
        {
          "id": "MDRW2100015883.1.2",
          "form": "원장님원감님",
          "original_form": "원장님원감님",
          "speaker_id": "2",
          "time": "20210826 02:21"
        },
        {
          "id": "MDRW2100015883.1.3",
          "form": "응 ㅋㅋㅋㅋ 매번 고민",
          "original_form": "응 ㅋㅋㅋㅋ 매번 고민",
          "speaker_id": "1",
          "time": "20210826 02:22"
        },
        {
          "id": "MDRW2100015883.1.4",
          "form": "이번엔 뭐살꺼야?!",
          "original_form": "이번엔 뭐살꺼야?!",
          "speaker_id": "2",
          "time": "20210826 02:23"
        },
        {
          "id": "MDRW2100015883.1.5",
          "form": "니가추천좀해봐 ㅋㅋ",
          "original_form": "니가추천좀해봐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 02:23"
        },
        {
          "id": "MDRW2100015883.1.6",
          "form": "정해진 금액에서 사야되니까 고민이겠다ㅠㅜ",
          "original_form": "정해진 금액에서 사야되니까 고민이겠다ㅠㅜ",
          "speaker_id": "2",
          "time": "20210826 02:23"
        },
        {
          "id": "MDRW2100015883.1.7",
          "form": "그니까 ㅜㅜ 홍삼이런게 무난쓰",
          "original_form": "그니까 ㅜㅜ 홍삼이런게 무난쓰",
          "speaker_id": "1",
          "time": "20210826 02:24"
        },
        {
          "id": "MDRW2100015883.1.8",
          "form": "홍삼 금요일부터 10프로 할인한다던데",
          "original_form": "홍삼 금요일부터 10프로 할인한다던데",
          "speaker_id": "2",
          "time": "20210826 02:24"
        },
        {
          "id": "MDRW2100015883.1.9",
          "form": "정관장",
          "original_form": "정관장",
          "speaker_id": "2",
          "time": "20210826 02:24"
        },
        {
          "id": "MDRW2100015883.1.10",
          "form": "정관장 활기력알아? 앰플같이생긴거",
          "original_form": "정관장 활기력알아? 앰플같이생긴거",
          "speaker_id": "1",
          "time": "20210826 11:39"
        },
        {
          "id": "MDRW2100015883.1.11",
          "form": "활기력 알아!",
          "original_form": "활기력 알아!",
          "speaker_id": "2",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100015883.1.12",
          "form": "저번에 돈 맞춘다고 사봤어!",
          "original_form": "저번에 돈 맞춘다고 사봤어!",
          "speaker_id": "2",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100015883.1.13",
          "form": "크기어때?",
          "original_form": "크기어때?",
          "speaker_id": "1",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100015883.1.14",
          "form": "손가락길이?",
          "original_form": "손가락길이?",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100015883.1.15",
          "form": "그거 괜찮을까?... 가격은 괜찮던데 ㅋㅋㅋ",
          "original_form": "그거 괜찮을까?... 가격은 괜찮던데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100015883.1.16",
          "form": "괜찮을듯! 팩으로든건 먹기도 귀찮고ㅋㅋ",
          "original_form": "괜찮을듯! 팩으로든건 먹기도 귀찮고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100015883.1.17",
          "form": "엑기스느낌나니까 괜찮을듯!",
          "original_form": "엑기스느낌나니까 괜찮을듯!",
          "speaker_id": "2",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100015883.1.18",
          "form": "ㅇㅋㅇㅋ 정관장 매장에서 할인하는거임?",
          "original_form": "ㅇㅋㅇㅋ 정관장 매장에서 할인하는거임?",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100015883.1.19",
          "form": "온라인으로도 하나?",
          "original_form": "온라인으로도 하나?",
          "speaker_id": "1",
          "time": "20210826 18:15"
        },
        {
          "id": "MDRW2100015883.1.20",
          "form": "난 매장 문자받아서 온라인은 모르겠엉",
          "original_form": "난 매장 문자받아서 온라인은 모르겠엉",
          "speaker_id": "2",
          "time": "20210826 18:16"
        },
        {
          "id": "MDRW2100015883.1.21",
          "form": "10프로 할인한다는데 활기력 포함인줄은 모르겠음",
          "original_form": "10프로 할인한다는데 활기력 포함인줄은 모르겠음",
          "speaker_id": "2",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100015883.1.22",
          "form": "아 품목마다 다를수있나보네.ㅡ",
          "original_form": "아 품목마다 다를수있나보네.ㅡ",
          "speaker_id": "1",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100015883.1.23",
          "form": "알아봐야겠다",
          "original_form": "알아봐야겠다",
          "speaker_id": "1",
          "time": "20210826 18:17"
        },
        {
          "id": "MDRW2100015883.1.24",
          "form": "프레첼 배송되는거없나",
          "original_form": "프레첼 배송되는거없나",
          "speaker_id": "1",
          "time": "20210827 01:15"
        }
      ]
    }
