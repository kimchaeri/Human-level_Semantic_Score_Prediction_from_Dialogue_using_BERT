      "utterance": [
        {
          "id": "MDRW2100042993.1.1",
          "form": "리듬게임말고 해보신거 있나요?",
          "original_form": "리듬게임말고 해보신거 있나요?",
          "speaker_id": "1",
          "time": "20210907 12:40"
        },
        {
          "id": "MDRW2100042993.1.2",
          "form": "음 핸드폰으로는 영원한 7일의 도시라는거 했었어요",
          "original_form": "음 핸드폰으로는 영원한 7일의 도시라는거 했었어요",
          "speaker_id": "2",
          "time": "20210907 12:56"
        },
        {
          "id": "MDRW2100042993.1.3",
          "form": "그거 인기 많았었죠",
          "original_form": "그거 인기 많았었죠",
          "speaker_id": "1",
          "time": "20210907 13:40"
        },
        {
          "id": "MDRW2100042993.1.4",
          "form": "친구 하는거 봤는데 현질도 많이 하더라구요",
          "original_form": "친구 하는거 봤는데 현질도 많이 하더라구요",
          "speaker_id": "1",
          "time": "20210907 13:40"
        },
        {
          "id": "MDRW2100042993.1.5",
          "form": "맞아요 근데 섭종해서 안타깝..",
          "original_form": "맞아요 근데 섭종해서 안타깝..",
          "speaker_id": "2",
          "time": "20210907 13:43"
        },
        {
          "id": "MDRW2100042993.1.6",
          "form": "요새 게임은 금방 섭종 하는것도 아쉽네요.",
          "original_form": "요새 게임은 금방 섭종 하는것도 아쉽네요.",
          "speaker_id": "1",
          "time": "20210907 14:08"
        },
        {
          "id": "MDRW2100042993.1.7",
          "form": "맞아요... 꼭 재밌는게 금방 섭종 하더라구요",
          "original_form": "맞아요... 꼭 재밌는게 금방 섭종 하더라구요",
          "speaker_id": "2",
          "time": "20210907 15:54"
        },
        {
          "id": "MDRW2100042993.1.8",
          "form": "저는 모바일 카트 하고있어요.",
          "original_form": "저는 모바일 카트 하고있어요.",
          "speaker_id": "1",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100042993.1.9",
          "form": "헉 그거 재밌나요?",
          "original_form": "헉 그거 재밌나요?",
          "speaker_id": "2",
          "time": "20210907 15:56"
        },
        {
          "id": "MDRW2100042993.1.10",
          "form": "출시할때부터 했는데 재밌어요ㅋㅋ",
          "original_form": "출시할때부터 했는데 재밌어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 15:59"
        },
        {
          "id": "MDRW2100042993.1.11",
          "form": "아진짜요? 한 번 해봐야겠어요",
          "original_form": "아진짜요? 한 번 해봐야겠어요",
          "speaker_id": "2",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100042993.1.12",
          "form": "조작법도 간단해서 하기 쉬운것같아요",
          "original_form": "조작법도 간단해서 하기 쉬운것같아요",
          "speaker_id": "1",
          "time": "20210907 19:06"
        },
        {
          "id": "MDRW2100042993.1.13",
          "form": "뭔가 카트는 컴퓨터로 해야 재밌을 거 같다는 편견이 있었네용",
          "original_form": "뭔가 카트는 컴퓨터로 해야 재밌을 거 같다는 편견이 있었네용",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100042993.1.14",
          "form": "컴퓨터로도 가끔 하는데 좀 귀찮더라구요ㅋㅋ",
          "original_form": "컴퓨터로도 가끔 하는데 좀 귀찮더라구요ㅋㅋ",
