      "utterance": [
        {
          "id": "MDRW2100014801.1.1",
          "form": "설거지 다 했냐",
          "original_form": "설거지 다 했냐",
          "speaker_id": "2",
          "time": "20210825 20:50"
        },
        {
          "id": "MDRW2100014801.1.2",
          "form": "언니도 좀 해",
          "original_form": "언니도 좀 해",
          "speaker_id": "1",
          "time": "20210825 20:59"
        },
        {
          "id": "MDRW2100014801.1.3",
          "form": "난 팔아프자냐",
          "original_form": "난 팔아프자냐",
          "speaker_id": "2",
          "time": "20210825 21:00"
        },
        {
          "id": "MDRW2100014801.1.4",
          "form": "난 손목도 아프다",
          "original_form": "난 손목도 아프다",
          "speaker_id": "2",
          "time": "20210825 21:01"
        },
        {
          "id": "MDRW2100014801.1.5",
          "form": "나도 손목 안좋아 같이하면 되겠다",
          "original_form": "나도 손목 안좋아 같이하면 되겠다",
          "speaker_id": "1",
          "time": "20210825 21:06"
        },
        {
          "id": "MDRW2100014801.1.6",
          "form": "설거지 너무 힘들다 그지",
          "original_form": "설거지 너무 힘들다 그지",
          "speaker_id": "2",
          "time": "20210825 21:08"
        },
        {
          "id": "MDRW2100014801.1.7",
          "form": "내가 다 하잖아",
          "original_form": "내가 다 하잖아",
          "speaker_id": "1",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014801.1.8",
          "form": "야 나도 한번씩 해",
          "original_form": "야 나도 한번씩 해",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014801.1.9",
          "form": "오늘도 했어",
          "original_form": "오늘도 했어",
          "speaker_id": "2",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014801.1.10",
          "form": "언제? 간식먹고?",
          "original_form": "언제? 간식먹고?",
          "speaker_id": "1",
          "time": "20210825 21:11"
        },
        {
          "id": "MDRW2100014801.1.11",
          "form": "ㅇㅇ니가 먹은거도 내가치움",
          "original_form": "ㅇㅇ니가 먹은거도 내가치움",
          "speaker_id": "2",
          "time": "20210825 21:12"
        },
        {
          "id": "MDRW2100014801.1.12",
          "form": "난 매일 언니가 점심저녁으로 먹는거 치운다 고맙지?",
          "original_form": "난 매일 언니가 점심저녁으로 먹는거 치운다 고맙지?",
          "speaker_id": "1",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014801.1.13",
          "form": "너는 날 붕노하게하는구낭 가사는 해도 티가 안나는것같아ㅜㅜ",
          "original_form": "너는 날 붕노하게하는구낭 가사는 해도 티가 안나는것같아ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 21:15"
        },
        {
          "id": "MDRW2100014801.1.14",
          "form": "붕노가 아니라 분노!",
          "original_form": "붕노가 아니라 분노!",
          "speaker_id": "1",
          "time": "20210825 21:18"
        },
        {
          "id": "MDRW2100014801.1.15",
          "form": "힘들어 죽겠는데 더힘들게하고이써.. !",
          "original_form": "힘들어 죽겠는데 더힘들게하고이써.. !",
