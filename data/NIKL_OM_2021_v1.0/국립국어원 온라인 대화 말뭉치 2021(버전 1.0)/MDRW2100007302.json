      "utterance": [
        {
          "id": "MDRW2100007302.1.1",
          "form": "무슨 음식 제일 좋아해?",
          "original_form": "무슨 음식 제일 좋아해?",
          "speaker_id": "1",
          "time": "20210822 01:15"
        },
        {
          "id": "MDRW2100007302.1.2",
          "form": "음 고기?",
          "original_form": "음 고기?",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007302.1.3",
          "form": "무슨 고기?",
          "original_form": "무슨 고기?",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007302.1.4",
          "form": "소고기",
          "original_form": "소고기",
          "speaker_id": "2",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007302.1.5",
          "form": "나도 소고기 좋아",
          "original_form": "나도 소고기 좋아",
          "speaker_id": "1",
          "time": "20210822 01:16"
        },
        {
          "id": "MDRW2100007302.1.6",
          "form": "ㅋㅋㅋ 또 좋아하는 음식 있어?",
          "original_form": "ㅋㅋㅋ 또 좋아하는 음식 있어?",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007302.1.7",
          "form": "난 양고기도 좋아 요즘은 인도커리 먹고싶어",
          "original_form": "난 양고기도 좋아 요즘은 인도커리 먹고싶어",
          "speaker_id": "1",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100007302.1.8",
          "form": "인도커리 맛있던데",
          "original_form": "인도커리 맛있던데",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007302.1.9",
          "form": "먹어봤어?",
          "original_form": "먹어봤어?",
          "speaker_id": "1",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007302.1.10",
          "form": "누나랑 엄마랑 같이 먹었잖아",
          "original_form": "누나랑 엄마랑 같이 먹었잖아",
          "speaker_id": "2",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007302.1.11",
          "form": "아 맞다",
          "original_form": "아 맞다",
          "speaker_id": "1",
          "time": "20210822 01:18"
        },
        {
          "id": "MDRW2100007302.1.12",
          "form": "또 다른 건?",
          "original_form": "또 다른 건?",
          "speaker_id": "2",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100007302.1.13",
          "form": "음 스테이크나 마라탕",
          "original_form": "음 스테이크나 마라탕",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007302.1.14",
          "form": "스테이크 먹고싶다",
          "original_form": "스테이크 먹고싶다",
          "speaker_id": "2",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007302.1.15",
          "form": "그러게 스테이크 못먹은지 오래된 거 같아",
          "original_form": "그러게 스테이크 못먹은지 오래된 거 같아",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
