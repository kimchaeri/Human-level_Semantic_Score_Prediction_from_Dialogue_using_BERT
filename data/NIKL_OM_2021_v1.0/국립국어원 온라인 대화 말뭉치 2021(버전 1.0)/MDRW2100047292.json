      "utterance": [
        {
          "id": "MDRW2100047292.1.1",
          "form": "잡채볶음밥\n김치볶음밥도 좋아해요",
          "original_form": "잡채볶음밥\n김치볶음밥도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 09:23"
        },
        {
          "id": "MDRW2100047292.1.2",
          "form": "김치는 정말 진리에여",
          "original_form": "김치는 정말 진리에여",
          "speaker_id": "2",
          "time": "20210826 09:23"
        },
        {
          "id": "MDRW2100047292.1.3",
          "form": "저도 김치볶음밥요",
          "original_form": "저도 김치볶음밥요{emoji:♡}",
          "speaker_id": "1",
          "time": "20210826 09:23"
        },
        {
          "id": "MDRW2100047292.1.4",
          "form": "볶음밥 종류 다 좋아하는데\n그 중 최애는 김치요ㅋㅋ",
          "original_form": "볶음밥 종류 다 좋아하는데\n그 중 최애는 김치요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:23"
        },
        {
          "id": "MDRW2100047292.1.5",
          "form": "네 저도 김치가 가장좋아요",
          "original_form": "네 저도 김치가 가장좋아요",
          "speaker_id": "2",
          "time": "20210826 09:24"
        },
        {
          "id": "MDRW2100047292.1.6",
          "form": "김치가 많이 남지않아 요즘 아껴먹고있어요",
          "original_form": "김치가 많이 남지않아 요즘 아껴먹고있어요",
          "speaker_id": "2",
          "time": "20210826 09:24"
        },
        {
          "id": "MDRW2100047292.1.7",
          "form": "아 김치도 냉동으로 사두셨어요?",
          "original_form": "아 김치도 냉동으로 사두셨어요?",
          "speaker_id": "1",
          "time": "20210826 09:24"
        },
        {
          "id": "MDRW2100047292.1.8",
          "form": "아뇨 시댁에서 같이 담근거에요",
          "original_form": "아뇨 시댁에서 같이 담근거에요",
          "speaker_id": "2",
          "time": "20210826 09:25"
        },
        {
          "id": "MDRW2100047292.1.9",
          "form": "앗ㅋㅋ 김치볶음밥\n말씀하신줄 알고...ㅋㅋ",
          "original_form": "앗ㅋㅋ 김치볶음밥\n말씀하신줄 알고...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:26"
        },
        {
          "id": "MDRW2100047292.1.10",
          "form": "김장도 직접 하시나보네용",
          "original_form": "김장도 직접 하시나보네용",
          "speaker_id": "1",
          "time": "20210826 09:26"
        },
        {
          "id": "MDRW2100047292.1.11",
          "form": "아 김치볶음밥은 해서 먹어요",
          "original_form": "아 김치볶음밥은 해서 먹어요",
          "speaker_id": "2",
          "time": "20210826 09:27"
        },
        {
          "id": "MDRW2100047292.1.12",
          "form": "네 시댁에서 담궈요 올핸 더많이 가져와야겠어요",
          "original_form": "네 시댁에서 담궈요 올핸 더많이 가져와야겠어요",
          "speaker_id": "2",
          "time": "20210826 09:27"
        },
        {
          "id": "MDRW2100047292.1.13",
          "form": "저는 엄마가 담궈주시는데\n벌써 다먹었어요 ㅜㅜㅋㅋ",
          "original_form": "저는 엄마가 담궈주시는데\n벌써 다먹었어요 ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:28"
        },
        {
          "id": "MDRW2100047292.1.14",
          "form": "엄마 김치 너무 맛있어요",
          "original_form": "엄마 김치 너무 맛있어요{emoji:♡}",
          "speaker_id": "1",
          "time": "20210826 09:28"
        },
        {
          "id": "MDRW2100047292.1.15",
          "form": "맞아요 엄마김치 너무아깝죠..아껴먹어야해요",
          "original_form": "맞아요 엄마김치 너무아깝죠..아껴먹어야해요",
          "speaker_id": "2",
          "time": "20210826 09:29"
        },
        {
          "id": "MDRW2100047292.1.16",
          "form": "돼지고기넣고 푹삶아먹어도 맛나요",
          "original_form": "돼지고기넣고 푹삶아먹어도 맛나요",
          "speaker_id": "2",
          "time": "20210826 09:31"
        },
        {
          "id": "MDRW2100047292.1.17",
          "form": "그쵸\n돼지고기랑 김치랑 조화는\n다 성공적인거 같아요ㅋㅋ",
          "original_form": "그쵸{emoji:♡}\n돼지고기랑 김치랑 조화는\n다 성공적인거 같아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:42"
        },
        {
          "id": "MDRW2100047292.1.18",
          "form": "맞아요 쌈장해서 상추에싸먹어도 맛나고",
          "original_form": "맞아요 쌈장해서 상추에싸먹어도 맛나고",
          "speaker_id": "2",
          "time": "20210826 09:43"
        },
        {
          "id": "MDRW2100047292.1.19",
          "form": "먹고싶네요",
          "original_form": "먹고싶네요",
          "speaker_id": "2",
          "time": "20210826 09:44"
        },
        {
          "id": "MDRW2100047292.1.20",
          "form": "크으 저도 침샘폭발요ㅋㅋ",
          "original_form": "크으{emoji:♡} 저도 침샘폭발요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:44"
        },
        {
          "id": "MDRW2100047292.1.21",
          "form": "아는맛은 무섭죠 ㅋㅋ",
          "original_form": "아는맛은 무섭죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 09:44"
        },
        {
          "id": "MDRW2100047292.1.22",
          "form": "정말...침샘이 ㅎㅎ\n맞아요 아는맛 한번먹으면..안먹을수없는",
          "original_form": "정말...침샘이 ㅎㅎ\n맞아요 아는맛 한번먹으면..안먹을수없는",
          "speaker_id": "2",
          "time": "20210826 09:44"
        }
      ]
