      "utterance": [
        {
          "id": "MDRW2100032431.1.1",
          "form": "저녁 언제 먹어?",
          "original_form": "저녁 언제 먹어?",
          "speaker_id": "1",
          "time": "20210905 17:25"
        },
        {
          "id": "MDRW2100032431.1.2",
          "form": "이제 먹어야지",
          "original_form": "이제 먹어야지",
          "speaker_id": "2",
          "time": "20210905 17:25"
        },
        {
          "id": "MDRW2100032431.1.3",
          "form": "짬뽕밥 먹는다고 했지",
          "original_form": "짬뽕밥 먹는다고 했지",
          "speaker_id": "1",
          "time": "20210905 17:25"
        },
        {
          "id": "MDRW2100032431.1.4",
          "form": "응 낮에 먹은 짬뽕 국물 남은 거에",
          "original_form": "응 낮에 먹은 짬뽕 국물 남은 거에",
          "speaker_id": "2",
          "time": "20210905 17:26"
        },
        {
          "id": "MDRW2100032431.1.5",
          "form": "나는 방금 핫도그 먹고 왔어 ㅋㅋ",
          "original_form": "나는 방금 핫도그 먹고 왔어 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 17:42"
        },
        {
          "id": "MDRW2100032431.1.6",
          "form": "맛있게 먹었어?",
          "original_form": "맛있게 먹었어?",
          "speaker_id": "2",
          "time": "20210905 17:56"
        },
        {
          "id": "MDRW2100032431.1.7",
          "form": "오랜만에 먹으니까 맛있더라고",
          "original_form": "오랜만에 먹으니까 맛있더라고",
          "speaker_id": "1",
          "time": "20210905 17:57"
        },
        {
          "id": "MDRW2100032431.1.8",
          "form": "핫도그랑 같이 먹으면 좋은 메뉴 추천해줘",
          "original_form": "핫도그랑 같이 먹으면 좋은 메뉴 추천해줘",
          "speaker_id": "2",
          "time": "20210905 17:58"
        },
        {
          "id": "MDRW2100032431.1.9",
          "form": "꿀조합은 역시 떡볶이?",
          "original_form": "꿀조합은 역시 떡볶이?",
          "speaker_id": "1",
          "time": "20210905 17:58"
        },
        {
          "id": "MDRW2100032431.1.10",
          "form": "맛있지 떡볶이도 먹고 싶네",
          "original_form": "맛있지 떡볶이도 먹고 싶네",
          "speaker_id": "2",
          "time": "20210905 17:58"
        },
        {
          "id": "MDRW2100032431.1.11",
          "form": "근데 핫도그는 뭐랑 먹어도 맛있잖아",
          "original_form": "근데 핫도그는 뭐랑 먹어도 맛있잖아",
          "speaker_id": "1",
          "time": "20210905 17:59"
        },
        {
          "id": "MDRW2100032431.1.12",
          "form": "나는 핫도그 먹을 때 치즈 더 뿌려먹어",
          "original_form": "나는 핫도그 먹을 때 치즈 더 뿌려먹어",
          "speaker_id": "2",
          "time": "20210905 17:59"
        },
        {
          "id": "MDRW2100032431.1.13",
          "form": "어 맛있겠다 그렇게 먹으면",
          "original_form": "어 맛있겠다 그렇게 먹으면",
          "speaker_id": "1",
          "time": "20210905 17:59"
        }
