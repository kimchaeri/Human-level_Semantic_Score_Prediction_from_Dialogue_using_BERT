        {
          "id": "MDRW2100006093.1.1",
          "form": "가쥬앗",
          "original_form": "가쥬앗",
          "speaker_id": "1",
          "time": "20210907 20:19"
        },
        {
          "id": "MDRW2100006093.1.2",
          "form": "어제오늘 재택교육들었는데",
          "original_form": "어제오늘 재택교육들었는데",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100006093.1.3",
          "form": "내일 출근하기가 실타",
          "original_form": "내일 출근하기가 실타",
          "speaker_id": "2",
          "time": "20210907 20:29"
        },
        {
          "id": "MDRW2100006093.1.4",
          "form": "집이었어 오늘?",
          "original_form": "집이었어 오늘?",
          "speaker_id": "1",
          "time": "20210907 20:30"
        },
        {
          "id": "MDRW2100006093.1.5",
          "form": "어 어제오늘 집돌이 ㅋㅋ",
          "original_form": "어 어제오늘 집돌이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:31"
        },
        {
          "id": "MDRW2100006093.1.6",
          "form": "와 부럽다",
          "original_form": "와 부럽다",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100006093.1.7",
          "form": "나 오늘 숙직이야",
          "original_form": "나 오늘 숙직이야",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100006093.1.8",
          "form": "지금당직실",
          "original_form": "지금당직실",
          "speaker_id": "1",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100006093.1.9",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100006093.1.10",
          "form": "내일 쉬겠네 그럼",
          "original_form": "내일 쉬겠네 그럼",
          "speaker_id": "2",
          "time": "20210907 20:32"
        },
        {
          "id": "MDRW2100006093.1.11",
          "form": "머할거야 내일은?",
          "original_form": "머할거야 내일은?",
          "speaker_id": "2",
          "time": "20210907 20:33"
        },
        {
          "id": "MDRW2100006093.1.12",
          "form": "자야지",
          "original_form": "자야지",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100006093.1.13",
          "form": "피곤하겠지",
          "original_form": "피곤하겠지",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100006093.1.14",
          "form": "제발 오늘 아무일 없어라",
          "original_form": "제발 오늘 아무일 없어라",
          "speaker_id": "1",
          "time": "20210907 20:35"
        },
        {
          "id": "MDRW2100006093.1.15",
          "form": "혼사 서??",
          "original_form": "혼사 서??",
          "speaker_id": "2",
          "time": "20210907 20:36"
        },
        {
          "id": "MDRW2100006093.1.16",
          "form": "아니 세명이 서",
          "original_form": "아니 세명이 서",
          "speaker_id": "1",
          "time": "20210907 20:37"
        },
        {
          "id": "MDRW2100006093.1.17",
          "form": "오 시설이 커서 그런가 많이 서네",
          "original_form": "오 시설이 커서 그런가 많이 서네",
          "speaker_id": "2",
          "time": "20210907 20:39"
        },
        {
          "id": "MDRW2100006093.1.18",
          "form": "인원이 좀 많으니까",
          "original_form": "인원이 좀 많으니까",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100006093.1.19",
          "form": "그렇게 서더라고",
          "original_form": "그렇게 서더라고",
          "speaker_id": "1",
          "time": "20210907 20:40"
        },
        {
          "id": "MDRW2100006093.1.20",
          "form": "좋다 나 내일 시보 끝나서 다음달부터 숙직들어가는데",
          "original_form": "좋다 나 내일 시보 끝나서 다음달부터 숙직들어가는데",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100006093.1.21",
          "form": "숙직 너무 싫어",
          "original_form": "숙직 너무 싫어",
          "speaker_id": "1",
          "time": "20210907 22:18"
        },
        {
          "id": "MDRW2100006093.1.22",
          "form": "맞아 왜하는지모르겠어",
          "original_form": "맞아 왜하는지모르겠어",
          "speaker_id": "2",
          "time": "20210907 22:29"
        },
        {
          "id": "MDRW2100006093.1.23",
          "form": "내일 쉬니까 참자",
          "original_form": "내일 쉬니까 참자",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100006093.1.24",
          "form": "별일없었어 name1?",
