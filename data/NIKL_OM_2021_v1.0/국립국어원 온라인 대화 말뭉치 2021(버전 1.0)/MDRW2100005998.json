      "utterance": [
        {
          "id": "MDRW2100005998.1.1",
          "form": "단백질 쉐이크도 드세요?",
          "original_form": "단백질 쉐이크도 드세요?",
          "speaker_id": "1",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100005998.1.2",
          "form": "아뇨 그정도로 ㅋㅋ 빡세게는 아니에요",
          "original_form": "아뇨 그정도로 ㅋㅋ 빡세게는 아니에요",
          "speaker_id": "2",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100005998.1.3",
          "form": "아그래요",
          "original_form": "아그래요",
          "speaker_id": "1",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100005998.1.4",
          "form": "그냥 적당히 한끼정도 건강하게!",
          "original_form": "그냥 적당히 한끼정도 건강하게!",
          "speaker_id": "2",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100005998.1.5",
          "form": "전 사놓고 뜯지도않았어요ㅠㅠ",
          "original_form": "전 사놓고 뜯지도않았어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 18:30"
        },
        {
          "id": "MDRW2100005998.1.6",
          "form": "단백질쉐이크요?ㅎㅎ",
          "original_form": "단백질쉐이크요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 18:32"
        },
        {
          "id": "MDRW2100005998.1.7",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100005998.1.8",
          "form": "초코맛은 친구주고 딸기맛은 안뜯었어요",
          "original_form": "초코맛은 친구주고 딸기맛은 안뜯었어요",
          "speaker_id": "1",
          "time": "20210907 18:58"
        },
        {
          "id": "MDRW2100005998.1.9",
          "form": "ㅋㅋ왜 사셨어용",
          "original_form": "ㅋㅋ왜 사셨어용",
          "speaker_id": "2",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100005998.1.10",
          "form": "그러게요 다이어트 불타올라서 사놓고 안먹네여",
          "original_form": "그러게요 다이어트 불타올라서 사놓고 안먹네여",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100005998.1.11",
          "form": "ㅋㅋ가끔씩이라도 드셔요!!",
          "original_form": "ㅋㅋ가끔씩이라도 드셔요!!",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100005998.1.12",
          "form": "네네 우유 사와서 타목어야겠어요",
          "original_form": "네네 우유 사와서 타목어야겠어요",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100005998.1.13",
          "form": "아깝다",
          "original_form": "아깝다",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100005998.1.14",
          "form": "맛은 있나요?",
          "original_form": "맛은 있나요?",
          "speaker_id": "2",
