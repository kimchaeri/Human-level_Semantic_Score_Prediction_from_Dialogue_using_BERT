      "utterance": [
        {
          "id": "MDRW2100019910.1.1",
          "form": "야식드세요?",
          "original_form": "야식드세요?",
          "speaker_id": "2",
          "time": "20210827 22:46"
        },
        {
          "id": "MDRW2100019910.1.2",
          "form": "아뇨 name2님이  라면이야기해서",
          "original_form": "아뇨 &name2&님이  라면이야기해서",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019910.1.3",
          "form": "생라면 고민되요",
          "original_form": "생라면 고민되요",
          "speaker_id": "1",
          "time": "20210827 22:47"
        },
        {
          "id": "MDRW2100019910.1.4",
          "form": "생라면 한개만ㅋ드세요",
          "original_form": "생라면 한개만ㅋ드세요",
          "speaker_id": "2",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019910.1.5",
          "form": "ㅋㅋㅋ 불닭소스에 콕 ㅋㅋ",
          "original_form": "ㅋㅋㅋ 불닭소스에 콕 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:48"
        },
        {
          "id": "MDRW2100019910.1.6",
          "form": "야호 대박 맛나겠는데요 고고",
          "original_form": "야호 대박 맛나겠는데요 고고",
          "speaker_id": "2",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019910.1.7",
          "form": "ㅋㅋㅋㅋ 티비버니 연애인 그리 먹더라구요",
          "original_form": "ㅋㅋㅋㅋ 티비버니 연애인 그리 먹더라구요",
          "speaker_id": "1",
          "time": "20210827 22:49"
        },
        {
          "id": "MDRW2100019910.1.8",
          "form": "불닭소스는 어디 써먹어도 굳이죠",
          "original_form": "불닭소스는 어디 써먹어도 굳이죠",
          "speaker_id": "2",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019910.1.9",
          "form": "맞아유!!!",
          "original_form": "맞아유!!!",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019910.1.10",
          "form": "이반에  청정원에서 찍먹소스 3종 샀는데",
          "original_form": "이반에  청정원에서 찍먹소스 3종 샀는데",
          "speaker_id": "1",
          "time": "20210827 22:50"
        },
        {
          "id": "MDRW2100019910.1.11",
          "form": "전 불닭이랑 순대섞어서",
          "original_form": "전 불닭이랑 순대섞어서",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019910.1.12",
          "form": "맛이  니맛도 내맛도 이닌",
          "original_form": "맛이  니맛도 내맛도 이닌",
          "speaker_id": "1",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019910.1.13",
          "form": "ㅋ 나름 괜춘해요",
          "original_form": "ㅋ 나름 괜춘해요",
          "speaker_id": "2",
          "time": "20210827 22:51"
        },
        {
          "id": "MDRW2100019910.1.14",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019910.1.15",
          "form": "맛잇겟다아!!",
          "original_form": "맛잇겟다아!!",
          "speaker_id": "1",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019910.1.16",
          "form": "양파좀넣고요",
          "original_form": "양파좀넣고요",
          "speaker_id": "2",
          "time": "20210827 22:53"
        },
        {
          "id": "MDRW2100019910.1.17",
          "form": "순대엔 역시 깻잎 팍",
          "original_form": "순대엔 역시 깻잎 팍",
