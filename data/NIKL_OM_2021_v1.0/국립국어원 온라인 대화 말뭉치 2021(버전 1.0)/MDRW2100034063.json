      "utterance": [
        {
          "id": "MDRW2100034063.1.1",
          "form": "대학졸업하셨나요?",
          "original_form": "대학졸업하셨나요?",
          "speaker_id": "2",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100034063.1.2",
          "form": "네 졸업했어요",
          "original_form": "네 졸업했어요",
          "speaker_id": "1",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100034063.1.3",
          "form": "현재 학생이세요?",
          "original_form": "현재 학생이세요?",
          "speaker_id": "1",
          "time": "20210827 13:49"
        },
        {
          "id": "MDRW2100034063.1.4",
          "form": "네! 대학생 3학년이에요",
          "original_form": "네! 대학생 3학년이에요",
          "speaker_id": "2",
          "time": "20210827 13:51"
        },
        {
          "id": "MDRW2100034063.1.5",
          "form": "그럼 곧 개강이신가요?",
          "original_form": "그럼 곧 개강이신가요?",
          "speaker_id": "1",
          "time": "20210827 13:58"
        },
        {
          "id": "MDRW2100034063.1.6",
          "form": "네 다음주 월요일에 개강이에요 ㅠㅠ",
          "original_form": "네 다음주 월요일에 개강이에요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 14:00"
        },
        {
          "id": "MDRW2100034063.1.7",
          "form": "헉 금방이네요ㅠㅠ",
          "original_form": "헉 금방이네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 14:02"
        },
        {
          "id": "MDRW2100034063.1.8",
          "form": "수강신청은 하셨나요?",
          "original_form": "수강신청은 하셨나요?",
          "speaker_id": "1",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100034063.1.9",
          "form": "네! 저저번주쯤에 했어요",
          "original_form": "네! 저저번주쯤에 했어요",
          "speaker_id": "2",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100034063.1.10",
          "form": "수강예약부터 했는데 인원초과안돼서 그대로 신청완료했어요 ㅎㅎ",
          "original_form": "수강예약부터 했는데 인원초과안돼서 그대로 신청완료했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 14:03"
        },
        {
          "id": "MDRW2100034063.1.11",
          "form": "아하 요즘은 그렇게 하는군요",
          "original_form": "아하 요즘은 그렇게 하는군요",
          "speaker_id": "1",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100034063.1.12",
          "form": "대면수업으로 진행하나요?",
          "original_form": "대면수업으로 진행하나요?",
          "speaker_id": "1",
          "time": "20210827 14:05"
        },
        {
          "id": "MDRW2100034063.1.13",
          "form": "코로나때문에 9월말까지는 비대면이에요",
          "original_form": "코로나때문에 9월말까지는 비대면이에요",
          "speaker_id": "2",
          "time": "20210827 14:06"
        },
        {
          "id": "MDRW2100034063.1.14",
          "form": "이미 정해져있군요 어때요? 비대면수업이 더 좋나요?",
          "original_form": "이미 정해져있군요 어때요? 비대면수업이 더 좋나요?",
          "speaker_id": "1",
          "time": "20210827 14:07"
        },
        {
          "id": "MDRW2100034063.1.15",
          "form": "등교 준비도 안해도돼서 시간절약도 되고 편하고 좋아요 ㅋㅋㅋ",
          "original_form": "등교 준비도 안해도돼서 시간절약도 되고 편하고 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:08"
        },
        {
          "id": "MDRW2100034063.1.16",
          "form": "확실히 편하긴하겠네요 실습수업은 어떻게 진행돼요?",
          "original_form": "확실히 편하긴하겠네요 실습수업은 어떻게 진행돼요?",
          "speaker_id": "1",
          "time": "20210827 14:15"
        },
        {
          "id": "MDRW2100034063.1.17",
          "form": "원래 실습수업이나 적은 인원수의 수업은 부분적으로 대면을 했어요",
          "original_form": "원래 실습수업이나 적은 인원수의 수업은 부분적으로 대면을 했어요",
          "speaker_id": "2",
          "time": "20210827 14:16"
        },
        {
          "id": "MDRW2100034063.1.18",
          "form": "근데 이번에는 코로나가 심각해져서 일단 9월말까지 모든 수업이 비대면이에요",
          "original_form": "근데 이번에는 코로나가 심각해져서 일단 9월말까지 모든 수업이 비대면이에요",
          "speaker_id": "2",
          "time": "20210827 14:16"
        },
        {
          "id": "MDRW2100034063.1.19",
          "form": "그렇게되었군요 궁금했는데 알려주셔서 감사해요",
          "original_form": "그렇게되었군요 궁금했는데 알려주셔서 감사해요",
          "speaker_id": "1",
          "time": "20210827 14:19"
        }
      ]
