      "utterance": [
        {
          "id": "MDRW2100005270.1.1",
          "form": "나 이번에 일하는 회사가 아이티쪽이잖아",
          "original_form": "나 이번에 일하는 회사가 아이티쪽이잖아",
          "speaker_id": "1",
          "time": "20210821 21:32"
        },
        {
          "id": "MDRW2100005270.1.2",
          "form": "이번에 nia에서 시무식하는데 코로나땜에 메타버스에서 시무식한다해서 구경갔는데",
          "original_form": "이번에 nia에서 시무식하는데 코로나땜에 메타버스에서 시무식한다해서 구경갔는데",
          "speaker_id": "1",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005270.1.3",
          "form": "2D게임처럼 꾸며놨더라",
          "original_form": "2D게임처럼 꾸며놨더라",
          "speaker_id": "1",
          "time": "20210821 21:37"
        },
        {
          "id": "MDRW2100005270.1.4",
          "form": "헉 하지만",
          "original_form": "헉 하지만",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005270.1.5",
          "form": "난가상세계에대해 잘모르는데",
          "original_form": "난가상세계에대해 잘모르는데",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005270.1.6",
          "form": "리스닝위주로해도되겠니?",
          "original_form": "리스닝위주로해도되겠니?",
          "speaker_id": "2",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005270.1.7",
          "form": "앗 잘 몰라?",
          "original_form": "앗 잘 몰라?",
          "speaker_id": "1",
          "time": "20210821 21:38"
        },
        {
          "id": "MDRW2100005270.1.8",
          "form": "2D게임을 현실에 옮겨온거야?",
          "original_form": "2D게임을 현실에 옮겨온거야?",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005270.1.9",
          "form": "아냐 하지만내가",
          "original_form": "아냐 하지만내가",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005270.1.10",
          "form": "리액션은 잘 할 수있오",
          "original_form": "리액션은 잘 할 수있오",
          "speaker_id": "2",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005270.1.11",
          "form": "아니아니 그 뭐라해야하지 ㅋㅋㅋㅋ",
          "original_form": "아니아니 그 뭐라해야하지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005270.1.12",
          "form": "게임속에서 서로 채팅하는",
          "original_form": "게임속에서 서로 채팅하는",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005270.1.13",
          "form": "그런 기분으로",
          "original_form": "그런 기분으로",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005270.1.14",
          "form": "가상세계를 만들었더라고",
          "original_form": "가상세계를 만들었더라고",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005270.1.15",
          "form": "도트게임감성으로",
          "original_form": "도트게임감성으로",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005270.1.16",
          "form": "내 아바타 만들어서",
          "original_form": "내 아바타 만들어서",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005270.1.17",
          "form": "상대 아바타에 접근해서 회의가능하고",
          "original_form": "상대 아바타에 접근해서 회의가능하고",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005270.1.18",
          "form": "방명록도 게임(?)에다가 남기고 그런 느낌",
          "original_form": "방명록도 게임(?)에다가 남기고 그런 느낌",
          "speaker_id": "1",
          "time": "20210821 21:39"
        },
        {
          "id": "MDRW2100005270.1.19",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005270.1.20",
          "form": "회사를 그런식느로",
          "original_form": "회사를 그런식느로",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005270.1.21",
          "form": "그런식느로",
          "original_form": "그런식느로",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005270.1.22",
          "form": "가상세계처럼 만든거야?",
          "original_form": "가상세계처럼 만든거야?",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005270.1.23",
          "form": "신기하당",
          "original_form": "신기하당",
          "speaker_id": "2",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005270.1.24",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:41"
        },
        {
          "id": "MDRW2100005270.1.25",
          "form": "나도 이번에 첨 해봤는데",
          "original_form": "나도 이번에 첨 해봤는데",
          "speaker_id": "1",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005270.1.26",
          "form": "매타버스 안에 간단하게 게임도 설치할수있고",
          "original_form": "매타버스 안에 간단하게 게임도 설치할수있고",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005270.1.27",
          "form": "그냥... 게임에 가깝게 구현됐더라고 아직은",
          "original_form": "그냥... 게임에 가깝게 구현됐더라고 아직은",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005270.1.28",
          "form": "근데 기사보니까 메타버스 이런거 코로나땜에 활성화하고 싶은지 이런저런 다양한 시도들이 있더라",
          "original_form": "근데 기사보니까 메타버스 이런거 코로나땜에 활성화하고 싶은지 이런저런 다양한 시도들이 있더라",
          "speaker_id": "1",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005270.1.29",
          "form": "이야 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "이야 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005270.1.30",
          "form": "확실히 이런게",
          "original_form": "확실히 이런게",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005270.1.31",
          "form": "발전에 발전을 하긴 하는구나 싶네",
          "original_form": "발전에 발전을 하긴 하는구나 싶네",
          "speaker_id": "2",
          "time": "20210821 21:44"
        },
        {
          "id": "MDRW2100005270.1.32",
          "form": "ㅁㅈㅁㅈ 근데 아직은 좀... 기술적으로 그정도 발전은 아니라서",
          "original_form": "ㅁㅈㅁㅈ 근데 아직은 좀... 기술적으로 그정도 발전은 아니라서",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005270.1.33",
          "form": "그냥...게임에 가까운 느낌...???",
          "original_form": "그냥...게임에 가까운 느낌...???",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005270.1.34",
          "form": "ㅇㅎㅇㅎ",
          "original_form": "ㅇㅎㅇㅎ",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005270.1.35",
          "form": "거기 가보고 잼썼겠다ㅋㅋㅋㅋ",
          "original_form": "거기 가보고 잼썼겠다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005270.1.36",
          "form": "그래도 메타버스 활성화시키려고",
          "original_form": "그래도 메타버스 활성화시키려고",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005270.1.37",
          "form": "굳~이 국가기관측에서 시무식 일부러",
          "original_form": "굳~이 국가기관측에서 시무식 일부러",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005270.1.38",
          "form": "메타버스에서 한거 아닌가 싶음..",
          "original_form": "메타버스에서 한거 아닌가 싶음..",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005270.1.39",
          "form": "그냥 줌으로 하지...",
          "original_form": "그냥 줌으로 하지...",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005270.1.40",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005270.1.41",
          "form": "근데 뭐 메타버스 이런게",
          "original_form": "근데 뭐 메타버스 이런게",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005270.1.42",
          "form": "나중에 또 산업 경쟁력이 있고 하면",
          "original_form": "나중에 또 산업 경쟁력이 있고 하면",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005270.1.43",
          "form": "국가의...발전을 위한",
          "original_form": "국가의...발전을 위한",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005270.1.44",
          "form": "그런걸로도 될수있지않을까해",
          "original_form": "그런걸로도 될수있지않을까해",
          "speaker_id": "2",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005270.1.45",
          "form": "몰라서 아무말이나 막 던지기",
          "original_form": "몰라서 아무말이나 막 던지기",
