      "utterance": [
        {
          "id": "MDRW2100003050.1.1",
          "form": "오늘은 퇴근 언제하세요?",
          "original_form": "오늘은 퇴근 언제하세요?",
          "speaker_id": "1",
          "time": "20210820 17:59"
        },
        {
          "id": "MDRW2100003050.1.2",
          "form": "오늘은 이제 퇴근해요ㅠㅠ",
          "original_form": "오늘은 이제 퇴근해요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 19:48"
        },
        {
          "id": "MDRW2100003050.1.3",
          "form": "오늘도 퇴근이 늦으셨네요ㅜㅜ",
          "original_form": "오늘도 퇴근이 늦으셨네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 19:49"
        },
        {
          "id": "MDRW2100003050.1.4",
          "form": "지금이 바쁠 시즌인가요?",
          "original_form": "지금이 바쁠 시즌인가요?",
          "speaker_id": "1",
          "time": "20210820 19:56"
        },
        {
          "id": "MDRW2100003050.1.5",
          "form": "아님 업무가 항상 많으세요?",
          "original_form": "아님 업무가 항상 많으세요?",
          "speaker_id": "1",
          "time": "20210820 19:57"
        },
        {
          "id": "MDRW2100003050.1.6",
          "form": "요즘 좀 일이 늘었어요",
          "original_form": "요즘 좀 일이 늘었어요",
          "speaker_id": "2",
          "time": "20210820 21:30"
        },
        {
          "id": "MDRW2100003050.1.7",
          "form": "워라밸을 유지하는게 목푠데",
          "original_form": "워라밸을 유지하는게 목푠데",
          "speaker_id": "2",
          "time": "20210820 21:30"
        },
        {
          "id": "MDRW2100003050.1.8",
          "form": "그래도 주말에는 온전히 쉬시는거죠?",
          "original_form": "그래도 주말에는 온전히 쉬시는거죠?",
          "speaker_id": "1",
          "time": "20210820 22:08"
        },
        {
          "id": "MDRW2100003050.1.9",
          "form": "주말은 쉬어야죠",
          "original_form": "주말은 쉬어야죠",
          "speaker_id": "2",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003050.1.10",
          "form": "주말에 뭐하세요?",
          "original_form": "주말에 뭐하세요?",
          "speaker_id": "2",
          "time": "20210820 22:10"
        },
        {
          "id": "MDRW2100003050.1.11",
          "form": "주말에 머리하고 싶은데\r\n어떻게 할지 모르겠어여 ㅋ",
          "original_form": "주말에 머리하고 싶은데\r\n어떻게 할지 모르겠어여 ㅋ",
          "speaker_id": "1",
          "time": "20210820 22:14"
        },
        {
          "id": "MDRW2100003050.1.12",
          "form": "앗 미용실가세요?",
          "original_form": "앗 미용실가세요?",
          "speaker_id": "2",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003050.1.13",
          "form": "저도 머리해야하는데",
          "original_form": "저도 머리해야하는데",
          "speaker_id": "2",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003050.1.14",
          "form": "니니님은 어떤 스탈 원하세요? ㅋㅋ",
          "original_form": "니니님은 어떤 스탈 원하세요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:17"
        },
        {
          "id": "MDRW2100003050.1.15",
          "form": "저는 또 단발병이 오고 있네여ㅠㅋ",
          "original_form": "저는 또 단발병이 오고 있네여ㅠㅋ",
          "speaker_id": "1",
          "time": "20210820 22:17"
        },
        {
          "id": "MDRW2100003050.1.16",
          "form": "저는 지금은 긴머리인데 오랜만에 기른거라",
          "original_form": "저는 지금은 긴머리인데 오랜만에 기른거라",
          "speaker_id": "2",
          "time": "20210820 22:21"
        },
        {
          "id": "MDRW2100003050.1.17",
          "form": "엇 저두요ㅋㅋ",
          "original_form": "엇 저두요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:21"
        },
        {
          "id": "MDRW2100003050.1.18",
          "form": "아깝죠 자르기ㅜㅜㅜㅜ",
          "original_form": "아깝죠 자르기ㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 22:22"
        },
        {
          "id": "MDRW2100003050.1.19",
          "form": "근데 또 긴것도 관리가 힘들어서",
          "original_form": "근데 또 긴것도 관리가 힘들어서",
          "speaker_id": "2",
          "time": "20210820 22:23"
        },
        {
          "id": "MDRW2100003050.1.20",
          "form": "맞아요 머리 말리는데도 오래걸리고ㅋㅋ",
          "original_form": "맞아요 머리 말리는데도 오래걸리고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:27"
        },
        {
          "id": "MDRW2100003050.1.21",
          "form": "ㅋㅋㅋㅋ머리 감는거는 빨래 빨듯이",
          "original_form": "ㅋㅋㅋㅋ머리 감는거는 빨래 빨듯이",
          "speaker_id": "2",
          "time": "20210820 22:29"
        },
        {
          "id": "MDRW2100003050.1.22",
          "form": "ㅋㅋㅋ말리는거는요?",
          "original_form": "ㅋㅋㅋ말리는거는요?",
          "speaker_id": "1",
          "time": "20210820 22:57"
        },
        {
          "id": "MDRW2100003050.1.23",
          "form": "ㅋㅋㅋ말리는것도 답이 없네여ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ말리는것도 답이 없네여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 23:22"
        },
        {
          "id": "MDRW2100003050.1.24",
          "form": "직장은 복장이나 이런건 자유롭나요?",
          "original_form": "직장은 복장이나 이런건 자유롭나요?",
          "speaker_id": "1",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003050.1.25",
          "form": "복장은 자유로워요",
          "original_form": "복장은 자유로워요",
          "speaker_id": "2",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003050.1.26",
          "form": "저는 주로 셔츠에 슬랙스 입는데",
          "original_form": "저는 주로 셔츠에 슬랙스 입는데",
          "speaker_id": "2",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003050.1.27",
          "form": "항상 옷이 없어요",
          "original_form": "항상 옷이 없어요",
          "speaker_id": "2",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100003050.1.28",
          "form": "모든 여자들의 고민일거 같아여ㅋㅋ",
          "original_form": "모든 여자들의 고민일거 같아여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:24"
        },
        {
          "id": "MDRW2100003050.1.29",
          "form": "옷장에 옷은 가득하지만",
          "original_form": "옷장에 옷은 가득하지만",
          "speaker_id": "1",
          "time": "20210820 23:24"
        },
        {
          "id": "MDRW2100003050.1.30",
          "form": "정작 입을 옷은 없다 ㅋㅋㅋ",
          "original_form": "정작 입을 옷은 없다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 23:24"
        },
        {
          "id": "MDRW2100003050.1.31",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 23:31"
        },
        {
          "id": "MDRW2100003050.1.32",
          "form": "또 가을옷 사고싶은데ㅠㅠ",
          "original_form": "또 가을옷 사고싶은데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100003050.1.33",
          "form": "저는 올해로 지금회사 입사2년차여서",
          "original_form": "저는 올해로 지금회사 입사2년차여서",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100003050.1.34",
          "form": "휴가가 3일 나와요",
          "original_form": "휴가가 3일 나와요",
          "speaker_id": "2",
          "time": "20210820 23:32"
        },
        {
          "id": "MDRW2100003050.1.35",
          "form": "휴가때는 뭐하실거에여?",
          "original_form": "휴가때는 뭐하실거에여?",
          "speaker_id": "1",
          "time": "20210820 23:36"
        }
      ]
