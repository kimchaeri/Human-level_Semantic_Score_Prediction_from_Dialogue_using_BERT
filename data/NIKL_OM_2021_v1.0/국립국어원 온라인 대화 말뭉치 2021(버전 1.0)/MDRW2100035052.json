        {
          "id": "MDRW2100035052.1.1",
          "form": "여행 계획을 짜고 있어",
          "original_form": "여행 계획을 짜고 있어",
          "speaker_id": "2",
          "time": "20210829 01:12"
        },
        {
          "id": "MDRW2100035052.1.2",
          "form": "휴가 받았니",
          "original_form": "휴가 받았니",
          "speaker_id": "1",
          "time": "20210829 01:13"
        },
        {
          "id": "MDRW2100035052.1.3",
          "form": "평일에 이 일간 쉴 수 있어",
          "original_form": "평일에 이 일간 쉴 수 있어",
          "speaker_id": "2",
          "time": "20210829 01:13"
        },
        {
          "id": "MDRW2100035052.1.4",
          "form": "부럽다 마니",
          "original_form": "부럽다 마니",
          "speaker_id": "1",
          "time": "20210829 01:13"
        },
        {
          "id": "MDRW2100035052.1.5",
          "form": "귀중한 휴가야",
          "original_form": "귀중한 휴가야",
          "speaker_id": "2",
          "time": "20210829 01:13"
        },
        {
          "id": "MDRW2100035052.1.6",
          "form": "그래서 어디를 가는데",
          "original_form": "그래서 어디를 가는데",
          "speaker_id": "1",
          "time": "20210829 01:13"
        },
        {
          "id": "MDRW2100035052.1.7",
          "form": "에버랜드에 가려고",
          "original_form": "에버랜드에 가려고",
          "speaker_id": "2",
          "time": "20210829 01:13"
        },
        {
          "id": "MDRW2100035052.1.8",
          "form": "에버랜드 좋지",
          "original_form": "에버랜드 좋지",
          "speaker_id": "1",
          "time": "20210829 01:13"
        },
        {
          "id": "MDRW2100035052.1.9",
          "form": "지금 날씨가 딱 좋아",
          "original_form": "지금 날씨가 딱 좋아",
          "speaker_id": "2",
          "time": "20210829 01:13"
        },
        {
          "id": "MDRW2100035052.1.10",
          "form": "에버랜드가면 무얼 제일 많이 하니",
          "original_form": "에버랜드가면 무얼 제일 많이 하니",
          "speaker_id": "1",
          "time": "20210829 01:13"
        },
        {
          "id": "MDRW2100035052.1.11",
          "form": "나는 사파리를 꼭 가",
          "original_form": "나는 사파리를 꼭 가",
          "speaker_id": "2",
          "time": "20210829 01:13"
        },
        {
          "id": "MDRW2100035052.1.12",
          "form": "사파리에는 사자가 있어",
          "original_form": "사파리에는 사자가 있어",
          "speaker_id": "1",
          "time": "20210829 01:14"
        },
        {
          "id": "MDRW2100035052.1.13",
          "form": "곰도 볼 수 있어",
          "original_form": "곰도 볼 수 있어",
          "speaker_id": "2",
          "time": "20210829 01:14"
        },
        {
