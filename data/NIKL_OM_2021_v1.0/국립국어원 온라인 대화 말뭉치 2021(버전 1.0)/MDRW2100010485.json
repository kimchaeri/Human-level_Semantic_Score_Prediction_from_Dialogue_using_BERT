        {
          "id": "MDRW2100010485.1.1",
          "form": "다이어트 2일차",
          "original_form": "다이어트 2일차",
          "speaker_id": "1",
          "time": "20210824 20:01"
        },
        {
          "id": "MDRW2100010485.1.2",
          "form": "배가고픔",
          "original_form": "배가고픔",
          "speaker_id": "1",
          "time": "20210824 20:01"
        },
        {
          "id": "MDRW2100010485.1.3",
          "form": "그래봤자 저녁 안먹는거지만",
          "original_form": "그래봤자 저녁 안먹는거지만",
          "speaker_id": "1",
          "time": "20210824 20:01"
        },
        {
          "id": "MDRW2100010485.1.4",
          "form": "아예안먹어여?",
          "original_form": "아예안먹어여?",
          "speaker_id": "2",
          "time": "20210824 20:14"
        },
        {
          "id": "MDRW2100010485.1.5",
          "form": "아예..",
          "original_form": "아예..",
          "speaker_id": "1",
          "time": "20210824 20:14"
        },
        {
          "id": "MDRW2100010485.1.6",
          "form": "이런",
          "original_form": "이런",
          "speaker_id": "2",
          "time": "20210824 20:14"
        },
        {
          "id": "MDRW2100010485.1.7",
          "form": "배고파",
          "original_form": "배고파",
          "speaker_id": "2",
          "time": "20210824 20:14"
        },
        {
          "id": "MDRW2100010485.1.8",
          "form": "맥주랑",
          "original_form": "맥주랑",
          "speaker_id": "1",
          "time": "20210824 20:14"
        },
        {
          "id": "MDRW2100010485.1.9",
          "form": "문어숙회",
          "original_form": "문어숙회",
          "speaker_id": "1",
          "time": "20210824 20:14"
        },
        {
          "id": "MDRW2100010485.1.10",
          "form": "돼지고기를 즐기는중 남편만",
          "original_form": "돼지고기를 즐기는중 남편만",
          "speaker_id": "1",
          "time": "20210824 20:15"
        },
        {
          "id": "MDRW2100010485.1.11",
          "form": "ㅋㅋ잔인해",
          "original_form": "ㅋㅋ잔인해",
          "speaker_id": "2",
          "time": "20210824 20:15"
        },
        {
          "id": "MDRW2100010485.1.12",
          "form": "나는 침만 꼴깍",
          "original_form": "나는 침만 꼴깍",
          "speaker_id": "1",
          "time": "20210824 20:15"
        },
        {
          "id": "MDRW2100010485.1.13",
          "form": "그래도 어제보단 몸이 조금 가벼움",
          "original_form": "그래도 어제보단 몸이 조금 가벼움",
          "speaker_id": "1",
          "time": "20210824 20:15"
        },
        {
          "id": "MDRW2100010485.1.14",
          "form": "힘든데 굶는건",
          "original_form": "힘든데 굶는건",
          "speaker_id": "2",
          "time": "20210824 20:16"
        },
        {
          "id": "MDRW2100010485.1.15",
          "form": "일단 위를 좀 비워야할듯",
          "original_form": "일단 위를 좀 비워야할듯",
          "speaker_id": "1",
          "time": "20210824 20:16"
        },
        {
          "id": "MDRW2100010485.1.16",
          "form": "자다깨서도 먹어",
          "original_form": "자다깨서도 먹어",
          "speaker_id": "1",
          "time": "20210824 20:16"
        },
        {
          "id": "MDRW2100010485.1.17",
          "form": "리프레시",
          "original_form": "리프레시",
          "speaker_id": "2",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100010485.1.18",
          "form": "밀가루를 너무먹어서 안되겠어",
          "original_form": "밀가루를 너무먹어서 안되겠어",
          "speaker_id": "1",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100010485.1.19",
          "form": "피부도 엉망이야",
          "original_form": "피부도 엉망이야",
          "speaker_id": "1",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100010485.1.20",
          "form": "맞아 저도",
          "original_form": "맞아 저도",
          "speaker_id": "2",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100010485.1.21",
          "form": "밀가루",
          "original_form": "밀가루",
          "speaker_id": "2",
          "time": "20210824 20:17"
        },
        {
          "id": "MDRW2100010485.1.22",
          "form": "밀가루가 날 망하게 함",
          "original_form": "밀가루가 날 망하게 함",
          "speaker_id": "1",
          "time": "20210824 20:18"
        },
        {
          "id": "MDRW2100010485.1.23",
          "form": "근데 사랑이지",
          "original_form": "근데 사랑이지",
          "speaker_id": "2",
          "time": "20210824 20:18"
        },
        {
          "id": "MDRW2100010485.1.24",
          "form": "맞아 내피부는 죽어가지만..",
          "original_form": "맞아 내피부는 죽어가지만..",
          "speaker_id": "1",
          "time": "20210824 20:18"
        },
        {
          "id": "MDRW2100010485.1.25",
          "form": "피부랑 바꿈",
          "original_form": "피부랑 바꿈",
          "speaker_id": "2",
          "time": "20210824 20:19"
        },
        {
          "id": "MDRW2100010485.1.26",
          "form": "운동은 차마 못하겠고",
          "original_form": "운동은 차마 못하겠고",
          "speaker_id": "1",
          "time": "20210824 20:19"
        },
        {
          "id": "MDRW2100010485.1.27",
          "form": "일단 일주일만 굶어봄",
          "original_form": "일단 일주일만 굶어봄",
          "speaker_id": "1",
          "time": "20210824 20:19"
        },
        {
          "id": "MDRW2100010485.1.28",
          "form": "위를 비우겟다",
          "original_form": "위를 비우겟다",
          "speaker_id": "2",
          "time": "20210824 20:19"
        },
        {
          "id": "MDRW2100010485.1.29",
          "form": "비우고 다시 채우겠지",
          "original_form": "비우고 다시 채우겠지",
          "speaker_id": "1",
          "time": "20210824 20:20"
        },
        {
          "id": "MDRW2100010485.1.30",
          "form": "ㅋㅋ비우는게 어디여",
          "original_form": "ㅋㅋ비우는게 어디여",
          "speaker_id": "2",
          "time": "20210824 21:12"
        },
        {
          "id": "MDRW2100010485.1.31",
          "form": "나야 ㅋㅋ",
          "original_form": "나야 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:57"
        }
      ]
    }
