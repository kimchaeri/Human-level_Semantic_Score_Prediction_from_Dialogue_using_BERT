      "utterance": [
        {
          "id": "MDRW2100042350.1.1",
          "form": "응 생선 들어가고 찜은 아냐",
          "original_form": "응 생선 들어가고 찜은 아냐",
          "speaker_id": "2",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042350.1.2",
          "form": "갈치조림이구나!!!",
          "original_form": "갈치조림이구나!!!",
          "speaker_id": "1",
          "time": "20210906 21:45"
        },
        {
          "id": "MDRW2100042350.1.3",
          "form": "아 비슷해!!!! 갈치는 아냐 갈치면 행복했을텐데!!!",
          "original_form": "아 비슷해!!!! 갈치는 아냐 갈치면 행복했을텐데!!!",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042350.1.4",
          "form": "고등어조림 이니",
          "original_form": "고등어조림 이니",
          "speaker_id": "1",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042350.1.5",
          "form": "정답정답! 짝짝짝",
          "original_form": "정답정답! 짝짝짝",
          "speaker_id": "2",
          "time": "20210906 21:47"
        },
        {
          "id": "MDRW2100042350.1.6",
          "form": "고등어조림이었습니다",
          "original_form": "고등어조림이었습니다",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042350.1.7",
          "form": "이번에 생각보다 더 빨리 맞췄네",
          "original_form": "이번에 생각보다 더 빨리 맞췄네",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042350.1.8",
          "form": "똑똑친구",
          "original_form": "똑똑친구",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042350.1.9",
          "form": "나 천재맞나봐",
          "original_form": "나 천재맞나봐",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042350.1.10",
          "form": "먹천재",
          "original_form": "먹천재",
          "speaker_id": "1",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042350.1.11",
          "form": "그럼그럼",
          "original_form": "그럼그럼",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042350.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 21:48"
        },
        {
          "id": "MDRW2100042350.1.13",
          "form": "이번엔 네 저녁메뉴를 내가 맞춰볼게",
          "original_form": "이번엔 네 저녁메뉴를 내가 맞춰볼게",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042350.1.14",
          "form": "어때?",
          "original_form": "어때?",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042350.1.15",
          "form": "좋아!!",
          "original_form": "좋아!!",
          "speaker_id": "1",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042350.1.16",
          "form": "쌀이 들어가니?",
          "original_form": "쌀이 들어가니?",
          "speaker_id": "2",
          "time": "20210906 21:49"
        },
        {
          "id": "MDRW2100042350.1.17",
          "form": "밥이랑 곁드려 먹는 독립된 음식이야",
          "original_form": "밥이랑 곁드려 먹는 독립된 음식이야",
          "speaker_id": "1",
          "time": "20210906 21:50"
        },
        {
          "id": "MDRW2100042350.1.18",
          "form": "곁들여..",
          "original_form": "곁들여..",
          "speaker_id": "1",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042350.1.19",
          "form": "으으으으으으으음",
          "original_form": "으으으으으으으음",
          "speaker_id": "2",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042350.1.20",
          "form": "반찬류니?",
          "original_form": "반찬류니?",
          "speaker_id": "2",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042350.1.21",
          "form": "메인요리에 가까워",
          "original_form": "메인요리에 가까워",
          "speaker_id": "1",
          "time": "20210906 21:51"
        },
        {
          "id": "MDRW2100042350.1.22",
          "form": "매운 음식이야?",
          "original_form": "매운 음식이야?",
          "speaker_id": "2",
          "time": "20210906 21:52"
        },
        {
