      "utterance": [
        {
          "id": "MDRW2100032015.1.1",
          "form": "아뇨 그렇진 않은데",
          "original_form": "아뇨 그렇진 않은데",
          "speaker_id": "2",
          "time": "20210905 10:57"
        },
        {
          "id": "MDRW2100032015.1.2",
          "form": "한글을 잘 안써요",
          "original_form": "한글을 잘 안써요",
          "speaker_id": "2",
          "time": "20210905 10:57"
        },
        {
          "id": "MDRW2100032015.1.3",
          "form": "공무원들이 한글 쓰던데",
          "original_form": "공무원들이 한글 쓰던데",
          "speaker_id": "2",
          "time": "20210905 10:57"
        },
        {
          "id": "MDRW2100032015.1.4",
          "form": "저흰 보통 공사비나",
          "original_form": "저흰 보통 공사비나",
          "speaker_id": "2",
          "time": "20210905 10:58"
        },
        {
          "id": "MDRW2100032015.1.5",
          "form": "예산 잡고 하는거땜에 엑셀을 많이쓰고",
          "original_form": "예산 잡고 하는거땜에 엑셀을 많이쓰고",
          "speaker_id": "2",
          "time": "20210905 10:58"
        },
        {
          "id": "MDRW2100032015.1.6",
          "form": "아하 안그래도 애들 한글로 과제 내주면 다들 파일 못열고 그래서 ㅋㅋㅋ",
          "original_form": "아하 안그래도 애들 한글로 과제 내주면 다들 파일 못열고 그래서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 10:59"
        },
        {
          "id": "MDRW2100032015.1.7",
          "form": "이제 한글이 필수인 시대가 아니구나 하면서 구글문서로 편집해서 주고 그래요..",
          "original_form": "이제 한글이 필수인 시대가 아니구나 하면서 구글문서로 편집해서 주고 그래요..",
          "speaker_id": "1",
          "time": "20210905 10:59"
        },
        {
          "id": "MDRW2100032015.1.8",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210905 11:04"
        },
        {
          "id": "MDRW2100032015.1.9",
          "form": "한글 별로 안좋아하는데",
          "original_form": "한글 별로 안좋아하는데",
          "speaker_id": "2",
          "time": "20210905 11:04"
        },
        {
          "id": "MDRW2100032015.1.10",
          "form": "집에 한글 없어서 못여는거죠?ㅋㅋㅋ",
          "original_form": "집에 한글 없어서 못여는거죠?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:05"
        },
        {
          "id": "MDRW2100032015.1.11",
          "form": "네ㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:05"
        },
        {
          "id": "MDRW2100032015.1.12",
          "form": "제컴에도 한글이 없답니다?ㅋㅋㅋㅋㅋ",
          "original_form": "제컴에도 한글이 없답니다?ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:05"
        },
        {
          "id": "MDRW2100032015.1.13",
          "form": "뷰어로 겨우 열고 편집은 못하고 ㅋㅋㅋ",
          "original_form": "뷰어로 겨우 열고 편집은 못하고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:05"
        },
        {
          "id": "MDRW2100032015.1.14",
          "form": "그렇겠네요ㅋㅋㅋㅋㅋ",
          "original_form": "그렇겠네요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:05"
        },
        {
          "id": "MDRW2100032015.1.15",
          "form": "집에서 컴을 켜고싶지가 않아요",
          "original_form": "집에서 컴을 켜고싶지가 않아요",
          "speaker_id": "2",
          "time": "20210905 11:06"
        },
        {
          "id": "MDRW2100032015.1.16",
          "form": "저도 혹시 모르니 서서히 한글을 버리고 있어요ㅋㅋㅋㅋㅋ 공문은 한글로 오지만..",
          "original_form": "저도 혹시 모르니 서서히 한글을 버리고 있어요ㅋㅋㅋㅋㅋ 공문은 한글로 오지만..",
          "speaker_id": "1",
          "time": "20210905 11:06"
        },
        {
          "id": "MDRW2100032015.1.17",
          "form": "윈도우도 버린다는",
          "original_form": "윈도우도 버린다는",
          "speaker_id": "2",
          "time": "20210905 11:06"
        },
        {
          "id": "MDRW2100032015.1.18",
          "form": "재택 안하나요",
          "original_form": "재택 안하나요",
          "speaker_id": "1",
          "time": "20210905 11:06"
        },
        {
          "id": "MDRW2100032015.1.19",
          "form": "흉흉한소문 돌던데",
          "original_form": "흉흉한소문 돌던데",
          "speaker_id": "2",
          "time": "20210905 11:07"
        },
        {
          "id": "MDRW2100032015.1.20",
          "form": "재택하죠 ㅋㅋㅋ",
          "original_form": "재택하죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:07"
        },
        {
          "id": "MDRW2100032015.1.21",
          "form": "아 진짜요 윈도우도 버리나요",
          "original_form": "아 진짜요 윈도우도 버리나요",
          "speaker_id": "1",
          "time": "20210905 11:07"
        },
        {
          "id": "MDRW2100032015.1.22",
          "form": "그럼 뭘...?",
          "original_form": "그럼 뭘...?",
          "speaker_id": "1",
          "time": "20210905 11:07"
        },
        {
          "id": "MDRW2100032015.1.23",
          "form": "재택하면 컴 켜야되자나요",
          "original_form": "재택하면 컴 켜야되자나요",
          "speaker_id": "1",
          "time": "20210905 11:07"
        },
        {
          "id": "MDRW2100032015.1.24",
          "form": "나라에서 운영체제를 만들겠대요",
          "original_form": "나라에서 운영체제를 만들겠대요",
          "speaker_id": "2",
          "time": "20210905 11:07"
        },
        {
          "id": "MDRW2100032015.1.25",
          "form": "그땐 켜는데",
          "original_form": "그땐 켜는데",
          "speaker_id": "2",
          "time": "20210905 11:07"
        },
        {
          "id": "MDRW2100032015.1.26",
          "form": "앜ㅋㅋ북한이야 뭐야",
          "original_form": "앜ㅋㅋ북한이야 뭐야",
          "speaker_id": "1",
          "time": "20210905 11:07"
        },
        {
          "id": "MDRW2100032015.1.27",
          "form": "나도 그생각했는데",
          "original_form": "나도 그생각했는데",
          "speaker_id": "2",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100032015.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100032015.1.29",
          "form": "안그래도 학교에서 뭐 프로젝트 진행하는게 잇어서",
          "original_form": "안그래도 학교에서 뭐 프로젝트 진행하는게 잇어서",
          "speaker_id": "1",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100032015.1.30",
          "form": "그럼 우린 일밖에 못해요",
          "original_form": "그럼 우린 일밖에 못해요",
          "speaker_id": "2",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100032015.1.31",
          "form": "저 구글 사이트로 프로젝트 정리중이었는데",
          "original_form": "저 구글 사이트로 프로젝트 정리중이었는데",
          "speaker_id": "1",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100032015.1.32",
          "form": "교무부장님이 학교 일이니까 학교 서버에 정리하라고",
          "original_form": "교무부장님이 학교 일이니까 학교 서버에 정리하라고",
          "speaker_id": "1",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100032015.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100032015.1.34",
          "form": "구글에 애들 과제 내주고 있는것도 맘에 안 들어하시고 ㅋㅋㅋ",
          "original_form": "구글에 애들 과제 내주고 있는것도 맘에 안 들어하시고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100032015.1.35",
          "form": "오피스도 안쓰는추세 아니에요?",
          "original_form": "오피스도 안쓰는추세 아니에요?",
          "speaker_id": "2",
          "time": "20210905 11:08"
        },
        {
          "id": "MDRW2100032015.1.36",
          "form": "여친은 한셀 쓴다던데",
          "original_form": "여친은 한셀 쓴다던데",
          "speaker_id": "2",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100032015.1.37",
          "form": "앜ㅋㅋㅋ 저흰 아직 파포는 쓰는데",
          "original_form": "앜ㅋㅋㅋ 저흰 아직 파포는 쓰는데",
          "speaker_id": "1",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100032015.1.38",
          "form": "마자요 한글 프로그램 거의 다 잇어요",
          "original_form": "마자요 한글 프로그램 거의 다 잇어요",
          "speaker_id": "1",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100032015.1.39",
          "form": "이정도면",
          "original_form": "이정도면",
          "speaker_id": "2",
          "time": "20210905 11:09"
        },
        {
          "id": "MDRW2100032015.1.40",
          "form": "근데 전 걍 전부 구글에서 하고 있어요...재택 근무 때 그냥 온라인으러 작업하는게 간편해서",
          "original_form": "근데 전 걍 전부 구글에서 하고 있어요...재택 근무 때 그냥 온라인으러 작업하는게 간편해서",
          "speaker_id": "1",
          "time": "20210905 11:10"
        },
        {
          "id": "MDRW2100032015.1.41",
          "form": "정부랑 한글과소프트 깊은 커넥션",
          "original_form": "정부랑 한글과소프트 깊은 커넥션",
          "speaker_id": "2",
          "time": "20210905 11:10"
        },
        {
          "id": "MDRW2100032015.1.42",
          "form": "의심",
          "original_form": "의심",
          "speaker_id": "2",
          "time": "20210905 11:10"
        },
        {
          "id": "MDRW2100032015.1.43",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:10"
        },
        {
          "id": "MDRW2100032015.1.44",
          "form": "누가봐도 마소 버리기하는건데",
          "original_form": "누가봐도 마소 버리기하는건데",
          "speaker_id": "2",
          "time": "20210905 11:10"
        },
        {
          "id": "MDRW2100032015.1.45",
          "form": "윈도우는 없어질일 없겟지하고 마소 주식 몰빵해놨는데",
          "original_form": "윈도우는 없어질일 없겟지하고 마소 주식 몰빵해놨는데",
          "speaker_id": "1",
          "time": "20210905 11:10"
        },
        {
          "id": "MDRW2100032015.1.46",
          "form": "버리지말지...^^....",
          "original_form": "버리지말지...^^....",
          "speaker_id": "1",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100032015.1.47",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100032015.1.48",
          "form": "마소 나쁜넘들",
          "original_form": "마소 나쁜넘들",
          "speaker_id": "2",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100032015.1.49",
          "form": "윈10이 마지막이라더니",
          "original_form": "윈10이 마지막이라더니",
          "speaker_id": "2",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100032015.1.50",
          "form": "윈11만드는",
          "original_form": "윈11만드는",
          "speaker_id": "2",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100032015.1.51",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100032015.1.52",
          "form": "아무것도 없엇던 양",
          "original_form": "아무것도 없엇던 양",
          "speaker_id": "1",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100032015.1.53",
          "form": "되게 혁신적인 느낌으로",
          "original_form": "되게 혁신적인 느낌으로",
          "speaker_id": "1",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100032015.1.54",
          "form": "참놔",
          "original_form": "참놔",
          "speaker_id": "2",
          "time": "20210905 11:11"
        },
        {
          "id": "MDRW2100032015.1.55",
          "form": "뒤통수 제대로치고",
          "original_form": "뒤통수 제대로치고",
          "speaker_id": "2",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100032015.1.56",
          "form": "윈도우만 팔아먹고",
          "original_form": "윈도우만 팔아먹고",
          "speaker_id": "2",
          "time": "20210905 11:12"
        },
        {
          "id": "MDRW2100032015.1.57",
          "form": "그래도 덕분에 주식 많이 올라서 즐겁단말이에요",
          "original_form": "그래도 덕분에 주식 많이 올라서 즐겁단말이에요",
