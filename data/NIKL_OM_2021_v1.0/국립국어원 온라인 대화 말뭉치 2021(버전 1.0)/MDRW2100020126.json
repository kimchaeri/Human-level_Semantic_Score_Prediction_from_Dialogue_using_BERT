      "utterance": [
        {
          "id": "MDRW2100020126.1.1",
          "form": "콩국수드세요?",
          "original_form": "콩국수드세요?",
          "speaker_id": "1",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020126.1.2",
          "form": "먹긴 먹는데 자주먹진않아요",
          "original_form": "먹긴 먹는데 자주먹진않아요",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020126.1.3",
          "form": "콩국수 소금 설탕",
          "original_form": "콩국수 소금 설탕",
          "speaker_id": "2",
          "time": "20210827 23:48"
        },
        {
          "id": "MDRW2100020126.1.4",
          "form": "이건 둘 다 넣습니다",
          "original_form": "이건 둘 다 넣습니다",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020126.1.5",
          "form": "그래야 단짠단짠",
          "original_form": "그래야 단짠단짠",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020126.1.6",
          "form": "완벽해지거든요",
          "original_form": "완벽해지거든요",
          "speaker_id": "1",
          "time": "20210827 23:49"
        },
        {
          "id": "MDRW2100020126.1.7",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020126.1.8",
          "form": "단짠단짠",
          "original_form": "단짠단짠",
          "speaker_id": "2",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020126.1.9",
          "form": "정말 생각도못햇다",
          "original_form": "정말 생각도못햇다",
          "speaker_id": "2",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020126.1.10",
          "form": "입틀막",
          "original_form": "입틀막",
          "speaker_id": "2",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020126.1.11",
          "form": "앞으로 기억하세요",
          "original_form": "앞으로 기억하세요",
          "speaker_id": "1",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020126.1.12",
          "form": "설탕 2에 소금 1비율로",
          "original_form": "설탕 2에 소금 1비율로",
          "speaker_id": "1",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020126.1.13",
          "form": "넣어야 완벽합니다",
          "original_form": "넣어야 완벽합니다",
          "speaker_id": "1",
          "time": "20210827 23:50"
        },
        {
          "id": "MDRW2100020126.1.14",
          "form": "그럼 팝콘은 어때요 팝콘은 좋아하시나요",
          "original_form": "그럼 팝콘은 어때요 팝콘은 좋아하시나요",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020126.1.15",
          "form": "어디 오늘 음식 끝까지 한번 가보자고",
          "original_form": "어디 오늘 음식 끝까지 한번 가보자고",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020126.1.16",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020126.1.17",
          "form": "2:1 메모.....",
          "original_form": "2:1 메모.....",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020126.1.18",
          "form": "#가보자고",
          "original_form": "#가보자고",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020126.1.19",
          "form": "팝콘 그 어니언? 갈릭?",
          "original_form": "팝콘 그 어니언? 갈릭?",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020126.1.20",
          "form": "팝콘어때요",
          "original_form": "팝콘어때요",
          "speaker_id": "1",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020126.1.21",
          "form": "그거좋아합니다",
          "original_form": "그거좋아합니다",
          "speaker_id": "2",
          "time": "20210827 23:51"
        },
        {
          "id": "MDRW2100020126.1.22",
          "form": "갈릭은 참을 수 없죠",
          "original_form": "갈릭은 참을 수 없죠",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020126.1.23",
          "form": "약간 캬라멜이랑은 다른 감칠맛",
          "original_form": "약간 캬라멜이랑은 다른 감칠맛",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020126.1.24",
          "form": "맞아요 손에 암청묻긴하지만 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 손에 암청묻긴하지만 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020126.1.25",
          "form": "파스타는 크림 토마토",
          "original_form": "파스타는 크림 토마토",
          "speaker_id": "2",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020126.1.26",
          "form": "아 어려운데",
          "original_form": "아 어려운데",
          "speaker_id": "1",
          "time": "20210827 23:52"
        },
        {
          "id": "MDRW2100020126.1.27",
          "form": "토마토",
          "original_form": "토마토",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020126.1.28",
          "form": "어떤거 좋아하세요",
          "original_form": "어떤거 좋아하세요",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020126.1.29",
          "form": "저도 토마토요",
          "original_form": "저도 토마토요",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020126.1.30",
          "form": "크림은 세입까진 맛잇는데",
          "original_form": "크림은 세입까진 맛잇는데",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020126.1.31",
          "form": "느끼해서 잘 못먹겟더라구요",
          "original_form": "느끼해서 잘 못먹겟더라구요",
          "speaker_id": "2",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020126.1.32",
          "form": "느끼해",
          "original_form": "느끼해",
          "speaker_id": "1",
          "time": "20210827 23:53"
        },
        {
          "id": "MDRW2100020126.1.33",
          "form": "물려 그래서 크림은 부담스러워요",
          "original_form": "물려 그래서 크림은 부담스러워요",
          "speaker_id": "1",
          "time": "20210827 23:53"
        }
      ]
    }
