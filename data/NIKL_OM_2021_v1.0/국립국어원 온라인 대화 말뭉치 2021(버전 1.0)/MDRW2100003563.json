      "utterance": [
        {
          "id": "MDRW2100003563.1.1",
          "form": "싸이월드는 그런데 왜 부활을 할까요?",
          "original_form": "싸이월드는 그런데 왜 부활을 할까요?",
          "speaker_id": "2",
          "time": "20210821 01:33"
        },
        {
          "id": "MDRW2100003563.1.2",
          "form": "추억이죠 ㅎㅎㄹ",
          "original_form": "추억이죠 ㅎㅎㄹ",
          "speaker_id": "1",
          "time": "20210821 01:33"
        },
        {
          "id": "MDRW2100003563.1.3",
          "form": "전에 서비스종료라고 해놓고ㅎ 음.. 반응이 뜨겁긴할것같은데ㅎㅎ",
          "original_form": "전에 서비스종료라고 해놓고ㅎ 음.. 반응이 뜨겁긴할것같은데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 01:34"
        },
        {
          "id": "MDRW2100003563.1.4",
          "form": "앗 그때 싸이없어졌을때",
          "original_form": "앗 그때 싸이없어졌을때",
          "speaker_id": "1",
          "time": "20210821 01:34"
        },
        {
          "id": "MDRW2100003563.1.5",
          "form": "무슨이유인지 모르겟지만ㅎ 저희한태는 좋은거니깐",
          "original_form": "무슨이유인지 모르겟지만ㅎ 저희한태는 좋은거니깐",
          "speaker_id": "2",
          "time": "20210821 01:34"
        },
        {
          "id": "MDRW2100003563.1.6",
          "form": "아기 엄마들이 제일 힘들어 했데요",
          "original_form": "아기 엄마들이 제일 힘들어 했데요",
          "speaker_id": "1",
          "time": "20210821 01:34"
        },
        {
          "id": "MDRW2100003563.1.7",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210821 01:34"
        },
        {
          "id": "MDRW2100003563.1.8",
          "form": "엄마들 육아 일기장으로 많이 썻다 하더라구요",
          "original_form": "엄마들 육아 일기장으로 많이 썻다 하더라구요",
          "speaker_id": "1",
          "time": "20210821 01:34"
        },
        {
          "id": "MDRW2100003563.1.9",
          "form": "아ㅎㅎㅎㅎ",
          "original_form": "아ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 01:34"
        },
        {
          "id": "MDRW2100003563.1.10",
          "form": "진짜 많은 추억이 있었네요ㅎ",
          "original_form": "진짜 많은 추억이 있었네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 01:35"
        },
        {
          "id": "MDRW2100003563.1.11",
          "form": "그러게여  근데 갑자기 폐쇄 됐을까요",
          "original_form": "그러게여  근데 갑자기 폐쇄 됐을까요",
          "speaker_id": "1",
          "time": "20210821 01:35"
        },
        {
          "id": "MDRW2100003563.1.12",
          "form": "기억이 안나는데",
          "original_form": "기억이 안나는데",
          "speaker_id": "1",
          "time": "20210821 01:35"
        },
        {
          "id": "MDRW2100003563.1.13",
          "form": "이번에 다기 불활하면 반짝하지말고 꾸준히 인기있으면 좋겟네요ㅎ",
          "original_form": "이번에 다기 불활하면 반짝하지말고 꾸준히 인기있으면 좋겟네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 01:35"
        },
        {
          "id": "MDRW2100003563.1.14",
          "form": "아마 유지비용때문이지 않을까요? 사용하는사람은 없지",
          "original_form": "아마 유지비용때문이지 않을까요? 사용하는사람은 없지",
          "speaker_id": "2",
          "time": "20210821 01:36"
        },
        {
          "id": "MDRW2100003563.1.15",
          "form": "음 자금난이네요",
          "original_form": "음 자금난이네요",
          "speaker_id": "1",
          "time": "20210821 01:37"
        },
        {
          "id": "MDRW2100003563.1.16",
          "form": "근데 또 열고 유지안되면 ㅜㅜ",
          "original_form": "근데 또 열고 유지안되면 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 01:37"
        },
        {
          "id": "MDRW2100003563.1.17",
          "form": "그것도 걱정",
          "original_form": "그것도 걱정",
          "speaker_id": "1",
          "time": "20210821 01:37"
        },
        {
          "id": "MDRW2100003563.1.18",
          "form": "그니깐요ㅎ 어짜피 유지못할꺼라면ㅎ 안하는게좋은데",
          "original_form": "그니깐요ㅎ 어짜피 유지못할꺼라면ㅎ 안하는게좋은데",
          "speaker_id": "2",
          "time": "20210821 01:38"
        },
        {
          "id": "MDRW2100003563.1.19",
          "form": "먼가 추억이 한순간애 사라지는것도 슬프긴 하더라구요",
          "original_form": "먼가 추억이 한순간애 사라지는것도 슬프긴 하더라구요",
          "speaker_id": "1",
