      "utterance": [
        {
          "id": "MDRW2100044758.1.1",
          "form": "일본 많이 가보셨어요?",
          "original_form": "일본 많이 가보셨어요?",
          "speaker_id": "1",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044758.1.2",
          "form": "두세번 가봤어요",
          "original_form": "두세번 가봤어요",
          "speaker_id": "2",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044758.1.3",
          "form": "일본이 만만하긴하죠",
          "original_form": "일본이 만만하긴하죠",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044758.1.4",
          "form": "해외여행 기분을 만끽하면서 큰돈과 시간 안들고",
          "original_form": "해외여행 기분을 만끽하면서 큰돈과 시간 안들고",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044758.1.5",
          "form": "저도 서너번 갔습니다.",
          "original_form": "저도 서너번 갔습니다.",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044758.1.6",
          "form": "어디어디 가셨나요?",
          "original_form": "어디어디 가셨나요?",
          "speaker_id": "2",
          "time": "20210907 23:48"
        },
        {
          "id": "MDRW2100044758.1.7",
          "form": "도쿄랑 오사카랑 훗카이도요",
          "original_form": "도쿄랑 오사카랑 훗카이도요",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044758.1.8",
          "form": "여러군데 다녀오셨네요",
          "original_form": "여러군데 다녀오셨네요",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044758.1.9",
          "form": "훗카이도는 어때요?",
          "original_form": "훗카이도는 어때요?",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044758.1.10",
          "form": "여름에 짧게 갔는데 쾌적했어요",
          "original_form": "여름에 짧게 갔는데 쾌적했어요",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044758.1.11",
          "form": "안덥고 조경도 잘해놔서 돌아다니기 좋았달까",
          "original_form": "안덥고 조경도 잘해놔서 돌아다니기 좋았달까",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044758.1.12",
          "form": "와 여름이면 라벤더 보셨겠네요",
          "original_form": "와 여름이면 라벤더 보셨겠네요",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044758.1.13",
          "form": "네, 봤습니다",
          "original_form": "네, 봤습니다",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044758.1.14",
          "form": "근데 좀 끝물이어서 그랬나 애들이 쬐금 시들했어요",
          "original_form": "근데 좀 끝물이어서 그랬나 애들이 쬐금 시들했어요",
          "speaker_id": "1",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044758.1.15",
          "form": "앗 아쉬워라",
          "original_form": "앗 아쉬워라",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044758.1.16",
          "form": "도쿄에선 어디 가셨나요?",
          "original_form": "도쿄에선 어디 가셨나요?",
          "speaker_id": "2",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044758.1.17",
          "form": "디즈니랜드요 ㅋㅋㅋㅋ",
          "original_form": "디즈니랜드요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044758.1.18",
          "form": "와 저 거기 가려다 말았는데",
          "original_form": "와 저 거기 가려다 말았는데",
          "speaker_id": "2",
          "time": "20210908 00:10"
        },
        {
          "id": "MDRW2100044758.1.19",
          "form": "나쁘진 않았는데 놀이기구의 재미짐은 오사카 유니버셜스튜디오가 더 좋았습니다",
          "original_form": "나쁘진 않았는데 놀이기구의 재미짐은 오사카 유니버셜스튜디오가 더 좋았습니다",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044758.1.20",
          "form": "ㅠㅠ 이렇게 될 줄 알았으면 갈 걸 하고 생각하는대ㅠㅠ",
          "original_form": "ㅠㅠ 이렇게 될 줄 알았으면 갈 걸 하고 생각하는대ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044758.1.21",
          "form": "지금 당장은 몰라도 몇년뒤엔 갈수 있을거에요",
          "original_form": "지금 당장은 몰라도 몇년뒤엔 갈수 있을거에요",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044758.1.22",
          "form": "아 확실히 디즈니는 애들도 다 탈 수 있는 수준이라고 하더라구요",
          "original_form": "아 확실히 디즈니는 애들도 다 탈 수 있는 수준이라고 하더라구요",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044758.1.23",
          "form": "네, 확실히 디즈니는 구경하는 재미지 놀이기구 재미는 그다지...",
          "original_form": "네, 확실히 디즈니는 구경하는 재미지 놀이기구 재미는 그다지...",
          "speaker_id": "1",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044758.1.24",
          "form": "놀이기구가 신나야한다는 쪽이면 유니버셜입니다",
          "original_form": "놀이기구가 신나야한다는 쪽이면 유니버셜입니다",
          "speaker_id": "1",
          "time": "20210908 00:13"
        }
      ]
    }
  ]
