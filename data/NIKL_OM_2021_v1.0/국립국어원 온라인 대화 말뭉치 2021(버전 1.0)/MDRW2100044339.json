      "utterance": [
        {
          "id": "MDRW2100044339.1.1",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044339.1.2",
          "form": "그렇겠다\r\n나만해도 영상 보는거 디기 안봤는데 어느새 유튜브 보고",
          "original_form": "그렇겠다\r\n나만해도 영상 보는거 디기 안봤는데 어느새 유튜브 보고",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044339.1.3",
          "form": "있으니ㅎ",
          "original_form": "있으니ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044339.1.4",
          "form": "ㅇㅇ 나도 이제 긴 영상 잘 못 보겠더라",
          "original_form": "ㅇㅇ 나도 이제 긴 영상 잘 못 보겠더라",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044339.1.5",
          "form": "드라마도 막 끊어서 봄",
          "original_form": "드라마도 막 끊어서 봄",
          "speaker_id": "1",
          "time": "20210907 22:22"
        },
        {
          "id": "MDRW2100044339.1.6",
          "form": "ㅜㅠ 예전엔 맥 끊김 이어서 잘 안봐지고 그랬는데 변하긴 변했다",
          "original_form": "ㅜㅠ 예전엔 맥 끊김 이어서 잘 안봐지고 그랬는데 변하긴 변했다",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044339.1.7",
          "form": "짧은게 좋아",
          "original_form": "짧은게 좋아",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044339.1.8",
          "form": "편해",
          "original_form": "편해",
          "speaker_id": "2",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044339.1.9",
          "form": "ㅇㅇ마져마져",
          "original_form": "ㅇㅇ마져마져",
          "speaker_id": "1",
          "time": "20210907 22:25"
        },
        {
          "id": "MDRW2100044339.1.10",
          "form": "나 요즘 대만드라마보는데",
          "original_form": "나 요즘 대만드라마보는데",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044339.1.11",
          "form": "출판사 편집하는사람이 로맨스 소설속에들어감",
          "original_form": "출판사 편집하는사람이 로맨스 소설속에들어감",
          "speaker_id": "1",
          "time": "20210907 22:26"
        },
        {
          "id": "MDRW2100044339.1.12",
          "form": "오 소설 잘 아니까 뭔가 색다르려나ㅎ",
          "original_form": "오 소설 잘 아니까 뭔가 색다르려나ㅎ",
          "speaker_id": "2",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044339.1.13",
          "form": "자기가 거기서 여주 되려고 온갖 짓 다하는데. 다 뻘짓이라 웃기더라",
          "original_form": "자기가 거기서 여주 되려고 온갖 짓 다하는데. 다 뻘짓이라 웃기더라",
          "speaker_id": "1",
          "time": "20210907 22:30"
        },
        {
          "id": "MDRW2100044339.1.14",
          "form": "소설 속이라 막나감 ㅋㅋ",
          "original_form": "소설 속이라 막나감 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044339.1.15",
          "form": "ㅋㅋㅋㅋ 재밌겠너",
          "original_form": "ㅋㅋㅋㅋ 재밌겠너",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044339.1.16",
          "form": "ㅇㅇ아직까진 볼만해 ㅎㅎ",
          "original_form": "ㅇㅇ아직까진 볼만해 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044339.1.17",
          "form": "맞다 소설이라 생각하고 막 나갈수있는거 좋다ㅋ",
          "original_form": "맞다 소설이라 생각하고 막 나갈수있는거 좋다ㅋ",
          "speaker_id": "2",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044339.1.18",
          "form": "ㅇㅇ그런거 재밌어",
          "original_form": "ㅇㅇ그런거 재밌어",
          "speaker_id": "1",
          "time": "20210907 22:31"
        },
        {
          "id": "MDRW2100044339.1.19",
          "form": "재밌는 영화 뭐 있을까",
          "original_form": "재밌는 영화 뭐 있을까",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044339.1.20",
          "form": "또 볼거를 막 찾아다닌다",
          "original_form": "또 볼거를 막 찾아다닌다",
          "speaker_id": "1",
          "time": "20210907 22:32"
        },
        {
          "id": "MDRW2100044339.1.21",
          "form": "아 넥플릭스에 자연 환경 다큐 추천받았는데\r\n알려주까?",
          "original_form": "아 넥플릭스에 자연 환경 다큐 추천받았는데\r\n알려주까?",
          "speaker_id": "2",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044339.1.22",
          "form": "오 좋지",
          "original_form": "오 좋지",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044339.1.23",
          "form": "뭔데 뭔데???",
          "original_form": "뭔데 뭔데???",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044339.1.24",
          "form": "재밌는건아닌데ㅋㅋ\r\n종이 빨대 같은 환경 다큐",
          "original_form": "재밌는건아닌데ㅋㅋ\r\n종이 빨대 같은 환경 다큐",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044339.1.25",
          "form": "종이빨대?",
          "original_form": "종이빨대?",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044339.1.26",
          "form": "환경문제가 요즘 중요하니까 좋은듯",
          "original_form": "환경문제가 요즘 중요하니까 좋은듯",
          "speaker_id": "1",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044339.1.27",
          "form": "씨스피라시",
          "original_form": "씨스피라시",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044339.1.28",
          "form": "오늘 회사에서 추천들음",
          "original_form": "오늘 회사에서 추천들음",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044339.1.29",
          "form": "아 그게 제목이야?",
          "original_form": "아 그게 제목이야?",
          "speaker_id": "1",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044339.1.30",
          "form": "넥플릭스 다큐래",
          "original_form": "넥플릭스 다큐래",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044339.1.31",
          "form": "응 씨스피라시 제목이야",
          "original_form": "응 씨스피라시 제목이야",
          "speaker_id": "2",
          "time": "20210907 22:36"
        },
        {
          "id": "MDRW2100044339.1.32",
          "form": "오  넷플 끊음 봐야지",
          "original_form": "오  넷플 끊음 봐야지",
          "speaker_id": "1",
          "time": "20210907 22:36"
        }
      ]
