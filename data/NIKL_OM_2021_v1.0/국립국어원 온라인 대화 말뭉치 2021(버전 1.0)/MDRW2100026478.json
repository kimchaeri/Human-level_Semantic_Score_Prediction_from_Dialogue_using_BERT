      "utterance": [
        {
          "id": "MDRW2100026478.1.1",
          "form": "좋아하는 티비프로그램 있으세요?",
          "original_form": "좋아하는 티비프로그램 있으세요?",
          "speaker_id": "1",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026478.1.2",
          "form": "음 요즘 넷플은",
          "original_form": "음 요즘 넷플은",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026478.1.3",
          "form": "체인지데이즈 돌싱글즈 디피 봐요",
          "original_form": "체인지데이즈 돌싱글즈 디피 봐요",
          "speaker_id": "2",
          "time": "20210901 22:33"
        },
        {
          "id": "MDRW2100026478.1.4",
          "form": "오 요즘핫한거 다보시네요!",
          "original_form": "오 요즘핫한거 다보시네요!",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026478.1.5",
          "form": "핫한가요?ㅋㅋㅋ 다넘 재밋네요",
          "original_form": "핫한가요?ㅋㅋㅋ 다넘 재밋네요",
          "speaker_id": "2",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026478.1.6",
          "form": "친구들이 재밌다고했던것들이에요 다ㅋㅋㅋ",
          "original_form": "친구들이 재밌다고했던것들이에요 다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026478.1.7",
          "form": "저는 웨이브써서 ,,",
          "original_form": "저는 웨이브써서 ,,",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026478.1.8",
          "form": "아 ㅋㅋㅋ 뭐보세용",
          "original_form": "아 ㅋㅋㅋ 뭐보세용",
          "speaker_id": "2",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026478.1.9",
          "form": "저는 런닝맨 노는언니 골때리는그녀들",
          "original_form": "저는 런닝맨 노는언니 골때리는그녀들",
          "speaker_id": "1",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026478.1.10",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "1",
          "time": "20210901 22:36"
        },
        {
          "id": "MDRW2100026478.1.11",
          "form": "아 골때리는그녀들 궁금란데..",
          "original_form": "아 골때리는그녀들 궁금란데..",
          "speaker_id": "2",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026478.1.12",
          "form": "넷플밖에안써서 ㅜㅜ",
          "original_form": "넷플밖에안써서 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026478.1.13",
          "form": "그쵸ㅡㅠㅠ 저도 딴걸로갈아타고싶은데",
          "original_form": "그쵸ㅡㅠㅠ 저도 딴걸로갈아타고싶은데",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026478.1.14",
          "form": "웨이브에만있대요그거",
          "original_form": "웨이브에만있대요그거",
          "speaker_id": "1",
          "time": "20210901 22:37"
        },
        {
          "id": "MDRW2100026478.1.15",
          "form": "ㅜㅡㅜ ㅋㅋㅋ  옛날에는 다 웹하드나 토렌트 받아보고 그랫엇는데 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅜㅡㅜ ㅋㅋㅋ  옛날에는 다 웹하드나 토렌트 받아보고 그랫엇는데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026478.1.16",
          "form": "아맞아요ㅋㅋㅋㄱㅋ",
          "original_form": "아맞아요ㅋㅋㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026478.1.17",
          "form": "베이코리언즈",
          "original_form": "베이코리언즈",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026478.1.18",
          "form": "그런걸로보고",
          "original_form": "그런걸로보고",
          "speaker_id": "1",
          "time": "20210901 22:38"
        },
        {
          "id": "MDRW2100026478.1.19",
          "form": "스트리밍이 편한대 통일이 안돼서 불편한거도잇네용ㅋㅋㅋㅋ",
          "original_form": "스트리밍이 편한대 통일이 안돼서 불편한거도잇네용ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:39"
        },
        {
          "id": "MDRW2100026478.1.20",
          "form": "티빙이랑 웨이브만이라도 합쳤으면 좋겠어요",
          "original_form": "티빙이랑 웨이브만이라도 합쳤으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210901 22:39"
        }
