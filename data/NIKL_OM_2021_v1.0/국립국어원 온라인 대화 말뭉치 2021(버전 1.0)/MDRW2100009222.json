      "utterance": [
        {
          "id": "MDRW2100009222.1.1",
          "form": "2만보 걸음 발 안아프셔요?",
          "original_form": "2만보 걸음 발 안아프셔요?",
          "speaker_id": "1",
          "time": "20210822 22:13"
        },
        {
          "id": "MDRW2100009222.1.2",
          "form": "발바닥 아파요",
          "original_form": "발바닥 아파요",
          "speaker_id": "2",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100009222.1.3",
          "form": "저 발도 작거든요 225인데",
          "original_form": "저 발도 작거든요 225인데",
          "speaker_id": "2",
          "time": "20210822 22:14"
        },
        {
          "id": "MDRW2100009222.1.4",
          "form": "뭔가 상체가 무거우니 고생한다 내작은 발바닥ㅜ같은 느낌ㅜㅜ",
          "original_form": "뭔가 상체가 무거우니 고생한다 내작은 발바닥ㅜ같은 느낌ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 22:15"
        },
        {
          "id": "MDRW2100009222.1.5",
          "form": "와 진짜 엄청 작으시네요",
          "original_form": "와 진짜 엄청 작으시네요",
          "speaker_id": "1",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009222.1.6",
          "form": "신발 사는것도 일이시겠어요ㅠㅠ",
          "original_form": "신발 사는것도 일이시겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009222.1.7",
          "form": "저는 왕발인데도 그렇개 걸으면 힘들던데",
          "original_form": "저는 왕발인데도 그렇개 걸으면 힘들던데",
          "speaker_id": "1",
          "time": "20210822 22:17"
        },
        {
          "id": "MDRW2100009222.1.8",
          "form": "사실 뱃살 집중운동을 좀 해야하는데 말이죠 ㅋㅋ",
          "original_form": "사실 뱃살 집중운동을 좀 해야하는데 말이죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:20"
        },
        {
          "id": "MDRW2100009222.1.9",
          "form": "아 복부운동 너무 힘들죠 ㅋㅋㅋㅋㅋ",
          "original_form": "아 복부운동 너무 힘들죠 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:30"
        },
        {
          "id": "MDRW2100009222.1.10",
          "form": "자신을 짜내는것같은 고통 ㅠㅠ",
          "original_form": "자신을 짜내는것같은 고통 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:31"
        },
        {
          "id": "MDRW2100009222.1.11",
          "form": "혼자두면 그래서인지 안하게되죠 ㅋㅋ",
          "original_form": "혼자두면 그래서인지 안하게되죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009222.1.12",
          "form": "운동 하시나요?",
          "original_form": "운동 하시나요?",
          "speaker_id": "2",
          "time": "20210822 22:32"
        },
        {
          "id": "MDRW2100009222.1.13",
          "form": "저 피티다녀요 ㅋㅋㅋㅋ",
          "original_form": "저 피티다녀요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 22:36"
        },
        {
          "id": "MDRW2100009222.1.14",
          "form": "복부운동할때마다 빌어요 ㅠㅠ",
          "original_form": "복부운동할때마다 빌어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:36"
        },
        {
          "id": "MDRW2100009222.1.15",
          "form": "살려달라고 ㅠㅠ",
          "original_form": "살려달라고 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:36"
        },
        {
          "id": "MDRW2100009222.1.16",
          "form": "전 좀 그런 강제적인게 필요한거같아요",
          "original_form": "전 좀 그런 강제적인게 필요한거같아요",
          "speaker_id": "2",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100009222.1.17",
          "form": "의지박약인 자에게는",
          "original_form": "의지박약인 자에게는",
          "speaker_id": "2",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100009222.1.18",
          "form": "저도 의지박약인데 ㅎㅎㅋㅋ ㅠㅠ",
          "original_form": "저도 의지박약인데 ㅎㅎㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009222.1.19",
          "form": "진짜 도움이 되긴하도라고요",
          "original_form": "진짜 도움이 되긴하도라고요",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009222.1.20",
          "form": "근데.. 너무 비싼..",
          "original_form": "근데.. 너무 비싼..",
          "speaker_id": "1",
          "time": "20210822 22:39"
        },
        {
          "id": "MDRW2100009222.1.21",
          "form": "아 그러게요ㅜ",
          "original_form": "아 그러게요ㅜ",
          "speaker_id": "2",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009222.1.22",
          "form": "진짜 넘 비싸서 솔직히 엄두가 안나여",
          "original_form": "진짜 넘 비싸서 솔직히 엄두가 안나여",
          "speaker_id": "2",
          "time": "20210822 22:45"
        },
        {
          "id": "MDRW2100009222.1.23",
          "form": "저도 20회하는데 100만원 나갔어요 ㅠㅠ",
          "original_form": "저도 20회하는데 100만원 나갔어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 22:47"
        },
        {
          "id": "MDRW2100009222.1.24",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "2",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009222.1.25",
          "form": "근데 전 좀 그거도 답답할거같은게",
          "original_form": "근데 전 좀 그거도 답답할거같은게",
          "speaker_id": "2",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009222.1.26",
          "form": "그냥도 죽을거같은더",
          "original_form": "그냥도 죽을거같은더",
          "speaker_id": "2",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009222.1.27",
          "form": "데",
          "original_form": "데",
          "speaker_id": "2",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009222.1.28",
          "form": "마스크끼고? 생각하니까 ㄷㄷ",
          "original_form": "마스크끼고? 생각하니까 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210822 22:52"
        },
        {
          "id": "MDRW2100009222.1.29",
          "form": "아 숨찰때는 진짜 힘들어요 ㅋㅋㅋㅋ",
          "original_form": "아 숨찰때는 진짜 힘들어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:30"
        }
      ]
