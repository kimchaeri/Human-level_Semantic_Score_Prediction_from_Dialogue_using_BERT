      "utterance": [
        {
          "id": "MDRW2100012641.1.1",
          "form": "뭐 상담해드릴까요?",
          "original_form": "뭐 상담해드릴까요?",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100012641.1.2",
          "form": "상담료는 계좌번호가...?ㅎㅎ",
          "original_form": "상담료는 계좌번호가...?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100012641.1.3",
          "form": "뭐든 물어보세요ㅎ 답해드릴께요ㅎㅎ",
          "original_form": "뭐든 물어보세요ㅎ 답해드릴께요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100012641.1.4",
          "form": "ㅎㅎ수다얘기하다보니",
          "original_form": "ㅎㅎ수다얘기하다보니",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100012641.1.5",
          "form": "연애는 어떠셨나해서ㅎ",
          "original_form": "연애는 어떠셨나해서ㅎ",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100012641.1.6",
          "form": "연애결혼이신가요?",
          "original_form": "연애결혼이신가요?",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100012641.1.7",
          "form": "연해결혼이라고해야하나ㅎㅎ",
          "original_form": "연해결혼이라고해야하나ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012641.1.8",
          "form": "모임을했었는데ㅎ 거기서만났어요ㅎ",
          "original_form": "모임을했었는데ㅎ 거기서만났어요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012641.1.9",
          "form": "무슨모임이요?ㅎ",
          "original_form": "무슨모임이요?ㅎ",
          "speaker_id": "1",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012641.1.10",
          "form": "이애기하면 길어지는데ㅎ 저는 원래 독신주의 비슷한거?",
          "original_form": "이애기하면 길어지는데ㅎ 저는 원래 독신주의 비슷한거?",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012641.1.11",
          "form": "술모임이죠뭐ㅎㅎ",
          "original_form": "술모임이죠뭐ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012641.1.12",
          "form": "여행동호회는 다른분인가ㅋ",
          "original_form": "여행동호회는 다른분인가ㅋ",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100012641.1.13",
          "form": "그거여서 개2마리키우면서 인생 즐겁게 살라고했었져ㅎㅎ",
          "original_form": "그거여서 개2마리키우면서 인생 즐겁게 살라고했었져ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100012641.1.14",
          "form": "같은분인것같은데요ㅎㅎㅎ",
          "original_form": "같은분인것같은데요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100012641.1.15",
          "form": "그럼 술모임은  아니자나요",
          "original_form": "그럼 술모임은  아니자나요",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100012641.1.16",
          "form": "자연스런 만남이니 연애결혼이 맞겠네요",
          "original_form": "자연스런 만남이니 연애결혼이 맞겠네요",
          "speaker_id": "1",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012641.1.17",
          "form": "맞겟죠?ㅎㅎ",
          "original_form": "맞겟죠?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 23:48"
        },
        {
          "id": "MDRW2100012641.1.18",
          "form": "모임해서 만났는데 첫눈에 반했어요ㅎ",
          "original_form": "모임해서 만났는데 첫눈에 반했어요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100012641.1.19",
          "form": "어쩌다보니 옆에앉게됬고 눈을 못마주쳤었죠ㅎ",
          "original_form": "어쩌다보니 옆에앉게됬고 눈을 못마주쳤었죠ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100012641.1.20",
          "form": "오ㅎ 그러기쉽지않은데",
          "original_form": "오ㅎ 그러기쉽지않은데",
          "speaker_id": "1",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100012641.1.21",
          "form": "근데 그날....",
          "original_form": "근데 그날....",
          "speaker_id": "2",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100012641.1.22",
          "form": "또르르.... 술취해서 술주정해ㅆ어요ㅠㅠ",
          "original_form": "또르르.... 술취해서 술주정해ㅆ어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100012641.1.23",
          "form": "저런~~",
          "original_form": "저런~~",
          "speaker_id": "1",
          "time": "20210824 23:51"
        },
        {
          "id": "MDRW2100012641.1.24",
          "form": "저는 술주정이 잠자는거에요ㅠ",
          "original_form": "저는 술주정이 잠자는거에요ㅠ",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012641.1.25",
          "form": "잠자고 혼잣말하는거ㅠ",
          "original_form": "잠자고 혼잣말하는거ㅠ",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012641.1.26",
          "form": "지금 아내가 그모습이 귀여웠다나봐요....",
          "original_form": "지금 아내가 그모습이 귀여웠다나봐요....",
          "speaker_id": "2",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012641.1.27",
          "form": "자는거면 그럴수도 있겠어요ㅎ",
          "original_form": "자는거면 그럴수도 있겠어요ㅎ",
          "speaker_id": "1",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012641.1.28",
          "form": "아내분도 첫눈에 반하셨나보네요",
          "original_form": "아내분도 첫눈에 반하셨나보네요",
          "speaker_id": "1",
          "time": "20210824 23:52"
        },
        {
          "id": "MDRW2100012641.1.29",
          "form": "그런건 아닐꺼에요ㅎ 나중에 이야기하는게 뭐 저런 놈이 다있나 했다내요ㅎ",
          "original_form": "그런건 아닐꺼에요ㅎ 나중에 이야기하는게 뭐 저런 놈이 다있나 했다내요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100012641.1.30",
          "form": "그리고 나서 몇번 모임가졌는데",
          "original_form": "그리고 나서 몇번 모임가졌는데",
          "speaker_id": "2",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100012641.1.31",
          "form": "그거알죠? 일부러 무관심하는거?",
          "original_form": "그거알죠? 일부러 무관심하는거?",
          "speaker_id": "2",
          "time": "20210824 23:54"
        },
        {
          "id": "MDRW2100012641.1.32",
          "form": "ㅎㅎ 질투심 유발인가요?ㅎ",
          "original_form": "ㅎㅎ 질투심 유발인가요?ㅎ",
          "speaker_id": "1",
          "time": "20210824 23:55"
        },
        {
          "id": "MDRW2100012641.1.33",
          "form": "에이ㅎㅎ 그건아니었어요ㅎ 그냥 못쳐다본거죠ㅎ",
          "original_form": "에이ㅎㅎ 그건아니었어요ㅎ 그냥 못쳐다본거죠ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100012641.1.34",
          "form": "근데 제가 인상이 날카로워서 무관심해보일수있아요ㅎ",
          "original_form": "근데 제가 인상이 날카로워서 무관심해보일수있아요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100012641.1.35",
          "form": "넘 좋아하면 그럴수있죠",
          "original_form": "넘 좋아하면 그럴수있죠",
          "speaker_id": "1",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100012641.1.36",
          "form": "그래서 제 생일이 되서 사궜던것 같애요ㅎ",
          "original_form": "그래서 제 생일이 되서 사궜던것 같애요ㅎ",
          "speaker_id": "2",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100012641.1.37",
          "form": "자연스럽게 모인끝나고 데려다 준다고하고ㅎ 단둘이서 걸어가면서... 입맞춤을....ㅎㅎㅎㅎㅎㅎ",
          "original_form": "자연스럽게 모인끝나고 데려다 준다고하고ㅎ 단둘이서 걸어가면서... 입맞춤을....ㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:00"
        },
        {
          "id": "MDRW2100012641.1.38",
          "form": "오모나ㅎ",
          "original_form": "오모나ㅎ",
          "speaker_id": "1",
          "time": "20210825 00:02"
        },
        {
          "id": "MDRW2100012641.1.39",
          "form": "그날이 제 생일 전날이었어요ㅎ",
          "original_form": "그날이 제 생일 전날이었어요ㅎ",
          "speaker_id": "2",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100012641.1.40",
          "form": "아내에게 제가 그랬죠... 우리 기억하기 쉽게 오늘 사귀지 말고 내일 사귀자고 ㅎ 그래야 내생일에 우리만난 1일이 되는거니깐ㅎㅎ",
          "original_form": "아내에게 제가 그랬죠... 우리 기억하기 쉽게 오늘 사귀지 말고 내일 사귀자고 ㅎ 그래야 내생일에 우리만난 1일이 되는거니깐ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:04"
        },
        {
          "id": "MDRW2100012641.1.41",
          "form": "그리고 다음날 만나서 연해했던 기억이나네요ㅎㅎ",
          "original_form": "그리고 다음날 만나서 연해했던 기억이나네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100012641.1.42",
          "form": "계획이있으셨군요ㅎ",
          "original_form": "계획이있으셨군요ㅎ",
          "speaker_id": "1",
          "time": "20210825 00:05"
        },
        {
          "id": "MDRW2100012641.1.43",
          "form": "올만에 남 연애얘기들으니 잼나네요",
          "original_form": "올만에 남 연애얘기들으니 잼나네요",
          "speaker_id": "1",
          "time": "20210825 00:06"
        }
