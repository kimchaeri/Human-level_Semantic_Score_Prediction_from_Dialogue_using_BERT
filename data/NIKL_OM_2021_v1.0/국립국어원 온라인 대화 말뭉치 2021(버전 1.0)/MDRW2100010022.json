      "utterance": [
        {
          "id": "MDRW2100010022.1.1",
          "form": "무슨게임 하고있어?",
          "original_form": "무슨게임 하고있어?",
          "speaker_id": "2",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100010022.1.2",
          "form": "에픽세븐",
          "original_form": "에픽세븐",
          "speaker_id": "1",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100010022.1.3",
          "form": "유희왕",
          "original_form": "유희왕",
          "speaker_id": "1",
          "time": "20210823 13:18"
        },
        {
          "id": "MDRW2100010022.1.4",
          "form": "요즘 광고많이나오는 리니지는 안해?",
          "original_form": "요즘 광고많이나오는 리니지는 안해?",
          "speaker_id": "2",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100010022.1.5",
          "form": "게임광고가 무슨영화광고 같더라",
          "original_form": "게임광고가 무슨영화광고 같더라",
          "speaker_id": "2",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100010022.1.6",
          "form": "리니지는 제스타일이 아니라...",
          "original_form": "리니지는 제스타일이 아니라...",
          "speaker_id": "1",
          "time": "20210823 13:19"
        },
        {
          "id": "MDRW2100010022.1.7",
          "form": "넌어떤 스타일 게임좋아하는데?",
          "original_form": "넌어떤 스타일 게임좋아하는데?",
          "speaker_id": "2",
          "time": "20210823 13:20"
        },
        {
          "id": "MDRW2100010022.1.8",
          "form": "전 rpg",
          "original_form": "전 rpg",
          "speaker_id": "1",
          "time": "20210823 13:20"
        },
        {
          "id": "MDRW2100010022.1.9",
          "form": "별이되어라 같은 게임이요",
          "original_form": "별이되어라 같은 게임이요",
          "speaker_id": "1",
          "time": "20210823 13:20"
        },
        {
          "id": "MDRW2100010022.1.10",
          "form": "유희왕게임은 카드게임이야?",
          "original_form": "유희왕게임은 카드게임이야?",
          "speaker_id": "2",
          "time": "20210823 13:20"
        },
        {
          "id": "MDRW2100010022.1.11",
          "form": "ㅇㅇ 다들 만화로알고있는데",
          "original_form": "ㅇㅇ 다들 만화로알고있는데",
          "speaker_id": "1",
          "time": "20210823 13:21"
        },
        {
          "id": "MDRW2100010022.1.12",
          "form": "게임으로도 나왔음",
          "original_form": "게임으로도 나왔음",
          "speaker_id": "1",
          "time": "20210823 13:21"
        },
        {
          "id": "MDRW2100010022.1.13",
          "form": "너 게임 랭킹높아?",
          "original_form": "너 게임 랭킹높아?",
          "speaker_id": "2",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100010022.1.14",
          "form": "맨날겜하니까 순위높겠네",
          "original_form": "맨날겜하니까 순위높겠네",
          "speaker_id": "2",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100010022.1.15",
          "form": "하핳....",
          "original_form": "하핳....",
          "speaker_id": "1",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100010022.1.16",
          "form": "아니높진않은뎅...",
          "original_form": "아니높진않은뎅...",
          "speaker_id": "1",
          "time": "20210823 13:22"
        },
        {
          "id": "MDRW2100010022.1.17",
          "form": "아그럼 프로게이머는 꿈도꾸지말아야겠다",
          "original_form": "아그럼 프로게이머는 꿈도꾸지말아야겠다",
          "speaker_id": "2",
          "time": "20210823 13:23"
        },
        {
          "id": "MDRW2100010022.1.18",
          "form": "흥",
          "original_form": "흥",
          "speaker_id": "1",
          "time": "20210823 13:23"
        },
        {
          "id": "MDRW2100010022.1.19",
          "form": "어차피 게이머할 생각없었어요",
          "original_form": "어차피 게이머할 생각없었어요",
