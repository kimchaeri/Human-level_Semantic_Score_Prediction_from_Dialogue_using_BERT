      "utterance": [
        {
          "id": "MDRW2100015960.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 03:54"
        },
        {
          "id": "MDRW2100015960.1.2",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100015960.1.3",
          "form": "일하고 계신가여??",
          "original_form": "일하고 계신가여??",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100015960.1.4",
          "form": "전 아직 취준 ㅎㅎ",
          "original_form": "전 아직 취준 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100015960.1.5",
          "form": "백수요ㅋㅋ",
          "original_form": "백수요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100015960.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ백수끼리 얘기해야되네여",
          "original_form": "ㅋㅋㅋㅋㅋㅋ백수끼리 얘기해야되네여",
          "speaker_id": "2",
          "time": "20210826 14:01"
        },
        {
          "id": "MDRW2100015960.1.7",
          "form": "뭐 어때요ㅋㅋ 자격증 있으신가요?",
          "original_form": "뭐 어때요ㅋㅋ 자격증 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 14:02"
        },
        {
          "id": "MDRW2100015960.1.8",
          "form": "전 아직 공부중이에여",
          "original_form": "전 아직 공부중이에여",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100015960.1.9",
          "form": "컴활 어려워요ㅜㅜ",
          "original_form": "컴활 어려워요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 14:03"
        },
        {
          "id": "MDRW2100015960.1.10",
          "form": "저는 전산 회계 세무 있어요",
          "original_form": "저는 전산 회계 세무 있어요",
          "speaker_id": "1",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100015960.1.11",
          "form": "공무원 준비중이세요?",
          "original_form": "공무원 준비중이세요?",
          "speaker_id": "1",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100015960.1.12",
          "form": "우아 머싯다",
          "original_form": "우아 머싯다",
          "speaker_id": "2",
          "time": "20210826 14:09"
        },
        {
          "id": "MDRW2100015960.1.13",
          "form": "아녀 공무원 말구 사무직 준비중이에여!",
          "original_form": "아녀 공무원 말구 사무직 준비중이에여!",
          "speaker_id": "2",
          "time": "20210826 14:10"
        },
        {
          "id": "MDRW2100015960.1.14",
          "form": "아르바이트 해보신적 있으신가요",
          "original_form": "아르바이트 해보신적 있으신가요",
          "speaker_id": "1",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100015960.1.15",
          "form": "네 서빙쪽",
          "original_form": "네 서빙쪽",
          "speaker_id": "2",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100015960.1.16",
          "form": "알바 많이 해보셔ㅛ나요??",
          "original_form": "알바 많이 해보셔ㅛ나요??",
          "speaker_id": "2",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100015960.1.17",
          "form": "한번이요",
          "original_form": "한번이요",
          "speaker_id": "1",
          "time": "20210826 14:12"
        },
        {
          "id": "MDRW2100015960.1.18",
          "form": "뭐했어요",
          "original_form": "뭐했어요",
          "speaker_id": "2",
          "time": "20210826 14:14"
        },
        {
          "id": "MDRW2100015960.1.19",
          "form": "전 서빙했는데 힘들어서 오래는 못햇어여",
          "original_form": "전 서빙했는데 힘들어서 오래는 못햇어여",
          "speaker_id": "2",
          "time": "20210826 14:14"
        },
        {
          "id": "MDRW2100015960.1.20",
          "form": "사무보조요",
          "original_form": "사무보조요",
          "speaker_id": "1",
          "time": "20210826 14:15"
        }
      ]
