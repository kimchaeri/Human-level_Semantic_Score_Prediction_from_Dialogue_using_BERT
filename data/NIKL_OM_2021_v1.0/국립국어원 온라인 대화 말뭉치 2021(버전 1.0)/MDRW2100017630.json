        },
        {
          "id": "MDRW2100017630.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210826 20:37"
        },
        {
          "id": "MDRW2100017630.1.3",
          "form": "뭐하시나요?",
          "original_form": "뭐하시나요?",
          "speaker_id": "2",
          "time": "20210826 20:40"
        },
        {
          "id": "MDRW2100017630.1.4",
          "form": "피파온라인이요",
          "original_form": "피파온라인이요",
          "speaker_id": "1",
          "time": "20210826 20:44"
        },
        {
          "id": "MDRW2100017630.1.5",
          "form": "피파도 롱런하는 게임같네요",
          "original_form": "피파도 롱런하는 게임같네요",
          "speaker_id": "2",
          "time": "20210826 21:23"
        },
        {
          "id": "MDRW2100017630.1.6",
          "form": "장수게임이죠ㅎㅎ",
          "original_form": "장수게임이죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:45"
        },
        {
          "id": "MDRW2100017630.1.7",
          "form": "저는 테트리스 정도..ㅋㅋ",
          "original_form": "저는 테트리스 정도..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:47"
        },
        {
          "id": "MDRW2100017630.1.8",
          "form": "헉 지뢰찾기도 좋아하시나요ㅎㅎ",
          "original_form": "헉 지뢰찾기도 좋아하시나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 22:53"
        },
        {
          "id": "MDRW2100017630.1.9",
          "form": "ㅋㅋ 그것도 하고 카드? 순서 맞추는것도 했네요",
          "original_form": "ㅋㅋ 그것도 하고 카드? 순서 맞추는것도 했네요",
          "speaker_id": "2",
          "time": "20210827 06:31"
        },
        {
          "id": "MDRW2100017630.1.10",
          "form": "와 대박 추억이네요ㅎㅎ",
          "original_form": "와 대박 추억이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 15:16"
        },
        {
          "id": "MDRW2100017630.1.11",
          "form": "기억 나시나요 윈도우 게임이었나",
          "original_form": "기억 나시나요 윈도우 게임이었나",
          "speaker_id": "2",
          "time": "20210828 15:18"
        },
        {
          "id": "MDRW2100017630.1.12",
          "form": "네네 기본게임ㅎㅎㅎ",
          "original_form": "네네 기본게임ㅎㅎㅎ",
          "speaker_id": "1",
