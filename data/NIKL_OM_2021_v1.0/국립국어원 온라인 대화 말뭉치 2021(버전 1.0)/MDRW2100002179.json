      "utterance": [
        {
          "id": "MDRW2100002179.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210820 00:17"
        },
        {
          "id": "MDRW2100002179.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210820 10:06"
        },
        {
          "id": "MDRW2100002179.1.3",
          "form": "혹시 반려동물이랑 함께 사시나요?",
          "original_form": "혹시 반려동물이랑 함께 사시나요?",
          "speaker_id": "2",
          "time": "20210820 10:06"
        },
        {
          "id": "MDRW2100002179.1.4",
          "form": "아뇨~ 강아지 키웠는데 나이들어서 먼저\r\n하늘나라 갔어요",
          "original_form": "아뇨~ 강아지 키웠는데 나이들어서 먼저\r\n하늘나라 갔어요",
          "speaker_id": "1",
          "time": "20210823 11:44"
        },
        {
          "id": "MDRW2100002179.1.5",
          "form": "몇년 키우셨어요??",
          "original_form": "몇년 키우셨어요??",
          "speaker_id": "2",
          "time": "20210823 11:47"
        },
        {
          "id": "MDRW2100002179.1.6",
          "form": "한 15년 키운거 같아요~",
          "original_form": "한 15년 키운거 같아요~",
          "speaker_id": "1",
          "time": "20210823 11:47"
        },
        {
          "id": "MDRW2100002179.1.7",
          "form": "아이구ㅠㅠ 많이 슬프셨겠어요",
          "original_form": "아이구ㅠㅠ 많이 슬프셨겠어요",
          "speaker_id": "2",
          "time": "20210823 11:48"
        },
        {
          "id": "MDRW2100002179.1.8",
          "form": "인생이 완전 바꼇어요 생명이 너무 소중해요",
          "original_form": "인생이 완전 바꼇어요 생명이 너무 소중해요",
          "speaker_id": "1",
          "time": "20210823 11:48"
        },
        {
          "id": "MDRW2100002179.1.9",
          "form": "맞아요ㅠ 저도 반려동물 한마리 무지개다리 건넜는데 계속 꿈에도 나오더라구요ㅠ",
          "original_form": "맞아요ㅠ 저도 반려동물 한마리 무지개다리 건넜는데 계속 꿈에도 나오더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100002179.1.10",
          "form": "아고ㅜ 다들 경험이 있으시네요",
          "original_form": "아고ㅜ 다들 경험이 있으시네요",
          "speaker_id": "1",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100002179.1.11",
          "form": "그래서 사료나 그런것도 굉장히 중요하게\r\n생각하게 됐어요",
          "original_form": "그래서 사료나 그런것도 굉장히 중요하게\r\n생각하게 됐어요",
          "speaker_id": "1",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100002179.1.12",
          "form": "어 맞아요ㅠㅠ 반려동물 공부도 더 열심히 하게되고요",
          "original_form": "어 맞아요ㅠㅠ 반려동물 공부도 더 열심히 하게되고요",
          "speaker_id": "2",
          "time": "20210823 11:50"
        },
        {
          "id": "MDRW2100002179.1.13",
          "form": "영양제를 평소에 먹이는 것도 중요하더라구요",
          "original_form": "영양제를 평소에 먹이는 것도 중요하더라구요",
          "speaker_id": "2",
          "time": "20210823 11:50"
        },
        {
          "id": "MDRW2100002179.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210823 11:50"
        },
        {
          "id": "MDRW2100002179.1.15",
          "form": "털 윤기부터 달라지더라구요~",
          "original_form": "털 윤기부터 달라지더라구요~",
          "speaker_id": "1",
          "time": "20210823 11:51"
        },
        {
          "id": "MDRW2100002179.1.16",
          "form": "하 보고싶다ㅠㅠ",
          "original_form": "하 보고싶다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 11:51"
        },
        {
          "id": "MDRW2100002179.1.17",
          "form": "맞아요ㅠㅠ 더 잘 해 줄걸 후회도 되네요ㅠ",
          "original_form": "맞아요ㅠㅠ 더 잘 해 줄걸 후회도 되네요ㅠ",
          "speaker_id": "2",
          "time": "20210823 11:53"
        },
        {
          "id": "MDRW2100002179.1.18",
          "form": "네~~",
          "original_form": "네~~",
          "speaker_id": "1",
          "time": "20210823 12:00"
        },
        {
          "id": "MDRW2100002179.1.19",
          "form": "우리 강아지는",
          "original_form": "우리 강아지는",
          "speaker_id": "1",
          "time": "20210823 12:00"
        },
        {
          "id": "MDRW2100002179.1.20",
          "form": "위염이랑 꼬리 피부병이 생기더라구요 아토피",
          "original_form": "위염이랑 꼬리 피부병이 생기더라구요 아토피",
