      "utterance": [
        {
          "id": "MDRW2100043415.1.1",
          "form": "그라게 요새 비가 많이 오는구만",
          "original_form": "그라게 요새 비가 많이 오는구만",
          "speaker_id": "2",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043415.1.2",
          "form": "가을장마는 가을장마여",
          "original_form": "가을장마는 가을장마여",
          "speaker_id": "2",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043415.1.3",
          "form": "비가 계속 와",
          "original_form": "비가 계속 와",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043415.1.4",
          "form": "내일은 개야하는디",
          "original_form": "내일은 개야하는디",
          "speaker_id": "1",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043415.1.5",
          "form": "그러게 사진찍을때 맑아야 좋은데",
          "original_form": "그러게 사진찍을때 맑아야 좋은데",
          "speaker_id": "2",
          "time": "20210907 18:53"
        },
        {
          "id": "MDRW2100043415.1.6",
          "form": "몇시촬영이랬지",
          "original_form": "몇시촬영이랬지",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043415.1.7",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043415.1.8",
          "form": "오전 9시부터ㅋㅋ",
          "original_form": "오전 9시부터ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043415.1.9",
          "form": "흙바닥에서",
          "original_form": "흙바닥에서",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043415.1.10",
          "form": "으 바닥 질척이면 싫은데",
          "original_form": "으 바닥 질척이면 싫은데",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043415.1.11",
          "form": "고생하겠네",
          "original_form": "고생하겠네",
          "speaker_id": "2",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043415.1.12",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043415.1.13",
          "form": "신발도 질척거리고",
          "original_form": "신발도 질척거리고",
          "speaker_id": "1",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043415.1.14",
          "form": "치마끝에 안묻게 조심해야겠다",
          "original_form": "치마끝에 안묻게 조심해야겠다",
          "speaker_id": "2",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043415.1.15",
          "form": "안닿겠지만",
          "original_form": "안닿겠지만",
          "speaker_id": "2",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043415.1.16",
          "form": "그치",
          "original_form": "그치",
          "speaker_id": "1",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043415.1.17",
          "form": "물기랑 진흙이라니...",
          "original_form": "물기랑 진흙이라니...",
          "speaker_id": "1",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043415.1.18",
          "form": "비가 일찍 그쳐야 마를텐데",
          "original_form": "비가 일찍 그쳐야 마를텐데",
          "speaker_id": "1",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043415.1.19",
          "form": "머리도 곱슬거려서 짜증나",
          "original_form": "머리도 곱슬거려서 짜증나",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043415.1.20",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043415.1.21",
          "form": "드라이해도 소용없고",
          "original_form": "드라이해도 소용없고",
          "speaker_id": "1",
          "time": "20210907 18:56"
        },
        {
          "id": "MDRW2100043415.1.22",
          "form": "곱슬기가 한층 강력해지지",
          "original_form": "곱슬기가 한층 강력해지지",
          "speaker_id": "2",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043415.1.23",
          "form": "응 걱정이다 진짜 내일 날씨",
          "original_form": "응 걱정이다 진짜 내일 날씨",
          "speaker_id": "1",
          "time": "20210907 18:57"
        },
        {
          "id": "MDRW2100043415.1.24",
          "form": "지금같아선 뭐 맑을거 같지 않네",
          "original_form": "지금같아선 뭐 맑을거 같지 않네",
