      "utterance": [
        {
          "id": "MDRW2100006024.1.1",
          "form": "추석때 할머니댁 못가겠지?",
          "original_form": "추석때 할머니댁 못가겠지?",
          "speaker_id": "1",
          "time": "20210907 19:13"
        },
        {
          "id": "MDRW2100006024.1.2",
          "form": "아빠엄마만 가셔야하지 않을까?",
          "original_form": "아빠엄마만 가셔야하지 않을까?",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100006024.1.3",
          "form": "사실 난 안가서 좋아",
          "original_form": "사실 난 안가서 좋아",
          "speaker_id": "2",
          "time": "20210907 19:14"
        },
        {
          "id": "MDRW2100006024.1.4",
          "form": "추석때 친척모임하면 우린 10명 넘으니까",
          "original_form": "추석때 친척모임하면 우린 10명 넘으니까",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100006024.1.5",
          "form": "백신접종 완료자가 다수여도",
          "original_form": "백신접종 완료자가 다수여도",
          "speaker_id": "1",
          "time": "20210907 19:15"
        },
        {
          "id": "MDRW2100006024.1.6",
          "form": "안 모이는 게 좋지",
          "original_form": "안 모이는 게 좋지",
          "speaker_id": "1",
          "time": "20210907 19:17"
        },
        {
          "id": "MDRW2100006024.1.7",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100006024.1.8",
          "form": "또 마스크 벗는거 자체가 신경쓰여",
          "original_form": "또 마스크 벗는거 자체가 신경쓰여",
          "speaker_id": "2",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100006024.1.9",
          "form": "그치 같이 밥 먹고",
          "original_form": "그치 같이 밥 먹고",
          "speaker_id": "1",
          "time": "20210907 19:18"
        },
        {
          "id": "MDRW2100006024.1.10",
          "form": "모임은 최소로 하는게 좋지",
          "original_form": "모임은 최소로 하는게 좋지",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100006024.1.11",
          "form": "맞아 그래서 추석 특별방역이래도",
          "original_form": "맞아 그래서 추석 특별방역이래도",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100006024.1.12",
          "form": "걍 집에서 있는게 좋을듯",
          "original_form": "걍 집에서 있는게 좋을듯",
          "speaker_id": "2",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100006024.1.13",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210907 19:22"
        },
        {
          "id": "MDRW2100006024.1.14",
          "form": "가급적 안움직이는 게",
          "original_form": "가급적 안움직이는 게",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100006024.1.15",
          "form": "사람들 돌아다니고나면 어떻게될지 불안해",
          "original_form": "사람들 돌아다니고나면 어떻게될지 불안해",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100006024.1.16",
          "form": "나도 가족도 국가도 살리는 길",
          "original_form": "나도 가족도 국가도 살리는 길",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100006024.1.17",
          "form": "또 올라갈까봐 확진자수",
          "original_form": "또 올라갈까봐 확진자수",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100006024.1.18",
          "form": "추석 지나면 늘겠지..",
          "original_form": "추석 지나면 늘겠지..",
          "speaker_id": "1",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100006024.1.19",
          "form": "세자릿수로 얼른 떨어지면 좋겠드",
          "original_form": "세자릿수로 얼른 떨어지면 좋겠드",
          "speaker_id": "2",
          "time": "20210907 19:23"
        },
        {
          "id": "MDRW2100006024.1.20",
          "form": "그러게 말여",
          "original_form": "그러게 말여",
          "speaker_id": "1",
          "time": "20210907 19:24"
        }
      ]
