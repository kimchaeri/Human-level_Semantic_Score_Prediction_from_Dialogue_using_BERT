      "utterance": [
        {
          "id": "MDRW2100007723.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210822 09:54"
        },
        {
          "id": "MDRW2100007723.1.2",
          "form": "네네~청소하고 빨래하고ㅠㅠ",
          "original_form": "네네~청소하고 빨래하고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007723.1.3",
          "form": "집안일 끝이 없구만요~",
          "original_form": "집안일 끝이 없구만요~",
          "speaker_id": "2",
          "time": "20210822 09:55"
        },
        {
          "id": "MDRW2100007723.1.4",
          "form": "그르네요",
          "original_form": "그르네요",
          "speaker_id": "1",
          "time": "20210822 09:56"
        },
        {
          "id": "MDRW2100007723.1.5",
          "form": "육아 하시나요?",
          "original_form": "육아 하시나요?",
          "speaker_id": "2",
          "time": "20210822 09:58"
        },
        {
          "id": "MDRW2100007723.1.6",
          "form": "네 초딩이요",
          "original_form": "네 초딩이요",
          "speaker_id": "1",
          "time": "20210822 09:59"
        },
        {
          "id": "MDRW2100007723.1.7",
          "form": "저희 집에도 초딩이 있네요 ㅋㅋㅋ",
          "original_form": "저희 집에도 초딩이 있네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:00"
        },
        {
          "id": "MDRW2100007723.1.8",
          "form": "아침먹고 온식구 폰타임이네요;;",
          "original_form": "아침먹고 온식구 폰타임이네요;;",
          "speaker_id": "2",
          "time": "20210822 10:00"
        },
        {
          "id": "MDRW2100007723.1.9",
          "form": "대화가 줄었어요",
          "original_form": "대화가 줄었어요",
          "speaker_id": "1",
          "time": "20210822 10:01"
        },
        {
          "id": "MDRW2100007723.1.10",
          "form": "맞아요~요즘은 진짜 친구랑 노는것만 좋아하고..ㅜ",
          "original_form": "맞아요~요즘은 진짜 친구랑 노는것만 좋아하고..ㅜ",
          "speaker_id": "2",
          "time": "20210822 10:03"
        },
        {
          "id": "MDRW2100007723.1.11",
          "form": "아니면 유튜브ㅠㅠ",
          "original_form": "아니면 유튜브ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 10:03"
        },
        {
          "id": "MDRW2100007723.1.12",
          "form": "둘째가 4살이라..얘 아님 집이 고요해요~",
          "original_form": "둘째가 4살이라..얘 아님 집이 고요해요~",
          "speaker_id": "2",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007723.1.13",
          "form": "네 어린시절이랑 비교되요",
          "original_form": "네 어린시절이랑 비교되요",
          "speaker_id": "1",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007723.1.14",
          "form": "얼른 방학이 끝나야 집안일에서도 해방될텐데ㅠ",
          "original_form": "얼른 방학이 끝나야 집안일에서도 해방될텐데ㅠ",
          "speaker_id": "2",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007723.1.15",
          "form": "코로나땜에 가려나 모르겠네요~",
          "original_form": "코로나땜에 가려나 모르겠네요~",
          "speaker_id": "2",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007723.1.16",
          "form": "네 다 나가야 편하죠",
          "original_form": "네 다 나가야 편하죠",
          "speaker_id": "1",
          "time": "20210822 10:04"
        },
        {
          "id": "MDRW2100007723.1.17",
          "form": "주말이 이렇게 지나가네요~",
          "original_form": "주말이 이렇게 지나가네요~",
