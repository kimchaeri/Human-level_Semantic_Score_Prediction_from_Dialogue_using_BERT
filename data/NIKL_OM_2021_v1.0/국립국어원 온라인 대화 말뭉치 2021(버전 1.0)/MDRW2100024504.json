      "utterance": [
        {
          "id": "MDRW2100024504.1.1",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210831 12:07"
        },
        {
          "id": "MDRW2100024504.1.2",
          "form": "전 요새 솔의눈이 땡기더라구요ㅋㅋ",
          "original_form": "전 요새 솔의눈이 땡기더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:07"
        },
        {
          "id": "MDRW2100024504.1.3",
          "form": "와 진짜예전에 먹어본 기억이",
          "original_form": "와 진짜예전에 먹어본 기억이",
          "speaker_id": "1",
          "time": "20210831 12:08"
        },
        {
          "id": "MDRW2100024504.1.4",
          "form": "ㅋㅋㅋ더워서 여기에 얼음타서 먹으니",
          "original_form": "ㅋㅋㅋ더워서 여기에 얼음타서 먹으니",
          "speaker_id": "2",
          "time": "20210831 12:10"
        },
        {
          "id": "MDRW2100024504.1.5",
          "form": "머릿속까지 시원해지더라구요ㅋ",
          "original_form": "머릿속까지 시원해지더라구요ㅋ",
          "speaker_id": "2",
          "time": "20210831 12:10"
        },
        {
          "id": "MDRW2100024504.1.6",
          "form": "오 저도 한번 보이면 사먹어봐야겠네요 ㅎㅎ",
          "original_form": "오 저도 한번 보이면 사먹어봐야겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:32"
        },
        {
          "id": "MDRW2100024504.1.7",
          "form": "여름에 마시기 좋은 음료에요ㅎㅎ",
          "original_form": "여름에 마시기 좋은 음료에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 12:36"
        },
        {
          "id": "MDRW2100024504.1.8",
          "form": "달달하고ㅎㅎ",
          "original_form": "달달하고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 12:36"
        },
        {
          "id": "MDRW2100024504.1.9",
          "form": "그러게요 깔끔한맛 ㅎㅎ",
          "original_form": "그러게요 깔끔한맛 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100024504.1.10",
          "form": "즐겨드시는 음료있으세요?",
          "original_form": "즐겨드시는 음료있으세요?",
          "speaker_id": "2",
          "time": "20210831 14:50"
        },
        {
          "id": "MDRW2100024504.1.11",
          "form": "콜라요ㅎㅎ",
          "original_form": "콜라요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:52"
        },
        {
          "id": "MDRW2100024504.1.12",
          "form": "콜라만 드시는군요ㅋ",
          "original_form": "콜라만 드시는군요ㅋ",
          "speaker_id": "2",
          "time": "20210831 14:52"
        },
        {
          "id": "MDRW2100024504.1.13",
          "form": "아 커피요 즐겨요 ㅎㅎ",
          "original_form": "아 커피요 즐겨요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100024504.1.14",
          "form": "주 음료가 콜라 ㅋ",
          "original_form": "주 음료가 콜라 ㅋ",
          "speaker_id": "1",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100024504.1.15",
          "form": "콜라에 진심이시네요ㅋㅋ",
          "original_form": "콜라에 진심이시네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100024504.1.16",
          "form": "네 진심입니다,ㅎㅎ 쟁여놓고 먹어요",
          "original_form": "네 진심입니다,ㅎㅎ 쟁여놓고 먹어요",
          "speaker_id": "1",
          "time": "20210831 14:56"
        },
        {
          "id": "MDRW2100024504.1.17",
          "form": "ㅋㅋㅋㅋ드시고 양치는 꼭  하세요\r\n당이 많이 들어서ㅎ",
          "original_form": "ㅋㅋㅋㅋ드시고 양치는 꼭  하세요\r\n당이 많이 들어서ㅎ",
