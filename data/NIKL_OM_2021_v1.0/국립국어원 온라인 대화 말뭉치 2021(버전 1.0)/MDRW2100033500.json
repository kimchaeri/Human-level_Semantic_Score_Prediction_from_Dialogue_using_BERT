      "utterance": [
        {
          "id": "MDRW2100033500.1.1",
          "form": "여기서 카페회원님을 만나다니ㅎ 반가워요",
          "original_form": "여기서 카페회원님을 만나다니ㅎ 반가워요",
          "speaker_id": "2",
          "time": "20210824 02:11"
        },
        {
          "id": "MDRW2100033500.1.2",
          "form": "그러게요~ 카페 활동 하다보면ㅋ 진짜 물건들 싸게사고 하니까",
          "original_form": "그러게요~ 카페 활동 하다보면ㅋ 진짜 물건들 싸게사고 하니까",
          "speaker_id": "1",
          "time": "20210824 02:11"
        },
        {
          "id": "MDRW2100033500.1.3",
          "form": "자주 들어가서 챙겨보게 되더라구요",
          "original_form": "자주 들어가서 챙겨보게 되더라구요",
          "speaker_id": "1",
          "time": "20210824 02:12"
        },
        {
          "id": "MDRW2100033500.1.4",
          "form": "놓칠수 없는ㅋㅋ",
          "original_form": "놓칠수 없는ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 02:12"
        },
        {
          "id": "MDRW2100033500.1.5",
          "form": "저도요ㅎ 회원가입을 얼마나 했는지",
          "original_form": "저도요ㅎ 회원가입을 얼마나 했는지",
          "speaker_id": "2",
          "time": "20210824 02:12"
        },
        {
          "id": "MDRW2100033500.1.6",
          "form": "요즘엔 거기만봐요~ ㅎ",
          "original_form": "요즘엔 거기만봐요~ ㅎ",
          "speaker_id": "1",
          "time": "20210824 02:12"
        },
        {
          "id": "MDRW2100033500.1.7",
          "form": "혹시 머지 털리셨나요?",
          "original_form": "혹시 머지 털리셨나요?",
          "speaker_id": "1",
          "time": "20210824 02:12"
        },
        {
          "id": "MDRW2100033500.1.8",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20210824 02:13"
        },
        {
          "id": "MDRW2100033500.1.9",
          "form": "아 전그건안했어요",
          "original_form": "아 전그건안했어요",
          "speaker_id": "2",
          "time": "20210824 02:13"
        },
        {
          "id": "MDRW2100033500.1.10",
          "form": "다행이시네요ㅠ",
          "original_form": "다행이시네요ㅠ",
          "speaker_id": "1",
          "time": "20210824 02:13"
        },
        {
          "id": "MDRW2100033500.1.11",
          "form": "환불 못받으셨어요?ㅠㅠ",
          "original_form": "환불 못받으셨어요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 02:13"
        },
        {
          "id": "MDRW2100033500.1.12",
          "form": "물건싸게사려다가 ㅋㅋㅋ",
          "original_form": "물건싸게사려다가 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 02:13"
        },
        {
          "id": "MDRW2100033500.1.13",
          "form": "머니는얼마없는데 연간권이 물려있어요ㅡ",
          "original_form": "머니는얼마없는데 연간권이 물려있어요ㅡ",
          "speaker_id": "1",
          "time": "20210824 02:14"
        },
        {
          "id": "MDRW2100033500.1.14",
          "form": "싸게사서 한푼두푼 아끼다가  큰코다쳤어여",
          "original_form": "싸게사서 한푼두푼 아끼다가  큰코다쳤어여",
          "speaker_id": "1",
          "time": "20210824 02:15"
        },
        {
          "id": "MDRW2100033500.1.15",
          "form": "에고 기사보니 3명 입건됐던데ㅠㅠ",
          "original_form": "에고 기사보니 3명 입건됐던데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 02:15"
        },
        {
          "id": "MDRW2100033500.1.16",
          "form": "금액권은 조심해야겠더라구요",
          "original_form": "금액권은 조심해야겠더라구요",
          "speaker_id": "2",
          "time": "20210824 02:15"
        },
        {
          "id": "MDRW2100033500.1.17",
          "form": "안그래도 그거보고 환불안되려나 했는데",
          "original_form": "안그래도 그거보고 환불안되려나 했는데",
          "speaker_id": "1",
          "time": "20210824 02:15"
        },
        {
          "id": "MDRW2100033500.1.18",
          "form": "환불받으신분도 있던데",
          "original_form": "환불받으신분도 있던데",
          "speaker_id": "2",
          "time": "20210824 02:15"
        },
        {
          "id": "MDRW2100033500.1.19",
          "form": "다행히  오늘도 되긴했던데 연간권은",
          "original_form": "다행히  오늘도 되긴했던데 연간권은",
          "speaker_id": "1",
          "time": "20210824 02:16"
        },
        {
          "id": "MDRW2100033500.1.20",
          "form": "가망이없어서ㅠ",
          "original_form": "가망이없어서ㅠ",
          "speaker_id": "1",
          "time": "20210824 02:16"
        },
        {
          "id": "MDRW2100033500.1.21",
          "form": "네 ㄷ근데보면 극소수인것 같아요",
          "original_form": "네 ㄷ근데보면 극소수인것 같아요",
          "speaker_id": "1",
          "time": "20210824 02:16"
        },
        {
          "id": "MDRW2100033500.1.22",
          "form": "아직도 첫날꺼 해주고있데요.",
          "original_form": "아직도 첫날꺼 해주고있데요.",
          "speaker_id": "1",
          "time": "20210824 02:16"
        },
        {
          "id": "MDRW2100033500.1.23",
          "form": "8시반인가",
          "original_form": "8시반인가",
          "speaker_id": "1",
          "time": "20210824 02:16"
        },
        {
          "id": "MDRW2100033500.1.24",
          "form": "아 아마 금액작은것만 해주나봐요",
          "original_form": "아 아마 금액작은것만 해주나봐요",
          "speaker_id": "2",
          "time": "20210824 02:17"
        },
        {
          "id": "MDRW2100033500.1.25",
          "form": "인증보면 큰 것도 있긴한데 시간끄는것 같아요ㅠ 머지도 있다 없으니 물건살때 한번씩 아쉽고. 그렇더라구요",
          "original_form": "인증보면 큰 것도 있긴한데 시간끄는것 같아요ㅠ 머지도 있다 없으니 물건살때 한번씩 아쉽고. 그렇더라구요",
          "speaker_id": "1",
          "time": "20210824 02:18"
        },
        {
          "id": "MDRW2100033500.1.26",
          "form": "먹튀안함 다행일듯요",
          "original_form": "먹튀안함 다행일듯요",
          "speaker_id": "2",
          "time": "20210824 02:19"
        }
      ]
    }
  ]
