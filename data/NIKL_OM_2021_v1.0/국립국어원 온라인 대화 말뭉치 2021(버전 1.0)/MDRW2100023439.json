      "utterance": [
        {
          "id": "MDRW2100023439.1.1",
          "form": "어떠셨어요?",
          "original_form": "어떠셨어요?",
          "speaker_id": "2",
          "time": "20210830 02:34"
        },
        {
          "id": "MDRW2100023439.1.2",
          "form": "도시와 역사가 공존하는 도시같았어요ㅋㅋㅋ",
          "original_form": "도시와 역사가 공존하는 도시같았어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 02:34"
        },
        {
          "id": "MDRW2100023439.1.3",
          "form": "사람들도 친절했고",
          "original_form": "사람들도 친절했고",
          "speaker_id": "1",
          "time": "20210830 02:34"
        },
        {
          "id": "MDRW2100023439.1.4",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210830 02:34"
        },
        {
          "id": "MDRW2100023439.1.5",
          "form": "현대화되어있으면서도 시골스러운?",
          "original_form": "현대화되어있으면서도 시골스러운?",
          "speaker_id": "1",
          "time": "20210830 02:34"
        },
        {
          "id": "MDRW2100023439.1.6",
          "form": "먹거리 많나여?",
          "original_form": "먹거리 많나여?",
          "speaker_id": "2",
          "time": "20210830 02:34"
        },
        {
          "id": "MDRW2100023439.1.7",
          "form": "네네! 여기 지역특산물 이런게",
          "original_form": "네네! 여기 지역특산물 이런게",
          "speaker_id": "1",
          "time": "20210830 02:34"
        },
        {
          "id": "MDRW2100023439.1.8",
          "form": "잘되어있는것같아요",
          "original_form": "잘되어있는것같아요",
          "speaker_id": "1",
          "time": "20210830 02:35"
        },
        {
          "id": "MDRW2100023439.1.9",
          "form": "헉 어떤게 제일 맛있었어요?",
          "original_form": "헉 어떤게 제일 맛있었어요?",
          "speaker_id": "2",
          "time": "20210830 02:35"
        },
        {
          "id": "MDRW2100023439.1.10",
          "form": "오사카 갔을때 그 돈키호테로 유명한 거리 아세용?",
          "original_form": "오사카 갔을때 그 돈키호테로 유명한 거리 아세용?",
          "speaker_id": "1",
          "time": "20210830 02:35"
        },
        {
          "id": "MDRW2100023439.1.11",
          "form": "거기서 타코야끼 먹었는데",
          "original_form": "거기서 타코야끼 먹었는데",
          "speaker_id": "1",
          "time": "20210830 02:35"
        },
        {
          "id": "MDRW2100023439.1.12",
          "form": "오사카가 두팔벌리고 뛰는 조명있는데죠?",
          "original_form": "오사카가 두팔벌리고 뛰는 조명있는데죠?",
          "speaker_id": "2",
          "time": "20210830 02:35"
        },
        {
          "id": "MDRW2100023439.1.13",
          "form": "진짜 맛있었어용",
          "original_form": "진짜 맛있었어용",
          "speaker_id": "1",
          "time": "20210830 02:35"
        },
        {
          "id": "MDRW2100023439.1.14",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210830 02:35"
        },
        {
          "id": "MDRW2100023439.1.15",
          "form": "타코야끼ㅠㅠ 안먹어봐도 맛있을 각이예요",
          "original_form": "타코야끼ㅠㅠ 안먹어봐도 맛있을 각이예요",
          "speaker_id": "2",
          "time": "20210830 02:36"
        },
        {
          "id": "MDRW2100023439.1.16",
          "form": "근데 한국이랑 맛이 거의 비슷하긴하더라구요ㅋㅋㅋㅋ",
          "original_form": "근데 한국이랑 맛이 거의 비슷하긴하더라구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 02:36"
        },
        {
          "id": "MDRW2100023439.1.17",
          "form": "좀 더 알이 컸어요",
          "original_form": "좀 더 알이 컸어요",
          "speaker_id": "1",
          "time": "20210830 02:36"
        },
        {
          "id": "MDRW2100023439.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아무래돜ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아무래돜ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 02:36"
        },
        {
          "id": "MDRW2100023439.1.19",
          "form": "한국분들도 일본에서 배워오신분들이라",
          "original_form": "한국분들도 일본에서 배워오신분들이라",
          "speaker_id": "1",
          "time": "20210830 02:36"
        },
        {
          "id": "MDRW2100023439.1.20",
          "form": "거의 흡사했어용",
          "original_form": "거의 흡사했어용",
          "speaker_id": "1",
          "time": "20210830 02:36"
        }
