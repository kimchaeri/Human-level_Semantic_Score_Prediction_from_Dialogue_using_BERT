      "utterance": [
        {
          "id": "MDRW2100029792.1.1",
          "form": "연애 좋아하세요??",
          "original_form": "연애 좋아하세요??",
          "speaker_id": "2",
          "time": "20210903 13:42"
        },
        {
          "id": "MDRW2100029792.1.2",
          "form": "네, 10명 채우려고요",
          "original_form": "네, 10명 채우려고요",
          "speaker_id": "1",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100029792.1.3",
          "form": "와우 대단하시네요",
          "original_form": "와우 대단하시네요",
          "speaker_id": "2",
          "time": "20210903 14:30"
        },
        {
          "id": "MDRW2100029792.1.4",
          "form": "6명 남았네요",
          "original_form": "6명 남았네요",
          "speaker_id": "1",
          "time": "20210903 14:31"
        },
        {
          "id": "MDRW2100029792.1.5",
          "form": "아직 갈길이 머시네요",
          "original_form": "아직 갈길이 머시네요",
          "speaker_id": "2",
          "time": "20210903 14:31"
        },
        {
          "id": "MDRW2100029792.1.6",
          "form": "지금 만나시는 분이랑 얼른 정리하시죠",
          "original_form": "지금 만나시는 분이랑 얼른 정리하시죠",
          "speaker_id": "2",
          "time": "20210903 14:32"
        },
        {
          "id": "MDRW2100029792.1.7",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210903 14:32"
        },
        {
          "id": "MDRW2100029792.1.8",
          "form": "몇 명이 목표세요?",
          "original_form": "몇 명이 목표세요?",
          "speaker_id": "1",
          "time": "20210903 14:32"
        },
        {
          "id": "MDRW2100029792.1.9",
          "form": "저는 2명 정도??",
          "original_form": "저는 2명 정도??",
          "speaker_id": "2",
          "time": "20210903 14:34"
        },
        {
          "id": "MDRW2100029792.1.10",
          "form": "현실적이죠?",
          "original_form": "현실적이죠?",
          "speaker_id": "2",
          "time": "20210903 14:35"
        },
        {
          "id": "MDRW2100029792.1.11",
          "form": "연애에 맛들리셨나 봐요",
          "original_form": "연애에 맛들리셨나 봐요",
          "speaker_id": "1",
          "time": "20210903 14:35"
        },
        {
          "id": "MDRW2100029792.1.12",
          "form": "들켰네요",
          "original_form": "들켰네요",
          "speaker_id": "2",
          "time": "20210903 14:38"
        },
        {
          "id": "MDRW2100029792.1.13",
          "form": "결혼 생각 있으세요??",
          "original_form": "결혼 생각 있으세요??",
          "speaker_id": "2",
          "time": "20210903 14:40"
        },
        {
          "id": "MDRW2100029792.1.14",
          "form": "없습니다",
          "original_form": "없습니다",
          "speaker_id": "1",
          "time": "20210903 14:58"
        },
        {
          "id": "MDRW2100029792.1.15",
          "form": "단호하시네요",
          "original_form": "단호하시네요",
          "speaker_id": "2",
          "time": "20210903 14:58"
        },
        {
          "id": "MDRW2100029792.1.16",
          "form": "생각 없으니까요",
          "original_form": "생각 없으니까요",
          "speaker_id": "1",
          "time": "20210903 17:08"
        },
        {
          "id": "MDRW2100029792.1.17",
          "form": "저기요",
