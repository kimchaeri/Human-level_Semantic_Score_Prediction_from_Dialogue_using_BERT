      "utterance": [
        {
          "id": "MDRW2100047645.1.1",
          "form": "저 지금까지 에어컨 틀다 껐어요",
          "original_form": "저 지금까지 에어컨 틀다 껐어요",
          "speaker_id": "2",
          "time": "20210826 23:29"
        },
        {
          "id": "MDRW2100047645.1.2",
          "form": "많이 더우세요?",
          "original_form": "많이 더우세요?",
          "speaker_id": "1",
          "time": "20210826 23:30"
        },
        {
          "id": "MDRW2100047645.1.3",
          "form": "그게 습도 때문에요",
          "original_form": "그게 습도 때문에요",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100047645.1.4",
          "form": "애아빠가 힘들어해서",
          "original_form": "애아빠가 힘들어해서",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100047645.1.5",
          "form": "맞아요 습하긴 해요 ㅠㅠ",
          "original_form": "맞아요 습하긴 해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100047645.1.6",
          "form": "습도 맞추려고",
          "original_form": "습도 맞추려고",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100047645.1.7",
          "form": "뽀송뽀송해 졌어요",
          "original_form": "뽀송뽀송해 졌어요",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100047645.1.8",
          "form": "저도 그냥 더운건 괜찮은데 습도높은게 힘들어요 ㅠㅠ",
          "original_form": "저도 그냥 더운건 괜찮은데 습도높은게 힘들어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100047645.1.9",
          "form": "저희 도마뱀도 습도 높음 아파요",
          "original_form": "저희 도마뱀도 습도 높음 아파요",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100047645.1.10",
          "form": "맞아요 끈적해요",
          "original_form": "맞아요 끈적해요",
          "speaker_id": "2",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100047645.1.11",
          "form": "아 도마뱀도 습도가 높으면 안되는구나",
          "original_form": "아 도마뱀도 습도가 높으면 안되는구나",
          "speaker_id": "1",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100047645.1.12",
          "form": "도마뱀도 종류가 다양해서 그런가요",
          "original_form": "도마뱀도 종류가 다양해서 그런가요",
          "speaker_id": "1",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100047645.1.13",
          "form": "동남아에서 봤던 도마뱀들은 그 습한 날씨에도 멀쩡해보엿는데 ㅋㅋㅋㅋ 신기하네요",
          "original_form": "동남아에서 봤던 도마뱀들은 그 습한 날씨에도 멀쩡해보엿는데 ㅋㅋㅋㅋ 신기하네요",
          "speaker_id": "1",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100047645.1.14",
          "form": "종류가 그래요",
          "original_form": "종류가 그래요",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100047645.1.15",
          "form": "건조한곳 살던 녀석이라",
          "original_form": "건조한곳 살던 녀석이라",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100047645.1.16",
          "form": "그래서 사육장에 제습기 돌려요",
          "original_form": "그래서 사육장에 제습기 돌려요",
          "speaker_id": "2",
          "time": "20210826 23:32"
        },
        {
          "id": "MDRW2100047645.1.17",
          "form": "저희는 없어도",
          "original_form": "저희는 없어도",
          "speaker_id": "2",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100047645.1.18",
          "form": "우와 그렇구나",
          "original_form": "우와 그렇구나",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100047645.1.19",
          "form": "도마뱀들은 사줬어요 ㅋㅋ",
          "original_form": "도마뱀들은 사줬어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100047645.1.20",
          "form": "올여름 쉴새없이 제습기 돌렷겟어요",
          "original_form": "올여름 쉴새없이 제습기 돌렷겟어요",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100047645.1.21",
          "form": "계속 습햇는데ㅠㅠ",
          "original_form": "계속 습햇는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100047645.1.22",
          "form": "전기세 ㅋㅋ10만원 넘어요",
          "original_form": "전기세 ㅋㅋ10만원 넘어요",
          "speaker_id": "2",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100047645.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100047645.1.24",
          "form": "전 에어컨요",
          "original_form": "전 에어컨요",
          "speaker_id": "2",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100047645.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ에어컨도 계속 틀고 ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ에어컨도 계속 틀고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100047645.1.26",
          "form": "계속 돌렸어요",
          "original_form": "계속 돌렸어요",
          "speaker_id": "2",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100047645.1.27",
          "form": "습하면 너무 불쾌해요",
          "original_form": "습하면 너무 불쾌해요",
          "speaker_id": "2",
          "time": "20210826 23:33"
        },
        {
          "id": "MDRW2100047645.1.28",
          "form": "맞아요 찐득찐득한 기분",
          "original_form": "맞아요 찐득찐득한 기분",
          "speaker_id": "1",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100047645.1.29",
          "form": "윽",
          "original_form": "윽",
          "speaker_id": "2",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100047645.1.30",
          "form": "오늘도 너무 찐득",
          "original_form": "오늘도 너무 찐득",
          "speaker_id": "2",
          "time": "20210826 23:35"
        },
        {
          "id": "MDRW2100047645.1.31",
          "form": "맞아요 오늘도 비는 안왓는데 습도는 높네요",
          "original_form": "맞아요 오늘도 비는 안왓는데 습도는 높네요",
