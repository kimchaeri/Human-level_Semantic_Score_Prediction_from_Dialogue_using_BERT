      "utterance": [
        {
          "id": "MDRW2100043525.1.1",
          "form": "저녁식사하셨나요",
          "original_form": "저녁식사하셨나요",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043525.1.2",
          "form": "저는 어머니가 콩나물국에 쇠고기를넣어주셨는데",
          "original_form": "저는 어머니가 콩나물국에 쇠고기를넣어주셨는데",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043525.1.3",
          "form": "세상 존맛",
          "original_form": "세상 존맛",
          "speaker_id": "2",
          "time": "20210907 19:34"
        },
        {
          "id": "MDRW2100043525.1.4",
          "form": "아직이요 슬슬 배고프네요 ㅠ",
          "original_form": "아직이요 슬슬 배고프네요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043525.1.5",
          "form": "안그래도 콩나물 사왔어요!",
          "original_form": "안그래도 콩나물 사왔어요!",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043525.1.6",
          "form": "두그릇 드셨나요",
          "original_form": "두그릇 드셨나요",
          "speaker_id": "1",
          "time": "20210907 19:35"
        },
        {
          "id": "MDRW2100043525.1.7",
          "form": "껄껄",
          "original_form": "껄껄",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043525.1.8",
          "form": "두그릇은 못먹겠더라구요",
          "original_form": "두그릇은 못먹겠더라구요",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043525.1.9",
          "form": "밥을 폰하면서 먹는버릇이있어서",
          "original_form": "밥을 폰하면서 먹는버릇이있어서",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043525.1.10",
          "form": "천천히먹으니까 한그릇만먹어도",
          "original_form": "천천히먹으니까 한그릇만먹어도",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043525.1.11",
          "form": "배가불러버리는...",
          "original_form": "배가불러버리는...",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043525.1.12",
          "form": "오옹 콩나물로",
          "original_form": "오옹 콩나물로",
          "speaker_id": "2",
          "time": "20210907 19:41"
        },
        {
          "id": "MDRW2100043525.1.13",
          "form": "무슨요리하실건가용",
          "original_form": "무슨요리하실건가용",
          "speaker_id": "2",
          "time": "20210907 19:42"
        },
        {
          "id": "MDRW2100043525.1.14",
          "form": "콩나물 무침하려고 했는데 name2님 말씀하시는거 보고 소소기는 없지만 뜨끈한 콩나물국도 먹고 싶어졌어요",
          "original_form": "콩나물 무침하려고 했는데 &name2&님 말씀하시는거 보고 소소기는 없지만 뜨끈한 콩나물국도 먹고 싶어졌어요",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043525.1.15",
          "form": "고민중입니닷..",
          "original_form": "고민중입니닷..",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043525.1.16",
          "form": "ㅋㅋ큐큐ㅠㅠ",
          "original_form": "ㅋㅋ큐큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100043525.1.17",
          "form": "비올때는",
          "original_form": "비올때는",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043525.1.18",
          "form": "역시 뜨끈한 콩나물국이죠",
          "original_form": "역시 뜨끈한 콩나물국이죠",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043525.1.19",
          "form": "근데 저는 사실 콩나물국 별로안좋아합니다..",
          "original_form": "근데 저는 사실 콩나물국 별로안좋아합니다..",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043525.1.20",
          "form": "그 콩나물삶을때",
          "original_form": "그 콩나물삶을때",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043525.1.21",
          "form": "냄새있잖아요",
          "original_form": "냄새있잖아요",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043525.1.22",
          "form": "콩비린내?",
          "original_form": "콩비린내?",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043525.1.23",
          "form": "그런 냄새가...",
          "original_form": "그런 냄새가...",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043525.1.24",
          "form": "뭔가",
          "original_form": "뭔가",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043525.1.25",
          "form": "별루더라구용",
          "original_form": "별루더라구용",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043525.1.26",
          "form": "근데 사실주면잘먹긴합니다",
          "original_form": "근데 사실주면잘먹긴합니다",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043525.1.27",
          "form": "콩나물 무침은 어떠신가요",
          "original_form": "콩나물 무침은 어떠신가요",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100043525.1.28",
          "form": "그럼 콩나물 무침으로 갈까요!!ㅋㅋㅋ",
          "original_form": "그럼 콩나물 무침으로 갈까요!!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043525.1.29",
          "form": "국은 어묵국 끓이면 되겠어요",
          "original_form": "국은 어묵국 끓이면 되겠어요",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043525.1.30",
          "form": "와",
          "original_form": "와",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043525.1.31",
          "form": "최고네요",
          "original_form": "최고네요",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043525.1.32",
          "form": "어묵국..",
          "original_form": "어묵국..",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043525.1.33",
          "form": "안주로딱이죠",
          "original_form": "안주로딱이죠",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043525.1.34",
          "form": "소주랑같이먹을때",
          "original_form": "소주랑같이먹을때",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043525.1.35",
          "form": "어묵국하나면 소주2병 뚝딱이죠",
          "original_form": "어묵국하나면 소주2병 뚝딱이죠",
          "speaker_id": "2",
          "time": "20210907 19:55"
        },
        {
          "id": "MDRW2100043525.1.36",
          "form": "어묵국은 국물떠먹다가",
          "original_form": "어묵국은 국물떠먹다가",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043525.1.37",
          "form": "좀없어지면",
          "original_form": "좀없어지면",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043525.1.38",
          "form": "물 냅다집어넣어서",
          "original_form": "물 냅다집어넣어서",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043525.1.39",
          "form": "다시 끓이면되는...",
          "original_form": "다시 끓이면되는...",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043525.1.40",
          "form": "무한 생성 국",
          "original_form": "무한 생성 국",
          "speaker_id": "2",
          "time": "20210907 19:56"
        },
        {
          "id": "MDRW2100043525.1.41",
          "form": "이야 name2님 주당이시군요",
          "original_form": "이야 &name2&님 주당이시군요",
          "speaker_id": "1",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100043525.1.42",
          "form": "하핳",
          "original_form": "하핳",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043525.1.43",
          "form": "몇년전까지는",
          "original_form": "몇년전까지는",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043525.1.44",
          "form": "위스키도 반병 썁가능이었는데",
          "original_form": "위스키도 반병 썁가능이었는데",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043525.1.45",
          "form": "요즘은 맥주만마셔도 금방 알딸딸해지네요",
          "original_form": "요즘은 맥주만마셔도 금방 알딸딸해지네요",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043525.1.46",
          "form": "간이...",
          "original_form": "간이...",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043525.1.47",
          "form": "안좋아졌나봐요...",
          "original_form": "안좋아졌나봐요...",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043525.1.48",
          "form": "역시 술은 적당히....",
          "original_form": "역시 술은 적당히....",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043525.1.49",
          "form": "술...좋아하시나요...",
          "original_form": "술...좋아하시나요...",
          "speaker_id": "2",
          "time": "20210907 19:59"
        },
        {
          "id": "MDRW2100043525.1.50",
          "form": "저는 알쓰입니다ㅋㅋㅋ 한잔만 마셔도 홍익인간이에용 ㅋㅋㅋ",
          "original_form": "저는 알쓰입니다ㅋㅋㅋ 한잔만 마셔도 홍익인간이에용 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 20:01"
        },
        {
          "id": "MDRW2100043525.1.51",
          "form": "허거걱",
          "original_form": "허거걱",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043525.1.52",
          "form": "오히려 그럼 안마시게되서",
          "original_form": "오히려 그럼 안마시게되서",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043525.1.53",
          "form": "건강에좋은~~~",
          "original_form": "건강에좋은~~~",
          "speaker_id": "2",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100043525.1.54",
          "form": "유흥을 즐기신다면",
          "original_form": "유흥을 즐기신다면",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043525.1.55",
          "form": "술대신에",
          "original_form": "술대신에",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043525.1.56",
          "form": "사이다를마시는편이신가요",
          "original_form": "사이다를마시는편이신가요",
          "speaker_id": "2",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043525.1.57",
          "form": "아뇨...ㅋㅋㅋ 탄산도 안좋아해서 안마셔요..ㅋㅋ 저는 물이 제일 좋아요",
          "original_form": "아뇨...ㅋㅋㅋ 탄산도 안좋아해서 안마셔요..ㅋㅋ 저는 물이 제일 좋아요",
          "speaker_id": "1",
          "time": "20210907 20:06"
        },
        {
          "id": "MDRW2100043525.1.58",
          "form": "허걱",
          "original_form": "허걱",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043525.1.59",
          "form": "짱건강한",
          "original_form": "짱건강한",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043525.1.60",
          "form": "식습관을",
          "original_form": "식습관을",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043525.1.61",
          "form": "갖고계시는군요",
          "original_form": "갖고계시는군요",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043525.1.62",
          "form": "물 많이마시는거좋죠",
          "original_form": "물 많이마시는거좋죠",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043525.1.63",
          "form": "하루에 2l많게는3l마셔야한데요",
          "original_form": "하루에 2l많게는3l마셔야한데요",
          "speaker_id": "2",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100043525.1.64",
          "form": "맞아요 근데 그렇게까진 못먹겠어요 ㅠㅋㅋㅋㅋ",
          "original_form": "맞아요 근데 그렇게까진 못먹겠어요 ㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
