        },
        {
          "id": "MDRW2100028197.1.3",
          "form": "name2님은요?!",
          "original_form": "&name2&님은요?!",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028197.1.4",
          "form": "불고기 버거 맛있겠다 저는 초밥먹었어요",
          "original_form": "불고기 버거 맛있겠다 저는 초밥먹었어요",
          "speaker_id": "2",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028197.1.5",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210902 20:25"
        },
        {
          "id": "MDRW2100028197.1.6",
          "form": "더맛있는거 드셨네요",
          "original_form": "더맛있는거 드셨네요",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028197.1.7",
          "form": "세상에?",
          "original_form": "세상에?",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028197.1.8",
          "form": "세상에!",
          "original_form": "세상에!",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028197.1.9",
          "form": "ㅋㅋㅋㅋㅋ 근데 생각보다 별로였어요 맛이 변했어요",
          "original_form": "ㅋㅋㅋㅋㅋ 근데 생각보다 별로였어요 맛이 변했어요",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028197.1.10",
          "form": "비싸지기만 하고 연어가 질이 떨어져서 슬펐어요",
          "original_form": "비싸지기만 하고 연어가 질이 떨어져서 슬펐어요",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028197.1.11",
          "form": "불고기버거 맛있겠네여...",
          "original_form": "불고기버거 맛있겠네여...",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028197.1.12",
          "form": "아 초밥이 약간",
          "original_form": "아 초밥이 약간",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028197.1.13",
          "form": "덜싱싱하셨나요?",
          "original_form": "덜싱싱하셨나요?",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028197.1.14",
          "form": "그 가게가 맛이 떨어진거 같아요",
          "original_form": "그 가게가 맛이 떨어진거 같아요",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028197.1.15",
          "form": "그래도 먹을때는 열심히 먹고 왔어요 ㅋㅋ",
          "original_form": "그래도 먹을때는 열심히 먹고 왔어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028197.1.16",
          "form": "새로운 맛집을 찾으셔야겠네요,..",
          "original_form": "새로운 맛집을 찾으셔야겠네요,..",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028197.1.17",
          "form": "원래 즐겨가던곳 맛변하는것만큼 속상한게 없는데,,,",
          "original_form": "원래 즐겨가던곳 맛변하는것만큼 속상한게 없는데,,,",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028197.1.18",
          "form": "맞아요 좀 아쉬운데 새로운 곳을 개척해야겠어요 ㅋㅋ",
          "original_form": "맞아요 좀 아쉬운데 새로운 곳을 개척해야겠어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 20:31"
        },
        {
          "id": "MDRW2100028197.1.19",
          "form": "맛집이나 유명카페 가는거 좋아하세요?!",
          "original_form": "맛집이나 유명카페 가는거 좋아하세요?!",
