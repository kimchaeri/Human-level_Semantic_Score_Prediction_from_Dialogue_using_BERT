      "utterance": [
        {
          "id": "MDRW2100041599.1.1",
          "form": "케이크모양의 아이스크림",
          "original_form": "케이크모양의 아이스크림",
          "speaker_id": "2",
          "time": "20210906 12:08"
        },
        {
          "id": "MDRW2100041599.1.2",
          "form": "케이크에 아이스크림이 추가된 형태가 아니라",
          "original_form": "케이크에 아이스크림이 추가된 형태가 아니라",
          "speaker_id": "1",
          "time": "20210906 12:08"
        },
        {
          "id": "MDRW2100041599.1.3",
          "form": "케이크 자체가",
          "original_form": "케이크 자체가",
          "speaker_id": "1",
          "time": "20210906 12:08"
        },
        {
          "id": "MDRW2100041599.1.4",
          "form": "아이스크림으로만 되어있는",
          "original_form": "아이스크림으로만 되어있는",
          "speaker_id": "1",
          "time": "20210906 12:09"
        },
        {
          "id": "MDRW2100041599.1.5",
          "form": "케이크는 한국에서 시작함 ㅇㅇ",
          "original_form": "케이크는 한국에서 시작함 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210906 12:09"
        },
        {
          "id": "MDRW2100041599.1.6",
          "form": "우앙 그랬구나",
          "original_form": "우앙 그랬구나",
          "speaker_id": "2",
          "time": "20210906 12:09"
        },
        {
          "id": "MDRW2100041599.1.7",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "2",
          "time": "20210906 12:09"
        },
        {
          "id": "MDRW2100041599.1.8",
          "form": "난 어렸을때 먹고 요즘엔 안먹어봄",
          "original_form": "난 어렸을때 먹고 요즘엔 안먹어봄",
          "speaker_id": "2",
          "time": "20210906 12:09"
        },
        {
          "id": "MDRW2100041599.1.9",
          "form": "파파는 어때",
          "original_form": "파파는 어때",
          "speaker_id": "2",
          "time": "20210906 12:09"
        },
        {
          "id": "MDRW2100041599.1.10",
          "form": "난 배라는 먹지만",
          "original_form": "난 배라는 먹지만",
          "speaker_id": "1",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100041599.1.11",
          "form": "아이스크림 케이크는",
          "original_form": "아이스크림 케이크는",
          "speaker_id": "1",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100041599.1.12",
          "form": "작은아버지가 집방문 선물했을 때",
          "original_form": "작은아버지가 집방문 선물했을 때",
          "speaker_id": "1",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100041599.1.13",
          "form": "한번 먹어본듯? 두번인가?",
          "original_form": "한번 먹어본듯? 두번인가?",
          "speaker_id": "1",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100041599.1.14",
          "form": "근데 애당초 입맛이 있어서",
          "original_form": "근데 애당초 입맛이 있어서",
          "speaker_id": "1",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100041599.1.15",
          "form": "웅",
          "original_form": "웅",
          "speaker_id": "2",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100041599.1.16",
          "form": "글케 랜덤구성 잘 못먹지",
          "original_form": "글케 랜덤구성 잘 못먹지",
          "speaker_id": "1",
          "time": "20210906 12:10"
        },
        {
          "id": "MDRW2100041599.1.17",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100041599.1.18",
          "form": "뭐 싫어해",
          "original_form": "뭐 싫어해",
          "speaker_id": "2",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100041599.1.19",
          "form": "그냥 샤베트류를 주로 먹으니까",
          "original_form": "그냥 샤베트류를 주로 먹으니까",
          "speaker_id": "1",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100041599.1.20",
          "form": "초콜릿류 잘 안먹는듯",
          "original_form": "초콜릿류 잘 안먹는듯",
          "speaker_id": "1",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100041599.1.21",
          "form": "맛 강한 거 금세 물려서",
          "original_form": "맛 강한 거 금세 물려서",
          "speaker_id": "1",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100041599.1.22",
          "form": "슈팅스타나",
          "original_form": "슈팅스타나",
          "speaker_id": "1",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100041599.1.23",
          "form": "초콜릿류",
          "original_form": "초콜릿류",
          "speaker_id": "1",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100041599.1.24",
          "form": "애들입맛 계통을 잘 못먹는듯",
          "original_form": "애들입맛 계통을 잘 못먹는듯",
          "speaker_id": "1",
          "time": "20210906 12:11"
        },
        {
          "id": "MDRW2100041599.1.25",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100041599.1.26",
          "form": "레인보우",
          "original_form": "레인보우",
          "speaker_id": "2",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100041599.1.27",
          "form": "좋아해?",
          "original_form": "좋아해?",
          "speaker_id": "2",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100041599.1.28",
          "form": "ㅇㅇ 레인보우 샤베트 라즈베리 소르베 블랙 소르베",
          "original_form": "ㅇㅇ 레인보우 샤베트 라즈베리 소르베 블랙 소르베",
          "speaker_id": "1",
          "time": "20210906 12:12"
        },
        {
          "id": "MDRW2100041599.1.29",
          "form": "상큼이 좋아하네",
          "original_form": "상큼이 좋아하네",
          "speaker_id": "2",
          "time": "20210906 12:13"
        },
        {
          "id": "MDRW2100041599.1.30",
          "form": "나는 민초 초코 그린티 엄외 쿠엔크",
          "original_form": "나는 민초 초코 그린티 엄외 쿠엔크",
          "speaker_id": "2",
          "time": "20210906 12:13"
        },
        {
          "id": "MDRW2100041599.1.31",
          "form": "웅ㅇㅇ 레샤도 요즘은 달아서",
          "original_form": "웅ㅇㅇ 레샤도 요즘은 달아서",
          "speaker_id": "1",
          "time": "20210906 12:13"
        },
        {
          "id": "MDRW2100041599.1.32",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210906 12:13"
        },
        {
          "id": "MDRW2100041599.1.33",
          "form": "반대야",
          "original_form": "반대야",
          "speaker_id": "2",
          "time": "20210906 12:13"
        },
        {
          "id": "MDRW2100041599.1.34",
          "form": "쿠엔크는 나도 좋아해 배라엔 안담지만",
          "original_form": "쿠엔크는 나도 좋아해 배라엔 안담지만",
          "speaker_id": "1",
          "time": "20210906 12:13"
        },
        {
          "id": "MDRW2100041599.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:13"
        },
        {
          "id": "MDRW2100041599.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100041599.1.37",
          "form": "민초도 싫어하지 않진 않아",
          "original_form": "민초도 싫어하지 않진 않아",
          "speaker_id": "1",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100041599.1.38",
          "form": "아니 말이이상해",
          "original_form": "아니 말이이상해",
          "speaker_id": "1",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100041599.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100041599.1.40",
          "form": "민초도 안담지만..",
          "original_form": "민초도 안담지만..",
          "speaker_id": "1",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100041599.1.41",
          "form": "요즘엔 그냥 슈퍼마켓에있는 통 사먹어",
          "original_form": "요즘엔 그냥 슈퍼마켓에있는 통 사먹어",
          "speaker_id": "2",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100041599.1.42",
          "form": "구구크러스트",
          "original_form": "구구크러스트",
          "speaker_id": "2",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100041599.1.43",
          "form": "투게더",
          "original_form": "투게더",
          "speaker_id": "2",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100041599.1.44",
          "form": "난 그냥 바 아이스크림",
          "original_form": "난 그냥 바 아이스크림",
          "speaker_id": "1",
          "time": "20210906 12:14"
        },
        {
          "id": "MDRW2100041599.1.45",
          "form": "바는 머먹어",
          "original_form": "바는 머먹어",
          "speaker_id": "2",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100041599.1.46",
          "form": "메로나?",
          "original_form": "메로나?",
          "speaker_id": "2",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100041599.1.47",
          "form": "올때 메로나",
          "original_form": "올때 메로나",
          "speaker_id": "2",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100041599.1.48",
          "form": "빙그레에서",
          "original_form": "빙그레에서",
          "speaker_id": "1",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100041599.1.49",
          "form": "레몬 나와서 그거 먹구",
          "original_form": "레몬 나와서 그거 먹구",
          "speaker_id": "1",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100041599.1.50",
          "form": "쿠앤크 먹구",
          "original_form": "쿠앤크 먹구",
          "speaker_id": "1",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100041599.1.51",
          "form": "걍 대충 보이는 거 사서",
          "original_form": "걍 대충 보이는 거 사서",
          "speaker_id": "1",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100041599.1.52",
          "form": "남는 거ㅜ먹는듯",
          "original_form": "남는 거ㅜ먹는듯",
          "speaker_id": "1",
          "time": "20210906 12:15"
        },
        {
          "id": "MDRW2100041599.1.53",
          "form": "남는 거...ㅋㅋㅋㅋㅋ",
          "original_form": "남는 거...ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:16"
        },
        {
          "id": "MDRW2100041599.1.54",
          "form": "돼지바도",
          "original_form": "돼지바도",
          "speaker_id": "2",
          "time": "20210906 12:16"
        },
        {
          "id": "MDRW2100041599.1.55",
          "form": "아니 모 동생 보고 먹고 싶은 만큼",
          "original_form": "아니 모 동생 보고 먹고 싶은 만큼",
          "speaker_id": "1",
          "time": "20210906 12:16"
        },
        {
          "id": "MDRW2100041599.1.56",
          "form": "먹으라거 하니까ㅋㅋㅋㅋㅋㅋ",
          "original_form": "먹으라거 하니까ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:16"
        }
      ]
