        {
          "id": "MDRW2100028126.1.20",
          "form": "후회중",
          "original_form": "후회중",
          "speaker_id": "2",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100028126.1.21",
          "form": "아 ㅋㅋㅋㅋ 날 따뜻해져야 할텐데요",
          "original_form": "아 ㅋㅋㅋㅋ 날 따뜻해져야 할텐데요",
          "speaker_id": "1",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100028126.1.22",
          "form": "어느 정도 길이로 자르셨어요?",
          "original_form": "어느 정도 길이로 자르셨어요?",
