        },
        {
          "id": "MDRW2100016965.1.2",
          "form": "아주 먹부림",
          "original_form": "아주 먹부림",
          "speaker_id": "2",
          "time": "20210826 16:33"
        },
        {
          "id": "MDRW2100016965.1.3",
          "form": "조와~~",
          "original_form": "조와~~",
          "speaker_id": "1",
          "time": "20210826 16:33"
        },
        {
          "id": "MDRW2100016965.1.4",
          "form": "요기요 오처넌할인쓰",
          "original_form": "요기요 오처넌할인쓰",
          "speaker_id": "2",
          "time": "20210826 16:33"
        },
        {
          "id": "MDRW2100016965.1.5",
          "form": "왕창 먹어버리자",
          "original_form": "왕창 먹어버리자",
          "speaker_id": "1",
          "time": "20210826 16:33"
        },
        {
          "id": "MDRW2100016965.1.6",
          "form": "난 베라 사고",
          "original_form": "난 베라 사고",
          "speaker_id": "1",
          "time": "20210826 16:33"
        },
        {
          "id": "MDRW2100016965.1.7",
          "form": "커피랑 먹어야지",
          "original_form": "커피랑 먹어야지",
          "speaker_id": "2",
          "time": "20210826 16:33"
        },
        {
          "id": "MDRW2100016965.1.8",
          "form": "나가기귀찮지만ㅜ",
          "original_form": "나가기귀찮지만ㅜ",
          "speaker_id": "2",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016965.1.9",
          "form": "카레 장보러 마트가야즤",
          "original_form": "카레 장보러 마트가야즤",
          "speaker_id": "1",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016965.1.10",
          "form": "카레?",
          "original_form": "카레?",
          "speaker_id": "2",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016965.1.11",
          "form": "오늘 저녁은",
          "original_form": "오늘 저녁은",
          "speaker_id": "1",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016965.1.12",
          "form": "카레~",
          "original_form": "카레~",
          "speaker_id": "1",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016965.1.13",
          "form": "3분카레?",
          "original_form": "3분카레?",
          "speaker_id": "2",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016965.1.14",
          "form": "룰루",
          "original_form": "룰루",
          "speaker_id": "1",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016965.1.15",
          "form": "카레용 돼지고기랑",
          "original_form": "카레용 돼지고기랑",
          "speaker_id": "1",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016965.1.16",
          "form": "우리집도 카레해놨는데",
          "original_form": "우리집도 카레해놨는데",
          "speaker_id": "2",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016965.1.17",
          "form": "난 쳐다도 안봄^^..",
          "original_form": "난 쳐다도 안봄^^..",
          "speaker_id": "2",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016965.1.18",
          "form": "넣어서 먹을라구",
          "original_form": "넣어서 먹을라구",
          "speaker_id": "1",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016965.1.19",
          "form": "청국장은 4일내내 먹어도 맛있는데",
          "original_form": "청국장은 4일내내 먹어도 맛있는데",
          "speaker_id": "2",
          "time": "20210826 16:34"
        },
        {
          "id": "MDRW2100016965.1.20",
          "form": "고기잔뜩카레.. 난 일본식 카레가 취향인듯",
          "original_form": "고기잔뜩카레.. 난 일본식 카레가 취향인듯",
          "speaker_id": "2",
          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016965.1.21",
          "form": "그래서 청국장만 조지고있어",
          "original_form": "그래서 청국장만 조지고있어",
          "speaker_id": "2",
          "time": "20210826 16:35"
        },
