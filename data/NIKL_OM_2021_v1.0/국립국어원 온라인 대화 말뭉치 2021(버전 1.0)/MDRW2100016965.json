          "time": "20210826 16:35"
        },
        {
          "id": "MDRW2100016965.1.23",
          "form": "글쿠먼",
          "original_form": "글쿠먼",
          "speaker_id": "1",
          "time": "20210826 16:36"
        },
        {
          "id": "MDRW2100016965.1.24",
