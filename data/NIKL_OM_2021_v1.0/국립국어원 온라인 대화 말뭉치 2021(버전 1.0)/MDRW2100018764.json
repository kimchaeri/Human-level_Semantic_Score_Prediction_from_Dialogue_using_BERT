      "utterance": [
        {
          "id": "MDRW2100018764.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210827 11:54"
        },
        {
          "id": "MDRW2100018764.1.2",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210827 11:54"
        },
        {
          "id": "MDRW2100018764.1.3",
          "form": "네 금붕어도 반려동물일까요?",
          "original_form": "네 금붕어도 반려동물일까요?",
          "speaker_id": "2",
          "time": "20210827 11:54"
        },
        {
          "id": "MDRW2100018764.1.4",
          "form": "그럼요",
          "original_form": "그럼요",
          "speaker_id": "1",
          "time": "20210827 11:54"
        },
        {
          "id": "MDRW2100018764.1.5",
          "form": "금붕어 몇마리 키우세요??",
          "original_form": "금붕어 몇마리 키우세요??",
          "speaker_id": "1",
          "time": "20210827 11:54"
        },
        {
          "id": "MDRW2100018764.1.6",
          "form": "3마리요! 키워본적있으세요?",
          "original_form": "3마리요! 키워본적있으세요?",
          "speaker_id": "2",
          "time": "20210827 11:55"
        },
        {
          "id": "MDRW2100018764.1.7",
          "form": "넵 전 엄청 어릴때",
          "original_form": "넵 전 엄청 어릴때",
          "speaker_id": "1",
          "time": "20210827 11:55"
        },
        {
          "id": "MDRW2100018764.1.8",
          "form": "4마리키워봤어요 ㅋㅋ",
          "original_form": "4마리키워봤어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:55"
        },
        {
          "id": "MDRW2100018764.1.9",
          "form": "지금은 반려동물 키우세요?",
          "original_form": "지금은 반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210827 11:55"
        },
        {
          "id": "MDRW2100018764.1.10",
          "form": "지금은 강아지랑 살구있습니당",
          "original_form": "지금은 강아지랑 살구있습니당",
          "speaker_id": "1",
          "time": "20210827 11:56"
        },
        {
          "id": "MDRW2100018764.1.11",
          "form": "갈색푸들이에요",
          "original_form": "갈색푸들이에요",
          "speaker_id": "1",
          "time": "20210827 11:56"
        },
        {
          "id": "MDRW2100018764.1.12",
          "form": "강아지 이름은 초코인가요?",
          "original_form": "강아지 이름은 초코인가요?",
          "speaker_id": "2",
          "time": "20210827 11:56"
        },
        {
          "id": "MDRW2100018764.1.13",
          "form": "아쉽게도",
          "original_form": "아쉽게도",
          "speaker_id": "1",
          "time": "20210827 11:56"
        },
        {
          "id": "MDRW2100018764.1.14",
          "form": "초코는 아니고",
          "original_form": "초코는 아니고",
          "speaker_id": "1",
          "time": "20210827 11:56"
        },
        {
          "id": "MDRW2100018764.1.15",
          "form": "name3입니다 ㅋㅋㅋ",
          "original_form": "&name3&입니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:57"
        },
        {
          "id": "MDRW2100018764.1.16",
          "form": "특이한 이름이네요! 빈둥빈둥거려서 name3 인가요?",
          "original_form": "특이한 이름이네요! 빈둥빈둥거려서 &name3& 인가요?",
          "speaker_id": "2",
          "time": "20210827 11:57"
        },
        {
          "id": "MDRW2100018764.1.17",
          "form": "ㅋㅋㅋㅋ빈둥거리라고 name3라고 지어줬는데",
          "original_form": "ㅋㅋㅋㅋ빈둥거리라고 &name3&라고 지어줬는데",
          "speaker_id": "1",
          "time": "20210827 11:58"
        },
        {
          "id": "MDRW2100018764.1.18",
          "form": "닉값하는 강아지입니다...",
          "original_form": "닉값하는 강아지입니다...",
          "speaker_id": "1",
          "time": "20210827 11:58"
        },
        {
          "id": "MDRW2100018764.1.19",
          "form": "매일매일 빈둥대요ㅠㅠ",
          "original_form": "매일매일 빈둥대요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 11:58"
        },
        {
          "id": "MDRW2100018764.1.20",
          "form": "댕댕팔자 상팔자네요ㅠㅠ",
          "original_form": "댕댕팔자 상팔자네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 11:59"
        },
        {
          "id": "MDRW2100018764.1.21",
          "form": "하지만 이미 귀여운걸로 자기 몫은 다 하고있지않나요?",
          "original_form": "하지만 이미 귀여운걸로 자기 몫은 다 하고있지않나요?",
          "speaker_id": "2",
          "time": "20210827 12:00"
        },
        {
          "id": "MDRW2100018764.1.22",
          "form": "ㅠㅠ맞아요 ㅋㅋㅋㅋ",
          "original_form": "ㅠㅠ맞아요 ㅋㅋㅋㅋ",
