      "utterance": [
        {
          "id": "MDRW2100016924.1.1",
          "form": "미래의 반려견 이름까지 생각하셨나요?",
          "original_form": "미래의 반려견 이름까지 생각하셨나요?",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016924.1.2",
          "form": "거의 털 색깔로 이름 지어요ㅡㅋ",
          "original_form": "거의 털 색깔로 이름 지어요ㅡㅋ",
          "speaker_id": "2",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016924.1.3",
          "form": "name3이 name4이 name5이",
          "original_form": "&name3&이 &name4&이 &name5&이",
          "speaker_id": "2",
          "time": "20210826 16:18"
        },
        {
          "id": "MDRW2100016924.1.4",
          "form": "털 알러지 같은거 있나요?",
          "original_form": "털 알러지 같은거 있나요?",
          "speaker_id": "2",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100016924.1.5",
          "form": "아뇨 키워보진 않았지만 없을거같아요",
          "original_form": "아뇨 키워보진 않았지만 없을거같아요",
          "speaker_id": "1",
          "time": "20210826 18:06"
        },
        {
          "id": "MDRW2100016924.1.6",
          "form": "만약 키울 수 있다면 어떤 동물 키워보고 싶나요",
          "original_form": "만약 키울 수 있다면 어떤 동물 키워보고 싶나요",
          "speaker_id": "2",
          "time": "20210826 19:02"
        },
        {
          "id": "MDRW2100016924.1.7",
          "form": "강아지가 귀여워서 좋을거같아요 ㅎ",
          "original_form": "강아지가 귀여워서 좋을거같아요 ㅎ",
          "speaker_id": "1",
          "time": "20210826 19:19"
        },
        {
          "id": "MDRW2100016924.1.8",
          "form": "강아지.귀엽죠 폴폴 뛰어다니는 것도 이쁘고",
          "original_form": "강아지.귀엽죠 폴폴 뛰어다니는 것도 이쁘고",
          "speaker_id": "2",
          "time": "20210826 21:27"
        },
        {
          "id": "MDRW2100016924.1.9",
          "form": "말만 들어도 귀엽네요 폴폴",
          "original_form": "말만 들어도 귀엽네요 폴폴",
          "speaker_id": "1",
          "time": "20210826 22:41"
        },
        {
          "id": "MDRW2100016924.1.10",
          "form": "강아지때는 귀가 아래로 접혀있는데 뛰면 접힌 귀가 펄럭펄럭 거려서 엄청 귀여워요",
          "original_form": "강아지때는 귀가 아래로 접혀있는데 뛰면 접힌 귀가 펄럭펄럭 거려서 엄청 귀여워요",
          "speaker_id": "2",
          "time": "20210826 23:49"
        },
        {
          "id": "MDRW2100016924.1.11",
          "form": "이름은 언제쯤부터 알아듣나요?",
          "original_form": "이름은 언제쯤부터 알아듣나요?",
          "speaker_id": "1",
          "time": "20210827 10:12"
        },
        {
          "id": "MDRW2100016924.1.12",
          "form": "부르면 쪼르르 오죠. \r\n쪼꼬미때 부터 그런거 같아요",
          "original_form": "부르면 쪼르르 오죠. \r\n쪼꼬미때 부터 그런거 같아요",
          "speaker_id": "2",
          "time": "20210827 10:22"
        },
        {
          "id": "MDRW2100016924.1.13",
          "form": "하나하나 알아간다는게 키우는 맛이겠어요",
          "original_form": "하나하나 알아간다는게 키우는 맛이겠어요",
          "speaker_id": "1",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100016924.1.14",
          "form": "네 아가때 부터 키우면 넘 좋죠 귀엽고",
          "original_form": "네 아가때 부터 키우면 넘 좋죠 귀엽고",
          "speaker_id": "2",
          "time": "20210827 20:46"
        },
        {
          "id": "MDRW2100016924.1.15",
          "form": "주변에서 달라고 하겠어요",
          "original_form": "주변에서 달라고 하겠어요",
          "speaker_id": "1",
          "time": "20210827 22:33"
        },
        {
          "id": "MDRW2100016924.1.16",
          "form": "못주죠 ㅠㅠ 귀요미를 안돼요",
          "original_form": "못주죠 ㅠㅠ 귀요미를 안돼요",
          "speaker_id": "2",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100016924.1.17",
          "form": "구경하고싶네요 ㅎ",
          "original_form": "구경하고싶네요 ㅎ",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100016924.1.18",
          "form": "울 멍이요?",
          "original_form": "울 멍이요?",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100016924.1.19",
          "form": "넹 ㅋ",
          "original_form": "넹 ㅋ",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100016924.1.20",
          "form": "지금은 태어난지 1년 정도 돼서 막 귀욤하진 않아요 ㅋ",
          "original_form": "지금은 태어난지 1년 정도 돼서 막 귀욤하진 않아요 ㅋ",
          "speaker_id": "2",
          "time": "20210827 23:16"
        }
