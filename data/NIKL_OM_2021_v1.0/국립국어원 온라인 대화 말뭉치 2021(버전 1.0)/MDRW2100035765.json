      "utterance": [
        {
          "id": "MDRW2100035765.1.1",
          "form": "아 궁금하여라 ㅎㅎ",
          "original_form": "아 궁금하여라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:07"
        },
        {
          "id": "MDRW2100035765.1.2",
          "form": "긴장되서 이불덮었어요 ㅋㅋㅋㅋ",
          "original_form": "긴장되서 이불덮었어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:13"
        },
        {
          "id": "MDRW2100035765.1.3",
          "form": "어머나 ㅎㅎㅎ",
          "original_form": "어머나 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100035765.1.4",
          "form": "예전에는 그래도 저도 밤에 전참시나 나혼산 챙겨본것 같은데",
          "original_form": "예전에는 그래도 저도 밤에 전참시나 나혼산 챙겨본것 같은데",
          "speaker_id": "2",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100035765.1.5",
          "form": "이제 그것도 안보게 되네여ㅠㅠ",
          "original_form": "이제 그것도 안보게 되네여ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 01:15"
        },
        {
          "id": "MDRW2100035765.1.6",
          "form": "저는 이렇게 까지 찾아본적은 없는데 이시간까지 보고있네요 ㅎㅎ",
          "original_form": "저는 이렇게 까지 찾아본적은 없는데 이시간까지 보고있네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:18"
        },
        {
          "id": "MDRW2100035765.1.7",
          "form": "이번이 마지막 시즌일까요ㅠㅠ",
          "original_form": "이번이 마지막 시즌일까요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 01:19"
        },
        {
          "id": "MDRW2100035765.1.8",
          "form": "피오군대가면..",
          "original_form": "피오군대가면..",
          "speaker_id": "2",
          "time": "20210830 01:19"
        },
        {
          "id": "MDRW2100035765.1.9",
          "form": "피오군대가도 사실 지장은 없을것 같아요 ㅎㅎ",
          "original_form": "피오군대가도 사실 지장은 없을것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100035765.1.10",
          "form": "신동이나 유병재가 빠지지 않는이상 ㅋㅋㅋㅋ",
          "original_form": "신동이나 유병재가 빠지지 않는이상 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100035765.1.11",
          "form": "좀 팩폭이긴한데..",
          "original_form": "좀 팩폭이긴한데..",
          "speaker_id": "1",
          "time": "20210830 01:21"
        },
        {
          "id": "MDRW2100035765.1.12",
          "form": "아 빠쟈도 할라나요 ㅎㅎ",
          "original_form": "아 빠쟈도 할라나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:22"
        },
        {
          "id": "MDRW2100035765.1.13",
          "form": "그러지않을까요? 멤버 그대로 간다고는 했지만 핵심멤버도 아니구... 시즌은 계속 되어야합니다!!! ㅋㅋㅋ",
          "original_form": "그러지않을까요? 멤버 그대로 간다고는 했지만 핵심멤버도 아니구... 시즌은 계속 되어야합니다!!! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100035765.1.14",
          "form": "벌써 다음시즌이 기대되는 1인",
          "original_form": "벌써 다음시즌이 기대되는 1인",
          "speaker_id": "1",
          "time": "20210830 01:23"
        },
        {
          "id": "MDRW2100035765.1.15",
          "form": "아 그럼 계속 하는가봐요 ㅎㅎ",
          "original_form": "아 그럼 계속 하는가봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:27"
        },
        {
          "id": "MDRW2100035765.1.16",
          "form": "솔직히 신동이랑 유병재가 다풀고",
          "original_form": "솔직히 신동이랑 유병재가 다풀고",
          "speaker_id": "2",
          "time": "20210830 01:27"
        },
        {
          "id": "MDRW2100035765.1.17",
          "form": "나머지 얻어걸리고 ㅎㅎ 잘찾고",
          "original_form": "나머지 얻어걸리고 ㅎㅎ 잘찾고",
          "speaker_id": "2",
          "time": "20210830 01:27"
        },
        {
          "id": "MDRW2100035765.1.18",
          "form": "그렇지않을까요..? 저는 피오빠진다고 안할거란 생각을 안해봤어요  ㅎㅎㅎ",
          "original_form": "그렇지않을까요..? 저는 피오빠진다고 안할거란 생각을 안해봤어요  ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:27"
        },
        {
          "id": "MDRW2100035765.1.19",
          "form": "김종민은 진짜 신기해요 ㅋㅋㅋㅋ 어리버리한거 같은데 엄청 잘찾고",
          "original_form": "김종민은 진짜 신기해요 ㅋㅋㅋㅋ 어리버리한거 같은데 엄청 잘찾고",
          "speaker_id": "1",
          "time": "20210830 01:28"
        },
        {
          "id": "MDRW2100035765.1.20",
          "form": "진짜 발견왕",
          "original_form": "진짜 발견왕",
          "speaker_id": "2",
          "time": "20210830 01:28"
        },
        {
          "id": "MDRW2100035765.1.21",
          "form": "김발견이 괜히 붙은게 아닌듯",
          "original_form": "김발견이 괜히 붙은게 아닌듯",
          "speaker_id": "2",
          "time": "20210830 01:28"
        },
        {
          "id": "MDRW2100035765.1.22",
          "form": "그쵸 ㅋㅋㅋㅋ 그리고 급박할때는 제일 잘하는것 같아요 ㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋ 그리고 급박할때는 제일 잘하는것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:30"
        },
        {
          "id": "MDRW2100035765.1.23",
          "form": "신기하게 그렇더라구요 중요한 순간에",
          "original_form": "신기하게 그렇더라구요 중요한 순간에",
