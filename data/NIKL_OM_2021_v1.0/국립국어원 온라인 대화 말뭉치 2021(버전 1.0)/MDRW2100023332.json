      "utterance": [
        {
          "id": "MDRW2100023332.1.1",
          "form": "싸이월드 하셨나요?",
          "original_form": "싸이월드 하셨나요?",
          "speaker_id": "2",
          "time": "20210830 00:14"
        },
        {
          "id": "MDRW2100023332.1.2",
          "form": "네 저 진짜 열심히 했었어요!!",
          "original_form": "네 저 진짜 열심히 했었어요!!",
          "speaker_id": "1",
          "time": "20210830 00:14"
        },
        {
          "id": "MDRW2100023332.1.3",
          "form": "도토리 좀 사셨나보네요~",
          "original_form": "도토리 좀 사셨나보네요~",
          "speaker_id": "2",
          "time": "20210830 00:14"
        },
        {
          "id": "MDRW2100023332.1.4",
          "form": "네네 ㅎㅎ",
          "original_form": "네네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 00:14"
        },
        {
          "id": "MDRW2100023332.1.5",
          "form": "음악이랑 스킨 사는걸 좋아했어요",
          "original_form": "음악이랑 스킨 사는걸 좋아했어요",
          "speaker_id": "1",
          "time": "20210830 00:14"
        },
        {
          "id": "MDRW2100023332.1.6",
          "form": "싸이월드 음악 뭐였는지 물어봐도 될까요?",
          "original_form": "싸이월드 음악 뭐였는지 물어봐도 될까요?",
          "speaker_id": "2",
          "time": "20210830 00:15"
        },
        {
          "id": "MDRW2100023332.1.7",
          "form": "저는 허밍어반스테레오",
          "original_form": "저는 허밍어반스테레오",
          "speaker_id": "1",
          "time": "20210830 00:28"
        },
        {
          "id": "MDRW2100023332.1.8",
          "form": "음악이 bgm이었어요~",
          "original_form": "음악이 bgm이었어요~",
          "speaker_id": "1",
          "time": "20210830 00:28"
        },
        {
          "id": "MDRW2100023332.1.9",
          "form": "name2은요???",
          "original_form": "&name2&은요???",
          "speaker_id": "1",
          "time": "20210830 00:28"
        },
        {
          "id": "MDRW2100023332.1.10",
          "form": "저는",
          "original_form": "저는",
          "speaker_id": "2",
          "time": "20210830 00:33"
        },
        {
          "id": "MDRW2100023332.1.11",
          "form": "싸이월드에 도토리를 안썼네요",
          "original_form": "싸이월드에 도토리를 안썼네요",
          "speaker_id": "2",
          "time": "20210830 00:33"
        },
        {
          "id": "MDRW2100023332.1.12",
          "form": "그러셨구나",
          "original_form": "그러셨구나",
          "speaker_id": "1",
          "time": "20210830 00:33"
        },
        {
          "id": "MDRW2100023332.1.13",
          "form": "대신",
          "original_form": "대신",
          "speaker_id": "2",
          "time": "20210830 00:33"
        },
        {
          "id": "MDRW2100023332.1.14",
          "form": "주변 사람들 싸이월드는 자주 들어갔어요 ㅎㅎ",
          "original_form": "주변 사람들 싸이월드는 자주 들어갔어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 00:34"
        },
        {
          "id": "MDRW2100023332.1.15",
          "form": "주변 분들이 좋아하셨겠어요~!",
          "original_form": "주변 분들이 좋아하셨겠어요~!",
          "speaker_id": "1",
          "time": "20210830 00:34"
        },
        {
          "id": "MDRW2100023332.1.16",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210830 00:34"
        },
        {
          "id": "MDRW2100023332.1.17",
          "form": "싸이월드가 망한 이유가",
          "original_form": "싸이월드가 망한 이유가",
          "speaker_id": "2",
          "time": "20210830 00:34"
        },
        {
          "id": "MDRW2100023332.1.18",
          "form": "도토리를",
          "original_form": "도토리를",
          "speaker_id": "2",
          "time": "20210830 00:35"
        },
        {
          "id": "MDRW2100023332.1.19",
          "form": "너무 비싸게 받아서",
          "original_form": "너무 비싸게 받아서",
          "speaker_id": "2",
          "time": "20210830 00:35"
        },
        {
          "id": "MDRW2100023332.1.20",
          "form": "그렇다고 하더라구요",
          "original_form": "그렇다고 하더라구요",
          "speaker_id": "2",
          "time": "20210830 00:35"
        },
        {
          "id": "MDRW2100023332.1.21",
          "form": "아 저도 그 말 들었어요!!!",
          "original_form": "아 저도 그 말 들었어요!!!",
          "speaker_id": "1",
          "time": "20210830 00:35"
        },
        {
          "id": "MDRW2100023332.1.22",
          "form": "그런데 이번에 부활하면",
          "original_form": "그런데 이번에 부활하면",
          "speaker_id": "1",
          "time": "20210830 00:35"
        },
        {
          "id": "MDRW2100023332.1.23",
          "form": "좀 달라지지 않을까요?",
          "original_form": "좀 달라지지 않을까요?",
          "speaker_id": "1",
          "time": "20210830 00:35"
        },
        {
          "id": "MDRW2100023332.1.24",
          "form": "하긴 그건 그래요",
          "original_form": "하긴 그건 그래요",
          "speaker_id": "2",
          "time": "20210830 00:35"
        },
        {
          "id": "MDRW2100023332.1.25",
          "form": "그리고",
          "original_form": "그리고",
          "speaker_id": "2",
          "time": "20210830 00:35"
        },
        {
          "id": "MDRW2100023332.1.26",
          "form": "요즘은 무료로도",
          "original_form": "요즘은 무료로도",
          "speaker_id": "2",
          "time": "20210830 00:35"
        },
        {
          "id": "MDRW2100023332.1.27",
          "form": "즐길 수 있는 플랫폼이 많으니까요",
          "original_form": "즐길 수 있는 플랫폼이 많으니까요",
          "speaker_id": "2",
          "time": "20210830 00:35"
        },
        {
          "id": "MDRW2100023332.1.28",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210830 00:36"
        },
        {
          "id": "MDRW2100023332.1.29",
          "form": "심지어 수익을 낼 수도 있죠!!!",
          "original_form": "심지어 수익을 낼 수도 있죠!!!",
