      "utterance": [
        {
          "id": "MDRW2100007362.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007362.1.2",
          "form": "한번도 안해봤는데ㅋㅋㅋㅋㅋ",
          "original_form": "한번도 안해봤는데ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007362.1.3",
          "form": "무슨 게임이 재미난가요?",
          "original_form": "무슨 게임이 재미난가요?",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007362.1.4",
          "form": "쉬운 거로요 접근성 높고",
          "original_form": "쉬운 거로요 접근성 높고",
          "speaker_id": "2",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007362.1.5",
          "form": "어 저도 잘 안 하긴 하는데, 저는 막 혼자 하는 걸 좋아해요 ㅋㅋㅋ 요즘은 스타듀밸리라는 게임을 가끔 해요 농장 키우는 게임이에요!",
          "original_form": "어 저도 잘 안 하긴 하는데, 저는 막 혼자 하는 걸 좋아해요 ㅋㅋㅋ 요즘은 스타듀밸리라는 게임을 가끔 해요 농장 키우는 게임이에요!",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007362.1.6",
          "form": "게임은 하는 것보단 보는 게 재미있더라고요",
          "original_form": "게임은 하는 것보단 보는 게 재미있더라고요",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007362.1.7",
          "form": "오 농장 키우기ㅋㅋㅋㅋ",
          "original_form": "오 농장 키우기ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007362.1.8",
          "form": "기여울 거 같아요",
          "original_form": "기여울 거 같아요",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007362.1.9",
          "form": "아 아주 옛날에 커피 만드는 게임한 적 있어요",
          "original_form": "아 아주 옛날에 커피 만드는 게임한 적 있어요",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007362.1.10",
          "form": "아 커피숍 운영인가ㅋㅋㅋㅋ",
          "original_form": "아 커피숍 운영인가ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007362.1.11",
          "form": "아아아 아이러브커피!",
          "original_form": "아아아 아이러브커피!",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007362.1.12",
          "form": "그거 정말 많은 사람이 했었죠",
          "original_form": "그거 정말 많은 사람이 했었죠",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007362.1.13",
          "form": "카카오 연동해서 친구랑도 했던 것 같은데",
          "original_form": "카카오 연동해서 친구랑도 했던 것 같은데",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007362.1.14",
          "form": "아아 맞다ㅋㅋㅋㅋ",
          "original_form": "아아 맞다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007362.1.15",
          "form": "맨날 커피 태워먹고ㅋㅋㅋㅋㅋ",
          "original_form": "맨날 커피 태워먹고ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007362.1.16",
          "form": "너무 옛날 얘기네요ㅋㅋ",
          "original_form": "너무 옛날 얘기네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007362.1.17",
          "form": "최근에는 비슷한 아이러브커피n이라는 게임이 있어서 했었는데 막 커피숍끼리 배틀도 뜨고 하는 게임이라",
          "original_form": "최근에는 비슷한 아이러브커피n이라는 게임이 있어서 했었는데 막 커피숍끼리 배틀도 뜨고 하는 게임이라",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007362.1.18",
          "form": "재미없어서 금방 지웠어요",
          "original_form": "재미없어서 금방 지웠어요",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007362.1.19",
          "form": "배틀까지ㄷㄷㄷㄷㄷ",
          "original_form": "배틀까지ㄷㄷㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007362.1.20",
          "form": "전 게임 영상 많이 봐요!",
          "original_form": "전 게임 영상 많이 봐요!",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007362.1.21",
          "form": "유튭으로요?",
          "original_form": "유튭으로요?",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007362.1.22",
          "form": "그럼 막 전략게임 이런 거 보세요?",
          "original_form": "그럼 막 전략게임 이런 거 보세요?",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007362.1.23",
          "form": "전략게임도 보고 잘하기 위해서 보는 건 아니고 그냥 세상엔 다양한 게임이 있구나~하는 걸 보는 거라",
          "original_form": "전략게임도 보고 잘하기 위해서 보는 건 아니고 그냥 세상엔 다양한 게임이 있구나~하는 걸 보는 거라",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007362.1.24",
          "form": "시뮬레이션 하는? 요리하는 게임이나",
          "original_form": "시뮬레이션 하는? 요리하는 게임이나",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007362.1.25",
          "form": "리그오브레전드도 가끔 봐요",
          "original_form": "리그오브레전드도 가끔 봐요",
          "speaker_id": "1",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007362.1.26",
          "form": "오오 요리하는 게임도 있구나",
          "original_form": "오오 요리하는 게임도 있구나",
          "speaker_id": "2",
          "time": "20210822 01:42"
        },
        {
          "id": "MDRW2100007362.1.27",
          "form": "하긴 친구 보니까 뭔 과일 수확하던데ㅋㅋㅋㅋ",
          "original_form": "하긴 친구 보니까 뭔 과일 수확하던데ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007362.1.28",
          "form": "과일을 수확해요? ㅋㅋㅋㅋ 우와",
          "original_form": "과일을 수확해요? ㅋㅋㅋㅋ 우와",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007362.1.29",
          "form": "그런 농장같은 거도 꾸미는 거 보는 재미도 있을 거 같아요",
          "original_form": "그런 농장같은 거도 꾸미는 거 보는 재미도 있을 거 같아요",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007362.1.30",
          "form": "저는 어른되면 게임을 안 하는 줄 알았거든요",
          "original_form": "저는 어른되면 게임을 안 하는 줄 알았거든요",
          "speaker_id": "1",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007362.1.31",
          "form": "더 마니 하세요?",
          "original_form": "더 마니 하세요?",
          "speaker_id": "2",
          "time": "20210822 01:43"
        },
        {
          "id": "MDRW2100007362.1.32",
          "form": "고삐가 풀린 것 같아요",
          "original_form": "고삐가 풀린 것 같아요",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007362.1.33",
          "form": "학생 땐 학교생활 때문에 잘 못하다가",
          "original_form": "학생 땐 학교생활 때문에 잘 못하다가",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007362.1.34",
          "form": "직장을 갖고 나니까",
          "original_form": "직장을 갖고 나니까",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007362.1.35",
          "form": "ㅋㅋㅋㅋㅋ 누가 제어해주질 않으니",
          "original_form": "ㅋㅋㅋㅋㅋ 누가 제어해주질 않으니",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007362.1.36",
          "form": "내 맘대로 하면 되니까ㅋㅋㅋㅋㅋ",
          "original_form": "내 맘대로 하면 되니까ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007362.1.37",
          "form": "보통 언제 게임 마니 하시는데요?",
          "original_form": "보통 언제 게임 마니 하시는데요?",
          "speaker_id": "2",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007362.1.38",
          "form": "쉬는 시간과 주말이 온전히 제 것이 된 느낌이라 ㅋㅋㅋㅋ 막 게임 하나에 빠지면 하루종일 하네요",
          "original_form": "쉬는 시간과 주말이 온전히 제 것이 된 느낌이라 ㅋㅋㅋㅋ 막 게임 하나에 빠지면 하루종일 하네요",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007362.1.39",
          "form": "밤에 많이 하죠!",
          "original_form": "밤에 많이 하죠!",
          "speaker_id": "1",
          "time": "20210822 01:44"
        },
        {
          "id": "MDRW2100007362.1.40",
          "form": "그래서 늦잠자구요",
          "original_form": "그래서 늦잠자구요",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007362.1.41",
          "form": "아아ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아아ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007362.1.42",
          "form": "최근에 리듬게임 하나 받아서 또 버닝하고 있어요",
          "original_form": "최근에 리듬게임 하나 받아서 또 버닝하고 있어요",
          "speaker_id": "1",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007362.1.43",
          "form": "리듬게임 같은 건 잼날 거 같아요ㅋㅋㅋ",
          "original_form": "리듬게임 같은 건 잼날 거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007362.1.44",
          "form": "손구락 운동도 되규",
          "original_form": "손구락 운동도 되규",
          "speaker_id": "2",
          "time": "20210822 01:45"
        },
        {
          "id": "MDRW2100007362.1.45",
          "form": "요즘은 리듬게임도",
          "original_form": "요즘은 리듬게임도",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007362.1.46",
          "form": "카드 모아야 하는 거 아세요??",
          "original_form": "카드 모아야 하는 거 아세요??",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007362.1.47",
          "form": "카드?",
          "original_form": "카드?",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007362.1.48",
          "form": "ㅋㅋㅋㅋ 현질 해야하는 건가요?",
          "original_form": "ㅋㅋㅋㅋ 현질 해야하는 건가요?",
          "speaker_id": "2",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007362.1.49",
          "form": "네네 카드를 모아야 점수가 높아져요",
          "original_form": "네네 카드를 모아야 점수가 높아져요",
          "speaker_id": "1",
          "time": "20210822 01:46"
        },
        {
          "id": "MDRW2100007362.1.50",
          "form": "아아ㅋㅋㅋㅋㅋ",
          "original_form": "아아ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007362.1.51",
          "form": "C~R까지 카드가 있는데",
          "original_form": "C~R까지 카드가 있는데",
          "speaker_id": "1",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007362.1.52",
          "form": "현질도 마니 하세요?",
          "original_form": "현질도 마니 하세요?",
          "speaker_id": "2",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007362.1.53",
          "form": "어 저는 요즘은 잘 안해요 할 땐 엄청 했어요",
          "original_form": "어 저는 요즘은 잘 안해요 할 땐 엄청 했어요",
          "speaker_id": "1",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007362.1.54",
          "form": "현질 하시는 편인가요?",
          "original_form": "현질 하시는 편인가요?",
          "speaker_id": "1",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007362.1.55",
          "form": "아 함도 안해봤어요ㅋㅋㅋㅋ",
          "original_form": "아 함도 안해봤어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:47"
        },
        {
          "id": "MDRW2100007362.1.56",
          "form": "겜이 아직 낯선 분야라ㅋㅋㅋ",
          "original_form": "겜이 아직 낯선 분야라ㅋㅋㅋ",
