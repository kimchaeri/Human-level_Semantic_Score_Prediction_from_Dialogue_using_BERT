      "utterance": [
        {
          "id": "MDRW2100018428.1.1",
          "form": "name2~~ 요즘 게임 뭐해",
          "original_form": "&name2&~~ 요즘 게임 뭐해",
          "speaker_id": "1",
          "time": "20210827 01:41"
        },
        {
          "id": "MDRW2100018428.1.2",
          "form": "나 요즘 게임 안 해~",
          "original_form": "나 요즘 게임 안 해~",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018428.1.3",
          "form": "하지만 게임하는 유튜버는 봐",
          "original_form": "하지만 게임하는 유튜버는 봐",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018428.1.4",
          "form": "우리 깜찍이는 무슨 게임해?^^",
          "original_form": "우리 깜찍이는 무슨 게임해?^^",
          "speaker_id": "2",
          "time": "20210827 01:42"
        },
        {
          "id": "MDRW2100018428.1.5",
          "form": "파이널 판타지 14",
          "original_form": "파이널 판타지 14",
          "speaker_id": "1",
          "time": "20210827 01:43"
        },
        {
          "id": "MDRW2100018428.1.6",
          "form": "우리 같이 할래?",
          "original_form": "우리 같이 할래?",
          "speaker_id": "1",
          "time": "20210827 01:43"
        },
        {
          "id": "MDRW2100018428.1.7",
          "form": "싫어요~><",
          "original_form": "싫어요~><",
          "speaker_id": "2",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018428.1.8",
          "form": "저는 오버워치랑",
          "original_form": "저는 오버워치랑",
          "speaker_id": "2",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018428.1.9",
          "form": "카트라이더랑 크아만 할꺼에염",
          "original_form": "카트라이더랑 크아만 할꺼에염",
          "speaker_id": "2",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018428.1.10",
          "form": "왜 옛날 게임만 해...???",
          "original_form": "왜 옛날 게임만 해...???",
          "speaker_id": "1",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018428.1.11",
          "form": "시대를 따라가자!",
          "original_form": "시대를 따라가자!",
          "speaker_id": "1",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018428.1.12",
          "form": "옛날 게임이지만",
          "original_form": "옛날 게임이지만",
          "speaker_id": "2",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018428.1.13",
          "form": "여전히 재미있다고!!",
          "original_form": "여전히 재미있다고!!",
          "speaker_id": "2",
          "time": "20210827 01:44"
        },
        {
          "id": "MDRW2100018428.1.14",
          "form": "게다가 오버워치는",
          "original_form": "게다가 오버워치는",
          "speaker_id": "2",
          "time": "20210827 01:45"
        },
        {
          "id": "MDRW2100018428.1.15",
          "form": "신캐도 나오고!!",
          "original_form": "신캐도 나오고!!",
          "speaker_id": "2",
          "time": "20210827 01:45"
        },
        {
          "id": "MDRW2100018428.1.16",
          "form": "으응.... 하지만",
          "original_form": "으응.... 하지만",
          "speaker_id": "1",
          "time": "20210827 01:45"
        },
        {
          "id": "MDRW2100018428.1.17",
          "form": "블리자드 크게 난리나지 않았나??",
          "original_form": "블리자드 크게 난리나지 않았나??",
          "speaker_id": "1",
          "time": "20210827 01:46"
        },
        {
          "id": "MDRW2100018428.1.18",
          "form": "엥 언제??",
          "original_form": "엥 언제??",
          "speaker_id": "2",
          "time": "20210827 01:46"
        },
        {
          "id": "MDRW2100018428.1.19",
          "form": "이번에 성차별이랑 그런 거로",
          "original_form": "이번에 성차별이랑 그런 거로",
          "speaker_id": "1",
          "time": "20210827 01:46"
        },
        {
          "id": "MDRW2100018428.1.20",
          "form": "엄청 심하게 터졌었어",
          "original_form": "엄청 심하게 터졌었어",
          "speaker_id": "1",
          "time": "20210827 01:46"
        },
        {
          "id": "MDRW2100018428.1.21",
          "form": "이런~^^",
          "original_form": "이런~^^",
          "speaker_id": "2",
          "time": "20210827 01:46"
        },
        {
          "id": "MDRW2100018428.1.22",
          "form": "요즘 게임을 안 했더니 전혀 몰랐네",
          "original_form": "요즘 게임을 안 했더니 전혀 몰랐네",
          "speaker_id": "2",
          "time": "20210827 01:46"
        },
        {
          "id": "MDRW2100018428.1.23",
          "form": "내가 보는 게임 유튜버는 주로 배그만 해서..",
          "original_form": "내가 보는 게임 유튜버는 주로 배그만 해서..",
          "speaker_id": "2",
          "time": "20210827 01:47"
        },
        {
          "id": "MDRW2100018428.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그럴 수 있지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 그럴 수 있지",
          "speaker_id": "1",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018428.1.25",
          "form": "그러니까 나랑 파판하자",
          "original_form": "그러니까 나랑 파판하자",
          "speaker_id": "1",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018428.1.26",
          "form": "게임 재밌어",
          "original_form": "게임 재밌어",
          "speaker_id": "1",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018428.1.27",
          "form": "내가 도와줄게",
          "original_form": "내가 도와줄게",
          "speaker_id": "1",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018428.1.28",
          "form": "싫어요싫어~",
          "original_form": "싫어요싫어~",
          "speaker_id": "2",
          "time": "20210827 01:48"
        },
        {
          "id": "MDRW2100018428.1.29",
          "form": "전 오버워치를 배신할 수는 없어요~",
          "original_form": "전 오버워치를 배신할 수는 없어요~",
          "speaker_id": "2",
          "time": "20210827 01:49"
        },
        {
          "id": "MDRW2100018428.1.30",
          "form": "근데 곧 개강인데 게임 계속 하려고..?",
          "original_form": "근데 곧 개강인데 게임 계속 하려고..?",
          "speaker_id": "2",
          "time": "20210827 01:49"
        },
        {
          "id": "MDRW2100018428.1.31",
          "form": "너 오버워치 별로 안 하잖아",
          "original_form": "너 오버워치 별로 안 하잖아",
          "speaker_id": "1",
          "time": "20210827 01:49"
        },
        {
          "id": "MDRW2100018428.1.32",
          "form": "으응......",
          "original_form": "으응......",
          "speaker_id": "1",
          "time": "20210827 01:49"
        },
        {
          "id": "MDRW2100018428.1.33",
          "form": "그게...",
          "original_form": "그게...",
          "speaker_id": "1",
          "time": "20210827 01:49"
        },
        {
          "id": "MDRW2100018428.1.34",
          "form": "그래서 이번에 결제한 것까지만 하고 종강때까지 참게",
          "original_form": "그래서 이번에 결제한 것까지만 하고 종강때까지 참게",
          "speaker_id": "1",
          "time": "20210827 01:50"
        },
        {
          "id": "MDRW2100018428.1.35",
          "form": "과연 참을 수 있을까..?",
          "original_form": "과연 참을 수 있을까..?",
          "speaker_id": "2",
          "time": "20210827 01:51"
        },
        {
          "id": "MDRW2100018428.1.36",
          "form": "근데 내가 파이널 판타지를 할 수 없는 이유는 하나가 아니여",
          "original_form": "근데 내가 파이널 판타지를 할 수 없는 이유는 하나가 아니여",
          "speaker_id": "2",
          "time": "20210827 01:52"
        },
        {
          "id": "MDRW2100018428.1.37",
          "form": "요즘 내 노트북이 이상하거덩",
          "original_form": "요즘 내 노트북이 이상하거덩",
          "speaker_id": "2",
          "time": "20210827 01:52"
        },
        {
          "id": "MDRW2100018428.1.38",
          "form": "그래서 게임을 할 수 없어!!",
          "original_form": "그래서 게임을 할 수 없어!!",
          "speaker_id": "2",
          "time": "20210827 01:52"
        },
        {
          "id": "MDRW2100018428.1.39",
          "form": "뭐...? 너무 슬프다",
          "original_form": "뭐...? 너무 슬프다",
          "speaker_id": "1",
          "time": "20210827 01:52"
        },
        {
          "id": "MDRW2100018428.1.40",
          "form": "코로나라 pc방도 못 가고...",
          "original_form": "코로나라 pc방도 못 가고...",
          "speaker_id": "1",
          "time": "20210827 01:53"
        },
        {
          "id": "MDRW2100018428.1.41",
          "form": "폰 게임만 하니??",
          "original_form": "폰 게임만 하니??",
          "speaker_id": "1",
          "time": "20210827 01:53"
        },
        {
          "id": "MDRW2100018428.1.42",
          "form": "요즘은 사실 폰 게임도 잘 안해",
          "original_form": "요즘은 사실 폰 게임도 잘 안해",
          "speaker_id": "2",
          "time": "20210827 01:54"
        },
        {
          "id": "MDRW2100018428.1.43",
          "form": "쿠킹덤 질려서 삭제했거덩^^",
          "original_form": "쿠킹덤 질려서 삭제했거덩^^",
          "speaker_id": "2",
          "time": "20210827 01:55"
        },
        {
          "id": "MDRW2100018428.1.44",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 그렇게 열심히 하더니 결국?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 그렇게 열심히 하더니 결국?",
          "speaker_id": "1",
          "time": "20210827 01:55"
        },
        {
          "id": "MDRW2100018428.1.45",
          "form": "근데 폰게임이 빨리 질리기는 해...",
          "original_form": "근데 폰게임이 빨리 질리기는 해...",
          "speaker_id": "1",
          "time": "20210827 01:55"
        },
        {
          "id": "MDRW2100018428.1.46",
          "form": "맞어..",
          "original_form": "맞어..",
          "speaker_id": "2",
          "time": "20210827 01:56"
        },
        {
          "id": "MDRW2100018428.1.47",
          "form": "너는 요즘 폰게임 안해??",
          "original_form": "너는 요즘 폰게임 안해??",
