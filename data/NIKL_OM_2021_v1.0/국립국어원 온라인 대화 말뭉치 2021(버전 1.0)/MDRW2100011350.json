      "utterance": [
        {
          "id": "MDRW2100011350.1.1",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100011350.1.2",
          "form": "운동은 하는데 스포츠는 따로 보는거 없어요 ㅠ",
          "original_form": "운동은 하는데 스포츠는 따로 보는거 없어요 ㅠ",
          "speaker_id": "2",
          "time": "20210824 11:38"
        },
        {
          "id": "MDRW2100011350.1.3",
          "form": "저희 회사 11시반부터 점심시간이라 전 밥 먹으러 가볼게요!",
          "original_form": "저희 회사 11시반부터 점심시간이라 전 밥 먹으러 가볼게요!",
          "speaker_id": "2",
          "time": "20210824 11:39"
        },
        {
          "id": "MDRW2100011350.1.4",
          "form": "누누님 맛점하세요",
          "original_form": "누누님 맛점하세요",
          "speaker_id": "2",
          "time": "20210824 11:39"
        },
        {
          "id": "MDRW2100011350.1.5",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210824 11:47"
        },
        {
          "id": "MDRW2100011350.1.6",
          "form": "어떤운동하시나요?",
          "original_form": "어떤운동하시나요?",
          "speaker_id": "1",
          "time": "20210824 11:47"
        },
        {
          "id": "MDRW2100011350.1.7",
          "form": "작년초까지만해도 헬스장 다녔는데 코로나 터지고는 집에서 홈트만하고 있어요 ㅠ",
          "original_form": "작년초까지만해도 헬스장 다녔는데 코로나 터지고는 집에서 홈트만하고 있어요 ㅠ",
          "speaker_id": "2",
          "time": "20210824 12:26"
        },
        {
          "id": "MDRW2100011350.1.8",
          "form": "전에는 스쿼트 플랭크 위주로 해서 살 좀 많이 뺐죠 ㅎ 요즘은 근력운동 위주로 하고 있어요",
          "original_form": "전에는 스쿼트 플랭크 위주로 해서 살 좀 많이 뺐죠 ㅎ 요즘은 근력운동 위주로 하고 있어요",
          "speaker_id": "2",
          "time": "20210824 12:27"
        },
        {
          "id": "MDRW2100011350.1.9",
          "form": "누누님은 스포츠 좋아하시는거 있으세요??",
          "original_form": "누누님은 스포츠 좋아하시는거 있으세요??",
          "speaker_id": "2",
          "time": "20210824 12:27"
        },
        {
          "id": "MDRW2100011350.1.10",
          "form": "남자분이신가보네요??? 아닌가 운동종류가",
          "original_form": "남자분이신가보네요??? 아닌가 운동종류가",
          "speaker_id": "1",
          "time": "20210824 12:32"
        },
        {
          "id": "MDRW2100011350.1.11",
          "form": "전 축구요ㅋㅋㅋ",
          "original_form": "전 축구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:32"
        },
        {
          "id": "MDRW2100011350.1.12",
          "form": "옿 축구",
          "original_form": "옿 축구",
          "speaker_id": "2",
          "time": "20210824 12:37"
        },
        {
          "id": "MDRW2100011350.1.13",
          "form": "해축 보시는거예요??? 직접 하시는건가???",
          "original_form": "해축 보시는거예요??? 직접 하시는건가???",
          "speaker_id": "2",
          "time": "20210824 12:38"
        },
        {
          "id": "MDRW2100011350.1.14",
          "form": "제 성별은 남자예여 ㅋㅋ 누누님은 성별이???",
          "original_form": "제 성별은 남자예여 ㅋㅋ 누누님은 성별이???",
          "speaker_id": "2",
          "time": "20210824 12:39"
        },
        {
          "id": "MDRW2100011350.1.15",
          "form": "네네 여자요",
          "original_form": "네네 여자요",
          "speaker_id": "1",
          "time": "20210824 12:45"
        },
        {
          "id": "MDRW2100011350.1.16",
          "form": "보는거요ㅋㅋ해축ㅋㅋㅋ",
          "original_form": "보는거요ㅋㅋ해축ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 12:45"
        },
        {
          "id": "MDRW2100011350.1.17",
          "form": "직접은 못한답니다",
          "original_form": "직접은 못한답니다",
          "speaker_id": "1",
          "time": "20210824 12:45"
        },
        {
          "id": "MDRW2100011350.1.18",
          "form": "ㅋㅋㅋ 주변에 여자분이 해축보는거 처음인듯",
          "original_form": "ㅋㅋㅋ 주변에 여자분이 해축보는거 처음인듯",
          "speaker_id": "2",
          "time": "20210824 13:38"
        },
        {
          "id": "MDRW2100011350.1.19",
          "form": "케이리그 직관도 해보셨어요???",
          "original_form": "케이리그 직관도 해보셨어요???",
          "speaker_id": "2",
          "time": "20210824 13:40"
        },
        {
          "id": "MDRW2100011350.1.20",
          "form": "아니요ㅋㅋㅋ 직관은 안해봤어요",
          "original_form": "아니요ㅋㅋㅋ 직관은 안해봤어요",
          "speaker_id": "1",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100011350.1.21",
          "form": "국내보단 해축좋아해서요!",
          "original_form": "국내보단 해축좋아해서요!",
          "speaker_id": "1",
          "time": "20210824 23:13"
        },
        {
          "id": "MDRW2100011350.1.22",
          "form": "ㅋㅋ 확실히 국내보단 해축이 보는 맛이 있져",
          "original_form": "ㅋㅋ 확실히 국내보단 해축이 보는 맛이 있져",
          "speaker_id": "2",
          "time": "20210824 23:22"
        },
        {
          "id": "MDRW2100011350.1.23",
          "form": "예전에 흥민이형 챔스결승 올라왔을 때 친구따라서 밤 새서 본적 있어여 저두 ㅋㅋ",
          "original_form": "예전에 흥민이형 챔스결승 올라왔을 때 친구따라서 밤 새서 본적 있어여 저두 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:23"
        },
        {
          "id": "MDRW2100011350.1.24",
          "form": "ㅋㅋㅋ 지금",
          "original_form": "ㅋㅋㅋ 지금",
          "speaker_id": "1",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100011350.1.25",
          "form": "리그중인데 아시나요????",
          "original_form": "리그중인데 아시나요????",
          "speaker_id": "1",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100011350.1.26",
          "form": "epl!",
          "original_form": "epl!",
          "speaker_id": "1",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100011350.1.27",
          "form": "ㅋㅋㅋ 친구들 단톡방에서 흥민이 시즌 첫경기 골 넣었다고 한거는 본거 같아요",
          "original_form": "ㅋㅋㅋ 친구들 단톡방에서 흥민이 시즌 첫경기 골 넣었다고 한거는 본거 같아요",
          "speaker_id": "2",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100011350.1.28",
          "form": "아 지금 경기 하고있구나",
          "original_form": "아 지금 경기 하고있구나",
          "speaker_id": "2",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100011350.1.29",
          "form": "아닌가?????????",
          "original_form": "아닌가?????????",
          "speaker_id": "2",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100011350.1.30",
          "form": "ㅋㅋㅋ 지금 이시간은아니고",
          "original_form": "ㅋㅋㅋ 지금 이시간은아니고",
          "speaker_id": "1",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100011350.1.31",
          "form": "epl개막이 며칠전이었어요",
          "original_form": "epl개막이 며칠전이었어요",
          "speaker_id": "1",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100011350.1.32",
          "form": "그 골 진짜 간지나게넣었답니다",
          "original_form": "그 골 진짜 간지나게넣었답니다",
          "speaker_id": "1",
          "time": "20210824 23:57"
        },
        {
          "id": "MDRW2100011350.1.33",
          "form": "아 그래여???? ㅋㅋ",
          "original_form": "아 그래여???? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100011350.1.34",
          "form": "언제 하이라이트 영상 찾아봐야겠네",
          "original_form": "언제 하이라이트 영상 찾아봐야겠네",
          "speaker_id": "2",
          "time": "20210824 23:58"
        },
        {
          "id": "MDRW2100011350.1.35",
          "form": "누누님 전 낼 출근해서 자러가야할 것 같아여 ㅠ",
          "original_form": "누누님 전 낼 출근해서 자러가야할 것 같아여 ㅠ",
          "speaker_id": "2",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100011350.1.36",
          "form": "조은 밤 되세여!!!",
          "original_form": "조은 밤 되세여!!!",
          "speaker_id": "2",
          "time": "20210824 23:59"
        },
        {
          "id": "MDRW2100011350.1.37",
          "form": "일찍자는군요",
          "original_form": "일찍자는군요",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100011350.1.38",
          "form": "역시 직장인은",
          "original_form": "역시 직장인은",
          "speaker_id": "1",
          "time": "20210825 00:08"
        },
        {
          "id": "MDRW2100011350.1.39",
          "form": "네네 내일뵈요ㅠ",
          "original_form": "네네 내일뵈요ㅠ",
          "speaker_id": "1",
