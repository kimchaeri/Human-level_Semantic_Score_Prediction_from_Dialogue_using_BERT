      "utterance": [
        {
          "id": "MDRW2100032447.1.1",
          "form": "돈은 얼마나 복구했나",
          "original_form": "돈은 얼마나 복구했나",
          "speaker_id": "1",
          "time": "20210905 22:28"
        },
        {
          "id": "MDRW2100032447.1.2",
          "form": "아직 멀었는데",
          "original_form": "아직 멀었는데",
          "speaker_id": "2",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100032447.1.3",
          "form": "요즘 많이 올랐던데",
          "original_form": "요즘 많이 올랐던데",
          "speaker_id": "1",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100032447.1.4",
          "form": "많이 올랐지",
          "original_form": "많이 올랐지",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100032447.1.5",
          "form": "근데 엄마는 너무 많이 잃어서...",
          "original_form": "근데 엄마는 너무 많이 잃어서...",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100032447.1.6",
          "form": "흑흑 내돈 찾아줘",
          "original_form": "흑흑 내돈 찾아줘",
          "speaker_id": "1",
          "time": "20210905 22:31"
        },
        {
          "id": "MDRW2100032447.1.7",
          "form": "나도 빨리 찾아주고 끝내고싶어ㅋㅋㅋ",
          "original_form": "나도 빨리 찾아주고 끝내고싶어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:32"
        },
        {
          "id": "MDRW2100032447.1.8",
          "form": "삼촌한테 빌려준돈은 받았어??",
          "original_form": "삼촌한테 빌려준돈은 받았어??",
          "speaker_id": "2",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100032447.1.9",
          "form": "아니 아직 안받았어",
          "original_form": "아니 아직 안받았어",
          "speaker_id": "1",
          "time": "20210905 22:33"
        },
        {
          "id": "MDRW2100032447.1.10",
          "form": "언제 받으려고",
          "original_form": "언제 받으려고",
          "speaker_id": "2",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100032447.1.11",
          "form": "삼촌이 삼촌한테 사기친놈한테 돈을 못 받았대",
          "original_form": "삼촌이 삼촌한테 사기친놈한테 돈을 못 받았대",
          "speaker_id": "1",
          "time": "20210905 22:34"
        },
        {
          "id": "MDRW2100032447.1.12",
          "form": "벌써 몇년이나 지났는데...",
          "original_form": "벌써 몇년이나 지났는데...",
          "speaker_id": "2",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032447.1.13",
          "form": "못 받을거같은데",
          "original_form": "못 받을거같은데",
          "speaker_id": "2",
          "time": "20210905 22:35"
        },
        {
          "id": "MDRW2100032447.1.14",
          "form": "나도 그렇게 생각해",
          "original_form": "나도 그렇게 생각해",
          "speaker_id": "1",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032447.1.15",
          "form": "근데 삼촌이 가만히 있어서 뭐라하기도 그려",
          "original_form": "근데 삼촌이 가만히 있어서 뭐라하기도 그려",
          "speaker_id": "1",
          "time": "20210905 22:36"
        },
        {
          "id": "MDRW2100032447.1.16",
          "form": "너무 속터져",
          "original_form": "너무 속터져",
          "speaker_id": "2",
          "time": "20210905 22:36"
        },
        {
