      "utterance": [
        {
          "id": "MDRW2100043506.1.1",
          "form": "저녁 오늘은 뭐 드시나요?",
          "original_form": "저녁 오늘은 뭐 드시나요?",
          "speaker_id": "1",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043506.1.2",
          "form": "저는 양배추 삶아서 쌈 싸먹으려고요",
          "original_form": "저는 양배추 삶아서 쌈 싸먹으려고요",
          "speaker_id": "2",
          "time": "20210907 19:19"
        },
        {
          "id": "MDRW2100043506.1.3",
          "form": "양배추 삶은거랑 뭐랑 드시나요?",
          "original_form": "양배추 삶은거랑 뭐랑 드시나요?",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043506.1.4",
          "form": "제육볶음이요",
          "original_form": "제육볶음이요",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043506.1.5",
          "form": "헉 너무 맛있겠어요..",
          "original_form": "헉 너무 맛있겠어요..",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043506.1.6",
          "form": "제육볶음 좋아하세요?",
          "original_form": "제육볶음 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043506.1.7",
          "form": "네...고기...",
          "original_form": "네...고기...",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043506.1.8",
          "form": "제육볶음이랑 양배추랑 잘어울려요",
          "original_form": "제육볶음이랑 양배추랑 잘어울려요",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043506.1.9",
          "form": "글게요ㅠㅠ 너무 먹고싶다..",
          "original_form": "글게요ㅠㅠ 너무 먹고싶다..",
          "speaker_id": "1",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043506.1.10",
          "form": "양배추 삶은거랑 먹으면 속도 편해요",
          "original_form": "양배추 삶은거랑 먹으면 속도 편해요",
          "speaker_id": "2",
          "time": "20210907 19:20"
        },
        {
          "id": "MDRW2100043506.1.11",
          "form": "아 저는 오늘 저녁 계란 삶은거 두갠데요..",
          "original_form": "아 저는 오늘 저녁 계란 삶은거 두갠데요..",
          "speaker_id": "1",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043506.1.12",
          "form": "헉 왜이렇게 부실하게 드세요",
          "original_form": "헉 왜이렇게 부실하게 드세요",
          "speaker_id": "2",
          "time": "20210907 19:21"
        },
        {
          "id": "MDRW2100043506.1.13",
          "form": "ㅠㅠ제육볶음 부럽다...",
          "original_form": "ㅠㅠ제육볶음 부럽다...",
