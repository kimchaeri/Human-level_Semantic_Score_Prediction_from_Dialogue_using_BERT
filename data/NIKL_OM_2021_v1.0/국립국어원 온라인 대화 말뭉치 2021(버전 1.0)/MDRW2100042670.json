      "utterance": [
        {
          "id": "MDRW2100042670.1.1",
          "form": "와~ 감자많이 넣었어??",
          "original_form": "와~ 감자많이 넣었어??",
          "speaker_id": "2",
          "time": "20210907 00:47"
        },
        {
          "id": "MDRW2100042670.1.2",
          "form": "더 넣.고 끓일게",
          "original_form": "더 넣.고 끓일게",
          "speaker_id": "1",
          "time": "20210907 00:48"
        },
        {
          "id": "MDRW2100042670.1.3",
          "form": "아 어제 신림갔다왔자나",
          "original_form": "아 어제 신림갔다왔자나",
          "speaker_id": "2",
          "time": "20210907 00:48"
        },
        {
          "id": "MDRW2100042670.1.4",
          "form": "거기 많이 바뀌었더라",
          "original_form": "거기 많이 바뀌었더라",
          "speaker_id": "2",
          "time": "20210907 00:49"
        },
        {
          "id": "MDRW2100042670.1.5",
          "form": "어떻게 바뀌었는데???",
          "original_form": "어떻게 바뀌었는데???",
          "speaker_id": "1",
          "time": "20210907 00:50"
        },
        {
          "id": "MDRW2100042670.1.6",
          "form": "내가 다니던 학원이 없어졌어...",
          "original_form": "내가 다니던 학원이 없어졌어...",
          "speaker_id": "2",
          "time": "20210907 00:50"
        },
        {
          "id": "MDRW2100042670.1.7",
          "form": "응....?학원이 없어졌다고???",
          "original_form": "응....?학원이 없어졌다고???",
          "speaker_id": "1",
          "time": "20210907 00:50"
        },
        {
          "id": "MDRW2100042670.1.8",
          "form": "응 철거하고있던데 학원근처 목욕탕까지 다",
          "original_form": "응 철거하고있던데 학원근처 목욕탕까지 다",
          "speaker_id": "2",
          "time": "20210907 00:51"
        },
        {
          "id": "MDRW2100042670.1.9",
          "form": "내 추억이 없어져써",
          "original_form": "내 추억이 없어져써",
          "speaker_id": "2",
          "time": "20210907 00:52"
        },
        {
          "id": "MDRW2100042670.1.10",
          "form": "그럼 거기있던 학원은 어디갔어",
          "original_form": "그럼 거기있던 학원은 어디갔어",
          "speaker_id": "1",
          "time": "20210907 00:52"
        },
        {
          "id": "MDRW2100042670.1.11",
          "form": "저 안쪽으로 옮겼더라",
          "original_form": "저 안쪽으로 옮겼더라",
          "speaker_id": "2",
          "time": "20210907 00:53"
        },
        {
          "id": "MDRW2100042670.1.12",
          "form": "추억이 사라져서 아쉽네",
          "original_form": "추억이 사라져서 아쉽네",
          "speaker_id": "1",
          "time": "20210907 00:55"
        },
        {
          "id": "MDRW2100042670.1.13",
          "form": "내 추억!!!ㅠ",
          "original_form": "내 추억!!!ㅠ",
          "speaker_id": "2",
          "time": "20210907 00:55"
        },
        {
          "id": "MDRW2100042670.1.14",
          "form": "새로운 학원에서 쌓아봐",
          "original_form": "새로운 학원에서 쌓아봐",
          "speaker_id": "1",
          "time": "20210907 00:55"
        },
        {
          "id": "MDRW2100042670.1.15",
          "form": "그러고 싶진않아",
          "original_form": "그러고 싶진않아",
          "speaker_id": "2",
          "time": "20210907 00:56"
        },
