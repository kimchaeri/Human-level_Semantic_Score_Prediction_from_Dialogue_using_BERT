      "utterance": [
        {
          "id": "MDRW2100016498.1.1",
          "form": "점심 드셨어요?",
          "original_form": "점심 드셨어요?",
          "speaker_id": "1",
          "time": "20210826 13:18"
        },
        {
          "id": "MDRW2100016498.1.2",
          "form": "점심시간에 도시락 배달시켜서 먹었어요~@",
          "original_form": "점심시간에 도시락 배달시켜서 먹었어요~@",
          "speaker_id": "2",
          "time": "20210826 13:19"
        },
        {
          "id": "MDRW2100016498.1.3",
          "form": "식사하셧어요?",
          "original_form": "식사하셧어요?",
          "speaker_id": "2",
          "time": "20210826 13:19"
        },
        {
          "id": "MDRW2100016498.1.4",
          "form": "네 백반먹었어요 ㅋㅋㅋ",
          "original_form": "네 백반먹었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:20"
        },
        {
          "id": "MDRW2100016498.1.5",
          "form": "저녁은 뭐드실계획이세요?",
          "original_form": "저녁은 뭐드실계획이세요?",
          "speaker_id": "1",
          "time": "20210826 13:21"
        },
        {
          "id": "MDRW2100016498.1.6",
          "form": "비빔면에 오이 왕창 넣어서 먹을려구욥",
          "original_form": "비빔면에 오이 왕창 넣어서 먹을려구욥",
          "speaker_id": "2",
          "time": "20210826 13:21"
        },
        {
          "id": "MDRW2100016498.1.7",
          "form": "비빔면!! 어느 비빔면 드세요?",
          "original_form": "비빔면!! 어느 비빔면 드세요?",
          "speaker_id": "1",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100016498.1.8",
          "form": "매운거좋아해서 팔도 매운맛 사놧어용",
          "original_form": "매운거좋아해서 팔도 매운맛 사놧어용",
          "speaker_id": "2",
          "time": "20210826 13:28"
        },
        {
          "id": "MDRW2100016498.1.9",
          "form": "팔도매운맛은 안먹어봤는데 많이맵나요?",
          "original_form": "팔도매운맛은 안먹어봤는데 많이맵나요?",
          "speaker_id": "1",
          "time": "20210826 13:29"
        },
        {
          "id": "MDRW2100016498.1.10",
          "form": "매콤!! 맛잇어요",
          "original_form": "매콤!! 맛잇어요",
          "speaker_id": "2",
          "time": "20210826 13:33"
        },
        {
          "id": "MDRW2100016498.1.11",
          "form": "저는 먹는낙으로 살아서 다 잘먹어욤",
          "original_form": "저는 먹는낙으로 살아서 다 잘먹어욤",
          "speaker_id": "2",
          "time": "20210826 13:34"
        },
        {
          "id": "MDRW2100016498.1.12",
          "form": "저도 먹는낙으로 사는데 ㅋㅋㅋㅋ너무 매운건 잘 못먹어요",
          "original_form": "저도 먹는낙으로 사는데 ㅋㅋㅋㅋ너무 매운건 잘 못먹어요",
          "speaker_id": "1",
          "time": "20210826 13:34"
        },
        {
          "id": "MDRW2100016498.1.13",
          "form": "좋아하는 음식?",
          "original_form": "좋아하는 음식?",
          "speaker_id": "2",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016498.1.14",
          "form": "최애메뉴?",
          "original_form": "최애메뉴?",
          "speaker_id": "2",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016498.1.15",
          "form": "잇으신가용",
          "original_form": "잇으신가용",
          "speaker_id": "2",
          "time": "20210826 13:35"
        },
        {
          "id": "MDRW2100016498.1.16",
          "form": "어렵네요 먹는거 다좋아해서",
          "original_form": "어렵네요 먹는거 다좋아해서",
          "speaker_id": "1",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100016498.1.17",
          "form": "음 피자좋아해요",
          "original_form": "음 피자좋아해요",
          "speaker_id": "1",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100016498.1.18",
          "form": "피자는 도미노!!!",
          "original_form": "피자는 도미노!!!",
          "speaker_id": "2",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100016498.1.19",
          "form": "도미노피자도 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "도미노피자도 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:37"
        },
        {
          "id": "MDRW2100016498.1.20",
          "form": "저녁엔 뭐드시나용?",
          "original_form": "저녁엔 뭐드시나용?",
          "speaker_id": "2",
          "time": "20210826 13:47"
        },
        {
          "id": "MDRW2100016498.1.21",
          "form": "아마 닭볶음탕 먹지 않을까 싶어요 ㅋㅋㅋㅋㅋ",
          "original_form": "아마 닭볶음탕 먹지 않을까 싶어요 ㅋㅋㅋㅋㅋ",
