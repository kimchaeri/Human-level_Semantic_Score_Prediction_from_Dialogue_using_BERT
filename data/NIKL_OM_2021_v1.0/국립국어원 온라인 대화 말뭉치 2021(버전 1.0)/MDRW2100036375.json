      "utterance": [
        {
          "id": "MDRW2100036375.1.1",
          "form": "돌체구스토??",
          "original_form": "돌체구스토??",
          "speaker_id": "1",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100036375.1.2",
          "form": "돌체구스토",
          "original_form": "돌체구스토",
          "speaker_id": "2",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100036375.1.3",
          "form": "집에있었는데 지금응 없어요!",
          "original_form": "집에있었는데 지금응 없어요!",
          "speaker_id": "2",
          "time": "20210830 22:05"
        },
        {
          "id": "MDRW2100036375.1.4",
          "form": "헉 그 친구는 어디로 갔나용",
          "original_form": "헉 그 친구는 어디로 갔나용",
          "speaker_id": "1",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100036375.1.5",
          "form": "네스프레소 사고싶엇는데",
          "original_form": "네스프레소 사고싶엇는데",
          "speaker_id": "2",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100036375.1.6",
          "form": "안사려규요 ㅋㅋㅋ",
          "original_form": "안사려규요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100036375.1.7",
          "form": "당근에 팔앗어요 ㅋㅋ",
          "original_form": "당근에 팔앗어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100036375.1.8",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100036375.1.9",
          "form": "커피 좋아하시는 분들은",
          "original_form": "커피 좋아하시는 분들은",
          "speaker_id": "1",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100036375.1.10",
          "form": "캡슐 자주 드셔서",
          "original_form": "캡슐 자주 드셔서",
          "speaker_id": "1",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100036375.1.11",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100036375.1.12",
          "form": "회사에서 캡슐 머신 아예 놓고",
          "original_form": "회사에서 캡슐 머신 아예 놓고",
          "speaker_id": "1",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100036375.1.13",
          "form": "저도 잇을땐 많이마시긴햇는데",
          "original_form": "저도 잇을땐 많이마시긴햇는데",
          "speaker_id": "2",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100036375.1.14",
          "form": "드시는분도 봤어요ㅋㅋㅋㅋ",
          "original_form": "드시는분도 봤어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:06"
        },
        {
          "id": "MDRW2100036375.1.15",
          "form": "지금응 거의 매일 밖에서 사먹어요 ㅋㅋㅋ",
          "original_form": "지금응 거의 매일 밖에서 사먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100036375.1.16",
          "form": "그게 제일 편하긴 해요...",
          "original_form": "그게 제일 편하긴 해요...",
          "speaker_id": "1",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100036375.1.17",
          "form": "기계 닦는게 은근 귀찮다고 하시더라구요",
          "original_form": "기계 닦는게 은근 귀찮다고 하시더라구요",
          "speaker_id": "1",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100036375.1.18",
          "form": "맞아여 캡슐도 그냥구렇구",
          "original_form": "맞아여 캡슐도 그냥구렇구",
          "speaker_id": "2",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100036375.1.19",
          "form": "저 오늘 그거 봤는대",
          "original_form": "저 오늘 그거 봤는대",
          "speaker_id": "1",
          "time": "20210830 22:07"
        },
        {
          "id": "MDRW2100036375.1.20",
          "form": "이디야 아메리카노가",
          "original_form": "이디야 아메리카노가",
          "speaker_id": "1",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100036375.1.21",
          "form": "샷이 한번만 들어간대요",
          "original_form": "샷이 한번만 들어간대요",
          "speaker_id": "1",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100036375.1.22",
          "form": "!?!",
          "original_form": "!?!",
          "speaker_id": "2",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100036375.1.23",
          "form": "비슷한 크기 스타벅스 톨사이즈는",
          "original_form": "비슷한 크기 스타벅스 톨사이즈는",
          "speaker_id": "1",
          "time": "20210830 22:08"
        },
        {
          "id": "MDRW2100036375.1.24",
          "form": "샷이 두번들어가는데ㅋㅋㅋ",
          "original_form": "샷이 두번들어가는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:08"
        }
      ]
    }
