      "utterance": [
        {
          "id": "MDRW2100007043.1.1",
          "form": "혹시 그럼 추리물 중에 추천해 주실 만한 거 더 있을까요?",
          "original_form": "혹시 그럼 추리물 중에 추천해 주실 만한 거 더 있을까요?",
          "speaker_id": "1",
          "time": "20210822 00:21"
        },
        {
          "id": "MDRW2100007043.1.2",
          "form": "비숲이랑 시그널말고는",
          "original_form": "비숲이랑 시그널말고는",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007043.1.3",
          "form": "딱히...생각나는게 없는데요",
          "original_form": "딱히...생각나는게 없는데요",
          "speaker_id": "2",
          "time": "20210822 00:22"
        },
        {
          "id": "MDRW2100007043.1.4",
          "form": "아..닥터",
          "original_form": "아..닥터",
          "speaker_id": "2",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007043.1.5",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007043.1.6",
          "form": "닥터?",
          "original_form": "닥터?",
          "speaker_id": "1",
          "time": "20210822 00:23"
        },
        {
          "id": "MDRW2100007043.1.7",
          "form": "갑자기 제목이 생각이 안나는데",
          "original_form": "갑자기 제목이 생각이 안나는데",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007043.1.8",
          "form": "정재영나오는거예요",
          "original_form": "정재영나오는거예요",
          "speaker_id": "2",
          "time": "20210822 00:25"
        },
        {
          "id": "MDRW2100007043.1.9",
          "form": "오 검색해 볼게요!",
          "original_form": "오 검색해 볼게요!",
          "speaker_id": "1",
          "time": "20210822 00:26"
        },
        {
          "id": "MDRW2100007043.1.10",
          "form": "검법남녀?",
          "original_form": "검법남녀?",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007043.1.11",
          "form": "검법남녀 네요 ㅎㅎ",
          "original_form": "검법남녀 네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007043.1.12",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007043.1.13",
          "form": "오 맞았네요!!",
          "original_form": "오 맞았네요!!",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007043.1.14",
          "form": "이것도 재미있나요?",
          "original_form": "이것도 재미있나요?",
          "speaker_id": "1",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007043.1.15",
          "form": "그것도 잼나게봤어요 ㅎㅎ",
          "original_form": "그것도 잼나게봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:27"
        },
        {
          "id": "MDRW2100007043.1.16",
          "form": "오 시간나면 봐야겠어요",
          "original_form": "오 시간나면 봐야겠어요",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007043.1.17",
          "form": "시즌 2까지 있네요",
          "original_form": "시즌 2까지 있네요",
          "speaker_id": "1",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007043.1.18",
          "form": "네 그랬던거같아요",
          "original_form": "네 그랬던거같아요",
          "speaker_id": "2",
          "time": "20210822 00:28"
        },
        {
          "id": "MDRW2100007043.1.19",
          "form": "정유미나오는게 정재영이랑 티격태격하는것도 잼났던거같아요",
          "original_form": "정유미나오는게 정재영이랑 티격태격하는것도 잼났던거같아요",
          "speaker_id": "2",
          "time": "20210822 00:29"
        },
        {
          "id": "MDRW2100007043.1.20",
          "form": "갑자기 보셔야할것이 많아지실듯요 ㅎㅎ",
          "original_form": "갑자기 보셔야할것이 많아지실듯요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007043.1.21",
          "form": "와 케미 좋을거 같아요ㅋㅋㅋ",
          "original_form": "와 케미 좋을거 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007043.1.22",
          "form": "그러게요 볼 게 쌓였네요ㅎㅎ",
          "original_form": "그러게요 볼 게 쌓였네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007043.1.23",
          "form": "연기파 배우들이라 볼만해요",
          "original_form": "연기파 배우들이라 볼만해요",
          "speaker_id": "2",
          "time": "20210822 00:30"
        },
        {
          "id": "MDRW2100007043.1.24",
          "form": "그러고보니 정유미는 요즘 머하시는지 궁금하네요 ㅎ",
          "original_form": "그러고보니 정유미는 요즘 머하시는지 궁금하네요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 00:31"
        },
        {
          "id": "MDRW2100007043.1.25",
          "form": "언제 한번 싹 정주행해야겠어요",
          "original_form": "언제 한번 싹 정주행해야겠어요",
          "speaker_id": "1",
          "time": "20210822 00:31"
        }
      ]
    }
