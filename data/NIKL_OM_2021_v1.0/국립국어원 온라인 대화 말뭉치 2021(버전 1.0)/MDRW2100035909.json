      "utterance": [
        {
          "id": "MDRW2100035909.1.1",
          "form": "혹시 반려동물 기르시나요?",
          "original_form": "혹시 반려동물 기르시나요?",
          "speaker_id": "2",
          "time": "20210830 11:15"
        },
        {
          "id": "MDRW2100035909.1.2",
          "form": "사실 저는 기르지는 않는데",
          "original_form": "사실 저는 기르지는 않는데",
          "speaker_id": "1",
          "time": "20210830 11:15"
        },
        {
          "id": "MDRW2100035909.1.3",
          "form": "다른 사람들 반려동물을 귀여워합니다 ㅎㅎㅎ",
          "original_form": "다른 사람들 반려동물을 귀여워합니다 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 11:15"
        },
        {
          "id": "MDRW2100035909.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210830 11:15"
        },
        {
          "id": "MDRW2100035909.1.5",
          "form": "앜ㅋㅋㅋㅋㅋㅋ 그거딱 저네요 ㅋㅋ\r\n 저는 반려동물이라할수있는진모르겠지만 구피기르고있어요 ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ 그거딱 저네요 ㅋㅋ\r\n 저는 반려동물이라할수있는진모르겠지만 구피기르고있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 11:17"
        },
        {
          "id": "MDRW2100035909.1.6",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210830 11:44"
        },
        {
          "id": "MDRW2100035909.1.7",
          "form": "물고기도 반려동물이죠!!",
          "original_form": "물고기도 반려동물이죠!!",
          "speaker_id": "1",
          "time": "20210830 11:44"
        },
        {
          "id": "MDRW2100035909.1.8",
          "form": "저 지인 중에 구피랑 안시 같이 기르는 사람 있는데",
          "original_form": "저 지인 중에 구피랑 안시 같이 기르는 사람 있는데",
          "speaker_id": "1",
          "time": "20210830 11:44"
        },
        {
          "id": "MDRW2100035909.1.9",
          "form": "서로 먹이를 바꿔 먹더라구요?;;;",
          "original_form": "서로 먹이를 바꿔 먹더라구요?;;;",
          "speaker_id": "1",
          "time": "20210830 11:45"
        },
        {
          "id": "MDRW2100035909.1.10",
          "form": "앜ㅋㅋㅌㅌ 오 정말요?ㅋㅋㅋ 저는 구피만 길러봐서 다른건잘모르겟어요 ㅋㅋㅋㅠ신기하네요",
          "original_form": "앜ㅋㅋㅌㅌ 오 정말요?ㅋㅋㅋ 저는 구피만 길러봐서 다른건잘모르겟어요 ㅋㅋㅋㅠ신기하네요",
          "speaker_id": "2",
          "time": "20210830 13:21"
        },
        {
          "id": "MDRW2100035909.1.11",
          "form": "맞아요 신기한 동물의 세계~",
          "original_form": "맞아요 신기한 동물의 세계~",
          "speaker_id": "1",
          "time": "20210830 13:51"
        },
        {
          "id": "MDRW2100035909.1.12",
          "form": "저는 어렸을 때 금붕어를 길러봤는데",
          "original_form": "저는 어렸을 때 금붕어를 길러봤는데",
          "speaker_id": "1",
          "time": "20210830 13:51"
        },
        {
          "id": "MDRW2100035909.1.13",
          "form": "무슨 이유 때문인지... 다음 날이면 항상 죽어있었어요ㅠ",
          "original_form": "무슨 이유 때문인지... 다음 날이면 항상 죽어있었어요ㅠ",
          "speaker_id": "1",
          "time": "20210830 13:52"
        },
        {
          "id": "MDRW2100035909.1.14",
          "form": "아 정말요? 아마 엄청 예민한종이엇나봐요 ㅠㅠ",
          "original_form": "아 정말요? 아마 엄청 예민한종이엇나봐요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 13:47"
        },
        {
          "id": "MDRW2100035909.1.15",
          "form": "그냥 금붕어였는데",
          "original_form": "그냥 금붕어였는데",
          "speaker_id": "1",
          "time": "20210831 14:03"
        },
        {
          "id": "MDRW2100035909.1.16",
          "form": "물에 산소가 부족했나봐요...",
          "original_form": "물에 산소가 부족했나봐요...",
          "speaker_id": "1",
          "time": "20210831 14:03"
        },
        {
          "id": "MDRW2100035909.1.17",
          "form": "구피는 몇 마리 기르시고 계신가요?",
          "original_form": "구피는 몇 마리 기르시고 계신가요?",
          "speaker_id": "1",
          "time": "20210831 14:04"
        },
        {
          "id": "MDRW2100035909.1.18",
          "form": "아하 ㅜㅜ 맞아요 붕어류는 산소공급 필수죠",
          "original_form": "아하 ㅜㅜ 맞아요 붕어류는 산소공급 필수죠",
          "speaker_id": "2",
          "time": "20210831 14:04"
        },
        {
          "id": "MDRW2100035909.1.19",
          "form": "저는 한 20마리는되는거같아요",
          "original_form": "저는 한 20마리는되는거같아요",
          "speaker_id": "2",
          "time": "20210831 14:04"
        },
        {
          "id": "MDRW2100035909.1.20",
          "form": "알을워낙많이낳아서 분양도하고 그래요!",
          "original_form": "알을워낙많이낳아서 분양도하고 그래요!",
          "speaker_id": "2",
          "time": "20210831 14:04"
        },
        {
          "id": "MDRW2100035909.1.21",
          "form": "산소 공급 필수였군요...",
          "original_form": "산소 공급 필수였군요...",
          "speaker_id": "1",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100035909.1.22",
          "form": "붕어야 미안하다......",
          "original_form": "붕어야 미안하다......",
          "speaker_id": "1",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100035909.1.23",
          "form": "주인을 잘못 만나서 ㅠㅠ 제가 많이 죽였네요. ..",
          "original_form": "주인을 잘못 만나서 ㅠㅠ 제가 많이 죽였네요. ..",
          "speaker_id": "1",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100035909.1.24",
          "form": "와 구피가 알을 많이 낳는군요!!!",
          "original_form": "와 구피가 알을 많이 낳는군요!!!",
          "speaker_id": "1",
          "time": "20210831 14:40"
        },
        {
          "id": "MDRW2100035909.1.25",
          "form": "넼ㅋㅋㅋㅋㅋㅋㅋ 아구 어쩔수없죠뭐~ㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋㅋㅋㅋ 아구 어쩔수없죠뭐~ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100035909.1.26",
          "form": "네 엄청엄청 많이낳어요..",
          "original_form": "네 엄청엄청 많이낳어요..",
          "speaker_id": "2",
          "time": "20210831 14:42"
        },
        {
          "id": "MDRW2100035909.1.27",
          "form": "와 상상이 안가네요~!",
          "original_form": "와 상상이 안가네요~!",
          "speaker_id": "1",
          "time": "20210831 15:33"
        },
        {
          "id": "MDRW2100035909.1.28",
          "form": "구피였나 안시였나",
          "original_form": "구피였나 안시였나",
          "speaker_id": "1",
          "time": "20210831 15:33"
        },
        {
