      "utterance": [
        {
          "id": "MDRW2100030064.1.1",
          "form": "스포츠 안챙기죠",
          "original_form": "스포츠 안챙기죠",
          "speaker_id": "2",
          "time": "20210905 18:22"
        },
        {
          "id": "MDRW2100030064.1.2",
          "form": "챙겨보는 경기는 없어ㅋㅋ",
          "original_form": "챙겨보는 경기는 없어ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:23"
        },
        {
          "id": "MDRW2100030064.1.3",
          "form": "나또한ㅋㅋ",
          "original_form": "나또한ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:25"
        },
        {
          "id": "MDRW2100030064.1.4",
          "form": "그나마 좋아하는 스포츠는?",
          "original_form": "그나마 좋아하는 스포츠는?",
          "speaker_id": "2",
          "time": "20210905 18:25"
        },
        {
          "id": "MDRW2100030064.1.5",
          "form": "축구?ㅋㅋㅋㅋ",
          "original_form": "축구?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:31"
        },
        {
          "id": "MDRW2100030064.1.6",
          "form": "그리고 국대경기 좋아해ㅋㅋㅋㅋ 재밌어ㅋㅋㅋㅋ",
          "original_form": "그리고 국대경기 좋아해ㅋㅋㅋㅋ 재밌어ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:32"
        },
        {
          "id": "MDRW2100030064.1.7",
          "form": "좋아하는 스포츠경기는?",
          "original_form": "좋아하는 스포츠경기는?",
          "speaker_id": "1",
          "time": "20210905 18:35"
        },
        {
          "id": "MDRW2100030064.1.8",
          "form": "체조!",
          "original_form": "체조!",
          "speaker_id": "2",
          "time": "20210905 18:35"
        },
        {
          "id": "MDRW2100030064.1.9",
          "form": "오?",
          "original_form": "오?",
          "speaker_id": "1",
          "time": "20210905 18:35"
        },
        {
          "id": "MDRW2100030064.1.10",
          "form": "체조 멋있죠?",
          "original_form": "체조 멋있죠?",
          "speaker_id": "1",
          "time": "20210905 18:36"
        },
        {
          "id": "MDRW2100030064.1.11",
          "form": "좋아하는 체조선수 있어?",
          "original_form": "좋아하는 체조선수 있어?",
          "speaker_id": "1",
          "time": "20210905 18:39"
        },
        {
          "id": "MDRW2100030064.1.12",
          "form": "아니ㅋㅋ 그냥 멋쪄!",
          "original_form": "아니ㅋㅋ 그냥 멋쪄!",
          "speaker_id": "2",
          "time": "20210905 18:41"
        },
        {
          "id": "MDRW2100030064.1.13",
          "form": "체조 좋아하는지 이제 알았네",
          "original_form": "체조 좋아하는지 이제 알았네",
          "speaker_id": "1",
          "time": "20210905 18:42"
        },
        {
          "id": "MDRW2100030064.1.14",
          "form": "ㅋㅋ이번에 알았어ㅋㅋ",
          "original_form": "ㅋㅋ이번에 알았어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:46"
        },
        {
          "id": "MDRW2100030064.1.15",
          "form": "나중에라도 직관이 되면 보러갈거?",
          "original_form": "나중에라도 직관이 되면 보러갈거?",
          "speaker_id": "1",
          "time": "20210905 18:48"
        },
        {
          "id": "MDRW2100030064.1.16",
          "form": "기회가 되면! ㅋㅋ",
          "original_form": "기회가 되면! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 18:51"
        },
        {
          "id": "MDRW2100030064.1.17",
          "form": "체조경기장도 있는데 보러갈 수 있었으면 좋겠네 ㅎㅎ",
          "original_form": "체조경기장도 있는데 보러갈 수 있었으면 좋겠네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 18:52"
        },
        {
          "id": "MDRW2100030064.1.18",
          "form": "그날이 오기를요!",
          "original_form": "그날이 오기를요!",
          "speaker_id": "2",
          "time": "20210905 18:53"
        }
      ]
