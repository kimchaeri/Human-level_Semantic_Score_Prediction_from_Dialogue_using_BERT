        {
          "id": "MDRW2100040405.1.1",
          "form": "사고싶었던게 품절되면 들어가서 검색해봐요",
          "original_form": "사고싶었던게 품절되면 들어가서 검색해봐요",
          "speaker_id": "2",
          "time": "20210904 18:21"
        },
        {
          "id": "MDRW2100040405.1.2",
          "form": "그로브꺼?",
          "original_form": "그로브꺼?",
          "speaker_id": "1",
          "time": "20210904 18:21"
        },
        {
          "id": "MDRW2100040405.1.3",
          "form": "네 그로브꺼 사봤어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네 그로브꺼 사봤어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 18:21"
        },
        {
          "id": "MDRW2100040405.1.4",
          "form": "name1님은요",
          "original_form": "&name1&님은요",
          "speaker_id": "2",
          "time": "20210904 18:22"
        },
        {
          "id": "MDRW2100040405.1.5",
          "form": "해보셨어요?",
          "original_form": "해보셨어요?",
          "speaker_id": "2",
          "time": "20210904 18:22"
        },
        {
          "id": "MDRW2100040405.1.6",
          "form": "name2님 전문가 다됏네요",
          "original_form": "&name2&님 전문가 다됏네요",
          "speaker_id": "1",
          "time": "20210904 18:22"
        },
        {
          "id": "MDRW2100040405.1.7",
          "form": "싸게삿어요?",
          "original_form": "싸게삿어요?",
          "speaker_id": "1",
          "time": "20210904 18:22"
        },
        {
          "id": "MDRW2100040405.1.8",
          "form": "엄청 옛날제품은 싸게 사봤어요",
          "original_form": "엄청 옛날제품은 싸게 사봤어요",
          "speaker_id": "2",
          "time": "20210904 18:23"
        },
        {
          "id": "MDRW2100040405.1.9",
          "form": "개이득이다 햇어요 ?",
          "original_form": "개이득이다 햇어요 ?",
          "speaker_id": "1",
          "time": "20210904 18:23"
        },
        {
          "id": "MDRW2100040405.1.10",
          "form": "당연하죠 옷도 맘에들었거든여",
          "original_form": "당연하죠 옷도 맘에들었거든여",
          "speaker_id": "2",
          "time": "20210904 18:24"
        },
        {
          "id": "MDRW2100040405.1.11",
          "form": "어떤거엿어요",
          "original_form": "어떤거엿어요",
          "speaker_id": "1",
          "time": "20210904 18:24"
        },
        {
          "id": "MDRW2100040405.1.12",
          "form": "린넨 반팔 셔츠요",
          "original_form": "린넨 반팔 셔츠요",
          "speaker_id": "2",
          "time": "20210904 18:24"
        },
        {
          "id": "MDRW2100040405.1.13",
          "form": "잘됐네요 또 사고픈거 잇어요?",
          "original_form": "잘됐네요 또 사고픈거 잇어요?",
          "speaker_id": "1",
          "time": "20210904 18:24"
        },
        {
          "id": "MDRW2100040405.1.14",
          "form": "지금 번장에서 보고있는건 있어요",
          "original_form": "지금 번장에서 보고있는건 있어요",
          "speaker_id": "2",
          "time": "20210904 18:25"
        },
        {
          "id": "MDRW2100040405.1.15",
          "form": "오 뭐죠",
          "original_form": "오 뭐죠",
          "speaker_id": "1",
          "time": "20210904 18:25"
        },
        {
          "id": "MDRW2100040405.1.16",
          "form": "마가린핑거스 바지요 네이비색",
          "original_form": "마가린핑거스 바지요 네이비색",
          "speaker_id": "2",
          "time": "20210904 18:25"
        },
        {
          "id": "MDRW2100040405.1.17",
          "form": "잘어울릴고같아요",
          "original_form": "잘어울릴고같아요",
          "speaker_id": "1",
