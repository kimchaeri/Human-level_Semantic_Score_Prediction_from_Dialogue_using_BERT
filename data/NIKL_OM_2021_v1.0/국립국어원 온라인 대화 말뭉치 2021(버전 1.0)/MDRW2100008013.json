      "utterance": [
        {
          "id": "MDRW2100008013.1.1",
          "form": "우리집 아이원이 전화했는데 너거집 원 왜 전화안받노 ㅎㅎ",
          "original_form": "우리집 아이원이 전화했는데 너거집 원 왜 전화안받노 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100008013.1.2",
          "form": "설거지하고 오니",
          "original_form": "설거지하고 오니",
          "speaker_id": "2",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100008013.1.3",
          "form": "땀이 줄줄 흐르네",
          "original_form": "땀이 줄줄 흐르네",
          "speaker_id": "2",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100008013.1.4",
          "form": "ㅋㅋㅋㅋ아 웃겨",
          "original_form": "ㅋㅋㅋㅋ아 웃겨",
          "speaker_id": "1",
          "time": "20210822 12:52"
        },
        {
          "id": "MDRW2100008013.1.5",
          "form": "원투 내도록 폰만 봐가지고",
          "original_form": "원투 내도록 폰만 봐가지고",
          "speaker_id": "2",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008013.1.6",
          "form": "놀이터갈라는데",
          "original_form": "놀이터갈라는데",
          "speaker_id": "2",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008013.1.7",
          "form": "ㅋㅋㅋ 진짜ㅋㅋ지금 나가나?",
          "original_form": "ㅋㅋㅋ 진짜ㅋㅋ지금 나가나?",
          "speaker_id": "1",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008013.1.8",
          "form": "눈감술하러 나오라고 얘기해봐",
          "original_form": "눈감술하러 나오라고 얘기해봐",
          "speaker_id": "2",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008013.1.9",
          "form": "아직 준비를 안해서",
          "original_form": "아직 준비를 안해서",
          "speaker_id": "2",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008013.1.10",
          "form": "준비하는데 30분",
          "original_form": "준비하는데 30분",
          "speaker_id": "2",
          "time": "20210822 12:53"
        },
        {
          "id": "MDRW2100008013.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ알았다.,  일단 애들 준비하고있을게",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ알았다.,  일단 애들 준비하고있을게",
          "speaker_id": "1",
          "time": "20210822 12:54"
        },
        {
          "id": "MDRW2100008013.1.12",
          "form": "엉 ㅋㅋㅋㅋㅋ 준비해서",
          "original_form": "엉 ㅋㅋㅋㅋㅋ 준비해서",
          "speaker_id": "2",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008013.1.13",
          "form": "반도 놀이터로 가께",
          "original_form": "반도 놀이터로 가께",
          "speaker_id": "2",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008013.1.14",
          "form": "줄넘기 준비해서 나온네이",
          "original_form": "줄넘기 준비해서 나온네이",
          "speaker_id": "2",
          "time": "20210822 12:55"
        },
        {
          "id": "MDRW2100008013.1.15",
          "form": "떠났나??",
          "original_form": "떠났나??",
          "speaker_id": "2",
          "time": "20210822 14:31"
        },
        {
          "id": "MDRW2100008013.1.16",
          "form": "배신자",
          "original_form": "배신자",
          "speaker_id": "2",
          "time": "20210822 14:31"
        },
        {
          "id": "MDRW2100008013.1.17",
          "form": "미안 미안 ㅠ",
          "original_form": "미안 미안 ㅠ",
          "speaker_id": "1",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008013.1.18",
          "form": "잘 놀다 갔어?",
          "original_form": "잘 놀다 갔어?",
          "speaker_id": "1",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008013.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㄱ 엉 거기 아파트 사는 동생네 애들 불러가지고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱ 엉 거기 아파트 사는 동생네 애들 불러가지고",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008013.1.20",
          "form": "신랑이랑 과학관 갔다 지금",
          "original_form": "신랑이랑 과학관 갔다 지금",
          "speaker_id": "2",
          "time": "20210822 16:13"
        },
        {
          "id": "MDRW2100008013.1.21",
          "form": "오오ㅋㅋㅋㅋ 인맥좋다 ㅋㅋㅋ진짜 대박ㅋㅋ",
          "original_form": "오오ㅋㅋㅋㅋ 인맥좋다 ㅋㅋㅋ진짜 대박ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008013.1.22",
          "form": "언니는 자유시간이가?ㅋ",
          "original_form": "언니는 자유시간이가?ㅋ",
          "speaker_id": "1",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008013.1.23",
          "form": "엉 나는 쉬고 ㅋㅋㅋㅋ",
          "original_form": "엉 나는 쉬고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008013.1.24",
          "form": "밥먹고 마트갔다 방금 들어옴",
          "original_form": "밥먹고 마트갔다 방금 들어옴",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008013.1.25",
          "form": "근데 너무 더워서 곧 온다길래",
          "original_form": "근데 너무 더워서 곧 온다길래",
          "speaker_id": "2",
          "time": "20210822 16:15"
        },
        {
          "id": "MDRW2100008013.1.26",
          "form": "더 놀길 바라며 쉬고있다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "더 놀길 바라며 쉬고있다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008013.1.27",
          "form": "ㅋㅋㅋ아하 좀 더 놀다오시지ㅋㅋ",
          "original_form": "ㅋㅋㅋ아하 좀 더 놀다오시지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008013.1.28",
          "form": "언니 좀 쉬게ㅋㅋㅋㅋ",
          "original_form": "언니 좀 쉬게ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008013.1.29",
          "form": "ㅋㅋㅋㅋㅋ 그래",
          "original_form": "ㅋㅋㅋㅋㅋ 그래",
          "speaker_id": "2",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008013.1.30",
          "form": "안덥나??",
          "original_form": "안덥나??",
          "speaker_id": "2",
          "time": "20210822 16:16"
        },
        {
          "id": "MDRW2100008013.1.31",
          "form": "더운데 올만하네ㅋㅋㅋ",
          "original_form": "더운데 올만하네ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008013.1.32",
          "form": "애들 여치, 나비잡고 다니고있다ㅋㅋ",
          "original_form": "애들 여치, 나비잡고 다니고있다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008013.1.33",
          "form": "어디갔노",
          "original_form": "어디갔노",
          "speaker_id": "2",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008013.1.34",
          "form": "고령쪽으로 왔는데 지나가다가 보이는데 바로 텐트설치함ㅋㅋ",
          "original_form": "고령쪽으로 왔는데 지나가다가 보이는데 바로 텐트설치함ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 16:17"
        },
        {
          "id": "MDRW2100008013.1.35",
          "form": "오호 좋구만 ㅎㅎ",
          "original_form": "오호 좋구만 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 16:18"
        }
      ]
