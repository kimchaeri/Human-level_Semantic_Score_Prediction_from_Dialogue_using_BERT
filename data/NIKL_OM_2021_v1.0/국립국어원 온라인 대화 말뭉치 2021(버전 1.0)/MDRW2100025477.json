        {
          "id": "MDRW2100025477.1.1",
          "form": "운동해요?",
          "original_form": "운동해요?",
          "speaker_id": "1",
          "time": "20210901 11:56"
        },
        {
          "id": "MDRW2100025477.1.2",
          "form": "가끔 런데이해요",
          "original_form": "가끔 런데이해요",
          "speaker_id": "2",
          "time": "20210901 11:58"
        },
        {
          "id": "MDRW2100025477.1.3",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "2",
          "time": "20210901 11:58"
        },
        {
          "id": "MDRW2100025477.1.4",
          "form": "네\n홈트요",
          "original_form": "네\n홈트요",
          "speaker_id": "1",
          "time": "20210901 12:01"
        },
        {
          "id": "MDRW2100025477.1.5",
          "form": "아하 자주하세요?",
          "original_form": "아하 자주하세요?",
          "speaker_id": "2",
          "time": "20210901 12:04"
        },
        {
          "id": "MDRW2100025477.1.6",
          "form": "매일요",
          "original_form": "매일요",
          "speaker_id": "1",
          "time": "20210901 12:05"
        },
        {
          "id": "MDRW2100025477.1.7",
          "form": "좋네요 식이도하시나요",
          "original_form": "좋네요 식이도하시나요",
          "speaker_id": "2",
          "time": "20210901 12:08"
        },
        {
          "id": "MDRW2100025477.1.8",
          "form": "식이까지는 ㅎㅎ",
          "original_form": "식이까지는 ㅎㅎ",
