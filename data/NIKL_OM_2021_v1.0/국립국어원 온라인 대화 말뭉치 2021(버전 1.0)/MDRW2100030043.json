      "utterance": [
        {
          "id": "MDRW2100030043.1.1",
          "form": "오늘도 운동 가냐",
          "original_form": "오늘도 운동 가냐",
          "speaker_id": "2",
          "time": "20210903 19:49"
        },
        {
          "id": "MDRW2100030043.1.2",
          "form": "요즘 피곤해서 운동안한지  3주됐음",
          "original_form": "요즘 피곤해서 운동안한지  3주됐음",
          "speaker_id": "1",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100030043.1.3",
          "form": "1주에 1키로씩  불고있어",
          "original_form": "1주에 1키로씩  불고있어",
          "speaker_id": "1",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100030043.1.4",
          "form": "와우 꽤 오래 쉬었네?",
          "original_form": "와우 꽤 오래 쉬었네?",
          "speaker_id": "2",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100030043.1.5",
          "form": "다이어트 관둠?",
          "original_form": "다이어트 관둠?",
          "speaker_id": "2",
          "time": "20210903 19:50"
        },
        {
          "id": "MDRW2100030043.1.6",
          "form": "아니 다이어트하는데 생리전증후군~ 생리까지 2주쉬고",
          "original_form": "아니 다이어트하는데 생리전증후군~ 생리까지 2주쉬고",
          "speaker_id": "1",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100030043.1.7",
          "form": "그후에는 너무피곤하고 운동하기싫어서 1주쉼",
          "original_form": "그후에는 너무피곤하고 운동하기싫어서 1주쉼",
          "speaker_id": "1",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100030043.1.8",
          "form": "담주부터 다시  다이어트해야지",
          "original_form": "담주부터 다시  다이어트해야지",
          "speaker_id": "1",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100030043.1.9",
          "form": "걷기도하고",
          "original_form": "걷기도하고",
          "speaker_id": "1",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100030043.1.10",
          "form": "자전거 안장이 너무 안맞는것같아서 바꾸려고",
          "original_form": "자전거 안장이 너무 안맞는것같아서 바꾸려고",
          "speaker_id": "1",
          "time": "20210903 19:52"
        },
        {
          "id": "MDRW2100030043.1.11",
          "form": "와 나도 생리전증후군 때문이었는지 두통오고 소화안되서 며칠 아픔",
          "original_form": "와 나도 생리전증후군 때문이었는지 두통오고 소화안되서 며칠 아픔",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100030043.1.12",
          "form": "그러고 지금 생리 왕 터져서 기운이없다",
          "original_form": "그러고 지금 생리 왕 터져서 기운이없다",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100030043.1.13",
          "form": "자전거안장? 왜",
          "original_form": "자전거안장? 왜",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100030043.1.14",
          "form": "나도 요즘  음식이 안맞아서   장염왔어",
          "original_form": "나도 요즘  음식이 안맞아서   장염왔어",
          "speaker_id": "1",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100030043.1.15",
          "form": "작아?",
          "original_form": "작아?",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100030043.1.16",
          "form": "오우 장염 조심하삼",
          "original_form": "오우 장염 조심하삼",
          "speaker_id": "2",
          "time": "20210903 19:53"
        },
        {
          "id": "MDRW2100030043.1.17",
          "form": "노놉!  작은것도 그렇고..모양이 내엉덩이하고 안맞는것같음",
          "original_form": "노놉!  작은것도 그렇고..모양이 내엉덩이하고 안맞는것같음",
          "speaker_id": "1",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100030043.1.18",
          "form": "불편해서 오래못타겠어",
          "original_form": "불편해서 오래못타겠어",
          "speaker_id": "1",
          "time": "20210903 19:54"
        },
        {
          "id": "MDRW2100030043.1.19",
          "form": "30대되니까 확실히 여기저기  잘아픈것같음",
          "original_form": "30대되니까 확실히 여기저기  잘아픈것같음",
          "speaker_id": "1",
          "time": "20210903 19:56"
        },
        {
          "id": "MDRW2100030043.1.20",
          "form": "아하 그런거군 자전거 진짜 잘탄다",
          "original_form": "아하 그런거군 자전거 진짜 잘탄다",
          "speaker_id": "2",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100030043.1.21",
          "form": "몸이 고장난거같음",
          "original_form": "몸이 고장난거같음",
          "speaker_id": "2",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100030043.1.22",
          "form": "30대도니깐",
          "original_form": "30대도니깐",
          "speaker_id": "2",
          "time": "20210903 20:00"
        },
        {
          "id": "MDRW2100030043.1.23",
          "form": "나쁜말 그만해",
          "original_form": "나쁜말 그만해",
          "speaker_id": "1",
          "time": "20210903 20:03"
        },
        {
          "id": "MDRW2100030043.1.24",
          "form": "ㅋㅋㅋ전 고운말을 써요",
          "original_form": "ㅋㅋㅋ전 고운말을 써요",
          "speaker_id": "2",
          "time": "20210903 20:04"
        },
        {
          "id": "MDRW2100030043.1.25",
          "form": "요즘 어떤 패션에 관심이 있냐",
          "original_form": "요즘 어떤 패션에 관심이 있냐",
          "speaker_id": "2",
          "time": "20210903 20:05"
        },
        {
          "id": "MDRW2100030043.1.26",
          "form": "옷에 노관심이야.  출퇴근만하니까",
          "original_form": "옷에 노관심이야.  출퇴근만하니까",
          "speaker_id": "1",
          "time": "20210903 20:05"
        },
        {
          "id": "MDRW2100030043.1.27",
          "form": "거리두기때문에 외출도 안하니까 더더욱",
          "original_form": "거리두기때문에 외출도 안하니까 더더욱",
          "speaker_id": "1",
          "time": "20210903 20:06"
        },
        {
          "id": "MDRW2100030043.1.28",
          "form": "츄리닝패션???",
          "original_form": "츄리닝패션???",
          "speaker_id": "1",
          "time": "20210903 20:06"
        },
        {
          "id": "MDRW2100030043.1.29",
          "form": "역시 그렇군",
          "original_form": "역시 그렇군",
          "speaker_id": "2",
          "time": "20210903 20:08"
        },
        {
          "id": "MDRW2100030043.1.30",
          "form": "나는 역시 와이드팬츠요",
          "original_form": "나는 역시 와이드팬츠요",
          "speaker_id": "2",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100030043.1.31",
          "form": "크록스와 슬리퍼",
          "original_form": "크록스와 슬리퍼",
          "speaker_id": "2",
          "time": "20210903 20:09"
        },
        {
          "id": "MDRW2100030043.1.32",
          "form": "나 그때 리리앤코에서 산 바지 잘입고다님",
          "original_form": "나 그때 리리앤코에서 산 바지 잘입고다님",
          "speaker_id": "1",
          "time": "20210903 20:10"
        },
        {
          "id": "MDRW2100030043.1.33",
          "form": "주제는  랜덤으로 정해지나?",
          "original_form": "주제는  랜덤으로 정해지나?",
          "speaker_id": "1",
          "time": "20210903 20:11"
        },
        {
          "id": "MDRW2100030043.1.34",
          "form": "아 그 바지ㅋㅋㅋ편해보이던거",
          "original_form": "아 그 바지ㅋㅋㅋ편해보이던거",
          "speaker_id": "2",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100030043.1.35",
          "form": "하늘색이던가",
          "original_form": "하늘색이던가",
          "speaker_id": "2",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100030043.1.36",
          "form": "검은색도있어. 엄청 넓은거",
          "original_form": "검은색도있어. 엄청 넓은거",
          "speaker_id": "1",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100030043.1.37",
          "form": "다른바지인데 통도 넓고. 고무줄이고 좋아",
          "original_form": "다른바지인데 통도 넓고. 고무줄이고 좋아",
          "speaker_id": "1",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100030043.1.38",
          "form": "너 백신 2차 맞았지?",
          "original_form": "너 백신 2차 맞았지?",
          "speaker_id": "1",
          "time": "20210903 20:26"
        },
        {
          "id": "MDRW2100030043.1.39",
          "form": "그래 그런스타일이 편해",
          "original_form": "그래 그런스타일이 편해",
          "speaker_id": "2",
          "time": "20210903 20:47"
        },
        {
          "id": "MDRW2100030043.1.40",
          "form": "어ㅋㅋ다 맞았엉",
          "original_form": "어ㅋㅋ다 맞았엉",
          "speaker_id": "2",
          "time": "20210903 20:48"
        }
      ]
    }
