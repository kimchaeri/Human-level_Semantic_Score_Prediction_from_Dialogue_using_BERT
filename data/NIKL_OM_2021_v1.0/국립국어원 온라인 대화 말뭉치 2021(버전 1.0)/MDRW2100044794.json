      "utterance": [
        {
          "id": "MDRW2100044794.1.1",
          "form": "그럼 꼭 하셔야겠네요",
          "original_form": "그럼 꼭 하셔야겠네요",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044794.1.2",
          "form": "하신지는 얼마나 됐어요?",
          "original_form": "하신지는 얼마나 됐어요?",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044794.1.3",
          "form": "시작은 꽤 됐는데",
          "original_form": "시작은 꽤 됐는데",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044794.1.4",
          "form": "잘 안나가서 ㅋㅋㅋㅋ큐ㅠㅠ",
          "original_form": "잘 안나가서 ㅋㅋㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044794.1.5",
          "form": "ㅠㅠㅠ 그러셨구나..ㅠㅠㅋㅋㅋ 얼른 체력증진을 위해 째끔만 더 힘내세요!!",
          "original_form": "ㅠㅠㅠ 그러셨구나..ㅠㅠㅋㅋㅋ 얼른 체력증진을 위해 째끔만 더 힘내세요!!",
          "speaker_id": "1",
          "time": "20210907 23:49"
        },
        {
          "id": "MDRW2100044794.1.6",
          "form": "네...ㅠㅠ 감사해여",
          "original_form": "네...ㅠㅠ 감사해여",
          "speaker_id": "2",
          "time": "20210907 23:51"
        },
        {
          "id": "MDRW2100044794.1.7",
          "form": "별말씀을여 ㅎㅎ 저는 아아아주 드믈게 가아아끔 실내 자전거 타요 ㅋㅋㅋㅋ",
          "original_form": "별말씀을여 ㅎㅎ 저는 아아아주 드믈게 가아아끔 실내 자전거 타요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:54"
        },
        {
          "id": "MDRW2100044794.1.8",
          "form": "앗 그거 옷걸이 아닌가여 ㅋㅋㅋ",
          "original_form": "앗 그거 옷걸이 아닌가여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:55"
        },
        {
          "id": "MDRW2100044794.1.9",
          "form": "ㅋㅋㅋㅋ아시는구나 ㅋㅋㅋㅋㅋㅋ name2님 집에도 있나요?",
          "original_form": "ㅋㅋㅋㅋ아시는구나 ㅋㅋㅋㅋㅋㅋ &name2&님 집에도 있나요?",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044794.1.10",
          "form": "ㅋㅋㅋ저희집엔 동생이 산 턱걸이? 그런 기구욬ㅋㅋ",
          "original_form": "ㅋㅋㅋ저희집엔 동생이 산 턱걸이? 그런 기구욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044794.1.11",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그건 진짜 옷걸이 아닌가요",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋ그건 진짜 옷걸이 아닌가요",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044794.1.12",
          "form": "네네 빨래도 말릴수잇어요 ㅋㅋㅋㅋ",
          "original_form": "네네 빨래도 말릴수잇어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:02"
        },
        {
          "id": "MDRW2100044794.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ 부모님이 좋아하시겠네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 부모님이 좋아하시겠네요",
          "speaker_id": "1",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044794.1.14",
          "form": "아니요 되게 걸리적 거려서 싫어해요 ㅋㅋㅋ",
          "original_form": "아니요 되게 걸리적 거려서 싫어해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044794.1.15",
          "form": "빨래 널때 안좋아하세요?",
          "original_form": "빨래 널때 안좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 00:05"
        },
        {
          "id": "MDRW2100044794.1.16",
          "form": "은근히 이거 있으니까 좋네 이런 늬앙스 없나요?",
