      "utterance": [
        {
          "id": "MDRW2100016943.1.1",
          "form": "안동은 혹시 가보셨나요?",
          "original_form": "안동은 혹시 가보셨나요?",
          "speaker_id": "1",
          "time": "20210826 16:28"
        },
        {
          "id": "MDRW2100016943.1.2",
          "form": "가봤어요. 안동도 너무 좋죠",
          "original_form": "가봤어요. 안동도 너무 좋죠",
          "speaker_id": "2",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016943.1.3",
          "form": "안동 딱 한번가봤는데 조용하고 너무 좋았어요",
          "original_form": "안동 딱 한번가봤는데 조용하고 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016943.1.4",
          "form": "어디가보셨나요?",
          "original_form": "어디가보셨나요?",
          "speaker_id": "1",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016943.1.5",
          "form": "하회마을도 가보고 월영교도 가보고ㅎㅎ",
          "original_form": "하회마을도 가보고 월영교도 가보고ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:33"
        },
        {
          "id": "MDRW2100016943.1.6",
          "form": "한적한게 매력이었어요",
          "original_form": "한적한게 매력이었어요",
          "speaker_id": "2",
          "time": "20210826 16:33"
        },
        {
          "id": "MDRW2100016943.1.7",
          "form": "하회마을도 좋았는데 월영교가 밤에 한적하면서 분위기있고 매력적이였어요",
          "original_form": "하회마을도 좋았는데 월영교가 밤에 한적하면서 분위기있고 매력적이였어요",
          "speaker_id": "1",
          "time": "20210826 16:40"
        },
        {
          "id": "MDRW2100016943.1.8",
          "form": "아직 한적해서 그점이 저도 너무 좋았어요",
          "original_form": "아직 한적해서 그점이 저도 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210826 16:40"
        },
        {
          "id": "MDRW2100016943.1.9",
          "form": "찜닭도 맛있었구요",
          "original_form": "찜닭도 맛있었구요",
          "speaker_id": "1",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100016943.1.10",
          "form": "찜닭 맛있죠ㅎㅎ아 만휴정 가보셨나요?",
          "original_form": "찜닭 맛있죠ㅎㅎ아 만휴정 가보셨나요?",
          "speaker_id": "2",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016943.1.11",
          "form": "가려다가 시간때문에 못갔어요ㅠㅠ만휴정 어떤가요?",
          "original_form": "가려다가 시간때문에 못갔어요ㅠㅠ만휴정 어떤가요?",
          "speaker_id": "1",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100016943.1.12",
          "form": "사진찍으러가는 곳인것같아요ㅎㅎ미스터선샤인땜에",
          "original_form": "사진찍으러가는 곳인것같아요ㅎㅎ미스터선샤인땜에",
          "speaker_id": "2",
          "time": "20210826 16:47"
        },
        {
          "id": "MDRW2100016943.1.13",
          "form": "그렇군요 미스터션샤인 보셨나요?",
          "original_form": "그렇군요 미스터션샤인 보셨나요?",
          "speaker_id": "1",
          "time": "20210826 16:48"
        },
        {
          "id": "MDRW2100016943.1.14",
          "form": "네ㅎㅎ드라마에 나온 곳은 실제로 가면 실망한 적이 많았어요ㅎ",
          "original_form": "네ㅎㅎ드라마에 나온 곳은 실제로 가면 실망한 적이 많았어요ㅎ",
          "speaker_id": "2",
          "time": "20210826 16:50"
        },
        {
          "id": "MDRW2100016943.1.15",
          "form": "미스터션샤인 촬영장이 논산에 있다고해서",
          "original_form": "미스터션샤인 촬영장이 논산에 있다고해서",
          "speaker_id": "1",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100016943.1.16",
          "form": "저도 한번 가보고 싶었는데",
          "original_form": "저도 한번 가보고 싶었는데",
          "speaker_id": "1",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100016943.1.17",
          "form": "별로일까요 ...",
          "original_form": "별로일까요 ...",
          "speaker_id": "1",
          "time": "20210826 16:52"
        },
        {
          "id": "MDRW2100016943.1.18",
          "form": "오 어디인가요?",
          "original_form": "오 어디인가요?",
          "speaker_id": "2",
          "time": "20210826 16:53"
        },
        {
          "id": "MDRW2100016943.1.19",
          "form": "미스터션샤인에 나오는 호텔이랑 다리 거리 촬영장이 논산에 있대요",
          "original_form": "미스터션샤인에 나오는 호텔이랑 다리 거리 촬영장이 논산에 있대요",
          "speaker_id": "1",
          "time": "20210826 16:56"
        },
        {
          "id": "MDRW2100016943.1.20",
          "form": "오 그렇군요 궁금하긴하네요ㅎㅎ",
          "original_form": "오 그렇군요 궁금하긴하네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:57"
        },
        {
          "id": "MDRW2100016943.1.21",
          "form": "논산은 훈련소 이미지가 넘 강렬",
          "original_form": "논산은 훈련소 이미지가 넘 강렬",
          "speaker_id": "2",
          "time": "20210826 16:58"
        },
        {
          "id": "MDRW2100016943.1.22",
          "form": "그죠 논산 너무 군인친구 배웅가야할거 같은 곳이예요",
          "original_form": "그죠 논산 너무 군인친구 배웅가야할거 같은 곳이예요",
          "speaker_id": "1",
          "time": "20210826 17:01"
        }
      ]
