      "utterance": [
        {
          "id": "MDRW2100045936.1.1",
          "form": "이번에 걍 더 저렴이로 바꿨어오",
          "original_form": "이번에 걍 더 저렴이로 바꿨어오",
          "speaker_id": "2",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045936.1.2",
          "form": "오오 어떤거요?",
          "original_form": "오오 어떤거요?",
          "speaker_id": "1",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045936.1.3",
          "form": "성분 괜찮은걸로",
          "original_form": "성분 괜찮은걸로",
          "speaker_id": "2",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045936.1.4",
          "form": "하긴 싼 게 진짜 좋죠",
          "original_form": "하긴 싼 게 진짜 좋죠",
          "speaker_id": "1",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045936.1.5",
          "form": "온더바디 퓨어더마 마일드 바디로션이라고",
          "original_form": "온더바디 퓨어더마 마일드 바디로션이라고",
          "speaker_id": "2",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045936.1.6",
          "form": "성분만 괜찮음",
          "original_form": "성분만 괜찮음",
          "speaker_id": "1",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045936.1.7",
          "form": "성분좋은데",
          "original_form": "성분좋은데",
          "speaker_id": "2",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045936.1.8",
          "form": "오 추천 감사해요",
          "original_form": "오 추천 감사해요",
          "speaker_id": "1",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045936.1.9",
          "form": "아주싸여 세일할때사면",
          "original_form": "아주싸여 세일할때사면",
          "speaker_id": "2",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045936.1.10",
          "form": "저도 바로 갈아탑니다",
          "original_form": "저도 바로 갈아탑니다",
          "speaker_id": "1",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045936.1.11",
          "form": "1000미리 두개에 만원",
          "original_form": "1000미리 두개에 만원",
          "speaker_id": "2",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045936.1.12",
          "form": "그 돈으로 옷 사야죠ㅎㅎ",
          "original_form": "그 돈으로 옷 사야죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045936.1.13",
          "form": "이게 뚜껑이",
          "original_form": "이게 뚜껑이",
          "speaker_id": "2",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045936.1.14",
          "form": "오오 완전 이득인데요?",
          "original_form": "오오 완전 이득인데요?",
          "speaker_id": "1",
          "time": "20210908 08:52"
        },
        {
          "id": "MDRW2100045936.1.15",
          "form": "크게열리는거라",
          "original_form": "크게열리는거라",
