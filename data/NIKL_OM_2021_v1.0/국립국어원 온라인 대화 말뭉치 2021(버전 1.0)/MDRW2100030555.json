      "utterance": [
        {
          "id": "MDRW2100030555.1.1",
          "form": "안녕안녕",
          "original_form": "안녕안녕",
          "speaker_id": "1",
          "time": "20210904 08:34"
        },
        {
          "id": "MDRW2100030555.1.2",
          "form": "좋아하는 연예인 있나요?",
          "original_form": "좋아하는 연예인 있나요?",
          "speaker_id": "1",
          "time": "20210904 08:34"
        },
        {
          "id": "MDRW2100030555.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210904 08:35"
        },
        {
          "id": "MDRW2100030555.1.4",
          "form": "전 아이들 슈화 좋아해요!",
          "original_form": "전 아이들 슈화 좋아해요!",
          "speaker_id": "2",
          "time": "20210904 08:35"
        },
        {
          "id": "MDRW2100030555.1.5",
          "form": "님은 누구 좋아해요?",
          "original_form": "님은 누구 좋아해요?",
          "speaker_id": "2",
          "time": "20210904 08:35"
        },
        {
          "id": "MDRW2100030555.1.6",
          "form": "외국멤버인가요?",
          "original_form": "외국멤버인가요?",
          "speaker_id": "1",
          "time": "20210904 08:35"
        },
        {
          "id": "MDRW2100030555.1.7",
          "form": "이름이 왠지 중국계?",
          "original_form": "이름이 왠지 중국계?",
          "speaker_id": "1",
          "time": "20210904 08:36"
        },
        {
          "id": "MDRW2100030555.1.8",
          "form": "전 BTS요",
          "original_form": "전 BTS요",
          "speaker_id": "1",
          "time": "20210904 08:36"
        },
        {
          "id": "MDRW2100030555.1.9",
          "form": "방탄 잘생겼죠 뷔 디오니소스 안경벗어던지는 영상보고 기절할 뻔 했어요ㅠㅠ",
          "original_form": "방탄 잘생겼죠 뷔 디오니소스 안경벗어던지는 영상보고 기절할 뻔 했어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 08:38"
        },
        {
          "id": "MDRW2100030555.1.10",
          "form": "역시 뭘좀아시는군요ㅋㅋㅋ",
          "original_form": "역시 뭘좀아시는군요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 08:38"
        },
        {
          "id": "MDRW2100030555.1.11",
          "form": "심미안이 뛰어나십니다",
          "original_form": "심미안이 뛰어나십니다",
          "speaker_id": "1",
          "time": "20210904 08:39"
        },
        {
          "id": "MDRW2100030555.1.12",
          "form": "어떻게 그걸 안보겠습니과....",
          "original_form": "어떻게 그걸 안보겠습니과....",
          "speaker_id": "2",
          "time": "20210904 08:40"
        },
        {
          "id": "MDRW2100030555.1.13",
          "form": "뭐였더라",
          "original_form": "뭐였더라",
          "speaker_id": "2",
          "time": "20210904 08:40"
        },
        {
          "id": "MDRW2100030555.1.14",
          "form": "그",
          "original_form": "그",
          "speaker_id": "2",
          "time": "20210904 08:40"
        },
        {
          "id": "MDRW2100030555.1.15",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210904 08:41"
        },
        {
          "id": "MDRW2100030555.1.16",
          "form": "뷔가 자기 16살 흉내내는?",
          "original_form": "뷔가 자기 16살 흉내내는?",
          "speaker_id": "2",
          "time": "20210904 08:41"
        },
        {
          "id": "MDRW2100030555.1.17",
          "form": "유튜브 클립보고",
          "original_form": "유튜브 클립보고",
          "speaker_id": "2",
          "time": "20210904 08:41"
        },
        {
          "id": "MDRW2100030555.1.18",
          "form": "한 10은 돌려봤네요...",
          "original_form": "한 10은 돌려봤네요...",
          "speaker_id": "2",
          "time": "20210904 08:41"
        },
        {
          "id": "MDRW2100030555.1.19",
          "form": "ㅋㅋ 막 그 사투리쓰는ㅋㄱㅋ",
          "original_form": "ㅋㅋ 막 그 사투리쓰는ㅋㄱㅋ",
          "speaker_id": "1",
          "time": "20210904 08:42"
        },
        {
          "id": "MDRW2100030555.1.20",
          "form": "매운거 못먹는다고 하는 그 클립 말씀하시는거죵",
          "original_form": "매운거 못먹는다고 하는 그 클립 말씀하시는거죵",
          "speaker_id": "1",
          "time": "20210904 08:43"
        },
        {
          "id": "MDRW2100030555.1.21",
          "form": "넘 귀엽고 웃겨요",
          "original_form": "넘 귀엽고 웃겨요",
          "speaker_id": "1",
          "time": "20210904 08:43"
        },
        {
          "id": "MDRW2100030555.1.22",
          "form": "네네ㅋㅋ유튜브 알고리즘에 연예인들 많이 뜨는데 방탄이랑     남돌들 여돌들 섞여들어와서",
          "original_form": "네네ㅋㅋ유튜브 알고리즘에 연예인들 많이 뜨는데 방탄이랑     남돌들 여돌들 섞여들어와서",
          "speaker_id": "2",
          "time": "20210904 08:43"
        },
        {
          "id": "MDRW2100030555.1.23",
          "form": "요즘 안구정화하는듯요",
          "original_form": "요즘 안구정화하는듯요",
          "speaker_id": "2",
          "time": "20210904 08:43"
        },
        {
          "id": "MDRW2100030555.1.24",
          "form": "글쿤요",
          "original_form": "글쿤요",
          "speaker_id": "1",
          "time": "20210904 08:43"
        },
        {
          "id": "MDRW2100030555.1.25",
          "form": "유튜브 나름 과학적",
          "original_form": "유튜브 나름 과학적",
          "speaker_id": "1",
          "time": "20210904 08:43"
        },
        {
          "id": "MDRW2100030555.1.26",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 08:43"
        },
        {
          "id": "MDRW2100030555.1.27",
          "form": "지금 사진 찾아봤는데 슈화도 넘 상큼하고 이쁘네요",
          "original_form": "지금 사진 찾아봤는데 슈화도 넘 상큼하고 이쁘네요",
          "speaker_id": "1",
          "time": "20210904 08:44"
        },
        {
          "id": "MDRW2100030555.1.28",
          "form": "그쵸ㅠㅠㅠ슈화가 브이앱에서 꼬질갱얼쥐로 나오는게 제일 귀여워요ㅠㅠ",
          "original_form": "그쵸ㅠㅠㅠ슈화가 브이앱에서 꼬질갱얼쥐로 나오는게 제일 귀여워요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 08:45"
        },
        {
          "id": "MDRW2100030555.1.29",
          "form": "화 무대중에 하얀 소복?같은 거 입고 나온적이 있는데 원래 하얘서 그런지 너무 예쁘더라구요ㅠㅠ",
          "original_form": "화 무대중에 하얀 소복?같은 거 입고 나온적이 있는데 원래 하얘서 그런지 너무 예쁘더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 08:46"
        },
        {
          "id": "MDRW2100030555.1.30",
          "form": "찾아봐야겠어요!!",
          "original_form": "찾아봐야겠어요!!",
          "speaker_id": "1",
