      "utterance": [
        {
          "id": "MDRW2100006458.1.1",
          "form": "공연?",
          "original_form": "공연?",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100006458.1.2",
          "form": "나 뮤지컬 좋아하긴해",
          "original_form": "나 뮤지컬 좋아하긴해",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100006458.1.3",
          "form": "지킬앤하이드 같은 거 유명하지 뮤지컬",
          "original_form": "지킬앤하이드 같은 거 유명하지 뮤지컬",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100006458.1.4",
          "form": "아 보러가기도해??",
          "original_form": "아 보러가기도해??",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100006458.1.5",
          "form": "맘마미아 봤었다",
          "original_form": "맘마미아 봤었다",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100006458.1.6",
          "form": "응응 좋아해",
          "original_form": "응응 좋아해",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100006458.1.7",
          "form": "오 그거 되게 유명한거자나",
          "original_form": "오 그거 되게 유명한거자나",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100006458.1.8",
          "form": "외국인 나오시는거 본거야?",
          "original_form": "외국인 나오시는거 본거야?",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100006458.1.9",
          "form": "맞아맞아 아니 한국에서 하는거 ㅋㅋㅋ",
          "original_form": "맞아맞아 아니 한국에서 하는거 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100006458.1.10",
          "form": "웅 그럼 한국인이얌?? 출연자가?",
          "original_form": "웅 그럼 한국인이얌?? 출연자가?",
          "speaker_id": "1",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100006458.1.11",
          "form": "외국인들 나오시는건 이해 못해서 재미 없어",
          "original_form": "외국인들 나오시는건 이해 못해서 재미 없어",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100006458.1.12",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100006458.1.13",
          "form": "응응 한국인들이 하는거",
          "original_form": "응응 한국인들이 하는거",
          "speaker_id": "2",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100006458.1.14",
          "form": "앗 영어 잘하자나?",
          "original_form": "앗 영어 잘하자나?",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100006458.1.15",
          "form": "뮤지컬이라 어렵나?",
          "original_form": "뮤지컬이라 어렵나?",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100006458.1.16",
          "form": "응응 그리고 외국인들이 노래부르면서 하는건 어려워 ㅋㅋㅋ",
          "original_form": "응응 그리고 외국인들이 노래부르면서 하는건 어려워 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100006458.1.17",
          "form": "앗 맞네 ㅋㅋㅋ 나는 간단한 대화도 어려워섴ㅋㅋ 뮤지컬은 못보겠당",
          "original_form": "앗 맞네 ㅋㅋㅋ 나는 간단한 대화도 어려워섴ㅋㅋ 뮤지컬은 못보겠당",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100006458.1.18",
          "form": "ㅋㅋㅋㅋㅋ한국인들이 하는거 보면 되지",
          "original_form": "ㅋㅋㅋㅋㅋ한국인들이 하는거 보면 되지",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100006458.1.19",
          "form": "재밌어",
          "original_form": "재밌어",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100006458.1.20",
          "form": "그래야겠다!",
          "original_form": "그래야겠다!",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100006458.1.21",
          "form": "뮤지컬 맘마미아 기억해두지",
          "original_form": "뮤지컬 맘마미아 기억해두지",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100006458.1.22",
          "form": "ㅋㅋㅋㅋㅋ다른 것도 재밌는 것 많아",
          "original_form": "ㅋㅋㅋㅋㅋ다른 것도 재밌는 것 많아",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100006458.1.23",
          "form": "티켓 생기면 부르지요",
          "original_form": "티켓 생기면 부르지요",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100006458.1.24",
          "form": "오 오킹",
          "original_form": "오 오킹",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100006458.1.25",
          "form": "최근에 뮤지컬 예매 했었는데 실패했었다?",
          "original_form": "최근에 뮤지컬 예매 했었는데 실패했었다?",
          "speaker_id": "1",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100006458.1.26",
          "form": "인기 엄청나더라",
          "original_form": "인기 엄청나더라",
          "speaker_id": "1",
          "time": "20210908 00:38"
        }
      ]
    }
