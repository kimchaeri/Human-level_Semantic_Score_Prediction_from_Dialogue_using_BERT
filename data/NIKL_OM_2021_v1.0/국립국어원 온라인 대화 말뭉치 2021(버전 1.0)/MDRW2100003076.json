        {
          "id": "MDRW2100003076.1.1",
          "form": "마라탕 끓여먹을까",
          "original_form": "마라탕 끓여먹을까",
          "speaker_id": "2",
          "time": "20210820 18:21"
        },
        {
          "id": "MDRW2100003076.1.2",
          "form": "재료가 있어?",
          "original_form": "재료가 있어?",
          "speaker_id": "1",
          "time": "20210820 18:21"
        },
        {
          "id": "MDRW2100003076.1.3",
          "form": "응 편의점 마라탕훠궈면으로 국물 해",
          "original_form": "응 편의점 마라탕훠궈면으로 국물 해",
          "speaker_id": "2",
          "time": "20210820 18:22"
        },
        {
          "id": "MDRW2100003076.1.4",
          "form": "그거 맛있어?",
          "original_form": "그거 맛있어?",
          "speaker_id": "1",
          "time": "20210820 18:22"
        },
        {
          "id": "MDRW2100003076.1.5",
          "form": "처음엔 향이 적응 안됐는데 먹다보니 맛있어",
          "original_form": "처음엔 향이 적응 안됐는데 먹다보니 맛있어",
          "speaker_id": "2",
          "time": "20210820 18:22"
        },
        {
          "id": "MDRW2100003076.1.6",
          "form": "뭐뭐 들어가?",
          "original_form": "뭐뭐 들어가?",
          "speaker_id": "1",
          "time": "20210820 18:22"
        },
        {
          "id": "MDRW2100003076.1.7",
          "form": "소고기 샤브샤브",
          "original_form": "소고기 샤브샤브",
          "speaker_id": "2",
          "time": "20210820 18:22"
        },
        {
          "id": "MDRW2100003076.1.8",
          "form": "또",
          "original_form": "또",
          "speaker_id": "1",
          "time": "20210820 18:22"
        },
        {
          "id": "MDRW2100003076.1.9",
          "form": "버섯하고 포두부",
          "original_form": "버섯하고 포두부",
          "speaker_id": "2",
          "time": "20210820 18:22"
        },
        {
          "id": "MDRW2100003076.1.10",
          "form": "당면",
          "original_form": "당면",
          "speaker_id": "2",
          "time": "20210820 18:22"
        },
        {
          "id": "MDRW2100003076.1.11",
          "form": "먹어보고 싶다",
          "original_form": "먹어보고 싶다",
          "speaker_id": "1",
          "time": "20210820 18:23"
        },
        {
          "id": "MDRW2100003076.1.12",
          "form": "나중에 한번 먹어봐",
