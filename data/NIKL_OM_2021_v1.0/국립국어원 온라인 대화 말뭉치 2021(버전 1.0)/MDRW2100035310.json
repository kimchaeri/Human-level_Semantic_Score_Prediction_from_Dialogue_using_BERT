      "utterance": [
        {
          "id": "MDRW2100035310.1.1",
          "form": "친구분 부러운걸요..",
          "original_form": "친구분 부러운걸요..",
          "speaker_id": "1",
          "time": "20210829 14:23"
        },
        {
          "id": "MDRW2100035310.1.2",
          "form": "네 근데 저는 그게 뭔지 못 알아보고",
          "original_form": "네 근데 저는 그게 뭔지 못 알아보고",
          "speaker_id": "2",
          "time": "20210829 14:25"
        },
        {
          "id": "MDRW2100035310.1.3",
          "form": "원하는만큼 신제품에 대한 격한 리액션을 못했어요",
          "original_form": "원하는만큼 신제품에 대한 격한 리액션을 못했어요",
          "speaker_id": "2",
          "time": "20210829 14:25"
        },
        {
          "id": "MDRW2100035310.1.4",
          "form": "저도 아빠 쓰시기전까지는 그냥 그랬어요",
          "original_form": "저도 아빠 쓰시기전까지는 그냥 그랬어요",
          "speaker_id": "1",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100035310.1.5",
          "form": "필요한가?는 생각이였거든요",
          "original_form": "필요한가?는 생각이였거든요",
          "speaker_id": "1",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100035310.1.6",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210829 14:28"
        },
        {
          "id": "MDRW2100035310.1.7",
          "form": "왠지 저는 사도 잘 안쓸 것 같고",
          "original_form": "왠지 저는 사도 잘 안쓸 것 같고",
          "speaker_id": "2",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100035310.1.8",
          "form": "저도요ㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 14:34"
        },
        {
          "id": "MDRW2100035310.1.9",
          "form": "근데 심장박동이던가 잠자는거 체크하는건 해보고싶더라구요",
          "original_form": "근데 심장박동이던가 잠자는거 체크하는건 해보고싶더라구요",
          "speaker_id": "1",
          "time": "20210829 14:34"
        },
        {
          "id": "MDRW2100035310.1.10",
          "form": "네네 궁금하긴 해요",
          "original_form": "네네 궁금하긴 해요",
          "speaker_id": "2",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100035310.1.11",
          "form": "새로 나온 갤럭시 워치는 인바디 기능도 있대요",
          "original_form": "새로 나온 갤럭시 워치는 인바디 기능도 있대요",
          "speaker_id": "2",
          "time": "20210829 14:37"
        },
        {
          "id": "MDRW2100035310.1.12",
          "form": "우와.. 인바디 따로 안사도 되네요..",
          "original_form": "우와.. 인바디 따로 안사도 되네요..",
          "speaker_id": "1",
          "time": "20210829 14:40"
        },
        {
          "id": "MDRW2100035310.1.13",
          "form": "생각할수록 탐나네요",
          "original_form": "생각할수록 탐나네요",
          "speaker_id": "1",
          "time": "20210829 14:41"
        },
        {
          "id": "MDRW2100035310.1.14",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210829 14:42"
        },
        {
          "id": "MDRW2100035310.1.15",
          "form": "최근 기종은 기능이 참 다양해서",
          "original_form": "최근 기종은 기능이 참 다양해서",
          "speaker_id": "2",
          "time": "20210829 14:42"
        },
        {
          "id": "MDRW2100035310.1.16",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "1",
          "time": "20210829 14:54"
        },
        {
          "id": "MDRW2100035310.1.17",
          "form": "저 인바디 기계 살까 했는데 그 돈이면 스마트 워치가 나을거같아요",
          "original_form": "저 인바디 기계 살까 했는데 그 돈이면 스마트 워치가 나을거같아요",
          "speaker_id": "1",
          "time": "20210829 14:54"
        },
        {
          "id": "MDRW2100035310.1.18",
          "form": "오 인바디 사려고 하셨구나",
          "original_form": "오 인바디 사려고 하셨구나",
          "speaker_id": "2",
          "time": "20210829 15:10"
        },
        {
          "id": "MDRW2100035310.1.19",
          "form": "그럼 스마트워치 사도 좋겠어요",
          "original_form": "그럼 스마트워치 사도 좋겠어요",
          "speaker_id": "2",
          "time": "20210829 15:10"
        },
        {
          "id": "MDRW2100035310.1.20",
          "form": "네넹! 알아봐야겠어요~",
          "original_form": "네넹! 알아봐야겠어요~",
          "speaker_id": "1",
          "time": "20210829 15:19"
        },
        {
          "id": "MDRW2100035310.1.21",
          "form": "구매 희망 물품 추가네요",
          "original_form": "구매 희망 물품 추가네요",
          "speaker_id": "2",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100035310.1.22",
          "form": "그러네용ㅋㅋㄱ",
          "original_form": "그러네용ㅋㅋㄱ",
