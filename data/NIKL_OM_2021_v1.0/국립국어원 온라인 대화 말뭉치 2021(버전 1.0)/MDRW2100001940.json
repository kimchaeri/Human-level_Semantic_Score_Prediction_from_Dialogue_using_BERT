      "utterance": [
        {
          "id": "MDRW2100001940.1.1",
          "form": "요즘 날씨가 건조해져서 그런가 주름이 많이 생겨",
          "original_form": "요즘 날씨가 건조해져서 그런가 주름이 많이 생겨",
          "speaker_id": "1",
          "time": "20210819 22:23"
        },
        {
          "id": "MDRW2100001940.1.2",
          "form": "얼굴에 팩 좀 해봐",
          "original_form": "얼굴에 팩 좀 해봐",
          "speaker_id": "2",
          "time": "20210819 22:23"
        },
        {
          "id": "MDRW2100001940.1.3",
          "form": "많이 있더만",
          "original_form": "많이 있더만",
          "speaker_id": "2",
          "time": "20210819 22:23"
        },
        {
          "id": "MDRW2100001940.1.4",
          "form": "고무팩이 귀찮기도 하고 ㅠ 손이 많이 가서",
          "original_form": "고무팩이 귀찮기도 하고 ㅠ 손이 많이 가서",
          "speaker_id": "1",
          "time": "20210819 22:23"
        },
        {
          "id": "MDRW2100001940.1.5",
          "form": "그래도 주름을 위해서라면",
          "original_form": "그래도 주름을 위해서라면",
          "speaker_id": "2",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100001940.1.6",
          "form": "팩으로 펴는 게 한계가 있어서 ㅎㅎ보톡스 맞아야 하나",
          "original_form": "팩으로 펴는 게 한계가 있어서 ㅎㅎ보톡스 맞아야 하나",
          "speaker_id": "1",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100001940.1.7",
          "form": "그거 잘못 맞으면 선풍이 아줌마 돼~ 지금도 충분히 예뻐 ㅋ",
          "original_form": "그거 잘못 맞으면 선풍이 아줌마 돼~ 지금도 충분히 예뻐 ㅋ",
          "speaker_id": "2",
          "time": "20210819 22:24"
        },
        {
          "id": "MDRW2100001940.1.8",
          "form": "그래도 웃을 때마다 신경쓰여 ㅠㅠ",
          "original_form": "그래도 웃을 때마다 신경쓰여 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 22:25"
        },
        {
          "id": "MDRW2100001940.1.9",
          "form": "화장품도 좋은 거 써~ 수분 잔뜩 있는 걸로",
          "original_form": "화장품도 좋은 거 써~ 수분 잔뜩 있는 걸로",
          "speaker_id": "2",
          "time": "20210819 22:25"
        },
        {
          "id": "MDRW2100001940.1.10",
          "form": "수분 영양 많은 거 쓰면 얼굴이 찐득거려서 불편 ㅎㅎ",
          "original_form": "수분 영양 많은 거 쓰면 얼굴이 찐득거려서 불편 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 22:25"
        },
        {
          "id": "MDRW2100001940.1.11",
          "form": "난 아예 못발라 ㅋㅋ 위에 한 겹이 더 있는 느낌이야",
          "original_form": "난 아예 못발라 ㅋㅋ 위에 한 겹이 더 있는 느낌이야",
          "speaker_id": "2",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001940.1.12",
          "form": "신랑은 좀 발라야돼 ㅎㅎ 나만 늙는 게 아냐",
          "original_form": "신랑은 좀 발라야돼 ㅎㅎ 나만 늙는 게 아냐",
          "speaker_id": "1",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001940.1.13",
          "form": "아오 늙어도 화장품은 못바르겠어",
          "original_form": "아오 늙어도 화장품은 못바르겠어",
          "speaker_id": "2",
          "time": "20210819 22:26"
        },
        {
          "id": "MDRW2100001940.1.14",
          "form": "뭐라도 해봐야겠다... 나중에 주름 너무 많다고 구박하지마",
          "original_form": "뭐라도 해봐야겠다... 나중에 주름 너무 많다고 구박하지마",
          "speaker_id": "1",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001940.1.15",
          "form": "ㅎㅎ 절대 그럴 일 없으니 안심햐",
          "original_form": "ㅎㅎ 절대 그럴 일 없으니 안심햐",
          "speaker_id": "2",
          "time": "20210819 22:27"
        },
        {
          "id": "MDRW2100001940.1.16",
          "form": "오늘 날씨 좋아서 산책 가야겠다",
          "original_form": "오늘 날씨 좋아서 산책 가야겠다",
