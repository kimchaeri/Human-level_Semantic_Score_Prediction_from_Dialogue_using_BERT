      "utterance": [
        {
          "id": "MDRW2100023543.1.1",
          "form": "오늘 점심머먹을꺼에요?",
          "original_form": "오늘 점심머먹을꺼에요?",
          "speaker_id": "1",
          "time": "20210830 11:48"
        },
        {
          "id": "MDRW2100023543.1.2",
          "form": "아직 미정이에요! name1은요?",
          "original_form": "아직 미정이에요! &name1&은요?",
          "speaker_id": "2",
          "time": "20210830 11:49"
        },
        {
          "id": "MDRW2100023543.1.3",
          "form": "저는 피자빵먹을려구요",
          "original_form": "저는 피자빵먹을려구요",
          "speaker_id": "1",
          "time": "20210830 11:50"
        },
        {
          "id": "MDRW2100023543.1.4",
          "form": "name1도 밀가루 좋아하시는거같아요!",
          "original_form": "&name1&도 밀가루 좋아하시는거같아요!",
          "speaker_id": "2",
          "time": "20210830 11:50"
        },
        {
          "id": "MDRW2100023543.1.5",
          "form": "맞아요ㅜㅜ밀가루음식좋아해요",
          "original_form": "맞아요ㅜㅜ밀가루음식좋아해요",
          "speaker_id": "1",
          "time": "20210830 15:31"
        },
        {
          "id": "MDRW2100023543.1.6",
          "form": "저녁은 햄버거 먹을까 고민되네요",
          "original_form": "저녁은 햄버거 먹을까 고민되네요",
          "speaker_id": "2",
          "time": "20210830 15:33"
        },
        {
          "id": "MDRW2100023543.1.7",
          "form": "저는 저녁에도 피자빵을 먹었어요",
          "original_form": "저는 저녁에도 피자빵을 먹었어요",
          "speaker_id": "1",
          "time": "20210830 21:23"
        },
        {
          "id": "MDRW2100023543.1.8",
          "form": "앗 그렇군요~~ 피자빵 좋아하시나봐요",
          "original_form": "앗 그렇군요~~ 피자빵 좋아하시나봐요",
          "speaker_id": "2",
          "time": "20210831 14:54"
        },
        {
          "id": "MDRW2100023543.1.9",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 14:56"
        },
        {
          "id": "MDRW2100023543.1.10",
          "form": "오늘 배스킨갈꺼에요",
          "original_form": "오늘 배스킨갈꺼에요",
          "speaker_id": "1",
          "time": "20210831 14:57"
        },
        {
          "id": "MDRW2100023543.1.11",
          "form": "오호 전 사전예약 해뒀어요",
          "original_form": "오호 전 사전예약 해뒀어요",
          "speaker_id": "2",
          "time": "20210831 14:57"
        },
        {
          "id": "MDRW2100023543.1.12",
          "form": "배스킨왔는데 바나나맛이 있어서 오늘 드디어 먹게 됐어요",
          "original_form": "배스킨왔는데 바나나맛이 있어서 오늘 드디어 먹게 됐어요",
          "speaker_id": "1",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100023543.1.13",
          "form": "그리고 오늘 지인한테 스벅음료무료로받아서 기분날아갈것같네요",
          "original_form": "그리고 오늘 지인한테 스벅음료무료로받아서 기분날아갈것같네요",
          "speaker_id": "1",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100023543.1.14",
          "form": "스벅도가야겠어요",
          "original_form": "스벅도가야겠어요",
          "speaker_id": "1",
          "time": "20210831 16:28"
        },
        {
          "id": "MDRW2100023543.1.15",
          "form": "바나나맛 맛있나보네요!!",
          "original_form": "바나나맛 맛있나보네요!!",
          "speaker_id": "2",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100023543.1.16",
          "form": "스벅에서 딸기음료 드실건가요? ㅋㅋ",
          "original_form": "스벅에서 딸기음료 드실건가요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 16:56"
        },
        {
          "id": "MDRW2100023543.1.17",
          "form": "바나나사서 버스탔는데 버스내리면 먹을려구요",
          "original_form": "바나나사서 버스탔는데 버스내리면 먹을려구요",
          "speaker_id": "1",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100023543.1.18",
          "form": "ㅡㅅㅡ",
          "original_form": "ㅡㅅㅡ",
          "speaker_id": "1",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100023543.1.19",
          "form": "버스안에서먹을수가없어서",
          "original_form": "버스안에서먹을수가없어서",
          "speaker_id": "1",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100023543.1.20",
          "form": "녹았어요ㅜㅜ",
          "original_form": "녹았어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100023543.1.21",
          "form": "40분타고 내렸는데",
          "original_form": "40분타고 내렸는데",
          "speaker_id": "1",
          "time": "20210831 17:30"
        },
        {
          "id": "MDRW2100023543.1.22",
          "form": "지금 바나나맛 떠먹어야하는데",
          "original_form": "지금 바나나맛 떠먹어야하는데",
          "speaker_id": "1",
          "time": "20210831 17:31"
        },
        {
          "id": "MDRW2100023543.1.23",
          "form": "마시고있엊요",
          "original_form": "마시고있엊요",
