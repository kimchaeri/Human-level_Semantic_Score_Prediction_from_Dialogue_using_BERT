        {
          "id": "MDRW2100008864.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210822 19:13"
        },
        {
          "id": "MDRW2100008864.1.2",
          "form": "최근에 가신 여행지 있으세요~!?",
          "original_form": "최근에 가신 여행지 있으세요~!?",
          "speaker_id": "1",
          "time": "20210822 19:13"
        },
        {
          "id": "MDRW2100008864.1.3",
          "form": "최근에 강릉 다녀왔어요 ~",
          "original_form": "최근에 강릉 다녀왔어요 ~",
          "speaker_id": "2",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008864.1.4",
          "form": "오 강릉!!",
          "original_form": "오 강릉!!",
          "speaker_id": "1",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008864.1.5",
          "form": "강릉 한번도 안가봤는데 머가 유명해요!?",
          "original_form": "강릉 한번도 안가봤는데 머가 유명해요!?",
          "speaker_id": "1",
          "time": "20210822 19:14"
        },
        {
          "id": "MDRW2100008864.1.6",
          "form": "헉 한번도 가보셨어요??",
          "original_form": "헉 한번도 가보셨어요??",
          "speaker_id": "2",
          "time": "20210822 19:15"
        },
        {
          "id": "MDRW2100008864.1.7",
          "form": "어디 다니세용 그럼?",
          "original_form": "어디 다니세용 그럼?",
          "speaker_id": "2",
          "time": "20210822 19:15"
        },
        {
          "id": "MDRW2100008864.1.8",
          "form": "음 최근에는 고흥 갔어욤",
          "original_form": "음 최근에는 고흥 갔어욤",
          "speaker_id": "1",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008864.1.9",
          "form": "오 전 고흥을 안가봣어여",
          "original_form": "오 전 고흥을 안가봣어여",
          "speaker_id": "2",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008864.1.10",
          "form": "강릉 볼거많아용 뭐니뭐니해도 동해바다죠",
          "original_form": "강릉 볼거많아용 뭐니뭐니해도 동해바다죠",
          "speaker_id": "2",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008864.1.11",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008864.1.12",
          "form": "오 바ㅏㄷ 좋죠!!",
          "original_form": "오 바ㅏㄷ 좋죠!!",
          "speaker_id": "1",
          "time": "20210822 19:16"
        },
        {
          "id": "MDRW2100008864.1.13",
          "form": "여행가면",
          "original_form": "여행가면",
          "speaker_id": "2",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008864.1.14",
          "form": "기념품가게 가는거 재밌어용",
          "original_form": "기념품가게 가는거 재밌어용",
          "speaker_id": "2",
          "time": "20210822 19:17"
        },
        {
          "id": "MDRW2100008864.1.15",
          "form": "아하 저는 딱히 기념품 모으는거 별로 안좋아해서 잘은 안가는데",
          "original_form": "아하 저는 딱히 기념품 모으는거 별로 안좋아해서 잘은 안가는데",
          "speaker_id": "1",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008864.1.16",
          "form": "이쁘면 사게되더라구요 ㅋㅋㅋㅋ",
          "original_form": "이쁘면 사게되더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 19:18"
        },
        {
          "id": "MDRW2100008864.1.17",
          "form": "맞아영 근데 비싸요 ㅠㅋㅋ",
          "original_form": "맞아영 근데 비싸요 ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 19:19"
        },
        {
          "id": "MDRW2100008864.1.18",
