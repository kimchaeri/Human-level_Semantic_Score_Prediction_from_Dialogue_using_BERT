      "utterance": [
        {
          "id": "MDRW2100021080.1.1",
          "form": "넷플릭스 좋아하는 프로 이써요?",
          "original_form": "넷플릭스 좋아하는 프로 이써요?",
          "speaker_id": "2",
          "time": "20210828 16:05"
        },
        {
          "id": "MDRW2100021080.1.2",
          "form": "킹덤이요!",
          "original_form": "킹덤이요!",
          "speaker_id": "1",
          "time": "20210828 16:30"
        },
        {
          "id": "MDRW2100021080.1.3",
          "form": "오  킹덤 재미있죠 은근 현실풍자도 많고 ㅋㅋㅋ",
          "original_form": "오  킹덤 재미있죠 은근 현실풍자도 많고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021080.1.4",
          "form": "아신전까지 봤네요ㅎㅎ",
          "original_form": "아신전까지 봤네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021080.1.5",
          "form": "저도 엄청 재미있게 봤어요 ㅎㅎ",
          "original_form": "저도 엄청 재미있게 봤어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100021080.1.6",
          "form": "저는 동물다큐멘터리 좋아해서 그런거 많이 봤어요 ㅎ",
          "original_form": "저는 동물다큐멘터리 좋아해서 그런거 많이 봤어요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100021080.1.7",
          "form": "넷플에도 있나요?",
          "original_form": "넷플에도 있나요?",
          "speaker_id": "1",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100021080.1.8",
          "form": "네 최근에 나의 문어 선생님  재미있게 봤어요",
          "original_form": "네 최근에 나의 문어 선생님  재미있게 봤어요",
          "speaker_id": "2",
          "time": "20210828 17:27"
        },
        {
          "id": "MDRW2100021080.1.9",
          "form": "오 신기하네요ㅎㅎ",
          "original_form": "오 신기하네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100021080.1.10",
          "form": "심신을 달랠만 한 다큐였어요 ㅎㅎ",
          "original_form": "심신을 달랠만 한 다큐였어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021080.1.11",
          "form": "동물 다큐 보면  살기위해 최선을 다 하는거 보면서 반성도 하고 깨달음을 얻기도 하고 ㅎㅎ",
          "original_form": "동물 다큐 보면  살기위해 최선을 다 하는거 보면서 반성도 하고 깨달음을 얻기도 하고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 17:31"
        },
        {
          "id": "MDRW2100021080.1.12",
          "form": "오 힐링되겠어요",
          "original_form": "오 힐링되겠어요",
          "speaker_id": "1",
          "time": "20210828 17:32"
        },
        {
          "id": "MDRW2100021080.1.13",
          "form": "네 그래서 자주봐요",
          "original_form": "네 그래서 자주봐요",
          "speaker_id": "2",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100021080.1.14",
          "form": "영화나 외국드라마도 간단히 봐요 ㅎ",
          "original_form": "영화나 외국드라마도 간단히 봐요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 17:48"
        },
        {
          "id": "MDRW2100021080.1.15",
          "form": "스위트홈도 재밌었네요",
          "original_form": "스위트홈도 재밌었네요",
