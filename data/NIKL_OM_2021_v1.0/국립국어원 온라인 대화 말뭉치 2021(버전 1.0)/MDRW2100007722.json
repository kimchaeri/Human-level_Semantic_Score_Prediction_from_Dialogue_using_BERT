      "utterance": [
        {
          "id": "MDRW2100007722.1.1",
          "form": "중고거래해요?",
          "original_form": "중고거래해요?",
          "speaker_id": "1",
          "time": "20210822 09:56"
        },
        {
          "id": "MDRW2100007722.1.2",
          "form": "안해요?",
          "original_form": "안해요?",
          "speaker_id": "1",
          "time": "20210823 08:40"
        },
        {
          "id": "MDRW2100007722.1.3",
          "form": "바빴어요. 중고거래하죠.ㅋ 당근 잘쓰고있어요",
          "original_form": "바빴어요. 중고거래하죠.ㅋ 당근 잘쓰고있어요",
          "speaker_id": "2",
          "time": "20210823 08:45"
        },
        {
          "id": "MDRW2100007722.1.4",
          "form": "중고거래하세요?",
          "original_form": "중고거래하세요?",
          "speaker_id": "2",
          "time": "20210823 08:45"
        },
        {
          "id": "MDRW2100007722.1.5",
          "form": "동네라 편하죠",
          "original_form": "동네라 편하죠",
          "speaker_id": "1",
          "time": "20210823 08:45"
        },
        {
          "id": "MDRW2100007722.1.6",
          "form": "네 동네라서 편해요. 저렴하기도하구요.",
          "original_form": "네 동네라서 편해요. 저렴하기도하구요.",
          "speaker_id": "2",
          "time": "20210823 08:48"
        },
        {
          "id": "MDRW2100007722.1.7",
          "form": "빠르죠",
          "original_form": "빠르죠",
          "speaker_id": "1",
          "time": "20210823 08:49"
        },
        {
          "id": "MDRW2100007722.1.8",
          "form": "네네 어떨때는 쿠팡보다 더 빠른듯요.ㅋㅋ",
          "original_form": "네네 어떨때는 쿠팡보다 더 빠른듯요.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 08:49"
        },
        {
          "id": "MDRW2100007722.1.9",
          "form": "맞아요 ㅋ",
          "original_form": "맞아요 ㅋ",
          "speaker_id": "1",
          "time": "20210823 08:50"
        },
        {
          "id": "MDRW2100007722.1.10",
          "form": "최근에는 첫째 한글공부책 팔았어요.",
          "original_form": "최근에는 첫째 한글공부책 팔았어요.",
          "speaker_id": "2",
          "time": "20210823 08:51"
        },
        {
          "id": "MDRW2100007722.1.11",
          "form": "누가 사갈까했는데",
          "original_form": "누가 사갈까했는데",
          "speaker_id": "2",
          "time": "20210823 08:51"
        },
        {
          "id": "MDRW2100007722.1.12",
          "form": "저렴하게 올리니 사가더라구요.",
          "original_form": "저렴하게 올리니 사가더라구요.",
          "speaker_id": "2",
          "time": "20210823 08:51"
        },
        {
          "id": "MDRW2100007722.1.13",
          "form": "용돈벌었네요ㅋ",
          "original_form": "용돈벌었네요ㅋ",
          "speaker_id": "2",
          "time": "20210823 08:51"
        },
        {
          "id": "MDRW2100007722.1.14",
          "form": "맞아요 저렴하면",
          "original_form": "맞아요 저렴하면",
          "speaker_id": "1",
          "time": "20210823 08:52"
        },
        {
          "id": "MDRW2100007722.1.15",
          "form": "그러니깐요. 딸도 해보고는 재미있다고 계속 팔거를 찾더라구요ㅋ",
          "original_form": "그러니깐요. 딸도 해보고는 재미있다고 계속 팔거를 찾더라구요ㅋ",
          "speaker_id": "2",
          "time": "20210823 08:52"
        },
        {
          "id": "MDRW2100007722.1.16",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 08:53"
        },
        {
          "id": "MDRW2100007722.1.17",
          "form": "적성이죠",
          "original_form": "적성이죠",
          "speaker_id": "1",
          "time": "20210823 08:53"
        },
        {
          "id": "MDRW2100007722.1.18",
          "form": "책도 팔고 매트도 팔았어요ㅋ",
          "original_form": "책도 팔고 매트도 팔았어요ㅋ",
          "speaker_id": "2",
          "time": "20210823 08:54"
        },
        {
          "id": "MDRW2100007722.1.19",
          "form": "집정리됐네요ㅋ",
          "original_form": "집정리됐네요ㅋ",
          "speaker_id": "2",
          "time": "20210823 08:54"
        },
        {
          "id": "MDRW2100007722.1.20",
          "form": "정리는 당근이죠",
          "original_form": "정리는 당근이죠",
          "speaker_id": "1",
          "time": "20210823 09:36"
        }
