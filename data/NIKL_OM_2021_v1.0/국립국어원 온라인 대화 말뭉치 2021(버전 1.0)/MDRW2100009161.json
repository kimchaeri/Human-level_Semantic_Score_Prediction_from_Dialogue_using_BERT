      "utterance": [
        {
          "id": "MDRW2100009161.1.1",
          "form": "여보, 오늘은 뭐해 먹을까요?",
          "original_form": "여보, 오늘은 뭐해 먹을까요?",
          "speaker_id": "2",
          "time": "20210823 07:50"
        },
        {
          "id": "MDRW2100009161.1.2",
          "form": "어제 먹나 남은 족발 어때?",
          "original_form": "어제 먹나 남은 족발 어때?",
          "speaker_id": "1",
          "time": "20210823 12:29"
        },
        {
          "id": "MDRW2100009161.1.3",
          "form": "그거도 먹고 또 뭘 할까오",
          "original_form": "그거도 먹고 또 뭘 할까오",
          "speaker_id": "2",
          "time": "20210823 12:31"
        },
        {
          "id": "MDRW2100009161.1.4",
          "form": "난 매콤한 것도 좋은데 김치볶음 어때?",
          "original_form": "난 매콤한 것도 좋은데 김치볶음 어때?",
          "speaker_id": "1",
          "time": "20210823 12:31"
        },
        {
          "id": "MDRW2100009161.1.5",
          "form": "좋아요 그리고요?",
          "original_form": "좋아요 그리고요?",
          "speaker_id": "2",
          "time": "20210823 12:31"
        },
        {
          "id": "MDRW2100009161.1.6",
          "form": "냉장고 보니까 나물재료도 있더만",
          "original_form": "냉장고 보니까 나물재료도 있더만",
          "speaker_id": "1",
          "time": "20210823 12:32"
        },
        {
          "id": "MDRW2100009161.1.7",
          "form": "나물도 무칠게요",
          "original_form": "나물도 무칠게요",
          "speaker_id": "2",
          "time": "20210823 12:32"
        },
        {
          "id": "MDRW2100009161.1.8",
          "form": "난 밥 해놓을게",
          "original_form": "난 밥 해놓을게",
          "speaker_id": "1",
          "time": "20210823 12:32"
        },
        {
          "id": "MDRW2100009161.1.9",
          "form": "너무질게하진말아요",
          "original_form": "너무질게하진말아요",
          "speaker_id": "2",
          "time": "20210823 12:33"
        },
        {
          "id": "MDRW2100009161.1.10",
          "form": "난 고들밥 시른더",
          "original_form": "난 고들밥 시른더",
          "speaker_id": "1",
          "time": "20210823 12:33"
        },
        {
          "id": "MDRW2100009161.1.11",
          "form": "그래도 죽밥처럼 되면 안되니까요",
          "original_form": "그래도 죽밥처럼 되면 안되니까요",
          "speaker_id": "2",
          "time": "20210823 12:33"
        },
        {
          "id": "MDRW2100009161.1.12",
          "form": "알았어 물양 잘 맞춰볼게",
          "original_form": "알았어 물양 잘 맞춰볼게",
          "speaker_id": "1",
          "time": "20210823 12:34"
        },
        {
          "id": "MDRW2100009161.1.13",
          "form": "더 필요한 것 없죠?",
          "original_form": "더 필요한 것 없죠?",
          "speaker_id": "2",
          "time": "20210823 12:34"
        },
        {
          "id": "MDRW2100009161.1.14",
          "form": "응 점심은 그렇게 먹고 저녁은 시켜먹자",
          "original_form": "응 점심은 그렇게 먹고 저녁은 시켜먹자",
          "speaker_id": "1",
          "time": "20210823 12:35"
        },
        {
          "id": "MDRW2100009161.1.15",
          "form": "그래요",
          "original_form": "그래요",
          "speaker_id": "2",
          "time": "20210823 12:35"
        }
      ]
