      "utterance": [
        {
          "id": "MDRW2100007517.1.1",
          "form": "어떻게 군필이신가요?",
          "original_form": "어떻게 군필이신가요?",
          "speaker_id": "1",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007517.1.2",
          "form": "네 저는 군필입니다 ㅋㅋㅋ 17군번이에요",
          "original_form": "네 저는 군필입니다 ㅋㅋㅋ 17군번이에요",
          "speaker_id": "2",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007517.1.3",
          "form": "형님이시네요 ㅋㅋㅋㅋㅋ전 19군번입니다",
          "original_form": "형님이시네요 ㅋㅋㅋㅋㅋ전 19군번입니다",
          "speaker_id": "1",
          "time": "20210822 02:46"
        },
        {
          "id": "MDRW2100007517.1.4",
          "form": "이제 전역한지 1년이에요",
          "original_form": "이제 전역한지 1년이에요",
          "speaker_id": "1",
          "time": "20210822 02:47"
        },
        {
          "id": "MDRW2100007517.1.5",
          "form": "육해공 중에 어디 다녀오셨어요?",
          "original_form": "육해공 중에 어디 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007517.1.6",
          "form": "ㅋㅋㅋ 저랑 딱 교대하셨네요! 저는 19년 전역이라",
          "original_form": "ㅋㅋㅋ 저랑 딱 교대하셨네요! 저는 19년 전역이라",
          "speaker_id": "2",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007517.1.7",
          "form": "저는 육군이었습니다 ㅋㅋ",
          "original_form": "저는 육군이었습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007517.1.8",
          "form": "저도 육군입니다!ㅋㅋㅋ",
          "original_form": "저도 육군입니다!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007517.1.9",
          "form": "17군번이시면 부조리가 좀 있었을 때 이셨겠네요?",
          "original_form": "17군번이시면 부조리가 좀 있었을 때 이셨겠네요?",
          "speaker_id": "1",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007517.1.10",
          "form": "기억나시는 선임이나 있으세요?? 오래전이라 잘 기억 안 나시려나?",
          "original_form": "기억나시는 선임이나 있으세요?? 오래전이라 잘 기억 안 나시려나?",
          "speaker_id": "1",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007517.1.11",
          "form": "ㅋㅋㅋ 그래도 저희는 부조리 거의 없었습니다",
          "original_form": "ㅋㅋㅋ 그래도 저희는 부조리 거의 없었습니다",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007517.1.12",
          "form": "ㅋㅋㅋㅋ다행이네요",
          "original_form": "ㅋㅋㅋㅋ다행이네요",
          "speaker_id": "1",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007517.1.13",
          "form": "지금 기준이라면 있다고 느껴질수도 있지만 ㅋㅋㅋ 그래도 거의 없었어요",
          "original_form": "지금 기준이라면 있다고 느껴질수도 있지만 ㅋㅋㅋ 그래도 거의 없었어요",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007517.1.14",
          "form": "혹시 보직은 어땠어요?",
          "original_form": "혹시 보직은 어땠어요?",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007517.1.15",
          "form": "전 그냥 보병이었어요",
          "original_form": "전 그냥 보병이었어요",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007517.1.16",
          "form": "그냥 예비군 훈련조교 같은 일 하다 나왔어요",
          "original_form": "그냥 예비군 훈련조교 같은 일 하다 나왔어요",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007517.1.17",
          "form": "아아 그쪽이구나",
          "original_form": "아아 그쪽이구나",
          "speaker_id": "2",
          "time": "20210822 02:52"
        },
        {
          "id": "MDRW2100007517.1.18",
          "form": "제 친구도 그런쪽으로 갔었는데 ㅋㅋㅋ",
          "original_form": "제 친구도 그런쪽으로 갔었는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:52"
        },
        {
          "id": "MDRW2100007517.1.19",
          "form": "나름 꿀이었죠ㅋㅋㅋㅋㅋ",
          "original_form": "나름 꿀이었죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007517.1.20",
          "form": "혹시 어느쪽으로 다녀오셨어요??",
          "original_form": "혹시 어느쪽으로 다녀오셨어요??",
          "speaker_id": "1",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007517.1.21",
          "form": "저는 헌병으로 다녀왔어요",
          "original_form": "저는 헌병으로 다녀왔어요",
          "speaker_id": "2",
          "time": "20210822 02:53"
        },
        {
          "id": "MDRW2100007517.1.22",
          "form": "엄청난 분이셨군요?ㅋㅋㅋㅋㅋ",
          "original_form": "엄청난 분이셨군요?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007517.1.23",
          "form": "헌병이 지원받나요?? 아니면 그냥 배치 받아요?",
          "original_form": "헌병이 지원받나요?? 아니면 그냥 배치 받아요?",
          "speaker_id": "1",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007517.1.24",
          "form": "저는 지원으로 갔는데 제 동기는 배치받았어요",
          "original_form": "저는 지원으로 갔는데 제 동기는 배치받았어요",
          "speaker_id": "2",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007517.1.25",
          "form": "훈련소에서 차출되더라구요 ㅋㅋㅋ",
          "original_form": "훈련소에서 차출되더라구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 02:54"
        },
        {
          "id": "MDRW2100007517.1.26",
          "form": "아 헌병도 훈련소 차출이 있나보네요",
          "original_form": "아 헌병도 훈련소 차출이 있나보네요",
          "speaker_id": "1",
          "time": "20210822 02:55"
        },
