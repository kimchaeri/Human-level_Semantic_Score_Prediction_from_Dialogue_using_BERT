      "utterance": [
        {
          "id": "MDRW2100016668.1.1",
          "form": "name3선배랑 name4선배도 같이 갔나?",
          "original_form": "&name3&선배랑 &name4&선배도 같이 갔나?",
          "speaker_id": "2",
          "time": "20210826 14:38"
        },
        {
          "id": "MDRW2100016668.1.2",
          "form": "2학년들 같이 떠남?",
          "original_form": "2학년들 같이 떠남?",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016668.1.3",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016668.1.4",
          "form": "그런듯",
          "original_form": "그런듯",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016668.1.5",
          "form": "둘이 룸메도 아니면서",
          "original_form": "둘이 룸메도 아니면서",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016668.1.6",
          "form": "유일한 동기라 그런가",
          "original_form": "유일한 동기라 그런가",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016668.1.7",
          "form": "걍 둘이 보면 연인보는 기분이야",
          "original_form": "걍 둘이 보면 연인보는 기분이야",
          "speaker_id": "2",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016668.1.8",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "1",
          "time": "20210826 14:40"
        },
        {
          "id": "MDRW2100016668.1.9",
          "form": "둘 보면 ㄹㅇ 데이트하는 연인",
          "original_form": "둘 보면 ㄹㅇ 데이트하는 연인",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016668.1.10",
          "form": "휴학하고 뭐한데?",
          "original_form": "휴학하고 뭐한데?",
          "speaker_id": "2",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016668.1.11",
          "form": "미국간다는데",
          "original_form": "미국간다는데",
          "speaker_id": "1",
          "time": "20210826 14:41"
        },
        {
          "id": "MDRW2100016668.1.12",
          "form": "뭐?",
          "original_form": "뭐?",
          "speaker_id": "2",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100016668.1.13",
          "form": "지금말고 좀 지나면 간다더라",
          "original_form": "지금말고 좀 지나면 간다더라",
          "speaker_id": "1",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100016668.1.14",
          "form": "뭐하러가는데?",
          "original_form": "뭐하러가는데?",
          "speaker_id": "2",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100016668.1.15",
          "form": "name5선배한테 들은거라 자세힌 몰라",
          "original_form": "&name5&선배한테 들은거라 자세힌 몰라",
          "speaker_id": "1",
          "time": "20210826 14:42"
        },
        {
          "id": "MDRW2100016668.1.16",
          "form": "아 name5선배 미국에서 왔지...",
          "original_form": "아 &name5&선배 미국에서 왔지...",
          "speaker_id": "2",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100016668.1.17",
          "form": "ㅇㅇ... 그래서 뭐 이민? 그런거 물어봤나봐",
          "original_form": "ㅇㅇ... 그래서 뭐 이민? 그런거 물어봤나봐",
          "speaker_id": "1",
          "time": "20210826 14:43"
        },
        {
          "id": "MDRW2100016668.1.18",
          "form": "둘이 이민가나?",
          "original_form": "둘이 이민가나?",
          "speaker_id": "2",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100016668.1.19",
          "form": "둘이라면 가능할것같은데",
          "original_form": "둘이라면 가능할것같은데",
          "speaker_id": "1",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100016668.1.20",
          "form": "그럼 휴학 아니라 자퇴인가?",
          "original_form": "그럼 휴학 아니라 자퇴인가?",
          "speaker_id": "2",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100016668.1.21",
          "form": "일단 휴학했나봐",
          "original_form": "일단 휴학했나봐",
          "speaker_id": "1",
          "time": "20210826 14:44"
        },
        {
          "id": "MDRW2100016668.1.22",
          "form": "편입하려나",
          "original_form": "편입하려나",
