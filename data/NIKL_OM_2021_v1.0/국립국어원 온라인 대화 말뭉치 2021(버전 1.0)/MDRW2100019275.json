      "utterance": [
        {
          "id": "MDRW2100019275.1.1",
          "form": "그래서 좀 씁쓸하기도...",
          "original_form": "그래서 좀 씁쓸하기도...",
          "speaker_id": "1",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100019275.1.2",
          "form": "온라인이 한계가있어요ㅜ",
          "original_form": "온라인이 한계가있어요ㅜ",
          "speaker_id": "2",
          "time": "20210827 16:52"
        },
        {
          "id": "MDRW2100019275.1.3",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100019275.1.4",
          "form": "온라인은 심심해요ㅜ",
          "original_form": "온라인은 심심해요ㅜ",
          "speaker_id": "1",
          "time": "20210827 16:56"
        },
        {
          "id": "MDRW2100019275.1.5",
          "form": "압도적인 매력이 있는데공연은ㅜ",
          "original_form": "압도적인 매력이 있는데공연은ㅜ",
          "speaker_id": "2",
          "time": "20210827 16:57"
        },
        {
          "id": "MDRW2100019275.1.6",
          "form": "몇개 온라인봤거든요..ㅜㅜ 심심해요",
          "original_form": "몇개 온라인봤거든요..ㅜㅜ 심심해요",
          "speaker_id": "2",
          "time": "20210827 16:57"
        },
        {
          "id": "MDRW2100019275.1.7",
          "form": "스포츠도 관중없이 보니까 심심하더라구요ㅜ",
          "original_form": "스포츠도 관중없이 보니까 심심하더라구요ㅜ",
          "speaker_id": "1",
          "time": "20210827 17:30"
        },
        {
          "id": "MDRW2100019275.1.8",
          "form": "왜 관중이 필요한지 알겠더라구요ㅜ",
          "original_form": "왜 관중이 필요한지 알겠더라구요ㅜ",
          "speaker_id": "1",
          "time": "20210827 17:30"
        },
        {
          "id": "MDRW2100019275.1.9",
          "form": "ㅜㅡㅜ 갖춘환경에 찾아가는것도 관람에 포함되나봐요ㅜ맘이 떠요",
          "original_form": "ㅜㅡㅜ 갖춘환경에 찾아가는것도 관람에 포함되나봐요ㅜ맘이 떠요",
          "speaker_id": "2",
          "time": "20210827 19:07"
        },
        {
          "id": "MDRW2100019275.1.10",
          "form": "맞아요ㅠㅜ",
          "original_form": "맞아요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019275.1.11",
          "form": "오프라인이",
          "original_form": "오프라인이",
          "speaker_id": "1",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019275.1.12",
          "form": "이렇게 중요한줄 몰랐어요 ㅎㅎ",
          "original_form": "이렇게 중요한줄 몰랐어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:41"
        },
        {
          "id": "MDRW2100019275.1.13",
          "form": "잃어버려야 소중함을 알게되는..어리석음ㅜㅜ",
          "original_form": "잃어버려야 소중함을 알게되는..어리석음ㅜㅜ",
          "speaker_id": "2",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019275.1.14",
          "form": "원래 모든게 다 그런거죠ㅜ",
          "original_form": "원래 모든게 다 그런거죠ㅜ",
          "speaker_id": "1",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019275.1.15",
          "form": "약간 좋은 건 볼 때 사람들이 많이 없다는거..???",
          "original_form": "약간 좋은 건 볼 때 사람들이 많이 없다는거..???",
          "speaker_id": "1",
          "time": "20210827 19:51"
        },
        {
          "id": "MDRW2100019275.1.16",
          "form": "ㅋㅋㅋ머보셨나보네요",
          "original_form": "ㅋㅋㅋ머보셨나보네요",
          "speaker_id": "2",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019275.1.17",
          "form": "머보셨나요",
          "original_form": "머보셨나요",
          "speaker_id": "2",
          "time": "20210827 19:52"
        },
        {
          "id": "MDRW2100019275.1.18",
          "form": "영화요ㅜ",
          "original_form": "영화요ㅜ",
          "speaker_id": "1",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019275.1.19",
          "form": "요즘 거의 없더라구요ㅋㅋㅋ",
          "original_form": "요즘 거의 없더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019275.1.20",
          "form": "근데 연극은 생각외로 꽉 차더라구요ㅎㅎ",
          "original_form": "근데 연극은 생각외로 꽉 차더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:50"
        },
        {
          "id": "MDRW2100019275.1.21",
          "form": "연극 안가본지 오래됐네요",
          "original_form": "연극 안가본지 오래됐네요",
          "speaker_id": "2",
          "time": "20210827 21:05"
        },
        {
          "id": "MDRW2100019275.1.22",
          "form": "영화관은 모가디슈봤는데",
          "original_form": "영화관은 모가디슈봤는데",
          "speaker_id": "2",
          "time": "20210827 21:05"
        },
        {
          "id": "MDRW2100019275.1.23",
          "form": "저도 오랜만에 갔는데",
          "original_form": "저도 오랜만에 갔는데",
          "speaker_id": "1",
          "time": "20210827 21:06"
        },
        {
          "id": "MDRW2100019275.1.24",
          "form": "머보셨나요",
          "original_form": "머보셨나요",
          "speaker_id": "2",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100019275.1.25",
          "form": "평점 1위 라서 그런지 꽉 차더라구요ㅎㅎㅎㅎ",
          "original_form": "평점 1위 라서 그런지 꽉 차더라구요ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100019275.1.26",
          "form": "저는 널널했엏는뎅",
          "original_form": "저는 널널했엏는뎅",
          "speaker_id": "2",
          "time": "20210827 21:07"
        },
        {
          "id": "MDRW2100019275.1.27",
          "form": "데스뭐시기였는데",
          "original_form": "데스뭐시기였는데",
          "speaker_id": "1",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100019275.1.28",
          "form": "데스트랩??",
          "original_form": "데스트랩??",
          "speaker_id": "1",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100019275.1.29",
          "form": "그게무지",
          "original_form": "그게무지",
          "speaker_id": "2",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100019275.1.30",
          "form": "데드트랩??",
          "original_form": "데드트랩??",
          "speaker_id": "1",
          "time": "20210827 21:08"
        },
        {
          "id": "MDRW2100019275.1.31",
          "form": "어떤영환가요",
          "original_form": "어떤영환가요",
          "speaker_id": "2",
          "time": "20210827 21:09"
        },
        {
          "id": "MDRW2100019275.1.32",
          "form": "약간 스릴러???",
          "original_form": "약간 스릴러???",
          "speaker_id": "1",
          "time": "20210827 21:10"
        },
        {
          "id": "MDRW2100019275.1.33",
          "form": "신입작가 시나리오가 너무 좋아서 잘나가는 작가가 시기 질투 하는데",
          "original_form": "신입작가 시나리오가 너무 좋아서 잘나가는 작가가 시기 질투 하는데",
          "speaker_id": "1",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100019275.1.34",
          "form": "그 상황이 약간 죽일려는 그런 상황이 나타나면서 일어나는??",
          "original_form": "그 상황이 약간 죽일려는 그런 상황이 나타나면서 일어나는??",
          "speaker_id": "1",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100019275.1.35",
          "form": "첨듣는",
          "original_form": "첨듣는",
          "speaker_id": "2",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100019275.1.36",
          "form": "반전이있습니다ㅋㅋㅋ",
          "original_form": "반전이있습니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100019275.1.37",
          "form": "내용이네요",
          "original_form": "내용이네요",
          "speaker_id": "2",
          "time": "20210827 21:11"
        },
        {
          "id": "MDRW2100019275.1.38",
          "form": "반전까지 스포해주셨지만 정말 모르는 영화네요ㅋㅋ",
          "original_form": "반전까지 스포해주셨지만 정말 모르는 영화네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100019275.1.39",
          "form": "제가 반전은 설명 안드렸지만ㅋㅋ",
          "original_form": "제가 반전은 설명 안드렸지만ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100019275.1.40",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100019275.1.41",
          "form": "이건 연극입니다ㅎㅎ",
          "original_form": "이건 연극입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:12"
        },
        {
          "id": "MDRW2100019275.1.42",
          "form": "아 ㅋㅋ글쿤요",
          "original_form": "아 ㅋㅋ글쿤요",
