      "utterance": [
        {
          "id": "MDRW2100035399.1.1",
          "form": "요리하시나요?",
          "original_form": "요리하시나요?",
          "speaker_id": "1",
          "time": "20210829 16:24"
        },
        {
          "id": "MDRW2100035399.1.2",
          "form": "네네 요리 하는편이에요!",
          "original_form": "네네 요리 하는편이에요!",
          "speaker_id": "2",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100035399.1.3",
          "form": "name1님은 요리 좋아하세요?",
          "original_form": "&name1&님은 요리 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100035399.1.4",
          "form": "최근들어 하려고 있어요!",
          "original_form": "최근들어 하려고 있어요!",
          "speaker_id": "1",
          "time": "20210829 20:46"
        },
        {
          "id": "MDRW2100035399.1.5",
          "form": "어플하고 책 샀어요~",
          "original_form": "어플하고 책 샀어요~",
          "speaker_id": "1",
          "time": "20210829 20:47"
        },
        {
          "id": "MDRW2100035399.1.6",
          "form": "오 그렇군요 만들고 싶은 요리 있으세요?",
          "original_form": "오 그렇군요 만들고 싶은 요리 있으세요?",
          "speaker_id": "2",
          "time": "20210830 23:58"
        },
        {
          "id": "MDRW2100035399.1.7",
          "form": "넹! 스파게티요!!!",
          "original_form": "넹! 스파게티요!!!",
          "speaker_id": "1",
          "time": "20210831 00:02"
        },
        {
          "id": "MDRW2100035399.1.8",
          "form": "오오 저두 스파게티좋아해서 자주해요 ㅋㅋㅋㅋㅋ",
          "original_form": "오오 저두 스파게티좋아해서 자주해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100035399.1.9",
          "form": "무슨스파게티 만드셧어요!?",
          "original_form": "무슨스파게티 만드셧어요!?",
          "speaker_id": "2",
          "time": "20210831 00:03"
        },
        {
          "id": "MDRW2100035399.1.10",
          "form": "저번에ㅋㄲㅋㅋ 두유스파게티 만들려했는데 두유가 없어서 우유넣고 뭔가 부족한거 같아 계란 넣고 했더니",
          "original_form": "저번에ㅋㄲㅋㅋ 두유스파게티 만들려했는데 두유가 없어서 우유넣고 뭔가 부족한거 같아 계란 넣고 했더니",
          "speaker_id": "1",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100035399.1.11",
          "form": "면부침개가 되었어욖ㅋㅋㄱㅋㅋ",
          "original_form": "면부침개가 되었어욖ㅋㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100035399.1.12",
          "form": "계란이요..?ㅋㅋㅋㅋㅋㅋ",
          "original_form": "계란이요..?ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100035399.1.13",
          "form": "시판 소스 쓰셔야겟네욬ㅋㅋㅋㅋ",
          "original_form": "시판 소스 쓰셔야겟네욬ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 00:09"
        },
        {
          "id": "MDRW2100035399.1.14",
          "form": "넼ㅋㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100035399.1.15",
          "form": "담에는 소스도 구비하고 만들어야겠어요ㅋㅋㅋ",
          "original_form": "담에는 소스도 구비하고 만들어야겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100035399.1.16",
          "form": "소생술!!",
          "original_form": "소생술!!",
          "speaker_id": "1",
          "time": "20210831 00:13"
        },
        {
          "id": "MDRW2100035399.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ네네 청정원꺼 맛잇어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ네네 청정원꺼 맛잇어요",
          "speaker_id": "2",
          "time": "20210831 19:36"
        },
        {
          "id": "MDRW2100035399.1.18",
          "form": "크림스파게티 소스는 생크림이랑 우유 치즈 정도만 있으면 되는거같아요!",
          "original_form": "크림스파게티 소스는 생크림이랑 우유 치즈 정도만 있으면 되는거같아요!",
          "speaker_id": "2",
          "time": "20210831 19:36"
        },
        {
          "id": "MDRW2100035399.1.19",
          "form": "맞아용!! 그건 만들기 편해서 좋아욥ㅋㅋ",
          "original_form": "맞아용!! 그건 만들기 편해서 좋아욥ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:38"
        },
        {
          "id": "MDRW2100035399.1.20",
          "form": "청정원꺼! 세일할때 쟁여둬용ㅋㅋ",
          "original_form": "청정원꺼! 세일할때 쟁여둬용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 19:38"
        },
        {
          "id": "MDRW2100035399.1.21",
          "form": "마자요 ㅋㅋㅋㅋ저두 청정원 세일 자주해서 쟁여둬요 ㅋㅋㅋㅋ",
          "original_form": "마자요 ㅋㅋㅋㅋ저두 청정원 세일 자주해서 쟁여둬요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 19:40"
        },
        {
          "id": "MDRW2100035399.1.22",
          "form": "로제 맛있더라구요!",
          "original_form": "로제 맛있더라구요!",
          "speaker_id": "2",
          "time": "20210831 19:40"
        },
        {
          "id": "MDRW2100035399.1.23",
          "form": "로제!! 거기 로제도 맛있군욥",
          "original_form": "로제!! 거기 로제도 맛있군욥",
          "speaker_id": "1",
          "time": "20210831 19:40"
        },
        {
          "id": "MDRW2100035399.1.24",
          "form": "네네 ㅋㅋㅋㅋ 그 알프레도소스도 맛잇어요 크림류에요",
