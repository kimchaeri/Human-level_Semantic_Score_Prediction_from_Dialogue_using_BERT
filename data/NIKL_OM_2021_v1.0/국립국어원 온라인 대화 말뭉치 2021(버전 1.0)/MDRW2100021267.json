      "utterance": [
        {
          "id": "MDRW2100021267.1.1",
          "form": "필테도 비싸죠?",
          "original_form": "필테도 비싸죠?",
          "speaker_id": "2",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021267.1.2",
          "form": "네ㅠㅠ 일대일은 진짜 너무 비싸고",
          "original_form": "네ㅠㅠ 일대일은 진짜 너무 비싸고",
          "speaker_id": "1",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021267.1.3",
          "form": "저는 그룹으로 하고 있어요",
          "original_form": "저는 그룹으로 하고 있어요",
          "speaker_id": "1",
          "time": "20210828 17:09"
        },
        {
          "id": "MDRW2100021267.1.4",
          "form": "그룹은 한명씩 자세 잘 봐주시나요?",
          "original_form": "그룹은 한명씩 자세 잘 봐주시나요?",
          "speaker_id": "2",
          "time": "20210828 17:10"
        },
        {
          "id": "MDRW2100021267.1.5",
          "form": "사실 엄청 세세하게 잘봐주지는 못하는거같아요",
          "original_form": "사실 엄청 세세하게 잘봐주지는 못하는거같아요",
          "speaker_id": "1",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100021267.1.6",
          "form": "아무래도 8명이서 하는거라...",
          "original_form": "아무래도 8명이서 하는거라...",
          "speaker_id": "1",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100021267.1.7",
          "form": "근데 가끔 금요일 저녁 이럴때는 2,3명이서 할때도 있거든요",
          "original_form": "근데 가끔 금요일 저녁 이럴때는 2,3명이서 할때도 있거든요",
          "speaker_id": "1",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100021267.1.8",
          "form": "와 8명이서 하는군요",
          "original_form": "와 8명이서 하는군요",
          "speaker_id": "2",
          "time": "20210828 17:11"
        },
        {
          "id": "MDRW2100021267.1.9",
          "form": "아 참석률에 따라",
          "original_form": "아 참석률에 따라",
          "speaker_id": "2",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100021267.1.10",
          "form": "달라지네요ㅋㅋ",
          "original_form": "달라지네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100021267.1.11",
          "form": "그럴때는 확실히 잘봐주세요 ㅎㅎㅎ",
          "original_form": "그럴때는 확실히 잘봐주세요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 17:12"
        },
        {
          "id": "MDRW2100021267.1.12",
          "form": "사실 하다보면 일대일로도 해보고 싶긴한데...",
          "original_form": "사실 하다보면 일대일로도 해보고 싶긴한데...",
          "speaker_id": "1",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100021267.1.13",
          "form": "일대일은 거의 한회에 5,6만원이 훌쩍 넘더라고요",
          "original_form": "일대일은 거의 한회에 5,6만원이 훌쩍 넘더라고요",
          "speaker_id": "1",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100021267.1.14",
          "form": "역시 비싸네요ㅠ",
          "original_form": "역시 비싸네요ㅠ",
          "speaker_id": "2",
          "time": "20210828 17:14"
        },
        {
          "id": "MDRW2100021267.1.15",
          "form": "네ㅠㅠ 피티도 비싸죠?",
          "original_form": "네ㅠㅠ 피티도 비싸죠?",
          "speaker_id": "1",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100021267.1.16",
          "form": "자세도 잘 봐주셔야 운동 효과도 더 좋을텐데요",
          "original_form": "자세도 잘 봐주셔야 운동 효과도 더 좋을텐데요",
          "speaker_id": "2",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100021267.1.17",
          "form": "네 피티도 그정도 하는거같아요",
          "original_form": "네 피티도 그정도 하는거같아요",
          "speaker_id": "2",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100021267.1.18",
          "form": "네 그래서 혼자 헬스도 했었는데",
          "original_form": "네 그래서 혼자 헬스도 했었는데",
          "speaker_id": "1",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100021267.1.19",
          "form": "매장마다 다르겠지만요ㅠ",
          "original_form": "매장마다 다르겠지만요ㅠ",
          "speaker_id": "2",
          "time": "20210828 17:15"
        },
        {
          "id": "MDRW2100021267.1.20",
          "form": "그래도 배우니까 좋더라구요",
          "original_form": "그래도 배우니까 좋더라구요",
          "speaker_id": "2",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100021267.1.21",
          "form": "네 아무래도 기간도 좀 차이가 있구요",
          "original_form": "네 아무래도 기간도 좀 차이가 있구요",
          "speaker_id": "1",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100021267.1.22",
          "form": "혼자 하면 아무래도 너무 힘들면 안하고 하니까",
          "original_form": "혼자 하면 아무래도 너무 힘들면 안하고 하니까",
          "speaker_id": "1",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100021267.1.23",
          "form": "효과가 확실히 덜하긴해요",
          "original_form": "효과가 확실히 덜하긴해요",
          "speaker_id": "1",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100021267.1.24",
          "form": "아 오히려 같이하는게 더 동기부여돼서 좋을수도 있겧네요",
          "original_form": "아 오히려 같이하는게 더 동기부여돼서 좋을수도 있겧네요",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100021267.1.25",
          "form": "네ㅋㅋㅋ 그리고 예전에 헬스할때 힘들면 무게빼고 그랬거든요",
          "original_form": "네ㅋㅋㅋ 그리고 예전에 헬스할때 힘들면 무게빼고 그랬거든요",
          "speaker_id": "1",
          "time": "20210828 17:19"
        },
        {
          "id": "MDRW2100021267.1.26",
          "form": "아ㅋㅋㅋㅋㅋ마자요 슬쩍 하나빼야죠ㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋ마자요 슬쩍 하나빼야죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:21"
        }
      ]
    }
