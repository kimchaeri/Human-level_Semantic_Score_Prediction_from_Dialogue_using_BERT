      "utterance": [
        {
          "id": "MDRW2100018805.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100018805.1.2",
          "form": "안녕하세요~ 거긴날씨가 어떤가용",
          "original_form": "안녕하세요~ 거긴날씨가 어떤가용",
          "speaker_id": "2",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100018805.1.3",
          "form": "여기는 적당히 선선합니당 딱 좋아요",
          "original_form": "여기는 적당히 선선합니당 딱 좋아요",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100018805.1.4",
          "form": "거긴 어떠신가요?",
          "original_form": "거긴 어떠신가요?",
          "speaker_id": "1",
          "time": "20210827 12:21"
        },
        {
          "id": "MDRW2100018805.1.5",
          "form": "저도! 습도만 좀 낮으면 최골텐데",
          "original_form": "저도! 습도만 좀 낮으면 최골텐데",
          "speaker_id": "2",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018805.1.6",
          "form": "그래두시원해용",
          "original_form": "그래두시원해용",
          "speaker_id": "2",
          "time": "20210827 12:22"
        },
        {
          "id": "MDRW2100018805.1.7",
          "form": "아 맞아요 ㅠㅠ 저도 습도 때문에 어제밤에 잠깐 에어컨 틀었어요",
          "original_form": "아 맞아요 ㅠㅠ 저도 습도 때문에 어제밤에 잠깐 에어컨 틀었어요",
          "speaker_id": "1",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100018805.1.8",
          "form": "요즘은 옷을 뭐입어야할지 모르겠네요",
          "original_form": "요즘은 옷을 뭐입어야할지 모르겠네요",
          "speaker_id": "1",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100018805.1.9",
          "form": "저도! 제습으로 ㅠ",
          "original_form": "저도! 제습으로 ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100018805.1.10",
          "form": "오늘은 머입으샸어용?",
          "original_form": "오늘은 머입으샸어용?",
          "speaker_id": "2",
          "time": "20210827 12:25"
        },
        {
          "id": "MDRW2100018805.1.11",
          "form": "저 반팔에 슬랙스 입었어요!",
          "original_form": "저 반팔에 슬랙스 입었어요!",
          "speaker_id": "1",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018805.1.12",
          "form": "쌀쌀하진 않으신가용",
          "original_form": "쌀쌀하진 않으신가용",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018805.1.13",
          "form": "저도 이제나가야하는데 ㅋㅋ",
          "original_form": "저도 이제나가야하는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018805.1.14",
          "form": "두께있는 반팔시라 괜찮은거 같아요ㅋㅋㅋㅋ",
          "original_form": "두께있는 반팔시라 괜찮은거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:26"
        },
        {
          "id": "MDRW2100018805.1.15",
          "form": "name2님은 뭐입으실건가요?",
          "original_form": "&name2&님은 뭐입으실건가요?",
          "speaker_id": "1",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100018805.1.16",
          "form": "아항 긴팔은 뭔가 아직 습도때문에 더운거같긴해요",
          "original_form": "아항 긴팔은 뭔가 아직 습도때문에 더운거같긴해요",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100018805.1.17",
          "form": "저도 두꺼운 반팔 입어야겠네요 ㅋㅋㅋ",
          "original_form": "저도 두꺼운 반팔 입어야겠네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100018805.1.18",
          "form": "맞아요 ㅠㅠ 담달엔 입을 수 있으려나",
          "original_form": "맞아요 ㅠㅠ 담달엔 입을 수 있으려나",
          "speaker_id": "1",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100018805.1.19",
          "form": "곧 가을이네요",
          "original_form": "곧 가을이네요",
          "speaker_id": "1",
          "time": "20210827 12:27"
        },
        {
          "id": "MDRW2100018805.1.20",
          "form": "담달은 충분할듯요!",
          "original_form": "담달은 충분할듯요!",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018805.1.21",
          "form": "그니까요ㅠㅠ날씨보면 지금도 가을 ㅋㅋㅋ",
          "original_form": "그니까요ㅠㅠ날씨보면 지금도 가을 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:34"
        },
        {
          "id": "MDRW2100018805.1.22",
          "form": "가을되면 이번엔 단풍놀이 가보려구요ㅋㅋㅋㅋ",
          "original_form": "가을되면 이번엔 단풍놀이 가보려구요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:36"
        },
        {
          "id": "MDRW2100018805.1.23",
          "form": "황매산에 갈거예요 벌써부터 정해둠,,",
          "original_form": "황매산에 갈거예요 벌써부터 정해둠,,",
          "speaker_id": "1",
          "time": "20210827 12:37"
        },
        {
          "id": "MDRW2100018805.1.24",
          "form": "허걱 거기가 좋나용",
          "original_form": "허걱 거기가 좋나용",
          "speaker_id": "2",
          "time": "20210827 12:38"
        }
      ]
    }
  ]
