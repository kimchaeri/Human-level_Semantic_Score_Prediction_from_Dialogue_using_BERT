      "utterance": [
        {
          "id": "MDRW2100002658.1.1",
          "form": "여행가고싶다",
          "original_form": "여행가고싶다",
          "speaker_id": "1",
          "time": "20210820 14:36"
        },
        {
          "id": "MDRW2100002658.1.2",
          "form": "일본가고싶어",
          "original_form": "일본가고싶어",
          "speaker_id": "1",
          "time": "20210820 14:36"
        },
        {
          "id": "MDRW2100002658.1.3",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210820 14:41"
        },
        {
          "id": "MDRW2100002658.1.4",
          "form": "눈 엄청올때 삿포르 가보고싶어",
          "original_form": "눈 엄청올때 삿포르 가보고싶어",
          "speaker_id": "2",
          "time": "20210820 14:42"
        },
        {
          "id": "MDRW2100002658.1.5",
          "form": "나는 후쿠오카 한번 다녀온게 다인데 너무좋았어",
          "original_form": "나는 후쿠오카 한번 다녀온게 다인데 너무좋았어",
          "speaker_id": "1",
          "time": "20210820 14:43"
        },
        {
          "id": "MDRW2100002658.1.6",
          "form": "어디든 가고싶다",
          "original_form": "어디든 가고싶다",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002658.1.7",
          "form": "내 카메라도 썩어가고 있엉...",
          "original_form": "내 카메라도 썩어가고 있엉...",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002658.1.8",
          "form": "휴가 다음달이라고?",
          "original_form": "휴가 다음달이라고?",
          "speaker_id": "1",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002658.1.9",
          "form": "10월초 생각하고있어",
          "original_form": "10월초 생각하고있어",
          "speaker_id": "2",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002658.1.10",
          "form": "추석전에 임시회있도 추석지나고 조금 한가하고 예산세우고 11월에 사무감사도있어...",
          "original_form": "추석전에 임시회있도 추석지나고 조금 한가하고 예산세우고 11월에 사무감사도있어...",
          "speaker_id": "2",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002658.1.11",
          "form": "어디 다녀올 생각이아?",
          "original_form": "어디 다녀올 생각이아?",
          "speaker_id": "1",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002658.1.12",
          "form": "영월 이쪽을 가볼까 생각중이야",
          "original_form": "영월 이쪽을 가볼까 생각중이야",
          "speaker_id": "2",
          "time": "20210820 15:58"
        },
        {
          "id": "MDRW2100002658.1.13",
          "form": "강원도?",
          "original_form": "강원도?",
          "speaker_id": "1",
          "time": "20210820 15:59"
        },
        {
          "id": "MDRW2100002658.1.14",
          "form": "어 강원도 중간은 잘 다녀보질않은거같아서 ㅎ",
          "original_form": "어 강원도 중간은 잘 다녀보질않은거같아서 ㅎ",
          "speaker_id": "2",
          "time": "20210820 16:03"
        },
        {
          "id": "MDRW2100002658.1.15",
          "form": "동해안 해안도로 따라 드라이브 어때",
          "original_form": "동해안 해안도로 따라 드라이브 어때",
          "speaker_id": "1",
          "time": "20210820 16:05"
        },
        {
          "id": "MDRW2100002658.1.16",
          "form": "가봤어 거기는 ㅎㅎ",
          "original_form": "가봤어 거기는 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 16:08"
        },
        {
          "id": "MDRW2100002658.1.17",
          "form": "오 진짜",
          "original_form": "오 진짜",
          "speaker_id": "1",
          "time": "20210820 16:10"
        },
        {
          "id": "MDRW2100002658.1.18",
          "form": "난 안가봄",
          "original_form": "난 안가봄",
          "speaker_id": "1",
          "time": "20210820 16:10"
        },
        {
          "id": "MDRW2100002658.1.19",
          "form": "경치 좋아",
          "original_form": "경치 좋아",
          "speaker_id": "1",
          "time": "20210820 16:10"
        },
        {
          "id": "MDRW2100002658.1.20",
          "form": "나 군대있을때 매달 7번국도 타고 울진까지 갔다왔자나",
          "original_form": "나 군대있을때 매달 7번국도 타고 울진까지 갔다왔자나",
          "speaker_id": "2",
          "time": "20210820 16:12"
        },
        {
          "id": "MDRW2100002658.1.21",
          "form": "10월말 11월초가 제일 좋앗ㅎ어",
          "original_form": "10월말 11월초가 제일 좋앗ㅎ어",
