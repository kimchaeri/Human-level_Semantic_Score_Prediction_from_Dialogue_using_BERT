      "utterance": [
        {
          "id": "MDRW2100009142.1.1",
          "form": "동호회같은것도 해보셨나요?",
          "original_form": "동호회같은것도 해보셨나요?",
          "speaker_id": "1",
          "time": "20210822 22:01"
        },
        {
          "id": "MDRW2100009142.1.2",
          "form": "아니요 혹시 하고 계신것 있으세요?",
          "original_form": "아니요 혹시 하고 계신것 있으세요?",
          "speaker_id": "2",
          "time": "20210822 22:01"
        },
        {
          "id": "MDRW2100009142.1.3",
          "form": "아뇨ㅎㅎ저도 모임같은건 관심은 많은데 많이 못해봣어요",
          "original_form": "아뇨ㅎㅎ저도 모임같은건 관심은 많은데 많이 못해봣어요",
          "speaker_id": "1",
          "time": "20210822 22:02"
        },
        {
          "id": "MDRW2100009142.1.4",
          "form": "저 걷기운동할때",
          "original_form": "저 걷기운동할때",
          "speaker_id": "2",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009142.1.5",
          "form": "자전거타시는분들 지나가시는데",
          "original_form": "자전거타시는분들 지나가시는데",
          "speaker_id": "2",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009142.1.6",
          "form": "저도 러닝모임은 한번 해봣어여",
          "original_form": "저도 러닝모임은 한번 해봣어여",
          "speaker_id": "1",
          "time": "20210822 22:03"
        },
        {
          "id": "MDRW2100009142.1.7",
          "form": "그런분들이 동호회 이실수도 ㅎ",
          "original_form": "그런분들이 동호회 이실수도 ㅎ",
          "speaker_id": "2",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009142.1.8",
          "form": "근데 힘들어서 포기..",
          "original_form": "근데 힘들어서 포기..",
          "speaker_id": "1",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009142.1.9",
          "form": "아 다리밑에서 중국에서아침에하는 운동 이름 뭐죠 그거하시는분들도 있으시던데",
          "original_form": "아 다리밑에서 중국에서아침에하는 운동 이름 뭐죠 그거하시는분들도 있으시던데",
          "speaker_id": "2",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009142.1.10",
          "form": "등산도 한두번ㅎㅎ길치라서요ㅎ",
          "original_form": "등산도 한두번ㅎㅎ길치라서요ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009142.1.11",
          "form": "기체조?그런건가요ㅋ",
          "original_form": "기체조?그런건가요ㅋ",
          "speaker_id": "1",
          "time": "20210822 22:04"
        },
        {
          "id": "MDRW2100009142.1.12",
          "form": "아아 그거요 ㅋㄱ",
          "original_form": "아아 그거요 ㅋㄱ",
          "speaker_id": "2",
          "time": "20210822 22:06"
        },
        {
          "id": "MDRW2100009142.1.13",
          "form": "깜놀랐어요",
          "original_form": "깜놀랐어요",
          "speaker_id": "2",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100009142.1.14",
          "form": "와 진짜 고요한표정으로 하고들계셔서",
          "original_form": "와 진짜 고요한표정으로 하고들계셔서",
          "speaker_id": "2",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100009142.1.15",
          "form": "진지하게 하시나봐요ㅎ",
          "original_form": "진지하게 하시나봐요ㅎ",
          "speaker_id": "1",
          "time": "20210822 22:07"
        },
        {
          "id": "MDRW2100009142.1.16",
          "form": "와 우리나라에도 이런게있어! 같은 느낌으로다가",
          "original_form": "와 우리나라에도 이런게있어! 같은 느낌으로다가",
          "speaker_id": "2",
          "time": "20210822 22:07"
        }
