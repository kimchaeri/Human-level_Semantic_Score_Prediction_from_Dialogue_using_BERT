      "utterance": [
        {
          "id": "MDRW2100009042.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210822 20:44"
        },
        {
          "id": "MDRW2100009042.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210822 20:44"
        },
        {
          "id": "MDRW2100009042.1.3",
          "form": "책 좋아하시나요?",
          "original_form": "책 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 20:45"
        },
        {
          "id": "MDRW2100009042.1.4",
          "form": "아니요ㅜㅜㅜ 좋아하시는 책 있으세요?",
          "original_form": "아니요ㅜㅜㅜ 좋아하시는 책 있으세요?",
          "speaker_id": "1",
          "time": "20210822 20:45"
        },
        {
          "id": "MDRW2100009042.1.5",
          "form": "추리나 범죄 소설 좋아해요!",
          "original_form": "추리나 범죄 소설 좋아해요!",
          "speaker_id": "2",
          "time": "20210822 20:45"
        },
        {
          "id": "MDRW2100009042.1.6",
          "form": "가끔 로맨스랑 판타지도 읽고요",
          "original_form": "가끔 로맨스랑 판타지도 읽고요",
          "speaker_id": "2",
          "time": "20210822 20:45"
        },
        {
          "id": "MDRW2100009042.1.7",
          "form": "전 추리 소설은 봉제인형 살인사건 읽은 것만 생각나요!",
          "original_form": "전 추리 소설은 봉제인형 살인사건 읽은 것만 생각나요!",
          "speaker_id": "1",
          "time": "20210822 20:47"
        },
        {
          "id": "MDRW2100009042.1.8",
          "form": "저도 그 책 읽었어요!",
          "original_form": "저도 그 책 읽었어요!",
          "speaker_id": "2",
          "time": "20210822 20:47"
        },
        {
          "id": "MDRW2100009042.1.9",
          "form": "봉제인형 살인사건은 시즌 2도 나왔어요!",
          "original_form": "봉제인형 살인사건은 시즌 2도 나왔어요!",
          "speaker_id": "2",
          "time": "20210822 20:48"
        },
        {
          "id": "MDRW2100009042.1.10",
          "form": "꼭두각시 살인사건 인가요?",
          "original_form": "꼭두각시 살인사건 인가요?",
          "speaker_id": "1",
          "time": "20210822 20:49"
        },
        {
          "id": "MDRW2100009042.1.11",
          "form": "오랜만에 술술 읽히는 책 만나서 좋았어요!",
          "original_form": "오랜만에 술술 읽히는 책 만나서 좋았어요!",
          "speaker_id": "1",
          "time": "20210822 20:49"
        },
        {
          "id": "MDRW2100009042.1.12",
          "form": "아마도 그럴거에요",
          "original_form": "아마도 그럴거에요",
          "speaker_id": "2",
          "time": "20210822 20:53"
        },
        {
          "id": "MDRW2100009042.1.13",
          "form": "근데 그 책은 아직 못 읽어 봤어요ㅠㅜ",
          "original_form": "근데 그 책은 아직 못 읽어 봤어요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210822 20:53"
        },
        {
          "id": "MDRW2100009042.1.14",
          "form": "올해 안에 읽어야겠어요!",
          "original_form": "올해 안에 읽어야겠어요!",
          "speaker_id": "1",
          "time": "20210822 20:56"
        },
        {
          "id": "MDRW2100009042.1.15",
          "form": "혹시 추천해주실만한 책 있으실까요?",
          "original_form": "혹시 추천해주실만한 책 있으실까요?",
          "speaker_id": "1",
          "time": "20210822 20:56"
        },
        {
          "id": "MDRW2100009042.1.16",
          "form": "판타지 로맨스 좋아하시면 레이디투퀸 추천 드려요!",
          "original_form": "판타지 로맨스 좋아하시면 레이디투퀸 추천 드려요!",
          "speaker_id": "2",
          "time": "20210822 20:57"
        },
        {
          "id": "MDRW2100009042.1.17",
          "form": "추리소설을 좋아하시면 해바라기가 피지 않는 여름 추천 드려요!",
          "original_form": "추리소설을 좋아하시면 해바라기가 피지 않는 여름 추천 드려요!",
          "speaker_id": "2",
          "time": "20210822 20:58"
        },
        {
          "id": "MDRW2100009042.1.18",
          "form": "둘 다 읽어볼게요! 로맨스판타지는 아직 읽어본 적없어서 기대 돼요!",
          "original_form": "둘 다 읽어볼게요! 로맨스판타지는 아직 읽어본 적없어서 기대 돼요!",
          "speaker_id": "1",
          "time": "20210822 20:59"
        },
        {
          "id": "MDRW2100009042.1.19",
          "form": "레이디투퀸은 재밌어서",
          "original_form": "레이디투퀸은 재밌어서",
          "speaker_id": "2",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100009042.1.20",
          "form": "2번 정도 읽었던 걸로 기억해요!",
          "original_form": "2번 정도 읽었던 걸로 기억해요!",
          "speaker_id": "2",
          "time": "20210822 21:00"
        },
        {
          "id": "MDRW2100009042.1.21",
          "form": "우와 꼭 읽어봐야겠어여ㅎㅎ",
          "original_form": "우와 꼭 읽어봐야겠어여ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 21:03"
        }
      ]
    }
