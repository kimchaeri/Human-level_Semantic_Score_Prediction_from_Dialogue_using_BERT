      "utterance": [
        {
          "id": "MDRW2100007519.1.1",
          "form": "웹툰 PD들도 많이 생겨나는 추세인 것 같더라고요",
          "original_form": "웹툰 PD들도 많이 생겨나는 추세인 것 같더라고요",
          "speaker_id": "2",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007519.1.2",
          "form": "근데 코로나로 일은 어째 쉬게되셨나요",
          "original_form": "근데 코로나로 일은 어째 쉬게되셨나요",
          "speaker_id": "2",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007519.1.3",
          "form": "잘렸어요..",
          "original_form": "잘렸어요..",
          "speaker_id": "1",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007519.1.4",
          "form": "혹시 여행쪽이신가요?",
          "original_form": "혹시 여행쪽이신가요?",
          "speaker_id": "2",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007519.1.5",
          "form": "인원감축으로",
          "original_form": "인원감축으로",
          "speaker_id": "1",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007519.1.6",
          "form": "아니요 돈이 필요해서 생산직쪽 갔었는데",
          "original_form": "아니요 돈이 필요해서 생산직쪽 갔었는데",
          "speaker_id": "1",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007519.1.7",
          "form": "타격이 직빵으로 가더라구요",
          "original_form": "타격이 직빵으로 가더라구요",
          "speaker_id": "1",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007519.1.8",
          "form": "나쁜 넘들",
          "original_form": "나쁜 넘들",
          "speaker_id": "2",
          "time": "20210822 02:48"
        },
        {
          "id": "MDRW2100007519.1.9",
          "form": "요즘 알바자리도 없대요",
          "original_form": "요즘 알바자리도 없대요",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007519.1.10",
          "form": "원래 조리쪽으로 일했구요",
          "original_form": "원래 조리쪽으로 일했구요",
          "speaker_id": "1",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007519.1.11",
          "form": "코로나때문에",
          "original_form": "코로나때문에",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007519.1.12",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007519.1.13",
          "form": "요리 하셨구나",
          "original_form": "요리 하셨구나",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007519.1.14",
          "form": "댑악",
          "original_form": "댑악",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007519.1.15",
          "form": "맞아요 하늘의 별따기라더라더구요",
          "original_form": "맞아요 하늘의 별따기라더라더구요",
          "speaker_id": "1",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007519.1.16",
          "form": "꽤 매력있다 생각했는데",
          "original_form": "꽤 매력있다 생각했는데",
          "speaker_id": "1",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007519.1.17",
          "form": "생각과 현실은 다르더라구요",
          "original_form": "생각과 현실은 다르더라구요",
          "speaker_id": "1",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007519.1.18",
          "form": "하ㅠㅠㅠㅠ",
          "original_form": "하ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007519.1.19",
          "form": "너무 슬프네요",
          "original_form": "너무 슬프네요",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007519.1.20",
          "form": "이게 취업을 해도",
          "original_form": "이게 취업을 해도",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007519.1.21",
          "form": "ㅎ.. 안맞아서 때려쳤는데 코로나가 터질줄이야",
          "original_form": "ㅎ.. 안맞아서 때려쳤는데 코로나가 터질줄이야",
          "speaker_id": "1",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007519.1.22",
          "form": "살아남는게 우선이니",
          "original_form": "살아남는게 우선이니",
          "speaker_id": "2",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007519.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210822 02:49"
        },
        {
          "id": "MDRW2100007519.1.24",
          "form": "취업시장 너무 빡세요ㅠㅠ",
          "original_form": "취업시장 너무 빡세요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007519.1.25",
          "form": "조리쪽 엄청 열심히 준비하셨을텐데",
          "original_form": "조리쪽 엄청 열심히 준비하셨을텐데",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007519.1.26",
          "form": "그래도 맞고 좋아하는 직업 꼭 찾으실거예요",
          "original_form": "그래도 맞고 좋아하는 직업 꼭 찾으실거예요",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007519.1.27",
          "form": "호텔",
          "original_form": "호텔",
          "speaker_id": "1",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007519.1.28",
          "form": "호텔?!!!!",
          "original_form": "호텔?!!!!",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007519.1.29",
          "form": "들어가려고했는데",
          "original_form": "들어가려고했는데",
          "speaker_id": "1",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007519.1.30",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210822 02:50"
        },
        {
          "id": "MDRW2100007519.1.31",
          "form": "음.. 어렵더라구요",
          "original_form": "음.. 어렵더라구요",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007519.1.32",
          "form": "빡세긴 한것같더라고요",
          "original_form": "빡세긴 한것같더라고요",
          "speaker_id": "2",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007519.1.33",
          "form": "얼마전에 오늘도 출근이라는 프로그램에",
          "original_form": "얼마전에 오늘도 출근이라는 프로그램에",
          "speaker_id": "2",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007519.1.34",
          "form": "호텔의 조리사?",
          "original_form": "호텔의 조리사?",
          "speaker_id": "2",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007519.1.35",
          "form": "분 나오셔서",
          "original_form": "분 나오셔서",
          "speaker_id": "2",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007519.1.36",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007519.1.37",
          "form": "요리하는 거 봤는데",
          "original_form": "요리하는 거 봤는데",
          "speaker_id": "2",
          "time": "20210822 02:51"
        },
        {
          "id": "MDRW2100007519.1.38",
          "form": "어우 어지간한 사람은 못버티겠더라고요",
          "original_form": "어우 어지간한 사람은 못버티겠더라고요",
          "speaker_id": "2",
          "time": "20210822 02:52"
        },
        {
          "id": "MDRW2100007519.1.39",
          "form": "진짜 주방은 전쟁이죠..ㅎ",
          "original_form": "진짜 주방은 전쟁이죠..ㅎ",
