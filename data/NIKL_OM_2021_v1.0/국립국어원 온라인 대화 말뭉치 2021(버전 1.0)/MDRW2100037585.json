      "utterance": [
        {
          "id": "MDRW2100037585.1.1",
          "form": "그쵸, 운명이죠",
          "original_form": "그쵸, 운명이죠",
          "speaker_id": "2",
          "time": "20210901 12:34"
        },
        {
          "id": "MDRW2100037585.1.2",
          "form": "집 근처면 ㅋㅋ 멀어서 못간다는 핑계도 못대죠..",
          "original_form": "집 근처면 ㅋㅋ 멀어서 못간다는 핑계도 못대죠..",
          "speaker_id": "1",
          "time": "20210901 12:36"
        },
        {
          "id": "MDRW2100037585.1.3",
          "form": "그래서 등록하고 다니게 됐습니자",
          "original_form": "그래서 등록하고 다니게 됐습니자",
          "speaker_id": "2",
          "time": "20210901 12:36"
        },
        {
          "id": "MDRW2100037585.1.4",
          "form": "아주 가깝진 않은데",
          "original_form": "아주 가깝진 않은데",
          "speaker_id": "2",
          "time": "20210901 12:36"
        },
        {
          "id": "MDRW2100037585.1.5",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210901 12:36"
        },
        {
          "id": "MDRW2100037585.1.6",
          "form": "걸어서 20분?",
          "original_form": "걸어서 20분?",
          "speaker_id": "2",
          "time": "20210901 12:36"
        },
        {
          "id": "MDRW2100037585.1.7",
          "form": "근데 자전거타면 금방이니까",
          "original_form": "근데 자전거타면 금방이니까",
          "speaker_id": "2",
          "time": "20210901 12:37"
        },
        {
          "id": "MDRW2100037585.1.8",
          "form": "아 그정도면 갈만하네요 !",
          "original_form": "아 그정도면 갈만하네요 !",
          "speaker_id": "1",
          "time": "20210901 12:37"
        },
        {
          "id": "MDRW2100037585.1.9",
          "form": "와 자전거 타고 다니세요??",
          "original_form": "와 자전거 타고 다니세요??",
          "speaker_id": "1",
          "time": "20210901 12:37"
        },
        {
          "id": "MDRW2100037585.1.10",
          "form": "따릉이요",
          "original_form": "따릉이요",
          "speaker_id": "2",
          "time": "20210901 12:37"
        },
        {
          "id": "MDRW2100037585.1.11",
          "form": "유산소도 하시는군요 ㅋㅋㅋ",
          "original_form": "유산소도 하시는군요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:37"
        },
        {
          "id": "MDRW2100037585.1.12",
          "form": "그렇게 되네요ㅎㅎ",
          "original_form": "그렇게 되네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 12:37"
        },
        {
          "id": "MDRW2100037585.1.13",
          "form": "따릉이 한번 쓰기 시작하니까 편하더라구요",
          "original_form": "따릉이 한번 쓰기 시작하니까 편하더라구요",
          "speaker_id": "2",
          "time": "20210901 12:38"
        },
        {
          "id": "MDRW2100037585.1.14",
          "form": "맞죠 !! 저는 공원가서 많이 써요 ㅎㅎ",
          "original_form": "맞죠 !! 저는 공원가서 많이 써요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 12:39"
        },
        {
          "id": "MDRW2100037585.1.15",
          "form": "집은 오르막길이여서...",
          "original_form": "집은 오르막길이여서...",
          "speaker_id": "1",
          "time": "20210901 12:39"
        },
        {
          "id": "MDRW2100037585.1.16",
          "form": "저희집은 오르막은 아닌데 보도블럭이 좀 울퉁불퉁해요",
          "original_form": "저희집은 오르막은 아닌데 보도블럭이 좀 울퉁불퉁해요",
          "speaker_id": "2",
          "time": "20210901 12:39"
        },
        {
          "id": "MDRW2100037585.1.17",
          "form": "그래서 안전운전 한답니다 ㅎ",
          "original_form": "그래서 안전운전 한답니다 ㅎ",
          "speaker_id": "2",
          "time": "20210901 12:40"
        },
        {
          "id": "MDRW2100037585.1.18",
          "form": "항상 조심하셔요 !!",
          "original_form": "항상 조심하셔요 !!",
          "speaker_id": "1",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100037585.1.19",
          "form": "그 클라이밍 하면",
          "original_form": "그 클라이밍 하면",
          "speaker_id": "1",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100037585.1.20",
          "form": "애들도 많이 온다던데 맞나요?",
          "original_form": "애들도 많이 온다던데 맞나요?",
          "speaker_id": "1",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100037585.1.21",
          "form": "아 저는 성인반 다녀서요",
          "original_form": "아 저는 성인반 다녀서요",
          "speaker_id": "2",
          "time": "20210901 12:42"
        },
        {
          "id": "MDRW2100037585.1.22",
          "form": "청소년 어린이 타임이 따로 있댕ㄷ",
          "original_form": "청소년 어린이 타임이 따로 있댕{emoji:‥}ㄷ",
          "speaker_id": "2",
          "time": "20210901 12:43"
        },
        {
          "id": "MDRW2100037585.1.23",
          "form": "있대요",
          "original_form": "있대요",
          "speaker_id": "2",
          "time": "20210901 12:43"
        },
        {
          "id": "MDRW2100037585.1.24",
          "form": "아 헐",
          "original_form": "아 헐",
          "speaker_id": "1",
          "time": "20210901 12:44"
        },
        {
          "id": "MDRW2100037585.1.25",
          "form": "은근히 많이 다니는군요??!",
          "original_form": "은근히 많이 다니는군요??!",
          "speaker_id": "1",
          "time": "20210901 12:44"
        }
      ]
