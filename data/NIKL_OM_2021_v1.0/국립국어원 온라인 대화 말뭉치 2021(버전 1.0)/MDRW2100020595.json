      "utterance": [
        {
          "id": "MDRW2100020595.1.1",
          "form": "아아 그렇구나",
          "original_form": "아아 그렇구나",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020595.1.2",
          "form": "스탠딩은 앞쪽이죠?",
          "original_form": "스탠딩은 앞쪽이죠?",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020595.1.3",
          "form": "진짜 엄청 밀더라구요",
          "original_form": "진짜 엄청 밀더라구요",
          "speaker_id": "1",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020595.1.4",
          "form": "저도 덕질하는 데 ㅠㅠ",
          "original_form": "저도 덕질하는 데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 10:17"
        },
        {
          "id": "MDRW2100020595.1.5",
          "form": "네 앞쪽이요",
          "original_form": "네 앞쪽이요",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020595.1.6",
          "form": "코로나 터지고 입덕해서 콘서트는 아직이요",
          "original_form": "코로나 터지고 입덕해서 콘서트는 아직이요",
          "speaker_id": "2",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020595.1.7",
          "form": "오! 누구요?",
          "original_form": "오! 누구요?",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020595.1.8",
          "form": "아.... 앞쪽이 좋을 거 같은데 힘들구나",
          "original_form": "아.... 앞쪽이 좋을 거 같은데 힘들구나",
          "speaker_id": "2",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020595.1.9",
          "form": "전 아스트로요!",
          "original_form": "전 아스트로요!",
          "speaker_id": "2",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020595.1.10",
          "form": "오호! 아스트로 알죠!",
          "original_form": "오호! 아스트로 알죠!",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020595.1.11",
          "form": "아 그리고 데이식스 탈퇴 말 꺼내서 죄송해요",
          "original_form": "아 그리고 데이식스 탈퇴 말 꺼내서 죄송해요",
          "speaker_id": "2",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020595.1.12",
          "form": "제가 눈치 없었네요 ㅠㅠ",
          "original_form": "제가 눈치 없었네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020595.1.13",
          "form": "아니에요 괜찮아요",
          "original_form": "아니에요 괜찮아요",
          "speaker_id": "1",
          "time": "20210828 10:18"
        },
        {
          "id": "MDRW2100020595.1.14",
          "form": "전 데이식스 팬은 아니에요ㅋㅋ",
          "original_form": "전 데이식스 팬은 아니에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020595.1.15",
          "form": "데이식스는 친구따라 간거라서ㅋㅋㅋ",
          "original_form": "데이식스는 친구따라 간거라서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020595.1.16",
          "form": "신경안쓰셔도 돼요!",
          "original_form": "신경안쓰셔도 돼요!",
          "speaker_id": "1",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020595.1.17",
          "form": "아아 ㅎㅎ",
          "original_form": "아아 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020595.1.18",
          "form": "아스트로는 어떤노래 나올때 입덕하신거에요?",
          "original_form": "아스트로는 어떤노래 나올때 입덕하신거에요?",
          "speaker_id": "1",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020595.1.19",
          "form": "데이식스 알게된 계기가 그거라 ㅎㅎ",
          "original_form": "데이식스 알게된 계기가 그거라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:19"
        },
        {
          "id": "MDRW2100020595.1.20",
          "form": "니가 불어와 때요 ㅎㅎ",
          "original_form": "니가 불어와 때요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100020595.1.21",
          "form": "그럴만하죠ㅋㅋ",
          "original_form": "그럴만하죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100020595.1.22",
          "form": "그때부터 살짝 살짝 좋아하게 됐다가",
          "original_form": "그때부터 살짝 살짝 좋아하게 됐다가",
          "speaker_id": "2",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100020595.1.23",
          "form": "정확히 입덕시기를 모르겠네요",
          "original_form": "정확히 입덕시기를 모르겠네요",
          "speaker_id": "2",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100020595.1.24",
          "form": "스며들었다....",
          "original_form": "스며들었다....",
          "speaker_id": "2",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100020595.1.25",
          "form": "아이돌 콘서트 그럼 스탠딩 비추이신 거예요?",
          "original_form": "아이돌 콘서트 그럼 스탠딩 비추이신 거예요?",
          "speaker_id": "2",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100020595.1.26",
          "form": "마자요 저도 보통 조금씩 입덕되더라구요ㅋㅋ",
          "original_form": "마자요 저도 보통 조금씩 입덕되더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:20"
        },
        {
          "id": "MDRW2100020595.1.27",
          "form": "음..지금으로써는 비추에요ㅠ",
          "original_form": "음..지금으로써는 비추에요ㅠ",
          "speaker_id": "1",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100020595.1.28",
          "form": "그때 기억이 진짜 안좋아서",
          "original_form": "그때 기억이 진짜 안좋아서",
          "speaker_id": "1",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100020595.1.29",
          "form": "근데  또 팬들끼리 안밀고 하면",
          "original_form": "근데  또 팬들끼리 안밀고 하면",
          "speaker_id": "1",
          "time": "20210828 10:21"
        },
        {
          "id": "MDRW2100020595.1.30",
          "form": "가까이서 보니까 너무 좋을거같은데ㅠ",
          "original_form": "가까이서 보니까 너무 좋을거같은데ㅠ",
          "speaker_id": "1",
          "time": "20210828 10:22"
        },
        {
          "id": "MDRW2100020595.1.31",
          "form": "아....ㅠㅠ",
          "original_form": "아....ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 10:22"
        }
      ]
    }
