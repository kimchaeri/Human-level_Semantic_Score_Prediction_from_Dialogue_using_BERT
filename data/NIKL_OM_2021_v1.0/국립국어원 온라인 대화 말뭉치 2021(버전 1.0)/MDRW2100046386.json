        },
        {
          "id": "MDRW2100046386.1.2",
          "form": "곧 퇴근시간이네요",
          "original_form": "곧 퇴근시간이네요",
          "speaker_id": "1",
          "time": "20211020 16:52"
        },
        {
          "id": "MDRW2100046386.1.3",
          "form": "저녁에 뭐하시나요",
          "original_form": "저녁에 뭐하시나요",
          "speaker_id": "1",
          "time": "20211020 16:53"
        },
        {
          "id": "MDRW2100046386.1.4",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211020 16:54"
        },
        {
          "id": "MDRW2100046386.1.5",
          "form": "저는 쇼핑 하려구요",
          "original_form": "저는 쇼핑 하려구요",
          "speaker_id": "2",
          "time": "20211020 16:54"
        },
        {
          "id": "MDRW2100046386.1.6",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20211020 16:54"
        },
        {
          "id": "MDRW2100046386.1.7",
          "form": "저녁먹고!",
          "original_form": "저녁먹고!",
          "speaker_id": "2",
          "time": "20211020 16:54"
        },
        {
          "id": "MDRW2100046386.1.8",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "1",
          "time": "20211020 16:54"
        },
        {
          "id": "MDRW2100046386.1.9",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20211020 16:54"
        },
        {
          "id": "MDRW2100046386.1.10",
          "form": "저도 쇼핑 좋아해요 저도 집에서",
          "original_form": "저도 쇼핑 좋아해요 저도 집에서",
          "speaker_id": "1",
          "time": "20211020 16:54"
        },
        {
          "id": "MDRW2100046386.1.11",
          "form": "인터넷 쇼핑할 거 같아요",
          "original_form": "인터넷 쇼핑할 거 같아요",
          "speaker_id": "1",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.12",
          "form": "뭐 사시려구요~?",
          "original_form": "뭐 사시려구요~?",
          "speaker_id": "2",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.13",
          "form": "저는 급 추워져서",
          "original_form": "저는 급 추워져서",
          "speaker_id": "2",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.14",
          "form": "아우터를 보려구요",
          "original_form": "아우터를 보려구요",
          "speaker_id": "2",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.15",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.16",
          "form": "추워져서 자켓",
          "original_form": "추워져서 자켓",
          "speaker_id": "1",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.17",
          "form": "옼ㅋㅋㅋㅋㅋ",
          "original_form": "옼ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.18",
          "form": "ㅋㅋㅋ저랑 같네요",
          "original_form": "ㅋㅋㅋ저랑 같네요",
          "speaker_id": "1",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.19",
          "form": "다들 비슷ㅎㅏ구나...",
          "original_form": "다들 비슷ㅎㅏ구나...",
          "speaker_id": "2",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.20",
          "form": "사람사는거 다 똑같다...",
          "original_form": "사람사는거 다 똑같다...",
          "speaker_id": "2",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.22",
          "form": "갑자기 겨울이 찾아와서....",
          "original_form": "갑자기 겨울이 찾아와서....",
          "speaker_id": "2",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.23",
          "form": "옷을 안살수가없자나욧",
          "original_form": "옷을 안살수가없자나욧",
          "speaker_id": "2",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.24",
          "form": "네 진짜 똑같아서 신기하네요",
          "original_form": "네 진짜 똑같아서 신기하네요",
          "speaker_id": "1",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.25",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.26",
          "form": "자켓 엄청 두꺼운거 사셔야겠어요..",
          "original_form": "자켓 엄청 두꺼운거 사셔야겠어요..",
          "speaker_id": "2",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.27",
          "form": "왠만한 자켓으로는..",
          "original_form": "왠만한 자켓으로는..",
          "speaker_id": "2",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.28",
          "form": "안될거같은 추위입니다",
          "original_form": "안될거같은 추위입니다",
          "speaker_id": "2",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.29",
          "form": "털달린 아우터 사려고요 ㅋㅋ",
          "original_form": "털달린 아우터 사려고요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211020 16:55"
        },
        {
          "id": "MDRW2100046386.1.30",
          "form": "옼ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "옼ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211020 16:56"
        },
        {
          "id": "MDRW2100046386.1.31",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20211020 16:56"
        },
        {
          "id": "MDRW2100046386.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211020 16:56"
        },
        {
          "id": "MDRW2100046386.1.33",
          "form": "양털후리스!",
          "original_form": "양털후리스!",
          "speaker_id": "2",
          "time": "20211020 16:56"
        },
        {
          "id": "MDRW2100046386.1.34",
          "form": "그러니까요 갑자기 겨울 ㅠ",
          "original_form": "그러니까요 갑자기 겨울 ㅠ",
          "speaker_id": "1",
          "time": "20211020 16:56"
        },
        {
          "id": "MDRW2100046386.1.35",
          "form": "이제 정말 가을은 스쳐지나가네요..흑흑",
          "original_form": "이제 정말 가을은 스쳐지나가네요..흑흑",
          "speaker_id": "2",
          "time": "20211020 16:56"
        },
        {
          "id": "MDRW2100046386.1.36",
          "form": "가을 넘 아쉬워요",
          "original_form": "가을 넘 아쉬워요",
          "speaker_id": "1",
          "time": "20211020 16:56"
        },
        {
          "id": "MDRW2100046386.1.37",
          "form": "ㅎㅎ 맞아여 ㅠㅠ",
          "original_form": "ㅎㅎ 맞아여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20211020 16:56"
        },
        {
          "id": "MDRW2100046386.1.38",
          "form": "저녁은 뭐드세요?",
          "original_form": "저녁은 뭐드세요?",
          "speaker_id": "1",
          "time": "20211020 16:56"
        },
        {
          "id": "MDRW2100046386.1.39",
          "form": "전 짬뽕 시켜먹으려규요",
          "original_form": "전 짬뽕 시켜먹으려규요",
          "speaker_id": "1",
          "time": "20211020 16:56"
        },
        {
          "id": "MDRW2100046386.1.40",
          "form": "저는 그냥 간단하게 바나나 같은거 먹을거같아용\r\n",
          "original_form": "저는 그냥 간단하게 바나나 같은거 먹을거같아용\r\n",
