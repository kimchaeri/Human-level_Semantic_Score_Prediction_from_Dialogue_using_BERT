        {
          "id": "MDRW2100040481.1.1",
          "form": "아침은 챙겨먹는 편인가요?",
          "original_form": "아침은 챙겨먹는 편인가요?",
          "speaker_id": "2",
          "time": "20210905 06:52"
        },
        {
          "id": "MDRW2100040481.1.2",
          "form": "아뇨 전 아침은 잘 안 먹네요",
          "original_form": "아뇨 전 아침은 잘 안 먹네요",
          "speaker_id": "1",
          "time": "20210905 14:47"
        },
        {
          "id": "MDRW2100040481.1.3",
          "form": "먹어도 빵? 커피?",
          "original_form": "먹어도 빵? 커피?",
          "speaker_id": "1",
          "time": "20210905 14:48"
        },
        {
          "id": "MDRW2100040481.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210905 14:48"
        },
        {
          "id": "MDRW2100040481.1.5",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100040481.1.6",
          "form": "저희 가족은 다 아침 꼭꼭 먹어요",
          "original_form": "저희 가족은 다 아침 꼭꼭 먹어요",
          "speaker_id": "2",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100040481.1.7",
          "form": "습관이 돼서요",
          "original_form": "습관이 돼서요",
          "speaker_id": "2",
          "time": "20210905 17:02"
        },
        {
          "id": "MDRW2100040481.1.8",
          "form": "오ㅎㅎ",
          "original_form": "오ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 17:16"
        },
        {
          "id": "MDRW2100040481.1.9",
          "form": "다같이 먹나요?",
          "original_form": "다같이 먹나요?",
          "speaker_id": "1",
          "time": "20210905 17:16"
        },
        {
          "id": "MDRW2100040481.1.10",
          "form": "화목한데요!!",
          "original_form": "화목한데요!!",
          "speaker_id": "1",
          "time": "20210905 17:16"
        },
        {
          "id": "MDRW2100040481.1.11",
          "form": "보통 뭐 먹어요?",
          "original_form": "보통 뭐 먹어요?",
          "speaker_id": "1",
          "time": "20210905 17:16"
        },
        {
          "id": "MDRW2100040481.1.12",
          "form": "원래는 일반 밥 국 반찬으로 먹는데",
          "original_form": "원래는 일반 밥 국 반찬으로 먹는데",
          "speaker_id": "2",
          "time": "20210905 17:20"
        },
        {
          "id": "MDRW2100040481.1.13",
          "form": "요새 조금씩 변화를 주고 있어요",
          "original_form": "요새 조금씩 변화를 주고 있어요",
          "speaker_id": "2",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100040481.1.14",
          "form": "오 딱 한식 아침밥이네요ㅎㅎ",
          "original_form": "오 딱 한식 아침밥이네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100040481.1.15",
          "form": "요거트에 견과류랑 과일 채소 넣어서 먹는 것도 하고",
          "original_form": "요거트에 견과류랑 과일 채소 넣어서 먹는 것도 하고",
          "speaker_id": "2",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100040481.1.16",
          "form": "계란토스트 해 먹기도 하고",
          "original_form": "계란토스트 해 먹기도 하고",
          "speaker_id": "2",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100040481.1.17",
          "form": "샐러드도 먹나요!?",
          "original_form": "샐러드도 먹나요!?",
          "speaker_id": "1",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100040481.1.18",
          "form": "마켓컬리에 요즘 그런 거 많던데",
          "original_form": "마켓컬리에 요즘 그런 거 많던데",
          "speaker_id": "1",
          "time": "20210905 17:21"
        },
        {
          "id": "MDRW2100040481.1.19",
          "form": "네 샐러드도 해먹고",
          "original_form": "네 샐러드도 해먹고",
          "speaker_id": "2",
          "time": "20210905 17:29"
        },
        {
          "id": "MDRW2100040481.1.20",
          "form": "점점 간소화 되고 있어요",
          "original_form": "점점 간소화 되고 있어요",
          "speaker_id": "2",
          "time": "20210905 17:30"
        },
        {
          "id": "MDRW2100040481.1.21",
          "form": "ㅎㅎ근데 하루 세끼 먹을거면",
          "original_form": "ㅎㅎ근데 하루 세끼 먹을거면",
          "speaker_id": "1",
          "time": "20210905 17:54"
        },
        {
          "id": "MDRW2100040481.1.22",
          "form": "아침은 위장에 무리안되는게",
          "original_form": "아침은 위장에 무리안되는게",
          "speaker_id": "1",
          "time": "20210905 17:54"
        },
        {
          "id": "MDRW2100040481.1.23",
          "form": "좋은거같네요",
          "original_form": "좋은거같네요",
          "speaker_id": "1",
          "time": "20210905 17:54"
        },
        {
