      "utterance": [
        {
          "id": "MDRW2100028164.1.1",
          "form": "우와 ㅋㅋㅋㅋㅋ",
          "original_form": "우와 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028164.1.2",
          "form": "엄마냥도 대단하네여",
          "original_form": "엄마냥도 대단하네여",
          "speaker_id": "2",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028164.1.3",
          "form": "좋은 사람을 알아보는 눈이 있나봐요",
          "original_form": "좋은 사람을 알아보는 눈이 있나봐요",
          "speaker_id": "2",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100028164.1.4",
          "form": "그니까요 ㄷㄷ 애기도 엄청 낳아놨더라구요",
          "original_form": "그니까요 ㄷㄷ 애기도 엄청 낳아놨더라구요",
          "speaker_id": "1",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028164.1.5",
          "form": "이럴때보면 모성애라는게",
          "original_form": "이럴때보면 모성애라는게",
          "speaker_id": "2",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028164.1.6",
          "form": "너무 신기해요",
          "original_form": "너무 신기해요",
          "speaker_id": "2",
          "time": "20210902 18:00"
        },
        {
          "id": "MDRW2100028164.1.7",
          "form": "그리고 메탈남님도 진짜 대단하신 거 같아요",
          "original_form": "그리고 메탈남님도 진짜 대단하신 거 같아요",
          "speaker_id": "2",
          "time": "20210902 18:01"
        },
        {
          "id": "MDRW2100028164.1.8",
          "form": "맞아요 덕분에 채널도 떡상 ㅋㅋㅋ",
          "original_form": "맞아요 덕분에 채널도 떡상 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:02"
        },
        {
          "id": "MDRW2100028164.1.9",
          "form": "그분 고양이 집에 안들이신다했는데 ㅋㅋ",
          "original_form": "그분 고양이 집에 안들이신다했는데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:03"
        },
        {
          "id": "MDRW2100028164.1.10",
          "form": "ㅋㅋㅋㅋㅋ으아",
          "original_form": "ㅋㅋㅋㅋㅋ으아",
          "speaker_id": "2",
          "time": "20210902 18:04"
        },
        {
          "id": "MDRW2100028164.1.11",
          "form": "그래도 큰 결심하신게 너무 멋있고",
          "original_form": "그래도 큰 결심하신게 너무 멋있고",
          "speaker_id": "2",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100028164.1.12",
          "form": "사람이 따듯해 보여요 ㅠㅠ",
          "original_form": "사람이 따듯해 보여요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 18:05"
        },
        {
          "id": "MDRW2100028164.1.13",
          "form": "저는 두마리도 힘든데",
          "original_form": "저는 두마리도 힘든데",
          "speaker_id": "1",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028164.1.14",
          "form": "도대체 몇마린지 ㄷㄷ",
          "original_form": "도대체 몇마린지 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210902 18:07"
        },
        {
          "id": "MDRW2100028164.1.15",
          "form": "그니까요 ㄷㄷㄷ 정성이 정말..",
          "original_form": "그니까요 ㄷㄷㄷ 정성이 정말..",
          "speaker_id": "2",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100028164.1.16",
          "form": "진짜 요즘에 그런 사람들 많은 거 같아요",
          "original_form": "진짜 요즘에 그런 사람들 많은 거 같아요",
          "speaker_id": "2",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100028164.1.17",
          "form": "잠깐 키우다가 유기하고..",
          "original_form": "잠깐 키우다가 유기하고..",
          "speaker_id": "2",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100028164.1.18",
          "form": "그런 무책임한 사람들이요..ㅠ",
          "original_form": "그런 무책임한 사람들이요..ㅠ",
          "speaker_id": "2",
          "time": "20210902 18:08"
        },
        {
          "id": "MDRW2100028164.1.19",
          "form": "ㅠㅠ 연예인들도요",
          "original_form": "ㅠㅠ 연예인들도요",
          "speaker_id": "1",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100028164.1.20",
          "form": "김희철 뭐 욕먹고있던데",
          "original_form": "김희철 뭐 욕먹고있던데",
          "speaker_id": "1",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100028164.1.21",
          "form": "아 그거 유기견 키우는 사람들 대단하다고,,",
          "original_form": "아 그거 유기견 키우는 사람들 대단하다고,,",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100028164.1.22",
          "form": "어떻게 유기견 키우는지 모르겠다고 그런 이야기 했나봐요",
          "original_form": "어떻게 유기견 키우는지 모르겠다고 그런 이야기 했나봐요",
          "speaker_id": "2",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100028164.1.23",
          "form": "저도 잠깐봤네요 ㅎ",
          "original_form": "저도 잠깐봤네요 ㅎ",
          "speaker_id": "2",
          "time": "20210902 18:12"
        },
        {
          "id": "MDRW2100028164.1.24",
          "form": "사지말고 보호소 입양하면 좋을텐데 ㅠ",
          "original_form": "사지말고 보호소 입양하면 좋을텐데 ㅠ",
          "speaker_id": "1",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028164.1.25",
          "form": "발언이 조금 문제가 있었나바요",
          "original_form": "발언이 조금 문제가 있었나바요",
          "speaker_id": "1",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028164.1.26",
          "form": "그니까요.. 연예인의 삶도 정말",
          "original_form": "그니까요.. 연예인의 삶도 정말",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028164.1.27",
          "form": "어려워 보여요 진짜..ㅠ",
          "original_form": "어려워 보여요 진짜..ㅠ",
          "speaker_id": "2",
          "time": "20210902 18:15"
        },
        {
          "id": "MDRW2100028164.1.28",
          "form": "오늘 티비에 축구 한다네요",
          "original_form": "오늘 티비에 축구 한다네요",
          "speaker_id": "1",
          "time": "20210902 18:17"
        },
        {
          "id": "MDRW2100028164.1.29",
          "form": "오 야구도 하는데..!",
          "original_form": "오 야구도 하는데..!",
          "speaker_id": "2",
          "time": "20210902 18:19"
        }
      ]
    }
