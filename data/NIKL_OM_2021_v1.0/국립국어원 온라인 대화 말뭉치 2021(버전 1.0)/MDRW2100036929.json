      "utterance": [
        {
          "id": "MDRW2100036929.1.1",
          "form": "점심은 드셨나요",
          "original_form": "점심은 드셨나요",
          "speaker_id": "1",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100036929.1.2",
          "form": "네 토스트요ㅋㅋ\n어떤거 드셨어요?",
          "original_form": "네 토스트요ㅋㅋ\n어떤거 드셨어요?",
          "speaker_id": "2",
          "time": "20210831 15:12"
        },
        {
          "id": "MDRW2100036929.1.3",
          "form": "저는 라면 끓여먹었어요",
          "original_form": "저는 라면 끓여먹었어요",
          "speaker_id": "1",
          "time": "20210831 15:14"
        },
        {
          "id": "MDRW2100036929.1.4",
          "form": "아 맛있겠네요\n전 밀가루종류를 좋아해서요ㅋㅋ",
          "original_form": "아 맛있겠네요\n전 밀가루종류를 좋아해서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:15"
        },
        {
          "id": "MDRW2100036929.1.5",
          "form": "저도 밀가루라면 정신을 못차려요",
          "original_form": "저도 밀가루라면 정신을 못차려요",
          "speaker_id": "1",
          "time": "20210831 15:16"
        },
        {
          "id": "MDRW2100036929.1.6",
          "form": "면, 빵, 밀떡 전부요",
          "original_form": "면, 빵, 밀떡 전부요",
          "speaker_id": "1",
          "time": "20210831 15:16"
        },
        {
          "id": "MDRW2100036929.1.7",
          "form": "비오는날은 밀가루죠ㅋㅋ",
          "original_form": "비오는날은 밀가루죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 15:16"
        },
        {
          "id": "MDRW2100036929.1.8",
          "form": "저녁에 수제비먹을까 생각중이에요",
          "original_form": "저녁에 수제비먹을까 생각중이에요",
          "speaker_id": "2",
          "time": "20210831 15:17"
        },
        {
          "id": "MDRW2100036929.1.9",
          "form": "수제비도 좋죠",
          "original_form": "수제비도 좋죠",
          "speaker_id": "1",
          "time": "20210831 15:18"
        },
        {
          "id": "MDRW2100036929.1.10",
          "form": "저는 떡볶이 생각하고 있습니다",
          "original_form": "저는 떡볶이 생각하고 있습니다",
          "speaker_id": "1",
          "time": "20210831 15:18"
        },
        {
          "id": "MDRW2100036929.1.11",
          "form": "앗 전요즘 미미네 떡볶이에 빠졌어여",
          "original_form": "앗 전요즘 미미네 떡볶이에 빠졌어여",
          "speaker_id": "2",
          "time": "20210831 15:19"
        },
        {
          "id": "MDRW2100036929.1.12",
          "form": "넘맛있어여ㄷㄷ",
          "original_form": "넘맛있어여ㄷㄷ",
          "speaker_id": "2",
          "time": "20210831 15:20"
        },
        {
          "id": "MDRW2100036929.1.13",
          "form": "미미네 떡볶이는 처음 들어봐요",
          "original_form": "미미네 떡볶이는 처음 들어봐요",
          "speaker_id": "1",
          "time": "20210831 15:21"
        },
        {
          "id": "MDRW2100036929.1.14",
          "form": "전요리는 못해서 사서 많이 먹거든요ㅋㅋ\n국물떡볶이인데 떡도 말랑하고 달아서 맛있어요",
          "original_form": "전요리는 못해서 사서 많이 먹거든요ㅋㅋ\n국물떡볶이인데 떡도 말랑하고 달아서 맛있어요",
          "speaker_id": "2",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100036929.1.15",
          "form": "달달하다니 궁금하네요",
          "original_form": "달달하다니 궁금하네요",
          "speaker_id": "1",
          "time": "20210831 15:22"
        },
        {
          "id": "MDRW2100036929.1.16",
          "form": "네 드셔보세요 요즘은 떡볶이도 종류가 엄청 많아여",
          "original_form": "네 드셔보세요 요즘은 떡볶이도 종류가 엄청 많아여",
          "speaker_id": "2",
          "time": "20210831 15:24"
        },
        {
          "id": "MDRW2100036929.1.17",
          "form": "맨날 매운 떡볶이만 먹어서 새로운게 먹고싶은 참이었어요",
          "original_form": "맨날 매운 떡볶이만 먹어서 새로운게 먹고싶은 참이었어요",
          "speaker_id": "1",
          "time": "20210831 15:26"
        },
        {
          "id": "MDRW2100036929.1.18",
          "form": "매운거 잘드세요?",
          "original_form": "매운거 잘드세요?",
          "speaker_id": "2",
          "time": "20210831 15:31"
        },
        {
          "id": "MDRW2100036929.1.19",
          "form": "잘 먹는 편은 아니에요",
          "original_form": "잘 먹는 편은 아니에요",
          "speaker_id": "1",
          "time": "20210831 15:31"
        },
        {
          "id": "MDRW2100036929.1.20",
          "form": "억지로라도 매운건 먹어요",
          "original_form": "억지로라도 매운건 먹어요",
          "speaker_id": "1",
          "time": "20210831 15:32"
        }
      ]
