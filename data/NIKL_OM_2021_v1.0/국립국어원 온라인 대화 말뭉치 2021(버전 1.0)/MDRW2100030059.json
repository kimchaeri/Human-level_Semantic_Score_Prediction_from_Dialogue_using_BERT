        },
        {
          "id": "MDRW2100030059.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210903 20:14"
        },
        {
          "id": "MDRW2100030059.1.3",
          "form": "온라인 커뮤니티 활동하시는 곳이 있나요?",
          "original_form": "온라인 커뮤니티 활동하시는 곳이 있나요?",
          "speaker_id": "2",
          "time": "20210903 20:14"
        },
        {
          "id": "MDRW2100030059.1.4",
          "form": "음... 연예인 좋아해서 트위터 정도 해요!",
          "original_form": "음... 연예인 좋아해서 트위터 정도 해요!",
          "speaker_id": "1",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100030059.1.5",
          "form": "활동하기는 곳 있나요?",
          "original_form": "활동하기는 곳 있나요?",
          "speaker_id": "1",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100030059.1.6",
          "form": "저도 트위터나 인스타그램 많이 둘러봐요!",
          "original_form": "저도 트위터나 인스타그램 많이 둘러봐요!",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100030059.1.7",
          "form": "그런데 인스타그램은 많이 어렵더라구요",
          "original_form": "그런데 인스타그램은 많이 어렵더라구요",
          "speaker_id": "2",
          "time": "20210903 20:16"
        },
        {
          "id": "MDRW2100030059.1.8",
          "form": "우왕 그러시구나 저도 인스타는 있어용!",
          "original_form": "우왕 그러시구나 저도 인스타는 있어용!",
          "speaker_id": "1",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100030059.1.9",
          "form": "저두요...잘 몰라서 그냥 연예인 팔로우해요",
          "original_form": "저두요...잘 몰라서 그냥 연예인 팔로우해요",
          "speaker_id": "1",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100030059.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100030059.1.11",
          "form": "트위터는 좀 익숙한데 인스타그램은 어떻게 활동하고 대화하는건지 잘 모르겠어요ㅋㅋㅋㅋㅋ",
          "original_form": "트위터는 좀 익숙한데 인스타그램은 어떻게 활동하고 대화하는건지 잘 모르겠어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:17"
        },
        {
          "id": "MDRW2100030059.1.12",
          "form": "저두요ㅋㅋㅋㅋ트위터는 좀 오래했어서...",
          "original_form": "저두요ㅋㅋㅋㅋ트위터는 좀 오래했어서...",
          "speaker_id": "1",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100030059.1.13",
          "form": "거의 플텍 걸고 고인물이지망요...",
          "original_form": "거의 플텍 걸고 고인물이지망요...",
          "speaker_id": "1",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100030059.1.14",
          "form": "스페이스? 생긴 건 좀 재밌더라구용",
          "original_form": "스페이스? 생긴 건 좀 재밌더라구용",
          "speaker_id": "1",
          "time": "20210903 20:18"
        },
        {
          "id": "MDRW2100030059.1.15",
          "form": "ㅋㅋㅋㅋㅋ저도 플텍걸었어요",
          "original_form": "ㅋㅋㅋㅋㅋ저도 플텍걸었어요",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100030059.1.16",
          "form": "스페이스가 그",
          "original_form": "스페이스가 그",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100030059.1.17",
          "form": "클럽하우스? 같은 대화방인가요?",
          "original_form": "클럽하우스? 같은 대화방인가요?",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100030059.1.18",
          "form": "아 마자요!!!",
          "original_form": "아 마자요!!!",
          "speaker_id": "1",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100030059.1.19",
          "form": "클럽하우스는 빨리 사라져버렸지만ㅋㅋㅋㅋㅋ",
          "original_form": "클럽하우스는 빨리 사라져버렸지만ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100030059.1.20",
          "form": "사라졌나요ㅋㅋㅋㅋㅠ",
          "original_form": "사라졌나요ㅋㅋㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100030059.1.21",
          "form": "한참 인기있어서",
          "original_form": "한참 인기있어서",
          "speaker_id": "2",
          "time": "20210903 20:19"
        },
        {
          "id": "MDRW2100030059.1.22",
          "form": "아니 먼가 유행이 빨리 사그라든 느낌? 이지않나용ㅋㅋㅋㅋ",
          "original_form": "아니 먼가 유행이 빨리 사그라든 느낌? 이지않나용ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100030059.1.23",
          "form": "사라진지는 모르겟어용",
          "original_form": "사라진지는 모르겟어용",
          "speaker_id": "1",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100030059.1.24",
          "form": "저만 초대장 없는줄 알았는데",
          "original_form": "저만 초대장 없는줄 알았는데",
          "speaker_id": "2",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100030059.1.25",
          "form": "저두 없었어요...ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저두 없었어요...ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100030059.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ저 빼고 다 하는 줄 알았어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ저 빼고 다 하는 줄 알았어요",
          "speaker_id": "2",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100030059.1.27",
          "form": "트위터 스페이스에서는 교류 많이하나요?",
          "original_form": "트위터 스페이스에서는 교류 많이하나요?",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100030059.1.28",
          "form": "온라인 정모같은걸까요? 아니면 한 사람만 계속 말하는 건가요?",
          "original_form": "온라인 정모같은걸까요? 아니면 한 사람만 계속 말하는 건가요?",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100030059.1.29",
          "form": "아녀아뇨ㅋㅋㅋㅋㅋ저는 완전 걍 듣기만 하는...",
          "original_form": "아녀아뇨ㅋㅋㅋㅋㅋ저는 완전 걍 듣기만 하는...",
          "speaker_id": "1",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100030059.1.30",
          "form": "음 저두 많이 해보진 않았는데",
          "original_form": "음 저두 많이 해보진 않았는데",
          "speaker_id": "1",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100030059.1.31",
          "form": "인스타 라방같은 느낌으로?",
          "original_form": "인스타 라방같은 느낌으로?",
          "speaker_id": "1",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100030059.1.32",
          "form": "하시는 거 같아요 이쪽은 좀ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "하시는 거 같아요 이쪽은 좀ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100030059.1.33",
          "form": "그래도 실시간으로 자리차지하고  듣는것도 일종의 교류인 것 같아오ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그래도 실시간으로 자리차지하고  듣는것도 일종의 교류인 것 같아오ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100030059.1.34",
          "form": "스페이스 들으면서 댓글같은것도 달 수 있나요?",
          "original_form": "스페이스 들으면서 댓글같은것도 달 수 있나요?",
          "speaker_id": "2",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100030059.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋ음 아뇨 페잉같은 걸로 올리면",
          "original_form": "ㅋㅋㅋㅋㅋㅋ음 아뇨 페잉같은 걸로 올리면",
          "speaker_id": "1",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100030059.1.36",
          "form": "계정주가 읽어요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "계정주가 읽어요ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100030059.1.37",
          "form": "저도 많이 해보진 않아서 민망...",
          "original_form": "저도 많이 해보진 않아서 민망...",
          "speaker_id": "1",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100030059.1.38",
          "form": "근데 그냥 정말 다같이 교류하는 곳도 잇구?",
          "original_form": "근데 그냥 정말 다같이 교류하는 곳도 잇구?",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100030059.1.39",
          "form": "저도 우물안 개구리라ㅠㅠㅠㅋ큐큐ㅠ",
          "original_form": "저도 우물안 개구리라ㅠㅠㅠㅋ큐큐ㅠ",
          "speaker_id": "1",
          "time": "20210903 20:25"
        },
        {
          "id": "MDRW2100030059.1.40",
          "form": "아마 다양한 방법으로 많이 하시지않을까 싶어요ㅋㅋㅋㅋ",
          "original_form": "아마 다양한 방법으로 많이 하시지않을까 싶어요ㅋㅋㅋㅋ",
