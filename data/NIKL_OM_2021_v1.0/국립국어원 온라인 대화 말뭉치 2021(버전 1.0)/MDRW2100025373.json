      "utterance": [
        {
          "id": "MDRW2100025373.1.1",
          "form": "안녕하세요! 여행 좋아하세요?",
          "original_form": "안녕하세요! 여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025373.1.2",
          "form": "네!!!! 좋아해요 지금은 자제중이지만요",
          "original_form": "네!!!! 좋아해요 지금은 자제중이지만요",
          "speaker_id": "2",
          "time": "20210901 11:51"
        },
        {
          "id": "MDRW2100025373.1.3",
          "form": "네. 여행가고 싶어요.",
          "original_form": "네. 여행가고 싶어요.",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025373.1.4",
          "form": "언제쯤 갈수있을까요?",
          "original_form": "언제쯤 갈수있을까요?",
          "speaker_id": "1",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100025373.1.5",
          "form": "그니까요 ㅠㅠ",
          "original_form": "그니까요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 13:33"
        },
        {
          "id": "MDRW2100025373.1.6",
          "form": "최근에 국내라도 다녀오신곳은 없으신가요?",
          "original_form": "최근에 국내라도 다녀오신곳은 없으신가요?",
          "speaker_id": "2",
          "time": "20210901 13:33"
        },
        {
          "id": "MDRW2100025373.1.7",
          "form": "제주도에 살아서 한번씩 바다정도만 봐요",
          "original_form": "제주도에 살아서 한번씩 바다정도만 봐요",
          "speaker_id": "1",
          "time": "20210901 13:33"
        },
        {
          "id": "MDRW2100025373.1.8",
          "form": "서귀포는 관광객 많아서 제주시쪽만 가요",
          "original_form": "서귀포는 관광객 많아서 제주시쪽만 가요",
          "speaker_id": "1",
          "time": "20210901 13:34"
        },
        {
          "id": "MDRW2100025373.1.9",
          "form": "아..제주도.. 전 딱 1번 갔었는데 여행으로요ㅎㅎ",
          "original_form": "아..제주도.. 전 딱 1번 갔었는데 여행으로요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 13:39"
        },
        {
          "id": "MDRW2100025373.1.10",
          "form": "그것도 벌써 4년전이네요",
          "original_form": "그것도 벌써 4년전이네요",
          "speaker_id": "2",
          "time": "20210901 13:39"
        },
        {
          "id": "MDRW2100025373.1.11",
          "form": "저는 육지로 여행을^^",
          "original_form": "저는 육지로 여행을^^",
          "speaker_id": "1",
          "time": "20210901 13:39"
        },
        {
          "id": "MDRW2100025373.1.12",
          "form": "최근 간곳이 부산이었어요. 2년전이요",
          "original_form": "최근 간곳이 부산이었어요. 2년전이요",
          "speaker_id": "1",
          "time": "20210901 13:40"
        },
        {
          "id": "MDRW2100025373.1.13",
          "form": "전 바다보러 여수에 갔었어요",
          "original_form": "전 바다보러 여수에 갔었어요",
          "speaker_id": "2",
          "time": "20210901 13:40"
        },
        {
          "id": "MDRW2100025373.1.14",
          "form": "부산은 한 5년전에 간거 같아요..",
          "original_form": "부산은 한 5년전에 간거 같아요..",
          "speaker_id": "2",
          "time": "20210901 13:40"
        },
        {
          "id": "MDRW2100025373.1.15",
          "form": "부산에서 뭐가 좋으셨나요?",
          "original_form": "부산에서 뭐가 좋으셨나요?",
          "speaker_id": "2",
          "time": "20210901 13:41"
        },
        {
          "id": "MDRW2100025373.1.16",
          "form": "회요ㅎㅎ",
          "original_form": "회요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 13:41"
        },
        {
          "id": "MDRW2100025373.1.17",
          "form": "솔직히 바다랑 산은 제주도가 낫구요",
          "original_form": "솔직히 바다랑 산은 제주도가 낫구요",
          "speaker_id": "1",
          "time": "20210901 13:41"
        },
        {
          "id": "MDRW2100025373.1.18",
          "form": "자갈치 생선이 저렴해서  좋아요",
          "original_form": "자갈치 생선이 저렴해서  좋아요",
          "speaker_id": "1",
          "time": "20210901 13:41"
        },
        {
          "id": "MDRW2100025373.1.19",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210901 13:42"
        },
        {
          "id": "MDRW2100025373.1.20",
          "form": "제주도는 다 비싸요",
          "original_form": "제주도는 다 비싸요",
          "speaker_id": "1",
          "time": "20210901 13:43"
        },
        {
          "id": "MDRW2100025373.1.21",
          "form": "인터넷 쇼핑하기 힘드니 여행지로만 좋아요ㅎ",
          "original_form": "인터넷 쇼핑하기 힘드니 여행지로만 좋아요ㅎ",
          "speaker_id": "1",
          "time": "20210901 13:43"
        },
        {
          "id": "MDRW2100025373.1.22",
          "form": "저는 여수바다도 좋았어요.. 바다앞 카페도 멋지구요 ㅋㅋ 제주도도 멋지지만요 ㅎ",
          "original_form": "저는 여수바다도 좋았어요.. 바다앞 카페도 멋지구요 ㅋㅋ 제주도도 멋지지만요 ㅎ",
          "speaker_id": "2",
          "time": "20210901 13:43"
        },
        {
          "id": "MDRW2100025373.1.23",
          "form": "여수도 가보고 싶어요",
          "original_form": "여수도 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100025373.1.24",
          "form": "아.. 그런 맹점이 또 있네요 ㅠㅠ",
          "original_form": "아.. 그런 맹점이 또 있네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100025373.1.25",
          "form": "국내여행도 볼게 많지요",
          "original_form": "국내여행도 볼게 많지요",
          "speaker_id": "1",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100025373.1.26",
          "form": "여수 예술랜드라고 리조트가 있는데요 숙박하지 않아도",
          "original_form": "여수 예술랜드라고 리조트가 있는데요 숙박하지 않아도",
          "speaker_id": "2",
