      "utterance": [
        {
          "id": "MDRW2100023303.1.1",
          "form": "아니요..1일1팩은 전 영양과다라뽀드락지생ㄱㅕ요",
          "original_form": "아니요..1일1팩은 전 영양과다라뽀드락지생ㄱㅕ요",
          "speaker_id": "2",
          "time": "20210829 23:51"
        },
        {
          "id": "MDRW2100023303.1.2",
          "form": "하지마세요",
          "original_form": "하지마세요",
          "speaker_id": "2",
          "time": "20210829 23:51"
        },
        {
          "id": "MDRW2100023303.1.3",
          "form": "아 저도 그렇긴했어요",
          "original_form": "아 저도 그렇긴했어요",
          "speaker_id": "1",
          "time": "20210829 23:51"
        },
        {
          "id": "MDRW2100023303.1.4",
          "form": "그냥잠을푹자는게 건강에 좋은것같아요",
          "original_form": "그냥잠을푹자는게 건강에 좋은것같아요",
          "speaker_id": "2",
          "time": "20210829 23:51"
        },
        {
          "id": "MDRW2100023303.1.5",
          "form": "맞아요ㅜ 피부 좋으세요?",
          "original_form": "맞아요ㅜ 피부 좋으세요?",
          "speaker_id": "1",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023303.1.6",
          "form": "저는 좋은편인데 모공이넓어요",
          "original_form": "저는 좋은편인데 모공이넓어요",
          "speaker_id": "2",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023303.1.7",
          "form": "저는 모공도 넓고ㅜ",
          "original_form": "저는 모공도 넓고ㅜ",
          "speaker_id": "1",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023303.1.8",
          "form": "여드름자국도 있어요 ᆢ",
          "original_form": "여드름자국도 있어요 ᆢ",
          "speaker_id": "1",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023303.1.9",
          "form": "아저는여드름은잘안났어요",
          "original_form": "아저는여드름은잘안났어요",
          "speaker_id": "2",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023303.1.10",
          "form": "피부과다니세요?",
          "original_form": "피부과다니세요?",
          "speaker_id": "2",
          "time": "20210829 23:52"
        },
        {
          "id": "MDRW2100023303.1.11",
          "form": "아 부러운 피부네요",
          "original_form": "아 부러운 피부네요",
          "speaker_id": "1",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023303.1.12",
          "form": "조금 다녔는데",
          "original_form": "조금 다녔는데",
          "speaker_id": "1",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023303.1.13",
          "form": "잠을 푹자면 피부도 좋아지고건강도좋아져요",
          "original_form": "잠을 푹자면 피부도 좋아지고건강도좋아져요",
          "speaker_id": "2",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023303.1.14",
          "form": "코로나땜에 가기 무서워졌어요",
          "original_form": "코로나땜에 가기 무서워졌어요",
          "speaker_id": "1",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023303.1.15",
          "form": "어떤시술받으셨어요?",
          "original_form": "어떤시술받으셨어요?",
          "speaker_id": "2",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023303.1.16",
          "form": "모델링팩은좋더라구요",
          "original_form": "모델링팩은좋더라구요",
          "speaker_id": "2",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023303.1.17",
          "form": "여드름 관리랑 프락셀이요",
          "original_form": "여드름 관리랑 프락셀이요",
          "speaker_id": "1",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023303.1.18",
          "form": "돈아까워서 집에서 하는중이예요",
          "original_form": "돈아까워서 집에서 하는중이예요",
          "speaker_id": "2",
          "time": "20210829 23:53"
        },
        {
          "id": "MDRW2100023303.1.19",
          "form": "프락셀효과어때요?",
          "original_form": "프락셀효과어때요?",
          "speaker_id": "2",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023303.1.20",
          "form": "오 모델링팩 참고할게요",
          "original_form": "오 모델링팩 참고할게요",
          "speaker_id": "1",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023303.1.21",
          "form": "세번은 해야한다는데",
          "original_form": "세번은 해야한다는데",
          "speaker_id": "1",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023303.1.22",
          "form": "네 몇번하셨나오",
          "original_form": "네 몇번하셨나오",
          "speaker_id": "2",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023303.1.23",
          "form": "코로나 터져서 한번밖에 못했어요ㅜ",
          "original_form": "코로나 터져서 한번밖에 못했어요ㅜ",
          "speaker_id": "1",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023303.1.24",
          "form": "그래서 별 효과가 ㅎㅎ",
          "original_form": "그래서 별 효과가 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023303.1.25",
          "form": "이런 ..저는 300샷 무슨..  레이져리프팅했는데",
          "original_form": "이런 ..저는 300샷 무슨..  레이져리프팅했는데",
          "speaker_id": "2",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023303.1.26",
          "form": "하지마세요",
          "original_form": "하지마세요",
          "speaker_id": "2",
          "time": "20210829 23:54"
        },
        {
          "id": "MDRW2100023303.1.27",
          "form": "아프기만하고 효과도 없어요",
          "original_form": "아프기만하고 효과도 없어요",
          "speaker_id": "2",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100023303.1.28",
          "form": "피부과 시술들이",
          "original_form": "피부과 시술들이",
          "speaker_id": "1",
          "time": "20210829 23:55"
        },
        {
          "id": "MDRW2100023303.1.29",
          "form": "복불복인거 같아요",
          "original_form": "복불복인거 같아요",
          "speaker_id": "1",
          "time": "20210829 23:55"
        }
      ]
