      "utterance": [
        {
          "id": "MDRW2100042002.1.1",
          "form": "미숫가루요?",
          "original_form": "미숫가루요?",
          "speaker_id": "2",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100042002.1.2",
          "form": "네 여름엔 꼭 마셔줘야 하는데ㅠㅠ",
          "original_form": "네 여름엔 꼭 마셔줘야 하는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 18:25"
        },
        {
          "id": "MDRW2100042002.1.3",
          "form": "미숫가루 안먹은지 오래되었는데..",
          "original_form": "미숫가루 안먹은지 오래되었는데..",
          "speaker_id": "2",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100042002.1.4",
          "form": "얼음동동띄워서 시원하게 마셔줘야하는데..",
          "original_form": "얼음동동띄워서 시원하게 마셔줘야하는데..",
          "speaker_id": "1",
          "time": "20210906 19:18"
        },
        {
          "id": "MDRW2100042002.1.5",
          "form": "우유에 타서 진하게 마시면 엄청 맛있죠",
          "original_form": "우유에 타서 진하게 마시면 엄청 맛있죠",
          "speaker_id": "2",
          "time": "20210906 19:19"
        },
        {
          "id": "MDRW2100042002.1.6",
          "form": "저는 우유에 탄것도 좋지만 물에탄거 더 좋아해요!",
          "original_form": "저는 우유에 탄것도 좋지만 물에탄거 더 좋아해요!",
          "speaker_id": "1",
          "time": "20210906 19:19"
        },
        {
          "id": "MDRW2100042002.1.7",
          "form": "약간 슴슴한 맛이요?",
          "original_form": "약간 슴슴한 맛이요?",
          "speaker_id": "2",
          "time": "20210906 19:19"
        },
        {
          "id": "MDRW2100042002.1.8",
          "form": "넿ㅎㅎ 좀더 맑은 느낌요",
          "original_form": "넿ㅎㅎ 좀더 맑은 느낌요",
          "speaker_id": "1",
          "time": "20210906 19:19"
        },
        {
          "id": "MDRW2100042002.1.9",
          "form": "맛있겠네요 설탕타서 달달하게 마시면 ㅎㅎ",
          "original_form": "맛있겠네요 설탕타서 달달하게 마시면 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 19:21"
        },
        {
          "id": "MDRW2100042002.1.10",
          "form": "선선해지면 이 맛이 안나기때문에 ㅋㅋ",
          "original_form": "선선해지면 이 맛이 안나기때문에 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:22"
        },
        {
          "id": "MDRW2100042002.1.11",
          "form": "시원한건 날이 추우면 맛이 없어요",
          "original_form": "시원한건 날이 추우면 맛이 없어요",
          "speaker_id": "2",
          "time": "20210906 19:22"
        },
        {
          "id": "MDRW2100042002.1.12",
          "form": "커피는 빼고요 ㅋㅋㅋ",
          "original_form": "커피는 빼고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:22"
