      "utterance": [
        {
          "id": "MDRW2100016983.1.1",
          "form": "아 여행 좋아하나요",
          "original_form": "아 여행 좋아하나요",
          "speaker_id": "2",
          "time": "20210826 16:40"
        },
        {
          "id": "MDRW2100016983.1.2",
          "form": "바다여행 해외여행 좋아해요",
          "original_form": "바다여행 해외여행 좋아해요",
          "speaker_id": "1",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100016983.1.3",
          "form": "올해 바다에 갔던가요",
          "original_form": "올해 바다에 갔던가요",
          "speaker_id": "2",
          "time": "20210826 16:41"
        },
        {
          "id": "MDRW2100016983.1.4",
          "form": "엄마랑 당일로 보고왔어요",
          "original_form": "엄마랑 당일로 보고왔어요",
          "speaker_id": "1",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016983.1.5",
          "form": "만리포 다녀왔는데 좋더라고요",
          "original_form": "만리포 다녀왔는데 좋더라고요",
          "speaker_id": "1",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016983.1.6",
          "form": "좋겠다...",
          "original_form": "좋겠다...",
          "speaker_id": "2",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016983.1.7",
          "form": "온가족이?",
          "original_form": "온가족이?",
          "speaker_id": "2",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016983.1.8",
          "form": "ㄴㄴ 엄마랑 동생이랑",
          "original_form": "ㄴㄴ 엄마랑 동생이랑",
          "speaker_id": "1",
          "time": "20210826 16:42"
        },
        {
          "id": "MDRW2100016983.1.9",
          "form": "5인이하는 지켜야하니까",
          "original_form": "5인이하는 지켜야하니까",
          "speaker_id": "1",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100016983.1.10",
          "form": "ㅇㅎ",
          "original_form": "ㅇㅎ",
          "speaker_id": "2",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100016983.1.11",
          "form": "만리포에서 서핑교습 있던데요",
          "original_form": "만리포에서 서핑교습 있던데요",
          "speaker_id": "1",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100016983.1.12",
          "form": "와 나도 갈래",
          "original_form": "와 나도 갈래",
          "speaker_id": "2",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100016983.1.13",
          "form": "나중에 배워보고싶어요",
          "original_form": "나중에 배워보고싶어요",
          "speaker_id": "1",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100016983.1.14",
          "form": "수영부터 마스터 합시다",
          "original_form": "수영부터 마스터 합시다",
          "speaker_id": "2",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100016983.1.15",
          "form": "얕은 바다에서 서는거 서핑보드 위에 엎어져서 헤엄치는거 그런거 봤어요",
          "original_form": "얕은 바다에서 서는거 서핑보드 위에 엎어져서 헤엄치는거 그런거 봤어요",
          "speaker_id": "1",
          "time": "20210826 16:43"
        },
        {
          "id": "MDRW2100016983.1.16",
          "form": "그정도는 우리도 할 수 있지 않을까요",
          "original_form": "그정도는 우리도 할 수 있지 않을까요",
          "speaker_id": "2",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016983.1.17",
          "form": "전 못서겠던데요",
          "original_form": "전 못서겠던데요",
          "speaker_id": "1",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100016983.1.18",
          "form": "음 엎어져서 헤엄만요",
          "original_form": "음 엎어져서 헤엄만요",
          "speaker_id": "2",
          "time": "20210826 16:46"
        },
        {
          "id": "MDRW2100016983.1.19",
          "form": "갈때 헤엄치고 올때 서서오고",
          "original_form": "갈때 헤엄치고 올때 서서오고",
          "speaker_id": "1",
          "time": "20210826 16:47"
        }
      ]
