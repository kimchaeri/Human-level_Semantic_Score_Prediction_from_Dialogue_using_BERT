      "utterance": [
        {
          "id": "MDRW2100007469.1.1",
          "form": "티켓팅 이야기 말인데 유진과 유진 공연에 안예은 가수가 작곡으로 참여했잖아",
          "original_form": "티켓팅 이야기 말인데 유진과 유진 공연에 안예은 가수가 작곡으로 참여했잖아",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007469.1.2",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007469.1.3",
          "form": "그 사람 첫공에 전석 매진 나서 못 갔어",
          "original_form": "그 사람 첫공에 전석 매진 나서 못 갔어",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007469.1.4",
          "form": "정말웃긴다",
          "original_form": "정말웃긴다",
          "speaker_id": "1",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007469.1.5",
          "form": "자기가 참여한 뮤지컬에 자기가 못 갔어",
          "original_form": "자기가 참여한 뮤지컬에 자기가 못 갔어",
          "speaker_id": "2",
          "time": "20210822 02:21"
        },
        {
          "id": "MDRW2100007469.1.6",
          "form": "티켓은 왜 항상 부족한걸까?",
          "original_form": "티켓은 왜 항상 부족한걸까?",
          "speaker_id": "2",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007469.1.7",
          "form": "원래 그런건 제작사에서 1역중블줘야하는게 아닌가?",
          "original_form": "원래 그런건 제작사에서 1역중블줘야하는게 아닌가?",
          "speaker_id": "1",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007469.1.8",
          "form": "모든 공연을 온라인으로 중계하면 이런 비극이 좀 줄어들텐데",
          "original_form": "모든 공연을 온라인으로 중계하면 이런 비극이 좀 줄어들텐데",
          "speaker_id": "1",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007469.1.9",
          "form": "저번에 브라더스 까라마조프 중계 봤어?",
          "original_form": "저번에 브라더스 까라마조프 중계 봤어?",
          "speaker_id": "2",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007469.1.10",
          "form": "미안",
          "original_form": "미안",
          "speaker_id": "1",
          "time": "20210822 02:22"
        },
        {
          "id": "MDRW2100007469.1.11",
          "form": "모든 사람이 소소티켓 할인 적용해서 봤다고 해도 1억이던데",
          "original_form": "모든 사람이 소소티켓 할인 적용해서 봤다고 해도 1억이던데",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007469.1.12",
          "form": "9000명이 봤대",
          "original_form": "9000명이 봤대",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007469.1.13",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "1",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007469.1.14",
          "form": "소소티켓 적용하면",
          "original_form": "소소티켓 적용하면",
          "speaker_id": "1",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007469.1.15",
          "form": "얼만디?",
          "original_form": "얼만디?",
          "speaker_id": "1",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007469.1.16",
          "form": "12000원인가",
          "original_form": "12000원인가",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007469.1.17",
          "form": "그랬던 것 같은데",
          "original_form": "그랬던 것 같은데",
          "speaker_id": "2",
          "time": "20210822 02:23"
        },
        {
          "id": "MDRW2100007469.1.18",
          "form": "꽤나",
          "original_form": "꽤나",
          "speaker_id": "1",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007469.1.19",
          "form": "배부르겠는걸",
          "original_form": "배부르겠는걸",
          "speaker_id": "1",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007469.1.20",
          "form": "온라인 중계도 돈이 된다는걸 보여줬으니",
          "original_form": "온라인 중계도 돈이 된다는걸 보여줬으니",
          "speaker_id": "2",
          "time": "20210822 02:24"
        },
        {
          "id": "MDRW2100007469.1.21",
          "form": "조금 더 활성화되기를 기대해도 좋지 않을까",
          "original_form": "조금 더 활성화되기를 기대해도 좋지 않을까",
          "speaker_id": "2",
          "time": "20210822 02:24"
        }
      ]
