        {
          "id": "MDRW2100000854.1.1",
          "form": "요즘 건강식품 드시는거  있나요?",
          "original_form": "요즘 건강식품 드시는거  있나요?",
          "speaker_id": "2",
          "time": "20210819 20:06"
        },
        {
          "id": "MDRW2100000854.1.2",
          "form": "락토핏이요",
          "original_form": "락토핏이요",
          "speaker_id": "1",
          "time": "20210819 20:11"
        },
        {
          "id": "MDRW2100000854.1.3",
          "form": "유산균이요? 주황색은 먹어봤어요",
          "original_form": "유산균이요? 주황색은 먹어봤어요",
          "speaker_id": "2",
          "time": "20210819 20:12"
        },
        {
          "id": "MDRW2100000854.1.4",
          "form": "전 노란색",
          "original_form": "전 노란색",
          "speaker_id": "1",
          "time": "20210819 20:13"
        },
        {
          "id": "MDRW2100000854.1.5",
          "form": "무슨 차이가 있나요?",
          "original_form": "무슨 차이가 있나요?",
          "speaker_id": "2",
          "time": "20210819 20:14"
        },
        {
          "id": "MDRW2100000854.1.6",
          "form": "저도 잘  ㅎㅎ",
          "original_form": "저도 잘  ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000854.1.7",
          "form": "와이프가 걍 노란색 사던데요",
          "original_form": "와이프가 걍 노란색 사던데요",
          "speaker_id": "1",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000854.1.8",
          "form": "그렇군요ㅎ 아내분이 주시는거 드셔야죠ㅋ",
          "original_form": "그렇군요ㅎ 아내분이 주시는거 드셔야죠ㅋ",
          "speaker_id": "2",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000854.1.9",
          "form": "네 주는대로 먹어애죠",
          "original_form": "네 주는대로 먹어애죠",
          "speaker_id": "1",
          "time": "20210819 20:17"
        },
        {
          "id": "MDRW2100000854.1.10",
          "form": "ㅎㅎ 코로나인데 운동은 따로하시는거 있으세요?",
          "original_form": "ㅎㅎ 코로나인데 운동은 따로하시는거 있으세요?",
          "speaker_id": "2",
          "time": "20210819 20:19"
        },
        {
          "id": "MDRW2100000854.1.11",
          "form": "홈트요",
          "original_form": "홈트요",
          "speaker_id": "1",
          "time": "20210819 20:21"
        },
        {
          "id": "MDRW2100000854.1.12",
          "form": "홈트도 여러가진데 뭐하시나요",
          "original_form": "홈트도 여러가진데 뭐하시나요",
          "speaker_id": "2",
          "time": "20210819 20:23"
        },
        {
          "id": "MDRW2100000854.1.13",
          "form": "플랭크랑 팔굽혀펴기만해요",
          "original_form": "플랭크랑 팔굽혀펴기만해요",
          "speaker_id": "1",
          "time": "20210819 20:24"
        },
        {
          "id": "MDRW2100000854.1.14",
          "form": "플랭크 힘들다던데 몇분버티세요?ㅋ",
