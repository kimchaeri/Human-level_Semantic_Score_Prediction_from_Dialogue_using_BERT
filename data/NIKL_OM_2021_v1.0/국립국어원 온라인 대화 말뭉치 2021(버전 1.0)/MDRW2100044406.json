      "utterance": [
        {
          "id": "MDRW2100044406.1.1",
          "form": "날씨 어때요",
          "original_form": "날씨 어때요",
          "speaker_id": "2",
          "time": "20210907 22:33"
        },
        {
          "id": "MDRW2100044406.1.2",
          "form": "날씨 지금 비오네요",
          "original_form": "날씨 지금 비오네요",
          "speaker_id": "1",
          "time": "20210907 22:34"
        },
        {
          "id": "MDRW2100044406.1.3",
          "form": "그쵸 ㅠ 흐려요",
          "original_form": "그쵸 ㅠ 흐려요",
          "speaker_id": "2",
          "time": "20210907 22:35"
        },
        {
          "id": "MDRW2100044406.1.4",
          "form": "그러게요 비그만 왔으면 좋겠네요",
          "original_form": "그러게요 비그만 왔으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210907 22:37"
        },
        {
          "id": "MDRW2100044406.1.5",
          "form": "날좋을때 남해나 제주도 가고파요 ㅠ",
          "original_form": "날좋을때 남해나 제주도 가고파요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 22:38"
        },
        {
          "id": "MDRW2100044406.1.6",
          "form": "오 맞아요 ㅜㅜ",
          "original_form": "오 맞아요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044406.1.7",
          "form": "어떤 계절 제일 좋아하세요?",
          "original_form": "어떤 계절 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 22:40"
        },
        {
          "id": "MDRW2100044406.1.8",
          "form": "저는 가을 겨울이요",
          "original_form": "저는 가을 겨울이요",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044406.1.9",
          "form": "님은 ?",
          "original_form": "님은 ?",
          "speaker_id": "2",
          "time": "20210907 22:41"
        },
        {
          "id": "MDRW2100044406.1.10",
          "form": "저도 가을 좋아해요 ㅎㅎ",
          "original_form": "저도 가을 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044406.1.11",
          "form": "날씨가 좋아요",
          "original_form": "날씨가 좋아요",
          "speaker_id": "1",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044406.1.12",
          "form": "겨울은요?",
          "original_form": "겨울은요?",
          "speaker_id": "2",
          "time": "20210907 22:42"
        },
        {
          "id": "MDRW2100044406.1.13",
          "form": "겨울은 눈오는걸 별로 안좋아해서요..!",
          "original_form": "겨울은 눈오는걸 별로 안좋아해서요..!",
          "speaker_id": "1",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044406.1.14",
          "form": "보는것도 안좋아하세여?",
          "original_form": "보는것도 안좋아하세여?",
          "speaker_id": "2",
          "time": "20210907 22:44"
        },
        {
          "id": "MDRW2100044406.1.15",
          "form": "아 보는 건 좋아해요 ㅎㅎ",
          "original_form": "아 보는 건 좋아해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044406.1.16",
          "form": "ㅋㅋㅋ저도요",
          "original_form": "ㅋㅋㅋ저도요",
          "speaker_id": "2",
          "time": "20210907 22:46"
        },
        {
          "id": "MDRW2100044406.1.17",
          "form": "저도 눈 엄청 무서워해요",
