        },
        {
          "id": "MDRW2100046483.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211025 17:39"
        },
        {
          "id": "MDRW2100046483.1.3",
          "form": "혹시 mbti 테스트 아시나요",
          "original_form": "혹시 mbti 테스트 아시나요",
          "speaker_id": "2",
          "time": "20211025 17:39"
        },
        {
          "id": "MDRW2100046483.1.4",
          "form": "들어는 봤습니다",
          "original_form": "들어는 봤습니다",
          "speaker_id": "1",
          "time": "20211025 17:39"
        },
        {
          "id": "MDRW2100046483.1.5",
          "form": "아하 해보시진 않으셨구나",
          "original_form": "아하 해보시진 않으셨구나",
          "speaker_id": "2",
          "time": "20211025 17:39"
        },
        {
          "id": "MDRW2100046483.1.6",
          "form": "네 해보지는 않았습니다",
          "original_form": "네 해보지는 않았습니다",
          "speaker_id": "1",
          "time": "20211025 17:40"
        },
        {
          "id": "MDRW2100046483.1.7",
          "form": "그 테스트가 좀 귀찮기는 하죠 ㅋㅋㅋㅋ",
          "original_form": "그 테스트가 좀 귀찮기는 하죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211025 17:40"
        },
        {
          "id": "MDRW2100046483.1.8",
          "form": "해보셨나요?",
          "original_form": "해보셨나요?",
          "speaker_id": "1",
          "time": "20211025 17:40"
        },
        {
          "id": "MDRW2100046483.1.9",
          "form": "네 저는 영어 버전으로도 ",
          "original_form": "네 저는 영어 버전으로도 ",
          "speaker_id": "2",
          "time": "20211025 17:40"
        },
        {
          "id": "MDRW2100046483.1.10",
          "form": "해봤어요",
          "original_form": "해봤어요",
          "speaker_id": "2",
          "time": "20211025 17:40"
        },
        {
          "id": "MDRW2100046483.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211025 17:40"
        },
        {
          "id": "MDRW2100046483.1.12",
          "form": "우와..결과가 궁금하군요",
          "original_form": "우와..결과가 궁금하군요",
          "speaker_id": "1",
          "time": "20211025 17:40"
        },
        {
          "id": "MDRW2100046483.1.13",
          "form": "한글로 변역되면서 질문이 좀 이상해지는게 많아서",
          "original_form": "한글로 변역되면서 질문이 좀 이상해지는게 많아서",
          "speaker_id": "2",
          "time": "20211025 17:40"
        },
        {
          "id": "MDRW2100046483.1.14",
          "form": "영어로 보는게 제일 문맥이 이해가 잘된다고해서",
          "original_form": "영어로 보는게 제일 문맥이 이해가 잘된다고해서",
          "speaker_id": "2",
          "time": "20211025 17:40"
        },
        {
          "id": "MDRW2100046483.1.15",
          "form": "그걸로해봤는데",
          "original_form": "그걸로해봤는데",
          "speaker_id": "2",
          "time": "20211025 17:41"
        },
        {
          "id": "MDRW2100046483.1.16",
          "form": "똑같아요",
          "original_form": "똑같아요",
          "speaker_id": "2",
          "time": "20211025 17:41"
        },
        {
          "id": "MDRW2100046483.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211025 17:41"
        },
        {
          "id": "MDRW2100046483.1.18",
          "form": "결과가..ㅎㅎ",
          "original_form": "결과가..ㅎㅎ",
          "speaker_id": "2",
          "time": "20211025 17:41"
        },
        {
          "id": "MDRW2100046483.1.19",
          "form": "저는 enfp에용",
          "original_form": "저는 enfp에용",
          "speaker_id": "2",
          "time": "20211025 17:41"
        },
        {
          "id": "MDRW2100046483.1.20",
