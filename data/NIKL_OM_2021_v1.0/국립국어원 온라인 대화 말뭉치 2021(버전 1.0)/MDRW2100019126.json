      "utterance": [
        {
          "id": "MDRW2100019126.1.1",
          "form": "방금 트레이더스 배송 왔는데 키위 대박",
          "original_form": "방금 트레이더스 배송 왔는데 키위 대박",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019126.1.2",
          "form": "왜? 문제 있어?",
          "original_form": "왜? 문제 있어?",
          "speaker_id": "1",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100019126.1.3",
          "form": "아니 ㅎㅎ 크기가 주먹만해",
          "original_form": "아니 ㅎㅎ 크기가 주먹만해",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019126.1.4",
          "form": "뭐야 ㅎㅎ 깜짝 놀랐잖아",
          "original_form": "뭐야 ㅎㅎ 깜짝 놀랐잖아",
          "speaker_id": "1",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019126.1.5",
          "form": "진짜 대박이라니깐~ 이렇게 큰 키위 처음 봐 ㅋㅋ",
          "original_form": "진짜 대박이라니깐~ 이렇게 큰 키위 처음 봐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:29"
        },
        {
          "id": "MDRW2100019126.1.6",
          "form": "ㅎㅎ 집에 가서 한 번 봐야겠네 얼마나 크길래",
          "original_form": "ㅎㅎ 집에 가서 한 번 봐야겠네 얼마나 크길래",
          "speaker_id": "1",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019126.1.7",
          "form": "어차피 껍질 까야 되니 큰 게 좋긴한데 너무 신기해서 ㅎㅎ",
          "original_form": "어차피 껍질 까야 되니 큰 게 좋긴한데 너무 신기해서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019126.1.8",
          "form": "ㅎㅎ 맛은 어때?",
          "original_form": "ㅎㅎ 맛은 어때?",
          "speaker_id": "1",
          "time": "20210827 15:31"
        },
        {
          "id": "MDRW2100019126.1.9",
          "form": "완전 맛있어 ㅎㅎ",
          "original_form": "완전 맛있어 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019126.1.10",
          "form": "트레이더스 물건이 좋은가 보네",
          "original_form": "트레이더스 물건이 좋은가 보네",
          "speaker_id": "1",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019126.1.11",
          "form": "양도 많고 질도 좋고 짱이네",
          "original_form": "양도 많고 질도 좋고 짱이네",
          "speaker_id": "2",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019126.1.12",
          "form": "애들 간식으로 먹었어?",
          "original_form": "애들 간식으로 먹었어?",
          "speaker_id": "1",
          "time": "20210827 15:32"
        },
        {
          "id": "MDRW2100019126.1.13",
          "form": "ㅇㅇ 큰 아들이 키위 엄청 좋아하니깐 ㅎㅎ",
          "original_form": "ㅇㅇ 큰 아들이 키위 엄청 좋아하니깐 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 15:33"
        },
        {
          "id": "MDRW2100019126.1.14",
          "form": "트레이더스는 바나나도 아주 신선하네",
          "original_form": "트레이더스는 바나나도 아주 신선하네",
