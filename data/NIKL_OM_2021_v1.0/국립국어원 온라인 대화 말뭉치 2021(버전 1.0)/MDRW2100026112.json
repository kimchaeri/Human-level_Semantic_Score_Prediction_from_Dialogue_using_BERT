      "utterance": [
        {
          "id": "MDRW2100026112.1.1",
          "form": "난 고양이...?",
          "original_form": "난 고양이...?",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100026112.1.2",
          "form": "사실 강아지을 더 좋아하긴 한데",
          "original_form": "사실 강아지을 더 좋아하긴 한데",
          "speaker_id": "2",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100026112.1.3",
          "form": "나도 고양이!",
          "original_form": "나도 고양이!",
          "speaker_id": "1",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100026112.1.4",
          "form": "강아지 귀엽고 너무 사랑스러운데",
          "original_form": "강아지 귀엽고 너무 사랑스러운데",
          "speaker_id": "1",
          "time": "20210901 19:49"
        },
        {
          "id": "MDRW2100026112.1.5",
          "form": "고양이의 도도한 매력? 가끔가다 개냥이 모먼트가 나무 좋아졌어",
          "original_form": "고양이의 도도한 매력? 가끔가다 개냥이 모먼트가 나무 좋아졌어",
          "speaker_id": "1",
          "time": "20210901 19:50"
        },
        {
          "id": "MDRW2100026112.1.6",
          "form": "음음 뭔지 알 거 같아",
          "original_form": "음음 뭔지 알 거 같아",
          "speaker_id": "2",
          "time": "20210901 19:50"
        },
        {
          "id": "MDRW2100026112.1.7",
          "form": "고양이가 나랑 완전 친하다면 사실 고양이도 좋을 거같아",
          "original_form": "고양이가 나랑 완전 친하다면 사실 고양이도 좋을 거같아",
          "speaker_id": "2",
          "time": "20210901 19:50"
        },
        {
          "id": "MDRW2100026112.1.8",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210901 19:51"
        },
        {
          "id": "MDRW2100026112.1.9",
          "form": "막 좋다고 머리 들이 미는게 너무 귀엽더라고",
          "original_form": "막 좋다고 머리 들이 미는게 너무 귀엽더라고",
          "speaker_id": "2",
          "time": "20210901 19:51"
        },
        {
          "id": "MDRW2100026112.1.10",
          "form": "머리 박치기..",
          "original_form": "머리 박치기..",
          "speaker_id": "2",
          "time": "20210901 19:51"
        },
        {
          "id": "MDRW2100026112.1.11",
          "form": "나한테 막 다가오는 고양이",
          "original_form": "나한테 막 다가오는 고양이",
          "speaker_id": "1",
          "time": "20210901 19:51"
        },
        {
          "id": "MDRW2100026112.1.12",
          "form": "꺄ㅠㅠㅠㅠㅠㅠ",
          "original_form": "꺄ㅠㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 19:51"
        },
        {
          "id": "MDRW2100026112.1.13",
          "form": "그르렁 소리내는 것도 너무 귀여워",
          "original_form": "그르렁 소리내는 것도 너무 귀여워",
          "speaker_id": "1",
          "time": "20210901 19:51"
        },
        {
          "id": "MDRW2100026112.1.14",
          "form": "맞아... 근데 그런 고양이가 많지 않아서",
          "original_form": "맞아... 근데 그런 고양이가 많지 않아서",
          "speaker_id": "2",
          "time": "20210901 19:51"
        },
        {
          "id": "MDRW2100026112.1.15",
          "form": "물론 무신경해도 고양이라 좋지만 ㅋㅋ",
          "original_form": "물론 무신경해도 고양이라 좋지만 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:52"
        },
        {
          "id": "MDRW2100026112.1.16",
          "form": "고양이 지체가 매력이지",
          "original_form": "고양이 지체가 매력이지",
          "speaker_id": "1",
          "time": "20210901 19:52"
        },
        {
          "id": "MDRW2100026112.1.17",
          "form": "넌 고양이 어떤 종류 좋아해?",
          "original_form": "넌 고양이 어떤 종류 좋아해?",
          "speaker_id": "1",
          "time": "20210901 19:52"
        },
        {
          "id": "MDRW2100026112.1.18",
          "form": "난 랙돌이랑 페르시안이 예쁘더라구",
          "original_form": "난 랙돌이랑 페르시안이 예쁘더라구",
          "speaker_id": "2",
          "time": "20210901 19:54"
        },
        {
          "id": "MDRW2100026112.1.19",
          "form": "사실 그냥 고양이는 다 좋지만...",
          "original_form": "사실 그냥 고양이는 다 좋지만...",
          "speaker_id": "2",
          "time": "20210901 19:55"
        },
        {
          "id": "MDRW2100026112.1.20",
          "form": "굳이 품종을 고르자면?",
          "original_form": "굳이 품종을 고르자면?",
          "speaker_id": "2",
          "time": "20210901 19:55"
        },
        {
          "id": "MDRW2100026112.1.21",
          "form": "삼색이도 예뻐",
          "original_form": "삼색이도 예뻐",
          "speaker_id": "2",
          "time": "20210901 19:55"
        },
        {
          "id": "MDRW2100026112.1.22",
          "form": "러시안블루도 좋고",
          "original_form": "러시안블루도 좋고",
          "speaker_id": "2",
          "time": "20210901 19:55"
        },
        {
          "id": "MDRW2100026112.1.23",
          "form": "샴도 좋아",
          "original_form": "샴도 좋아",
          "speaker_id": "2",
          "time": "20210901 19:56"
        },
        {
          "id": "MDRW2100026112.1.24",
          "form": "와.. 어쩜 나랑 똑같니",
          "original_form": "와.. 어쩜 나랑 똑같니",
          "speaker_id": "1",
          "time": "20210901 19:56"
        },
        {
          "id": "MDRW2100026112.1.25",
          "form": "삼색이 진짜 너무 사랑하고",
          "original_form": "삼색이 진짜 너무 사랑하고",
          "speaker_id": "1",
          "time": "20210901 19:56"
        },
        {
          "id": "MDRW2100026112.1.26",
          "form": "러시안 블루 내 맘속에 원픽",
          "original_form": "러시안 블루 내 맘속에 원픽",
          "speaker_id": "1",
          "time": "20210901 19:56"
        },
        {
          "id": "MDRW2100026112.1.27",
          "form": "샴 부끄둥이 너무 귀여워ㅠㅠㅠ",
          "original_form": "샴 부끄둥이 너무 귀여워ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 19:56"
        },
        {
          "id": "MDRW2100026112.1.28",
          "form": "페르시안 우아함속에 치대는 애기들 보니까 그렇게 귀여울 수가 없더라ㅠㅠ",
          "original_form": "페르시안 우아함속에 치대는 애기들 보니까 그렇게 귀여울 수가 없더라ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 19:57"
        },
        {
          "id": "MDRW2100026112.1.29",
          "form": "맞아.. 아 보니까 내가 페르시안이랑 노르웨이 숲을 헷갈렸나봐",
          "original_form": "맞아.. 아 보니까 내가 페르시안이랑 노르웨이 숲을 헷갈렸나봐",
          "speaker_id": "2",
          "time": "20210901 19:57"
        },
        {
          "id": "MDRW2100026112.1.30",
          "form": "페르시안도 좋은데 노르웨이 숲이 예쁘게 생겼어",
          "original_form": "페르시안도 좋은데 노르웨이 숲이 예쁘게 생겼어",
          "speaker_id": "2",
          "time": "20210901 19:58"
        },
        {
          "id": "MDRW2100026112.1.31",
          "form": "메인쿤 알아? 완전 큰 고양이 ㅋㅋ",
          "original_form": "메인쿤 알아? 완전 큰 고양이 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:58"
        },
        {
          "id": "MDRW2100026112.1.32",
          "form": "와 노르웨이 숲 나 색이 너무 마음에 들었어",
          "original_form": "와 노르웨이 숲 나 색이 너무 마음에 들었어",
          "speaker_id": "1",
          "time": "20210901 19:58"
        },
        {
          "id": "MDRW2100026112.1.33",
          "form": "알지ㅋㅋㅋ",
          "original_form": "알지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:58"
        },
        {
          "id": "MDRW2100026112.1.34",
          "form": "진짜 크더라고",
          "original_form": "진짜 크더라고",
          "speaker_id": "2",
          "time": "20210901 19:58"
        },
        {
          "id": "MDRW2100026112.1.35",
          "form": "이름에 비해 산만한 고양이",
          "original_form": "이름에 비해 산만한 고양이",
          "speaker_id": "1",
          "time": "20210901 19:58"
        },
        {
          "id": "MDRW2100026112.1.36",
          "form": "산처람 큰",
          "original_form": "산처람 큰",
          "speaker_id": "1",
          "time": "20210901 19:59"
        },
        {
          "id": "MDRW2100026112.1.37",
          "form": "띄어쓰기의 중요성..",
          "original_form": "띄어쓰기의 중요성..",
          "speaker_id": "1",
          "time": "20210901 19:59"
        },
        {
          "id": "MDRW2100026112.1.38",
          "form": "ㅋㅋㅋㅋ 산만한 산 만한",
          "original_form": "ㅋㅋㅋㅋ 산만한 산 만한",
          "speaker_id": "2",
          "time": "20210901 19:59"
        }
      ]
    }
