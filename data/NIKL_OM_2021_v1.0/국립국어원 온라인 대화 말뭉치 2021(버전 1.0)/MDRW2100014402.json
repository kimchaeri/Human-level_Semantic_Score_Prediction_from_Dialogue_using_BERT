      "utterance": [
        {
          "id": "MDRW2100014402.1.1",
          "form": "가장 최근에 간 여행지가 어디예요?",
          "original_form": "가장 최근에 간 여행지가 어디예요?",
          "speaker_id": "1",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014402.1.2",
          "form": "음  ...딱 생각이안나네요ㅠㅠㅠ",
          "original_form": "음  ...딱 생각이안나네요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014402.1.3",
          "form": "여행간지 너무오래됐나봐요",
          "original_form": "여행간지 너무오래됐나봐요",
          "speaker_id": "2",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014402.1.4",
          "form": "휴가안다녀오셨나봐요",
          "original_form": "휴가안다녀오셨나봐요",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014402.1.5",
          "form": "요즘코로나때문에ㅠ",
          "original_form": "요즘코로나때문에ㅠ",
          "speaker_id": "1",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014402.1.6",
          "form": "네 이번에는 안갔었어요 ㅜㅜㅜ",
          "original_form": "네 이번에는 안갔었어요 ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014402.1.7",
          "form": "휴가 어디로 다녀오셨어요?",
          "original_form": "휴가 어디로 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210825 20:04"
        },
        {
          "id": "MDRW2100014402.1.8",
          "form": "저는 가평 다녀왔어요",
          "original_form": "저는 가평 다녀왔어요",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014402.1.9",
          "form": "계곡에잠깐들리구",
          "original_form": "계곡에잠깐들리구",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014402.1.10",
          "form": "와 요즘 가평 예쁜펜션 많더라구요",
          "original_form": "와 요즘 가평 예쁜펜션 많더라구요",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014402.1.11",
          "form": "힐링하고 오셨겠어요",
          "original_form": "힐링하고 오셨겠어요",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014402.1.12",
          "form": "거의 숙소에있긴했는데 그래도 재미 있었어요",
          "original_form": "거의 숙소에있긴했는데 그래도 재미 있었어요",
          "speaker_id": "1",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014402.1.13",
          "form": "요즘에는 여행가면 숙소를 넓고 좋은곳으로 가고싶은 마음이들더라구요",
          "original_form": "요즘에는 여행가면 숙소를 넓고 좋은곳으로 가고싶은 마음이들더라구요",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014402.1.14",
          "form": "여행지에 이동 제한이 있으니. .",
          "original_form": "여행지에 이동 제한이 있으니. .",
          "speaker_id": "1",
          "time": "20210825 20:09"
        },
        {
          "id": "MDRW2100014402.1.15",
          "form": "맞아요ㅜㅜ 해수욕장이나 워터파크도 가고싶은데 내년엔 갈수있을지 모르겠어요",
          "original_form": "맞아요ㅜㅜ 해수욕장이나 워터파크도 가고싶은데 내년엔 갈수있을지 모르겠어요",
          "speaker_id": "2",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014402.1.16",
          "form": "맞아요ㅠ 워터파크 가고싶네요",
          "original_form": "맞아요ㅠ 워터파크 가고싶네요",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014402.1.17",
          "form": "물에들어갈때 마스크를쓰라는데",
          "original_form": "물에들어갈때 마스크를쓰라는데",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014402.1.18",
          "form": "그게의미가있나싶어요",
          "original_form": "그게의미가있나싶어요",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014402.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ물에들어갈때요? 와...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ물에들어갈때요? 와...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014402.1.20",
          "form": "전국민 기인열전 나가야겠어요",
          "original_form": "전국민 기인열전 나가야겠어요",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014402.1.21",
          "form": "ㅋㅋㅋㄱㅋㅋ기",
          "original_form": "ㅋㅋㅋㄱㅋㅋ기",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014402.1.22",
          "form": "코로나끝나면 가고싶은 여행지 있으세요?",
          "original_form": "코로나끝나면 가고싶은 여행지 있으세요?",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014402.1.23",
          "form": "저는 외국아무데나가보고싶어요ㅜㅜㅜ외국나갈계획하다가",
          "original_form": "저는 외국아무데나가보고싶어요ㅜㅜㅜ외국나갈계획하다가",
          "speaker_id": "2",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014402.1.24",
          "form": "코로나가와서 결국 외국 한번도못나갔어요",
          "original_form": "코로나가와서 결국 외국 한번도못나갔어요",
          "speaker_id": "2",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014402.1.25",
          "form": "혹시 외국가보셨나요?",
          "original_form": "혹시 외국가보셨나요?",
          "speaker_id": "2",
          "time": "20210825 20:24"
        },
        {
          "id": "MDRW2100014402.1.26",
          "form": "아 그렇군요.  저도많이다닌것은아니지만",
          "original_form": "아 그렇군요.  저도많이다닌것은아니지만",
          "speaker_id": "1",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014402.1.27",
          "form": "일본중국 체코 호주 필리핀 베트남",
          "original_form": "일본중국 체코 호주 필리핀 베트남",
          "speaker_id": "1",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014402.1.28",
          "form": "이정도 갔어요ㅎㅎ",
          "original_form": "이정도 갔어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014402.1.29",
          "form": "와 많이다니셨어요!! 체코는 주변에",
          "original_form": "와 많이다니셨어요!! 체코는 주변에",
          "speaker_id": "2",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014402.1.30",
          "form": "갔다온사람이 없어서 어떠셨는지 더 궁금해요",
          "original_form": "갔다온사람이 없어서 어떠셨는지 더 궁금해요",
          "speaker_id": "2",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014402.1.31",
          "form": "프라하다녀왔는데 도시가크지않아서 더 좋은것같아요",
          "original_form": "프라하다녀왔는데 도시가크지않아서 더 좋은것같아요",
          "speaker_id": "1",
          "time": "20210825 20:33"
        },
        {
          "id": "MDRW2100014402.1.32",
          "form": "일주일있었는데 동네처럼 지내보는게 목표였거든요.",
          "original_form": "일주일있었는데 동네처럼 지내보는게 목표였거든요.",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014402.1.33",
          "form": "와....프라하는 프라하의 연인보면서 로망을 키웠었는데ㅜㅜ",
          "original_form": "와....프라하는 프라하의 연인보면서 로망을 키웠었는데ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014402.1.34",
          "form": "애긔야 가자!!",
          "original_form": "애긔야 가자!!",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014402.1.35",
          "form": "헉ㅋㅋㅋㅋㅋ",
          "original_form": "헉ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014402.1.36",
          "form": "일주일",
          "original_form": "일주일",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014402.1.37",
          "form": "ㅋㅋㅋ앜ㅋㅋ",
          "original_form": "ㅋㅋㅋ앜ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014402.1.38",
          "form": "그것을안다면ㅋㄱㅋㅋ",
          "original_form": "그것을안다면ㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:34"
        },
        {
          "id": "MDRW2100014402.1.39",
          "form": "일주일이면 일상같아질즈음 떠나셔서 아쉬으셨겠어요ㅜㅜ",
          "original_form": "일주일이면 일상같아질즈음 떠나셔서 아쉬으셨겠어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014402.1.40",
          "form": "저도 프라하연인 보고 가보고싶었거든요",
          "original_form": "저도 프라하연인 보고 가보고싶었거든요",
          "speaker_id": "1",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014402.1.41",
          "form": "ㅋㅋㄱ동년배인가요",
          "original_form": "ㅋㅋㄱ동년배인가요",
          "speaker_id": "2",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014402.1.42",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014402.1.43",
          "form": "ㅋㅋ 그 까를교를 직접가봤죠",
          "original_form": "ㅋㅋ 그 까를교를 직접가봤죠",
          "speaker_id": "1",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014402.1.44",
          "form": "와.... 멋지다ㅜㅜ",
          "original_form": "와.... 멋지다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014402.1.45",
          "form": "코로나끝나구 기회되면 가보셔요!",
          "original_form": "코로나끝나구 기회되면 가보셔요!",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014402.1.46",
          "form": "네 감사합니다!!\r\n그런데 일주일씩 가시면",
          "original_form": "네 감사합니다!!\r\n그런데 일주일씩 가시면",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014402.1.47",
          "form": "숙소는 어떻게 잡으셨는지 궁금해요!",
          "original_form": "숙소는 어떻게 잡으셨는지 궁금해요!",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014402.1.48",
          "form": "호텔이나 에어비엔비로 가셨나요?",
          "original_form": "호텔이나 에어비엔비로 가셨나요?",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014402.1.49",
          "form": "동네처럼 지낸다는게 너무 낭만적이네요",
          "original_form": "동네처럼 지낸다는게 너무 낭만적이네요",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014402.1.50",
          "form": "호텔예약했어요",
          "original_form": "호텔예약했어요",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014402.1.51",
          "form": "ㅎㅎ 걸어다니면서보이는 건물들도 이쁘구",
          "original_form": "ㅎㅎ 걸어다니면서보이는 건물들도 이쁘구",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014402.1.52",
          "form": "맥주도 뭔가 진하고 맛있고",
          "original_form": "맥주도 뭔가 진하고 맛있고",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014402.1.53",
          "form": "대화하니 더 생각나네요",
          "original_form": "대화하니 더 생각나네요",
          "speaker_id": "1",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014402.1.54",
          "form": "우와아ㅜㅜㅜ 야경도 되게예뻤을거같아요",
          "original_form": "우와아ㅜㅜㅜ 야경도 되게예뻤을거같아요",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014402.1.55",
          "form": "얼른 코로나 끝나고 또가셨으면...!!",
          "original_form": "얼른 코로나 끝나고 또가셨으면...!!",
          "speaker_id": "2",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014402.1.56",
          "form": "체코도 가고싶은 여행지에 체크해둬야겠어요",
          "original_form": "체코도 가고싶은 여행지에 체크해둬야겠어요",
          "speaker_id": "2",
          "time": "20210825 20:41"
