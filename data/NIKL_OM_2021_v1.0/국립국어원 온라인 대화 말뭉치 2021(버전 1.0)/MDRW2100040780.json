      "utterance": [
        {
          "id": "MDRW2100040780.1.1",
          "form": "날씨좋다",
          "original_form": "날씨좋다",
          "speaker_id": "2",
          "time": "20210905 09:35"
        },
        {
          "id": "MDRW2100040780.1.2",
          "form": "주말도 옴총 일찍일어나는 구먼ㅋ",
          "original_form": "주말도 옴총 일찍일어나는 구먼ㅋ",
          "speaker_id": "2",
          "time": "20210905 09:35"
        },
        {
          "id": "MDRW2100040780.1.3",
          "form": "이제 아침에도 쌀랑 하네여~",
          "original_form": "이제 아침에도 쌀랑 하네여~",
          "speaker_id": "1",
          "time": "20210906 08:59"
        },
        {
          "id": "MDRW2100040780.1.4",
          "form": "못일어나겠어요 ㅋㅋㅋㅋ",
          "original_form": "못일어나겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 08:59"
        },
        {
          "id": "MDRW2100040780.1.5",
          "form": "시원하니 좋네",
          "original_form": "시원하니 좋네",
          "speaker_id": "2",
          "time": "20210906 09:33"
        },
        {
          "id": "MDRW2100040780.1.6",
          "form": "일하러가겠네ㅋㅋ",
          "original_form": "일하러가겠네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 09:33"
        },
        {
          "id": "MDRW2100040780.1.7",
          "form": "아이고 언니 박스나 르다 보니 땀에 쩔었음",
          "original_form": "아이고 언니 박스나 르다 보니 땀에 쩔었음",
          "speaker_id": "1",
          "time": "20210906 12:34"
        },
        {
          "id": "MDRW2100040780.1.8",
          "form": "빨리 예전과 같은 평화가 오기를ㅋㅋㅋ",
          "original_form": "빨리 예전과 같은 평화가 오기를ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:34"
        },
        {
          "id": "MDRW2100040780.1.9",
          "form": "내일바쁘나?",
          "original_form": "내일바쁘나?",
          "speaker_id": "2",
          "time": "20210906 12:48"
        },
        {
          "id": "MDRW2100040780.1.10",
          "form": "나는 한번씩  아가방 가께ㅋㅋ",
          "original_form": "나는 한번씩  아가방 가께ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 12:48"
        },
        {
          "id": "MDRW2100040780.1.11",
          "form": "움직이니 덥다",
          "original_form": "움직이니 덥다",
          "speaker_id": "2",
          "time": "20210906 12:48"
        },
        {
          "id": "MDRW2100040780.1.12",
          "form": "요번 주는  바쁠 듯요ㅠ",
          "original_form": "요번 주는  바쁠 듯요ㅠ",
          "speaker_id": "1",
          "time": "20210906 14:12"
        },
        {
          "id": "MDRW2100040780.1.13",
          "form": "날씨가 갑자기 추워져서",
          "original_form": "날씨가 갑자기 추워져서",
          "speaker_id": "1",
          "time": "20210906 14:12"
        },
        {
          "id": "MDRW2100040780.1.14",
          "form": "손님이 많이 와서 일이 자꾸 밀려요",
          "original_form": "손님이 많이 와서 일이 자꾸 밀려요",
          "speaker_id": "1",
          "time": "20210906 14:12"
        },
        {
          "id": "MDRW2100040780.1.15",
          "form": "아;;장사가잘되는구먼",
          "original_form": "아;;장사가잘되는구먼",
          "speaker_id": "2",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100040780.1.16",
          "form": "날씨영향을 많이받네ㅋ",
          "original_form": "날씨영향을 많이받네ㅋ",
          "speaker_id": "2",
          "time": "20210906 14:16"
        },
        {
          "id": "MDRW2100040780.1.17",
          "form": "그니까요 갑자기 뭐긴 거 지금 뭐 또 재고 없음",
          "original_form": "그니까요 갑자기 뭐긴 거 지금 뭐 또 재고 없음",
          "speaker_id": "1",
          "time": "20210906 14:16"
        },
        {
          "id": "MDRW2100040780.1.18",
          "form": "사이즈 다 빠지고 난립니다ㅋㅋㅋ",
          "original_form": "사이즈 다 빠지고 난립니다ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:16"
        },
        {
          "id": "MDRW2100040780.1.19",
          "form": "난 지금네일샵왔다",
          "original_form": "난 지금네일샵왔다",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100040780.1.20",
          "form": "놀러ㅋㅋ",
          "original_form": "놀러ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100040780.1.21",
          "form": "가을가을한 색 많네ㅋ",
          "original_form": "가을가을한 색 많네ㅋ",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100040780.1.22",
          "form": "집 좀 지키고있어요 ㅋㅋㅋㅋ",
          "original_form": "집 좀 지키고있어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100040780.1.23",
          "form": "애들 배웅도 좀 해주고ㅋㅋㅋ",
          "original_form": "애들 배웅도 좀 해주고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100040780.1.24",
          "form": "ㅋㅋ 애들배웅ㅋㅋ",
          "original_form": "ㅋㅋ 애들배웅ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100040780.1.25",
          "form": "생각해보께ㅋㅋ",
          "original_form": "생각해보께ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:30"
        },
        {
          "id": "MDRW2100040780.1.26",
          "form": "나도 가을 가을 가을 한 건 내일 받고 싶네요",
          "original_form": "나도 가을 가을 가을 한 건 내일 받고 싶네요",
          "speaker_id": "1",
          "time": "20210906 14:32"
        },
        {
          "id": "MDRW2100040780.1.27",
          "form": "나도 아직 손은안했는데",
          "original_form": "나도 아직 손은안했는데",
          "speaker_id": "2",
          "time": "20210906 15:33"
        },
        {
          "id": "MDRW2100040780.1.28",
          "form": "하고싶네",
          "original_form": "하고싶네",
          "speaker_id": "2",
          "time": "20210906 15:33"
        },
        {
          "id": "MDRW2100040780.1.29",
          "form": "신상색깔들보니",
          "original_form": "신상색깔들보니",
          "speaker_id": "2",
          "time": "20210906 15:33"
        }
      ]
    }
