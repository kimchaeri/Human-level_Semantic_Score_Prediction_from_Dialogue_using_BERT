      "utterance": [
        {
          "id": "MDRW2100017231.1.1",
          "form": "오 반려동물!",
          "original_form": "오 반려동물!",
          "speaker_id": "2",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100017231.1.2",
          "form": "동물좋아하세요?",
          "original_form": "동물좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100017231.1.3",
          "form": "키우세요?",
          "original_form": "키우세요?",
          "speaker_id": "2",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100017231.1.4",
          "form": "좋아하는 데 키우지는 않아요",
          "original_form": "좋아하는 데 키우지는 않아요",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017231.1.5",
          "form": "어 저 허스키한마리 키웁니다",
          "original_form": "어 저 허스키한마리 키웁니다",
          "speaker_id": "1",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017231.1.6",
          "form": "책임감이 ㅎㄷㄷ",
          "original_form": "책임감이 ㅎㄷㄷ",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017231.1.7",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017231.1.8",
          "form": "허스키좋아하시나요",
          "original_form": "허스키좋아하시나요",
          "speaker_id": "1",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017231.1.9",
          "form": "네네 ㅎㅎ",
          "original_form": "네네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017231.1.10",
          "form": "근데 커서 키울 생각은 못해봤어요",
          "original_form": "근데 커서 키울 생각은 못해봤어요",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017231.1.11",
          "form": "아무래도 대형견이다보니",
          "original_form": "아무래도 대형견이다보니",
          "speaker_id": "1",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017231.1.12",
          "form": "요즘 여름이라 많이 고생하겠어요",
          "original_form": "요즘 여름이라 많이 고생하겠어요",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017231.1.13",
          "form": "끝나가서 다행이네요",
          "original_form": "끝나가서 다행이네요",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017231.1.14",
          "form": "맞아요 에어컨 쭉틀어놔요 그래서",
          "original_form": "맞아요 에어컨 쭉틀어놔요 그래서",
          "speaker_id": "1",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017231.1.15",
          "form": "실외배변 시키세요?",
          "original_form": "실외배변 시키세요?",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017231.1.16",
          "form": "실외배변이 안되서",
          "original_form": "실외배변이 안되서",
          "speaker_id": "1",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100017231.1.17",
          "form": "에구 ㅠㅜ 애기가 고생하네",
          "original_form": "에구 ㅠㅜ 애기가 고생하네",
          "speaker_id": "2",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100017231.1.18",
          "form": "실내배변시켜오",
          "original_form": "실내배변시켜오",
          "speaker_id": "1",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100017231.1.19",
          "form": "에구 ㅠㅜ name1님도 고생하시네요",
          "original_form": "에구 ㅠㅜ &name1&님도 고생하시네요",
          "speaker_id": "2",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100017231.1.20",
          "form": "감수해야죠ㅎ",
          "original_form": "감수해야죠ㅎ",
          "speaker_id": "1",
          "time": "20210826 18:11"
        },
        {
          "id": "MDRW2100017231.1.21",
          "form": "그래도 가족이 있는 건 좋은 것 같아요",
