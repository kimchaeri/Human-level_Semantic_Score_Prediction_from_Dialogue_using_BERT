        {
          "id": "MDRW2100011475.1.1",
          "form": "결혼에 대해 어떻게 생각하세요?",
          "original_form": "결혼에 대해 어떻게 생각하세요?",
          "speaker_id": "1",
          "time": "20210824 12:57"
        },
        {
          "id": "MDRW2100011475.1.2",
          "form": "요즘은 선택의 분야라고 생각해요",
          "original_form": "요즘은 선택의 분야라고 생각해요",
          "speaker_id": "2",
          "time": "20210824 14:04"
        },
        {
          "id": "MDRW2100011475.1.3",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100011475.1.4",
          "form": "미팅해보셨나요?",
          "original_form": "미팅해보셨나요?",
          "speaker_id": "1",
          "time": "20210824 23:17"
        },
        {
          "id": "MDRW2100011475.1.5",
          "form": "아니요 누누님은요?",
          "original_form": "아니요 누누님은요?",
          "speaker_id": "2",
          "time": "20210824 23:20"
        },
        {
          "id": "MDRW2100011475.1.6",
          "form": "저도요ㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100011475.1.7",
          "form": "한번도요",
          "original_form": "한번도요",
          "speaker_id": "1",
          "time": "20210824 23:36"
        },
        {
          "id": "MDRW2100011475.1.8",
          "form": "자만추밖에 안해봐서 미팅생각하면 너무 어색할거 같아요",
          "original_form": "자만추밖에 안해봐서 미팅생각하면 너무 어색할거 같아요",
          "speaker_id": "2",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100011475.1.9",
          "form": "ㅋㅋㅋ 글고이제",
          "original_form": "ㅋㅋㅋ 글고이제",
          "speaker_id": "1",
          "time": "20210824 23:46"
        },
        {
          "id": "MDRW2100011475.1.10",
