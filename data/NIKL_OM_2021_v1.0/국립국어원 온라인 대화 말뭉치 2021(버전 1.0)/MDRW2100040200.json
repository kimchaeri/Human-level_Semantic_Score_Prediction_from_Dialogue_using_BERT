      "utterance": [
        {
          "id": "MDRW2100040200.1.1",
          "form": "저 오늘 맨투맨 하나 샀네요!",
          "original_form": "저 오늘 맨투맨 하나 샀네요!",
          "speaker_id": "1",
          "time": "20210905 19:59"
        },
        {
          "id": "MDRW2100040200.1.2",
          "form": "오 가을맞이 샤핑 하셨나요? ㅋㅋ",
          "original_form": "오 가을맞이 샤핑 하셨나요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 21:20"
        },
        {
          "id": "MDRW2100040200.1.3",
          "form": "네~  휴일에 휘뚜루마뚜루 입을 맨투맨 샀어요 펑퍼짐하게 ㅎㅎ",
          "original_form": "네~  휴일에 휘뚜루마뚜루 입을 맨투맨 샀어요 펑퍼짐하게 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 19:04"
        },
        {
          "id": "MDRW2100040200.1.4",
          "form": "오버핏인가요? ㅋㅋㅋ",
          "original_form": "오버핏인가요? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:08"
        },
        {
          "id": "MDRW2100040200.1.5",
          "form": "네 ㅎㅎㅎㅎㅎ 오버핏이 편하고 좋아요...ㅋㅋㅋㅋㅋㅋ 너무 거적대기마냥 클까봐걱정도되지만 ㅠㅠㅠㅠㅠㅋㅋ",
          "original_form": "네 ㅎㅎㅎㅎㅎ 오버핏이 편하고 좋아요...ㅋㅋㅋㅋㅋㅋ 너무 거적대기마냥 클까봐걱정도되지만 ㅠㅠㅠㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:09"
        },
        {
          "id": "MDRW2100040200.1.6",
          "form": "ㅋㅋ잘하셨네요:)",
          "original_form": "ㅋㅋ잘하셨네요:)",
          "speaker_id": "2",
          "time": "20210906 19:13"
        },
        {
          "id": "MDRW2100040200.1.7",
          "form": "색상은요? ㅋㅋㄲ",
          "original_form": "색상은요? ㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210906 19:13"
        },
        {
          "id": "MDRW2100040200.1.8",
          "form": "검정색요 ㅎㅎ 잘 산거 맞으려나....",
          "original_form": "검정색요 ㅎㅎ 잘 산거 맞으려나....",
          "speaker_id": "1",
          "time": "20210906 19:13"
        },
        {
          "id": "MDRW2100040200.1.9",
          "form": "무난하겠네요:)",
          "original_form": "무난하겠네요:)",
          "speaker_id": "2",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100040200.1.10",
          "form": "약기모 들어간거에용?",
          "original_form": "약기모 들어간거에용?",
          "speaker_id": "2",
          "time": "20210906 19:38"
        },
        {
          "id": "MDRW2100040200.1.11",
          "form": "딩동댕~~~~ ㅎㅎㅎㅎ 정답이에요!",
          "original_form": "딩동댕~~~~ ㅎㅎㅎㅎ 정답이에요!",
          "speaker_id": "1",
          "time": "20210906 19:48"
        },
        {
          "id": "MDRW2100040200.1.12",
          "form": "ㅋㅋ잘사셨네요 ",
          "original_form": "ㅋㅋ잘사셨네요 ",
          "speaker_id": "2",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100040200.1.13",
          "form": "직접 가서 고르셨어요?",
          "original_form": "직접 가서 고르셨어요?",
          "speaker_id": "2",
          "time": "20210906 19:49"
        },
        {
          "id": "MDRW2100040200.1.14",
          "form": "아뇨 ㅎㅎㅎ 인터넷요 요즘 뭔가 옷 장만 신발 장만하느라 할인쿠폰도 알아보고 역시즌도 알아보고 하다보니까 괜히 진빠져요 ㅋㅋ 그냥 정가주고 사는게 마음 편할수도 있겠어요",
          "original_form": "아뇨 ㅎㅎㅎ 인터넷요 요즘 뭔가 옷 장만 신발 장만하느라 할인쿠폰도 알아보고 역시즌도 알아보고 하다보니까 괜히 진빠져요 ㅋㅋ 그냥 정가주고 사는게 마음 편할수도 있겠어요",
          "speaker_id": "1",
          "time": "20210906 19:50"
        },
        {
          "id": "MDRW2100040200.1.15",
          "form": "ㅋㅋ싸게사는게 싸게사는게\r\n아니란 생각이 문득 들더라구요",
          "original_form": "ㅋㅋ싸게사는게 싸게사는게\r\n아니란 생각이 문득 들더라구요",
          "speaker_id": "2",
          "time": "20210906 19:51"
        },
        {
          "id": "MDRW2100040200.1.16",
          "form": "그걸 찾기위해 들이는 시간과\r\n노동을 생각하면 ㅋㅋ",
          "original_form": "그걸 찾기위해 들이는 시간과\r\n노동을 생각하면 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:52"
        },
        {
          "id": "MDRW2100040200.1.17",
          "form": "맞아요... 요 며칠 아주 징하게 깨달았어요 ㅋㅋㅋ",
          "original_form": "맞아요... 요 며칠 아주 징하게 깨달았어요 ㅋㅋㅋ",
