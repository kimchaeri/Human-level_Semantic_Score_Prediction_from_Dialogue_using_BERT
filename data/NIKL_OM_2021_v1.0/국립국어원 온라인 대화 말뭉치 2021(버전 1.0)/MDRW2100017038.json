      "utterance": [
        {
          "id": "MDRW2100017038.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 17:00"
        },
        {
          "id": "MDRW2100017038.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017038.1.3",
          "form": "점심은 드셧어요?",
          "original_form": "점심은 드셧어요?",
          "speaker_id": "1",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017038.1.4",
          "form": "네! 엽떡 먹었어요",
          "original_form": "네! 엽떡 먹었어요",
          "speaker_id": "2",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017038.1.5",
          "form": "name1님은 점심 뭐 드셨나요?",
          "original_form": "&name1&님은 점심 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210826 17:01"
        },
        {
          "id": "MDRW2100017038.1.6",
          "form": "저는 백반먹엇어요 ㅋㅋㅋㅋ",
          "original_form": "저는 백반먹엇어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:02"
        },
        {
          "id": "MDRW2100017038.1.7",
          "form": "매운거 잘 드세요?",
          "original_form": "매운거 잘 드세요?",
          "speaker_id": "1",
          "time": "20210826 17:03"
        },
        {
          "id": "MDRW2100017038.1.8",
          "form": "잘 먹는건 아니지만 좋아해요!",
          "original_form": "잘 먹는건 아니지만 좋아해요!",
          "speaker_id": "2",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017038.1.9",
          "form": "저랑 비슷하네요 ㅋㅋㅋㅋ저도 잘먹는건 아닌데 좋아해요",
          "original_form": "저랑 비슷하네요 ㅋㅋㅋㅋ저도 잘먹는건 아닌데 좋아해요",
          "speaker_id": "1",
          "time": "20210826 17:04"
        },
        {
          "id": "MDRW2100017038.1.10",
          "form": "저 가끔 울면서 먹을때 있어요...ㅎㅎ",
          "original_form": "저 가끔 울면서 먹을때 있어요...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100017038.1.11",
          "form": "저는 먹을때는 어찌어찌 먹고서 다음날 처절하게 후회를 ㅋㅋㅋㅋ",
          "original_form": "저는 먹을때는 어찌어찌 먹고서 다음날 처절하게 후회를 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100017038.1.12",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100017038.1.13",
          "form": "다음날은 괜찮은데",
          "original_form": "다음날은 괜찮은데",
          "speaker_id": "2",
          "time": "20210826 17:06"
        },
        {
          "id": "MDRW2100017038.1.14",
          "form": "진짜 매우면 울면서 먹어서 ㅎㅎㅎㅎ",
          "original_form": "진짜 매우면 울면서 먹어서 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100017038.1.15",
          "form": "그래도 다음날 괜찮으신거면 위장이 튼튼하신가봐요",
          "original_form": "그래도 다음날 괜찮으신거면 위장이 튼튼하신가봐요",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017038.1.16",
          "form": "저는 다음날 화장실에서 울어욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저는 다음날 화장실에서 울어욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017038.1.17",
          "form": "위장이 소화는 못시키지만",
          "original_form": "위장이 소화는 못시키지만",
          "speaker_id": "2",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017038.1.18",
          "form": "매운거에는 앵간 괜찮더라구요",
          "original_form": "매운거에는 앵간 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017038.1.19",
          "form": "너무 매운거는",
          "original_form": "너무 매운거는",
          "speaker_id": "2",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017038.1.20",
          "form": "속 뒤집어지고",
          "original_form": "속 뒤집어지고",
          "speaker_id": "2",
          "time": "20210826 17:10"
        },
        {
          "id": "MDRW2100017038.1.21",
          "form": "난리나지만요..ㅎ",
          "original_form": "난리나지만요..ㅎ",
          "speaker_id": "2",
          "time": "20210826 17:11"
        },
        {
          "id": "MDRW2100017038.1.22",
          "form": "오오 부럽다...",
          "original_form": "오오 부럽다...",
          "speaker_id": "1",
          "time": "20210826 17:12"
        },
        {
          "id": "MDRW2100017038.1.23",
          "form": "그럼 전체적으로 매운음식을 주로 드시는 편이세요?",
          "original_form": "그럼 전체적으로 매운음식을 주로 드시는 편이세요?",
          "speaker_id": "1",
          "time": "20210826 17:13"
        },
        {
          "id": "MDRW2100017038.1.24",
          "form": "매운음식 자주 먹지는 못해요...",
          "original_form": "매운음식 자주 먹지는 못해요...",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017038.1.25",
          "form": "저녁은 뭐드실거예요?",
          "original_form": "저녁은 뭐드실거예요?",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017038.1.26",
          "form": "저녁은 김치찌개요!",
          "original_form": "저녁은 김치찌개요!",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017038.1.27",
          "form": "김치찌개 맛잇겟네요 ㅋㅋㅋㅋ",
          "original_form": "김치찌개 맛잇겟네요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017038.1.28",
          "form": "참치인가요 돼지고기인가요",
          "original_form": "참치인가요 돼지고기인가요",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017038.1.29",
          "form": "돼지고기요",
          "original_form": "돼지고기요",
          "speaker_id": "2",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017038.1.30",
          "form": "두부도 들어갔어요",
          "original_form": "두부도 들어갔어요",
          "speaker_id": "2",
          "time": "20210826 17:26"
        },
        {
          "id": "MDRW2100017038.1.31",
          "form": "맛잇겟네요",
          "original_form": "맛잇겟네요",
          "speaker_id": "1",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017038.1.32",
          "form": "맛있을것 같아요",
          "original_form": "맛있을것 같아요",
          "speaker_id": "2",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017038.1.33",
          "form": "저녁이 기대가 됩니당",
          "original_form": "저녁이 기대가 됩니당",
          "speaker_id": "2",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017038.1.34",
          "form": "name1님은 저녁 뭐드실 예정이세요?",
          "original_form": "&name1&님은 저녁 뭐드실 예정이세요?",
          "speaker_id": "2",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017038.1.35",
          "form": "전 닭볶음탕 먹을 예정이에요",
          "original_form": "전 닭볶음탕 먹을 예정이에요",
          "speaker_id": "1",
          "time": "20210826 17:29"
        },
        {
          "id": "MDRW2100017038.1.36",
          "form": "오오 맛있겠다...",
          "original_form": "오오 맛있겠다...",
          "speaker_id": "2",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017038.1.37",
          "form": "침이 고여요...",
          "original_form": "침이 고여요...",
          "speaker_id": "2",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017038.1.38",
          "form": "맛있겠다... 닭볶음탕...",
          "original_form": "맛있겠다... 닭볶음탕...",
          "speaker_id": "2",
          "time": "20210826 17:30"
        },
        {
          "id": "MDRW2100017038.1.39",
          "form": "닭볶음탕 양념 밴 감자먹을생각에 설레요",
          "original_form": "닭볶음탕 양념 밴 감자먹을생각에 설레요",
          "speaker_id": "1",
          "time": "20210826 17:32"
        },
        {
          "id": "MDRW2100017038.1.40",
          "form": "그게 제일 맛있죠",
          "original_form": "그게 제일 맛있죠",
          "speaker_id": "2",
          "time": "20210826 17:33"
        },
        {
          "id": "MDRW2100017038.1.41",
          "form": "그쵸 감자 너무 맛잇어요 특히 요즘",
          "original_form": "그쵸 감자 너무 맛잇어요 특히 요즘",
          "speaker_id": "1",
          "time": "20210826 17:34"
        },
        {
          "id": "MDRW2100017038.1.42",
          "form": "전 닭볶음탕에 단호박 넣어서 먹는것도 좋아해요",
          "original_form": "전 닭볶음탕에 단호박 넣어서 먹는것도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 17:35"
        },
        {
          "id": "MDRW2100017038.1.43",
          "form": "우와 한번도 단호박은 안넣어먹어봣는데",
          "original_form": "우와 한번도 단호박은 안넣어먹어봣는데",
          "speaker_id": "1",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100017038.1.44",
          "form": "은근 맛잇을것같아요",
          "original_form": "은근 맛잇을것같아요",
          "speaker_id": "1",
          "time": "20210826 18:09"
        },
        {
          "id": "MDRW2100017038.1.45",
          "form": "달달하니 맛있어요",
          "original_form": "달달하니 맛있어요",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017038.1.46",
          "form": "단호박 아니면 고구마도 맛있어요!",
          "original_form": "단호박 아니면 고구마도 맛있어요!",
          "speaker_id": "2",
          "time": "20210826 18:10"
        },
        {
          "id": "MDRW2100017038.1.47",
          "form": "맞아요 고구마가 매운거랑 잘어울리더라구요",
          "original_form": "맞아요 고구마가 매운거랑 잘어울리더라구요",
          "speaker_id": "1",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017038.1.48",
          "form": "떡볶이에 고구마 넣어도 은근 맛잇어요",
          "original_form": "떡볶이에 고구마 넣어도 은근 맛잇어요",
          "speaker_id": "1",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017038.1.49",
          "form": "마자여!",
          "original_form": "마자여!",
          "speaker_id": "2",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017038.1.50",
          "form": "떡볶이에 고구마튀김",
          "original_form": "떡볶이에 고구마튀김",
          "speaker_id": "2",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017038.1.51",
          "form": "그건 사랑입니다",
          "original_form": "그건 사랑입니다",
          "speaker_id": "2",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017038.1.52",
          "form": "고구마튀김을 떡볶이소스에 찍어먹어도 맛잇고",
          "original_form": "고구마튀김을 떡볶이소스에 찍어먹어도 맛잇고",
          "speaker_id": "1",
          "time": "20210826 18:12"
        },
        {
          "id": "MDRW2100017038.1.53",
          "form": "그냥 고구마를 아예 떡볶이에 넣어먹어도 맛잇어요",
          "original_form": "그냥 고구마를 아예 떡볶이에 넣어먹어도 맛잇어요",
          "speaker_id": "1",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100017038.1.54",
          "form": "오오 고구마를 넣어본적은 없는것 같아요",
          "original_form": "오오 고구마를 넣어본적은 없는것 같아요",
          "speaker_id": "2",
          "time": "20210826 18:13"
        },
        {
          "id": "MDRW2100017038.1.55",
          "form": "매콤달달하니 맛잇어요",
          "original_form": "매콤달달하니 맛잇어요",
          "speaker_id": "1",
          "time": "20210826 18:18"
        },
        {
          "id": "MDRW2100017038.1.56",
          "form": "오오 나중에 넣어서 먹어봐야겠어요",
          "original_form": "오오 나중에 넣어서 먹어봐야겠어요",
