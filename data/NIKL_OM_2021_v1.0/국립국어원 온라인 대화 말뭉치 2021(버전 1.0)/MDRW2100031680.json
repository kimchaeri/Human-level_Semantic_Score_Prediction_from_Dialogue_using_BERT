        },
        {
          "id": "MDRW2100031680.1.22",
          "form": "코로나라서",
          "original_form": "코로나라서",
          "speaker_id": "1",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100031680.1.23",
          "form": "저는 잘 됩니다요",
          "original_form": "저는 잘 됩니다요",
          "speaker_id": "1",
          "time": "20210904 23:34"
        },
        {
          "id": "MDRW2100031680.1.24",
          "form": "술 마시면 다 발음이 세지요 뭐",
