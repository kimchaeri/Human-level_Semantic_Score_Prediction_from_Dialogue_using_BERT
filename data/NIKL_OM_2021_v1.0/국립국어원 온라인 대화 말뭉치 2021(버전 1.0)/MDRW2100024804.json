      "utterance": [
        {
          "id": "MDRW2100024804.1.1",
          "form": "출산하고살이쪄서 건강을 위해 다이어트하고있어요",
          "original_form": "출산하고살이쪄서 건강을 위해 다이어트하고있어요",
          "speaker_id": "2",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100024804.1.2",
          "form": "다이어트 한약 드셔 보신 거 있어요?",
          "original_form": "다이어트 한약 드셔 보신 거 있어요?",
          "speaker_id": "1",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100024804.1.3",
          "form": "다이어트가 최고의 미용이니",
          "original_form": "다이어트가 최고의 미용이니",
          "speaker_id": "2",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100024804.1.4",
          "form": "젊을때ㅋㅋㅋㅋ",
          "original_form": "젊을때ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100024804.1.5",
          "form": "효과좋았어요",
          "original_form": "효과좋았어요",
          "speaker_id": "2",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100024804.1.6",
          "form": "하시는 운동있나요?",
          "original_form": "하시는 운동있나요?",
          "speaker_id": "2",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100024804.1.7",
          "form": "부작용은 있다고 하던데 그건 괜찮으셨나 봐요",
          "original_form": "부작용은 있다고 하던데 그건 괜찮으셨나 봐요",
          "speaker_id": "1",
          "time": "20210831 18:58"
        },
        {
          "id": "MDRW2100024804.1.8",
          "form": "예전에 수영 했는데 요즘에는 안 하고 있어요",
          "original_form": "예전에 수영 했는데 요즘에는 안 하고 있어요",
          "speaker_id": "1",
          "time": "20210831 18:59"
        },
        {
          "id": "MDRW2100024804.1.9",
          "form": "운동다니고 싶어도 요즘은 코로나때문에ㅠ",
          "original_form": "운동다니고 싶어도 요즘은 코로나때문에ㅠ",
          "speaker_id": "2",
          "time": "20210831 18:59"
        },
        {
          "id": "MDRW2100024804.1.10",
          "form": "그러니까요 코로나가 웬수에요 얼른 끝났으면 좋겠어요",
          "original_form": "그러니까요 코로나가 웬수에요 얼른 끝났으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210831 18:59"
        },
        {
          "id": "MDRW2100024804.1.11",
          "form": "너무 길어지니 힘드네요",
          "original_form": "너무 길어지니 힘드네요",
          "speaker_id": "2",
          "time": "20210831 19:00"
        },
        {
          "id": "MDRW2100024804.1.12",
          "form": "백신은 맞으셨나요?",
          "original_form": "백신은 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210831 19:00"
        },
        {
          "id": "MDRW2100024804.1.13",
          "form": "아니요 백신 아직 안 맞았어요",
          "original_form": "아니요 백신 아직 안 맞았어요",
          "speaker_id": "1",
          "time": "20210831 19:00"
        },
        {
          "id": "MDRW2100024804.1.14",
          "form": "맞으셨나요?",
          "original_form": "맞으셨나요?",
          "speaker_id": "1",
          "time": "20210831 19:00"
        },
        {
          "id": "MDRW2100024804.1.15",
          "form": "담달 예정인데 겁나네욧",
          "original_form": "담달 예정인데 겁나네욧",
          "speaker_id": "2",
          "time": "20210831 19:02"
        },
        {
          "id": "MDRW2100024804.1.16",
          "form": "화이자로 맞을꺼같아요",
          "original_form": "화이자로 맞을꺼같아요",
          "speaker_id": "2",
          "time": "20210831 19:02"
        },
        {
          "id": "MDRW2100024804.1.17",
          "form": "화이자로 먹고 싶다고 화이를 맞을 수 있는 거예요 별로 관심이 없어서 어떻게 신청할지 하나도 몰라요",
          "original_form": "화이자로 먹고 싶다고 화이를 맞을 수 있는 거예요 별로 관심이 없어서 어떻게 신청할지 하나도 몰라요",
          "speaker_id": "1",
          "time": "20210831 19:03"
        },
        {
          "id": "MDRW2100024804.1.18",
          "form": "모더나 화이자 랜덤인듯 하드라구요",
          "original_form": "모더나 화이자 랜덤인듯 하드라구요",
          "speaker_id": "2",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100024804.1.19",
          "form": "가끔 백신 부작용으로 죽는 사람들 보면 너무 무서워요",
          "original_form": "가끔 백신 부작용으로 죽는 사람들 보면 너무 무서워요",
          "speaker_id": "1",
          "time": "20210831 19:10"
        },
        {
          "id": "MDRW2100024804.1.20",
          "form": "그러게요 그래서 최대한 늦게맞는걸로 했어요",
          "original_form": "그러게요 그래서 최대한 늦게맞는걸로 했어요",
          "speaker_id": "2",
          "time": "20210831 19:10"
        }
      ]
