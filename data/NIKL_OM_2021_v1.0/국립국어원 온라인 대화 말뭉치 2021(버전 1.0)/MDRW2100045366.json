      "utterance": [
        {
          "id": "MDRW2100045366.1.1",
          "form": "스타벅스 엠디에 관심 있으신가요?",
          "original_form": "스타벅스 엠디에 관심 있으신가요?",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045366.1.2",
          "form": "자주는아닌데 가끔맘에드는거있으면 사요",
          "original_form": "자주는아닌데 가끔맘에드는거있으면 사요",
          "speaker_id": "1",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045366.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210908 02:07"
        },
        {
          "id": "MDRW2100045366.1.4",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210908 02:10"
        },
        {
          "id": "MDRW2100045366.1.5",
          "form": "이번년도에는 텀블러하나샀어요",
          "original_form": "이번년도에는 텀블러하나샀어요",
          "speaker_id": "1",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045366.1.6",
          "form": "오늘출시됐다해서 퇴근하고 갔더니",
          "original_form": "오늘출시됐다해서 퇴근하고 갔더니",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045366.1.7",
          "form": "남는게 별로 없더라고요",
          "original_form": "남는게 별로 없더라고요",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045366.1.8",
          "form": "뭘사셧나요?",
          "original_form": "뭘사셧나요?",
          "speaker_id": "2",
          "time": "20210908 02:11"
        },
        {
          "id": "MDRW2100045366.1.9",
          "form": "전 가서 걍 이뻐보이는거샀었는데 봄이랑여름사이에나온 엠디였어요",
          "original_form": "전 가서 걍 이뻐보이는거샀었는데 봄이랑여름사이에나온 엠디였어요",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045366.1.10",
          "form": "오늘도 나왔었나요? 어떤거였어요?",
          "original_form": "오늘도 나왔었나요? 어떤거였어요?",
          "speaker_id": "1",
          "time": "20210908 02:12"
        },
        {
          "id": "MDRW2100045366.1.11",
          "form": "가을이라 갈색으로 된 상품있었구요",
          "original_form": "가을이라 갈색으로 된 상품있었구요",
          "speaker_id": "2",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045366.1.12",
          "form": "아 완전가을느낌으로",
          "original_form": "아 완전가을느낌으로",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045366.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045366.1.14",
          "form": "토끼랑 다람쥐가 포인트인 상풍도 있었어요",
          "original_form": "토끼랑 다람쥐가 포인트인 상풍도 있었어요",
          "speaker_id": "2",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045366.1.15",
          "form": "사신게 하늘색이면 오늘 나온거지 싶은대..",
          "original_form": "사신게 하늘색이면 오늘 나온거지 싶은대..",
          "speaker_id": "2",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045366.1.16",
          "form": "오 토끼랑다람쥐 귀엽네요",
          "original_form": "오 토끼랑다람쥐 귀엽네요",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045366.1.17",
          "form": "구경한번해봐야겠어요",
          "original_form": "구경한번해봐야겠어요",
          "speaker_id": "1",
          "time": "20210908 02:17"
        },
        {
          "id": "MDRW2100045366.1.18",
          "form": "네네 저 개인적으룬",
          "original_form": "네네 저 개인적으룬",
          "speaker_id": "2",
          "time": "20210908 02:19"
        },
        {
          "id": "MDRW2100045366.1.19",
          "form": "토끼가 컵안을 보는 파란컵이 마음에 들었어요",
          "original_form": "토끼가 컵안을 보는 파란컵이 마음에 들었어요",
          "speaker_id": "2",
          "time": "20210908 02:20"
        },
        {
          "id": "MDRW2100045366.1.20",
          "form": "끄아 넘귀여울거같아요",
          "original_form": "끄아 넘귀여울거같아요",
          "speaker_id": "1",
          "time": "20210908 02:20"
        }
      ]
