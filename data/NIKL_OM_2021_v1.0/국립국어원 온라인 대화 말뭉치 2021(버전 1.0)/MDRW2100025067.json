      "utterance": [
        {
          "id": "MDRW2100025067.1.1",
          "form": "좋은 친구네",
          "original_form": "좋은 친구네",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100025067.1.2",
          "form": "엄마랑 같이간다고 해볼까?ㅋㅋ",
          "original_form": "엄마랑 같이간다고 해볼까?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100025067.1.3",
          "form": "부담스러워 하지",
          "original_form": "부담스러워 하지",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100025067.1.4",
          "form": "ㅋㅋㅋ 맛있는거 사준다고 하고",
          "original_form": "ㅋㅋㅋ 맛있는거 사준다고 하고",
          "speaker_id": "1",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100025067.1.5",
          "form": "그러든지~~",
          "original_form": "그러든지~~",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100025067.1.6",
          "form": "아 추석때는 어디 안가?",
          "original_form": "아 추석때는 어디 안가?",
          "speaker_id": "1",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100025067.1.7",
          "form": "글쎄~ 가고싶은데 있어?",
          "original_form": "글쎄~ 가고싶은데 있어?",
          "speaker_id": "2",
          "time": "20210831 23:00"
        },
        {
          "id": "MDRW2100025067.1.8",
          "form": "전에 춘천에 괜찮은 펜션 있지 않았어?",
          "original_form": "전에 춘천에 괜찮은 펜션 있지 않았어?",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025067.1.9",
          "form": "엄마가 갔던데?",
          "original_form": "엄마가 갔던데?",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025067.1.10",
          "form": "응 다음에 같이 가자고 한곳",
          "original_form": "응 다음에 같이 가자고 한곳",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025067.1.11",
          "form": "아 거기는 안면도",
          "original_form": "아 거기는 안면도",
          "speaker_id": "2",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025067.1.12",
          "form": "안면도였어?",
          "original_form": "안면도였어?",
          "speaker_id": "1",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100025067.1.13",
          "form": "나 왜 잘못 기억하고있지?",
          "original_form": "나 왜 잘못 기억하고있지?",
          "speaker_id": "1",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100025067.1.14",
          "form": "그 큰 강아지 있는곳",
          "original_form": "그 큰 강아지 있는곳",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100025067.1.15",
          "form": "아 맞아 골든리트리버",
          "original_form": "아 맞아 골든리트리버",
