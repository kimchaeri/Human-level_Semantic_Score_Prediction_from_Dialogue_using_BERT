      "utterance": [
        {
          "id": "MDRW2100014985.1.1",
          "form": "요새 다시 장마라서 습해요",
          "original_form": "요새 다시 장마라서 습해요",
          "speaker_id": "2",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014985.1.2",
          "form": "그러게요 다음주도 비오고..",
          "original_form": "그러게요 다음주도 비오고..",
          "speaker_id": "1",
          "time": "20210825 21:36"
        },
        {
          "id": "MDRW2100014985.1.3",
          "form": "비가 너무너무시러용",
          "original_form": "비가 너무너무시러용",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100014985.1.4",
          "form": "가을장마라니",
          "original_form": "가을장마라니",
          "speaker_id": "1",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100014985.1.5",
          "form": "여름보다 더 오는데여",
          "original_form": "여름보다 더 오는데여",
          "speaker_id": "1",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100014985.1.6",
          "form": "봄에 미리와서",
          "original_form": "봄에 미리와서",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100014985.1.7",
          "form": "늦가을에오나봐요",
          "original_form": "늦가을에오나봐요",
          "speaker_id": "2",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100014985.1.8",
          "form": "이러고 갑자기 추워질 것 같아요..",
          "original_form": "이러고 갑자기 추워질 것 같아요..",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100014985.1.9",
          "form": "겨울..금방입니다",
          "original_form": "겨울..금방입니다",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100014985.1.10",
          "form": "여맬 준비하세요",
          "original_form": "여맬 준비하세요",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100014985.1.11",
          "form": "아니 무슨 여름가고 바로 겨울이에요;",
          "original_form": "아니 무슨 여름가고 바로 겨울이에요;",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100014985.1.12",
          "form": "물건이었으면 환불이라도 하지 ㅡㅡ",
          "original_form": "물건이었으면 환불이라도 하지 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100014985.1.13",
          "form": "사실상 두계절이죠..",
          "original_form": "사실상 두계절이죠..",
          "speaker_id": "2",
          "time": "20210825 21:45"
        },
        {
          "id": "MDRW2100014985.1.14",
          "form": "여름이랑 겨울중에",
          "original_form": "여름이랑 겨울중에",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100014985.1.15",
          "form": "뭐가 더 최악이세요",
          "original_form": "뭐가 더 최악이세요",
          "speaker_id": "1",
          "time": "20210825 21:47"
        },
        {
          "id": "MDRW2100014985.1.16",
          "form": "여름이요",
          "original_form": "여름이요",
          "speaker_id": "2",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100014985.1.17",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "1",
          "time": "20210825 21:48"
        },
        {
          "id": "MDRW2100014985.1.18",
          "form": "온몸에서 냄새나요",
          "original_form": "온몸에서 냄새나요",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100014985.1.19",
          "form": "스트레스에요",
          "original_form": "스트레스에요",
          "speaker_id": "2",
          "time": "20210825 21:53"
        },
        {
          "id": "MDRW2100014985.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100014985.1.21",
          "form": "쉰내나겠네요",
          "original_form": "쉰내나겠네요",
          "speaker_id": "1",
          "time": "20210825 21:54"
        },
        {
          "id": "MDRW2100014985.1.22",
          "form": "홀애비",
          "original_form": "홀애비",
          "speaker_id": "2",
