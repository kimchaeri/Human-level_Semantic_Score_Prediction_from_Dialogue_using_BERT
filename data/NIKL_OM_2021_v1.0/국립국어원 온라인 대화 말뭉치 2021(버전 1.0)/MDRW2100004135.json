        {
          "id": "MDRW2100004135.1.1",
          "form": "떡볶이 좋아해?",
          "original_form": "떡볶이 좋아해?",
          "speaker_id": "2",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004135.1.2",
          "form": "떡볶이 맛있지",
          "original_form": "떡볶이 맛있지",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004135.1.3",
          "form": "나도 떡볶이 맛집 찾아 다닌 적 있는데",
          "original_form": "나도 떡볶이 맛집 찾아 다닌 적 있는데",
          "speaker_id": "2",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004135.1.4",
          "form": "나는 학교 앞 분식 떡볶이가 좋아",
          "original_form": "나는 학교 앞 분식 떡볶이가 좋아",
          "speaker_id": "1",
          "time": "20210821 15:06"
        },
        {
          "id": "MDRW2100004135.1.5",
          "form": "맛있지",
          "original_form": "맛있지",
          "speaker_id": "2",
          "time": "20210821 15:07"
        },
        {
          "id": "MDRW2100004135.1.6",
          "form": "엽떡이나 신전은?",
          "original_form": "엽떡이나 신전은?",
          "speaker_id": "1",
          "time": "20210821 15:07"
        },
        {
          "id": "MDRW2100004135.1.7",
          "form": "맛있는데 엽떡은 양이 너무 많아",
          "original_form": "맛있는데 엽떡은 양이 너무 많아",
          "speaker_id": "2",
          "time": "20210821 15:07"
        },
        {
          "id": "MDRW2100004135.1.8",
          "form": "나는 고춧가루가 너무 많이 들어가서 별로....",
          "original_form": "나는 고춧가루가 너무 많이 들어가서 별로....",
          "speaker_id": "1",
          "time": "20210821 15:07"
        },
        {
          "id": "MDRW2100004135.1.9",
          "form": "엽떡 너무 비싸",
          "original_form": "엽떡 너무 비싸",
          "speaker_id": "2",
          "time": "20210821 15:07"
        },
        {
          "id": "MDRW2100004135.1.10",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210821 15:07"
        },
        {
          "id": "MDRW2100004135.1.11",
          "form": "학교 앞 추억의 떡볶이가 맛있는데",
          "original_form": "학교 앞 추억의 떡볶이가 맛있는데",
          "speaker_id": "2",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004135.1.12",
          "form": "달다구리 맛있지",
          "original_form": "달다구리 맛있지",
          "speaker_id": "1",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004135.1.13",
          "form": "요즘엔 잘 못찾겠던데",
          "original_form": "요즘엔 잘 못찾겠던데",
          "speaker_id": "2",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004135.1.14",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004135.1.15",
          "form": "그래서 나는 집에서 만들어 먹어",
          "original_form": "그래서 나는 집에서 만들어 먹어",
          "speaker_id": "1",
          "time": "20210821 15:08"
        },
        {
          "id": "MDRW2100004135.1.16",
          "form": "오 맛있겠다",
