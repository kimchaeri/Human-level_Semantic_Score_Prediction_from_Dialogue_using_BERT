      "utterance": [
        {
          "id": "MDRW2100038743.1.1",
          "form": "소면!!",
          "original_form": "소면!!",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100038743.1.2",
          "form": "쌀국수도좋고ㅠ",
          "original_form": "쌀국수도좋고ㅠ",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100038743.1.3",
          "form": "그냥 가늘고 긴건 다좋아하는듯",
          "original_form": "그냥 가늘고 긴건 다좋아하는듯",
          "speaker_id": "2",
          "time": "20210902 20:44"
        },
        {
          "id": "MDRW2100038743.1.4",
          "form": "오 저는 면을 그렇게 좋아하진 않은데 쌀국수는 맛있더라구요!!",
          "original_form": "오 저는 면을 그렇게 좋아하진 않은데 쌀국수는 맛있더라구요!!",
          "speaker_id": "1",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100038743.1.5",
          "form": "고수도 드시나요??",
          "original_form": "고수도 드시나요??",
          "speaker_id": "1",
          "time": "20210902 20:46"
        },
        {
          "id": "MDRW2100038743.1.6",
          "form": "네! 고수도 좋아해요ㅠㅠ!!!",
          "original_form": "네! 고수도 좋아해요ㅠㅠ!!!",
          "speaker_id": "2",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100038743.1.7",
          "form": "오!!!!!!",
          "original_form": "오!!!!!!",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100038743.1.8",
          "form": "고수 좋아하는분 처음이에요!!!",
          "original_form": "고수 좋아하는분 처음이에요!!!",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100038743.1.9",
          "form": "마라탕도 좋아해요 ㅋㅋㅋ",
          "original_form": "마라탕도 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100038743.1.10",
          "form": "마라탕 맛있더라구요!!",
          "original_form": "마라탕 맛있더라구요!!",
          "speaker_id": "1",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100038743.1.11",
          "form": "마라탕 재료중에 뭐 제일 좋아하세요?",
          "original_form": "마라탕 재료중에 뭐 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100038743.1.12",
          "form": "분모자도 좋고 ㅋㅋㅋ두부피도 좋고 ㅋㅋㅋ야채류는 다좋고",
          "original_form": "분모자도 좋고 ㅋㅋㅋ두부피도 좋고 ㅋㅋㅋ야채류는 다좋고",
          "speaker_id": "2",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100038743.1.13",
          "form": "저는 청경채 제일 좋아해요ㅎㅎ",
          "original_form": "저는 청경채 제일 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100038743.1.14",
          "form": "마라샹궈도 맛있더라구요!!",
          "original_form": "마라샹궈도 맛있더라구요!!",
          "speaker_id": "1",
          "time": "20210902 21:37"
        },
        {
          "id": "MDRW2100038743.1.15",
          "form": "청경채넘맛잇죠!! 샤브샤브에도 맛잇고",
          "original_form": "청경채넘맛잇죠!! 샤브샤브에도 맛잇고",
          "speaker_id": "2",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100038743.1.16",
          "form": "전 마랴샹궈는 아직안먹어봤네요!!!",
          "original_form": "전 마랴샹궈는 아직안먹어봤네요!!!",
          "speaker_id": "2",
          "time": "20210902 22:57"
        },
        {
          "id": "MDRW2100038743.1.17",
          "form": "나중에 드셔보세요!",
          "original_form": "나중에 드셔보세요!",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100038743.1.18",
          "form": "마라탕이랑은 또 다른 맛이더라구요ㅎㅎ",
          "original_form": "마라탕이랑은 또 다른 맛이더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100038743.1.19",
          "form": "조만간먹어봐야겠네여 내일은 마라탕 배달 벌써 찜했어요 ㅋㅋ",
          "original_form": "조만간먹어봐야겠네여 내일은 마라탕 배달 벌써 찜했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:00"
        },
        {
          "id": "MDRW2100038743.1.20",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:05"
        },
        {
          "id": "MDRW2100038743.1.21",
          "form": "내일 점심은 마라탕 드시는건가요?",
          "original_form": "내일 점심은 마라탕 드시는건가요?",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100038743.1.22",
          "form": "네네 ㅋㅋㅋ친한동생이 집에 놀러오기로해서",
          "original_form": "네네 ㅋㅋㅋ친한동생이 집에 놀러오기로해서",
          "speaker_id": "2",
          "time": "20210902 23:45"
        },
        {
          "id": "MDRW2100038743.1.23",
          "form": "동생은 첫도전이에요",
          "original_form": "동생은 첫도전이에요",
          "speaker_id": "2",
          "time": "20210902 23:46"
        },
        {
          "id": "MDRW2100038743.1.24",
          "form": "동생분 입에도 맞으면 좋겠네요!",
          "original_form": "동생분 입에도 맞으면 좋겠네요!",
          "speaker_id": "1",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100038743.1.25",
          "form": "저도 내일 친구 만나는데 마라탕 먹을까봐요ㅎㅎ",
          "original_form": "저도 내일 친구 만나는데 마라탕 먹을까봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 01:38"
        },
        {
          "id": "MDRW2100038743.1.26",
          "form": "땡기네요",
          "original_form": "땡기네요",
          "speaker_id": "1",
