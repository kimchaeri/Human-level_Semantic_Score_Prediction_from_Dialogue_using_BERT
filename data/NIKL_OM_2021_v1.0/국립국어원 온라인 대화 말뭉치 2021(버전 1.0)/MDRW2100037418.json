      "utterance": [
        {
          "id": "MDRW2100037418.1.1",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100037418.1.2",
          "form": "거기서말하길 전자레임지에 잠깐돌리면 껍질수분이날라가고 속은 온전하다고하더라고요",
          "original_form": "거기서말하길 전자레임지에 잠깐돌리면 껍질수분이날라가고 속은 온전하다고하더라고요",
          "speaker_id": "1",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100037418.1.3",
          "form": "그래사 솔깃햇어요 ㅋㅋㅋ\r\n그런데 마늘지옥에빠져서 대용량마늘만큼 까보니까 \r\n다시는 안하도싶더라고교",
          "original_form": "그래사 솔깃햇어요 ㅋㅋㅋ\r\n그런데 마늘지옥에빠져서 대용량마늘만큼 까보니까 \r\n다시는 안하도싶더라고교",
          "speaker_id": "1",
          "time": "20210831 23:29"
        },
        {
          "id": "MDRW2100037418.1.4",
          "form": "오 그런 방법이 있군요",
          "original_form": "오 그런 방법이 있군요",
          "speaker_id": "2",
          "time": "20210831 23:30"
        },
        {
          "id": "MDRW2100037418.1.5",
          "form": "오늘 저녁준비하는데",
          "original_form": "오늘 저녁준비하는데",
          "speaker_id": "2",
          "time": "20210831 23:30"
        },
        {
          "id": "MDRW2100037418.1.6",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100037418.1.7",
          "form": "막내가 마늘 까면서 얘기가 나왔는데",
          "original_form": "막내가 마늘 까면서 얘기가 나왔는데",
          "speaker_id": "2",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100037418.1.8",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100037418.1.9",
          "form": "저는 마늘까는게 더 귀찮다 \r\n엄마는 양파까는게 더 귀찮다로 나뉘더라구요ㅋㅋㅋ",
          "original_form": "저는 마늘까는게 더 귀찮다 \r\n엄마는 양파까는게 더 귀찮다로 나뉘더라구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100037418.1.10",
          "form": "name1님은 뭐가 더 귀찮으세요?",
          "original_form": "&name1&님은 뭐가 더 귀찮으세요?",
          "speaker_id": "2",
          "time": "20210831 23:31"
        },
        {
          "id": "MDRW2100037418.1.11",
          "form": "오... 몇개까느냐가 중요한데\r\n1이요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "오... 몇개까느냐가 중요한데\r\n1이요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100037418.1.12",
          "form": "그러시군요ㅋㅋㅋㅋ",
          "original_form": "그러시군요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100037418.1.13",
          "form": "양파를 대량으로 까면 이것도 짜증나긴라겍죠. 마늘때문에 손매워갖고",
          "original_form": "양파를 대량으로 까면 이것도 짜증나긴라겍죠. 마늘때문에 손매워갖고",
          "speaker_id": "1",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100037418.1.14",
          "form": "고통에 시달려봐서..",
          "original_form": "고통에 시달려봐서..",
          "speaker_id": "1",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100037418.1.15",
          "form": "예전에 로망이잇엇으나..\r\n마늘은 돈주고사는게 졸죠",
          "original_form": "예전에 로망이잇엇으나..\r\n마늘은 돈주고사는게 졸죠",
          "speaker_id": "1",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100037418.1.16",
          "form": "저는  마늘을 까려고 하면 넘 안 까져서 짜증이 나려고 할 때도 있어요",
          "original_form": "저는  마늘을 까려고 하면 넘 안 까져서 짜증이 나려고 할 때도 있어요",
          "speaker_id": "2",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100037418.1.17",
          "form": "저희엄마는 썩은마늘도잇다며 깐마늘좋아해요",
          "original_form": "저희엄마는 썩은마늘도잇다며 깐마늘좋아해요",
          "speaker_id": "1",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100037418.1.18",
          "form": "맨날까시나봐요? 마늘장아찌?",
          "original_form": "맨날까시나봐요? 마늘장아찌?",
          "speaker_id": "1",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100037418.1.19",
          "form": "마늘 깐 마늘 다진 마늘 사는게 편하긴 해요",
          "original_form": "마늘 깐 마늘 다진 마늘 사는게 편하긴 해요",
          "speaker_id": "2",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100037418.1.20",
          "form": "아니요ㅋㅋㅋ 마늘 거의 안 까요ㅋㅋ",
          "original_form": "아니요ㅋㅋㅋ 마늘 거의 안 까요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100037418.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100037418.1.22",
          "form": "까기가 귀찮아서 요리에 마늘을 잘 안 넣어요",
          "original_form": "까기가 귀찮아서 요리에 마늘을 잘 안 넣어요",
          "speaker_id": "2",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100037418.1.23",
          "form": "근데 제가 요리카페에서보니까",
          "original_form": "근데 제가 요리카페에서보니까",
          "speaker_id": "1",
          "time": "20210831 23:35"
        },
        {
          "id": "MDRW2100037418.1.24",
          "form": "마늘을 다진걸사면 마늘고유의 향이 날라간다고하더라고요",
          "original_form": "마늘을 다진걸사면 마늘고유의 향이 날라간다고하더라고요",
          "speaker_id": "1",
          "time": "20210831 23:36"
        }
      ]
