        {
          "id": "MDRW2100013358.1.1",
          "form": "전 독서가 좋아요",
          "original_form": "전 독서가 좋아요",
          "speaker_id": "1",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013358.1.2",
          "form": "요즘 읽는 책 있나요",
          "original_form": "요즘 읽는 책 있나요",
          "speaker_id": "2",
          "time": "20210825 14:09"
        },
        {
          "id": "MDRW2100013358.1.3",
          "form": "오늘도 도서관 갔어요",
          "original_form": "오늘도 도서관 갔어요",
          "speaker_id": "1",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013358.1.4",
          "form": "전 조선왕조실록을 좋아해요",
          "original_form": "전 조선왕조실록을 좋아해요",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013358.1.5",
          "form": "베르나르베르베르 개미 아세요?",
          "original_form": "베르나르베르베르 개미 아세요?",
          "speaker_id": "1",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013358.1.6",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013358.1.7",
          "form": "진짜 재밌어요",
          "original_form": "진짜 재밌어요",
          "speaker_id": "1",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013358.1.8",
          "form": "전 한국사를 좋아해요",
          "original_form": "전 한국사를 좋아해요",
          "speaker_id": "2",
          "time": "20210825 14:10"
        },
        {
          "id": "MDRW2100013358.1.9",
          "form": "전 한국사 안좋아해요",
          "original_form": "전 한국사 안좋아해요",
          "speaker_id": "1",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013358.1.10",
          "form": "잡지도 좋아해요",
          "original_form": "잡지도 좋아해요",
          "speaker_id": "2",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013358.1.11",
          "form": "사극로맨스 도서는 좋아요",
          "original_form": "사극로맨스 도서는 좋아요",
          "speaker_id": "1",
          "time": "20210825 14:11"
        },
        {
          "id": "MDRW2100013358.1.12",
          "form": "군대에서 에스콰이어 잡지 봤어요",
