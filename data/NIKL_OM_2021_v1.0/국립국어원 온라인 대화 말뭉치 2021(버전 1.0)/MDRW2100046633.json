      "utterance": [
        {
          "id": "MDRW2100046633.1.1",
          "form": "요즘 사고싶은 물건 있으신가요?",
          "original_form": "요즘 사고싶은 물건 있으신가요?",
          "speaker_id": "2",
          "time": "20211103 19:00"
        },
        {
          "id": "MDRW2100046633.1.2",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20211103 19:13"
        },
        {
          "id": "MDRW2100046633.1.3",
          "form": "요즘 차바꾸고 싶네요",
          "original_form": "요즘 차바꾸고 싶네요",
          "speaker_id": "1",
          "time": "20211103 19:13"
        },
        {
          "id": "MDRW2100046633.1.4",
          "form": "헉 저도 차 사고 싶어요",
          "original_form": "헉 저도 차 사고 싶어요",
          "speaker_id": "2",
          "time": "20211103 19:19"
        },
        {
          "id": "MDRW2100046633.1.5",
          "form": "어떤 차 사고싶으신가요?",
          "original_form": "어떤 차 사고싶으신가요?",
          "speaker_id": "2",
          "time": "20211103 19:19"
        },
        {
          "id": "MDRW2100046633.1.6",
          "form": "음 미니요",
          "original_form": "음 미니요",
          "speaker_id": "1",
          "time": "20211103 19:56"
        },
        {
          "id": "MDRW2100046633.1.7",
          "form": "미니 쿠퍼 말씀하시는건가요? ㅎㅎ",
          "original_form": "미니 쿠퍼 말씀하시는건가요? ㅎㅎ",
          "speaker_id": "2",
          "time": "20211103 21:12"
        },
        {
          "id": "MDRW2100046633.1.8",
          "form": "차알못이라서요 ㅎㅎ",
          "original_form": "차알못이라서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211103 21:12"
        },
        {
          "id": "MDRW2100046633.1.9",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20211103 21:12"
        },
        {
          "id": "MDRW2100046633.1.10",
          "form": "저도 그차만 압니다",
          "original_form": "저도 그차만 압니다",
          "speaker_id": "1",
          "time": "20211103 21:13"
        },
        {
          "id": "MDRW2100046633.1.11",
          "form": "미니 쿠퍼 예쁘더라구요",
          "original_form": "미니 쿠퍼 예쁘더라구요",
          "speaker_id": "2",
          "time": "20211103 21:13"
        },
        {
          "id": "MDRW2100046633.1.12",
          "form": "저는 이번에 나온",
          "original_form": "저는 이번에 나온",
          "speaker_id": "2",
          "time": "20211103 21:14"
        },
        {
          "id": "MDRW2100046633.1.13",
          "form": "스포티지 신형이 예뻐서 맘에 두고 있어요 ㅋㅋㅋ",
          "original_form": "스포티지 신형이 예뻐서 맘에 두고 있어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211103 21:14"
        },
        {
          "id": "MDRW2100046633.1.14",
          "form": "오봤어요",
          "original_form": "오봤어요",
          "speaker_id": "1",
          "time": "20211103 21:14"
        },
        {
          "id": "MDRW2100046633.1.15",
          "form": "이뻐요",
          "original_form": "이뻐요",
          "speaker_id": "1",
          "time": "20211103 21:14"
        },
        {
          "id": "MDRW2100046633.1.16",
          "form": "그쵸! ㅋㅋㅋ",
          "original_form": "그쵸! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211103 21:14"
        },
        {
          "id": "MDRW2100046633.1.17",
          "form": "근데 자꾸 동생이 못생겼대요 ㅠ",
          "original_form": "근데 자꾸 동생이 못생겼대요 ㅠ",
          "speaker_id": "2",
          "time": "20211103 21:14"
        },
        {
          "id": "MDRW2100046633.1.18",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20211103 21:14"
        },
        {
          "id": "MDRW2100046633.1.19",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "1",
          "time": "20211103 21:14"
        },
        {
          "id": "MDRW2100046633.1.20",
          "form": "차도 취향인가봐요",
          "original_form": "차도 취향인가봐요",
          "speaker_id": "2",
          "time": "20211103 21:15"
        },
        {
          "id": "MDRW2100046633.1.21",
          "form": "저희 집사람은",
          "original_form": "저희 집사람은",
          "speaker_id": "1",
          "time": "20211103 21:15"
        },
        {
          "id": "MDRW2100046633.1.22",
          "form": "웅이아버지 차라고 놀립니다ㅜ",
          "original_form": "웅이아버지 차라고 놀립니다ㅜ",
          "speaker_id": "1",
          "time": "20211103 21:15"
        },
        {
          "id": "MDRW2100046633.1.23",
          "form": "웅이아버지 차 ㅋㅋㅋ 왜요?",
          "original_form": "웅이아버지 차 ㅋㅋㅋ 왜요?",
          "speaker_id": "2",
          "time": "20211103 21:15"
        },
        {
          "id": "MDRW2100046633.1.24",
          "form": "이번해 디자인이 웅이아버지랑 닮았어요ㅋㅋ",
          "original_form": "이번해 디자인이 웅이아버지랑 닮았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20211103 21:16"
        },
        {
          "id": "MDRW2100046633.1.25",
          "form": "#미니웅이아버지",
          "original_form": "#미니웅이아버지",
          "speaker_id": "1",
          "time": "20211103 21:16"
        },
        {
          "id": "MDRW2100046633.1.26",
          "form": "아하 웅이 아버지 닮은 차 ㅋㅋㅋ 웃기네요 ㅋㅋㅋㅋ",
          "original_form": "아하 웅이 아버지 닮은 차 ㅋㅋㅋ 웃기네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211103 21:16"
        }
      ]
