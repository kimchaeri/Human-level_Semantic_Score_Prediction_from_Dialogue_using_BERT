      "utterance": [
        {
          "id": "MDRW2100022341.1.1",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022341.1.2",
          "form": "그래 뉴발과 나이키중에",
          "original_form": "그래 뉴발과 나이키중에",
          "speaker_id": "2",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022341.1.3",
          "form": "내생각은 일단 신어보고정하라는거지",
          "original_form": "내생각은 일단 신어보고정하라는거지",
          "speaker_id": "2",
          "time": "20210829 12:49"
        },
        {
          "id": "MDRW2100022341.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022341.1.5",
          "form": "근데 신어볼수 있을까",
          "original_form": "근데 신어볼수 있을까",
          "speaker_id": "1",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022341.1.6",
          "form": "두개다 인기모델이라",
          "original_form": "두개다 인기모델이라",
          "speaker_id": "1",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022341.1.7",
          "form": "온라인도 다품절이야",
          "original_form": "온라인도 다품절이야",
          "speaker_id": "1",
          "time": "20210829 12:50"
        },
        {
          "id": "MDRW2100022341.1.8",
          "form": "너무 인기있어서 오프라인에없어??",
          "original_form": "너무 인기있어서 오프라인에없어??",
          "speaker_id": "2",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022341.1.9",
          "form": "쉽지않네",
          "original_form": "쉽지않네",
          "speaker_id": "2",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022341.1.10",
          "form": "입고 맞춰서 사야할거같은데",
          "original_form": "입고 맞춰서 사야할거같은데",
          "speaker_id": "1",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022341.1.11",
          "form": "미리 매장에 물어봐야하는거아냐?",
          "original_form": "미리 매장에 물어봐야하는거아냐?",
          "speaker_id": "2",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022341.1.12",
          "form": "온라인도 씨가 말랐으면",
          "original_form": "온라인도 씨가 말랐으면",
          "speaker_id": "1",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022341.1.13",
          "form": "근데 진짜 신어보고사야하는데",
          "original_form": "근데 진짜 신어보고사야하는데",
          "speaker_id": "2",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022341.1.14",
          "form": "오프라인도 잘 없겠지?",
          "original_form": "오프라인도 잘 없겠지?",
          "speaker_id": "1",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022341.1.15",
          "form": "와 나이키는진짜의외야",
          "original_form": "와 나이키는진짜의외야",
          "speaker_id": "2",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022341.1.16",
          "form": "못생겻는데",
          "original_form": "못생겻는데",
          "speaker_id": "2",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022341.1.17",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022341.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022341.1.19",
          "form": "내눈에만 못생겼나?",
          "original_form": "내눈에만 못생겼나?",
          "speaker_id": "2",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022341.1.20",
          "form": "취존합니다...",
          "original_form": "취존합니다...",
          "speaker_id": "1",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022341.1.21",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210829 12:51"
        },
        {
          "id": "MDRW2100022341.1.22",
          "form": "사진 보낸거 봐봐",
          "original_form": "사진 보낸거 봐봐",
          "speaker_id": "1",
          "time": "20210829 12:52"
        },
        {
          "id": "MDRW2100022341.1.23",
          "form": "이거도?",
          "original_form": "이거도?",
          "speaker_id": "1",
          "time": "20210829 12:52"
        },
        {
          "id": "MDRW2100022341.1.24",
          "form": "못생겼어?",
          "original_form": "못생겼어?",
          "speaker_id": "1",
          "time": "20210829 12:52"
        },
        {
          "id": "MDRW2100022341.1.25",
          "form": "저건예쁘지",
          "original_form": "저건예쁘지",
          "speaker_id": "2",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100022341.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100022341.1.27",
          "form": "저 까만 색의 조합이",
          "original_form": "저 까만 색의 조합이",
          "speaker_id": "2",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100022341.1.28",
          "form": "마음에 안들어",
          "original_form": "마음에 안들어",
          "speaker_id": "2",
          "time": "20210829 12:53"
        },
        {
          "id": "MDRW2100022341.1.29",
          "form": "근데 한가지 명심해야할건",
          "original_form": "근데 한가지 명심해야할건",
          "speaker_id": "2",
          "time": "20210829 12:55"
        },
        {
          "id": "MDRW2100022341.1.30",
          "form": "난 뉴발만 맘에들었는데",
          "original_form": "난 뉴발만 맘에들었는데",
          "speaker_id": "2",
          "time": "20210829 12:55"
        },
        {
          "id": "MDRW2100022341.1.31",
          "form": "다른사람들은 뉴발을 극구반대했다는거야",
          "original_form": "다른사람들은 뉴발을 극구반대했다는거야",
          "speaker_id": "2",
          "time": "20210829 12:55"
        },
        {
          "id": "MDRW2100022341.1.32",
          "form": "내가 아주 마이너할 가능성이 있어",
          "original_form": "내가 아주 마이너할 가능성이 있어",
          "speaker_id": "2",
          "time": "20210829 12:55"
        },
        {
          "id": "MDRW2100022341.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:22"
        },
        {
          "id": "MDRW2100022341.1.34",
          "form": "뉴발도",
          "original_form": "뉴발도",
          "speaker_id": "1",
          "time": "20210829 13:22"
        },
        {
          "id": "MDRW2100022341.1.35",
          "form": "내눈에는 맘에 들었는데",
          "original_form": "내눈에는 맘에 들었는데",
          "speaker_id": "1",
          "time": "20210829 13:23"
        },
        {
          "id": "MDRW2100022341.1.36",
          "form": "반대 근거가",
          "original_form": "반대 근거가",
          "speaker_id": "1",
          "time": "20210829 13:23"
        },
        {
          "id": "MDRW2100022341.1.37",
          "form": "색이라고 했단 말이지?",
          "original_form": "색이라고 했단 말이지?",
          "speaker_id": "1",
          "time": "20210829 13:23"
        },
        {
          "id": "MDRW2100022341.1.38",
          "form": "사람들이 흰색을 더",
          "original_form": "사람들이 흰색을 더",
          "speaker_id": "1",
          "time": "20210829 13:23"
        },
        {
          "id": "MDRW2100022341.1.39",
          "form": "좋아하는건가?",
          "original_form": "좋아하는건가?",
          "speaker_id": "1",
          "time": "20210829 13:23"
        },
        {
          "id": "MDRW2100022341.1.40",
          "form": "색이라고???",
          "original_form": "색이라고???",
          "speaker_id": "2",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100022341.1.41",
          "form": "완전 어디에는",
          "original_form": "완전 어디에는",
          "speaker_id": "2",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100022341.1.42",
          "form": "어디에든 무난하게",
          "original_form": "어디에든 무난하게",
          "speaker_id": "2",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100022341.1.43",
          "form": "어울릴 색 아닌가",
          "original_form": "어울릴 색 아닌가",
          "speaker_id": "2",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100022341.1.44",
          "form": "어째서",
          "original_form": "어째서",
          "speaker_id": "2",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100022341.1.45",
          "form": "나도 그렇게 생각했었는데",
          "original_form": "나도 그렇게 생각했었는데",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100022341.1.46",
          "form": "친구가 아니래",
          "original_form": "친구가 아니래",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100022341.1.47",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100022341.1.48",
          "form": "이건 진짜 신어봐야 할듯",
          "original_form": "이건 진짜 신어봐야 할듯",
          "speaker_id": "1",
          "time": "20210829 13:52"
        },
        {
          "id": "MDRW2100022341.1.49",
          "form": "색깔별로 신발 하나씩 갖고싶어서",
          "original_form": "색깔별로 신발 하나씩 갖고싶어서",
          "speaker_id": "1",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100022341.1.50",
          "form": "나이키로 흰색 사고",
          "original_form": "나이키로 흰색 사고",
          "speaker_id": "1",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100022341.1.51",
          "form": "뉴발로 회색을 나중에 사는건 어떨까도",
          "original_form": "뉴발로 회색을 나중에 사는건 어떨까도",
          "speaker_id": "1",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100022341.1.52",
          "form": "생각해봤어",
          "original_form": "생각해봤어",
          "speaker_id": "1",
          "time": "20210829 13:53"
        },
        {
          "id": "MDRW2100022341.1.53",
          "form": "흰색이 더 기본템이긴하지",
          "original_form": "흰색이 더 기본템이긴하지",
          "speaker_id": "2",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100022341.1.54",
          "form": "신어봐야해",
          "original_form": "신어봐야해",
          "speaker_id": "2",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100022341.1.55",
          "form": "나이키가 완전흰색이아니고",
          "original_form": "나이키가 완전흰색이아니고",
          "speaker_id": "2",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100022341.1.56",
          "form": "누리끼리한흰색이라",
          "original_form": "누리끼리한흰색이라",
          "speaker_id": "2",
          "time": "20210829 13:58"
        },
        {
          "id": "MDRW2100022341.1.57",
          "form": "의외로 커버 범위가 좁을수도있어",
          "original_form": "의외로 커버 범위가 좁을수도있어",
          "speaker_id": "2",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100022341.1.58",
          "form": "아하?",
          "original_form": "아하?",
          "speaker_id": "1",
          "time": "20210829 14:01"
        },
        {
          "id": "MDRW2100022341.1.59",
          "form": "그럴수도 있겠다",
          "original_form": "그럴수도 있겠다",
          "speaker_id": "1",
          "time": "20210829 14:02"
        },
        {
          "id": "MDRW2100022341.1.60",
          "form": "으음",
          "original_form": "으음",
          "speaker_id": "1",
          "time": "20210829 14:02"
        },
        {
          "id": "MDRW2100022341.1.61",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "1",
          "time": "20210829 14:02"
        },
        {
          "id": "MDRW2100022341.1.62",
          "form": "좀 참아보는거로",
          "original_form": "좀 참아보는거로",
          "speaker_id": "1",
          "time": "20210829 14:02"
        },
        {
          "id": "MDRW2100022341.1.63",
          "form": "컨버스로 버텨본다..",
          "original_form": "컨버스로 버텨본다..",
          "speaker_id": "1",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100022341.1.64",
          "form": "무신사 8월 쿠폰을 쓰려고",
          "original_form": "무신사 8월 쿠폰을 쓰려고",
          "speaker_id": "1",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100022341.1.65",
          "form": "뭘살까 고르고있어",
          "original_form": "뭘살까 고르고있어",
          "speaker_id": "1",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100022341.1.66",
          "form": "실패할수도 있으니",
          "original_form": "실패할수도 있으니",
