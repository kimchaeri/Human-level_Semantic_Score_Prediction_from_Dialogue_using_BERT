      "utterance": [
        {
          "id": "MDRW2100040161.1.1",
          "form": "자전거 잘 타고 있나??",
          "original_form": "자전거 잘 타고 있나??",
          "speaker_id": "1",
          "time": "20210904 13:28"
        },
        {
          "id": "MDRW2100040161.1.2",
          "form": "타고 들어왔다. 와 이제 좀쉰다.",
          "original_form": "타고 들어왔다. 와 이제 좀쉰다.",
          "speaker_id": "2",
          "time": "20210904 13:33"
        },
        {
          "id": "MDRW2100040161.1.3",
          "form": "디다마ㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋ자유시간이다ㅋㅋ",
          "original_form": "디다마ㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋ자유시간이다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:34"
        },
        {
          "id": "MDRW2100040161.1.4",
          "form": "아우 수고했다 수고했어",
          "original_form": "아우 수고했다 수고했어",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100040161.1.5",
          "form": "인형놀이 하다가 목쉬겠다 정말",
          "original_form": "인형놀이 하다가 목쉬겠다 정말",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100040161.1.6",
          "form": "응 ㅋㅋㅋㅋㅋ아 웃겨ㅋㅋ",
          "original_form": "응 ㅋㅋㅋㅋㅋ아 웃겨ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100040161.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 급열정 식을거같은 피곤함 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 급열정 식을거같은 피곤함 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100040161.1.8",
          "form": "이집 저집 쉽지않네ㅋㅋ",
          "original_form": "이집 저집 쉽지않네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100040161.1.9",
          "form": "맞어ㅋㅋㅋㅋㅋ",
          "original_form": "맞어ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:35"
        },
        {
          "id": "MDRW2100040161.1.10",
          "form": "나도 내가 피곤해서ㅋㅋㅋ",
          "original_form": "나도 내가 피곤해서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100040161.1.11",
          "form": "그러니까",
          "original_form": "그러니까",
          "speaker_id": "1",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100040161.1.12",
          "form": "우리 신랑도 어제 팝콘 브레인 얘기 듣고",
          "original_form": "우리 신랑도 어제 팝콘 브레인 얘기 듣고",
          "speaker_id": "1",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100040161.1.13",
          "form": "아침에 티비랑 패드도 안 보여주고",
          "original_form": "아침에 티비랑 패드도 안 보여주고",
          "speaker_id": "1",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100040161.1.14",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100040161.1.15",
          "form": "놀아준다고 벌써 지쳤음",
          "original_form": "놀아준다고 벌써 지쳤음",
          "speaker_id": "1",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100040161.1.16",
          "form": "멋지네이ㅋㄱㅋㅈ",
          "original_form": "멋지네이ㅋㄱㅋㅈ",
          "speaker_id": "2",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100040161.1.17",
          "form": "나두 벌써지치면안되는데 아오ㅋㅋ",
          "original_form": "나두 벌써지치면안되는데 아오ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100040161.1.18",
          "form": "내일으신랑도 없는데",
          "original_form": "내일으신랑도 없는데",
          "speaker_id": "1",
          "time": "20210904 13:37"
        },
        {
          "id": "MDRW2100040161.1.19",
          "form": "무사히 주말을 버틸수 있을지 ㅋㅋㅋㅋㅋ",
          "original_form": "무사히 주말을 버틸수 있을지 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:38"
        },
        {
          "id": "MDRW2100040161.1.20",
          "form": "언ㅇ니 난 틀어줬다 내가 안되겠다",
          "original_form": "언ㅇ니 난 틀어줬다 내가 안되겠다",
          "speaker_id": "2",
          "time": "20210904 13:45"
        },
        {
          "id": "MDRW2100040161.1.21",
          "form": "ㅋㅋㅋㅋㅋㄱㄲㄱㄲㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㄱㄲㄱㄲㅋ",
          "speaker_id": "2",
          "time": "20210904 13:45"
        },
        {
          "id": "MDRW2100040161.1.22",
          "form": "그래 하루 1시간은 허용하자 우리 주말이잖아",
          "original_form": "그래 하루 1시간은 허용하자 우리 주말이잖아",
          "speaker_id": "1",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100040161.1.23",
          "form": "우리도 지금 오후에 1시간 틀려고 버티고 있다",
          "original_form": "우리도 지금 오후에 1시간 틀려고 버티고 있다",
          "speaker_id": "1",
          "time": "20210904 13:50"
        },
        {
          "id": "MDRW2100040161.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋ",
