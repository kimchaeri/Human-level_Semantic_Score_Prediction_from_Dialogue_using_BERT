      "utterance": [
        {
          "id": "MDRW2100024462.1.1",
          "form": "name2님이 계신 지역의 날씨는 어떤가요?",
          "original_form": "&name2&님이 계신 지역의 날씨는 어떤가요?",
          "speaker_id": "1",
          "time": "20210831 10:41"
        },
        {
          "id": "MDRW2100024462.1.2",
          "form": "해가쨍쨍입니다",
          "original_form": "해가쨍쨍입니다",
          "speaker_id": "2",
          "time": "20210831 10:43"
        },
        {
          "id": "MDRW2100024462.1.3",
          "form": "여긴 비가 오려나 흐리네요ㅜ",
          "original_form": "여긴 비가 오려나 흐리네요ㅜ",
          "speaker_id": "1",
          "time": "20210831 10:43"
        },
        {
          "id": "MDRW2100024462.1.4",
          "form": "아여기도 아까는 비왔는데 지금은 해가ㅠㅠ",
          "original_form": "아여기도 아까는 비왔는데 지금은 해가ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 10:44"
        },
        {
          "id": "MDRW2100024462.1.5",
          "form": "엄청 습하겠네요ㅜㅜ",
          "original_form": "엄청 습하겠네요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100024462.1.6",
          "form": "얼른 시원해지면 좋겠네요ㅜ",
          "original_form": "얼른 시원해지면 좋겠네요ㅜ",
          "speaker_id": "1",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100024462.1.7",
          "form": "해가있어서 더워여ㅠㅠ밤에는그래도 추워서문닫고 잤어요",
          "original_form": "해가있어서 더워여ㅠㅠ밤에는그래도 추워서문닫고 잤어요",
          "speaker_id": "2",
          "time": "20210831 10:52"
        },
        {
          "id": "MDRW2100024462.1.8",
          "form": "맞아요!\r\n저도 새벽에 깨서  창문닫고 잤는데ㅋㅋ",
          "original_form": "맞아요!\r\n저도 새벽에 깨서  창문닫고 잤는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024462.1.9",
          "form": "이런날 감기잘 걸리니 건강 조심하세요~~^^",
          "original_form": "이런날 감기잘 걸리니 건강 조심하세요~~^^",
          "speaker_id": "1",
          "time": "20210831 10:53"
        },
        {
          "id": "MDRW2100024462.1.10",
          "form": "맞아요 저도 새벽에 닫었어요",
          "original_form": "맞아요 저도 새벽에 닫었어요",
          "speaker_id": "2",
          "time": "20210831 10:56"
        },
        {
          "id": "MDRW2100024462.1.11",
          "form": "ㅎㅎㅎ날씨는 그래도 어느지역이든 비슷한가보네요ㅎ",
          "original_form": "ㅎㅎㅎ날씨는 그래도 어느지역이든 비슷한가보네요ㅎ",
          "speaker_id": "1",
          "time": "20210831 10:56"
        },
        {
          "id": "MDRW2100024462.1.12",
          "form": "그런가봐요ㅋㅋ여기는아랫지역",
          "original_form": "그런가봐요ㅋㅋ여기는아랫지역",
          "speaker_id": "2",
          "time": "20210831 10:58"
        },
        {
          "id": "MDRW2100024462.1.13",
          "form": "앗 저도 아랫지역인데ㅋㅋㅋ",
          "original_form": "앗 저도 아랫지역인데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:00"
        },
        {
          "id": "MDRW2100024462.1.14",
          "form": "저는 대구요",
          "original_form": "저는 대구요",
          "speaker_id": "2",
          "time": "20210831 11:00"
        },
        {
          "id": "MDRW2100024462.1.15",
          "form": "전 더 밑에 부산이요ㅎㅎ",
          "original_form": "전 더 밑에 부산이요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 11:00"
        },
        {
          "id": "MDRW2100024462.1.16",
          "form": "그래서 날씨가  비슷했군요ㅋㅋ",
          "original_form": "그래서 날씨가  비슷했군요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:01"
        },
        {
          "id": "MDRW2100024462.1.17",
          "form": "그런가봐요ㅎㅎ. 그래도 에어컨안켜도",
          "original_form": "그런가봐요ㅎㅎ. 그래도 에어컨안켜도",
