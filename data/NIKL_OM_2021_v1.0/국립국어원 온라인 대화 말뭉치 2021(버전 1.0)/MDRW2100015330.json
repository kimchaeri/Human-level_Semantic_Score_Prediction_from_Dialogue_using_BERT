        {
          "id": "MDRW2100015330.1.1",
          "form": "게임 좋아하세요?",
          "original_form": "게임 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015330.1.2",
          "form": "네 좋아합니다",
          "original_form": "네 좋아합니다",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015330.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 22:51"
        },
        {
          "id": "MDRW2100015330.1.4",
          "form": "오 저도 좋아해요",
          "original_form": "오 저도 좋아해요",
          "speaker_id": "1",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015330.1.5",
          "form": "오버워치 했었어요",
          "original_form": "오버워치 했었어요",
          "speaker_id": "1",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015330.1.6",
          "form": "롤은요?",
          "original_form": "롤은요?",
          "speaker_id": "2",
          "time": "20210825 22:58"
        },
        {
          "id": "MDRW2100015330.1.7",
          "form": "롤은 친구들이랑 AI끼고 어쩌다 한번해요 ㅋㅋㅋㅋ",
          "original_form": "롤은 친구들이랑 AI끼고 어쩌다 한번해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015330.1.8",
          "form": "사람들이랑 하면 욕먹을까 무서워서요",
          "original_form": "사람들이랑 하면 욕먹을까 무서워서요",
          "speaker_id": "1",
          "time": "20210825 22:59"
        },
        {
          "id": "MDRW2100015330.1.9",
          "form": "그렇긴해요",
          "original_form": "그렇긴해요",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015330.1.10",
          "form": "멘탈나가죠",
          "original_form": "멘탈나가죠",
          "speaker_id": "2",
          "time": "20210825 23:00"
        },
        {
          "id": "MDRW2100015330.1.11",
          "form": "그쳐 롤은 워낙 말 막 하기로 유명하잖아요",
          "original_form": "그쳐 롤은 워낙 말 막 하기로 유명하잖아요",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015330.1.12",
          "form": "그래서 오버워치햇어요",
          "original_form": "그래서 오버워치햇어요",
          "speaker_id": "1",
          "time": "20210825 23:11"
        },
        {
          "id": "MDRW2100015330.1.13",
          "form": "옵치는 좀 더 즐겜느낌이라",
          "original_form": "옵치는 좀 더 즐겜느낌이라",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100015330.1.14",
          "form": "옵치는 제가 못해서",
          "original_form": "옵치는 제가 못해서",
          "speaker_id": "2",
          "time": "20210825 23:18"
        },
        {
          "id": "MDRW2100015330.1.15",
          "form": "앗 그렇군요ㅠ",
          "original_form": "앗 그렇군요ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015330.1.16",
          "form": "롤은 잘하세요?",
          "original_form": "롤은 잘하세요?",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015330.1.17",
          "form": "음..  잘 못해요",
          "original_form": "음..  잘 못해요",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015330.1.18",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015330.1.19",
          "form": "티어가 어캐돼요?",
          "original_form": "티어가 어캐돼요?",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015330.1.20",
          "form": "골드요 ㅎㅎ",
          "original_form": "골드요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015330.1.21",
          "form": "골드면 잘하시는거자나요 ㅋㅋㅋㅋㅋ",
          "original_form": "골드면 잘하시는거자나요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:36"
        },
        {
          "id": "MDRW2100015330.1.22",
          "form": "ㅋㅋㅋㅋㅋ 플레이상은 되아죠 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 플레이상은 되아죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015330.1.23",
          "form": "브실골이자나요ㅋㅋ",
          "original_form": "브실골이자나요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:38"
        },
        {
          "id": "MDRW2100015330.1.24",
          "form": "ㅋㅋㅋㅋㅋ브실골 같이 묶이긴하는데",
          "original_form": "ㅋㅋㅋㅋㅋ브실골 같이 묶이긴하는데",
          "speaker_id": "1",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015330.1.25",
          "form": "브론즈랑 실버 사이",
          "original_form": "브론즈랑 실버 사이",
          "speaker_id": "1",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015330.1.26",
          "form": "실버와 골드 사이엔 벽이 있다고 생각해요",
          "original_form": "실버와 골드 사이엔 벽이 있다고 생각해요",
          "speaker_id": "1",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015330.1.27",
          "form": "특히 부론즈요 ㅋㅋ",
          "original_form": "특히 부론즈요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015330.1.28",
