      "utterance": [
        {
          "id": "MDRW2100020535.1.1",
          "form": "매운거 좋아하시나요",
          "original_form": "매운거 좋아하시나요",
          "speaker_id": "1",
          "time": "20210828 09:18"
        },
        {
          "id": "MDRW2100020535.1.2",
          "form": "넵 저는 매운거 좋아합니다",
          "original_form": "넵 저는 매운거 좋아합니다",
          "speaker_id": "2",
          "time": "20210828 09:18"
        },
        {
          "id": "MDRW2100020535.1.3",
          "form": "약간 열정만 넘치는타입ㅋㅋㅋㅋㅋ",
          "original_form": "약간 열정만 넘치는타입ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 09:19"
        },
        {
          "id": "MDRW2100020535.1.4",
          "form": "불닭 좋아하시겠어요 그럼",
          "original_form": "불닭 좋아하시겠어요 그럼",
          "speaker_id": "1",
          "time": "20210828 09:19"
        },
        {
          "id": "MDRW2100020535.1.5",
          "form": "아하ㅋㅋㅋ 저도 딱 그래요ㅋㅋㅋ",
          "original_form": "아하ㅋㅋㅋ 저도 딱 그래요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:19"
        },
        {
          "id": "MDRW2100020535.1.6",
          "form": "불닭볶음면 좋아합니다",
          "original_form": "불닭볶음면 좋아합니다",
          "speaker_id": "2",
          "time": "20210828 09:19"
        },
        {
          "id": "MDRW2100020535.1.7",
          "form": "근데 엄청 열심히 비비진 않고",
          "original_form": "근데 엄청 열심히 비비진 않고",
          "speaker_id": "2",
          "time": "20210828 09:19"
        },
        {
          "id": "MDRW2100020535.1.8",
          "form": "좀 듬성듬성 비벼먹어야해요",
          "original_form": "좀 듬성듬성 비벼먹어야해요",
          "speaker_id": "2",
          "time": "20210828 09:19"
        },
        {
          "id": "MDRW2100020535.1.9",
          "form": "으음",
          "original_form": "으음",
          "speaker_id": "1",
          "time": "20210828 09:19"
        },
        {
          "id": "MDRW2100020535.1.10",
          "form": "약간 그 섞인과 안섞인곳의 조화",
          "original_form": "약간 그 섞인과 안섞인곳의 조화",
          "speaker_id": "1",
          "time": "20210828 09:20"
        },
        {
          "id": "MDRW2100020535.1.11",
          "form": "뭔지압니다",
          "original_form": "뭔지압니다",
          "speaker_id": "1",
          "time": "20210828 09:20"
        },
        {
          "id": "MDRW2100020535.1.12",
          "form": "그렇답니다",
          "original_form": "그렇답니다",
          "speaker_id": "2",
          "time": "20210828 09:20"
        },
        {
          "id": "MDRW2100020535.1.13",
          "form": "치즈 추가해서 드시나요",
          "original_form": "치즈 추가해서 드시나요",
          "speaker_id": "1",
          "time": "20210828 09:20"
        },
        {
          "id": "MDRW2100020535.1.14",
          "form": "매움에 강약이 있어야해요",
          "original_form": "매움에 강약이 있어야해요",
          "speaker_id": "2",
          "time": "20210828 09:20"
        },
        {
          "id": "MDRW2100020535.1.15",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 09:20"
        },
        {
          "id": "MDRW2100020535.1.16",
          "form": "치즈가 옆에있다면 거의 넣습니다",
          "original_form": "치즈가 옆에있다면 거의 넣습니다",
          "speaker_id": "2",
          "time": "20210828 09:20"
        },
        {
          "id": "MDRW2100020535.1.17",
          "form": "너무 맵기만하면 못먹어요",
          "original_form": "너무 맵기만하면 못먹어요",
          "speaker_id": "1",
          "time": "20210828 09:20"
        },
        {
          "id": "MDRW2100020535.1.18",
          "form": "저 불닭을 처음 먹던 때가 생각납니다",
          "original_form": "저 불닭을 처음 먹던 때가 생각납니다",
          "speaker_id": "1",
          "time": "20210828 09:20"
        },
        {
          "id": "MDRW2100020535.1.19",
          "form": "맞아요 혀고문..ㅠ",
          "original_form": "맞아요 혀고문..ㅠ",
          "speaker_id": "2",
          "time": "20210828 09:20"
        },
        {
          "id": "MDRW2100020535.1.20",
          "form": "어땠나요 깜놀했죠",
          "original_form": "어땠나요 깜놀했죠",
          "speaker_id": "2",
          "time": "20210828 09:20"
        },
        {
          "id": "MDRW2100020535.1.21",
          "form": "때는 불닭이 유명하지않던시절",
          "original_form": "때는 불닭이 유명하지않던시절",
          "speaker_id": "1",
          "time": "20210828 09:20"
        },
        {
          "id": "MDRW2100020535.1.22",
          "form": "닭이라는 단어하나보고 집어왔어요",
          "original_form": "닭이라는 단어하나보고 집어왔어요",
          "speaker_id": "1",
          "time": "20210828 09:21"
        },
        {
          "id": "MDRW2100020535.1.23",
          "form": "전 닭을 좋아하니까",
          "original_form": "전 닭을 좋아하니까",
          "speaker_id": "1",
          "time": "20210828 09:21"
        },
        {
          "id": "MDRW2100020535.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ아니.. 불자는 어디 떼놓고..",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아니.. 불자는 어디 떼놓고..",
          "speaker_id": "2",
          "time": "20210828 09:21"
        },
        {
          "id": "MDRW2100020535.1.25",
          "form": "근데 먹고 뭐 이런 미친맛이있나",
          "original_form": "근데 먹고 뭐 이런 미친맛이있나",
          "speaker_id": "1",
          "time": "20210828 09:21"
        },
        {
          "id": "MDRW2100020535.1.26",
          "form": "그니까요ㅋㅋㅋ",
          "original_form": "그니까요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 09:21"
        },
        {
          "id": "MDRW2100020535.1.27",
          "form": "지가 매워봤자 얼마나맵다고",
          "original_form": "지가 매워봤자 얼마나맵다고",
          "speaker_id": "1",
          "time": "20210828 09:21"
        },
        {
          "id": "MDRW2100020535.1.28",
          "form": "외국인들 막 집어와서 놀라는거랑 비슷하네요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "외국인들 막 집어와서 놀라는거랑 비슷하네요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 09:21"
        },
        {
          "id": "MDRW2100020535.1.29",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 09:21"
        },
        {
          "id": "MDRW2100020535.1.30",
          "form": "하면서 끓였다가 울었어요",
          "original_form": "하면서 끓였다가 울었어요",
          "speaker_id": "1",
          "time": "20210828 09:21"
        },
        {
          "id": "MDRW2100020535.1.31",
          "form": "저도 첨에 그런 생각을 했었어요",
          "original_form": "저도 첨에 그런 생각을 했었어요",
          "speaker_id": "2",
          "time": "20210828 09:21"
        },
        {
          "id": "MDRW2100020535.1.32",
          "form": "그쵸ㅋㅋㅋ 다 비슷한가봐요ㅋㅋㅋ",
          "original_form": "그쵸ㅋㅋㅋ 다 비슷한가봐요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 09:22"
        },
        {
          "id": "MDRW2100020535.1.33",
          "form": "매워봤자 얼마나 맵겠냐...",
          "original_form": "매워봤자 얼마나 맵겠냐...",
          "speaker_id": "2",
          "time": "20210828 09:22"
        },
        {
          "id": "MDRW2100020535.1.34",
          "form": "큰코다쳤죠",
          "original_form": "큰코다쳤죠",
          "speaker_id": "2",
          "time": "20210828 09:22"
        },
        {
          "id": "MDRW2100020535.1.35",
          "form": "지가 매워봤자시",
          "original_form": "지가 매워봤자시",
          "speaker_id": "1",
          "time": "20210828 09:22"
        }
      ]
