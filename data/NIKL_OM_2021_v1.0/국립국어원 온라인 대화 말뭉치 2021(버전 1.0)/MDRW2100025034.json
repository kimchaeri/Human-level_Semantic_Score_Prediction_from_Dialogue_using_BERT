      "utterance": [
        {
          "id": "MDRW2100025034.1.1",
          "form": "후후 저는 부대찌개의 고장에서 나고 자라",
          "original_form": "후후 저는 부대찌개의 고장에서 나고 자라",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100025034.1.2",
          "form": "스팸하나 살짝 넣어서....",
          "original_form": "스팸하나 살짝 넣어서....{emoji:☺}",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100025034.1.3",
          "form": "웬만한 부대찌개엔 감동하지 않습니다",
          "original_form": "웬만한 부대찌개엔 감동하지 않습니다",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100025034.1.4",
          "form": "오...의정부?",
          "original_form": "오...의정부?",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100025034.1.5",
          "form": "의정부라녀!",
          "original_form": "의정부라녀!",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100025034.1.6",
          "form": "아닌가요?????",
          "original_form": "아닌가요?????",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100025034.1.7",
          "form": "송탄입니당",
          "original_form": "송탄입니당",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100025034.1.8",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100025034.1.9",
          "form": "송탄",
          "original_form": "송탄",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100025034.1.10",
          "form": "이나경부대찌개",
          "original_form": "이나경부대찌개",
          "speaker_id": "2",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100025034.1.11",
          "form": "그게 뭐져..?",
          "original_form": "그게 뭐져..?",
          "speaker_id": "1",
          "time": "20210831 22:31"
        },
        {
          "id": "MDRW2100025034.1.12",
          "form": "거기 완전유명하던데...",
          "original_form": "거기 완전유명하던데...",
          "speaker_id": "2",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100025034.1.13",
          "form": "엇 저는 처음 들어요",
          "original_form": "엇 저는 처음 들어요",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100025034.1.14",
          "form": "송탄이나경부대찌개라고 부대찌개 성지중하나라고하더라구요",
          "original_form": "송탄이나경부대찌개라고 부대찌개 성지중하나라고하더라구요",
          "speaker_id": "2",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100025034.1.15",
          "form": "그래용..?",
          "original_form": "그래용..?",
          "speaker_id": "1",
          "time": "20210831 22:32"
        },
        {
          "id": "MDRW2100025034.1.16",
          "form": "ㅋㄱㅋㅋㅋㄱ그 부대찌개 완전좋아하는",
          "original_form": "ㅋㄱㅋㅋㅋㄱ그 부대찌개 완전좋아하는",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100025034.1.17",
          "form": "송탄하면",
          "original_form": "송탄하면",
          "speaker_id": "1",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100025034.1.18",
          "form": "김네집인데..!!",
          "original_form": "김네집인데..!!",
          "speaker_id": "1",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100025034.1.19",
          "form": "대한미국인이라고 있는데",
          "original_form": "대한미국인이라고 있는데",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100025034.1.20",
          "form": "그사람도 송탄이나경갔다왔다고 막 인증올리고 그럴정도...",
          "original_form": "그사람도 송탄이나경갔다왔다고 막 인증올리고 그럴정도...",
          "speaker_id": "2",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100025034.1.21",
          "form": "아 누군지 알아요",
          "original_form": "아 누군지 알아요",
          "speaker_id": "1",
          "time": "20210831 22:33"
        },
        {
          "id": "MDRW2100025034.1.22",
          "form": "김네집...저장",
          "original_form": "김네집...저장",
          "speaker_id": "2",
          "time": "20210831 22:33"
        }
      ]
