      "utterance": [
        {
          "id": "MDRW2100031557.1.1",
          "form": "주말인데 맛있는거 드셨나요?",
          "original_form": "주말인데 맛있는거 드셨나요?",
          "speaker_id": "1",
          "time": "20210904 22:43"
        },
        {
          "id": "MDRW2100031557.1.2",
          "form": "저는 아침으로 유부초밥을 먹었어요!",
          "original_form": "저는 아침으로 유부초밥을 먹었어요!",
          "speaker_id": "2",
          "time": "20210905 10:20"
        },
        {
          "id": "MDRW2100031557.1.3",
          "form": "아침드셨나요?",
          "original_form": "아침드셨나요?",
          "speaker_id": "2",
          "time": "20210905 10:20"
        },
        {
          "id": "MDRW2100031557.1.4",
          "form": "저... 지금 일어났어요... ㅎㅎ",
          "original_form": "저... 지금 일어났어요... ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100031557.1.5",
          "form": "이제 걍 점심 먹어야죠... ㅋㅋㅋ",
          "original_form": "이제 걍 점심 먹어야죠... ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100031557.1.6",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 12:11"
        },
        {
          "id": "MDRW2100031557.1.7",
          "form": "점심은 어떤 메뉴드실건가요?",
          "original_form": "점심은 어떤 메뉴드실건가요?",
          "speaker_id": "2",
          "time": "20210905 12:11"
        },
        {
          "id": "MDRW2100031557.1.8",
          "form": "음..  원래 아침에ㅋㅋㅋ생선을 구워먹으려고 했어요",
          "original_form": "음..  원래 아침에ㅋㅋㅋ생선을 구워먹으려고 했어요",
          "speaker_id": "1",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100031557.1.9",
          "form": "그거 먹으려구요ㅋㅋㅋ",
          "original_form": "그거 먹으려구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:15"
        },
        {
          "id": "MDRW2100031557.1.10",
          "form": "생선 맛있죠!!",
          "original_form": "생선 맛있죠!!",
          "speaker_id": "2",
          "time": "20210905 12:23"
        },
        {
          "id": "MDRW2100031557.1.11",
          "form": "요즘 전어철인가요? 갈치낚시도 많이 보이던데요",
          "original_form": "요즘 전어철인가요? 갈치낚시도 많이 보이던데요",
          "speaker_id": "2",
          "time": "20210905 12:23"
        },
        {
          "id": "MDRW2100031557.1.12",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 12:23"
        },
        {
          "id": "MDRW2100031557.1.13",
          "form": "무슨생선드세요?",
          "original_form": "무슨생선드세요?",
          "speaker_id": "2",
          "time": "20210905 12:23"
        },
        {
          "id": "MDRW2100031557.1.14",
          "form": "옥돔이 선물로 들어와서 그거 먹으려구요ㅋㅋㄱ",
          "original_form": "옥돔이 선물로 들어와서 그거 먹으려구요ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210905 12:25"
        },
        {
          "id": "MDRW2100031557.1.15",
          "form": "낚시하세요?",
          "original_form": "낚시하세요?",
          "speaker_id": "1",
          "time": "20210905 12:25"
        },
        {
          "id": "MDRW2100031557.1.16",
          "form": "와 옥돔이라니 ㅠㅠ 귀한거네요",
          "original_form": "와 옥돔이라니 ㅠㅠ 귀한거네요",
          "speaker_id": "2",
          "time": "20210905 12:32"
        },
        {
          "id": "MDRW2100031557.1.17",
          "form": "정말맛있겠어요 회로 드세요 구워서드세요?",
          "original_form": "정말맛있겠어요 회로 드세요 구워서드세요?",
          "speaker_id": "2",
          "time": "20210905 12:32"
        },
        {
          "id": "MDRW2100031557.1.18",
          "form": "낚시는 저는 안하지만 제가 사는곳이 워낙 낚시하는사람이 많아서요 ㅎㅎ",
          "original_form": "낚시는 저는 안하지만 제가 사는곳이 워낙 낚시하는사람이 많아서요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 12:32"
        },
        {
          "id": "MDRW2100031557.1.19",
          "form": "옥돔이 생물로 온게 아니라 손질해서 진공포장되서 온 그런거였거든요",
          "original_form": "옥돔이 생물로 온게 아니라 손질해서 진공포장되서 온 그런거였거든요",
          "speaker_id": "1",
          "time": "20210905 12:35"
        },
        {
          "id": "MDRW2100031557.1.20",
          "form": "구워먹을수 밖에요 ㅋㅋㅋ",
          "original_form": "구워먹을수 밖에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:37"
        },
        {
          "id": "MDRW2100031557.1.21",
          "form": "아 그런거군요",
          "original_form": "아 그런거군요",
          "speaker_id": "2",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100031557.1.22",
          "form": "너무 맛있었겠어요",
          "original_form": "너무 맛있었겠어요",
          "speaker_id": "2",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100031557.1.23",
          "form": "돔은 어떻게 먹어도 최고의 맛이죠!!",
          "original_form": "돔은 어떻게 먹어도 최고의 맛이죠!!",
          "speaker_id": "2",
          "time": "20210905 16:21"
        },
        {
          "id": "MDRW2100031557.1.24",
          "form": "네, 맛있더라구요 ㅋㅋㅋ",
          "original_form": "네, 맛있더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100031557.1.25",
          "form": "점심 드셨나요? ㅋㅋㅋ",
          "original_form": "점심 드셨나요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 16:43"
        },
        {
          "id": "MDRW2100031557.1.26",
          "form": "네 저는 초밥먹었어요ㅎㅎ",
          "original_form": "네 저는 초밥먹었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 19:22"
        },
        {
          "id": "MDRW2100031557.1.27",
          "form": "저녁은 뭐드실건가요?",
          "original_form": "저녁은 뭐드실건가요?",
          "speaker_id": "2",
          "time": "20210905 19:22"
        },
        {
          "id": "MDRW2100031557.1.28",
          "form": "저녁 방금 먹었는데",
          "original_form": "저녁 방금 먹었는데",
          "speaker_id": "1",
          "time": "20210905 20:02"
        },
        {
          "id": "MDRW2100031557.1.29",
          "form": "닭곰탕먹었어요",
          "original_form": "닭곰탕먹었어요",
          "speaker_id": "1",
          "time": "20210905 20:02"
        },
        {
          "id": "MDRW2100031557.1.30",
          "form": "그쪽은요?",
          "original_form": "그쪽은요?",
          "speaker_id": "1",
          "time": "20210905 20:02"
        },
        {
          "id": "MDRW2100031557.1.31",
          "form": "그러셨군요",
          "original_form": "그러셨군요",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100031557.1.32",
          "form": "저는 떡갈비 먹었어요",
          "original_form": "저는 떡갈비 먹었어요",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100031557.1.33",
          "form": "마트에서 완제품 사서요 ㅎㅎ",
          "original_form": "마트에서 완제품 사서요 ㅎㅎ",
          "speaker_id": "2",
