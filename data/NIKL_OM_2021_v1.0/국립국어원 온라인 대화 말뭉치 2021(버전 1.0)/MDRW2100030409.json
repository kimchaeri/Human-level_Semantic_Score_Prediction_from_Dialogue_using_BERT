        {
          "id": "MDRW2100030409.1.1",
          "form": "음성 인식 편하네요",
          "original_form": "음성 인식 편하네요",
          "speaker_id": "2",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030409.1.2",
          "form": "이야기가 빨리 진행되네요",
          "original_form": "이야기가 빨리 진행되네요",
          "speaker_id": "1",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030409.1.3",
          "form": "먹부림 할 수 있는 곳으로 여행 가고 싶어요",
          "original_form": "먹부림 할 수 있는 곳으로 여행 가고 싶어요",
          "speaker_id": "2",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030409.1.4",
          "form": "진작할 것",
          "original_form": "진작할 것",
          "speaker_id": "1",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030409.1.5",
          "form": "남천동에 빵집 아시나요",
          "original_form": "남천동에 빵집 아시나요",
          "speaker_id": "1",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030409.1.6",
          "form": "빵천동",
          "original_form": "빵천동",
          "speaker_id": "1",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030409.1.7",
          "form": "들어는 본 거 같은데",
          "original_form": "들어는 본 거 같은데",
          "speaker_id": "2",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030409.1.8",
          "form": "빵 먹으러 투어 간대요",
          "original_form": "빵 먹으러 투어 간대요",
          "speaker_id": "1",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030409.1.9",
          "form": "뭐가 유명한 곳이죠",
          "original_form": "뭐가 유명한 곳이죠",
          "speaker_id": "2",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030409.1.10",
          "form": "빵 맛집이 많대요",
          "original_form": "빵 맛집이 많대요",
          "speaker_id": "1",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030409.1.11",
          "form": "무슨 빵이 맛있나요",
          "original_form": "무슨 빵이 맛있나요",
