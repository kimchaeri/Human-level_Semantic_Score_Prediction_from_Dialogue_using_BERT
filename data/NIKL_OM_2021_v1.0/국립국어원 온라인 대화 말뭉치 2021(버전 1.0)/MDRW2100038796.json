      "utterance": [
        {
          "id": "MDRW2100038796.1.1",
          "form": "동물 좋아하세요?",
          "original_form": "동물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210902 21:26"
        },
        {
          "id": "MDRW2100038796.1.2",
          "form": "엄청 좋아해요",
          "original_form": "엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100038796.1.3",
          "form": "근데 키운적은 없어요ㅠ",
          "original_form": "근데 키운적은 없어요ㅠ",
          "speaker_id": "2",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100038796.1.4",
          "form": "아 일거리 많아요ㅎ",
          "original_form": "아 일거리 많아요ㅎ",
          "speaker_id": "1",
          "time": "20210902 21:27"
        },
        {
          "id": "MDRW2100038796.1.5",
          "form": "털이 그렇게 많이 빠진대죠",
          "original_form": "털이 그렇게 많이 빠진대죠",
          "speaker_id": "2",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100038796.1.6",
          "form": "귀여움속에 감춰진게 많아요ㅋ",
          "original_form": "귀여움속에 감춰진게 많아요ㅋ",
          "speaker_id": "1",
          "time": "20210902 21:50"
        },
        {
          "id": "MDRW2100038796.1.7",
          "form": "ㅋㅋ 맞네요",
          "original_form": "ㅋㅋ 맞네요",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038796.1.8",
          "form": "넘 귀여워서 참아요ㅎ",
          "original_form": "넘 귀여워서 참아요ㅎ",
          "speaker_id": "1",
          "time": "20210902 21:59"
        },
        {
          "id": "MDRW2100038796.1.9",
          "form": "그만큼 귀여운가 봐요",
          "original_form": "그만큼 귀여운가 봐요",
          "speaker_id": "2",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100038796.1.10",
          "form": "저도 귀여운 친구랑 같이 살고 싶은데ㅠ",
          "original_form": "저도 귀여운 친구랑 같이 살고 싶은데ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:00"
        },
        {
          "id": "MDRW2100038796.1.11",
          "form": "근데 고민많이해야할것같아요",
          "original_form": "근데 고민많이해야할것같아요",
          "speaker_id": "1",
          "time": "20210902 22:15"
        },
        {
          "id": "MDRW2100038796.1.12",
          "form": "공부도 많이 해야겠죠",
          "original_form": "공부도 많이 해야겠죠",
          "speaker_id": "2",
          "time": "20210902 22:19"
        },
        {
          "id": "MDRW2100038796.1.13",
          "form": "생각보다 일이많더라구오",
          "original_form": "생각보다 일이많더라구오",
          "speaker_id": "1",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100038796.1.14",
          "form": "돈도 많이들고",
          "original_form": "돈도 많이들고",
          "speaker_id": "1",
          "time": "20210902 22:56"
        },
        {
          "id": "MDRW2100038796.1.15",
          "form": "지금으로서는 전 못키우겠네요ㅠ",
          "original_form": "지금으로서는 전 못키우겠네요ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:58"
        },
        {
          "id": "MDRW2100038796.1.16",
          "form": "에구ㅠ",
          "original_form": "에구ㅠ",
          "speaker_id": "1",
