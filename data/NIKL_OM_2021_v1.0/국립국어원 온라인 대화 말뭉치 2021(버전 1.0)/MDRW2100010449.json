      "utterance": [
        {
          "id": "MDRW2100010449.1.1",
          "form": "청소 해야하는데 하기 귀찮네요..",
          "original_form": "청소 해야하는데 하기 귀찮네요..",
          "speaker_id": "2",
          "time": "20210823 18:27"
        },
        {
          "id": "MDRW2100010449.1.2",
          "form": "하...저도요...저도 넘...넘 귀찮아요",
          "original_form": "하...저도요...저도 넘...넘 귀찮아요",
          "speaker_id": "1",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100010449.1.3",
          "form": "설거지가 특히.",
          "original_form": "설거지가 특히.",
          "speaker_id": "1",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100010449.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:29"
        },
        {
          "id": "MDRW2100010449.1.5",
          "form": "헐 인정이요",
          "original_form": "헐 인정이요",
          "speaker_id": "2",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100010449.1.6",
          "form": "설거지 빨래 널기 개기",
          "original_form": "설거지 빨래 널기 개기",
          "speaker_id": "2",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100010449.1.7",
          "form": "이런게 너무 귀찮아요",
          "original_form": "이런게 너무 귀찮아요",
          "speaker_id": "2",
          "time": "20210823 18:31"
        },
        {
          "id": "MDRW2100010449.1.8",
          "form": "하 헐 맞아요!!!!!!!!!!",
          "original_form": "하 헐 맞아요!!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210823 18:53"
        },
        {
          "id": "MDRW2100010449.1.9",
          "form": "그리고 가끔 선반 위 먼지 청소 진짜..너무귀찮아요",
          "original_form": "그리고 가끔 선반 위 먼지 청소 진짜..너무귀찮아요",
          "speaker_id": "1",
          "time": "20210823 18:53"
        },
        {
          "id": "MDRW2100010449.1.10",
          "form": "앗 인정요 ㅎ 먼지는 왜이리",
          "original_form": "앗 인정요 ㅎ 먼지는 왜이리",
          "speaker_id": "2",
          "time": "20210823 18:55"
        },
        {
          "id": "MDRW2100010449.1.11",
          "form": "빨리 쌓이는지 모르겠어요",
          "original_form": "빨리 쌓이는지 모르겠어요",
          "speaker_id": "2",
          "time": "20210823 18:55"
        },
        {
          "id": "MDRW2100010449.1.12",
          "form": "1일 1먼지...",
          "original_form": "1일 1먼지...",
          "speaker_id": "2",
          "time": "20210823 18:55"
        },
        {
          "id": "MDRW2100010449.1.13",
          "form": "하 진짜요ㅠ 그리고 바닥 머리카락도........",
          "original_form": "하 진짜요ㅠ 그리고 바닥 머리카락도........",
          "speaker_id": "1",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100010449.1.14",
          "form": "이게 제일 짜증나요....",
          "original_form": "이게 제일 짜증나요....",
          "speaker_id": "1",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100010449.1.15",
          "form": "청소는 정말 공감대라 풍성하네요 ㅎ",
          "original_form": "청소는 정말 공감대라 풍성하네요 ㅎ",
          "speaker_id": "2",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100010449.1.16",
          "form": "청소를 해도 티가 나지 않아서",
          "original_form": "청소를 해도 티가 나지 않아서",
          "speaker_id": "2",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100010449.1.17",
          "form": "너무 싫어요 하하",
          "original_form": "너무 싫어요 하하",
          "speaker_id": "2",
          "time": "20210823 18:56"
        },
        {
          "id": "MDRW2100010449.1.18",
          "form": "아 씁 맞아요!!!!!!!!",
          "original_form": "아 씁 맞아요!!!!!!!!",
          "speaker_id": "1",
          "time": "20210823 19:56"
        },
        {
          "id": "MDRW2100010449.1.19",
          "form": "집안일 특징 : 나는 힘든데 일한 티가 안남",
          "original_form": "집안일 특징 : 나는 힘든데 일한 티가 안남",
          "speaker_id": "1",
          "time": "20210823 19:56"
        },
        {
          "id": "MDRW2100010449.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 특징이 아주 정확합니다",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 특징이 아주 정확합니다",
          "speaker_id": "2",
          "time": "20210823 20:04"
        },
        {
          "id": "MDRW2100010449.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 20:10"
        },
        {
          "id": "MDRW2100010449.1.22",
          "form": "청소중에 화장실 청소가 제일 티가 안나죠...",
          "original_form": "청소중에 화장실 청소가 제일 티가 안나죠...",
          "speaker_id": "1",
          "time": "20210823 20:11"
        },
        {
          "id": "MDRW2100010449.1.23",
          "form": "앗 화장실 물 때 정말 답이 없죠  ",
          "original_form": "앗 화장실 물 때 정말 답이 없죠  ",
          "speaker_id": "2",
          "time": "20210823 20:12"
        },
        {
          "id": "MDRW2100010449.1.24",
          "form": "와 진짜 관리 조금만 안해줘도 티가 나는데 진짜....짜증나요ㅠ",
          "original_form": "와 진짜 관리 조금만 안해줘도 티가 나는데 진짜....짜증나요ㅠ",
          "speaker_id": "1",
          "time": "20210823 20:15"
        },
        {
          "id": "MDRW2100010449.1.25",
          "form": "전주인이 곰팡이 청소 한번도 안한 집에서 산 적 있는데 진짜 청소하다가 얼마나 욕했는지 몰라요....",
          "original_form": "전주인이 곰팡이 청소 한번도 안한 집에서 산 적 있는데 진짜 청소하다가 얼마나 욕했는지 몰라요....",
          "speaker_id": "1",
          "time": "20210823 20:15"
        },
        {
          "id": "MDRW2100010449.1.26",
          "form": "헉 전주인은 어떻게 산건지...",
          "original_form": "헉 전주인은 어떻게 산건지...",
