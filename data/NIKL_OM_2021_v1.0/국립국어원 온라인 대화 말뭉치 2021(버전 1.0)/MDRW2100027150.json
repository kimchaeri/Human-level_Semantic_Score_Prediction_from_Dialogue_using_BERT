        },
        {
          "id": "MDRW2100027150.1.2",
          "form": "전 시리얼이오",
          "original_form": "전 시리얼이오",
          "speaker_id": "1",
          "time": "20210902 10:01"
        },
        {
          "id": "MDRW2100027150.1.3",
          "form": "전 팥물 마시고있어요",
          "original_form": "전 팥물 마시고있어요",
          "speaker_id": "2",
          "time": "20210902 10:03"
        },
        {
          "id": "MDRW2100027150.1.4",
          "form": "맛나요?",
          "original_form": "맛나요?",
          "speaker_id": "1",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027150.1.5",
          "form": "다이어트하려고마시는거죠뭐",
          "original_form": "다이어트하려고마시는거죠뭐",
          "speaker_id": "2",
          "time": "20210902 10:09"
        },
        {
          "id": "MDRW2100027150.1.6",
          "form": "효과는용?",
          "original_form": "효과는용?",
          "speaker_id": "1",
          "time": "20210902 10:23"
        },
        {
          "id": "MDRW2100027150.1.7",
          "form": "오늘 처음마시는거라 ㅎㅎ",
          "original_form": "오늘 처음마시는거라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 10:25"
        },
        {
          "id": "MDRW2100027150.1.8",
          "form": "아 ㅋㅋ",
          "original_form": "아 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:28"
        },
        {
          "id": "MDRW2100027150.1.9",
          "form": "맛은 나쁘지않아요",
          "original_form": "맛은 나쁘지않아요",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100027150.1.10",
          "form": "오 만드는 것 어렵죠?",
          "original_form": "오 만드는 것 어렵죠?",
          "speaker_id": "1",
          "time": "20210902 10:39"
        },
        {
          "id": "MDRW2100027150.1.11",
          "form": "음 그냥 차끓이듯이 끓이면되서",
          "original_form": "음 그냥 차끓이듯이 끓이면되서",
          "speaker_id": "2",
          "time": "20210902 10:45"
        },
        {
          "id": "MDRW2100027150.1.12",
          "form": "쉽네여",
          "original_form": "쉽네여",
          "speaker_id": "1",
          "time": "20210902 10:49"
        },
        {
          "id": "MDRW2100027150.1.13",
