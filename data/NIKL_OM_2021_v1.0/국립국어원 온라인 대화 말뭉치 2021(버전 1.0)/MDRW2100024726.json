      "utterance": [
        {
          "id": "MDRW2100024726.1.1",
          "form": "제일 기억에 남는 여행지 있으세여?",
          "original_form": "제일 기억에 남는 여행지 있으세여?",
          "speaker_id": "1",
          "time": "20210831 17:36"
        },
        {
          "id": "MDRW2100024726.1.2",
          "form": "저는 보라카이가 기억에 남아요",
          "original_form": "저는 보라카이가 기억에 남아요",
          "speaker_id": "2",
          "time": "20210831 17:36"
        },
        {
          "id": "MDRW2100024726.1.3",
          "form": "빨간머리앤님은요?",
          "original_form": "빨간머리앤님은요?",
          "speaker_id": "2",
          "time": "20210831 17:37"
        },
        {
          "id": "MDRW2100024726.1.4",
          "form": "와 보라카이",
          "original_form": "와 보라카이",
          "speaker_id": "1",
          "time": "20210831 17:47"
        },
        {
          "id": "MDRW2100024726.1.5",
          "form": "코로나이전의 여행 추억이지요ㅎㅎ",
          "original_form": "코로나이전의 여행 추억이지요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 17:50"
        },
        {
          "id": "MDRW2100024726.1.6",
          "form": "저는 싱가포르요!",
          "original_form": "저는 싱가포르요!",
          "speaker_id": "1",
          "time": "20210831 17:51"
        },
        {
          "id": "MDRW2100024726.1.7",
          "form": "싱가포르 너무 좋았겠다ㅎㅎ",
          "original_form": "싱가포르 너무 좋았겠다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 17:52"
        },
        {
          "id": "MDRW2100024726.1.8",
          "form": "싱가포르 중 어디가 가장 좋았어요?",
          "original_form": "싱가포르 중 어디가 가장 좋았어요?",
          "speaker_id": "2",
          "time": "20210831 17:52"
        },
        {
          "id": "MDRW2100024726.1.9",
          "form": "저는 거기 섬이 이름이 생각안나는데 재밌었어요",
          "original_form": "저는 거기 섬이 이름이 생각안나는데 재밌었어요",
          "speaker_id": "1",
          "time": "20210831 18:14"
        },
        {
          "id": "MDRW2100024726.1.10",
          "form": "짚라인도 타고",
          "original_form": "짚라인도 타고",
          "speaker_id": "1",
          "time": "20210831 18:14"
        },
        {
          "id": "MDRW2100024726.1.11",
          "form": "오ㅋㅋ재밌었겠다ㅋㅋ",
          "original_form": "오ㅋㅋ재밌었겠다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:15"
        },
        {
          "id": "MDRW2100024726.1.12",
          "form": "혹시 그 유명한 사자인어상,ㅈ",
          "original_form": "혹시 그 유명한 사자인어상,ㅈ",
          "speaker_id": "2",
          "time": "20210831 18:15"
        },
        {
          "id": "MDRW2100024726.1.13",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210831 18:15"
        },
        {
          "id": "MDRW2100024726.1.14",
          "form": "맞아요!!!!",
          "original_form": "맞아요!!!!",
          "speaker_id": "1",
          "time": "20210831 18:15"
        },
        {
          "id": "MDRW2100024726.1.15",
          "form": "거기가 싱가폴이죠?",
          "original_form": "거기가 싱가폴이죠?",
          "speaker_id": "2",
          "time": "20210831 18:15"
        },
        {
          "id": "MDRW2100024726.1.16",
          "form": "아 생각났다",
          "original_form": "아 생각났다",
          "speaker_id": "1",
          "time": "20210831 18:15"
        },
        {
          "id": "MDRW2100024726.1.17",
          "form": "센토사?",
          "original_form": "센토사?",
          "speaker_id": "1",
          "time": "20210831 18:15"
        },
        {
          "id": "MDRW2100024726.1.18",
          "form": "였던거같아요",
          "original_form": "였던거같아요",
          "speaker_id": "1",
          "time": "20210831 18:15"
        },
        {
          "id": "MDRW2100024726.1.19",
          "form": "오ㅋㅋ역시 ㅋㅋ랜드마크ㅋㅋ",
          "original_form": "오ㅋㅋ역시 ㅋㅋ랜드마크ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 18:15"
        },
        {
          "id": "MDRW2100024726.1.20",
          "form": "저는 해변이 제일 좋았어요ㅋ",
          "original_form": "저는 해변이 제일 좋았어요ㅋ",
          "speaker_id": "2",
          "time": "20210831 18:16"
        },
        {
          "id": "MDRW2100024726.1.21",
          "form": "네네 ㅋㅋ혼자 잘 싸돌아다니는",
          "original_form": "네네 ㅋㅋ혼자 잘 싸돌아다니는",
          "speaker_id": "1",
          "time": "20210831 18:21"
        },
        {
          "id": "MDRW2100024726.1.22",
          "form": "보라카이는 어때요?",
          "original_form": "보라카이는 어때요?",
          "speaker_id": "1",
          "time": "20210831 18:21"
        },
        {
          "id": "MDRW2100024726.1.23",
          "form": "보라카이 겨울에 갔거든요",
          "original_form": "보라카이 겨울에 갔거든요",
          "speaker_id": "2",
          "time": "20210831 18:21"
        },
        {
          "id": "MDRW2100024726.1.24",
          "form": "겨울 추울때마다 보라카이의 해변이 떠올라요",
          "original_form": "겨울 추울때마다 보라카이의 해변이 떠올라요",
          "speaker_id": "2",
          "time": "20210831 18:22"
        }
      ]
    }
