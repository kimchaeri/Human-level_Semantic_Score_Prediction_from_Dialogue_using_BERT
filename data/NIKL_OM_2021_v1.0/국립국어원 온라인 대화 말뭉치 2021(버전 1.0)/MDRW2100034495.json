      "utterance": [
        {
          "id": "MDRW2100034495.1.1",
          "form": "오리고기는 주로 간 안하고 구워서 드세요?",
          "original_form": "오리고기는 주로 간 안하고 구워서 드세요?",
          "speaker_id": "1",
          "time": "20210828 13:47"
        },
        {
          "id": "MDRW2100034495.1.2",
          "form": "네~ 오리고기랑 양파랑 부추랑 구워서 같이먹어용 ㅎㅎ",
          "original_form": "네~ 오리고기랑 양파랑 부추랑 구워서 같이먹어용 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 14:14"
        },
        {
          "id": "MDRW2100034495.1.3",
          "form": "간 없이도 맛있어요!",
          "original_form": "간 없이도 맛있어요!",
          "speaker_id": "2",
          "time": "20210828 14:15"
        },
        {
          "id": "MDRW2100034495.1.4",
          "form": "담백하게 드시는구나",
          "original_form": "담백하게 드시는구나",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100034495.1.5",
          "form": "전 주로 주물럭으로 먹어요 ㅋㅋㅋㅋ",
          "original_form": "전 주로 주물럭으로 먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:20"
        },
        {
          "id": "MDRW2100034495.1.6",
          "form": "그것두 맛있을것 같아요! ㅎㅎ 저는 주물럭으로는 안먹어본것 같아요",
          "original_form": "그것두 맛있을것 같아요! ㅎㅎ 저는 주물럭으로는 안먹어본것 같아요",
          "speaker_id": "2",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100034495.1.7",
          "form": "하지만 지금은 오리고기 넘나 끔찍 ㅠㅠㅠ 진짜 체해서 죽겠어요ㅠㅠ",
          "original_form": "하지만 지금은 오리고기 넘나 끔찍 ㅠㅠㅠ 진짜 체해서 죽겠어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100034495.1.8",
          "form": "ㅠㅠ아직도 체기가 남아있어요??",
          "original_form": "ㅠㅠ아직도 체기가 남아있어요??",
          "speaker_id": "1",
          "time": "20210828 15:02"
        },
        {
          "id": "MDRW2100034495.1.9",
          "form": "남은 정도가 아니에요ㅠㅜ",
          "original_form": "남은 정도가 아니에요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100034495.1.10",
          "form": "두통에 속쓰림에.. 당분간 오리고기 안먹습니다",
          "original_form": "두통에 속쓰림에.. 당분간 오리고기 안먹습니다",
          "speaker_id": "2",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100034495.1.11",
          "form": "너무 힘들어요ㅠㅜ",
          "original_form": "너무 힘들어요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100034495.1.12",
          "form": "ㅠㅠ병원다녀오셧어요??",
          "original_form": "ㅠㅠ병원다녀오셧어요??",
          "speaker_id": "1",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100034495.1.13",
          "form": "토욜이라 병원 일찍 닫아서 못갔어요ㅜㅜ으어어...",
          "original_form": "토욜이라 병원 일찍 닫아서 못갔어요ㅜㅜ으어어...",
          "speaker_id": "2",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100034495.1.14",
          "form": "ㅠㅠ아이고",
          "original_form": "ㅠㅠ아이고",
          "speaker_id": "1",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100034495.1.15",
          "form": "응급실이라도 다녀와야 하는거 아니에요?",
          "original_form": "응급실이라도 다녀와야 하는거 아니에요?",
          "speaker_id": "1",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100034495.1.16",
          "form": "저번에도 오리고기 먹고 체했었는데 또르르",
          "original_form": "저번에도 오리고기 먹고 체했었는데 또르르",
          "speaker_id": "2",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100034495.1.17",
          "form": "단단히 체하신것 같은데 ㅠㅠ",
          "original_form": "단단히 체하신것 같은데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100034495.1.18",
          "form": "아니에요 ㅎㅎㅎ 좀 참아보져....",
          "original_form": "아니에요 ㅎㅎㅎ 좀 참아보져....",
          "speaker_id": "2",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100034495.1.19",
          "form": "무슨 죽을 먹어야할지 고민이에요 ㅎ",
          "original_form": "무슨 죽을 먹어야할지 고민이에요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100034495.1.20",
          "form": "저희동네는 본죽이 맛있어요!",
          "original_form": "저희동네는 본죽이 맛있어요!",
          "speaker_id": "2",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100034495.1.21",
          "form": "ㅠㅠ괜찮아지실때까지 굶으셔야겟어요",
          "original_form": "ㅠㅠ괜찮아지실때까지 굶으셔야겟어요",
          "speaker_id": "1",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100034495.1.22",
          "form": "체했을땐 굶는게 직빵이던데ㅠㅠ",
          "original_form": "체했을땐 굶는게 직빵이던데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100034495.1.23",
          "form": "일단 죽은 사려구요 ㅎㅎ",
          "original_form": "일단 죽은 사려구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 15:31"
        }
      ]
    }
  ]
