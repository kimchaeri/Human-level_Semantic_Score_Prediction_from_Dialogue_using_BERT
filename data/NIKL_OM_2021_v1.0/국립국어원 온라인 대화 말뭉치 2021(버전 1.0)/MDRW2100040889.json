          "time": "20210905 13:07"
        },
        {
          "id": "MDRW2100040889.1.13",
          "form": "주문했는데",
          "original_form": "주문했는데",
          "speaker_id": "2",
          "time": "20210905 13:08"
        },
        {
          "id": "MDRW2100040889.1.14",
          "form": "한시간 걸린대서 기다리고 있어요ㅜ",
          "original_form": "한시간 걸린대서 기다리고 있어요ㅜ",
          "speaker_id": "2",
          "time": "20210905 13:08"
        },
        {
