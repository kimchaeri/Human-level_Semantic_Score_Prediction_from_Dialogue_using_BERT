        },
        {
          "id": "MDRW2100026904.1.2",
          "form": "우왕 근데 저는 스포츠를 자주안봐요 알못임(tmi",
          "original_form": "우왕 근데 저는 스포츠를 자주안봐요 알못임(tmi",
          "speaker_id": "2",
          "time": "20210902 01:06"
        },
        {
          "id": "MDRW2100026904.1.3",
          "form": "사실저도 그래요",
          "original_form": "사실저도 그래요",
          "speaker_id": "1",
          "time": "20210902 01:06"
        },
        {
