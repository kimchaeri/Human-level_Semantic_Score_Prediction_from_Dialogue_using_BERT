      "utterance": [
        {
          "id": "MDRW2100014530.1.1",
          "form": "네 전 근데 막 ㅁ좀만 힘들면하기 싫어요 ㅜㅜ",
          "original_form": "네 전 근데 막 ㅁ좀만 힘들면하기 싫어요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100014530.1.2",
          "form": "30분하고 딴거하고",
          "original_form": "30분하고 딴거하고",
          "speaker_id": "2",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100014530.1.3",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100014530.1.4",
          "form": "30분도 제입장에선 오래하시는 거예요 / 전 지금 홈트 5분이 목표에요 ㅋ",
          "original_form": "30분도 제입장에선 오래하시는 거예요 / 전 지금 홈트 5분이 목표에요 ㅋ",
          "speaker_id": "1",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014530.1.5",
          "form": "오 5분도 숨차요",
          "original_form": "오 5분도 숨차요",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014530.1.6",
          "form": "홈트 30분하면 쫙쫙빠져요 ㅋㅋ",
          "original_form": "홈트 30분하면 쫙쫙빠져요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014530.1.7",
          "form": "네 점점 시간을 늘릴려구요~",
          "original_form": "네 점점 시간을 늘릴려구요~",
          "speaker_id": "1",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014530.1.8",
          "form": "저도 다시 한다니 죽겠네요 ㅋㅋ",
          "original_form": "저도 다시 한다니 죽겠네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014530.1.9",
          "form": "걷기만 할래요 ㅋㅋ",
          "original_form": "걷기만 할래요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014530.1.10",
          "form": "홈트 엄마tv",
          "original_form": "홈트 엄마tv",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014530.1.11",
          "form": "ㅋㅋ좀쉽고 신나요",
          "original_form": "ㅋㅋ좀쉽고 신나요",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014530.1.12",
          "form": "음악에 춤요",
          "original_form": "음악에 춤요",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014530.1.13",
          "form": "절위한 엄마용 홈트요 ㅋㅋ",
          "original_form": "절위한 엄마용 홈트요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014530.1.14",
          "form": "요샌 안해서 ㅋㅋ어찌되나 몰라요",
          "original_form": "요샌 안해서 ㅋㅋ어찌되나 몰라요",
          "speaker_id": "2",
          "time": "20210825 20:30"
        },
        {
          "id": "MDRW2100014530.1.15",
          "form": "뭐보고 하세요?",
          "original_form": "뭐보고 하세요?",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014530.1.16",
          "form": "전 뭔가 보고 따라하는 게 힘들더라구요 / 땅끄부부 한번 봤었는데 못따라했어요",
          "original_form": "전 뭔가 보고 따라하는 게 힘들더라구요 / 땅끄부부 한번 봤었는데 못따라했어요",
          "speaker_id": "1",
          "time": "20210826 07:00"
        },
        {
          "id": "MDRW2100014530.1.17",
          "form": "그죠 땅끄도 너무 어려워요",
          "original_form": "그죠 땅끄도 너무 어려워요",
          "speaker_id": "2",
          "time": "20210826 07:04"
        },
        {
          "id": "MDRW2100014530.1.18",
          "form": "잘따라 하는분들 대단해요",
          "original_form": "잘따라 하는분들 대단해요",
          "speaker_id": "2",
          "time": "20210826 07:04"
        },
        {
          "id": "MDRW2100014530.1.19",
          "form": "전 엄마티비만 봤었어요",
          "original_form": "전 엄마티비만 봤었어요",
          "speaker_id": "2",
          "time": "20210826 07:04"
        },
        {
          "id": "MDRW2100014530.1.20",
          "form": "엄마티비는 안봤는데/한번 봐야겠네오 ㅎㅎ",
          "original_form": "엄마티비는 안봤는데/한번 봐야겠네오 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 07:07"
        },
        {
          "id": "MDRW2100014530.1.21",
          "form": "ㅋㅋ아줌마 위한 쉬운거예요",
          "original_form": "ㅋㅋ아줌마 위한 쉬운거예요",
          "speaker_id": "2",
          "time": "20210826 07:07"
        },
        {
          "id": "MDRW2100014530.1.22",
          "form": "근데 막상하면 힘들긴해요",
          "original_form": "근데 막상하면 힘들긴해요",
          "speaker_id": "2",
          "time": "20210826 07:07"
        },
        {
          "id": "MDRW2100014530.1.23",
          "form": "홈트 막돌리며 쉬운거 찾아해요 ㅎㅎ",
          "original_form": "홈트 막돌리며 쉬운거 찾아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 07:08"
        },
        {
          "id": "MDRW2100014530.1.24",
          "form": "전 요새 보톡스 맞고싶어요 ㅋㅋ",
          "original_form": "전 요새 보톡스 맞고싶어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 07:08"
        },
        {
          "id": "MDRW2100014530.1.25",
          "form": "미간에 주름이 너무 크게 생겼어요",
          "original_form": "미간에 주름이 너무 크게 생겼어요",
          "speaker_id": "2",
          "time": "20210826 07:09"
        },
        {
          "id": "MDRW2100014530.1.26",
          "form": "저는 팔자주름이요 ㅜ",
          "original_form": "저는 팔자주름이요 ㅜ",
          "speaker_id": "1",
          "time": "20210826 07:19"
        },
        {
          "id": "MDRW2100014530.1.27",
          "form": "아 그럼 고민돼죠",
          "original_form": "아 그럼 고민돼죠",
          "speaker_id": "2",
          "time": "20210826 07:21"
        },
        {
          "id": "MDRW2100014530.1.28",
          "form": "요새 다들 맞으셨더라구요",
          "original_form": "요새 다들 맞으셨더라구요",
          "speaker_id": "2",
          "time": "20210826 07:21"
        },
        {
          "id": "MDRW2100014530.1.29",
          "form": "한번도전해 보고 싶은데 좀 겁나요",
          "original_form": "한번도전해 보고 싶은데 좀 겁나요",
          "speaker_id": "2",
          "time": "20210826 07:21"
        },
        {
          "id": "MDRW2100014530.1.30",
          "form": "보톡스는 겁나서 못하겠고 화장품을 사용해보려구요",
          "original_form": "보톡스는 겁나서 못하겠고 화장품을 사용해보려구요",
          "speaker_id": "1",
          "time": "20210826 07:28"
        },
        {
          "id": "MDRW2100014530.1.31",
          "form": "제가 스킨로션도 잘 안바르는 타입이라 ㅋ",
          "original_form": "제가 스킨로션도 잘 안바르는 타입이라 ㅋ",
          "speaker_id": "1",
          "time": "20210826 07:28"
        },
        {
          "id": "MDRW2100014530.1.32",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 07:28"
        },
        {
          "id": "MDRW2100014530.1.33",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210826 07:28"
        },
        {
          "id": "MDRW2100014530.1.34",
          "form": "잔뜩사놓고 ㅋㅋ",
          "original_form": "잔뜩사놓고 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 07:29"
        },
        {
          "id": "MDRW2100014530.1.35",
          "form": "아직 개봉도 안했어요",
          "original_form": "아직 개봉도 안했어요",
          "speaker_id": "2",
          "time": "20210826 07:29"
        },
        {
          "id": "MDRW2100014530.1.36",
          "form": "안티링클제품을 바르면 좋을까 싶네요",
          "original_form": "안티링클제품을 바르면 좋을까 싶네요",
