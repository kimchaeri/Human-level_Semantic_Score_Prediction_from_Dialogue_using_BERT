      "utterance": [
        {
          "id": "MDRW2100005330.1.1",
          "form": "혹시 싸이월드 쓰신적 있으세요?",
          "original_form": "혹시 싸이월드 쓰신적 있으세요?",
          "speaker_id": "1",
          "time": "20210821 21:51"
        },
        {
          "id": "MDRW2100005330.1.2",
          "form": "네 있습니다",
          "original_form": "네 있습니다",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005330.1.3",
          "form": "싸이월드 부활했다던데",
          "original_form": "싸이월드 부활했다던데",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005330.1.4",
          "form": "그거 보셨어요?",
          "original_form": "그거 보셨어요?",
          "speaker_id": "1",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005330.1.5",
          "form": "아직 보지는 않았어요",
          "original_form": "아직 보지는 않았어요",
          "speaker_id": "2",
          "time": "20210821 21:52"
        },
        {
          "id": "MDRW2100005330.1.6",
          "form": "싸이월드 부활하는게 좋은거같아요 나쁜거같아요?",
          "original_form": "싸이월드 부활하는게 좋은거같아요 나쁜거같아요?",
          "speaker_id": "1",
          "time": "20210821 21:53"
        },
        {
          "id": "MDRW2100005330.1.7",
          "form": "저는 좋은것 같아요",
          "original_form": "저는 좋은것 같아요",
          "speaker_id": "2",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005330.1.8",
          "form": "왜 좋은거같아요?",
          "original_form": "왜 좋은거같아요?",
          "speaker_id": "1",
          "time": "20210821 21:59"
        },
        {
          "id": "MDRW2100005330.1.9",
          "form": "싸이월드 했었던 추억이 생각나서요",
          "original_form": "싸이월드 했었던 추억이 생각나서요",
          "speaker_id": "2",
          "time": "20210821 22:00"
        },
        {
          "id": "MDRW2100005330.1.10",
          "form": "싸이월드 하셨을때 나이가 어떻게되세요? 궁금하네요",
          "original_form": "싸이월드 하셨을때 나이가 어떻게되세요? 궁금하네요",
          "speaker_id": "1",
          "time": "20210821 22:01"
        },
        {
          "id": "MDRW2100005330.1.11",
          "form": "중1 이요",
          "original_form": "중1 이요",
          "speaker_id": "2",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005330.1.12",
          "form": "아 되게 추억일거같아요",
          "original_form": "아 되게 추억일거같아요",
          "speaker_id": "1",
          "time": "20210821 22:02"
        },
        {
          "id": "MDRW2100005330.1.13",
          "form": "네 맞아요 그때 했었던 친구들도 그립네요",
          "original_form": "네 맞아요 그때 했었던 친구들도 그립네요",
          "speaker_id": "2",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005330.1.14",
          "form": "싸이월드하면 그 친구분들것도 있지 않을까요?",
          "original_form": "싸이월드하면 그 친구분들것도 있지 않을까요?",
          "speaker_id": "1",
          "time": "20210821 22:03"
        },
        {
          "id": "MDRW2100005330.1.15",
          "form": "그건 잘 모르겠네요",
          "original_form": "그건 잘 모르겠네요",
