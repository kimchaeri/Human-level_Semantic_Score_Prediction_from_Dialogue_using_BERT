      "utterance": [
        {
          "id": "MDRW2100047596.1.1",
          "form": "여행 어디 가고싶으세요?",
          "original_form": "여행 어디 가고싶으세요?",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100047596.1.2",
          "form": "저는 호주요~",
          "original_form": "저는 호주요~",
          "speaker_id": "1",
          "time": "20210826 22:11"
        },
        {
          "id": "MDRW2100047596.1.3",
          "form": "오 호주 이유를",
          "original_form": "오 호주 이유를",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100047596.1.4",
          "form": "알 수 있을까요?",
          "original_form": "알 수 있을까요?",
          "speaker_id": "2",
          "time": "20210826 22:32"
        },
        {
          "id": "MDRW2100047596.1.5",
          "form": "호주에서 크리스마스 보내보고싶어요! 어떨지 궁금해서요 ㅎㅎ",
          "original_form": "호주에서 크리스마스 보내보고싶어요! 어떨지 궁금해서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100047596.1.6",
          "form": "아하 전 미국에서요ㅋㅋ!",
          "original_form": "아하 전 미국에서요ㅋㅋ!",
          "speaker_id": "2",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100047596.1.7",
          "form": "아하! 미국 동부가고싶으신가요 서부가고싶으신가요?",
          "original_form": "아하! 미국 동부가고싶으신가요 서부가고싶으신가요?",
          "speaker_id": "1",
          "time": "20210827 04:32"
        },
        {
          "id": "MDRW2100047596.1.8",
          "form": "ㅋㅋ 전 그냥 아무 곳이나요",
          "original_form": "ㅋㅋ 전 그냥 아무 곳이나요",
          "speaker_id": "2",
          "time": "20210827 07:46"
        },
        {
          "id": "MDRW2100047596.1.9",
          "form": "일단 여행을..",
          "original_form": "일단 여행을..",
          "speaker_id": "2",
          "time": "20210827 07:46"
        },
        {
          "id": "MDRW2100047596.1.10",
          "form": "앜ㅋㅋㅋㅋㅋㅋ저두요.,ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ저두요.,ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 12:07"
        },
        {
          "id": "MDRW2100047596.1.11",
          "form": "ㅋㅋㅋㅋㅋ가는게 소원이죠",
          "original_form": "ㅋㅋㅋㅋㅋ가는게 소원이죠",
          "speaker_id": "2",
          "time": "20210827 12:18"
        },
        {
          "id": "MDRW2100047596.1.12",
          "form": "맞아요 언제쯤 이 코로롱이 끝날지 ㅜ",
          "original_form": "맞아요 언제쯤 이 코로롱이 끝날지 ㅜ",
          "speaker_id": "1",
          "time": "20210827 12:20"
        },
        {
          "id": "MDRW2100047596.1.13",
          "form": "진짜요 ㅠ",
          "original_form": "진짜요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 12:23"
        },
        {
          "id": "MDRW2100047596.1.14",
          "form": "그래야 여행을 가는데..",
          "original_form": "그래야 여행을 가는데..",
          "speaker_id": "2",
          "time": "20210827 12:24"
        },
        {
          "id": "MDRW2100047596.1.15",
          "form": "어쩔수없죠뭐ㅠ그래도뭔가 내년엔 괜찮아질거같아요",
          "original_form": "어쩔수없죠뭐ㅠ그래도뭔가 내년엔 괜찮아질거같아요",
          "speaker_id": "1",
          "time": "20210827 12:25"
        }
