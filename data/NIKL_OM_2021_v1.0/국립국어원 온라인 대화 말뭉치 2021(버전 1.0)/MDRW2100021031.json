      "utterance": [
        {
          "id": "MDRW2100021031.1.1",
          "form": "요즘 여행가고싶네요 ㅜ",
          "original_form": "요즘 여행가고싶네요 ㅜ",
          "speaker_id": "1",
          "time": "20210828 15:09"
        },
        {
          "id": "MDRW2100021031.1.2",
          "form": "휴가 다녀오셨어요?",
          "original_form": "휴가 다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210828 15:16"
        },
        {
          "id": "MDRW2100021031.1.3",
          "form": "이번 여름에요?",
          "original_form": "이번 여름에요?",
          "speaker_id": "1",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100021031.1.4",
          "form": "전혀요 ..",
          "original_form": "전혀요 ..",
          "speaker_id": "1",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100021031.1.5",
          "form": "휴가 갔다 확진 되면 중징계 각오하라고 협박 당해서^^",
          "original_form": "휴가 갔다 확진 되면 중징계 각오하라고 협박 당해서^^",
          "speaker_id": "1",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100021031.1.6",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100021031.1.7",
          "form": "중징계요?",
          "original_form": "중징계요?",
          "speaker_id": "2",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100021031.1.8",
          "form": "네 ㅋㅋㅋㅋㅋ황당하죠?",
          "original_form": "네 ㅋㅋㅋㅋㅋ황당하죠?",
          "speaker_id": "1",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021031.1.9",
          "form": "그럼 휴가가 없었어요?",
          "original_form": "그럼 휴가가 없었어요?",
          "speaker_id": "2",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021031.1.10",
          "form": "시국이 그런건 알겠는데 그렇게까지 말해야되나 싶더라구요",
          "original_form": "시국이 그런건 알겠는데 그렇게까지 말해야되나 싶더라구요",
          "speaker_id": "1",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021031.1.11",
          "form": "넹 ^^",
          "original_form": "넹 ^^",
          "speaker_id": "1",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021031.1.12",
          "form": "name2님은 휴가 다녀오셨어요?",
          "original_form": "&name2&님은 휴가 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021031.1.13",
          "form": "전 호캉스갔다왔어요",
          "original_form": "전 호캉스갔다왔어요",
          "speaker_id": "2",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100021031.1.14",
          "form": "어디 가기가 뭐해서",
          "original_form": "어디 가기가 뭐해서",
          "speaker_id": "2",
          "time": "20210828 15:25"
        },
        {
          "id": "MDRW2100021031.1.15",
          "form": "오 ㅎㅎ 호캉스 어디로 다녀오셨나요",
          "original_form": "오 ㅎㅎ 호캉스 어디로 다녀오셨나요",
          "speaker_id": "1",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100021031.1.16",
          "form": "노보텔 엠버서더 용산갓는데",
          "original_form": "노보텔 엠버서더 용산갓는데",
          "speaker_id": "2",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100021031.1.17",
          "form": "그냥 그랬어요 ㅋㅋ",
          "original_form": "그냥 그랬어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100021031.1.18",
          "form": "좀더 좋은데로 갈껄...",
          "original_form": "좀더 좋은데로 갈껄...",
          "speaker_id": "2",
          "time": "20210828 15:27"
        },
        {
          "id": "MDRW2100021031.1.19",
          "form": "룸서비스 시켜먹고 쉬다가 왔어요",
          "original_form": "룸서비스 시켜먹고 쉬다가 왔어요",
          "speaker_id": "2",
          "time": "20210828 15:28"
        },
        {
          "id": "MDRW2100021031.1.20",
          "form": "아 ㅋㅋㅋ 혹시 용산에 그",
          "original_form": "아 ㅋㅋㅋ 혹시 용산에 그",
          "speaker_id": "1",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100021031.1.21",
          "form": "드래곤힐스파? 가보셨어요?",
          "original_form": "드래곤힐스파? 가보셨어요?",
          "speaker_id": "1",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100021031.1.22",
          "form": "그거 제가 갔던데 있는거 같은데",
          "original_form": "그거 제가 갔던데 있는거 같은데",
          "speaker_id": "2",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100021031.1.23",
          "form": "스파는 안가봤어요",
          "original_form": "스파는 안가봤어요",
          "speaker_id": "2",
          "time": "20210828 16:23"
        },
        {
          "id": "MDRW2100021031.1.24",
          "form": "맞아요 바로 옆 건물일거에요",
          "original_form": "맞아요 바로 옆 건물일거에요",
          "speaker_id": "1",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100021031.1.25",
          "form": "거기 되게 은근 유명하던데 궁금해서 가보고 싶어요",
          "original_form": "거기 되게 은근 유명하던데 궁금해서 가보고 싶어요",
