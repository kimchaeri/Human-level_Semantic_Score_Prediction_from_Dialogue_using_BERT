      "utterance": [
        {
          "id": "MDRW2100028476.1.1",
          "form": "나 요즘 고영이 역방쿠 사주려고 당근 맨날 보는데",
          "original_form": "나 요즘 고영이 역방쿠 사주려고 당근 맨날 보는데",
          "speaker_id": "1",
          "time": "20210902 20:13"
        },
        {
          "id": "MDRW2100028476.1.2",
          "form": "매물이 없나요",
          "original_form": "매물이 없나요",
          "speaker_id": "2",
          "time": "20210902 20:14"
        },
        {
          "id": "MDRW2100028476.1.3",
          "form": "그렇기도 하고... 가까운 데서 사고 싶은데 좀 멀기도 하고",
          "original_form": "그렇기도 하고... 가까운 데서 사고 싶은데 좀 멀기도 하고",
          "speaker_id": "1",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028476.1.4",
          "form": "부피가 좀 있어서",
          "original_form": "부피가 좀 있어서",
          "speaker_id": "1",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028476.1.5",
          "form": "아무래도 쿠션이니까...",
          "original_form": "아무래도 쿠션이니까...",
          "speaker_id": "2",
          "time": "20210902 20:15"
        },
        {
          "id": "MDRW2100028476.1.6",
          "form": "택배 보내주는 매물이 없나",
          "original_form": "택배 보내주는 매물이 없나",
          "speaker_id": "2",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100028476.1.7",
          "form": "택배는 택배비가 많이 붙지 않으려나",
          "original_form": "택배는 택배비가 많이 붙지 않으려나",
          "speaker_id": "1",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100028476.1.8",
          "form": "급한 건 아니라 걍 존버하는 중",
          "original_form": "급한 건 아니라 걍 존버하는 중",
          "speaker_id": "1",
          "time": "20210902 20:16"
        },
        {
          "id": "MDRW2100028476.1.9",
          "form": "그건 그렇지",
          "original_form": "그건 그렇지",
          "speaker_id": "2",
          "time": "20210902 20:18"
        },
        {
          "id": "MDRW2100028476.1.10",
          "form": "근데 고영 장난감 진짜 많이 사가도 되나",
          "original_form": "근데 고영 장난감 진짜 많이 사가도 되나",
          "speaker_id": "2",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100028476.1.11",
          "form": "엥 당연... 근데 어디서 그렇게 사오려는",
          "original_form": "엥 당연... 근데 어디서 그렇게 사오려는",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028476.1.12",
          "form": "이마트...",
          "original_form": "이마트...",
          "speaker_id": "2",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028476.1.13",
          "form": "거기 뭐가 그렇게 많지는 않을텐데",
          "original_form": "거기 뭐가 그렇게 많지는 않을텐데",
          "speaker_id": "1",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028476.1.14",
          "form": "취향일 것 같은 거 여러 개 사오게",
          "original_form": "취향일 것 같은 거 여러 개 사오게",
          "speaker_id": "2",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028476.1.15",
          "form": "살 게 있기만 하다면... 좋지...",
          "original_form": "살 게 있기만 하다면... 좋지...",
          "speaker_id": "1",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028476.1.16",
          "form": "강아지풀 있으면 쟁여올게.",
          "original_form": "강아지풀 있으면 쟁여올게.",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028476.1.17",
          "form": "웃기다 감사합니다",
          "original_form": "웃기다 감사합니다",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028476.1.18",
          "form": "진짜 너덜너덜함",
          "original_form": "진짜 너덜너덜함",
          "speaker_id": "1",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028476.1.19",
          "form": "그래보임",
          "original_form": "그래보임",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028476.1.20",
          "form": "마지막으로 올라온 사진이",
          "original_form": "마지막으로 올라온 사진이",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028476.1.21",
          "form": "처참하던데",
          "original_form": "처참하던데",
          "speaker_id": "2",
          "time": "20210902 20:23"
        },
        {
          "id": "MDRW2100028476.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028476.1.23",
          "form": "진짜로 잘근잘근 씹어서",
          "original_form": "진짜로 잘근잘근 씹어서",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028476.1.24",
          "form": "그게 왜 그렇게 좋은 거지",
          "original_form": "그게 왜 그렇게 좋은 거지",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028476.1.25",
          "form": "취존해줘",
          "original_form": "취존해줘",
          "speaker_id": "2",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028476.1.26",
          "form": "너만 좋으면 됐다",
          "original_form": "너만 좋으면 됐다",
          "speaker_id": "1",
          "time": "20210902 20:24"
        }
      ]
    }
