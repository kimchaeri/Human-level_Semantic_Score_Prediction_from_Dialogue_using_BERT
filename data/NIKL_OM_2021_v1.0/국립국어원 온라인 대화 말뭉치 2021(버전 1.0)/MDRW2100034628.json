      "utterance": [
        {
          "id": "MDRW2100034628.1.1",
          "form": "name2님은 떡볶이를 좋아하세요?",
          "original_form": "&name2&님은 떡볶이를 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 16:38"
        },
        {
          "id": "MDRW2100034628.1.2",
          "form": "네 너무 좋아해요 ㅋㅋㅋ",
          "original_form": "네 너무 좋아해요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100034628.1.3",
          "form": "좋아하세요?",
          "original_form": "좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100034628.1.4",
          "form": "좋아하는데 집에선 안해먹어 봤어요 해드셔 보셨나요?",
          "original_form": "좋아하는데 집에선 안해먹어 봤어요 해드셔 보셨나요?",
          "speaker_id": "1",
          "time": "20210828 16:39"
        },
        {
          "id": "MDRW2100034628.1.5",
          "form": "해먹긴하는데",
          "original_form": "해먹긴하는데",
          "speaker_id": "2",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100034628.1.6",
          "form": "역시 사먹는게 최고에요!",
          "original_form": "역시 사먹는게 최고에요!",
          "speaker_id": "2",
          "time": "20210828 16:40"
        },
        {
          "id": "MDRW2100034628.1.7",
          "form": "로제 떡볶이 재료가 궁금해요 생크림이 들어가나요?",
          "original_form": "로제 떡볶이 재료가 궁금해요 생크림이 들어가나요?",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034628.1.8",
          "form": "아님 우유?",
          "original_form": "아님 우유?",
          "speaker_id": "1",
          "time": "20210828 16:41"
        },
        {
          "id": "MDRW2100034628.1.9",
          "form": "아 그건 못해봤구",
          "original_form": "아 그건 못해봤구",
          "speaker_id": "2",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100034628.1.10",
          "form": "기본 떡볶이만 해봤어요 ㅋㅋ",
          "original_form": "기본 떡볶이만 해봤어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100034628.1.11",
          "form": "아하 저도요",
          "original_form": "아하 저도요",
          "speaker_id": "1",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100034628.1.12",
          "form": "그래서 로제에 들어가는게 어떤 크림인지 궁금했어요",
          "original_form": "그래서 로제에 들어가는게 어떤 크림인지 궁금했어요",
          "speaker_id": "1",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100034628.1.13",
          "form": "흠 아무래도",
          "original_form": "흠 아무래도",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100034628.1.14",
          "form": "유크림인지 팜유 크림같은건지",
          "original_form": "유크림인지 팜유 크림같은건지",
          "speaker_id": "1",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100034628.1.15",
          "form": "아 뭔가 카레맛도 나지 않나요..??",
          "original_form": "아 뭔가 카레맛도 나지 않나요..??",
          "speaker_id": "2",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100034628.1.16",
          "form": "ㅠ전 한 번 밖에 안 먹어봐서 어떤 브랜드거냐에 따라 다를 것 같아여",
          "original_form": "ㅠ전 한 번 밖에 안 먹어봐서 어떤 브랜드거냐에 따라 다를 것 같아여",
          "speaker_id": "1",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100034628.1.17",
          "form": "그 로제찜닭은 드셔보셨나요?",
          "original_form": "그 로제찜닭은 드셔보셨나요?",
          "speaker_id": "2",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100034628.1.18",
          "form": "아뇨 ㅠㅠㅠ 어디서 파나요?",
          "original_form": "아뇨 ㅠㅠㅠ 어디서 파나요?",
          "speaker_id": "1",
          "time": "20210828 16:45"
        },
        {
          "id": "MDRW2100034628.1.19",
          "form": "두찜이요!",
          "original_form": "두찜이요!",
          "speaker_id": "2",
          "time": "20210828 16:46"
        },
        {
          "id": "MDRW2100034628.1.20",
          "form": "거기 당면이랑 고구마가 꿀맛입니다",
          "original_form": "거기 당면이랑 고구마가 꿀맛입니다",
          "speaker_id": "2",
          "time": "20210828 16:46"
        }
      ]
