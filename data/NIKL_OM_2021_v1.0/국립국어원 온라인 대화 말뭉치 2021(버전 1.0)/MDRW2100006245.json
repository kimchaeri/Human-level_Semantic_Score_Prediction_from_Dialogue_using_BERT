      "utterance": [
        {
          "id": "MDRW2100006245.1.1",
          "form": "알프스산맥요",
          "original_form": "알프스산맥요",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006245.1.2",
          "form": "스위스에요?",
          "original_form": "스위스에요?",
          "speaker_id": "1",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006245.1.3",
          "form": "케이플카타고 가서 구경넘좋아요",
          "original_form": "케이플카타고 가서 구경넘좋아요",
          "speaker_id": "2",
          "time": "20210907 22:08"
        },
        {
          "id": "MDRW2100006245.1.4",
          "form": "오 어떤 가요 정말 웅장한가요??",
          "original_form": "오 어떤 가요 정말 웅장한가요??",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006245.1.5",
          "form": "멋져요",
          "original_form": "멋져요",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006245.1.6",
          "form": "깨끗하고..ㅈ",
          "original_form": "깨끗하고..ㅈ",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006245.1.7",
          "form": "저는 유명한 산맥을 다 가보고 싶어요",
          "original_form": "저는 유명한 산맥을 다 가보고 싶어요",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006245.1.8",
          "form": "코로나끝나면가보세요",
          "original_form": "코로나끝나면가보세요",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006245.1.9",
          "form": "특히 알프스 산맥은 더요",
          "original_form": "특히 알프스 산맥은 더요",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006245.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006245.1.11",
          "form": "저도 코로나끝나면가려구요",
          "original_form": "저도 코로나끝나면가려구요",
          "speaker_id": "2",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006245.1.12",
          "form": "비행기는 타본지도",
          "original_form": "비행기는 타본지도",
          "speaker_id": "1",
          "time": "20210907 22:09"
        },
        {
          "id": "MDRW2100006245.1.13",
          "form": "오래 되었네요",
          "original_form": "오래 되었네요",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100006245.1.14",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100006245.1.15",
          "form": "기억이안나네요",
          "original_form": "기억이안나네요",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100006245.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100006245.1.17",
          "form": "여행기시면 뭐하시나요?",
          "original_form": "여행기시면 뭐하시나요?",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100006245.1.18",
          "form": "저는 숙소에서 머무르는 편이예요",
          "original_form": "저는 숙소에서 머무르는 편이예요",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100006245.1.19",
          "form": "밖에 여러군데 다니는 것을",
          "original_form": "밖에 여러군데 다니는 것을",
          "speaker_id": "1",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100006245.1.20",
          "form": "휴양? 관광?",
          "original_form": "휴양? 관광?",
          "speaker_id": "2",
          "time": "20210907 22:10"
        },
        {
          "id": "MDRW2100006245.1.21",
          "form": "별로 좋아하지 않아요",
          "original_form": "별로 좋아하지 않아요",
          "speaker_id": "1",
          "time": "20210907 22:11"
        },
        {
          "id": "MDRW2100006245.1.22",
          "form": "아.쉬시는구나",
          "original_form": "아.쉬시는구나",
          "speaker_id": "2",
          "time": "20210907 22:11"
        },
