      "utterance": [
        {
          "id": "MDRW2100019032.1.1",
          "form": "최근에 산 물건 있어요?",
          "original_form": "최근에 산 물건 있어요?",
          "speaker_id": "2",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100019032.1.2",
          "form": "네 최근에 당근으로 에그팬샀어요~ㅋㅋ",
          "original_form": "네 최근에 당근으로 에그팬샀어요~ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100019032.1.3",
          "form": "와 에그팬! 귀엽겠어요ㅎㅎ 저도 에그팬 사은품으로 받은게 있는데 팔아야 겠네요",
          "original_form": "와 에그팬! 귀엽겠어요ㅎㅎ 저도 에그팬 사은품으로 받은게 있는데 팔아야 겠네요",
          "speaker_id": "2",
          "time": "20210827 15:07"
        },
        {
          "id": "MDRW2100019032.1.4",
          "form": "ㅋㅋㅋ 중고로 주방용품 팔면 리젠이 자주오더라구요~",
          "original_form": "ㅋㅋㅋ 중고로 주방용품 팔면 리젠이 자주오더라구요~",
          "speaker_id": "1",
          "time": "20210827 15:10"
        },
        {
          "id": "MDRW2100019032.1.5",
          "form": "ㅋㅋㅋ당근도 부지런해야 거래하겠더라구요",
          "original_form": "ㅋㅋㅋ당근도 부지런해야 거래하겠더라구요",
          "speaker_id": "2",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019032.1.6",
          "form": "맞아요! 뭐든 부지런해야하는거같아요 ㅜ",
          "original_form": "맞아요! 뭐든 부지런해야하는거같아요 ㅜ",
          "speaker_id": "1",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100019032.1.7",
          "form": "전 한번 거래갔다가 바람 맞아서 ㅠㅠ",
          "original_form": "전 한번 거래갔다가 바람 맞아서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100019032.1.8",
          "form": "아 헐 ㅜㅜ상대분 나쁘네요",
          "original_form": "아 헐 ㅜㅜ상대분 나쁘네요",
          "speaker_id": "1",
          "time": "20210827 15:30"
        },
        {
          "id": "MDRW2100019032.1.9",
          "form": "팔까말까 고민하던거라",
          "original_form": "팔까말까 고민하던거라",
          "speaker_id": "2",
          "time": "20210827 16:23"
        },
        {
          "id": "MDRW2100019032.1.10",
          "form": "아직 팔 운명이 아닌 물건인가보다 하고",
          "original_form": "아직 팔 운명이 아닌 물건인가보다 하고",
          "speaker_id": "2",
          "time": "20210827 16:23"
        },
        {
          "id": "MDRW2100019032.1.11",
          "form": "좋게 넘어갔어요 ㅋㅋㅋ",
          "original_form": "좋게 넘어갔어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:23"
        },
        {
          "id": "MDRW2100019032.1.12",
          "form": "아.. 맞아요 운명인가봐요 ㅋㅋㅋㅋ",
          "original_form": "아.. 맞아요 운명인가봐요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:28"
        },
        {
          "id": "MDRW2100019032.1.13",
          "form": "팔려던게 미러볼 조명이었는데",
          "original_form": "팔려던게 미러볼 조명이었는데",
          "speaker_id": "2",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019032.1.14",
          "form": "뭔가 팔기 아까웠어요ㅋㅋㅋㅋㅋ",
          "original_form": "뭔가 팔기 아까웠어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:40"
        },
        {
          "id": "MDRW2100019032.1.15",
          "form": "아 ㅋㅋㅋㅋㅋ 미러볼 ㅋㅋㅋㅋㅋ주로뭐할때쓰셨나요?",
          "original_form": "아 ㅋㅋㅋㅋㅋ 미러볼 ㅋㅋㅋㅋㅋ주로뭐할때쓰셨나요?",
          "speaker_id": "1",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019032.1.16",
          "form": "홈파티한다고 샀었는데",
          "original_form": "홈파티한다고 샀었는데",
          "speaker_id": "2",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019032.1.17",
          "form": "코로나땜에 뭐 놀기도 좀 그렇고 해서ㅋㅋㅋㅋ",
          "original_form": "코로나땜에 뭐 놀기도 좀 그렇고 해서ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 16:41"
        },
        {
          "id": "MDRW2100019032.1.18",
          "form": "코로나끝나면 즐기시면되죠뭐~",
          "original_form": "코로나끝나면 즐기시면되죠뭐~",
          "speaker_id": "1",
          "time": "20210827 16:42"
        }
      ]
