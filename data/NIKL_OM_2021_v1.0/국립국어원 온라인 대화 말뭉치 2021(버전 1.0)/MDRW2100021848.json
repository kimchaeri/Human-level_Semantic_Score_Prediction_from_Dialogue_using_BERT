      "utterance": [
        {
          "id": "MDRW2100021848.1.1",
          "form": "책이나 잡지 보시는거 있나여?",
          "original_form": "책이나 잡지 보시는거 있나여?",
          "speaker_id": "1",
          "time": "20210828 23:01"
        },
        {
          "id": "MDRW2100021848.1.2",
          "form": "미비포유 후속작 봤어요",
          "original_form": "미비포유 후속작 봤어요",
          "speaker_id": "2",
          "time": "20210828 23:02"
        },
        {
          "id": "MDRW2100021848.1.3",
          "form": "아.. 모르는거내요.. 미비포유? 뭐애요?",
          "original_form": "아.. 모르는거내요.. 미비포유? 뭐애요?",
          "speaker_id": "1",
          "time": "20210828 23:03"
        },
        {
          "id": "MDRW2100021848.1.4",
          "form": "영화로도 나왔어요",
          "original_form": "영화로도 나왔어요",
          "speaker_id": "2",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100021848.1.5",
          "form": "무슨 책 좋아하세요?",
          "original_form": "무슨 책 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100021848.1.6",
          "form": "책은 피와살이되는 사회과학쪽이요.. \r\n주로 소설 보세요?",
          "original_form": "책은 피와살이되는 사회과학쪽이요.. \r\n주로 소설 보세요?",
          "speaker_id": "1",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100021848.1.7",
          "form": "네ㅋㅋ",
          "original_form": "네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100021848.1.8",
          "form": "사회과학 추천해주세요",
          "original_form": "사회과학 추천해주세요",
          "speaker_id": "2",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021848.1.9",
          "form": "최근건 사피엔스가 대박이엇고",
          "original_form": "최근건 사피엔스가 대박이엇고",
          "speaker_id": "1",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100021848.1.10",
          "form": "상식밖의 경제학도 유명햇엇고..",
          "original_form": "상식밖의 경제학도 유명햇엇고..",
          "speaker_id": "1",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100021848.1.11",
          "form": "아 박학다식해요!!!",
          "original_form": "아 박학다식해요!!!",
          "speaker_id": "2",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100021848.1.12",
          "form": "적어둬야겠어요",
          "original_form": "적어둬야겠어요",
          "speaker_id": "2",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100021848.1.13",
          "form": "오.. 적극적인 자세.. ㅋㅋㅋ",
          "original_form": "오.. 적극적인 자세.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100021848.1.14",
          "form": "도서관 자주가시나여?",
          "original_form": "도서관 자주가시나여?",
          "speaker_id": "1",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100021848.1.15",
          "form": "오늘 갔었어요",
          "original_form": "오늘 갔었어요",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100021848.1.16",
          "form": "주위분들 많이 가더라고요",
          "original_form": "주위분들 많이 가더라고요",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100021848.1.17",
          "form": "대출하러요?",
          "original_form": "대출하러요?",
          "speaker_id": "1",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100021848.1.18",
          "form": "반납이요",
          "original_form": "반납이요",
          "speaker_id": "2",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100021848.1.19",
          "form": "무슨 책 보셧어요?",
          "original_form": "무슨 책 보셧어요?",
          "speaker_id": "1",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100021848.1.20",
