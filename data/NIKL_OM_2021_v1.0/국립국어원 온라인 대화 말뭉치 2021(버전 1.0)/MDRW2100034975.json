      "utterance": [
        {
          "id": "MDRW2100034975.1.1",
          "form": "팩은 어디꺼 쓰세요?",
          "original_form": "팩은 어디꺼 쓰세요?",
          "speaker_id": "1",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034975.1.2",
          "form": "그냥 저렴한거 뭉탱이로 사요",
          "original_form": "그냥 저렴한거 뭉탱이로 사요",
          "speaker_id": "2",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100034975.1.3",
          "form": "전에 일일팩 유행했잖아요",
          "original_form": "전에 일일팩 유행했잖아요",
          "speaker_id": "2",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100034975.1.4",
          "form": "네넹 ㅎㅎ  저도 이벤트로 받은거 다쓰고 잇어요 ㅎㅎ",
          "original_form": "네넹 ㅎㅎ  저도 이벤트로 받은거 다쓰고 잇어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100034975.1.5",
          "form": "ㅎㅎ 그리고 그때 들은게 일일팩하려면 무거운 거는 역효과라고",
          "original_form": "ㅎㅎ 그리고 그때 들은게 일일팩하려면 무거운 거는 역효과라고",
          "speaker_id": "2",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100034975.1.6",
          "form": "고농축보다 가벼운걸로 쓰라 그러더라고요",
          "original_form": "고농축보다 가벼운걸로 쓰라 그러더라고요",
          "speaker_id": "2",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100034975.1.7",
          "form": "오혼 첨듣는 정보!!",
          "original_form": "오혼 첨듣는 정보!!",
          "speaker_id": "1",
          "time": "20210828 23:17"
        },
        {
          "id": "MDRW2100034975.1.8",
          "form": "ㅎㅎ 그래서 그냥 저가 팩 쌓아두고 지갑 아꼈죠.",
          "original_form": "ㅎㅎ 그래서 그냥 저가 팩 쌓아두고 지갑 아꼈죠.",
          "speaker_id": "2",
          "time": "20210828 23:18"
        },
        {
          "id": "MDRW2100034975.1.9",
          "form": "ㅎㅎ 뭐든 꾸준히가 답인거 같은 느낌이에요 ㅎㅎ",
          "original_form": "ㅎㅎ 뭐든 꾸준히가 답인거 같은 느낌이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:20"
        },
        {
          "id": "MDRW2100034975.1.10",
          "form": "다이어트는 해보셨나요",
          "original_form": "다이어트는 해보셨나요",
          "speaker_id": "2",
          "time": "20210828 23:32"
        },
        {
          "id": "MDRW2100034975.1.11",
          "form": "맨날 하쥬 ㅋㅋ 말만 ㅜㅜㅋ",
          "original_form": "맨날 하쥬 ㅋㅋ 말만 ㅜㅜㅋ",
          "speaker_id": "1",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100034975.1.12",
          "form": "ㅎㅎ 다이어트는 내일부터 하는 거잖아요",
          "original_form": "ㅎㅎ 다이어트는 내일부터 하는 거잖아요",
          "speaker_id": "2",
          "time": "20210828 23:36"
        },
        {
          "id": "MDRW2100034975.1.13",
          "form": "그래서 살이 안빠지쥬 ㅋㅋ",
          "original_form": "그래서 살이 안빠지쥬 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100034975.1.14",
          "form": "맛있게 먹으면 영칼로리",
          "original_form": "맛있게 먹으면 영칼로리",
          "speaker_id": "2",
          "time": "20210828 23:37"
        },
        {
          "id": "MDRW2100034975.1.15",
          "form": "내일의 내가 어떻게든 해줄 거예요ㅎ",
          "original_form": "내일의 내가 어떻게든 해줄 거예요ㅎ",
          "speaker_id": "2",
          "time": "20210828 23:38"
        },
