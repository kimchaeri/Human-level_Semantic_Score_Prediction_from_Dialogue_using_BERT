      "utterance": [
        {
          "id": "MDRW2100010532.1.1",
          "form": "순살이면 넘좋쥬",
          "original_form": "순살이면 넘좋쥬",
          "speaker_id": "2",
          "time": "20210823 19:17"
        },
        {
          "id": "MDRW2100010532.1.2",
          "form": "이전에는 폭탄꼬지에도 포도맛이 있었는데",
          "original_form": "이전에는 폭탄꼬지에도 포도맛이 있었는데",
          "speaker_id": "2",
          "time": "20210823 19:17"
        },
        {
          "id": "MDRW2100010532.1.3",
          "form": "요즘은 떡볶이 순한맛도 약간씩은 맵잖아요ㅋㅋ",
          "original_form": "요즘은 떡볶이 순한맛도 약간씩은 맵잖아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:17"
        },
        {
          "id": "MDRW2100010532.1.4",
          "form": "맞아요! 매운맛 싫어하지 않지만 선택권이 사라지는게 싫어요",
          "original_form": "맞아요! 매운맛 싫어하지 않지만 선택권이 사라지는게 싫어요",
          "speaker_id": "1",
          "time": "20210823 19:18"
        },
        {
          "id": "MDRW2100010532.1.5",
          "form": "매운걸 먹고싶으면.. 매운양념을 시켰겠지..!",
          "original_form": "매운걸 먹고싶으면.. 매운양념을 시켰겠지..!",
          "speaker_id": "1",
          "time": "20210823 19:18"
        },
        {
          "id": "MDRW2100010532.1.6",
          "form": "하지만 맵다고 적혀있는 양념은 보통 지옥불맛ㅋㅋ",
          "original_form": "하지만 맵다고 적혀있는 양념은 보통 지옥불맛ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:19"
        },
        {
          "id": "MDRW2100010532.1.7",
          "form": "혀와 식도와 위장이 타들어가는 지옥불이죠 ㅋㅋ",
          "original_form": "혀와 식도와 위장이 타들어가는 지옥불이죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:19"
        },
        {
          "id": "MDRW2100010532.1.8",
          "form": "아 진짜 너무 고문화되고 있어요 괴로워라",
          "original_form": "아 진짜 너무 고문화되고 있어요 괴로워라",
          "speaker_id": "1",
          "time": "20210823 19:19"
        },
        {
          "id": "MDRW2100010532.1.9",
          "form": "ㅋㅋㅋ 맞아요 극단적인맛",
          "original_form": "ㅋㅋㅋ 맞아요 극단적인맛",
          "speaker_id": "2",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010532.1.10",
          "form": "요즘은 단맛도 어엄청 달고요",
          "original_form": "요즘은 단맛도 어엄청 달고요",
          "speaker_id": "2",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010532.1.11",
          "form": "사람들이 스트레스를 받아서 그런가",
          "original_form": "사람들이 스트레스를 받아서 그런가",
          "speaker_id": "1",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010532.1.12",
          "form": "혈관이 읍소하는맛",
          "original_form": "혈관이 읍소하는맛",
          "speaker_id": "2",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010532.1.13",
          "form": "모든 감각을 최대화하고 있는 듯 해요 정말 ㅋㅋ",
          "original_form": "모든 감각을 최대화하고 있는 듯 해요 정말 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 19:20"
        },
        {
          "id": "MDRW2100010532.1.14",
          "form": "요즘 꿀젤리먹방 보셨어요?",
          "original_form": "요즘 꿀젤리먹방 보셨어요?",
          "speaker_id": "2",
          "time": "20210823 19:21"
        },
        {
          "id": "MDRW2100010532.1.15",
          "form": "헉 아뇨 검색해봐야지",
          "original_form": "헉 아뇨 검색해봐야지",
          "speaker_id": "1",
          "time": "20210823 19:22"
        },
        {
          "id": "MDRW2100010532.1.16",
          "form": "꿀을 뭉근하게 끓이면 젤리처럼 굳는데",
          "original_form": "꿀을 뭉근하게 끓이면 젤리처럼 굳는데",
          "speaker_id": "2",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100010532.1.17",
          "form": "그걸 막 먹는거에요ㄷㄷ",
          "original_form": "그걸 막 먹는거에요ㄷㄷ",
          "speaker_id": "2",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100010532.1.18",
          "form": "아 지금 검색해보니 이거 신문기사에 떴던거 같아요",
          "original_form": "아 지금 검색해보니 이거 신문기사에 떴던거 같아요",
          "speaker_id": "1",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100010532.1.19",
          "form": "꿀 저렇게 대량섭취하면 죽을 수도 있다고",
          "original_form": "꿀 저렇게 대량섭취하면 죽을 수도 있다고",
          "speaker_id": "1",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100010532.1.20",
          "form": "미국 틱톡에서 열풍~ 이랬는데 우리나라에서도 먹고 있었군요",
          "original_form": "미국 틱톡에서 열풍~ 이랬는데 우리나라에서도 먹고 있었군요",
          "speaker_id": "1",
          "time": "20210823 19:23"
        },
        {
          "id": "MDRW2100010532.1.21",
          "form": "미국에서 먼저유행했군요",
          "original_form": "미국에서 먼저유행했군요",
          "speaker_id": "2",
          "time": "20210823 19:24"
        },
        {
          "id": "MDRW2100010532.1.22",
          "form": "역시 미국 햄버거튀김의 나라",
          "original_form": "역시 미국 햄버거튀김의 나라",
          "speaker_id": "2",
          "time": "20210823 19:24"
        },
        {
          "id": "MDRW2100010532.1.23",
          "form": "약간 저런 혈관따위 무시하는 식습관은 미국이쥬",
          "original_form": "약간 저런 혈관따위 무시하는 식습관은 미국이쥬",
          "speaker_id": "1",
          "time": "20210823 19:24"
        },
        {
          "id": "MDRW2100010532.1.24",
          "form": "꿀조차 그냥 두지 못하고",
          "original_form": "꿀조차 그냥 두지 못하고",
          "speaker_id": "2",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100010532.1.25",
          "form": "피자도 튀기고 초코바도 튀기고...",
          "original_form": "피자도 튀기고 초코바도 튀기고...",
          "speaker_id": "1",
          "time": "20210823 19:25"
        },
        {
          "id": "MDRW2100010532.1.26",
          "form": "미국 사람들 혈관 괜찮은지ㅋㅋ",
          "original_form": "미국 사람들 혈관 괜찮은지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 19:26"
        }
      ]
