          "speaker_id": "1",
          "time": "20210904 11:48"
        },
        {
          "id": "MDRW2100047168.1.5",
          "form": "카페",
          "original_form": "카페",
          "speaker_id": "1",
          "time": "20210904 11:48"
        },
        {
          "id": "MDRW2100047168.1.6",
          "form": "매니저",
          "original_form": "매니저",
