      "utterance": [
        {
          "id": "MDRW2100002177.1.1",
          "form": "당근은 거래가 괜찮긴 했는데 앞으로 혹시 진상있을까 꺼려져요",
          "original_form": "당근은 거래가 괜찮긴 했는데 앞으로 혹시 진상있을까 꺼려져요",
          "speaker_id": "2",
          "time": "20210820 00:18"
        },
        {
          "id": "MDRW2100002177.1.2",
          "form": "어디서 중고 거래 하나요?",
          "original_form": "어디서 중고 거래 하나요?",
          "speaker_id": "2",
          "time": "20210820 00:18"
        },
        {
          "id": "MDRW2100002177.1.3",
          "form": "전 주로 캐롯이요 ~ ㅎㅎ\r\n근데 주로 판매만 해봣어요",
          "original_form": "전 주로 캐롯이요 ~ ㅎㅎ\r\n근데 주로 판매만 해봣어요",
          "speaker_id": "1",
          "time": "20210820 09:14"
        },
        {
          "id": "MDRW2100002177.1.4",
          "form": "저도 판매만요",
          "original_form": "저도 판매만요",
          "speaker_id": "2",
          "time": "20210820 09:15"
        },
        {
          "id": "MDRW2100002177.1.5",
          "form": "요샌 대면 꺼려져서 중고로 해요",
          "original_form": "요샌 대면 꺼려져서 중고로 해요",
          "speaker_id": "2",
          "time": "20210820 09:15"
        },
        {
          "id": "MDRW2100002177.1.6",
          "form": "애들 장난감이 잘팔리네요",
          "original_form": "애들 장난감이 잘팔리네요",
          "speaker_id": "2",
          "time": "20210820 09:15"
        },
        {
          "id": "MDRW2100002177.1.7",
          "form": "저는 주로 옷이요  ㅎㅎ",
          "original_form": "저는 주로 옷이요  ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 09:16"
        },
        {
          "id": "MDRW2100002177.1.8",
          "form": "아 옷은 몇번 못팔았어요",
          "original_form": "아 옷은 몇번 못팔았어요",
          "speaker_id": "2",
          "time": "20210820 09:16"
        },
        {
          "id": "MDRW2100002177.1.9",
          "form": "저 재주가 없나 잘안팔려서요",
          "original_form": "저 재주가 없나 잘안팔려서요",
          "speaker_id": "2",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002177.1.10",
          "form": "저는 단골?? 분이 사더라구요 ㅎㅎㅎㅎ\r\n사셨던분이 재구매 해서 거의 편해요 ㅎㅎ",
          "original_form": "저는 단골?? 분이 사더라구요 ㅎㅎㅎㅎ\r\n사셨던분이 재구매 해서 거의 편해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002177.1.11",
          "form": "옷은 오프라인 장처에서 잘 팔리던데 요새 하는데가 없어 아쉬워요",
          "original_form": "옷은 오프라인 장처에서 잘 팔리던데 요새 하는데가 없어 아쉬워요",
          "speaker_id": "2",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002177.1.12",
          "form": "오 너무 좋은데요",
          "original_form": "오 너무 좋은데요",
          "speaker_id": "2",
          "time": "20210820 09:17"
        },
        {
          "id": "MDRW2100002177.1.13",
          "form": "어떤옷인가요? 어른옷이요?",
          "original_form": "어떤옷인가요? 어른옷이요?",
          "speaker_id": "2",
          "time": "20210820 09:18"
        },
        {
          "id": "MDRW2100002177.1.14",
          "form": "네네 어른옷이요 ㅎㅎㅎ",
          "original_form": "네네 어른옷이요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 09:19"
        },
        {
          "id": "MDRW2100002177.1.15",
          "form": "아 그렇군요 옷 고르는 센스가 남다르신가봐요",
          "original_form": "아 그렇군요 옷 고르는 센스가 남다르신가봐요",
          "speaker_id": "2",
          "time": "20210820 09:20"
        },
        {
          "id": "MDRW2100002177.1.16",
          "form": "그러니 잘 팔리시죠 ㅎㅎ",
          "original_form": "그러니 잘 팔리시죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 09:20"
        },
        {
          "id": "MDRW2100002177.1.17",
          "form": "아뇨 센스는 없는편이에요 ㅎㅎㅎ",
          "original_form": "아뇨 센스는 없는편이에요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 09:21"
        },
        {
          "id": "MDRW2100002177.1.18",
          "form": "사진도 잘찍어야 잘나가던데 에이 겸손하시네요",
          "original_form": "사진도 잘찍어야 잘나가던데 에이 겸손하시네요",
          "speaker_id": "2",
          "time": "20210820 09:21"
        },
        {
          "id": "MDRW2100002177.1.19",
          "form": "전 사진고 엉망이예요 ㅎㅎ",
          "original_form": "전 사진고 엉망이예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 09:21"
        },
        {
          "id": "MDRW2100002177.1.20",
          "form": "저도 엉망이에요 ㅋㅋ \r\n쇼핑몰 그대로 올려요 \r\n옷고르는 취향이 비슷하신분 같아요 사가는분이 ㅎ",
          "original_form": "저도 엉망이에요 ㅋㅋ \r\n쇼핑몰 그대로 올려요 \r\n옷고르는 취향이 비슷하신분 같아요 사가는분이 ㅎ",
          "speaker_id": "1",
          "time": "20210820 09:22"
        },
        {
          "id": "MDRW2100002177.1.21",
          "form": "저보고 사진을 발로 찍는데요 ㅋㅋ",
          "original_form": "저보고 사진을 발로 찍는데요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 09:23"
        },
        {
          "id": "MDRW2100002177.1.22",
          "form": "비슷하신분있으니 판매가 쉽고 서로 팔기 좋은거 같아요",
          "original_form": "비슷하신분있으니 판매가 쉽고 서로 팔기 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210820 09:24"
        },
        {
          "id": "MDRW2100002177.1.23",
          "form": "맞아요 ㅎㅎ \r\nㅋㅋ 저도 발로찍는다네요 ㅋㅋ 그뒤로부터  쇼핑몰 모델로 스샷을 ㅋㅋ",
          "original_form": "맞아요 ㅎㅎ \r\nㅋㅋ 저도 발로찍는다네요 ㅋㅋ 그뒤로부터  쇼핑몰 모델로 스샷을 ㅋㅋ",
          "speaker_id": "1",
