      "utterance": [
        {
          "id": "MDRW2100003452.1.1",
          "form": "지금 여기 갑자기 비온다",
          "original_form": "지금 여기 갑자기 비온다",
          "speaker_id": "1",
          "time": "20210820 23:15"
        },
        {
          "id": "MDRW2100003452.1.2",
          "form": "환기 시키고 있었는데 문 다 닫음",
          "original_form": "환기 시키고 있었는데 문 다 닫음",
          "speaker_id": "1",
          "time": "20210820 23:16"
        },
        {
          "id": "MDRW2100003452.1.3",
          "form": "그니까 여기도 갑자기 엄청 쏟아짐 ㅋㅋ",
          "original_form": "그니까 여기도 갑자기 엄청 쏟아짐 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 00:01"
        },
        {
          "id": "MDRW2100003452.1.4",
          "form": "창문 잽싸게 닫아야됨",
          "original_form": "창문 잽싸게 닫아야됨",
          "speaker_id": "2",
          "time": "20210821 00:01"
        },
        {
          "id": "MDRW2100003452.1.5",
          "form": "그래도 요즘 미세먼지가 좋네",
          "original_form": "그래도 요즘 미세먼지가 좋네",
          "speaker_id": "2",
          "time": "20210821 00:01"
        },
        {
          "id": "MDRW2100003452.1.6",
          "form": "내내 계속 오려나보네...",
          "original_form": "내내 계속 오려나보네...",
          "speaker_id": "1",
          "time": "20210821 10:17"
        },
        {
          "id": "MDRW2100003452.1.7",
          "form": "장마 시작인듯",
          "original_form": "장마 시작인듯",
          "speaker_id": "1",
          "time": "20210821 10:17"
        },
        {
          "id": "MDRW2100003452.1.8",
          "form": "2차장마올라는듯",
          "original_form": "2차장마올라는듯",
          "speaker_id": "2",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100003452.1.9",
          "form": "장마끝나면 날씨도 좀 풀리려나",
          "original_form": "장마끝나면 날씨도 좀 풀리려나",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100003452.1.10",
          "form": "이제 또 춥다 춥다 할일만 남은듯",
          "original_form": "이제 또 춥다 춥다 할일만 남은듯",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100003452.1.11",
          "form": "그렇게 쏟아지더니 또 바로 맑아버리네",
          "original_form": "그렇게 쏟아지더니 또 바로 맑아버리네",
          "speaker_id": "1",
          "time": "20210821 16:41"
        },
        {
          "id": "MDRW2100003452.1.12",
          "form": "ㅋㅋㅋㅋ날씨가 왔다갔다함",
          "original_form": "ㅋㅋㅋㅋ날씨가 왔다갔다함",
          "speaker_id": "2",
          "time": "20210821 18:19"
        },
        {
          "id": "MDRW2100003452.1.13",
          "form": "올해 비 별로 안올거같더니 조금씩 자주 쏟아지네",
          "original_form": "올해 비 별로 안올거같더니 조금씩 자주 쏟아지네",
          "speaker_id": "2",
          "time": "20210821 18:20"
        },
        {
          "id": "MDRW2100003452.1.14",
          "form": "원래 8월 말에 장마가 왔나...",
          "original_form": "원래 8월 말에 장마가 왔나...",
          "speaker_id": "1",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100003452.1.15",
          "form": "기억이 전혀 안나네 ㅋㅋㅋㅋㅋ",
          "original_form": "기억이 전혀 안나네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:12"
        },
        {
          "id": "MDRW2100003452.1.16",
          "form": "7월말부터 장마라고했었는데 조금 오고 지나감 ㅋㅋ",
          "original_form": "7월말부터 장마라고했었는데 조금 오고 지나감 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100003452.1.17",
          "form": "이제 가을오면 옷사러가야되는데",
          "original_form": "이제 가을오면 옷사러가야되는데",
          "speaker_id": "2",
          "time": "20210821 22:37"
        },
        {
          "id": "MDRW2100003452.1.18",
          "form": "그니까 7월은 좀.. 일기예보 정확했는데",
          "original_form": "그니까 7월은 좀.. 일기예보 정확했는데",
          "speaker_id": "1",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100003452.1.19",
          "form": "ㅋㅋㅋㅋ 가을 옷 사야지..... 작년에 뭐 입고 다녔나",
          "original_form": "ㅋㅋㅋㅋ 가을 옷 사야지..... 작년에 뭐 입고 다녔나",
          "speaker_id": "1",
          "time": "20210822 12:59"
        },
        {
          "id": "MDRW2100003452.1.20",
          "form": "옷사러 쇼핑가고싶다",
          "original_form": "옷사러 쇼핑가고싶다",
          "speaker_id": "2",
          "time": "20210823 10:01"
        },
        {
          "id": "MDRW2100003452.1.21",
          "form": "오늘내일 또 비오네",
          "original_form": "오늘내일 또 비오네",
          "speaker_id": "2",
          "time": "20210823 10:01"
        },
        {
          "id": "MDRW2100003452.1.22",
          "form": "이번주 내내 올걸.... ㅜㅜ",
          "original_form": "이번주 내내 올걸.... ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 11:48"
        },
        {
          "id": "MDRW2100003452.1.23",
          "form": "옷 세탁해도 늦게 마르게 생겼네",
          "original_form": "옷 세탁해도 늦게 마르게 생겼네",
          "speaker_id": "1",
          "time": "20210823 11:49"
        },
        {
          "id": "MDRW2100003452.1.24",
          "form": "ㅋㅋㅋㅋ건조기가 개꿀",
          "original_form": "ㅋㅋㅋㅋ건조기가 개꿀",
          "speaker_id": "2",
          "time": "20210823 11:55"
        },
        {
          "id": "MDRW2100003452.1.25",
          "form": "비 끝나면 이제 시원해지겠다",
          "original_form": "비 끝나면 이제 시원해지겠다",
          "speaker_id": "2",
          "time": "20210823 11:56"
        },
        {
          "id": "MDRW2100003452.1.26",
          "form": "건조기 없다..ㅜㅜ둘 곳이 없어",
          "original_form": "건조기 없다..ㅜㅜ둘 곳이 없어",
          "speaker_id": "1",
          "time": "20210823 12:40"
        },
        {
          "id": "MDRW2100003452.1.27",
          "form": "습한것만 좀 없어지길",
          "original_form": "습한것만 좀 없어지길",
