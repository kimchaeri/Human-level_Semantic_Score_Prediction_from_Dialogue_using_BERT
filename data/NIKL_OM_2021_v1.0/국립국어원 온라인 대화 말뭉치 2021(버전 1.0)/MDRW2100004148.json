        },
        {
          "id": "MDRW2100004148.1.2",
          "form": "name1님?",
          "original_form": "&name1&님?",
          "speaker_id": "2",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100004148.1.3",
          "form": "게임 콘텐츠에 대한 이야기를 나눠봅시다",
          "original_form": "게임 콘텐츠에 대한 이야기를 나눠봅시다",
          "speaker_id": "2",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100004148.1.4",
          "form": "게임 콘텐츠",
          "original_form": "게임 콘텐츠",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004148.1.5",
          "form": "아는게 없다",
          "original_form": "아는게 없다",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004148.1.6",
          "form": "그러하다",
          "original_form": "그러하다",
          "speaker_id": "2",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004148.1.7",
          "form": "로오히라던가",
          "original_form": "로오히라던가",
          "speaker_id": "2",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004148.1.8",
          "form": "페그오랑",
          "original_form": "페그오랑",
          "speaker_id": "2",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004148.1.9",
          "form": "로오히 재미없어서",
          "original_form": "로오히 재미없어서",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004148.1.10",
          "form": "프리파라도 게임이지",
          "original_form": "프리파라도 게임이지",
          "speaker_id": "2",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004148.1.11",
          "form": "안한ㅣ 오래됐는걸",
          "original_form": "안한ㅣ 오래됐는걸",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004148.1.12",
          "form": "프리파라ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "프리파라ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:12"
        },
        {
          "id": "MDRW2100004148.1.13",
          "form": "줄곧 궁금했던게 있어",
          "original_form": "줄곧 궁금했던게 있어",
          "speaker_id": "2",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004148.1.14",
          "form": "뭐",
          "original_form": "뭐",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004148.1.15",
          "form": "대체 프리즘의 힘이 뭐야",
          "original_form": "대체 프리즘의 힘이 뭐야",
          "speaker_id": "2",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004148.1.16",
          "form": "그걸 게임하는 사람도 모른다는게",
          "original_form": "그걸 게임하는 사람도 모른다는게",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004148.1.17",
          "form": "매력이지",
          "original_form": "매력이지",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004148.1.18",
          "form": "그리고 프리파라는 그렇게 샤랄라한데",
          "original_form": "그리고 프리파라는 그렇게 샤랄라한데",
          "speaker_id": "2",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100004148.1.19",
          "form": "같은 회사에서 나온 킹프리는",
          "original_form": "같은 회사에서 나온 킹프리는",
          "speaker_id": "2",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004148.1.20",
          "form": "대체 왜 그래...?",
          "original_form": "대체 왜 그래...?",
          "speaker_id": "2",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004148.1.21",
          "form": "이건 애니얘기아니냐",
          "original_form": "이건 애니얘기아니냐",
          "speaker_id": "1",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004148.1.22",
          "form": "그런가",
          "original_form": "그런가",
          "speaker_id": "2",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004148.1.23",
          "form": "11월에 스위치 포켓몬 게임 나온대",
          "original_form": "11월에 스위치 포켓몬 게임 나온대",
          "speaker_id": "1",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100004148.1.24",
