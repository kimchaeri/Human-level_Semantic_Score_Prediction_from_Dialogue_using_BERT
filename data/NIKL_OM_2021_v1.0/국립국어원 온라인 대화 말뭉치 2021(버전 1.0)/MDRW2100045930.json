      "utterance": [
        {
          "id": "MDRW2100045930.1.1",
          "form": "요즘 마스크를 끼니 피부 트러블이 장난아니네요",
          "original_form": "요즘 마스크를 끼니 피부 트러블이 장난아니네요",
          "speaker_id": "1",
          "time": "20210908 08:48"
        },
        {
          "id": "MDRW2100045930.1.2",
          "form": "저두요!",
          "original_form": "저두요!",
          "speaker_id": "2",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045930.1.3",
          "form": "왕뾰루지 났어요 ㅠㅠ",
          "original_form": "왕뾰루지 났어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045930.1.4",
          "form": "ㅠㅠ 그럼 너무 신경쓰이죠",
          "original_form": "ㅠㅠ 그럼 너무 신경쓰이죠",
          "speaker_id": "2",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045930.1.5",
          "form": "네!! 진짜 괜히 이것만 보이는 것 같고",
          "original_form": "네!! 진짜 괜히 이것만 보이는 것 같고",
          "speaker_id": "1",
          "time": "20210908 08:49"
        },
        {
          "id": "MDRW2100045930.1.6",
          "form": "마스크를 쓰니까 모공도 넓어지는것 같네요",
          "original_form": "마스크를 쓰니까 모공도 넓어지는것 같네요",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045930.1.7",
          "form": "맞아요 뜨거운 숨이 자꾸 마스크 안에 들어오니 ㅠ",
          "original_form": "맞아요 뜨거운 숨이 자꾸 마스크 안에 들어오니 ㅠ",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045930.1.8",
          "form": "그러게요.. 얼른 마스크를 벗는 날이 와야하는데요",
          "original_form": "그러게요.. 얼른 마스크를 벗는 날이 와야하는데요",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045930.1.9",
          "form": "ㅠㅠ 과연 올까요",
          "original_form": "ㅠㅠ 과연 올까요",
          "speaker_id": "1",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045930.1.10",
          "form": "의문이긴하네요",
          "original_form": "의문이긴하네요",
          "speaker_id": "2",
          "time": "20210908 08:50"
        },
        {
          "id": "MDRW2100045930.1.11",
          "form": "뾰루지 너무 아파요",
          "original_form": "뾰루지 너무 아파요",
          "speaker_id": "1",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045930.1.12",
          "form": "여드름 패치 붙이셨나요",
          "original_form": "여드름 패치 붙이셨나요",
          "speaker_id": "2",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045930.1.13",
          "form": "안그래도 붙이려했는데",
          "original_form": "안그래도 붙이려했는데",
          "speaker_id": "1",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045930.1.14",
          "form": "다 떨어져서 오늘 사려구요",
          "original_form": "다 떨어져서 오늘 사려구요",
          "speaker_id": "1",
          "time": "20210908 08:51"
        },
        {
          "id": "MDRW2100045930.1.15",
          "form": "그거 붙이면 좀 좋더라구요",
          "original_form": "그거 붙이면 좀 좋더라구요",
