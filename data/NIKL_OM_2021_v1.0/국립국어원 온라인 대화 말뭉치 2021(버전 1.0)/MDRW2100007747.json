      "utterance": [
        {
          "id": "MDRW2100007747.1.1",
          "form": "오늘 비 안오제??",
          "original_form": "오늘 비 안오제??",
          "speaker_id": "2",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100007747.1.2",
          "form": "애들 데리고 밖에 갔다와야겠구먼",
          "original_form": "애들 데리고 밖에 갔다와야겠구먼",
          "speaker_id": "2",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100007747.1.3",
          "form": "안오네ㅋㅋ",
          "original_form": "안오네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100007747.1.4",
          "form": "어디가는데?ㅋ",
          "original_form": "어디가는데?ㅋ",
          "speaker_id": "1",
          "time": "20210822 10:12"
        },
        {
          "id": "MDRW2100007747.1.5",
          "form": "글쎄.....놀이터?? ㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "글쎄.....놀이터?? ㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210822 10:14"
        },
        {
          "id": "MDRW2100007747.1.6",
          "form": "반도 놀이터에서 만나까?? ㅋㅋㅋㅋㅋㅋ",
          "original_form": "반도 놀이터에서 만나까?? ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:14"
        },
        {
          "id": "MDRW2100007747.1.7",
          "form": "어제 그 멤버그대로 ㅋㅋㅋㄱ",
          "original_form": "어제 그 멤버그대로 ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210822 10:14"
        },
        {
          "id": "MDRW2100007747.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 정말 밥먹고...설거지하고 돌아서니 점심시간이고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ아 정말 밥먹고...설거지하고 돌아서니 점심시간이고",
          "speaker_id": "1",
          "time": "20210822 12:13"
        },
        {
          "id": "MDRW2100007747.1.9",
          "form": "와... ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "와... ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:13"
        },
        {
          "id": "MDRW2100007747.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007747.1.11",
          "form": "나도 지금 나와서 계속 청소하고",
          "original_form": "나도 지금 나와서 계속 청소하고",
          "speaker_id": "2",
          "time": "20210822 12:24"
        },
        {
          "id": "MDRW2100007747.1.12",
          "form": "빨래하고 신발도 세탁하고",
          "original_form": "빨래하고 신발도 세탁하고",
          "speaker_id": "2",
          "time": "20210822 12:25"
        },
        {
          "id": "MDRW2100007747.1.13",
          "form": "이제 설거지할라한다",
          "original_form": "이제 설거지할라한다",
          "speaker_id": "2",
          "time": "20210822 12:26"
        },
        {
          "id": "MDRW2100007747.1.14",
          "form": "ㅋㅋㅋㅋ아이고 많이했네ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ아이고 많이했네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007747.1.15",
          "form": "나는 설거지만했다ㅋ",
          "original_form": "나는 설거지만했다ㅋ",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007747.1.16",
          "form": "청소는 슬슬해야된다",
          "original_form": "청소는 슬슬해야된다",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007747.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 화장실 청소 좀 하고",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ 화장실 청소 좀 하고",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007747.1.18",
          "form": "집안일은 너무 안끝난다.ㅋㅋ",
          "original_form": "집안일은 너무 안끝난다.ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007747.1.19",
          "form": "그래 청소 디다",
          "original_form": "그래 청소 디다",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007747.1.20",
          "form": "그러니깐ㅋㅋㅋㅋㅋ",
          "original_form": "그러니깐ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007747.1.21",
          "form": "피곤피곤ㅋㅋ",
          "original_form": "피곤피곤ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007747.1.22",
          "form": "맨날 하는건데 맨날 더러움",
          "original_form": "맨날 하는건데 맨날 더러움",
          "speaker_id": "2",
          "time": "20210822 12:27"
        },
        {
          "id": "MDRW2100007747.1.23",
          "form": "끝이없는 집안일",
          "original_form": "끝이없는 집안일",
          "speaker_id": "2",
          "time": "20210822 12:28"
        },
        {
          "id": "MDRW2100007747.1.24",
          "form": "그러니깐 또 집안일하고 점심차려야지ㅋㅋㅋ",
          "original_form": "그러니깐 또 집안일하고 점심차려야지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007747.1.25",
          "form": "점심먹고",
          "original_form": "점심먹고",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007747.1.26",
          "form": "나갈까 생각중이다ㅋㅋ",
          "original_form": "나갈까 생각중이다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007747.1.27",
          "form": "어디나갈지도 고민이네ㅋㅋ",
          "original_form": "어디나갈지도 고민이네ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 12:29"
        },
        {
          "id": "MDRW2100007747.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㄱ 놀이터",
          "original_form": "ㅋㅋㅋㅋㅋㅋㄱ 놀이터",
          "speaker_id": "2",
          "time": "20210822 12:30"
        },
        {
          "id": "MDRW2100007747.1.29",
