      "utterance": [
        {
          "id": "MDRW2100044072.1.1",
          "form": "제주에서 음식은 다 성공한것같아",
          "original_form": "제주에서 음식은 다 성공한것같아",
          "speaker_id": "1",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100044072.1.2",
          "form": "뭐 먹었어?",
          "original_form": "뭐 먹었어?",
          "speaker_id": "2",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100044072.1.3",
          "form": "소린이네횟집을 또갔어",
          "original_form": "소린이네횟집을 또갔어",
          "speaker_id": "1",
          "time": "20210907 21:34"
        },
        {
          "id": "MDRW2100044072.1.4",
          "form": "아.. 파라솔 아래서 먹었던집?",
          "original_form": "아.. 파라솔 아래서 먹었던집?",
          "speaker_id": "2",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044072.1.5",
          "form": "벵에돔, 뿔소라, 고등어회 먹었어",
          "original_form": "벵에돔, 뿔소라, 고등어회 먹었어",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044072.1.6",
          "form": "아니 시장횟집",
          "original_form": "아니 시장횟집",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044072.1.7",
          "form": "한치물회는 없더라구",
          "original_form": "한치물회는 없더라구",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044072.1.8",
          "form": "그집이 소린이네였어?",
          "original_form": "그집이 소린이네였어?",
          "speaker_id": "2",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044072.1.9",
          "form": "한치는 끝났데",
          "original_form": "한치는 끝났데",
          "speaker_id": "1",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044072.1.10",
          "form": "벵에돔 남편이 찾던거?",
          "original_form": "벵에돔 남편이 찾던거?",
          "speaker_id": "2",
          "time": "20210907 21:35"
        },
        {
          "id": "MDRW2100044072.1.11",
          "form": "어~마자",
          "original_form": "어~마자",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044072.1.12",
          "form": "있더라구",
          "original_form": "있더라구",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044072.1.13",
          "form": "얼마나왔어?",
          "original_form": "얼마나왔어?",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044072.1.14",
          "form": "양도 많았어?",
          "original_form": "양도 많았어?",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044072.1.15",
          "form": "13만원정도먹었지",
          "original_form": "13만원정도먹었지",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044072.1.16",
          "form": "또 먹고싶다",
          "original_form": "또 먹고싶다",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044072.1.17",
          "form": "회 생각난다",
          "original_form": "회 생각난다",
          "speaker_id": "2",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044072.1.18",
          "form": "정말 맛있었어",
          "original_form": "정말 맛있었어",
          "speaker_id": "1",
          "time": "20210907 21:36"
        },
        {
          "id": "MDRW2100044072.1.19",
          "form": "또 다른건 어떤거?",
          "original_form": "또 다른건 어떤거?",
          "speaker_id": "2",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044072.1.20",
          "form": "제주약수터라는 수제 맥주집도 가서 사서 먹었는데",
          "original_form": "제주약수터라는 수제 맥주집도 가서 사서 먹었는데",
          "speaker_id": "1",
          "time": "20210907 21:37"
        },
        {
          "id": "MDRW2100044072.1.21",
          "form": "수제맥주? 오호~",
          "original_form": "수제맥주? 오호~",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044072.1.22",
          "form": "귤향나는 수제맥주 진짜 맛잇었어",
          "original_form": "귤향나는 수제맥주 진짜 맛잇었어",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044072.1.23",
          "form": "담에 또 먹고싶더라",
          "original_form": "담에 또 먹고싶더라",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044072.1.24",
          "form": "아흐~ 좋았겠다",
          "original_form": "아흐~ 좋았겠다",
          "speaker_id": "2",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044072.1.25",
          "form": "오는날 동문시장 갈치조림도 맛있었구",
          "original_form": "오는날 동문시장 갈치조림도 맛있었구",
          "speaker_id": "1",
          "time": "20210907 21:38"
        },
        {
          "id": "MDRW2100044072.1.26",
          "form": "갈치조림?  맛있었겠네.",
          "original_form": "갈치조림?  맛있었겠네.",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044072.1.27",
          "form": "지난번과 다른메뉴네",
          "original_form": "지난번과 다른메뉴네",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044072.1.28",
          "form": "우도에서는 뭐 먹었어?",
          "original_form": "우도에서는 뭐 먹었어?",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044072.1.29",
          "form": "자장면?",
          "original_form": "자장면?",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044072.1.30",
          "form": "아니 뿔소라와 보말칼국수",
          "original_form": "아니 뿔소라와 보말칼국수",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044072.1.31",
          "form": "땅콩막걸리 아이스크림",
          "original_form": "땅콩막걸리 아이스크림",
          "speaker_id": "1",
          "time": "20210907 21:40"
        }
      ]
