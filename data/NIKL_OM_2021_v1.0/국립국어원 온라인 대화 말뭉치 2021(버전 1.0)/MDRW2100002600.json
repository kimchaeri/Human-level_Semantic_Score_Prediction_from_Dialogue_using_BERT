      "utterance": [
        {
          "id": "MDRW2100002600.1.1",
          "form": "민트초코 좋아하시나 보네요",
          "original_form": "민트초코 좋아하시나 보네요",
          "speaker_id": "1",
          "time": "20210820 13:50"
        },
        {
          "id": "MDRW2100002600.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210820 13:51"
        },
        {
          "id": "MDRW2100002600.1.3",
          "form": "네 좋아합니다",
          "original_form": "네 좋아합니다",
          "speaker_id": "2",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002600.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002600.1.5",
          "form": "저도 좋아해요! 민초단입니다 ㅎㅎ",
          "original_form": "저도 좋아해요! 민초단입니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002600.1.6",
          "form": "민트초코 들어간 커피 음료 넘 맛난것 같아요",
          "original_form": "민트초코 들어간 커피 음료 넘 맛난것 같아요",
          "speaker_id": "1",
          "time": "20210820 13:52"
        },
        {
          "id": "MDRW2100002600.1.7",
          "form": "동지네요ㅋㅋ",
          "original_form": "동지네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002600.1.8",
          "form": "어디꺼 좋아하시나요",
          "original_form": "어디꺼 좋아하시나요",
          "speaker_id": "2",
          "time": "20210820 13:53"
        },
        {
          "id": "MDRW2100002600.1.9",
          "form": "음 스타벅스에서 팔던거 괜찮았어요!ㅋㅋ",
          "original_form": "음 스타벅스에서 팔던거 괜찮았어요!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002600.1.10",
          "form": "민초 아이스크림도 좋아하시나요 ㅎㅎ",
          "original_form": "민초 아이스크림도 좋아하시나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 13:54"
        },
        {
          "id": "MDRW2100002600.1.11",
          "form": "스타벅스 맛있다던데 아직 못먹어봤네요",
          "original_form": "스타벅스 맛있다던데 아직 못먹어봤네요",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002600.1.12",
          "form": "네 민초아이스크림",
          "original_form": "네 민초아이스크림",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002600.1.13",
          "form": "정말 좋아해요",
          "original_form": "정말 좋아해요",
          "speaker_id": "2",
          "time": "20210820 13:55"
        },
        {
          "id": "MDRW2100002600.1.14",
          "form": "저두요 ㅎㅎ 싫어하는 사람들을 이해할수가 없어요",
          "original_form": "저두요 ㅎㅎ 싫어하는 사람들을 이해할수가 없어요",
          "speaker_id": "1",
          "time": "20210820 13:57"
        },
        {
          "id": "MDRW2100002600.1.15",
          "form": "그니까여ㅋㅋ",
          "original_form": "그니까여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002600.1.16",
          "form": "을매나 맛있는데..",
          "original_form": "을매나 맛있는데..",
          "speaker_id": "2",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002600.1.17",
          "form": "마자용 ㅋㅋ 이해불가~",
          "original_form": "마자용 ㅋㅋ 이해불가~",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002600.1.18",
          "form": "주말에 한번 먹어야겠어요",
          "original_form": "주말에 한번 먹어야겠어요",
          "speaker_id": "1",
          "time": "20210820 13:59"
        },
        {
          "id": "MDRW2100002600.1.19",
          "form": "저는 오늘...먹어야겠어요",
          "original_form": "저는 오늘...먹어야겠어요",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002600.1.20",
          "form": "그 개운한맛이",
          "original_form": "그 개운한맛이",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002600.1.21",
          "form": "그립네요",
          "original_form": "그립네요",
          "speaker_id": "2",
          "time": "20210820 14:00"
        },
        {
          "id": "MDRW2100002600.1.22",
          "form": "맞아요 주기적으로 먹어줘야해요 ..",
          "original_form": "맞아요 주기적으로 먹어줘야해요 ..",
          "speaker_id": "1",
          "time": "20210820 14:02"
        },
        {
          "id": "MDRW2100002600.1.23",
          "form": "민초말고는 무슨맛 좋아하시나요?",
          "original_form": "민초말고는 무슨맛 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210820 14:04"
        },
        {
          "id": "MDRW2100002600.1.24",
          "form": "그냥 초코맛도 좋아해요!",
          "original_form": "그냥 초코맛도 좋아해요!",
          "speaker_id": "1",
          "time": "20210820 14:10"
        }
      ]
