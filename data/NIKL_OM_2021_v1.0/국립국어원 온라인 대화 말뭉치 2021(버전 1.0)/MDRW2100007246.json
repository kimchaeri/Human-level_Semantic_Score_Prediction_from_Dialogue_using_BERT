      "utterance": [
        {
          "id": "MDRW2100007246.1.1",
          "form": "장칼국수 좋아하시나봐요!",
          "original_form": "장칼국수 좋아하시나봐요!",
          "speaker_id": "1",
          "time": "20210822 01:02"
        },
        {
          "id": "MDRW2100007246.1.2",
          "form": "좋아하긴 해요",
          "original_form": "좋아하긴 해요",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007246.1.3",
          "form": "음식을 좋아하고 닉네임을 지을 때 그때그때 생각나는 음식으로 하는 편이에요ㅎㅎ",
          "original_form": "음식을 좋아하고 닉네임을 지을 때 그때그때 생각나는 음식으로 하는 편이에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007246.1.4",
          "form": "아하 그럼 최근에 자주 드셨던 음식은 무엇인가요?",
          "original_form": "아하 그럼 최근에 자주 드셨던 음식은 무엇인가요?",
          "speaker_id": "1",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100007246.1.5",
          "form": "음 자주 먹는 건 떡볶이에요",
          "original_form": "음 자주 먹는 건 떡볶이에요",
          "speaker_id": "2",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007246.1.6",
          "form": "좋아하는 음식이 뭐예요?",
          "original_form": "좋아하는 음식이 뭐예요?",
          "speaker_id": "2",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007246.1.7",
          "form": "어 저도 떡볶이 자주 먹어요",
          "original_form": "어 저도 떡볶이 자주 먹어요",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007246.1.8",
          "form": "오늘 엽떡 배달 시켜먹었거든요!",
          "original_form": "오늘 엽떡 배달 시켜먹었거든요!",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007246.1.9",
          "form": "좋아하는건 치킨 좋아해요",
          "original_form": "좋아하는건 치킨 좋아해요",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007246.1.10",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007246.1.11",
          "form": "저는 삼겹살 먹었어요",
          "original_form": "저는 삼겹살 먹었어요",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007246.1.12",
          "form": "떡볶이는 언제나 맛있죠",
          "original_form": "떡볶이는 언제나 맛있죠",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007246.1.13",
          "form": "치킨은 양념vs후라이드",
          "original_form": "치킨은 양념vs후라이드",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007246.1.14",
          "form": "와 삼겹살에 된장찌개/김치찌개",
          "original_form": "와 삼겹살에 된장찌개/김치찌개",
          "speaker_id": "1",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007246.1.15",
          "form": "전 닥 후라이드요",
          "original_form": "전 닥 후라이드요",
          "speaker_id": "1",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007246.1.16",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007246.1.17",
          "form": "전 된찌요!",
          "original_form": "전 된찌요!",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007246.1.18",
          "form": "김치찌개가 소울푸드긴한데 삼겹살엔 된장찌개가 좋아요",
          "original_form": "김치찌개가 소울푸드긴한데 삼겹살엔 된장찌개가 좋아요",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007246.1.19",
          "form": "치킨은 후라이드구요ㅎㅎ",
          "original_form": "치킨은 후라이드구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007246.1.20",
          "form": "저도 삼겹살엔 된찌요! 호박이랑 두부 많은...",
          "original_form": "저도 삼겹살엔 된찌요! 호박이랑 두부 많은...",
          "speaker_id": "1",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007246.1.21",
          "form": "오호 후라이드도 겹치네요!",
          "original_form": "오호 후라이드도 겹치네요!",
          "speaker_id": "1",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007246.1.22",
          "form": "그렇담 물냉과 비냉 사이는~~?",
          "original_form": "그렇담 물냉과 비냉 사이는~~?",
          "speaker_id": "1",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007246.1.23",
          "form": "음 물냉비냉은 다 좋은데 굳이 고르자면 비냉요",
          "original_form": "음 물냉비냉은 다 좋은데 굳이 고르자면 비냉요",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007246.1.24",
          "form": "근데 비냉에 육수 좀 부어먹는 걸 좋아해요",
          "original_form": "근데 비냉에 육수 좀 부어먹는 걸 좋아해요",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007246.1.25",
          "form": "어 저도요 거의 비냉만 먹어요",
          "original_form": "어 저도요 거의 비냉만 먹어요",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007246.1.26",
          "form": "거기에 온육수 주는 가게면 완전 땡큐..",
          "original_form": "거기에 온육수 주는 가게면 완전 땡큐..",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007246.1.27",
          "form": "그럼 평양냉면은 안 좋아하시나요?",
          "original_form": "그럼 평양냉면은 안 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007246.1.28",
          "form": "온육수 너무 좋죠",
          "original_form": "온육수 너무 좋죠",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007246.1.29",
          "form": "평양냉면은 안먹어봤어요! 완전 슴슴한 맛이라던데 맞나요?",
          "original_form": "평양냉면은 안먹어봤어요! 완전 슴슴한 맛이라던데 맞나요?",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007246.1.30",
          "form": "맞아요 차가워진 속을 따땃하게..적셔..",
          "original_form": "맞아요 차가워진 속을 따땃하게..적셔..",
          "speaker_id": "1",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007246.1.31",
          "form": "완전 슴슴한데 전 좋아해요",
          "original_form": "완전 슴슴한데 전 좋아해요",
          "speaker_id": "2",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007246.1.32",
          "form": "제가 평양냉면 처음 먹었을 때 같이 갔던 세 명이 다 그날 처음 먹었던 건데 저만 맛있어했어요..",
          "original_form": "제가 평양냉면 처음 먹었을 때 같이 갔던 세 명이 다 그날 처음 먹었던 건데 저만 맛있어했어요..",
          "speaker_id": "2",
          "time": "20210822 01:34"
        }
      ]
