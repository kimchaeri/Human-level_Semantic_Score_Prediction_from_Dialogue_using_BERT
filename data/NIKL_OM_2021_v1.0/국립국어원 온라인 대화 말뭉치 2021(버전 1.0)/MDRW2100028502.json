      "utterance": [
        {
          "id": "MDRW2100028502.1.1",
          "form": "저는 최근에 갤럭시 핏을 구매했답니다",
          "original_form": "저는 최근에 갤럭시 핏을 구매했답니다",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028502.1.2",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028502.1.3",
          "form": "부러워요!",
          "original_form": "부러워요!",
          "speaker_id": "2",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028502.1.4",
          "form": "저는 기계치라 기계욕심이 없어서 사진않지만",
          "original_form": "저는 기계치라 기계욕심이 없어서 사진않지만",
          "speaker_id": "2",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028502.1.5",
          "form": "멋있는거같아요",
          "original_form": "멋있는거같아요",
          "speaker_id": "2",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028502.1.6",
          "form": "저도 이런거 처음 사봐요",
          "original_form": "저도 이런거 처음 사봐요",
          "speaker_id": "1",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028502.1.7",
          "form": "자는 거 기록이랑 걷는거 기록이 돼서 좀 좋아요",
          "original_form": "자는 거 기록이랑 걷는거 기록이 돼서 좀 좋아요",
          "speaker_id": "1",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028502.1.8",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028502.1.9",
          "form": "잘때끼는거 안불편하세요?!",
          "original_form": "잘때끼는거 안불편하세요?!",
          "speaker_id": "2",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028502.1.10",
          "form": "어려운 기계는 쓸 엄두가 안나서 일부러 좀 저렴한거 샀어요",
          "original_form": "어려운 기계는 쓸 엄두가 안나서 일부러 좀 저렴한거 샀어요",
          "speaker_id": "1",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028502.1.11",
          "form": "처음에 불편했는데 지금은 적응됐어요ㅎㅎ",
          "original_form": "처음에 불편했는데 지금은 적응됐어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 20:30"
        },
        {
          "id": "MDRW2100028502.1.12",
          "form": "아 핏은 좀 저렴한가요?!!",
          "original_form": "아 핏은 좀 저렴한가요?!!",
          "speaker_id": "2",
          "time": "20210902 20:33"
        },
        {
          "id": "MDRW2100028502.1.13",
          "form": "3만원대 후반에 살수있어요!",
          "original_form": "3만원대 후반에 살수있어요!",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028502.1.14",
          "form": "대신 기능은 좀 덜 좋은거 같긴 해요",
          "original_form": "대신 기능은 좀 덜 좋은거 같긴 해요",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028502.1.15",
          "form": "name2님은 요즘 뭐 사신거 있나요",
          "original_form": "&name2&님은 요즘 뭐 사신거 있나요",
          "speaker_id": "1",
          "time": "20210902 20:34"
        },
        {
          "id": "MDRW2100028502.1.16",
          "form": "저는 발목접질러서 발목아대샀어요,,,",
          "original_form": "저는 발목접질러서 발목아대샀어요,,,",
          "speaker_id": "2",
          "time": "20210902 20:35"
        },
        {
          "id": "MDRW2100028502.1.17",
          "form": "아이고ㅠㅠ",
          "original_form": "아이고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028502.1.18",
          "form": "지금은 발목 좀 괜찮으세요?",
          "original_form": "지금은 발목 좀 괜찮으세요?",
          "speaker_id": "1",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028502.1.19",
          "form": "요즘 운동에 맛들렸었는데 하필다쳐서",
          "original_form": "요즘 운동에 맛들렸었는데 하필다쳐서",
          "speaker_id": "2",
          "time": "20210902 20:36"
        },
        {
          "id": "MDRW2100028502.1.20",
          "form": "요즘 못가고있어서 아쉬워요,,,",
          "original_form": "요즘 못가고있어서 아쉬워요,,,",
          "speaker_id": "2",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028502.1.21",
          "form": "ㅜㅜ 얼른 나으셔야할텐데",
          "original_form": "ㅜㅜ 얼른 나으셔야할텐데",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028502.1.22",
          "form": "운동 뭐하세요?",
          "original_form": "운동 뭐하세요?",
          "speaker_id": "1",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028502.1.23",
          "form": "공원 걷고뛰고해요!",
          "original_form": "공원 걷고뛰고해요!",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028502.1.24",
          "form": "그래서 스마트워치류에 관심이,,,!",
          "original_form": "그래서 스마트워치류에 관심이,,,!",
          "speaker_id": "2",
          "time": "20210902 20:38"
        },
        {
          "id": "MDRW2100028502.1.25",
          "form": "와 저도 요즘 뛰고 걷느라 산거예요!",
          "original_form": "와 저도 요즘 뛰고 걷느라 산거예요!",
          "speaker_id": "1",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028502.1.26",
          "form": "런데이하는분들 진짜많더라고요",
          "original_form": "런데이하는분들 진짜많더라고요",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028502.1.27",
