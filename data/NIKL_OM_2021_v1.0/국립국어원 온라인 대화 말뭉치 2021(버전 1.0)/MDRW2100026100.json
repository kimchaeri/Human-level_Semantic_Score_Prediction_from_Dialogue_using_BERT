        },
        {
          "id": "MDRW2100026100.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026100.1.10",
          "form": "스포츠경기",
          "original_form": "스포츠경기",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026100.1.11",
          "form": "관련대화해야햐",
          "original_form": "관련대화해야햐",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026100.1.12",
          "form": "맞네..",
          "original_form": "맞네..",
          "speaker_id": "1",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026100.1.13",
          "form": "최근에 올림픽이긴했는데",
          "original_form": "최근에 올림픽이긴했는데",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026100.1.14",
          "form": "나 올림픽 안봤어",
          "original_form": "나 올림픽 안봤어",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026100.1.15",
          "form": "넌 봤어?",
          "original_form": "넌 봤어?",
          "speaker_id": "2",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026100.1.16",
          "form": "나는 올림픽",
          "original_form": "나는 올림픽",
          "speaker_id": "1",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026100.1.17",
          "form": "여자양궁이랑 여자배구 각각 마지막 봤어",
          "original_form": "여자양궁이랑 여자배구 각각 마지막 봤어",
          "speaker_id": "1",
          "time": "20210901 19:44"
        },
        {
          "id": "MDRW2100026100.1.18",
          "form": "난 결과만 본듯??",
          "original_form": "난 결과만 본듯??",
          "speaker_id": "2",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026100.1.19",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026100.1.20",
          "form": "여자배구 마지막은 집에 오면서 버스타고",
          "original_form": "여자배구 마지막은 집에 오면서 버스타고",
          "speaker_id": "2",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026100.1.21",
          "form": "오우",
          "original_form": "오우",
          "speaker_id": "1",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026100.1.22",
          "form": "오는데 기사님이 틀어주셔서 조금 봤어",
          "original_form": "오는데 기사님이 틀어주셔서 조금 봤어",
          "speaker_id": "2",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026100.1.23",
          "form": "상대팀 엄청... 무서웠어",
          "original_form": "상대팀 엄청... 무서웠어",
          "speaker_id": "2",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026100.1.24",
          "form": "웹툰 에필로그에 나올 거 같다",
          "original_form": "웹툰 에필로그에 나올 거 같다",
          "speaker_id": "1",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026100.1.25",
          "form": "맞아...",
          "original_form": "맞아...",
          "speaker_id": "1",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026100.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:45"
        },
        {
          "id": "MDRW2100026100.1.27",
          "form": "나는 집에서 티비로 봤어",
          "original_form": "나는 집에서 티비로 봤어",
          "speaker_id": "1",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026100.1.28",
          "form": "여자배구 선수의 꿈을 키우는",
          "original_form": "여자배구 선수의 꿈을 키우는",
          "speaker_id": "2",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026100.1.29",
          "form": "좋다",
          "original_form": "좋다",
          "speaker_id": "2",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026100.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026100.1.31",
          "form": "치킨도 먹었어?",
          "original_form": "치킨도 먹었어?",
          "speaker_id": "2",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026100.1.32",
          "form": "운좋게 안산 선수 마지막 볼 때는",
          "original_form": "운좋게 안산 선수 마지막 볼 때는",
          "speaker_id": "1",
          "time": "20210901 19:46"
        },
        {
          "id": "MDRW2100026100.1.33",
          "form": "치킨과 함께했다",
          "original_form": "치킨과 함께했다",
          "speaker_id": "1",
          "time": "20210901 19:46"
        },
