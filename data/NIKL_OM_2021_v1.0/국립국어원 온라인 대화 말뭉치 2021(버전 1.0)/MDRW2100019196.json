        },
        {
          "id": "MDRW2100019196.1.2",
          "form": "도토리 조회 해봣음?",
          "original_form": "도토리 조회 해봣음?",
          "speaker_id": "2",
          "time": "20210827 16:09"
        },
        {
          "id": "MDRW2100019196.1.3",
          "form": "오  그거 아직도 남아있남?",
          "original_form": "오  그거 아직도 남아있남?",
          "speaker_id": "1",
          "time": "20210827 16:09"
        },
        {
          "id": "MDRW2100019196.1.4",
          "form": "아니 도토리 남아잇음 환불 해준다던데",
          "original_form": "아니 도토리 남아잇음 환불 해준다던데",
          "speaker_id": "2",
          "time": "20210827 16:10"
        },
        {
          "id": "MDRW2100019196.1.5",
          "form": "아..아직 로그인도 못해봤네",
          "original_form": "아..아직 로그인도 못해봤네",
          "speaker_id": "1",
          "time": "20210827 16:10"
        },
        {
          "id": "MDRW2100019196.1.6",
          "form": "니는 확인함??",
          "original_form": "니는 확인함??",
          "speaker_id": "1",
          "time": "20210827 16:11"
        },
        {
          "id": "MDRW2100019196.1.7",
          "form": "아니... 나의 흑역사..",
          "original_form": "아니... 나의 흑역사..",
          "speaker_id": "2",
          "time": "20210827 16:11"
        },
        {
          "id": "MDRW2100019196.1.8",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100019196.1.9",
          "form": "저번에 사진챙기랄때 다 챙겨갖고",
          "original_form": "저번에 사진챙기랄때 다 챙겨갖고",
          "speaker_id": "1",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100019196.1.10",
          "form": "사진 챙기랫음?",
          "original_form": "사진 챙기랫음?",
          "speaker_id": "2",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100019196.1.11",
          "form": "예전에 문닫는다고 그랬던적있자나",
          "original_form": "예전에 문닫는다고 그랬던적있자나",
          "speaker_id": "1",
          "time": "20210827 16:13"
        },
        {
          "id": "MDRW2100019196.1.12",
          "form": "근데 싸이월드는 정식 오픈한건가?",
          "original_form": "근데 싸이월드는 정식 오픈한건가?",
          "speaker_id": "1",
          "time": "20210827 16:14"
        },
        {
          "id": "MDRW2100019196.1.13",
          "form": "사진은 첨 들엇네",
          "original_form": "사진은 첨 들엇네",
          "speaker_id": "2",
          "time": "20210827 16:14"
        },
        {
          "id": "MDRW2100019196.1.14",
          "form": "아니 심폐소생술중",
          "original_form": "아니 심폐소생술중",
          "speaker_id": "2",
          "time": "20210827 16:14"
        },
        {
          "id": "MDRW2100019196.1.15",
          "form": "앜ㅋㅋ",
          "original_form": "앜ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 16:14"
        },
        {
          "id": "MDRW2100019196.1.16",
          "form": "광고는 많이하던데",
          "original_form": "광고는 많이하던데",
          "speaker_id": "1",
          "time": "20210827 16:14"
        },
        {
          "id": "MDRW2100019196.1.17",
          "form": "나도 영화관서 봄",
          "original_form": "나도 영화관서 봄",
          "speaker_id": "2",
          "time": "20210827 16:15"
        },
        {
          "id": "MDRW2100019196.1.18",
          "form": "추억음악 소환하던데 그래서 오픈한줄",
