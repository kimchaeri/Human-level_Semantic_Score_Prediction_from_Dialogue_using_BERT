      "utterance": [
        {
          "id": "MDRW2100015137.1.1",
          "form": "요즘 날씨 풀려서 너무 좋아요!",
          "original_form": "요즘 날씨 풀려서 너무 좋아요!",
          "speaker_id": "1",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015137.1.2",
          "form": "이제 진짜 가을이 온 것 같네요 ㅜㅜ",
          "original_form": "이제 진짜 가을이 온 것 같네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210825 21:57"
        },
        {
          "id": "MDRW2100015137.1.3",
          "form": "맞아요 ㅋㅋ 선선해졌어요",
          "original_form": "맞아요 ㅋㅋ 선선해졌어요",
          "speaker_id": "2",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015137.1.4",
          "form": "근데 계속 비가 와서 ㅠ",
          "original_form": "근데 계속 비가 와서 ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015137.1.5",
          "form": "맞아요.. 저 우산 안들고 나왔는데 그때마다 내리는 소나기..",
          "original_form": "맞아요.. 저 우산 안들고 나왔는데 그때마다 내리는 소나기..",
          "speaker_id": "1",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015137.1.6",
          "form": "당황스럽더라구요..",
          "original_form": "당황스럽더라구요..",
          "speaker_id": "1",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015137.1.7",
          "form": "맞아요 금욜까지는 많이 내린다고하던데",
          "original_form": "맞아요 금욜까지는 많이 내린다고하던데",
          "speaker_id": "2",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015137.1.8",
          "form": "점점 아열대 날씨로 바뀌는거같아요 ㅠ",
          "original_form": "점점 아열대 날씨로 바뀌는거같아요 ㅠ",
          "speaker_id": "2",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015137.1.9",
          "form": "봄가을 없어지구",
          "original_form": "봄가을 없어지구",
          "speaker_id": "2",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015137.1.10",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015137.1.11",
          "form": "그럼 올해 여름은 작년 여름하고 비교했을 때 어땠어요?",
          "original_form": "그럼 올해 여름은 작년 여름하고 비교했을 때 어땠어요?",
          "speaker_id": "1",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015137.1.12",
          "form": "어 일단 비가 진짜 작년에 비해서",
          "original_form": "어 일단 비가 진짜 작년에 비해서",
          "speaker_id": "2",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015137.1.13",
          "form": "안왔어요",
          "original_form": "안왔어요",
          "speaker_id": "2",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015137.1.14",
          "form": "작년이 최장기간 장마였나요?ㅋㅋㅋ",
          "original_form": "작년이 최장기간 장마였나요?ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015137.1.15",
          "form": "그런가요? 그래서 더 더웠던거같애요",
          "original_form": "그런가요? 그래서 더 더웠던거같애요",
          "speaker_id": "2",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015137.1.16",
          "form": "비내려야지 좀시원한데 ㅋㅋ",
          "original_form": "비내려야지 좀시원한데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015137.1.17",
          "form": "그래도 올해 코로나 덕분에 집에만 있었더니",
          "original_form": "그래도 올해 코로나 덕분에 집에만 있었더니",
          "speaker_id": "1",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015137.1.18",
          "form": "에어컨 밑에서 선선하게 보냈네요",
          "original_form": "에어컨 밑에서 선선하게 보냈네요",
          "speaker_id": "1",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015137.1.19",
          "form": "아 맞아요 ㅋㅋㅋ비오는 날 좋아하세요??",
          "original_form": "아 맞아요 ㅋㅋㅋ비오는 날 좋아하세요??",
          "speaker_id": "2",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015137.1.20",
          "form": "예전엔 싫었는데 한 때는 좋았다가 지금은 다시 싫어졌어요.",
          "original_form": "예전엔 싫었는데 한 때는 좋았다가 지금은 다시 싫어졌어요.",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015137.1.21",
          "form": "좋았던 이유는 오로지 파전과 막걸리 때문입니다..",
          "original_form": "좋았던 이유는 오로지 파전과 막걸리 때문입니다..",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015137.1.22",
          "form": "아하 ㅋㅋㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015137.1.23",
          "form": "싫은 이유는 우산 들고 다니기 거추장스러워요.",
          "original_form": "싫은 이유는 우산 들고 다니기 거추장스러워요.",
          "speaker_id": "1",
          "time": "20210825 22:01"
        },
        {
          "id": "MDRW2100015137.1.24",
          "form": "저는 양말젖고 물 튀기는게 싫더라고요",
          "original_form": "저는 양말젖고 물 튀기는게 싫더라고요",
          "speaker_id": "2",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015137.1.25",
          "form": "저 새신발 샀는데 벌써 헌신발 됐잖아요..",
          "original_form": "저 새신발 샀는데 벌써 헌신발 됐잖아요..",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015137.1.26",
          "form": "우산도 진짜 거추장스럽죠",
          "original_form": "우산도 진짜 거추장스럽죠",
          "speaker_id": "2",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015137.1.27",
          "form": "양말 젖는거 공감 ㅋㅋㅋㅋ",
          "original_form": "양말 젖는거 공감 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:02"
        }
      ]
