      "utterance": [
        {
          "id": "MDRW2100038244.1.1",
          "form": "그렇겠지",
          "original_form": "그렇겠지",
          "speaker_id": "2",
          "time": "20210902 01:45"
        },
        {
          "id": "MDRW2100038244.1.2",
          "form": "응 이서진이 안한다고 하지만 열심히 하니까",
          "original_form": "응 이서진이 안한다고 하지만 열심히 하니까",
          "speaker_id": "1",
          "time": "20210902 01:45"
        },
        {
          "id": "MDRW2100038244.1.3",
          "form": "첫날 전기가 나갔네",
          "original_form": "첫날 전기가 나갔네",
          "speaker_id": "2",
          "time": "20210902 01:45"
        },
        {
          "id": "MDRW2100038244.1.4",
          "form": "그러게 그래도 금방 다시 전기가 들어와서 다행",
          "original_form": "그러게 그래도 금방 다시 전기가 들어와서 다행",
          "speaker_id": "1",
          "time": "20210902 01:46"
        },
        {
          "id": "MDRW2100038244.1.5",
          "form": "1인1음식해야하는지 서로 의논하네",
          "original_form": "1인1음식해야하는지 서로 의논하네",
          "speaker_id": "2",
          "time": "20210902 01:46"
        },
        {
          "id": "MDRW2100038244.1.6",
          "form": "요즘 인건비도 비싸고 그래서 1인1음식주문은 기본인듯해",
          "original_form": "요즘 인건비도 비싸고 그래서 1인1음식주문은 기본인듯해",
          "speaker_id": "1",
          "time": "20210902 01:47"
        },
        {
          "id": "MDRW2100038244.1.7",
          "form": "첫 손님들이 스웨덴",
          "original_form": "첫 손님들이 스웨덴",
          "speaker_id": "2",
          "time": "20210902 01:47"
        },
        {
          "id": "MDRW2100038244.1.8",
          "form": "생각보다 손님들이 많이 왔네 그러게",
          "original_form": "생각보다 손님들이 많이 왔네 그러게",
          "speaker_id": "1",
          "time": "20210902 01:47"
        },
        {
          "id": "MDRW2100038244.1.9",
          "form": "외국은 저런 팁문화",
          "original_form": "외국은 저런 팁문화",
          "speaker_id": "2",
          "time": "20210902 01:48"
        },
        {
          "id": "MDRW2100038244.1.10",
          "form": "엉 우리에게는 생소하지",
          "original_form": "엉 우리에게는 생소하지",
          "speaker_id": "1",
          "time": "20210902 01:48"
        },
        {
          "id": "MDRW2100038244.1.11",
          "form": "괜히 돈을 더 내는거 같으니까",
          "original_form": "괜히 돈을 더 내는거 같으니까",
          "speaker_id": "2",
          "time": "20210902 01:48"
        },
        {
          "id": "MDRW2100038244.1.12",
          "form": "그래도 외국나가면 외국에서하는대로 해야지",
          "original_form": "그래도 외국나가면 외국에서하는대로 해야지",
          "speaker_id": "1",
          "time": "20210902 01:49"
        },
        {
          "id": "MDRW2100038244.1.13",
          "form": "브레이크 타임",
          "original_form": "브레이크 타임",
          "speaker_id": "2",
          "time": "20210902 01:49"
        },
        {
          "id": "MDRW2100038244.1.14",
          "form": "직원들 점심 먹을려나봐",
          "original_form": "직원들 점심 먹을려나봐",
          "speaker_id": "1",
          "time": "20210902 01:49"
        },
        {
          "id": "MDRW2100038244.1.15",
          "form": "쉬라고 하는데도 윤사장 따라와서 잔소리 ㅋ",
          "original_form": "쉬라고 하는데도 윤사장 따라와서 잔소리 ㅋ",
