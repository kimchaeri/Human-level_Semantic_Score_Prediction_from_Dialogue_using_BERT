      "utterance": [
        {
          "id": "MDRW2100018565.1.1",
          "form": "혹시 지금 위시템 있나요?",
          "original_form": "혹시 지금 위시템 있나요?",
          "speaker_id": "2",
          "time": "20210827 12:09"
        },
        {
          "id": "MDRW2100018565.1.2",
          "form": "딱히 갖고 싶은게 없어서",
          "original_form": "딱히 갖고 싶은게 없어서",
          "speaker_id": "1",
          "time": "20210827 12:10"
        },
        {
          "id": "MDRW2100018565.1.3",
          "form": "name2님은 있으신가요?",
          "original_form": "&name2&님은 있으신가요?",
          "speaker_id": "1",
          "time": "20210827 12:11"
        },
        {
          "id": "MDRW2100018565.1.4",
          "form": "저는 삼성 제트플립이요!!",
          "original_form": "저는 삼성 제트플립이요!!",
          "speaker_id": "2",
          "time": "20210827 12:12"
        },
        {
          "id": "MDRW2100018565.1.5",
          "form": "청소기죠",
          "original_form": "청소기죠",
          "speaker_id": "1",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018565.1.6",
          "form": "좋아보이더라",
          "original_form": "좋아보이더라",
          "speaker_id": "1",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018565.1.7",
          "form": "저는 최근에 머지포인트 구입했다가",
          "original_form": "저는 최근에 머지포인트 구입했다가",
          "speaker_id": "1",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018565.1.8",
          "form": "사용처가 중지돼서",
          "original_form": "사용처가 중지돼서",
          "speaker_id": "1",
          "time": "20210827 12:15"
        },
        {
          "id": "MDRW2100018565.1.9",
          "form": "사용을 못하고 있어요",
          "original_form": "사용을 못하고 있어요",
          "speaker_id": "1",
          "time": "20210827 12:16"
        },
        {
          "id": "MDRW2100018565.1.10",
          "form": "아.. 머지포인트.. 말이참많았죠 ㅋㅋㅋㅋ",
          "original_form": "아.. 머지포인트.. 말이참많았죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 12:16"
        },
        {
          "id": "MDRW2100018565.1.11",
          "form": "환불못받으셧나요?",
          "original_form": "환불못받으셧나요?",
          "speaker_id": "2",
          "time": "20210827 12:16"
        },
        {
          "id": "MDRW2100018565.1.12",
          "form": "저는 아직 못받았는데",
          "original_form": "저는 아직 못받았는데",
          "speaker_id": "1",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018565.1.13",
          "form": "받은사람들도 있다니 기다려야죠",
          "original_form": "받은사람들도 있다니 기다려야죠",
          "speaker_id": "1",
          "time": "20210827 12:38"
        },
        {
          "id": "MDRW2100018565.1.14",
          "form": "속상하네요",
          "original_form": "속상하네요",
          "speaker_id": "1",
          "time": "20210827 12:39"
        },
        {
          "id": "MDRW2100018565.1.15",
          "form": "아 아직 못받으셨구나.. 많이속상하시겠어요 ㅠ 받을수있을것같나요??",
          "original_form": "아 아직 못받으셨구나.. 많이속상하시겠어요 ㅠ 받을수있을것같나요??",
          "speaker_id": "2",
          "time": "20210827 12:42"
        },
        {
          "id": "MDRW2100018565.1.16",
          "form": "잘 모르겠어요",
          "original_form": "잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100018565.1.17",
          "form": "기다려봐야죠",
          "original_form": "기다려봐야죠",
          "speaker_id": "1",
          "time": "20210827 12:53"
        },
        {
          "id": "MDRW2100018565.1.18",
          "form": "받을수있길 바랍니다..,",
          "original_form": "받을수있길 바랍니다..,",
          "speaker_id": "2",
          "time": "20210827 12:54"
        },
        {
          "id": "MDRW2100018565.1.19",
          "form": "감사합니나",
          "original_form": "감사합니나",
          "speaker_id": "1",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018565.1.20",
          "form": "name2님은 이런일 경험하신적 없나요?",
          "original_form": "&name2&님은 이런일 경험하신적 없나요?",
          "speaker_id": "1",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018565.1.21",
          "form": "음 아뇨 다행이도 아직까진 없어요!",
          "original_form": "음 아뇨 다행이도 아직까진 없어요!",
