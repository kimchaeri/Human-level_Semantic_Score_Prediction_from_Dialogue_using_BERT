      "utterance": [
        {
          "id": "MDRW2100007326.1.1",
          "form": "연애,, 결혼 ",
          "original_form": "연애,, 결혼 ",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007326.1.2",
          "form": "난 안해..",
          "original_form": "난 안해..",
          "speaker_id": "1",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007326.1.3",
          "form": "솔직히 그닥 관심도 없는게 맞긴 한 거 같애,, ㅎ",
          "original_form": "솔직히 그닥 관심도 없는게 맞긴 한 거 같애,, ㅎ",
          "speaker_id": "2",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007326.1.4",
          "form": "연애에 ㅎㅎ",
          "original_form": "연애에 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007326.1.5",
          "form": "난원래 사람안좋아하는데",
          "original_form": "난원래 사람안좋아하는데",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007326.1.6",
          "form": "부대낄생각하면 못하겠어 ㅎ..",
          "original_form": "부대낄생각하면 못하겠어 ㅎ..",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007326.1.7",
          "form": "평생 혼자 살으려고",
          "original_form": "평생 혼자 살으려고",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007326.1.8",
          "form": "맞지맞지 ㅎ 부대낀다고 표현하니까 되게 어색하고 불편하네ㅋㅋㅋ",
          "original_form": "맞지맞지 ㅎ 부대낀다고 표현하니까 되게 어색하고 불편하네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007326.1.9",
          "form": "자매있엉??",
          "original_form": "자매있엉??",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007326.1.10",
          "form": "나는 울언니랑 비혼하고 같이 살기로 했다 ㅋㅋㅋㅋㅋ",
          "original_form": "나는 울언니랑 비혼하고 같이 살기로 했다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007326.1.11",
          "form": "와 좋겠다 자매 ㅜㅜ",
          "original_form": "와 좋겠다 자매 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007326.1.12",
          "form": "자매부러워",
          "original_form": "자매부러워",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007326.1.13",
          "form": "나는 오빠랑 남동생있는데",
          "original_form": "나는 오빠랑 남동생있는데",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007326.1.14",
          "form": "대충 싸울때빼고는 좋은 거 같기는 하다 ㅎ...",
          "original_form": "대충 싸울때빼고는 좋은 거 같기는 하다 ㅎ...",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007326.1.15",
          "form": "오빠나 남동생이면 그 주변 남자들 만난 적 있지 않아?",
          "original_form": "오빠나 남동생이면 그 주변 남자들 만난 적 있지 않아?",
          "speaker_id": "2",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007326.1.16",
          "form": "자매도 엄청싸우겠지만 죽이맞을때도많을거같애",
          "original_form": "자매도 엄청싸우겠지만 죽이맞을때도많을거같애",
          "speaker_id": "1",
          "time": "20210822 01:30"
        },
        {
          "id": "MDRW2100007326.1.17",
          "form": "울가족 다 아싸라서 주변에 사람없는편ㅋㅋㅋ",
          "original_form": "울가족 다 아싸라서 주변에 사람없는편ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100007326.1.18",
          "form": "그리고 요즘 인터넷보면",
          "original_form": "그리고 요즘 인터넷보면",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007326.1.19",
          "form": "남자들 이상해서",
          "original_form": "남자들 이상해서",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007326.1.20",
          "form": "진짜 그런생각 들려고 해도 다시 죽이는듯",
          "original_form": "진짜 그런생각 들려고 해도 다시 죽이는듯",
          "speaker_id": "1",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007326.1.21",
          "form": "아 ㅎ 제발 인터넷속에만 사는 남자이길 바라는건 맞는거 같다 제발..",
          "original_form": "아 ㅎ 제발 인터넷속에만 사는 남자이길 바라는건 맞는거 같다 제발..",
          "speaker_id": "2",
          "time": "20210822 01:32"
        },
        {
          "id": "MDRW2100007326.1.22",
          "form": "나도 숏컷 저번에 크게 터졌을때",
          "original_form": "나도 숏컷 저번에 크게 터졌을때",
          "speaker_id": "2",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007326.1.23",
          "form": "했엉! ㅋㅋㅋㅋㅋ",
          "original_form": "했엉! ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007326.1.24",
          "form": "인터넷 보고 남자들 다 그렇다는 것도 아니긴 한데 만날때 더 조심스럽긴 한 거 같아 ㅋㅋㅋ",
          "original_form": "인터넷 보고 남자들 다 그렇다는 것도 아니긴 한데 만날때 더 조심스럽긴 한 거 같아 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:33"
        },
        {
          "id": "MDRW2100007326.1.25",
          "form": "그리고 난 연애감정이랄게없는듯..",
          "original_form": "그리고 난 연애감정이랄게없는듯..",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007326.1.26",
          "form": "인터넷이 곧 현실이지뭐",
          "original_form": "인터넷이 곧 현실이지뭐",
          "speaker_id": "1",
          "time": "20210822 01:35"
        },
        {
          "id": "MDRW2100007326.1.27",
          "form": "연애감정이라고 하니까 생각나는건데",
          "original_form": "연애감정이라고 하니까 생각나는건데",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007326.1.28",
          "form": "외로움이라는 게 연애같은걸로 외롭지 않다는 걸 느낀 사람이 헤어지고 나서 외롭다고 느끼는거래 ㅋㅋㅋㅋ 되게 그럴싸한듯",
          "original_form": "외로움이라는 게 연애같은걸로 외롭지 않다는 걸 느낀 사람이 헤어지고 나서 외롭다고 느끼는거래 ㅋㅋㅋㅋ 되게 그럴싸한듯",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007326.1.29",
          "form": "안느껴본 외로움은 인식도 안되는 게 맞는 거 같아",
          "original_form": "안느껴본 외로움은 인식도 안되는 게 맞는 거 같아",
          "speaker_id": "2",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007326.1.30",
          "form": "남자들은아닐걸 안느껴봐도 외롭다그럼",
          "original_form": "남자들은아닐걸 안느껴봐도 외롭다그럼",
          "speaker_id": "1",
          "time": "20210822 01:37"
        },
        {
          "id": "MDRW2100007326.1.31",
          "form": "엌 저런 태생적 외로움이넼ㅋㅋ",
          "original_form": "엌 저런 태생적 외로움이넼ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007326.1.32",
          "form": "그러니까 ㅋㅋㅋㅋㅋㅋ타고났어",
          "original_form": "그러니까 ㅋㅋㅋㅋㅋㅋ타고났어",
          "speaker_id": "1",
          "time": "20210822 01:38"
        }
      ]
    }
