      "utterance": [
        {
          "id": "MDRW2100025053.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025053.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025053.1.3",
          "form": "음식에 관심 있나봐요",
          "original_form": "음식에 관심 있나봐요",
          "speaker_id": "2",
          "time": "20210831 22:45"
        },
        {
          "id": "MDRW2100025053.1.4",
          "form": "먹는게 더 좋은가??",
          "original_form": "먹는게 더 좋은가??",
          "speaker_id": "2",
          "time": "20210831 22:46"
        },
        {
          "id": "MDRW2100025053.1.5",
          "form": "둘다좋아여 ㅋㅋㅋㅋ",
          "original_form": "둘다좋아여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:47"
        },
        {
          "id": "MDRW2100025053.1.6",
          "form": "오늘저녁드셧나요?",
          "original_form": "오늘저녁드셧나요?",
          "speaker_id": "1",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025053.1.7",
          "form": "아뇨 저녁을 거의 안먹어요",
          "original_form": "아뇨 저녁을 거의 안먹어요",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025053.1.8",
          "form": "늦은 점심을 먹어요",
          "original_form": "늦은 점심을 먹어요",
          "speaker_id": "2",
          "time": "20210831 22:48"
        },
        {
          "id": "MDRW2100025053.1.9",
          "form": "전 김치를 담궈 보고 싶은데",
          "original_form": "전 김치를 담궈 보고 싶은데",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025053.1.10",
          "form": "자신이 없어요",
          "original_form": "자신이 없어요",
          "speaker_id": "2",
          "time": "20210831 22:49"
        },
        {
          "id": "MDRW2100025053.1.11",
          "form": "아정말요? 오늘뭐드셨어요??",
          "original_form": "아정말요? 오늘뭐드셨어요??",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025053.1.12",
          "form": "아하 김치 유투브나 이런데에 쉬운레시피 검색해보셔요~",
          "original_form": "아하 김치 유투브나 이런데에 쉬운레시피 검색해보셔요~",
          "speaker_id": "1",
          "time": "20210831 22:50"
        },
        {
          "id": "MDRW2100025053.1.13",
          "form": "오늘 라면 먹었어요 김말이튀김에",
          "original_form": "오늘 라면 먹었어요 김말이튀김에",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025053.1.14",
          "form": "다른 음식은 하는데  김치는 어려워요",
          "original_form": "다른 음식은 하는데  김치는 어려워요",
          "speaker_id": "2",
          "time": "20210831 22:51"
        },
        {
          "id": "MDRW2100025053.1.15",
          "form": "오우 라면어디꺼드셨어요??",
          "original_form": "오우 라면어디꺼드셨어요??",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100025053.1.16",
          "form": "김치 안해봐서 감이안오네요 ㅠ",
          "original_form": "김치 안해봐서 감이안오네요 ㅠ",
          "speaker_id": "1",
          "time": "20210831 22:54"
        },
        {
          "id": "MDRW2100025053.1.17",
          "form": "신라면요  ㅎㅎ",
          "original_form": "신라면요  ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100025053.1.18",
          "form": "김치가 은근 어려워요",
          "original_form": "김치가 은근 어려워요",
          "speaker_id": "2",
          "time": "20210831 22:55"
        },
        {
          "id": "MDRW2100025053.1.19",
          "form": "아하! 근본신라면! 근데전 진라면더좋아해여 ㅋㅋㅋ",
          "original_form": "아하! 근본신라면! 근데전 진라면더좋아해여 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100025053.1.20",
          "form": "가게에서 사먹었어요 ㅎ",
          "original_form": "가게에서 사먹었어요 ㅎ",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100025053.1.21",
          "form": "집에선 진라면 먹어요",
          "original_form": "집에선 진라면 먹어요",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100025053.1.22",
          "form": "좋아하는 음식이...",
          "original_form": "좋아하는 음식이...",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100025053.1.23",
          "form": "아하정말요?? ㅋㅋㅋㅋ 전사실 라면중에선 짜파게티 젤 좋아해용",
          "original_form": "아하정말요?? ㅋㅋㅋㅋ 전사실 라면중에선 짜파게티 젤 좋아해용",
          "speaker_id": "1",
          "time": "20210831 23:01"
        },
        {
          "id": "MDRW2100025053.1.24",
          "form": "라면에 계란을 넣어야 더 맛있는거 같아요",
          "original_form": "라면에 계란을 넣어야 더 맛있는거 같아요",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100025053.1.25",
          "form": "맞아여 저도 계란넣은라면 더 좋아해요",
          "original_form": "맞아여 저도 계란넣은라면 더 좋아해요",
          "speaker_id": "1",
          "time": "20210831 23:04"
        }
      ]
    }
  ]
