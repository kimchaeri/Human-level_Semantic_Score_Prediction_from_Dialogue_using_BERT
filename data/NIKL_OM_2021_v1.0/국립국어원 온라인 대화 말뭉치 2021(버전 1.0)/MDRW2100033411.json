      "utterance": [
        {
          "id": "MDRW2100033411.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210907 07:12"
        },
        {
          "id": "MDRW2100033411.1.2",
          "form": "오늘 아침 먹었나요?",
          "original_form": "오늘 아침 먹었나요?",
          "speaker_id": "1",
          "time": "20210907 07:12"
        },
        {
          "id": "MDRW2100033411.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100033411.1.4",
          "form": "아침은 챙겨먹는 주의라",
          "original_form": "아침은 챙겨먹는 주의라",
          "speaker_id": "2",
          "time": "20210907 08:51"
        },
        {
          "id": "MDRW2100033411.1.5",
          "form": "저는 오늘 드디어 편의점 샌드위치먹었어요",
          "original_form": "저는 오늘 드디어 편의점 샌드위치먹었어요",
          "speaker_id": "1",
          "time": "20210907 09:29"
        },
        {
          "id": "MDRW2100033411.1.6",
          "form": "드디어요?",
          "original_form": "드디어요?",
          "speaker_id": "2",
          "time": "20210907 10:57"
        },
        {
          "id": "MDRW2100033411.1.7",
          "form": "인기가 많았던 메뉴가 있었나봐요?",
          "original_form": "인기가 많았던 메뉴가 있었나봐요?",
          "speaker_id": "2",
          "time": "20210907 10:57"
        },
        {
          "id": "MDRW2100033411.1.8",
          "form": "편의점에서요???",
          "original_form": "편의점에서요???",
          "speaker_id": "1",
          "time": "20210907 17:10"
        },
        {
          "id": "MDRW2100033411.1.9",
          "form": "많이 안사먹어봐서 잘모르겠어요ㅜㅜ",
          "original_form": "많이 안사먹어봐서 잘모르겠어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 17:11"
        },
        {
          "id": "MDRW2100033411.1.10",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210907 17:11"
        },
        {
          "id": "MDRW2100033411.1.11",
          "form": "그 드디어가",
          "original_form": "그 드디어가",
          "speaker_id": "2",
          "time": "20210907 17:11"
        },
        {
          "id": "MDRW2100033411.1.12",
          "form": "그 드디어였군요",
          "original_form": "그 드디어였군요",
          "speaker_id": "2",
          "time": "20210907 17:11"
        },
        {
          "id": "MDRW2100033411.1.13",
          "form": "저는 피치 못하게 못 먹고 계셨던건줄 알았네요",
          "original_form": "저는 피치 못하게 못 먹고 계셨던건줄 알았네요",
          "speaker_id": "2",
          "time": "20210907 17:11"
        },
        {
          "id": "MDRW2100033411.1.14",
          "form": "어제까지만해도 아침을 못먹고다녔는데",
          "original_form": "어제까지만해도 아침을 못먹고다녔는데",
          "speaker_id": "1",
          "time": "20210907 17:13"
        },
        {
          "id": "MDRW2100033411.1.15",
          "form": "오늘 편의점에서 샌드위치사먹었어요",
          "original_form": "오늘 편의점에서 샌드위치사먹었어요",
          "speaker_id": "1",
          "time": "20210907 17:13"
        },
        {
          "id": "MDRW2100033411.1.16",
          "form": "낼도 사먹어야할것같아요ㅜㅜ",
          "original_form": "낼도 사먹어야할것같아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 17:13"
        },
        {
          "id": "MDRW2100033411.1.17",
          "form": "ㅠ...",
          "original_form": "ㅠ...",
          "speaker_id": "2",
          "time": "20210907 17:21"
        },
        {
          "id": "MDRW2100033411.1.18",
          "form": "다만 저는",
          "original_form": "다만 저는",
          "speaker_id": "2",
          "time": "20210907 17:21"
        },
        {
          "id": "MDRW2100033411.1.19",
          "form": "점심을 정말 적게 먹는 편인지라",
          "original_form": "점심을 정말 적게 먹는 편인지라",
          "speaker_id": "2",
          "time": "20210907 17:21"
        },
        {
          "id": "MDRW2100033411.1.20",
          "form": "아침과 저녁에 충실한 편이긴 해요",
          "original_form": "아침과 저녁에 충실한 편이긴 해요",
          "speaker_id": "2",
          "time": "20210907 17:22"
        },
        {
          "id": "MDRW2100033411.1.21",
          "form": "어떻게ㅜㅜ저는 점심때 돈내고 먹는거라 주어진메뉴보고머키때문메",
          "original_form": "어떻게ㅜㅜ저는 점심때 돈내고 먹는거라 주어진메뉴보고머키때문메",
          "speaker_id": "1",
          "time": "20210907 17:23"
        },
        {
          "id": "MDRW2100033411.1.22",
          "form": "반찬안남기고",
          "original_form": "반찬안남기고",
          "speaker_id": "1",
          "time": "20210907 17:23"
        },
        {
          "id": "MDRW2100033411.1.23",
          "form": "다 먹어요",
          "original_form": "다 먹어요",
          "speaker_id": "1",
          "time": "20210907 17:23"
        },
        {
          "id": "MDRW2100033411.1.24",
          "form": "유일하게 제대로 먹는 밥",
          "original_form": "유일하게 제대로 먹는 밥",
          "speaker_id": "1",
          "time": "20210907 17:23"
        },
        {
          "id": "MDRW2100033411.1.25",
          "form": "아아...",
          "original_form": "아아...",
          "speaker_id": "2",
          "time": "20210907 17:25"
        },
        {
          "id": "MDRW2100033411.1.26",
          "form": "저녁은요...?",
          "original_form": "저녁은요...?",
          "speaker_id": "2",
          "time": "20210907 17:25"
        },
        {
          "id": "MDRW2100033411.1.27",
          "form": "저녁은 패스트푸드먹어요!!",
          "original_form": "저녁은 패스트푸드먹어요!!",
          "speaker_id": "1",
          "time": "20210907 17:25"
        },
        {
          "id": "MDRW2100033411.1.28",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:26"
        },
        {
          "id": "MDRW2100033411.1.29",
          "form": "어제 홈플치킨먹었머요",
          "original_form": "어제 홈플치킨먹었머요",
          "speaker_id": "1",
          "time": "20210907 17:26"
        },
        {
          "id": "MDRW2100033411.1.30",
          "form": "않이",
          "original_form": "않이",
          "speaker_id": "2",
          "time": "20210907 17:31"
        },
        {
          "id": "MDRW2100033411.1.31",
          "form": "충분히 제대로된",
          "original_form": "충분히 제대로된",
          "speaker_id": "2",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100033411.1.32",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100033411.1.33",
          "form": "밥류가 아니니까...",
          "original_form": "밥류가 아니니까...",
          "speaker_id": "2",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100033411.1.34",
          "form": "네ㅜㅜ점심때만 제대로 갖춰진 밥을 먹을수가있어요",
          "original_form": "네ㅜㅜ점심때만 제대로 갖춰진 밥을 먹을수가있어요",
          "speaker_id": "1",
          "time": "20210907 17:32"
        },
        {
          "id": "MDRW2100033411.1.35",
          "form": "낼은 새우튀김나와요",
          "original_form": "낼은 새우튀김나와요",
          "speaker_id": "1",
          "time": "20210907 17:32"
        }
      ]
