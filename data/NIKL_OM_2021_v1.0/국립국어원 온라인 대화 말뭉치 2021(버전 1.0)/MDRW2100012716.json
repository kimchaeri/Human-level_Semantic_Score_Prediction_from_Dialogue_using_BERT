      "utterance": [
        {
          "id": "MDRW2100012716.1.1",
          "form": "name2은 무슨운동보세요",
          "original_form": "&name2&은 무슨운동보세요",
          "speaker_id": "1",
          "time": "20210825 00:12"
        },
        {
          "id": "MDRW2100012716.1.2",
          "form": "저 예전에 류현진이 미국갔을때 해외야구 챙겨봤었어요!",
          "original_form": "저 예전에 류현진이 미국갔을때 해외야구 챙겨봤었어요!",
          "speaker_id": "2",
          "time": "20210825 21:30"
        },
        {
          "id": "MDRW2100012716.1.3",
          "form": "와 찐팬이시다",
          "original_form": "와 찐팬이시다",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100012716.1.4",
          "form": "류현진 짤방재밌어요",
          "original_form": "류현진 짤방재밌어요",
          "speaker_id": "1",
          "time": "20210825 21:35"
        },
        {
          "id": "MDRW2100012716.1.5",
          "form": "저는 한화시절은 잘 모르고 딱 다저스 이후로만 알아요 ㅎㅎ",
          "original_form": "저는 한화시절은 잘 모르고 딱 다저스 이후로만 알아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 21:37"
        },
        {
          "id": "MDRW2100012716.1.6",
          "form": "보시는 스포츠 있으세요?",
          "original_form": "보시는 스포츠 있으세요?",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100012716.1.7",
          "form": "저는 롯데응원했는데...훗..",
          "original_form": "저는 롯데응원했는데...훗..",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100012716.1.8",
          "form": "아래서 일등입니다",
          "original_form": "아래서 일등입니다",
          "speaker_id": "1",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100012716.1.9",
          "form": "앗.. 롯데가 지금 그렇군요...",
          "original_form": "앗.. 롯데가 지금 그렇군요...",
          "speaker_id": "2",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100012716.1.10",
          "form": "직관도 가보셨어요?",
          "original_form": "직관도 가보셨어요?",
          "speaker_id": "2",
          "time": "20210825 21:40"
        },
        {
          "id": "MDRW2100012716.1.11",
          "form": "친구랑 한번 잠실가봤어요",
          "original_form": "친구랑 한번 잠실가봤어요",
          "speaker_id": "1",
          "time": "20210825 21:41"
        },
        {
          "id": "MDRW2100012716.1.12",
          "form": "오 저도 잠실 두번인가 가봤네요",
          "original_form": "오 저도 잠실 두번인가 가봤네요",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100012716.1.13",
          "form": "그땐..이겼는데...",
          "original_form": "그땐..이겼는데...",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100012716.1.14",
          "form": "말줄임표가 아련하네요...",
          "original_form": "말줄임표가 아련하네요...",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100012716.1.15",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100012716.1.16",
          "form": "봉지 머리에 쓰고 재밌었어요",
          "original_form": "봉지 머리에 쓰고 재밌었어요",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100012716.1.17",
          "form": "직관재밌으셨나요",
          "original_form": "직관재밌으셨나요",
          "speaker_id": "1",
          "time": "20210825 21:42"
        },
        {
          "id": "MDRW2100012716.1.18",
          "form": "네 치킨 먹으면서 응원하니까 재밌더라고요 ㅋㅋㅋ",
          "original_form": "네 치킨 먹으면서 응원하니까 재밌더라고요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100012716.1.19",
          "form": "앗 저는치킨 안먹었었는데 ㅜ",
          "original_form": "앗 저는치킨 안먹었었는데 ㅜ",
          "speaker_id": "1",
          "time": "20210825 21:43"
        },
        {
          "id": "MDRW2100012716.1.20",
          "form": "아쉽군요",
          "original_form": "아쉽군요",
          "speaker_id": "1",
          "time": "20210825 21:44"
        },
        {
          "id": "MDRW2100012716.1.21",
          "form": "네 맛있긴 하더라고요 ㅎㅎ",
          "original_form": "네 맛있긴 하더라고요 ㅎㅎ",
          "speaker_id": "2",
