      "utterance": [
        {
          "id": "MDRW2100007290.1.1",
          "form": "자주나오는 학식은 김치볶음밥...?",
          "original_form": "자주나오는 학식은 김치볶음밥...?",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100007290.1.2",
          "form": "저흰 라면..",
          "original_form": "저흰 라면..",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100007290.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ 라면은 아예 상시메뉴더라구요!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 라면은 아예 상시메뉴더라구요!",
          "speaker_id": "2",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100007290.1.4",
          "form": "그쵸그쵸ㅋㅋㅋㅋㅋ",
          "original_form": "그쵸그쵸ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100007290.1.5",
          "form": "졸업생이시면 자취하시나요?!",
          "original_form": "졸업생이시면 자취하시나요?!",
          "speaker_id": "1",
          "time": "20210822 15:31"
        },
        {
          "id": "MDRW2100007290.1.6",
          "form": "아 비대면 시작하면서 집에 와서 살게 되었어요 ㅠㅠ ㅋㅋㅋ",
          "original_form": "아 비대면 시작하면서 집에 와서 살게 되었어요 ㅠㅠ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:32"
        },
        {
          "id": "MDRW2100007290.1.7",
          "form": "자취하는데 돈이 너무 많이 빠져나가서요 ㅋㅋㅋ ㅠㅠ",
          "original_form": "자취하는데 돈이 너무 많이 빠져나가서요 ㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100007290.1.8",
          "form": "맞아요ㅋㅋㅋㅋ 자취 돈 많이 나가죠ㅠ",
          "original_form": "맞아요ㅋㅋㅋㅋ 자취 돈 많이 나가죠ㅠ",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100007290.1.9",
          "form": "숨만 쉬어도 나가는 돈...",
          "original_form": "숨만 쉬어도 나가는 돈...",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100007290.1.10",
          "form": "진짜 자취할때 알게되었어요.... 자취하면서 돈모으는 사람 대단하다고....",
          "original_form": "진짜 자취할때 알게되었어요.... 자취하면서 돈모으는 사람 대단하다고....",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100007290.1.11",
          "form": "나는 그냥 생활만 했는데 왜 통장은 텅장이 되는지....",
          "original_form": "나는 그냥 생활만 했는데 왜 통장은 텅장이 되는지....",
          "speaker_id": "1",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100007290.1.12",
          "form": "진짜 자취하면서 돈 천만원 모으는 사람들",
          "original_form": "진짜 자취하면서 돈 천만원 모으는 사람들",
          "speaker_id": "1",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100007290.1.13",
          "form": "너무 대단한거 같아요",
          "original_form": "너무 대단한거 같아요",
          "speaker_id": "1",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100007290.1.14",
          "form": "진짜 내 잔고... 작고 소중해....",
          "original_form": "진짜 내 잔고... 작고 소중해....",
          "speaker_id": "2",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100007290.1.15",
          "form": "자취하면서 돈모으는거 그거 뭔데.... 어떻게하는건데...",
          "original_form": "자취하면서 돈모으는거 그거 뭔데.... 어떻게하는건데...",
          "speaker_id": "2",
          "time": "20210822 15:34"
        },
        {
          "id": "MDRW2100007290.1.16",
          "form": "진짜 어떻게 하는건데ㅠㅠㅠ",
          "original_form": "진짜 어떻게 하는건데ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100007290.1.17",
          "form": "자취의 장점은 나혼자라는것과",
          "original_form": "자취의 장점은 나혼자라는것과",
          "speaker_id": "1",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100007290.1.18",
          "form": "단점은 나 혼자라는 것..",
          "original_form": "단점은 나 혼자라는 것..",
          "speaker_id": "1",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100007290.1.19",
          "form": "내가 아니면 아무도 하지 않는 가사ㅠ",
          "original_form": "내가 아니면 아무도 하지 않는 가사ㅠ",
          "speaker_id": "1",
          "time": "20210822 15:35"
        },
        {
          "id": "MDRW2100007290.1.20",
          "form": "진짜 빨래를 그냥 규칙적으로 하는게 엄청 대단하다는걸 알게되었어요 ㅠㅠ",
          "original_form": "진짜 빨래를 그냥 규칙적으로 하는게 엄청 대단하다는걸 알게되었어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100007290.1.21",
          "form": "입을 옷이 없을때즈음 간신히 하는 빨래...",
          "original_form": "입을 옷이 없을때즈음 간신히 하는 빨래...",
          "speaker_id": "2",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100007290.1.22",
          "form": "자취생 꿀템에 진짜 그거 있어요",
          "original_form": "자취생 꿀템에 진짜 그거 있어요",
          "speaker_id": "1",
          "time": "20210822 15:36"
        },
        {
          "id": "MDRW2100007290.1.23",
          "form": "건조기 진짜 자취생 다 필요해",
          "original_form": "건조기 진짜 자취생 다 필요해",
          "speaker_id": "1",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100007290.1.24",
          "form": "건조대는 쓰레기다",
          "original_form": "건조대는 쓰레기다",
          "speaker_id": "1",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100007290.1.25",
          "form": "아 맞아요 건조기 엄청 꿀템이랬는데",
          "original_form": "아 맞아요 건조기 엄청 꿀템이랬는데",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100007290.1.26",
          "form": "자취방에서 1도 못써봤어요",
          "original_form": "자취방에서 1도 못써봤어요",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100007290.1.27",
          "form": "계절별로 대비해서 사고 갖출건 왜이리 많은지...!",
          "original_form": "계절별로 대비해서 사고 갖출건 왜이리 많은지...!",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100007290.1.28",
          "form": "제습기 건조기 열풍기..",
          "original_form": "제습기 건조기 열풍기..",
          "speaker_id": "1",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100007290.1.29",
          "form": "근데 만약 여기서 꼭 하나만 한다면",
          "original_form": "근데 만약 여기서 꼭 하나만 한다면",
          "speaker_id": "1",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100007290.1.30",
          "form": "저는 건조기요",
          "original_form": "저는 건조기요",
          "speaker_id": "1",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100007290.1.31",
          "form": "이불 빨래할때 건조기 무조건 있어야해요ㅠ",
          "original_form": "이불 빨래할때 건조기 무조건 있어야해요ㅠ",
          "speaker_id": "1",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100007290.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 저는 제습기 진짜....",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 저는 제습기 진짜....",
          "speaker_id": "2",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100007290.1.33",
          "form": "여름에 어디 나갈때 그거 필수였어요 ㅠㅠㅠㅠ",
          "original_form": "여름에 어디 나갈때 그거 필수였어요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100007290.1.34",
          "form": "건조기는.... 코인세탁소에서만 ㅋㅋㅋ큐ㅠㅠㅠ",
          "original_form": "건조기는.... 코인세탁소에서만 ㅋㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 15:38"
        },
        {
          "id": "MDRW2100007290.1.35",
          "form": "집에서 가까운 코인세탁소 있는게 은근 중요해요",
          "original_form": "집에서 가까운 코인세탁소 있는게 은근 중요해요",
          "speaker_id": "1",
          "time": "20210822 15:39"
        },
        {
          "id": "MDRW2100007290.1.36",
          "form": "집근처에 있으면 좋은거 코인세탁소 편의점...",
          "original_form": "집근처에 있으면 좋은거 코인세탁소 편의점...",
          "speaker_id": "2",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100007290.1.37",
          "form": "그리고 코인노래방....ㅎ",
          "original_form": "그리고 코인노래방....ㅎ",
          "speaker_id": "2",
          "time": "20210822 15:40"
        },
        {
          "id": "MDRW2100007290.1.38",
          "form": "역시 코인이 들어가면 가사 활동이 편해집니다",
          "original_form": "역시 코인이 들어가면 가사 활동이 편해집니다",
          "speaker_id": "1",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100007290.1.39",
          "form": "ㅋㅋㅋㅋㅋ 아마 저희 집 근쳐 코인노래방 마이크 하나정도는 제 돈일거에요",
          "original_form": "ㅋㅋㅋㅋㅋ 아마 저희 집 근쳐 코인노래방 마이크 하나정도는 제 돈일거에요",
          "speaker_id": "2",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100007290.1.40",
          "form": "부럽다 저는 집 근처에는 없어서ㅠㅠ",
          "original_form": "부럽다 저는 집 근처에는 없어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100007290.1.41",
          "form": "조금 나가야하는데..",
          "original_form": "조금 나가야하는데..",
          "speaker_id": "1",
          "time": "20210822 15:42"
        },
        {
          "id": "MDRW2100007290.1.42",
          "form": "ㅋㅋㅋㅋㅋ 대신 저희는 경사가....ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ 대신 저희는 경사가....ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100007290.1.43",
          "form": "그래서 한번가면 3천원 기본이에욬ㅋㅋㅋ",
          "original_form": "그래서 한번가면 3천원 기본이에욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100007290.1.44",
          "form": "저는 만원부터 시작합니다ㅋㅋㅋㅋㅋ",
          "original_form": "저는 만원부터 시작합니다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100007290.1.45",
          "form": "진짜 자취생에게 최고의 문화공간이에요 ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 자취생에게 최고의 문화공간이에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:47"
        },
        {
          "id": "MDRW2100007290.1.46",
          "form": "자취생들 집 주변에 꼭 있어야하는 거..",
          "original_form": "자취생들 집 주변에 꼭 있어야하는 거..",
          "speaker_id": "1",
          "time": "20210822 15:48"
        },
        {
          "id": "MDRW2100007290.1.47",
          "form": "편의점이랑 코인세탁방",
          "original_form": "편의점이랑 코인세탁방",
