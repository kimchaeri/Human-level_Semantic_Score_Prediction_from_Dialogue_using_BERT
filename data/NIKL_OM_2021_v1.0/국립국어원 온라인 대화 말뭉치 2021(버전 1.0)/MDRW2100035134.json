      "utterance": [
        {
          "id": "MDRW2100035134.1.1",
          "form": "최근에 뭐 사셨어요?",
          "original_form": "최근에 뭐 사셨어요?",
          "speaker_id": "2",
          "time": "20210829 12:04"
        },
        {
          "id": "MDRW2100035134.1.2",
          "form": "전최근에 당근으로 에그팬 샀어요 ㅋㅋㅋ",
          "original_form": "전최근에 당근으로 에그팬 샀어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 10:45"
        },
        {
          "id": "MDRW2100035134.1.3",
          "form": "에그팬이면 계란후라이 할 때 쓰는 틀이죠?",
          "original_form": "에그팬이면 계란후라이 할 때 쓰는 틀이죠?",
          "speaker_id": "2",
          "time": "20210830 12:31"
        },
        {
          "id": "MDRW2100035134.1.4",
          "form": "맞아요~ 작은팬! 편하고좋더라구요",
          "original_form": "맞아요~ 작은팬! 편하고좋더라구요",
          "speaker_id": "1",
          "time": "20210830 13:28"
        },
        {
          "id": "MDRW2100035134.1.5",
          "form": "저도 하나 사고 싶네요ㅠ",
          "original_form": "저도 하나 사고 싶네요ㅠ",
          "speaker_id": "2",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100035134.1.6",
          "form": "계란후라이만 하면 꼭 노른자가 터져서",
          "original_form": "계란후라이만 하면 꼭 노른자가 터져서",
          "speaker_id": "2",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100035134.1.7",
          "form": "아정말요?? ㅋㅋㅋ 에그팬 작아서 세척도좋아요~",
          "original_form": "아정말요?? ㅋㅋㅋ 에그팬 작아서 세척도좋아요~",
          "speaker_id": "1",
          "time": "20210831 14:00"
        },
        {
          "id": "MDRW2100035134.1.8",
          "form": "장점이 많군요",
          "original_form": "장점이 많군요",
          "speaker_id": "2",
          "time": "20210831 14:11"
        },
        {
          "id": "MDRW2100035134.1.9",
          "form": "맞아여 ㅋㅋ소소한장점들",
          "original_form": "맞아여 ㅋㅋ소소한장점들",
          "speaker_id": "1",
          "time": "20210831 14:17"
        },
        {
          "id": "MDRW2100035134.1.10",
          "form": "요즘 나오는 도구들이 다 그런거 같아요 소소한 귀찮음 해결용으로다가",
          "original_form": "요즘 나오는 도구들이 다 그런거 같아요 소소한 귀찮음 해결용으로다가",
          "speaker_id": "2",
          "time": "20210831 14:18"
        },
        {
          "id": "MDRW2100035134.1.11",
          "form": "맞아요 ㅋㅋ 이런거 궁금해서 꼭 사보는사람 저에요..",
          "original_form": "맞아요 ㅋㅋ 이런거 궁금해서 꼭 사보는사람 저에요..",
          "speaker_id": "1",
          "time": "20210831 14:23"
        },
        {
          "id": "MDRW2100035134.1.12",
          "form": "저는 사지는 못하고 구경만...",
          "original_form": "저는 사지는 못하고 구경만...",
          "speaker_id": "2",
          "time": "20210831 14:32"
        },
        {
          "id": "MDRW2100035134.1.13",
          "form": "앜ㅋㅋㅋㅋ 현명하시네요..",
          "original_form": "앜ㅋㅋㅋㅋ 현명하시네요..",
