      "utterance": [
        {
          "id": "MDRW2100000144.1.1",
          "form": "언니 오늘 몇시에 끝나?",
          "original_form": "언니 오늘 몇시에 끝나?",
          "speaker_id": "1",
          "time": "20210518 01:16"
