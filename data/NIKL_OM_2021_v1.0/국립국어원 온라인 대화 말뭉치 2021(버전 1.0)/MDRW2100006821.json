      "utterance": [
        {
          "id": "MDRW2100006821.1.1",
          "form": "아이스크림좋아하시나요",
          "original_form": "아이스크림좋아하시나요",
          "speaker_id": "1",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100006821.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100006821.1.3",
          "form": "뉴욕치즈케이크 최애에요",
          "original_form": "뉴욕치즈케이크 최애에요",
          "speaker_id": "2",
          "time": "20210908 10:13"
        },
        {
          "id": "MDRW2100006821.1.4",
          "form": "오 치즈 맛 맛있죠",
          "original_form": "오 치즈 맛 맛있죠",
          "speaker_id": "1",
          "time": "20210908 10:14"
        },
        {
          "id": "MDRW2100006821.1.5",
          "form": "소소님은 무슨 맛 좋아하시나요??",
          "original_form": "소소님은 무슨 맛 좋아하시나요??",
          "speaker_id": "2",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100006821.1.6",
          "form": "저는 녹차맛좋아해요",
          "original_form": "저는 녹차맛좋아해요",
          "speaker_id": "1",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100006821.1.7",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100006821.1.8",
          "form": "ㅋㅋㅋ안좋아하시나요",
          "original_form": "ㅋㅋㅋ안좋아하시나요",
          "speaker_id": "1",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100006821.1.9",
          "form": "저 녹차맛을 먹어본 적이 없는 거 같아요",
          "original_form": "저 녹차맛을 먹어본 적이 없는 거 같아요",
          "speaker_id": "2",
          "time": "20210908 10:19"
        },
        {
          "id": "MDRW2100006821.1.10",
          "form": "아 안 좋아하진 않는데",
          "original_form": "아 안 좋아하진 않는데",
          "speaker_id": "2",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100006821.1.11",
          "form": "굳이 녹차를 선택하지 않는",
          "original_form": "굳이 녹차를 선택하지 않는",
          "speaker_id": "2",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100006821.1.12",
          "form": "아 정말요? ㅎㅎㅎ상큼한건어때요",
          "original_form": "아 정말요? ㅎㅎㅎ상큼한건어때요",
          "speaker_id": "1",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100006821.1.13",
          "form": "상큼한건 좋아해요!",
          "original_form": "상큼한건 좋아해요!",
          "speaker_id": "2",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100006821.1.14",
          "form": "저는 레인보우샤베트는안좋아해서 ㅎㅎㅎ",
          "original_form": "저는 레인보우샤베트는안좋아해서 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 10:20"
        },
        {
          "id": "MDRW2100006821.1.15",
          "form": "아 저번달인가",
          "original_form": "아 저번달인가",
          "speaker_id": "2",
          "time": "20210908 10:23"
        },
        {
          "id": "MDRW2100006821.1.16",
          "form": "아이스꼬북칩도 맛있더라고요",
          "original_form": "아이스꼬북칩도 맛있더라고요",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100006821.1.17",
          "form": "신메뉴나왔나요?",
          "original_form": "신메뉴나왔나요?",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100006821.1.18",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100006821.1.19",
          "form": "꼬북칩을 좋아해서 ㅋㅋㅋ",
          "original_form": "꼬북칩을 좋아해서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100006821.1.20",
          "form": "맛있었을거같아요",
          "original_form": "맛있었을거같아요",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100006821.1.21",
          "form": "아직 있나요?",
          "original_form": "아직 있나요?",
          "speaker_id": "1",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100006821.1.22",
          "form": "아마 없을거에요...",
          "original_form": "아마 없을거에요...",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100006821.1.23",
          "form": "8월 이달의 맛이었는데",
          "original_form": "8월 이달의 맛이었는데",
          "speaker_id": "2",
          "time": "20210908 10:24"
        },
        {
          "id": "MDRW2100006821.1.24",
          "form": "저도꼬북칩좋아해서",
          "original_form": "저도꼬북칩좋아해서",
          "speaker_id": "1",
          "time": "20210908 10:24"
        }
