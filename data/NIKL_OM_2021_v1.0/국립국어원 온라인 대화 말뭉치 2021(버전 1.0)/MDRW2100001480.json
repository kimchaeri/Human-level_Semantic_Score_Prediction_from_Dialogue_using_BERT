        },
        {
          "id": "MDRW2100001480.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211015 13:23"
        },
        {
          "id": "MDRW2100001480.1.3",
          "form": "혹시 여행 좋아하시나요?",
          "original_form": "혹시 여행 좋아하시나요?",
          "speaker_id": "2",
          "time": "20211015 13:24"
        },
        {
          "id": "MDRW2100001480.1.4",
          "form": "여행 좋죠~",
          "original_form": "여행 좋죠~",
          "speaker_id": "1",
          "time": "20211015 13:24"
        },
        {
          "id": "MDRW2100001480.1.5",
          "form": "지난주에 부산 다녀왔어요",
          "original_form": "지난주에 부산 다녀왔어요",
          "speaker_id": "1",
          "time": "20211015 13:24"
        },
        {
          "id": "MDRW2100001480.1.6",
          "form": "우와 부산여행! 저 한 번도 못가봤어요",
          "original_form": "우와 부산여행! 저 한 번도 못가봤어요",
          "speaker_id": "2",
          "time": "20211015 13:28"
        },
        {
          "id": "MDRW2100001480.1.7",
          "form": "부산 돼지국밥이랑 밀면 먹어보는게",
          "original_form": "부산 돼지국밥이랑 밀면 먹어보는게",
          "speaker_id": "2",
          "time": "20211015 13:28"
        },
        {
          "id": "MDRW2100001480.1.8",
          "form": "나름 버킷리스트에요 ㅎㅎㅎ",
          "original_form": "나름 버킷리스트에요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 13:28"
        },
        {
          "id": "MDRW2100001480.1.9",
          "form": "아 정말요 ㅎㅎ",
          "original_form": "아 정말요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20211015 13:37"
        },
        {
          "id": "MDRW2100001480.1.10",
          "form": "둘다 너무 맛있죠!",
          "original_form": "둘다 너무 맛있죠!",
          "speaker_id": "1",
          "time": "20211015 13:37"
        },
        {
          "id": "MDRW2100001480.1.11",
          "form": "부산 가셨을 때 제일 좋다고 생각하신 곳이 어디에요?",
          "original_form": "부산 가셨을 때 제일 좋다고 생각하신 곳이 어디에요?",
          "speaker_id": "2",
          "time": "20211015 13:49"
        },
        {
          "id": "MDRW2100001480.1.12",
          "form": "광안리였죠!",
          "original_form": "광안리였죠!",
          "speaker_id": "1",
          "time": "20211015 13:50"
        },
        {
          "id": "MDRW2100001480.1.13",
          "form": "야경이 아주 기가막혔어요ㅠ",
          "original_form": "야경이 아주 기가막혔어요ㅠ",
          "speaker_id": "1",
          "time": "20211015 13:50"
        },
        {
          "id": "MDRW2100001480.1.14",
          "form": "말로만 듣던 광안리..",
          "original_form": "말로만 듣던 광안리..",
          "speaker_id": "2",
          "time": "20211015 13:50"
        },
        {
          "id": "MDRW2100001480.1.15",
          "form": "정말 좋으셨겠어요 ㅠㅠ 부럽부럽",
          "original_form": "정말 좋으셨겠어요 ㅠㅠ 부럽부럽",
          "speaker_id": "2",
          "time": "20211015 13:51"
        },
        {
          "id": "MDRW2100001480.1.16",
          "form": "그 해운대는 안가셨나요?",
          "original_form": "그 해운대는 안가셨나요?",
          "speaker_id": "2",
          "time": "20211015 13:51"
        },
        {
          "id": "MDRW2100001480.1.17",
          "form": "요즘 비행기랑 숙박이 많이 저렴해서 마음만 먹으면 크게 부담없이 가겠드라구요",
          "original_form": "요즘 비행기랑 숙박이 많이 저렴해서 마음만 먹으면 크게 부담없이 가겠드라구요",
          "speaker_id": "1",
          "time": "20211015 13:51"
        },
        {
          "id": "MDRW2100001480.1.18",
          "form": "해운대는 잠시 바람만 쐬구 왔어요ㅎㅎ",
          "original_form": "해운대는 잠시 바람만 쐬구 왔어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20211015 13:52"
        },
        {
          "id": "MDRW2100001480.1.19",
          "form": "오 코로나 때문에 오히려 저렴해진건가요!",
          "original_form": "오 코로나 때문에 오히려 저렴해진건가요!",
          "speaker_id": "2",
          "time": "20211015 13:52"
        },
        {
          "id": "MDRW2100001480.1.20",
          "form": "여행 계획을 좀 세워봐야겠네요 ㅎㅎ",
          "original_form": "여행 계획을 좀 세워봐야겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211015 13:52"
        },
        {
          "id": "MDRW2100001480.1.21",
          "form": "해운대랑 광안리랑 느낌이 다른가요?",
          "original_form": "해운대랑 광안리랑 느낌이 다른가요?",
          "speaker_id": "2",
          "time": "20211015 13:52"
        },
        {
          "id": "MDRW2100001480.1.22",
          "form": "코로나 때문이기도 하겠네요ㅎㅎ",
          "original_form": "코로나 때문이기도 하겠네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20211015 13:59"
        },
        {
          "id": "MDRW2100001480.1.23",
          "form": "느낌,, 뭐 비슷할수도 있는데 경치가 광안리가 더 좋드라구요",
          "original_form": "느낌,, 뭐 비슷할수도 있는데 경치가 광안리가 더 좋드라구요",
          "speaker_id": "1",
          "time": "20211015 13:59"
        },
        {
          "id": "MDRW2100001480.1.24",
          "form": "해운대 보단 광안리..!",
          "original_form": "해운대 보단 광안리..!",
          "speaker_id": "2",
          "time": "20211015 14:02"
        },
        {
          "id": "MDRW2100001480.1.25",
