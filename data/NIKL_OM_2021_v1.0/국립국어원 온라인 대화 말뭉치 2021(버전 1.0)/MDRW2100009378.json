        {
          "id": "MDRW2100009378.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009378.1.2",
          "form": "네 좀 합니다",
          "original_form": "네 좀 합니다",
          "speaker_id": "2",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009378.1.3",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210822 23:03"
        },
        {
          "id": "MDRW2100009378.1.4",
          "form": "청소 잘 하세요?",
          "original_form": "청소 잘 하세요?",
          "speaker_id": "2",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009378.1.5",
          "form": "열심히해요",
          "original_form": "열심히해요",
          "speaker_id": "1",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009378.1.6",
          "form": "다른 집안일은 안하시나요?",
          "original_form": "다른 집안일은 안하시나요?",
          "speaker_id": "2",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009378.1.7",
          "form": "설거지요",
          "original_form": "설거지요",
          "speaker_id": "1",
          "time": "20210822 23:09"
        },
        {
          "id": "MDRW2100009378.1.8",
          "form": "설거지도 하고 청소도 하면 많이 힘드시겠네요",
          "original_form": "설거지도 하고 청소도 하면 많이 힘드시겠네요",
          "speaker_id": "2",
          "time": "20210822 23:10"
        },
        {
          "id": "MDRW2100009378.1.9",
          "form": "분담해야죠",
          "original_form": "분담해야죠",
          "speaker_id": "1",
          "time": "20210822 23:11"
        },
        {
          "id": "MDRW2100009378.1.10",
          "form": "요리는 배우자분이 하시는 건가요?",
          "original_form": "요리는 배우자분이 하시는 건가요?",
          "speaker_id": "2",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009378.1.11",
          "form": "네 요똥이요",
          "original_form": "네 요똥이요",
          "speaker_id": "1",
          "time": "20210822 23:13"
        },
        {
          "id": "MDRW2100009378.1.12",
          "form": "요똥이 뭐예요?",
          "original_form": "요똥이 뭐예요?",
