        {
          "id": "MDRW2100045911.1.1",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045911.1.2",
          "form": "참치 맛 별로 여러가지ㅋㅋㅋ",
          "original_form": "참치 맛 별로 여러가지ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045911.1.3",
          "form": "참치는 유통기한 몇년 넘겨도된자고",
          "original_form": "참치는 유통기한 몇년 넘겨도된자고",
          "speaker_id": "2",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045911.1.4",
          "form": "그러던데 또",
          "original_form": "그러던데 또",
          "speaker_id": "2",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045911.1.5",
          "form": "그래도 역시 기본이 제일 많긴 해요",
          "original_form": "그래도 역시 기본이 제일 많긴 해요",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045911.1.6",
          "form": "그죠그죠",
          "original_form": "그죠그죠",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045911.1.7",
          "form": "크 완벽하다",
          "original_form": "크 완벽하다",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045911.1.8",
          "form": "저도기본",
          "original_form": "저도기본",
          "speaker_id": "2",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045911.1.9",
          "form": "기본이 최고죠",
          "original_form": "기본이 최고죠",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045911.1.10",
          "form": "물리지도 않구",
          "original_form": "물리지도 않구",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045911.1.11",
          "form": "그래도 뿌듯한 건",
          "original_form": "그래도 뿌듯한 건",
          "speaker_id": "1",
          "time": "20210908 08:36"
        },
        {
          "id": "MDRW2100045911.1.12",
          "form": "겨울 오기 전에 역시즌으로 패딩 산 거예요ㅎㅎ",
          "original_form": "겨울 오기 전에 역시즌으로 패딩 산 거예요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:37"
        },
        {
          "id": "MDRW2100045911.1.13",
          "form": "역시즌에 진짜 싸지더라고요",
          "original_form": "역시즌에 진짜 싸지더라고요",
          "speaker_id": "1",
          "time": "20210908 08:37"
        },
