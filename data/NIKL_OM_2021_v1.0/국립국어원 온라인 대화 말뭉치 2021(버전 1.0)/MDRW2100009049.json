      "utterance": [
        {
          "id": "MDRW2100009049.1.1",
          "form": "오늘 친구 만나서",
          "original_form": "오늘 친구 만나서",
          "speaker_id": "2",
          "time": "20210822 20:48"
        },
        {
          "id": "MDRW2100009049.1.2",
          "form": "국밥 먹고 카페가는 일정이였거든",
          "original_form": "국밥 먹고 카페가는 일정이였거든",
          "speaker_id": "2",
          "time": "20210822 20:48"
        },
        {
          "id": "MDRW2100009049.1.3",
          "form": "목적이 밥 먹는거였어",
          "original_form": "목적이 밥 먹는거였어",
          "speaker_id": "2",
          "time": "20210822 20:48"
        },
        {
          "id": "MDRW2100009049.1.4",
          "form": "국밥 좋지",
          "original_form": "국밥 좋지",
          "speaker_id": "1",
          "time": "20210822 20:48"
        },
        {
          "id": "MDRW2100009049.1.5",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210822 20:49"
        },
        {
          "id": "MDRW2100009049.1.6",
          "form": "맛있더라",
          "original_form": "맛있더라",
          "speaker_id": "2",
          "time": "20210822 20:49"
        },
        {
          "id": "MDRW2100009049.1.7",
          "form": "카페는 일부러 집 근처 사람 없는 곳 갔는데",
          "original_form": "카페는 일부러 집 근처 사람 없는 곳 갔는데",
          "speaker_id": "2",
          "time": "20210822 20:49"
        },
        {
          "id": "MDRW2100009049.1.8",
          "form": "좋더라",
          "original_form": "좋더라",
          "speaker_id": "2",
          "time": "20210822 20:49"
        },
        {
          "id": "MDRW2100009049.1.9",
          "form": "어디 국밥집 갔어?",
          "original_form": "어디 국밥집 갔어?",
          "speaker_id": "1",
          "time": "20210822 20:49"
        },
        {
          "id": "MDRW2100009049.1.10",
          "form": "장수촌",
          "original_form": "장수촌",
          "speaker_id": "2",
          "time": "20210822 20:49"
        },
        {
          "id": "MDRW2100009049.1.11",
          "form": "아 들어본 것 같다",
          "original_form": "아 들어본 것 같다",
          "speaker_id": "1",
          "time": "20210822 20:49"
        },
        {
          "id": "MDRW2100009049.1.12",
          "form": "거기 보다는 다른데가 맛있는데",
          "original_form": "거기 보다는 다른데가 맛있는데",
          "speaker_id": "2",
          "time": "20210822 20:49"
        },
        {
          "id": "MDRW2100009049.1.13",
          "form": "그냥 할인행사해서",
          "original_form": "그냥 할인행사해서",
          "speaker_id": "2",
          "time": "20210822 20:49"
        },
        {
          "id": "MDRW2100009049.1.14",
          "form": "거기 갔지",
          "original_form": "거기 갔지",
          "speaker_id": "2",
          "time": "20210822 20:50"
        },
        {
          "id": "MDRW2100009049.1.15",
          "form": "수백당 맛있음",
          "original_form": "수백당 맛있음",
          "speaker_id": "1",
          "time": "20210822 20:50"
        },
        {
          "id": "MDRW2100009049.1.16",
          "form": "그 고려병원 쪽에",
          "original_form": "그 고려병원 쪽에",
          "speaker_id": "1",
          "time": "20210822 20:50"
        },
        {
          "id": "MDRW2100009049.1.17",
          "form": "거기 가고 싶은데..",
          "original_form": "거기 가고 싶은데..",
          "speaker_id": "2",
          "time": "20210822 20:50"
        },
        {
          "id": "MDRW2100009049.1.18",
          "form": "갈 사람이 없다..",
          "original_form": "갈 사람이 없다..",
          "speaker_id": "2",
          "time": "20210822 20:50"
        },
        {
          "id": "MDRW2100009049.1.19",
          "form": "ㅠㅠ 그런... 같이 가자",
          "original_form": "ㅠㅠ 그런... 같이 가자",
          "speaker_id": "1",
          "time": "20210822 20:50"
        },
        {
          "id": "MDRW2100009049.1.20",
          "form": "아버지가 국밥이 질리셔서",
          "original_form": "아버지가 국밥이 질리셔서",
          "speaker_id": "2",
          "time": "20210822 20:50"
        },
        {
          "id": "MDRW2100009049.1.21",
          "form": "같이 안 가줌",
          "original_form": "같이 안 가줌",
          "speaker_id": "2",
          "time": "20210822 20:50"
        },
        {
          "id": "MDRW2100009049.1.22",
          "form": "좋아좋아",
          "original_form": "좋아좋아",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009049.1.23",
          "form": "너랑 밥 안 먹은지 엄청 오래된 기분..",
          "original_form": "너랑 밥 안 먹은지 엄청 오래된 기분..",
          "speaker_id": "1",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009049.1.24",
          "form": "맞아..",
          "original_form": "맞아..",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009049.1.25",
          "form": "개강 전에 만날래?ㅜㅜ",
          "original_form": "개강 전에 만날래?ㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009049.1.26",
          "form": "좋지",
          "original_form": "좋지",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009049.1.27",
          "form": "확진자도 요즘 좀 줄던데",
          "original_form": "확진자도 요즘 좀 줄던데",
          "speaker_id": "1",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009049.1.28",
          "form": "애들이랑 얘기해보자",
          "original_form": "애들이랑 얘기해보자",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009049.1.29",
          "form": "지금 아니면",
          "original_form": "지금 아니면",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009049.1.30",
          "form": "우리 또 못 만남..",
          "original_form": "우리 또 못 만남..",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009049.1.31",
          "form": "흑흑",
          "original_form": "흑흑",
          "speaker_id": "2",
          "time": "20210822 20:51"
        },
        {
          "id": "MDRW2100009049.1.32",
          "form": "오늘 친구랑 카페도 괜찮더라",
          "original_form": "오늘 친구랑 카페도 괜찮더라",
          "speaker_id": "2",
          "time": "20210822 20:52"
        },
        {
          "id": "MDRW2100009049.1.33",
          "form": "ㅠㅠ 근데 6명 다 만나는 건 좀 그렇고",
          "original_form": "ㅠㅠ 근데 6명 다 만나는 건 좀 그렇고",
          "speaker_id": "1",
          "time": "20210822 20:52"
        },
        {
          "id": "MDRW2100009049.1.34",
          "form": "나눠서 만나야 될 듯",
          "original_form": "나눠서 만나야 될 듯",
          "speaker_id": "1",
          "time": "20210822 20:52"
        },
        {
          "id": "MDRW2100009049.1.35",
          "form": "아메리카노가 3500원인데 1L고 보틀도 줌",
          "original_form": "아메리카노가 3500원인데 1L고 보틀도 줌",
          "speaker_id": "2",
          "time": "20210822 20:52"
        },
        {
          "id": "MDRW2100009049.1.36",
          "form": "그래야 될듯",
          "original_form": "그래야 될듯",
          "speaker_id": "2",
          "time": "20210822 20:52"
        },
        {
          "id": "MDRW2100009049.1.37",
          "form": "경우의 수가 시작되는 건가",
          "original_form": "경우의 수가 시작되는 건가",
          "speaker_id": "2",
          "time": "20210822 20:52"
        },
        {
          "id": "MDRW2100009049.1.38",
          "form": "아 거기 들어본 것 같다",
          "original_form": "아 거기 들어본 것 같다",
          "speaker_id": "1",
          "time": "20210822 20:53"
        },
        {
          "id": "MDRW2100009049.1.39",
          "form": "카페 이름이 원유로였어",
          "original_form": "카페 이름이 원유로였어",
          "speaker_id": "2",
          "time": "20210822 20:53"
        },
        {
          "id": "MDRW2100009049.1.40",
          "form": "어 맞아 거기..",
          "original_form": "어 맞아 거기..",
          "speaker_id": "1",
          "time": "20210822 20:54"
        }
      ]
    }
