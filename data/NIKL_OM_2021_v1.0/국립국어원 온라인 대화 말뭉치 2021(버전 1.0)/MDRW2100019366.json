      "utterance": [
        {
          "id": "MDRW2100019366.1.1",
          "form": "바로 이혼한다 나는",
          "original_form": "바로 이혼한다 나는",
          "speaker_id": "2",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019366.1.2",
          "form": "그래",
          "original_form": "그래",
          "speaker_id": "2",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019366.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ나도",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ나도",
          "speaker_id": "1",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019366.1.4",
          "form": "우리엄마 진짜똑똑한데",
          "original_form": "우리엄마 진짜똑똑한데",
          "speaker_id": "1",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019366.1.5",
          "form": "근데 솔직히 이혼하는것도",
          "original_form": "근데 솔직히 이혼하는것도",
          "speaker_id": "2",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019366.1.6",
          "form": "머리아플것 같아서",
          "original_form": "머리아플것 같아서",
          "speaker_id": "2",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019366.1.7",
          "form": "너무 집안일하고 돈버는 기계처럼살아서",
          "original_form": "너무 집안일하고 돈버는 기계처럼살아서",
          "speaker_id": "1",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019366.1.8",
          "form": "그냥 결혼을 안하는게...",
          "original_form": "그냥 결혼을 안하는게...",
          "speaker_id": "2",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019366.1.9",
          "form": "안쓰러움",
          "original_form": "안쓰러움",
          "speaker_id": "1",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019366.1.10",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019366.1.11",
          "form": "이혼도 일이지",
          "original_form": "이혼도 일이지",
          "speaker_id": "1",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019366.1.12",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019366.1.13",
          "form": "여자들 너무",
          "original_form": "여자들 너무",
          "speaker_id": "2",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019366.1.14",
          "form": "진짜...",
          "original_form": "진짜...",
          "speaker_id": "2",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019366.1.15",
          "form": "우ㅜ리ㅣ엄ㅁ마 중학생인ㄴㄴ가 그때",
          "original_form": "우ㅜ리ㅣ엄ㅁ마 중학생인ㄴㄴ가 그때",
          "speaker_id": "1",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100019366.1.16",
          "form": "서울이랑 제주도로",
          "original_form": "서울이랑 제주도로",
          "speaker_id": "1",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100019366.1.17",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100019366.1.18",
          "form": "식모로 팔려간적도 있댜",
          "original_form": "식모로 팔려간적도 있댜",
          "speaker_id": "1",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100019366.1.19",
          "form": "할아버지가 돈주고 팔아서ㅋㅋㅋ",
          "original_form": "할아버지가 돈주고 팔아서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100019366.1.20",
          "form": "하.....",
          "original_form": "하.....",
          "speaker_id": "2",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100019366.1.21",
          "form": "진짜",
          "original_form": "진짜",
          "speaker_id": "2",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100019366.1.22",
          "form": "빨리 취직 성공해서",
          "original_form": "빨리 취직 성공해서",
          "speaker_id": "2",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100019366.1.23",
          "form": "집 나오자 ㅠㅠ",
          "original_form": "집 나오자 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100019366.1.24",
          "form": "나는 법 공부 열심히 해서",
          "original_form": "나는 법 공부 열심히 해서",
          "speaker_id": "2",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100019366.1.25",
          "form": "최대한 돈 많이 뜯어내는 법을 찾아볼게",
          "original_form": "최대한 돈 많이 뜯어내는 법을 찾아볼게",
          "speaker_id": "2",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100019366.1.26",
          "form": "뜯어내는것도 아니지",
          "original_form": "뜯어내는것도 아니지",
          "speaker_id": "2",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100019366.1.27",
          "form": "오ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ최고야",
          "original_form": "오ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ최고야",
          "speaker_id": "1",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100019366.1.28",
          "form": "돌려받는거지",
          "original_form": "돌려받는거지",
          "speaker_id": "2",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100019366.1.29",
          "form": "맞아 이혼할때",
          "original_form": "맞아 이혼할때",
          "speaker_id": "1",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100019366.1.30",
          "form": "애 한명당 양육비가 보통",
          "original_form": "애 한명당 양육비가 보통",
          "speaker_id": "1",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100019366.1.31",
          "form": "진짜 보면 볼수록 결혼은 손해야",
          "original_form": "진짜 보면 볼수록 결혼은 손해야",
          "speaker_id": "2",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100019366.1.32",
          "form": "5~60만원정도로 계산된다매 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "5~60만원정도로 계산된다매 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019366.1.33",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "2",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019366.1.34",
          "form": "어이없어진짜",
          "original_form": "어이없어진짜",
          "speaker_id": "1",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019366.1.35",
          "form": "엥",
          "original_form": "엥",
          "speaker_id": "2",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019366.1.36",
          "form": "5 6천도 적겠구만",
          "original_form": "5 6천도 적겠구만",
          "speaker_id": "2",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019366.1.37",
          "form": "최소 달마다 100은 줘야하는거아니냐",
          "original_form": "최소 달마다 100은 줘야하는거아니냐",
          "speaker_id": "1",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019366.1.38",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "2",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019366.1.39",
          "form": "옷이랑 식비 이것저것 하면",
          "original_form": "옷이랑 식비 이것저것 하면",
          "speaker_id": "2",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019366.1.40",
          "form": "100은 기본임",
          "original_form": "100은 기본임",
          "speaker_id": "2",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019366.1.41",
          "form": "아 저거 달마다 5 60 말한거임",
          "original_form": "아 저거 달마다 5 60 말한거임",
          "speaker_id": "1",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019366.1.42",
          "form": "ㅁㅈ ㅡㅡ",
          "original_form": "ㅁㅈ ㅡㅡ",
          "speaker_id": "1",
          "time": "20210827 18:28"
        },
