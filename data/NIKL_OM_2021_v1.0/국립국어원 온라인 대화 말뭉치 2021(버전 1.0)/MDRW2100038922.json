      "utterance": [
        {
          "id": "MDRW2100038922.1.1",
          "form": "나 다이어트 할꺼야",
          "original_form": "나 다이어트 할꺼야",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100038922.1.2",
          "form": "같이 할래?",
          "original_form": "같이 할래?",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100038922.1.3",
          "form": "갑자기 왜?",
          "original_form": "갑자기 왜?",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100038922.1.4",
          "form": "다이어트 어떻게 하는지 알아?",
          "original_form": "다이어트 어떻게 하는지 알아?",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100038922.1.5",
          "form": "그냥 다이어트하면",
          "original_form": "그냥 다이어트하면",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100038922.1.6",
          "form": "어떻게할건데?",
          "original_form": "어떻게할건데?",
          "speaker_id": "2",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100038922.1.7",
          "form": "건강에도 좋고",
          "original_form": "건강에도 좋고",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100038922.1.8",
          "form": "폐활량도 좋아지잖아",
          "original_form": "폐활량도 좋아지잖아",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100038922.1.9",
          "form": "폐활량 그지라서 다이어트해야돼",
          "original_form": "폐활량 그지라서 다이어트해야돼",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100038922.1.10",
          "form": "음..",
          "original_form": "음..",
          "speaker_id": "1",
          "time": "20210902 22:53"
        },
        {
          "id": "MDRW2100038922.1.11",
          "form": "음식을 덜 먹구",
          "original_form": "음식을 덜 먹구",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038922.1.12",
          "form": "싸이클 운동기구 좀 하구?",
          "original_form": "싸이클 운동기구 좀 하구?",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038922.1.13",
          "form": "음식을 덜 먹는다구 폐활량이 늘어나지 않아~!!!!!!",
          "original_form": "음식을 덜 먹는다구 폐활량이 늘어나지 않아~!!!!!!",
          "speaker_id": "2",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038922.1.14",
          "form": "평일엔 걸으니까 괜찮구 안걷는날에",
          "original_form": "평일엔 걸으니까 괜찮구 안걷는날에",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038922.1.15",
          "form": "그러면 뭐가 좋을까?",
          "original_form": "그러면 뭐가 좋을까?",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038922.1.16",
          "form": "건강을",
          "original_form": "건강을",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038922.1.17",
          "form": "자전거를 돌려",
          "original_form": "자전거를 돌려",
          "speaker_id": "2",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038922.1.18",
          "form": "챙기기위해서",
          "original_form": "챙기기위해서",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038922.1.19",
          "form": "걷기운동하구",
          "original_form": "걷기운동하구",
          "speaker_id": "2",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038922.1.20",
          "form": "양파도 먹어!!",
          "original_form": "양파도 먹어!!",
          "speaker_id": "2",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038922.1.21",
          "form": "걷기운동은 맨날 하는데",
          "original_form": "걷기운동은 맨날 하는데",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038922.1.22",
          "form": "딱히 도움안돼",
          "original_form": "딱히 도움안돼",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038922.1.23",
          "form": "양파안먹어",
          "original_form": "양파안먹어",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038922.1.24",
          "form": "덜하는거지",
          "original_form": "덜하는거지",
          "speaker_id": "2",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038922.1.25",
          "form": "대신 난 해산물을 잘먹잖어",
          "original_form": "대신 난 해산물을 잘먹잖어",
          "speaker_id": "1",
          "time": "20210902 22:54"
        },
        {
          "id": "MDRW2100038922.1.26",
          "form": "한시간씩 학교를 걸어다니면",
          "original_form": "한시간씩 학교를 걸어다니면",
          "speaker_id": "2",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100038922.1.27",
          "form": "효과가 있을거야",
          "original_form": "효과가 있을거야",
          "speaker_id": "2",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100038922.1.28",
          "form": "ㅇㅁㅇ",
          "original_form": "ㅇㅁㅇ",
          "speaker_id": "1",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100038922.1.29",
          "form": "그니까 갈때 1시간",
          "original_form": "그니까 갈때 1시간",
          "speaker_id": "1",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100038922.1.30",
          "form": "올때 1시간?",
          "original_form": "올때 1시간?",
          "speaker_id": "1",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100038922.1.31",
          "form": "바로그거야",
          "original_form": "바로그거야",
          "speaker_id": "2",
          "time": "20210902 22:55"
        },
        {
          "id": "MDRW2100038922.1.32",
          "form": "근데 평소에 30분씩 4번걷자나",
          "original_form": "근데 평소에 30분씩 4번걷자나",
