      "utterance": [
        {
          "id": "MDRW2100041178.1.1",
          "form": "건강을 위해서 꼭 하는게 있으실까요?",
          "original_form": "건강을 위해서 꼭 하는게 있으실까요?",
          "speaker_id": "1",
          "time": "20210908 08:11"
        },
        {
          "id": "MDRW2100041178.1.2",
          "form": "많이 걸으려고 하는데 잘안되네요",
          "original_form": "많이 걸으려고 하는데 잘안되네요",
          "speaker_id": "2",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100041178.1.3",
          "form": "저도오",
          "original_form": "저도오",
          "speaker_id": "1",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100041178.1.4",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100041178.1.5",
          "form": "운동 잘안하게 되니 움직여야 하는데ㅠ",
          "original_form": "운동 잘안하게 되니 움직여야 하는데ㅠ",
          "speaker_id": "1",
          "time": "20210908 08:13"
        },
        {
          "id": "MDRW2100041178.1.6",
          "form": "맞아요 거기다 비오고 더우면 더안나가고",
          "original_form": "맞아요 거기다 비오고 더우면 더안나가고",
          "speaker_id": "2",
          "time": "20210908 08:14"
        },
        {
          "id": "MDRW2100041178.1.7",
          "form": "맞아요ㅋㅋㅋㅋㅜ",
          "original_form": "맞아요ㅋㅋㅋㅋㅜ",
          "speaker_id": "1",
          "time": "20210908 08:15"
        },
        {
          "id": "MDRW2100041178.1.8",
          "form": "저는 걷는것도 있는데 아침에 일어나 물을 마십니다",
          "original_form": "저는 걷는것도 있는데 아침에 일어나 물을 마십니다",
          "speaker_id": "1",
          "time": "20210908 08:16"
        },
        {
          "id": "MDRW2100041178.1.9",
          "form": "물은 진짜 안먹혀요",
          "original_form": "물은 진짜 안먹혀요",
          "speaker_id": "2",
          "time": "20210908 08:17"
        },
        {
          "id": "MDRW2100041178.1.10",
          "form": "그래서요. 물을 2리터정도 먹어야한다는데",
          "original_form": "그래서요. 물을 2리터정도 먹어야한다는데",
          "speaker_id": "1",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100041178.1.11",
          "form": "낮에 잘안먹어서 ㅋㅋㅋㅋ",
          "original_form": "낮에 잘안먹어서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:19"
        },
        {
          "id": "MDRW2100041178.1.12",
          "form": "네 하루에 텀블러 세잔은 마셔야지 하는데",
          "original_form": "네 하루에 텀블러 세잔은 마셔야지 하는데",
          "speaker_id": "2",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100041178.1.13",
          "form": "진짜 안되더라고요",
          "original_form": "진짜 안되더라고요",
          "speaker_id": "2",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100041178.1.14",
          "form": "그래도 2잔은 마시겠어요.. !!",
          "original_form": "그래도 2잔은 마시겠어요.. !!",
          "speaker_id": "1",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100041178.1.15",
          "form": "전 사무실에 900짜리컵 두고 먹는데 많이 먹어야.. 반반씩 2번인가봐요 ㅋㅋㅋ",
          "original_form": "전 사무실에 900짜리컵 두고 먹는데 많이 먹어야.. 반반씩 2번인가봐요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:24"
        },
        {
          "id": "MDRW2100041178.1.16",
          "form": "저도 오백짜리 잇어서 세시간에 한잔씩 먹자 하는데 하루 한잔이요ㅋㅋㅋ",
          "original_form": "저도 오백짜리 잇어서 세시간에 한잔씩 먹자 하는데 하루 한잔이요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:25"
        },
        {
          "id": "MDRW2100041178.1.17",
          "form": "요즘은 콤부차가루 넣어 먹으면",
          "original_form": "요즘은 콤부차가루 넣어 먹으면",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100041178.1.18",
          "form": "이온음료같아서 먹을만 하다는데..",
          "original_form": "이온음료같아서 먹을만 하다는데..",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100041178.1.19",
          "form": "그렇게까지는 안하고 싶고ㅋㅋㅋ",
          "original_form": "그렇게까지는 안하고 싶고ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:26"
        },
        {
          "id": "MDRW2100041178.1.20",
          "form": "아 그렇게 하면 좀더 먹히긴 하겟네요ㅋ",
          "original_form": "아 그렇게 하면 좀더 먹히긴 하겟네요ㅋ",
          "speaker_id": "2",
          "time": "20210908 08:27"
        },
        {
          "id": "MDRW2100041178.1.21",
          "form": "그쵸? ㅎㅎ",
          "original_form": "그쵸? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:28"
        },
        {
          "id": "MDRW2100041178.1.22",
          "form": "하루에 얼마나 걸으시나요?",
          "original_form": "하루에 얼마나 걸으시나요?",
          "speaker_id": "1",
