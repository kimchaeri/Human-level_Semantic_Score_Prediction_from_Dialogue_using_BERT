      "utterance": [
        {
          "id": "MDRW2100014019.1.1",
          "form": "화장품 좋아하세요 ?",
          "original_form": "화장품 좋아하세요 ?",
          "speaker_id": "1",
          "time": "20210825 16:51"
        },
        {
          "id": "MDRW2100014019.1.2",
          "form": "평소에 화장 적당히 하는 편인데, 마스크 쓰고 다닌 이후로는 거의 안하는 거 같아요ㅋㅋㅋ",
          "original_form": "평소에 화장 적당히 하는 편인데, 마스크 쓰고 다닌 이후로는 거의 안하는 거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014019.1.3",
          "form": "평소에 화장 하세요?",
          "original_form": "평소에 화장 하세요?",
          "speaker_id": "2",
          "time": "20210825 16:52"
        },
        {
          "id": "MDRW2100014019.1.4",
          "form": "ㅋㅋㅋㅋㅋ 앗 저두요",
          "original_form": "ㅋㅋㅋㅋㅋ 앗 저두요",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014019.1.5",
          "form": "놀러갈때만 해용 ㅎㅎ",
          "original_form": "놀러갈때만 해용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 16:53"
        },
        {
          "id": "MDRW2100014019.1.6",
          "form": "이제 화장품도 거의 안사게 되더라구요..!",
          "original_form": "이제 화장품도 거의 안사게 되더라구요..!",
          "speaker_id": "1",
          "time": "20210825 16:54"
        },
        {
          "id": "MDRW2100014019.1.7",
          "form": "그쵸! 근데 색조 화장품 안사는 만큼 기초 화장품을 사고 있더라구욬ㅋㅋㅋㅋㅋ",
          "original_form": "그쵸! 근데 색조 화장품 안사는 만큼 기초 화장품을 사고 있더라구욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014019.1.8",
          "form": "피부에 계속 뭐가 나다보니ㅜㅜ",
          "original_form": "피부에 계속 뭐가 나다보니ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 16:55"
        },
        {
          "id": "MDRW2100014019.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아맞아요 그래소 요즘 팩,, 하고있어요 흑흑",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 아맞아요 그래소 요즘 팩,, 하고있어요 흑흑",
          "speaker_id": "1",
          "time": "20210825 16:57"
        },
        {
          "id": "MDRW2100014019.1.10",
          "form": "오 무슨 팩하세요? 저는 게을러서 팩 쌓아만 두고 하질 않아서ㅜㅜ",
          "original_form": "오 무슨 팩하세요? 저는 게을러서 팩 쌓아만 두고 하질 않아서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100014019.1.11",
          "form": "저는 로션엑스라구 피지연화제쓰고 화산송이 하고 시트팩 아무거나 덮어줘용!",
          "original_form": "저는 로션엑스라구 피지연화제쓰고 화산송이 하고 시트팩 아무거나 덮어줘용!",
          "speaker_id": "1",
          "time": "20210825 16:58"
        },
        {
          "id": "MDRW2100014019.1.12",
          "form": "저는 지성이라 ㅠㅠ",
          "original_form": "저는 지성이라 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 16:59"
        },
        {
          "id": "MDRW2100014019.1.13",
          "form": "혹시 말랑피부 루틴인가요?!",
          "original_form": "혹시 말랑피부 루틴인가요?!",
          "speaker_id": "2",
          "time": "20210825 17:00"
        },
        {
          "id": "MDRW2100014019.1.14",
          "form": "앗넹 ㅋㅋㅋㅋㅋ 간소화버전이요!!",
          "original_form": "앗넹 ㅋㅋㅋㅋㅋ 간소화버전이요!!",
          "speaker_id": "1",
          "time": "20210825 17:02"
        },
        {
          "id": "MDRW2100014019.1.15",
          "form": "자기 전에 세수하는 것도 귀찮아하는 굼벵이라 알기만하고 해보질 못했네요ㅋㅋㅋㅋ",
          "original_form": "자기 전에 세수하는 것도 귀찮아하는 굼벵이라 알기만하고 해보질 못했네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100014019.1.16",
          "form": "효과 좋나요?",
          "original_form": "효과 좋나요?",
          "speaker_id": "2",
          "time": "20210825 17:03"
        },
        {
          "id": "MDRW2100014019.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ 지성이라 각질제거에 진짜 효과있는것같아요!!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 지성이라 각질제거에 진짜 효과있는것같아요!!",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014019.1.18",
          "form": "지성이신가용?!",
          "original_form": "지성이신가용?!",
          "speaker_id": "1",
          "time": "20210825 17:05"
        },
        {
          "id": "MDRW2100014019.1.19",
          "form": "네네! 여름엔 피지폭발 지성인데, 겨울엔 복합성 돼서 많이 터요ㅜㅜ",
          "original_form": "네네! 여름엔 피지폭발 지성인데, 겨울엔 복합성 돼서 많이 터요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014019.1.20",
          "form": "으악 ㅜㅜ 관리 힘드시겠어요",
          "original_form": "으악 ㅜㅜ 관리 힘드시겠어요",
          "speaker_id": "1",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014019.1.21",
          "form": "화산송이 사야하나.... 좀 혹하는데요?ㅋㅋㅋㅋ",
          "original_form": "화산송이 사야하나.... 좀 혹하는데요?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 17:06"
        },
        {
          "id": "MDRW2100014019.1.22",
          "form": "ㅋㅋㅋㅋ 그런데 저는 심한지성이라.. 단독으로 그것만 쓰기에는 별로인것같아요 ㅠ",
          "original_form": "ㅋㅋㅋㅋ 그런데 저는 심한지성이라.. 단독으로 그것만 쓰기에는 별로인것같아요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 17:07"
        }
      ]
