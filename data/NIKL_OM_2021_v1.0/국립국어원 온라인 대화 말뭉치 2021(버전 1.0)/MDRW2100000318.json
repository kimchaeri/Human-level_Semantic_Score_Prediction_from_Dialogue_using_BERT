      "utterance": [
        {
          "id": "MDRW2100000318.1.1",
          "form": "치킨시켜먹이까?",
          "original_form": "치킨시켜먹이까?",
          "speaker_id": "1",
          "time": "20210819 17:30"
        },
        {
          "id": "MDRW2100000318.1.2",
          "form": "시켜먹던지 난 간단히 먹었어",
          "original_form": "시켜먹던지 난 간단히 먹었어",
          "speaker_id": "2",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000318.1.3",
          "form": "name3이 먹고싶다니까",
          "original_form": "&name3&이 먹고싶다니까",
          "speaker_id": "2",
          "time": "20210819 17:31"
        },
        {
          "id": "MDRW2100000318.1.4",
          "form": "깊콘있으니깐 뿌링클",
          "original_form": "깊콘있으니깐 뿌링클",
          "speaker_id": "1",
          "time": "20210819 17:35"
        },
        {
          "id": "MDRW2100000318.1.5",
          "form": "암꺼나 먹으세요",
          "original_form": "암꺼나 먹으세요",
          "speaker_id": "2",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000318.1.6",
          "form": "네 ~~~~~  일요일은 쉬나",
          "original_form": "네 ~~~~~  일요일은 쉬나",
          "speaker_id": "1",
          "time": "20210819 17:43"
        },
        {
          "id": "MDRW2100000318.1.7",
          "form": "응 일요일은 쉬어",
          "original_form": "응 일요일은 쉬어",
          "speaker_id": "2",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000318.1.8",
          "form": "거기 가나?",
          "original_form": "거기 가나?",
          "speaker_id": "2",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000318.1.9",
          "form": "삼국유사 뭐시기하는곳",
          "original_form": "삼국유사 뭐시기하는곳",
          "speaker_id": "2",
          "time": "20210819 17:44"
        },
        {
          "id": "MDRW2100000318.1.10",
          "form": "못가지 비오는데 애들이랑 또 집콕 해야지모",
          "original_form": "못가지 비오는데 애들이랑 또 집콕 해야지모",
          "speaker_id": "1",
          "time": "20210819 17:46"
        },
        {
          "id": "MDRW2100000318.1.11",
          "form": "토요일만 비오던데",
          "original_form": "토요일만 비오던데",
          "speaker_id": "2",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000318.1.12",
          "form": "주말마다 비오는것같아",
          "original_form": "주말마다 비오는것같아",
          "speaker_id": "2",
          "time": "20210819 17:47"
        },
        {
          "id": "MDRW2100000318.1.13",
          "form": "그러게  애들이랑 집에서 뒹굴뒹굴하고..밥이나 먹고 놀고 청노나하구",
          "original_form": "그러게  애들이랑 집에서 뒹굴뒹굴하고..밥이나 먹고 놀고 청노나하구",
          "speaker_id": "1",
          "time": "20210819 17:49"
        },
        {
          "id": "MDRW2100000318.1.14",
          "form": "토요일날 형님이 비와도 친다 라고하던데",
          "original_form": "토요일날 형님이 비와도 친다 라고하던데",
          "speaker_id": "2",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100000318.1.15",
          "form": "내생각에 비안올것같애",
          "original_form": "내생각에 비안올것같애",
          "speaker_id": "2",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100000318.1.16",
          "form": "일요일은 왜?",
          "original_form": "일요일은 왜?",
          "speaker_id": "2",
          "time": "20210819 17:53"
        },
        {
          "id": "MDRW2100000318.1.17",
          "form": "일요일은 실장님 집에서 쉬시니깐 나도 쉬어야지",
          "original_form": "일요일은 실장님 집에서 쉬시니깐 나도 쉬어야지",
          "speaker_id": "1",
          "time": "20210819 17:56"
        },
        {
          "id": "MDRW2100000318.1.18",
          "form": "그게 무슨말이야?",
          "original_form": "그게 무슨말이야?",
          "speaker_id": "2",
          "time": "20210819 17:56"
        },
        {
          "id": "MDRW2100000318.1.19",
          "form": "토요일만 활동하고 일요일은 집에서 쉬고 월요일을시작하시거든",
          "original_form": "토요일만 활동하고 일요일은 집에서 쉬고 월요일을시작하시거든",
          "speaker_id": "1",
          "time": "20210819 17:57"
        },
        {
          "id": "MDRW2100000318.1.20",
          "form": "토요일 비와서 취소된거 아냐? 애들한테 이야기 했어?",
          "original_form": "토요일 비와서 취소된거 아냐? 애들한테 이야기 했어?",
          "speaker_id": "2",
          "time": "20210819 17:58"
        },
        {
          "id": "MDRW2100000318.1.21",
          "form": "애들은 아직 가는거 몰라",
          "original_form": "애들은 아직 가는거 몰라",
          "speaker_id": "1",
          "time": "20210819 17:59"
        }
