        {
          "id": "MDRW2100023366.1.1",
          "form": "형제자매 있으세요?",
          "original_form": "형제자매 있으세요?",
          "speaker_id": "1",
          "time": "20210830 00:56"
        },
        {
          "id": "MDRW2100023366.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210830 00:56"
        },
        {
          "id": "MDRW2100023366.1.3",
          "form": "오 어떤 관계인가요?",
          "original_form": "오 어떤 관계인가요?",
          "speaker_id": "1",
          "time": "20210830 00:57"
        },
        {
          "id": "MDRW2100023366.1.4",
          "form": "동생이요",
          "original_form": "동생이요",
          "speaker_id": "2",
          "time": "20210830 00:57"
        },
        {
          "id": "MDRW2100023366.1.5",
          "form": "남동생이요?",
          "original_form": "남동생이요?",
          "speaker_id": "1",
          "time": "20210830 00:59"
        },
        {
          "id": "MDRW2100023366.1.6",
          "form": "네 님은요?",
          "original_form": "네 님은요?",
          "speaker_id": "2",
          "time": "20210830 00:59"
        },
        {
          "id": "MDRW2100023366.1.7",
          "form": "전 언니요 ㅎㅎ",
          "original_form": "전 언니요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:00"
        },
        {
          "id": "MDRW2100023366.1.8",
          "form": "아 여자세요?",
          "original_form": "아 여자세요?",
          "speaker_id": "2",
          "time": "20210830 01:00"
        },
        {
          "id": "MDRW2100023366.1.9",
          "form": "넵 남자분이시군요?",
          "original_form": "넵 남자분이시군요?",
          "speaker_id": "1",
          "time": "20210830 01:00"
        },
        {
          "id": "MDRW2100023366.1.10",
          "form": "네ㅋㅋ 성별이 중요한건 아니다만",
          "original_form": "네ㅋㅋ 성별이 중요한건 아니다만",
          "speaker_id": "2",
          "time": "20210830 01:03"
        },
        {
          "id": "MDRW2100023366.1.11",
          "form": "지금까지 남자분인줄",
          "original_form": "지금까지 남자분인줄",
          "speaker_id": "2",
          "time": "20210830 01:03"
        },
        {
          "id": "MDRW2100023366.1.12",
          "form": "아 진짜요 ㅋㅋ ??",
          "original_form": "아 진짜요 ㅋㅋ ??",
          "speaker_id": "1",
          "time": "20210830 01:03"
        },
        {
          "id": "MDRW2100023366.1.13",
          "form": "말투가 그랬나요 ㅋㅋㅋ",
          "original_form": "말투가 그랬나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100023366.1.14",
          "form": "집에선 거의 아들이지만 ㅋㅋ",
          "original_form": "집에선 거의 아들이지만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100023366.1.15",
          "form": "털털하신분인가봐요",
          "original_form": "털털하신분인가봐요",
          "speaker_id": "2",
          "time": "20210830 01:04"
        },
        {
          "id": "MDRW2100023366.1.16",
          "form": "성격이 좋으신듯",
          "original_form": "성격이 좋으신듯",
          "speaker_id": "2",
          "time": "20210830 01:05"
        },
        {
          "id": "MDRW2100023366.1.17",
          "form": "칭찬 감사합니다 ㅋㅋ !",
          "original_form": "칭찬 감사합니다 ㅋㅋ !",
          "speaker_id": "1",
