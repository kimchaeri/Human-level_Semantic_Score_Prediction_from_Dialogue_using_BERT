      "utterance": [
        {
          "id": "MDRW2100013381.1.1",
          "form": "올림픽 보셨나요?",
          "original_form": "올림픽 보셨나요?",
          "speaker_id": "2",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100013381.1.2",
          "form": "올림픽 좋아하시나요",
          "original_form": "올림픽 좋아하시나요",
          "speaker_id": "1",
          "time": "20210825 14:18"
        },
        {
          "id": "MDRW2100013381.1.3",
          "form": "보다말다 했네요",
          "original_form": "보다말다 했네요",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013381.1.4",
          "form": "저는 얼마전에 클라이밍보고",
          "original_form": "저는 얼마전에 클라이밍보고",
          "speaker_id": "2",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013381.1.5",
          "form": "올림픽 선수가 꿈인가요?",
          "original_form": "올림픽 선수가 꿈인가요?",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013381.1.6",
          "form": "너무 재밌어서 손에 땀이 한가득 났었어요",
          "original_form": "너무 재밌어서 손에 땀이 한가득 났었어요",
          "speaker_id": "2",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013381.1.7",
          "form": "다음생에 하려고요",
          "original_form": "다음생에 하려고요",
          "speaker_id": "2",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013381.1.8",
          "form": "와 손땀나면 클라이밍 못해요",
          "original_form": "와 손땀나면 클라이밍 못해요",
          "speaker_id": "1",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013381.1.9",
          "form": "그래서 포기했어요",
          "original_form": "그래서 포기했어요",
          "speaker_id": "2",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100013381.1.10",
          "form": "그래도 취미로 한번 도전해보고싶어요",
          "original_form": "그래도 취미로 한번 도전해보고싶어요",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013381.1.11",
          "form": "애초에 체력은 있으신지?",
          "original_form": "애초에 체력은 있으신지?",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013381.1.12",
          "form": "포기할수나 있으실지?",
          "original_form": "포기할수나 있으실지?",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013381.1.13",
          "form": "전신에 코어가 잘 잡혀야 되더라고요",
          "original_form": "전신에 코어가 잘 잡혀야 되더라고요",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013381.1.14",
          "form": "체력은 차근차근 키웠어야했는데",
          "original_form": "체력은 차근차근 키웠어야했는데",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013381.1.15",
          "form": "올림픽엔 코어가 중요하죠",
          "original_form": "올림픽엔 코어가 중요하죠",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013381.1.16",
          "form": "기회가없었네요",
          "original_form": "기회가없었네요",
          "speaker_id": "2",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013381.1.17",
          "form": "클라이밍 안무섭나요?",
          "original_form": "클라이밍 안무섭나요?",
          "speaker_id": "1",
          "time": "20210825 14:20"
        },
        {
          "id": "MDRW2100013381.1.18",
          "form": "한번 떨어지고 또 떨어지면 점점 체력이 약화되더라고요",
          "original_form": "한번 떨어지고 또 떨어지면 점점 체력이 약화되더라고요",
          "speaker_id": "2",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013381.1.19",
          "form": "완전 무서울듯요",
          "original_form": "완전 무서울듯요",
          "speaker_id": "1",
          "time": "20210825 14:21"
        },
        {
          "id": "MDRW2100013381.1.20",
          "form": "무섭겠지만 사다리탈때 나름재밌었어서",
          "original_form": "무섭겠지만 사다리탈때 나름재밌었어서",
          "speaker_id": "2",
          "time": "20210825 14:21"
        }
      ]
