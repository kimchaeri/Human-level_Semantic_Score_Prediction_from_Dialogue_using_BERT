      "utterance": [
        {
          "id": "MDRW2100010263.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210823 16:14"
        },
        {
          "id": "MDRW2100010263.1.2",
          "form": "요새 재밌게보고계신거있으세요?",
          "original_form": "요새 재밌게보고계신거있으세요?",
          "speaker_id": "1",
          "time": "20210823 16:14"
        },
        {
          "id": "MDRW2100010263.1.3",
          "form": "네 저는 대탈출이요",
          "original_form": "네 저는 대탈출이요",
          "speaker_id": "2",
          "time": "20210823 16:15"
        },
        {
          "id": "MDRW2100010263.1.4",
          "form": "아시나요?",
          "original_form": "아시나요?",
          "speaker_id": "2",
          "time": "20210823 16:15"
        },
        {
          "id": "MDRW2100010263.1.5",
          "form": "아니요ㅜㅜ 못보던거네요",
          "original_form": "아니요ㅜㅜ 못보던거네요",
          "speaker_id": "1",
          "time": "20210823 16:15"
        },
        {
          "id": "MDRW2100010263.1.6",
          "form": "집에 티비가 있으신가봐요 저는 유투브랑 왓챠로 봐요",
          "original_form": "집에 티비가 있으신가봐요 저는 유투브랑 왓챠로 봐요",
          "speaker_id": "1",
          "time": "20210823 16:16"
        },
        {
          "id": "MDRW2100010263.1.7",
          "form": "네네 티비도있구 넷플릭스랑 티빙으로도 봐요",
          "original_form": "네네 티비도있구 넷플릭스랑 티빙으로도 봐요",
          "speaker_id": "2",
          "time": "20210823 16:22"
        },
        {
          "id": "MDRW2100010263.1.8",
          "form": "와 부럽다",
          "original_form": "와 부럽다",
          "speaker_id": "1",
          "time": "20210823 16:24"
        },
        {
          "id": "MDRW2100010263.1.9",
          "form": "저는 킹덤보고싶어요ㅜ",
          "original_form": "저는 킹덤보고싶어요ㅜ",
          "speaker_id": "1",
          "time": "20210823 16:24"
        },
        {
          "id": "MDRW2100010263.1.10",
          "form": "넷플릭스랑 티빙 통신사덕에 공짜로 봐서 하지 돈주고는 안봤을꺼같아여ㅋㅋ 근대 킹덤은 재밋어요",
          "original_form": "넷플릭스랑 티빙 통신사덕에 공짜로 봐서 하지 돈주고는 안봤을꺼같아여ㅋㅋ 근대 킹덤은 재밋어요",
          "speaker_id": "2",
          "time": "20210824 10:40"
        },
        {
          "id": "MDRW2100010263.1.11",
          "form": "통신사 어디세요? 전 알뜰폰이거든욧ㅜ",
          "original_form": "통신사 어디세요? 전 알뜰폰이거든욧ㅜ",
          "speaker_id": "1",
          "time": "20210824 10:42"
        },
        {
          "id": "MDRW2100010263.1.12",
          "form": "유플러스에요~ 결합할인받고있는데 요금줄이려면 알뜰폰이 좋긴할꺼같아요",
          "original_form": "유플러스에요~ 결합할인받고있는데 요금줄이려면 알뜰폰이 좋긴할꺼같아요",
          "speaker_id": "2",
          "time": "20210825 17:56"
        },
        {
          "id": "MDRW2100010263.1.13",
          "form": "전에 저도 대형3사꺼 썼거든요 전시 티켓50할인 잘썼는데 코로나로 갈일이 없어져서 갈아탔어요",
          "original_form": "전에 저도 대형3사꺼 썼거든요 전시 티켓50할인 잘썼는데 코로나로 갈일이 없어져서 갈아탔어요",
          "speaker_id": "1",
          "time": "20210825 17:58"
        },
        {
          "id": "MDRW2100010263.1.14",
          "form": "저도 통신사 약정 끝나면 알뜰요금제로 갈아타려구요 영화보는거 빼고? 딱히 혜택도 잘모르겠네요",
          "original_form": "저도 통신사 약정 끝나면 알뜰요금제로 갈아타려구요 영화보는거 빼고? 딱히 혜택도 잘모르겠네요",
          "speaker_id": "2",
          "time": "20210825 18:28"
        },
        {
          "id": "MDRW2100010263.1.15",
          "form": "저도 약정위약금땜에 끝나자마자 바로 갈아탔어요",
          "original_form": "저도 약정위약금땜에 끝나자마자 바로 갈아탔어요",
          "speaker_id": "1",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100010263.1.16",
          "form": "지금 5900원정도 내고 있어요",
          "original_form": "지금 5900원정도 내고 있어요",
          "speaker_id": "1",
          "time": "20210825 18:29"
        },
        {
          "id": "MDRW2100010263.1.17",
          "form": "와 대박 전 10만원정도낸거같은데 데이터는 많이 안쓰시나요?",
          "original_form": "와 대박 전 10만원정도낸거같은데 데이터는 많이 안쓰시나요?",
          "speaker_id": "2",
          "time": "20210825 18:30"
        },
        {
          "id": "MDRW2100010263.1.18",
          "form": "월 1.5기가 인데 이걸로 길가면서 유투브보긴 좀 그래요ㅎㅎ",
          "original_form": "월 1.5기가 인데 이걸로 길가면서 유투브보긴 좀 그래요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 18:31"
        },
        {
          "id": "MDRW2100010263.1.19",
          "form": "아 유튭은 데이터가 많이들긴하죠~\r\n어디가든 와이파이 찾아야겠네요 장단점이있긴한데 요금이 너무 큰 메리트네요",
          "original_form": "아 유튭은 데이터가 많이들긴하죠~\r\n어디가든 와이파이 찾아야겠네요 장단점이있긴한데 요금이 너무 큰 메리트네요",
          "speaker_id": "2",
          "time": "20210825 18:35"
        },
        {
          "id": "MDRW2100010263.1.20",
          "form": "네 근데 저는 써보니까 여기저기 와파 많아서 유툽 다봐요ㅋㅋ",
          "original_form": "네 근데 저는 써보니까 여기저기 와파 많아서 유툽 다봐요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:36"
        },
        {
          "id": "MDRW2100010263.1.21",
          "form": "맞죠 어디든 와이파이는 잘되어있으니 저도 꼭 끝나면 갈아타야겠어요~",
          "original_form": "맞죠 어디든 와이파이는 잘되어있으니 저도 꼭 끝나면 갈아타야겠어요~",
          "speaker_id": "2",
          "time": "20210825 18:36"
        }
      ]
    }
