      "utterance": [
        {
          "id": "MDRW2100002285.1.1",
          "form": "안녕하세여",
          "original_form": "안녕하세여",
          "speaker_id": "2",
          "time": "20210820 08:07"
        },
        {
          "id": "MDRW2100002285.1.2",
          "form": "아침은 챙겨드시나여",
          "original_form": "아침은 챙겨드시나여",
          "speaker_id": "2",
          "time": "20210820 08:07"
        },
        {
          "id": "MDRW2100002285.1.3",
          "form": "지금 고구마먹고있어요",
          "original_form": "지금 고구마먹고있어요",
          "speaker_id": "1",
          "time": "20210820 08:07"
        },
        {
          "id": "MDRW2100002285.1.4",
          "form": "아침 드셨어요?",
          "original_form": "아침 드셨어요?",
          "speaker_id": "1",
          "time": "20210820 08:07"
        },
        {
          "id": "MDRW2100002285.1.5",
          "form": "오 아녀 ㅠ 전 아침은 안먹고",
          "original_form": "오 아녀 ㅠ 전 아침은 안먹고",
          "speaker_id": "2",
          "time": "20210820 08:08"
        },
        {
          "id": "MDRW2100002285.1.6",
          "form": "점심먹고 저녁은 간단히 ㅎㅎ",
          "original_form": "점심먹고 저녁은 간단히 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 08:08"
        },
        {
          "id": "MDRW2100002285.1.7",
          "form": "뭐좋아하세녀?",
          "original_form": "뭐좋아하세녀?",
          "speaker_id": "2",
          "time": "20210820 08:08"
        },
        {
          "id": "MDRW2100002285.1.8",
          "form": "점심은 회사에서 사먹고 저녁은 간단히 먹어요",
          "original_form": "점심은 회사에서 사먹고 저녁은 간단히 먹어요",
          "speaker_id": "1",
          "time": "20210820 08:09"
        },
        {
          "id": "MDRW2100002285.1.9",
          "form": "다 좋아해요",
          "original_form": "다 좋아해요",
          "speaker_id": "1",
          "time": "20210820 08:09"
        },
        {
          "id": "MDRW2100002285.1.10",
          "form": "고기랑 해산물 중엔 뭐좋아하시나요",
          "original_form": "고기랑 해산물 중엔 뭐좋아하시나요",
          "speaker_id": "2",
          "time": "20210820 08:10"
        },
        {
          "id": "MDRW2100002285.1.11",
          "form": "육지에 살아서 해산물은 바다 놀러갈때 자주먹어요",
          "original_form": "육지에 살아서 해산물은 바다 놀러갈때 자주먹어요",
          "speaker_id": "1",
          "time": "20210820 08:10"
        },
        {
          "id": "MDRW2100002285.1.12",
          "form": "고기는 일주일에 2회 이상 먹어요",
          "original_form": "고기는 일주일에 2회 이상 먹어요",
          "speaker_id": "1",
          "time": "20210820 08:11"
        },
        {
          "id": "MDRW2100002285.1.13",
          "form": "다좋아해요~",
          "original_form": "다좋아해요~",
          "speaker_id": "1",
          "time": "20210820 08:11"
        },
        {
          "id": "MDRW2100002285.1.14",
          "form": "점심은 회사에서 나오나요?",
          "original_form": "점심은 회사에서 나오나요?",
          "speaker_id": "2",
          "time": "20210820 08:29"
        },
        {
          "id": "MDRW2100002285.1.15",
          "form": "아님 배달 ?",
          "original_form": "아님 배달 ?",
          "speaker_id": "2",
          "time": "20210820 08:29"
        },
        {
          "id": "MDRW2100002285.1.16",
          "form": "식당가서먹어요 저희지역은3단계라4명까지 가능",
          "original_form": "식당가서먹어요 저희지역은3단계라4명까지 가능",
          "speaker_id": "1",
          "time": "20210820 08:50"
        },
        {
          "id": "MDRW2100002285.1.17",
          "form": "오 저흰 시켜먹는데",
          "original_form": "오 저흰 시켜먹는데",
          "speaker_id": "2",
          "time": "20210820 08:53"
        },
        {
          "id": "MDRW2100002285.1.18",
          "form": "오늘은 뭐먹을까 벌써고민이네영 ㅋㅋ",
          "original_form": "오늘은 뭐먹을까 벌써고민이네영 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 08:53"
        },
        {
          "id": "MDRW2100002285.1.19",
          "form": "회사에서 시켜먹어요?",
          "original_form": "회사에서 시켜먹어요?",
          "speaker_id": "1",
          "time": "20210820 09:08"
        },
        {
          "id": "MDRW2100002285.1.20",
          "form": "백신맞았어요? 저희는오늘2차맞아요",
          "original_form": "백신맞았어요? 저희는오늘2차맞아요",
          "speaker_id": "1",
          "time": "20210820 09:08"
        },
        {
          "id": "MDRW2100002285.1.21",
          "form": "네네 전 치과에있어서 2차까지 전에다맞았어용 !!",
          "original_form": "네네 전 치과에있어서 2차까지 전에다맞았어용 !!",
