      "utterance": [
        {
          "id": "MDRW2100043973.1.1",
          "form": "인형물품 살 때 중고품 많이 사다보니까",
          "original_form": "인형물품 살 때 중고품 많이 사다보니까",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043973.1.2",
          "form": "요즘도 간간이 두장에 21만원짜리 글은 올라오더라구",
          "original_form": "요즘도 간간이 두장에 21만원짜리 글은 올라오더라구",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043973.1.3",
          "form": "자주 키워드 알림하는듯",
          "original_form": "자주 키워드 알림하는듯",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043973.1.4",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043973.1.5",
          "form": "21망면 10만 오천원씩ㄴ가",
          "original_form": "21망면 10만 오천원씩ㄴ가",
          "speaker_id": "2",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043973.1.6",
          "form": "ㅇㅇ 배송비 포함!",
          "original_form": "ㅇㅇ 배송비 포함!",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043973.1.7",
          "form": "근데 이런건 진짜 순삭이라",
          "original_form": "근데 이런건 진짜 순삭이라",
          "speaker_id": "1",
          "time": "20210907 21:08"
        },
        {
          "id": "MDRW2100043973.1.8",
          "form": "계속 보고있거나 타이밍 좋게 알림 안뜨면 놓치나봐",
          "original_form": "계속 보고있거나 타이밍 좋게 알림 안뜨면 놓치나봐",
          "speaker_id": "1",
          "time": "20210907 21:09"
        },
        {
          "id": "MDRW2100043973.1.9",
          "form": "ㅜㅜ 그렇다고 맨날 폰만 볼 수도 없고",
          "original_form": "ㅜㅜ 그렇다고 맨날 폰만 볼 수도 없고",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043973.1.10",
          "form": "그니까....",
          "original_form": "그니까....",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043973.1.11",
          "form": "애초에 중고나라는 왜 키워드알림을",
          "original_form": "애초에 중고나라는 왜 키워드알림을",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043973.1.12",
          "form": "팝업으로 안띄워주는거야...",
          "original_form": "팝업으로 안띄워주는거야...",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100043973.1.13",
          "form": "ㅜㅜ 팝업으로 띄워줄 때도 있는데 뭐 겹치거나 하면 안띄워주더라고",
          "original_form": "ㅜㅜ 팝업으로 띄워줄 때도 있는데 뭐 겹치거나 하면 안띄워주더라고",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043973.1.14",
          "form": "나도 그래서 놓친거 은근 많당..",
          "original_form": "나도 그래서 놓친거 은근 많당..",
          "speaker_id": "2",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043973.1.15",
          "form": "ㅇㅎ 겹쳐서 안띄워줬나?",
          "original_form": "ㅇㅎ 겹쳐서 안띄워줬나?",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043973.1.16",
          "form": "나 키워드 설정한 이후로 팝업 딱 한번 받았어...",
          "original_form": "나 키워드 설정한 이후로 팝업 딱 한번 받았어...",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100043973.1.17",
          "form": "ㅠㅜㅜㅜ 은근 먹통이라니까....",
          "original_form": "ㅠㅜㅜㅜ 은근 먹통이라니까....",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100043973.1.18",
          "form": "몰랐다가 나중에 보고 우는 경우 많아..",
          "original_form": "몰랐다가 나중에 보고 우는 경우 많아..",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100043973.1.19",
          "form": "특히 나는 팝업 알람해둔게 많아서",
          "original_form": "특히 나는 팝업 알람해둔게 많아서",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100043973.1.20",
          "form": "그중 다른 키워드 낑겨 넣은건 잘 올라오는데 필요한 건 없는 알람만 주구장창 와가지고",
          "original_form": "그중 다른 키워드 낑겨 넣은건 잘 올라오는데 필요한 건 없는 알람만 주구장창 와가지고",
          "speaker_id": "2",
          "time": "20210907 21:19"
        },
        {
          "id": "MDRW2100043973.1.21",
          "form": "ㅇㅎ...",
          "original_form": "ㅇㅎ...",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043973.1.22",
          "form": "그치만 난",
          "original_form": "그치만 난",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043973.1.23",
          "form": "다른 필요한 알람 안오는 경우 은근 많더라",
          "original_form": "다른 필요한 알람 안오는 경우 은근 많더라",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043973.1.24",
          "form": "얘네밖에 알람설정 안해놧는데...ㅠ",
          "original_form": "얘네밖에 알람설정 안해놧는데...ㅠ",
          "speaker_id": "1",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043973.1.25",
          "form": "ㅋㅋ큐ㅠㅠㅠ",
          "original_form": "ㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 21:20"
        },
        {
          "id": "MDRW2100043973.1.26",
          "form": "일해라 카페 어플",
          "original_form": "일해라 카페 어플",
          "speaker_id": "2",
          "time": "20210907 21:20"
        }
      ]
    }
  ]
