      "utterance": [
        {
          "id": "MDRW2100024700.1.1",
          "form": "거의 밥값아니냐",
          "original_form": "거의 밥값아니냐",
          "speaker_id": "2",
          "time": "20210831 17:22"
        },
        {
          "id": "MDRW2100024700.1.2",
          "form": "행궁자체를 구경할만한 것도 잘없고",
          "original_form": "행궁자체를 구경할만한 것도 잘없고",
          "speaker_id": "2",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024700.1.3",
          "form": "진짜 비싸다 해외여행객들 상대로 팔아서 그런가",
          "original_form": "진짜 비싸다 해외여행객들 상대로 팔아서 그런가",
          "speaker_id": "1",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024700.1.4",
          "form": "코로나  끝나면 차라리 서울로 여행가지",
          "original_form": "코로나  끝나면 차라리 서울로 여행가지",
          "speaker_id": "2",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024700.1.5",
          "form": "수원이라는 메리트가 없더라",
          "original_form": "수원이라는 메리트가 없더라",
          "speaker_id": "2",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024700.1.6",
          "form": "ㅋㅋㅋ 우리 코로나 끝나면 서울투어 가자",
          "original_form": "ㅋㅋㅋ 우리 코로나 끝나면 서울투어 가자",
          "speaker_id": "1",
          "time": "20210831 17:23"
        },
        {
          "id": "MDRW2100024700.1.7",
          "form": "그러자 먹방조져",
          "original_form": "그러자 먹방조져",
          "speaker_id": "2",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024700.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ좋지!",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ좋지!",
          "speaker_id": "1",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024700.1.9",
          "form": "서울에 골동품시장 가보고싶어",
          "original_form": "서울에 골동품시장 가보고싶어",
          "speaker_id": "2",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024700.1.10",
          "form": "나도 가보고싶어 그건 시간여행 하는 기분일 것 같아",
          "original_form": "나도 가보고싶어 그건 시간여행 하는 기분일 것 같아",
          "speaker_id": "1",
          "time": "20210831 17:24"
        },
        {
          "id": "MDRW2100024700.1.11",
          "form": "동묘같은곳에 가면",
          "original_form": "동묘같은곳에 가면",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024700.1.12",
          "form": "보물찾는 느낌",
          "original_form": "보물찾는 느낌",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024700.1.13",
          "form": "구제옷여행도 가보자ㅋㅋㅋㅋㅋ",
          "original_form": "구제옷여행도 가보자ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024700.1.14",
          "form": "저번에 하말넘많 동묘가서",
          "original_form": "저번에 하말넘많 동묘가서",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024700.1.15",
          "form": "쇼핑하던데",
          "original_form": "쇼핑하던데",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024700.1.16",
          "form": "너무 웃기더라",
          "original_form": "너무 웃기더라",
          "speaker_id": "2",
          "time": "20210831 17:25"
        },
        {
          "id": "MDRW2100024700.1.17",
          "form": "엇 하말넘많이 찍은 건 못 봤는데 봐야겠는걸",
          "original_form": "엇 하말넘많이 찍은 건 못 봤는데 봐야겠는걸",
          "speaker_id": "1",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100024700.1.18",
          "form": "난 소그노편 봤었어",
          "original_form": "난 소그노편 봤었어",
          "speaker_id": "1",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100024700.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋ잡템 많더라",
          "original_form": "ㅋㅋㅋㅋㅋㅋ잡템 많더라",
          "speaker_id": "1",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100024700.1.20",
          "form": "넌 어떤거 찾고싶어?",
          "original_form": "넌 어떤거 찾고싶어?",
          "speaker_id": "2",
          "time": "20210831 17:26"
        },
        {
          "id": "MDRW2100024700.1.21",
          "form": "난 자켓종류 찾아보고싶어",
          "original_form": "난 자켓종류 찾아보고싶어",
          "speaker_id": "1",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024700.1.22",
          "form": "그 벨트 동묘가면",
          "original_form": "그 벨트 동묘가면",
          "speaker_id": "2",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024700.1.23",
          "form": "3천원이다",
          "original_form": "3천원이다",
          "speaker_id": "2",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024700.1.24",
          "form": "알지",
          "original_form": "알지",
          "speaker_id": "2",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024700.1.25",
          "form": "아! 벨트도 좋네",
          "original_form": "아! 벨트도 좋네",
          "speaker_id": "1",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024700.1.26",
          "form": "여행지에서 악세사리 살 때",
          "original_form": "여행지에서 악세사리 살 때",
          "speaker_id": "1",
          "time": "20210831 17:27"
        },
        {
          "id": "MDRW2100024700.1.27",
          "form": "벨트나 사올 걸",
          "original_form": "벨트나 사올 걸",
          "speaker_id": "1",
          "time": "20210831 17:28"
        },
        {
          "id": "MDRW2100024700.1.28",
          "form": "난 보석함을",
          "original_form": "난 보석함을",
          "speaker_id": "2",
          "time": "20210831 17:28"
        },
        {
          "id": "MDRW2100024700.1.29",
          "form": "찾아보려고",
          "original_form": "찾아보려고",
          "speaker_id": "2",
          "time": "20210831 17:28"
        },
        {
          "id": "MDRW2100024700.1.30",
          "form": "컵이나",
          "original_form": "컵이나",
          "speaker_id": "2",
          "time": "20210831 17:28"
        },
        {
          "id": "MDRW2100024700.1.31",
          "form": "보관하고싶은 짱돌이 있니?",
          "original_form": "보관하고싶은 짱돌이 있니?",
          "speaker_id": "1",
          "time": "20210831 17:28"
        },
        {
          "id": "MDRW2100024700.1.32",
          "form": "좋은거 건져서",
          "original_form": "좋은거 건져서",
          "speaker_id": "2",
          "time": "20210831 17:28"
        },
        {
          "id": "MDRW2100024700.1.33",
          "form": "다시 되팔면 돈좀되더라",
          "original_form": "다시 되팔면 돈좀되더라",
          "speaker_id": "2",
          "time": "20210831 17:28"
        }
      ]
    }
  ]
