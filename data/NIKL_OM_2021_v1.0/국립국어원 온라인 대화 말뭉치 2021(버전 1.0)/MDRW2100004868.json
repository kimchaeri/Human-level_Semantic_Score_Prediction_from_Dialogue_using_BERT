      "utterance": [
        {
          "id": "MDRW2100004868.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210821 18:55"
        },
        {
          "id": "MDRW2100004868.1.2",
          "form": "가족끼리 나눠서 해요",
          "original_form": "가족끼리 나눠서 해요",
          "speaker_id": "2",
          "time": "20210821 18:56"
        },
        {
          "id": "MDRW2100004868.1.3",
          "form": "저는 주로 빨래랑 요리해요",
          "original_form": "저는 주로 빨래랑 요리해요",
          "speaker_id": "2",
          "time": "20210821 18:56"
        },
        {
          "id": "MDRW2100004868.1.4",
          "form": "저는 설거지",
          "original_form": "저는 설거지",
          "speaker_id": "1",
          "time": "20210821 18:57"
        },
        {
          "id": "MDRW2100004868.1.5",
          "form": "설거지는 해도해도 끝이 없죠",
          "original_form": "설거지는 해도해도 끝이 없죠",
          "speaker_id": "2",
          "time": "20210821 19:00"
        },
        {
          "id": "MDRW2100004868.1.6",
          "form": "장은 어떻게 보시나요?",
          "original_form": "장은 어떻게 보시나요?",
          "speaker_id": "2",
          "time": "20210821 19:00"
        },
        {
          "id": "MDRW2100004868.1.7",
          "form": "주말에 한꺼번에",
          "original_form": "주말에 한꺼번에",
          "speaker_id": "1",
          "time": "20210821 19:15"
        },
        {
          "id": "MDRW2100004868.1.8",
          "form": "어디로 사러 가나요??",
          "original_form": "어디로 사러 가나요??",
          "speaker_id": "2",
          "time": "20210821 19:17"
        },
        {
          "id": "MDRW2100004868.1.9",
          "form": "마트죠 ㄹㅎ",
          "original_form": "마트죠 ㄹㅎ",
          "speaker_id": "1",
          "time": "20210821 19:20"
        },
        {
          "id": "MDRW2100004868.1.10",
          "form": "요리는 주로 누가 하나요?",
          "original_form": "요리는 주로 누가 하나요?",
          "speaker_id": "2",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004868.1.11",
          "form": "와이프요 ㅎㅎ",
          "original_form": "와이프요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004868.1.12",
          "form": "아 그래서 설거지 주로 하시는 군요",
          "original_form": "아 그래서 설거지 주로 하시는 군요",
          "speaker_id": "2",
          "time": "20210821 19:26"
        },
        {
          "id": "MDRW2100004868.1.13",
          "form": "아이도 있나요??",
          "original_form": "아이도 있나요??",
          "speaker_id": "2",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004868.1.14",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210821 21:15"
        },
        {
          "id": "MDRW2100004868.1.15",
          "form": "코로나 때문에 육아도 쉽지 않겠어요",
          "original_form": "코로나 때문에 육아도 쉽지 않겠어요",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004868.1.16",
          "form": "자녀가 학교를 다니나요?",
          "original_form": "자녀가 학교를 다니나요?",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004868.1.17",
          "form": "네 초딩이요",
          "original_form": "네 초딩이요",
          "speaker_id": "1",
          "time": "20210821 21:18"
        }
