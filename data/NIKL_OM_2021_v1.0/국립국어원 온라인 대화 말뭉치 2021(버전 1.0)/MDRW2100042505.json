          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042505.1.5",
          "form": "또 서로 남탓하깅",
          "original_form": "또 서로 남탓하깅",
          "speaker_id": "2",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042505.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:30"
        },
        {
          "id": "MDRW2100042505.1.7",
          "form": "시트콤같다",
          "original_form": "시트콤같다",
          "speaker_id": "1",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042505.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:31"
        },
        {
          "id": "MDRW2100042505.1.9",
          "form": "우도도 갈까?",
          "original_form": "우도도 갈까?",
          "speaker_id": "2",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042505.1.10",
          "form": "오 우도",
          "original_form": "오 우도",
          "speaker_id": "1",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042505.1.11",
          "form": "초딩때 패키지여행가면서 가봤엇지",
          "original_form": "초딩때 패키지여행가면서 가봤엇지",
          "speaker_id": "1",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042505.1.12",
          "form": "근데 하나도 기억안남",
          "original_form": "근데 하나도 기억안남",
          "speaker_id": "1",
          "time": "20210906 22:32"
        },
        {
          "id": "MDRW2100042505.1.13",
          "form": "우도 바다가 진짜 이쁜것 같음",
          "original_form": "우도 바다가 진짜 이쁜것 같음",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042505.1.14",
          "form": "한 2주 잡고",
          "original_form": "한 2주 잡고",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042505.1.15",
          "form": "서울 제주도 여행하려면 힘드려나",
          "original_form": "서울 제주도 여행하려면 힘드려나",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042505.1.16",
          "form": "와 2주?",
          "original_form": "와 2주?",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042505.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042505.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042505.1.19",
          "form": "너무했낰ㅋㅋㅋㅋㅋ",
          "original_form": "너무했낰ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:33"
        },
        {
          "id": "MDRW2100042505.1.20",
          "form": "힘들거같은데?ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "힘들거같은데?ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042505.1.21",
          "form": "일단 열심히 체력을 길러볼게",
          "original_form": "일단 열심히 체력을 길러볼게",
          "speaker_id": "1",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042505.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042505.1.23",
          "form": "그래 2주는 엄마들이",
          "original_form": "그래 2주는 엄마들이",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042505.1.24",
          "form": "우리 보고싶어할듯ㅋㅋㅋㅋ",
          "original_form": "우리 보고싶어할듯ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042505.1.25",
          "form": "그럼 1주 여행은 괜찮나??",
          "original_form": "그럼 1주 여행은 괜찮나??",
          "speaker_id": "2",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042505.1.26",
          "form": "아아아 맞네 엄마 생각을못했네",
          "original_form": "아아아 맞네 엄마 생각을못했네",
          "speaker_id": "1",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042505.1.27",
          "form": "엄마가 2주는 허락안할지도모르겟다",
          "original_form": "엄마가 2주는 허락안할지도모르겟다",
          "speaker_id": "1",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042505.1.28",
          "form": "일주일정도는 괜찮지않을까?",
          "original_form": "일주일정도는 괜찮지않을까?",
          "speaker_id": "1",
          "time": "20210906 22:34"
        },
        {
          "id": "MDRW2100042505.1.29",
          "form": "그러면 1주일 제주도 여행하고",
          "original_form": "그러면 1주일 제주도 여행하고",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042505.1.30",
          "form": "좀 공부도 하고 쉬다가",
          "original_form": "좀 공부도 하고 쉬다가",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042505.1.31",
          "form": "서울도 가자",
          "original_form": "서울도 가자",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042505.1.32",
          "form": "오 좋아좋아",
          "original_form": "오 좋아좋아",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042505.1.33",
          "form": "서울에 전시회같은거 많아서",
          "original_form": "서울에 전시회같은거 많아서",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042505.1.34",
          "form": "놀러가고 싶음",
          "original_form": "놀러가고 싶음",
          "speaker_id": "2",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042505.1.35",
          "form": "서울가면 진짜",
          "original_form": "서울가면 진짜",
          "speaker_id": "1",
          "time": "20210906 22:35"
        },
        {
          "id": "MDRW2100042505.1.36",
          "form": "유명한 맛집 다 털고오자",
          "original_form": "유명한 맛집 다 털고오자",
          "speaker_id": "1",
          "time": "20210906 22:35"
        }
      ]
