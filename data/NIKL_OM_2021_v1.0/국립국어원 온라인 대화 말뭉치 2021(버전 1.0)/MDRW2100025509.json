      "utterance": [
        {
          "id": "MDRW2100025509.1.1",
          "form": "이러다 맛집 추천 어플되겄네",
          "original_form": "이러다 맛집 추천 어플되겄네",
          "speaker_id": "2",
          "time": "20210901 12:40"
        },
        {
          "id": "MDRW2100025509.1.2",
          "form": "그럼좋지",
          "original_form": "그럼좋지",
          "speaker_id": "1",
          "time": "20210901 12:40"
        },
        {
          "id": "MDRW2100025509.1.3",
          "form": "효뜨 가면 후기좀 찜쪄주셈",
          "original_form": "효뜨 가면 후기좀 찜쪄주셈",
          "speaker_id": "2",
          "time": "20210901 12:40"
        },
        {
          "id": "MDRW2100025509.1.4",
          "form": "남박효뜨",
          "original_form": "남박효뜨",
          "speaker_id": "1",
          "time": "20210901 12:40"
        },
        {
          "id": "MDRW2100025509.1.5",
          "form": "당신의선택",
          "original_form": "당신의선택",
          "speaker_id": "1",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100025509.1.6",
          "form": "전 남박이요",
          "original_form": "전 남박이요",
          "speaker_id": "2",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100025509.1.7",
          "form": "생고기 올라간게 조크든요",
          "original_form": "생고기 올라간게 조크든요",
          "speaker_id": "2",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100025509.1.8",
          "form": "남박은 먼데...",
          "original_form": "남박은 먼데...",
          "speaker_id": "1",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100025509.1.9",
          "form": "ㅋㅋㄱㅋㅋㅋㅋㅋ 생고기 맞아",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋㅋ 생고기 맞아",
          "speaker_id": "1",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100025509.1.10",
          "form": "비주얼이 넘맛있어보여 거기",
          "original_form": "비주얼이 넘맛있어보여 거기",
          "speaker_id": "1",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100025509.1.11",
          "form": "근데오늘 열었으려나",
          "original_form": "근데오늘 열었으려나",
          "speaker_id": "1",
          "time": "20210901 12:41"
        },
        {
          "id": "MDRW2100025509.1.12",
          "form": "비와서 난 엄마랑 생선구이집갔음",
          "original_form": "비와서 난 엄마랑 생선구이집갔음",
          "speaker_id": "2",
          "time": "20210901 12:42"
        },
        {
          "id": "MDRW2100025509.1.13",
          "form": "오늘 용리단길 맛집가보려고",
          "original_form": "오늘 용리단길 맛집가보려고",
          "speaker_id": "1",
          "time": "20210901 12:42"
        },
        {
          "id": "MDRW2100025509.1.14",
          "form": "시상에",
          "original_form": "시상에",
          "speaker_id": "1",
          "time": "20210901 12:42"
        },
        {
          "id": "MDRW2100025509.1.15",
          "form": "생선구이집 부럽쓰",
          "original_form": "생선구이집 부럽쓰",
          "speaker_id": "1",
          "time": "20210901 12:42"
        },
        {
          "id": "MDRW2100025509.1.16",
          "form": "밀가루 안들어간거랑 기름진거 제외하니까 남은게 생선구이집",
          "original_form": "밀가루 안들어간거랑 기름진거 제외하니까 남은게 생선구이집",
          "speaker_id": "2",
          "time": "20210901 12:42"
        },
        {
          "id": "MDRW2100025509.1.17",
          "form": "뿐이었스",
          "original_form": "뿐이었스",
          "speaker_id": "2",
          "time": "20210901 12:42"
        },
        {
          "id": "MDRW2100025509.1.18",
