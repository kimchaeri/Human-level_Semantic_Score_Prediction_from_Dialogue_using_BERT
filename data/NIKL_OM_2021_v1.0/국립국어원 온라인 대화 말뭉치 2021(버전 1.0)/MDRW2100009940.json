      "utterance": [
        {
          "id": "MDRW2100009940.1.1",
          "form": "곰표맥주드셔보셨어요?",
          "original_form": "곰표맥주드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210823 11:59"
        },
        {
          "id": "MDRW2100009940.1.2",
          "form": "네 맛있더라구요ㅎㅎ곰표 팝콘도 맛있구",
          "original_form": "네 맛있더라구요ㅎㅎ곰표 팝콘도 맛있구",
          "speaker_id": "1",
          "time": "20210823 14:18"
        },
        {
          "id": "MDRW2100009940.1.3",
          "form": "팝콘이 맛있더라구요",
          "original_form": "팝콘이 맛있더라구요",
          "speaker_id": "2",
          "time": "20210823 14:54"
        },
        {
          "id": "MDRW2100009940.1.4",
          "form": "집에 쟁여놓고 먹고싶어요",
          "original_form": "집에 쟁여놓고 먹고싶어요",
          "speaker_id": "2",
          "time": "20210823 14:54"
        },
        {
          "id": "MDRW2100009940.1.5",
          "form": "곰도 귀엽구",
          "original_form": "곰도 귀엽구",
          "speaker_id": "2",
          "time": "20210823 14:54"
        },
        {
          "id": "MDRW2100009940.1.6",
          "form": "맞아요ㅎㅎ은근 크기도 크고ㅎㅎ",
          "original_form": "맞아요ㅎㅎ은근 크기도 크고ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 14:54"
        },
        {
          "id": "MDRW2100009940.1.7",
          "form": "곰표 콜라보는 먹는거라서 위화감이없어서 좋아요",
          "original_form": "곰표 콜라보는 먹는거라서 위화감이없어서 좋아요",
          "speaker_id": "2",
          "time": "20210823 15:12"
        },
        {
          "id": "MDRW2100009940.1.8",
          "form": "딱풀콜라보 음료는 먹기싫어요ㅜ",
          "original_form": "딱풀콜라보 음료는 먹기싫어요ㅜ",
          "speaker_id": "2",
          "time": "20210823 15:12"
        },
        {
          "id": "MDRW2100009940.1.9",
          "form": "그런건 안사게되여ㅠㅠ너무 선을 넘은것같구",
          "original_form": "그런건 안사게되여ㅠㅠ너무 선을 넘은것같구",
          "speaker_id": "1",
          "time": "20210823 15:13"
        },
        {
          "id": "MDRW2100009940.1.10",
          "form": "애들도 착각하면 위험하구",
          "original_form": "애들도 착각하면 위험하구",
          "speaker_id": "2",
          "time": "20210823 15:13"
        },
        {
          "id": "MDRW2100009940.1.11",
          "form": "생각이 없는거같아요",
          "original_form": "생각이 없는거같아요",
          "speaker_id": "2",
          "time": "20210823 15:14"
        },
        {
          "id": "MDRW2100009940.1.12",
          "form": "맞아요 어는순간 너무 질린다 싶구",
          "original_form": "맞아요 어는순간 너무 질린다 싶구",
          "speaker_id": "1",
          "time": "20210823 15:14"
        },
        {
          "id": "MDRW2100009940.1.13",
          "form": "진로는 캐릭터가 귀여워요 인형콜라보도",
          "original_form": "진로는 캐릭터가 귀여워요 인형콜라보도",
          "speaker_id": "2",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100009940.1.14",
          "form": "나왔으면좋겠어요",
          "original_form": "나왔으면좋겠어요",
          "speaker_id": "2",
          "time": "20210823 15:19"
        },
        {
          "id": "MDRW2100009940.1.15",
          "form": "오 맞아요ㅋㅋ사이즈 다양하게ㅎㅎ",
          "original_form": "오 맞아요ㅋㅋ사이즈 다양하게ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 15:20"
        },
        {
          "id": "MDRW2100009940.1.16",
          "form": "그런콜라보는 좋아요",
          "original_form": "그런콜라보는 좋아요",
          "speaker_id": "2",
          "time": "20210823 15:20"
        },
        {
          "id": "MDRW2100009940.1.17",
          "form": "색도예뻐요",
          "original_form": "색도예뻐요",
          "speaker_id": "2",
          "time": "20210823 15:20"
        },
        {
          "id": "MDRW2100009940.1.18",
          "form": "맞아요 소장욕구고 생기궁",
          "original_form": "맞아요 소장욕구고 생기궁",
          "speaker_id": "1",
          "time": "20210823 15:20"
        },
        {
          "id": "MDRW2100009940.1.19",
          "form": "곰표팝콘먹고싶네요",
          "original_form": "곰표팝콘먹고싶네요",
          "speaker_id": "2",
          "time": "20210823 15:21"
        },
        {
          "id": "MDRW2100009940.1.20",
          "form": "저도 또 사와야겠어요ㅎㅎ",
          "original_form": "저도 또 사와야겠어요ㅎㅎ",
