      "utterance": [
        {
          "id": "MDRW2100042964.1.1",
          "form": "베라좋아하세요?",
          "original_form": "베라좋아하세요?",
          "speaker_id": "1",
          "time": "20210907 12:07"
        },
        {
          "id": "MDRW2100042964.1.2",
          "form": "저 그것때문에 심심이 시작했어요",
          "original_form": "저 그것때문에 심심이 시작했어요",
          "speaker_id": "2",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100042964.1.3",
          "form": "ㅋㅋㅋ 그래놓고 하면서 사먹고 있습니다",
          "original_form": "ㅋㅋㅋ 그래놓고 하면서 사먹고 있습니다",
          "speaker_id": "2",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100042964.1.4",
          "form": "ㅎㅎㅎ어떤맛좋아하세요",
          "original_form": "ㅎㅎㅎ어떤맛좋아하세요",
          "speaker_id": "1",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100042964.1.5",
          "form": "확실히 꽁으로 먹는 돈은 없네여ㅋㅋㅋㅋ",
          "original_form": "확실히 꽁으로 먹는 돈은 없네여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:08"
        },
        {
          "id": "MDRW2100042964.1.6",
          "form": "바람과함께 사라지다 좋아해요",
          "original_form": "바람과함께 사라지다 좋아해요",
          "speaker_id": "2",
          "time": "20210907 12:09"
        },
        {
          "id": "MDRW2100042964.1.7",
          "form": "name1님은?",
          "original_form": "&name1&님은?",
          "speaker_id": "2",
          "time": "20210907 12:09"
        },
        {
          "id": "MDRW2100042964.1.8",
          "form": "ㅎㅎ그죠",
          "original_form": "ㅎㅎ그죠",
          "speaker_id": "1",
          "time": "20210907 12:10"
        },
        {
          "id": "MDRW2100042964.1.9",
          "form": "저는민초요ㅎㅎ",
          "original_form": "저는민초요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 12:10"
        },
        {
          "id": "MDRW2100042964.1.10",
          "form": "저 민트초코파이 나온거 먹어봤었는데 맛있더라고요",
          "original_form": "저 민트초코파이 나온거 먹어봤었는데 맛있더라고요",
          "speaker_id": "2",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100042964.1.11",
          "form": "우왕 궁금하네요",
          "original_form": "우왕 궁금하네요",
          "speaker_id": "1",
          "time": "20210907 12:13"
        },
        {
          "id": "MDRW2100042964.1.12",
          "form": "맛집자주가세요!",
          "original_form": "맛집자주가세요!",
          "speaker_id": "1",
          "time": "20210907 12:13"
        },
        {
          "id": "MDRW2100042964.1.13",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210907 12:13"
        },
        {
          "id": "MDRW2100042964.1.14",
          "form": "적당한 민트맛!",
          "original_form": "적당한 민트맛!",
          "speaker_id": "2",
          "time": "20210907 12:14"
        },
        {
          "id": "MDRW2100042964.1.15",
          "form": "요즘엔 잘 못가는데 가는거 좋아했어요",
          "original_form": "요즘엔 잘 못가는데 가는거 좋아했어요",
          "speaker_id": "2",
          "time": "20210907 12:14"
        },
        {
          "id": "MDRW2100042964.1.16",
          "form": "맛있는 곳 아세여? 추천좀ㅋㅋㅋㅌ",
          "original_form": "맛있는 곳 아세여? 추천좀ㅋㅋㅋㅌ",
          "speaker_id": "2",
          "time": "20210907 12:14"
        },
        {
          "id": "MDRW2100042964.1.17",
          "form": "음지역을모르니",
          "original_form": "음지역을모르니",
          "speaker_id": "1",
          "time": "20210907 12:16"
        },
        {
          "id": "MDRW2100042964.1.18",
          "form": "프랜차이즈로ㅎㅎ",
          "original_form": "프랜차이즈로ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 12:16"
        },
        {
          "id": "MDRW2100042964.1.19",
          "form": "청년다방차돌떡볶이맛있어요",
          "original_form": "청년다방차돌떡볶이맛있어요",
          "speaker_id": "1",
          "time": "20210907 12:17"
        },
        {
          "id": "MDRW2100042964.1.20",
          "form": "전 인천이긴 한데 나중에 여행처럼 갈수도 있으니까 어디든지 좋아요 ㅋㅋㅋ",
          "original_form": "전 인천이긴 한데 나중에 여행처럼 갈수도 있으니까 어디든지 좋아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:17"
        },
        {
          "id": "MDRW2100042964.1.21",
          "form": "청년다방에서 오징어튀김은 먹었는데 차돌이군여",
          "original_form": "청년다방에서 오징어튀김은 먹었는데 차돌이군여",
          "speaker_id": "2",
          "time": "20210907 12:17"
        },
        {
          "id": "MDRW2100042964.1.22",
          "form": "ㅎㅎ아그렇네용ㅎㅎ",
          "original_form": "ㅎㅎ아그렇네용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 12:18"
        },
        {
          "id": "MDRW2100042964.1.23",
          "form": "오짱도맛있어요",
          "original_form": "오짱도맛있어요",
          "speaker_id": "1",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042964.1.24",
          "form": "생각해보니 차돌 먹는분이 많았던거 같아요 ㅋㅋㅋㅋ",
          "original_form": "생각해보니 차돌 먹는분이 많았던거 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042964.1.25",
          "form": "인천의 맛집은 냉면집인듯 합니다",
          "original_form": "인천의 맛집은 냉면집인듯 합니다",
          "speaker_id": "2",
          "time": "20210907 12:19"
        },
        {
          "id": "MDRW2100042964.1.26",
          "form": "저도 잘 몰랐는데 냉면집이 은근 많아요",
          "original_form": "저도 잘 몰랐는데 냉면집이 은근 많아요",
          "speaker_id": "2",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042964.1.27",
          "form": "사곶냉면 맛있더라고요",
          "original_form": "사곶냉면 맛있더라고요",
          "speaker_id": "2",
          "time": "20210907 12:20"
        },
        {
          "id": "MDRW2100042964.1.28",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210907 12:22"
        },
        {
          "id": "MDRW2100042964.1.29",
          "form": "담에가게되면먹어봐야겠어요",
          "original_form": "담에가게되면먹어봐야겠어요",
