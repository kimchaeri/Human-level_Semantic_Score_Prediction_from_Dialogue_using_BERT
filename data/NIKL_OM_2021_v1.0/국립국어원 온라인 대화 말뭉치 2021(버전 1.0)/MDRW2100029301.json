      "utterance": [
        {
          "id": "MDRW2100029301.1.1",
          "form": "저는 새벽이라 야식땡겨요",
          "original_form": "저는 새벽이라 야식땡겨요",
          "speaker_id": "1",
          "time": "20210903 01:12"
        },
        {
          "id": "MDRW2100029301.1.2",
          "form": "저두요 ㅜㅡ",
          "original_form": "저두요 ㅜㅡ",
          "speaker_id": "2",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029301.1.3",
          "form": "특히 라면",
          "original_form": "특히 라면",
          "speaker_id": "1",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029301.1.4",
          "form": "치킨!!!",
          "original_form": "치킨!!!",
          "speaker_id": "1",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029301.1.5",
          "form": "헐 ㅜㅡ 치킨은 언제나 옳죠 ㅎ!!",
          "original_form": "헐 ㅜㅡ 치킨은 언제나 옳죠 ㅎ!!",
          "speaker_id": "2",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029301.1.6",
          "form": "뿌링클 좋아하시나요",
          "original_form": "뿌링클 좋아하시나요",
          "speaker_id": "1",
          "time": "20210903 01:13"
        },
        {
          "id": "MDRW2100029301.1.7",
          "form": "저는 일주일에 2번은 치킨 시켜 먹는듯요",
          "original_form": "저는 일주일에 2번은 치킨 시켜 먹는듯요",
          "speaker_id": "2",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029301.1.8",
          "form": "뿌링클은 아직 못먹어봤어요",
          "original_form": "뿌링클은 아직 못먹어봤어요",
          "speaker_id": "2",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029301.1.9",
          "form": "뿌링클 맛있나요?",
          "original_form": "뿌링클 맛있나요?",
          "speaker_id": "2",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029301.1.10",
          "form": "네네! 단짠단짠 조화가 미쳐요",
          "original_form": "네네! 단짠단짠 조화가 미쳐요",
          "speaker_id": "1",
          "time": "20210903 01:14"
        },
        {
          "id": "MDRW2100029301.1.11",
          "form": "ㅎ 꼭 한번 시켜먹어봐야겠네요 ㅎㅎㅎ",
          "original_form": "ㅎ 꼭 한번 시켜먹어봐야겠네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029301.1.12",
          "form": "저는 보통 한군데 맛있으면 ㅎ 거기서만 시켜서오",
          "original_form": "저는 보통 한군데 맛있으면 ㅎ 거기서만 시켜서오",
          "speaker_id": "2",
          "time": "20210903 01:15"
        },
        {
          "id": "MDRW2100029301.1.13",
          "form": "어디에서 주로드세요?",
          "original_form": "어디에서 주로드세요?",
          "speaker_id": "1",
          "time": "20210903 01:16"
        },
        {
          "id": "MDRW2100029301.1.14",
          "form": "굽네 시키다가 60계 시키다 지금은 바른치킨이요 ㅎㅎㅎ",
          "original_form": "굽네 시키다가 60계 시키다 지금은 바른치킨이요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:17"
        },
        {
          "id": "MDRW2100029301.1.15",
          "form": "후라이드를 주로 먹어서 ㅎㅎㅎㅎ",
          "original_form": "후라이드를 주로 먹어서 ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:17"
        },
        {
          "id": "MDRW2100029301.1.16",
          "form": "바른치킨은 아직안먹어봤는데!",
          "original_form": "바른치킨은 아직안먹어봤는데!",
          "speaker_id": "1",
          "time": "20210903 01:17"
        },
        {
          "id": "MDRW2100029301.1.17",
          "form": "바른치킨 괜찬아요 ㅎ 현미바사삭이 기본인데 바삭함 ㅎ",
          "original_form": "바른치킨 괜찬아요 ㅎ 현미바사삭이 기본인데 바삭함 ㅎ",
          "speaker_id": "2",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029301.1.18",
          "form": "뿌링클은 어디꺼죠?",
          "original_form": "뿌링클은 어디꺼죠?",
          "speaker_id": "2",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029301.1.19",
          "form": "오오오 꿀팁이네요",
          "original_form": "오오오 꿀팁이네요",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029301.1.20",
          "form": "bhc에용",
          "original_form": "bhc에용",
          "speaker_id": "1",
          "time": "20210903 01:18"
        },
        {
          "id": "MDRW2100029301.1.21",
          "form": "ㅎㅎㅎ 저는 대부분 맛이 바꺼서 다른데루 옮기는데 다음엔 bhc도 먹어봐야겠네요",
          "original_form": "ㅎㅎㅎ 저는 대부분 맛이 바꺼서 다른데루 옮기는데 다음엔 bhc도 먹어봐야겠네요",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029301.1.22",
          "form": "그러구보니 bhc는 한번도 안먹어봤네요",
          "original_form": "그러구보니 bhc는 한번도 안먹어봤네요",
          "speaker_id": "2",
          "time": "20210903 01:19"
        },
        {
          "id": "MDRW2100029301.1.23",
          "form": "집근처에 매장이 있어서ㅋㅋㅋ",
          "original_form": "집근처에 매장이 있어서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029301.1.24",
          "form": "뿌링클은 소스덕에 진짜맛있어요",
          "original_form": "뿌링클은 소스덕에 진짜맛있어요",
          "speaker_id": "1",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029301.1.25",
          "form": "ㅎㅎㅎ 이번주말엔 뿌링클 한번 먹어봐야겠네요 ㅎㅎㅎ",
          "original_form": "ㅎㅎㅎ 이번주말엔 뿌링클 한번 먹어봐야겠네요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 01:20"
        },
        {
          "id": "MDRW2100029301.1.26",
          "form": "치킨말곤 또 뭐뭐 좋아하세요?",
          "original_form": "치킨말곤 또 뭐뭐 좋아하세요?",
          "speaker_id": "2",
          "time": "20210903 01:21"
        },
        {
          "id": "MDRW2100029301.1.27",
