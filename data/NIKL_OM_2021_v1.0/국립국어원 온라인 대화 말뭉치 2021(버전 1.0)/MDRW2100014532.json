      "utterance": [
        {
          "id": "MDRW2100014532.1.1",
          "form": "싸이해보셨나요?",
          "original_form": "싸이해보셨나요?",
          "speaker_id": "1",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100014532.1.2",
          "form": "싸이 도토리질 좀 했지",
          "original_form": "싸이 도토리질 좀 했지",
          "speaker_id": "2",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100014532.1.3",
          "form": "그 시간에 공부를 하시지.. ㅋㅋㅋㅋ",
          "original_form": "그 시간에 공부를 하시지.. ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014532.1.4",
          "form": "싸이월드가 부활하나봐?",
          "original_form": "싸이월드가 부활하나봐?",
          "speaker_id": "1",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014532.1.5",
          "form": "아니 나 공부 못하진않았다곸ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니 나 공부 못하진않았다곸ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100014532.1.6",
          "form": "싸이 부활소식 들었움 !",
          "original_form": "싸이 부활소식 들었움 !",
          "speaker_id": "2",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100014532.1.7",
          "form": "근데 예전만큼 싸이가 다시 활성화될수있을깡",
          "original_form": "근데 예전만큼 싸이가 다시 활성화될수있을깡",
          "speaker_id": "2",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014532.1.8",
          "form": "글쿠나... 근데 너무 옛날꺼라.. 누가 할려나..",
          "original_form": "글쿠나... 근데 너무 옛날꺼라.. 누가 할려나..",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014532.1.9",
          "form": "일촌 많았어?",
          "original_form": "일촌 많았어?",
          "speaker_id": "1",
          "time": "20210825 19:42"
        },
        {
          "id": "MDRW2100014532.1.10",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014532.1.11",
          "form": "일촌많은애들 좀 부러워했던거같음ㅎㅎ...",
          "original_form": "일촌많은애들 좀 부러워했던거같음ㅎㅎ...",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014532.1.12",
          "form": "넌 일촌 많았어?",
          "original_form": "넌 일촌 많았어?",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014532.1.13",
          "form": "너무 옛날이라... 한 100명있었나?",
          "original_form": "너무 옛날이라... 한 100명있었나?",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014532.1.14",
          "form": "난..아싸라.. \r\n인싸한테 친구비 상납하면 일촌해주고그랬어..ㅋㅋㅋㅋ",
          "original_form": "난..아싸라.. \r\n인싸한테 친구비 상납하면 일촌해주고그랬어..ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014532.1.15",
          "form": "친구비 상납이라니 너무 무서운데ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "친구비 상납이라니 너무 무서운데ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:52"
        },
        {
          "id": "MDRW2100014532.1.16",
          "form": "싸이 부활하면 그때 그 일촌들도 그대로 남아있으려나",
          "original_form": "싸이 부활하면 그때 그 일촌들도 그대로 남아있으려나",
          "speaker_id": "2",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014532.1.17",
          "form": "ㅋㅋㅋ 어.. 그러게.. \r\n궁금하긴하다..",
          "original_form": "ㅋㅋㅋ 어.. 그러게.. \r\n궁금하긴하다..",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014532.1.18",
          "form": "근데 부끄러울듯..",
          "original_form": "근데 부끄러울듯..",
          "speaker_id": "1",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014532.1.19",
          "form": "넌 견딜 수 있어? 그때 그 사진.. 코멘트.. ㅋㅋ",
          "original_form": "넌 견딜 수 있어? 그때 그 사진.. 코멘트.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014532.1.20",
          "form": "나두..... 만약 부활하면 계정삭제부터 할 사람 많을거야....",
          "original_form": "나두..... 만약 부활하면 계정삭제부터 할 사람 많을거야....",
          "speaker_id": "2",
          "time": "20210825 19:54"
        },
        {
          "id": "MDRW2100014532.1.21",
          "form": "근데 아이디 자체가 기억이 안나..",
          "original_form": "근데 아이디 자체가 기억이 안나..",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014532.1.22",
          "form": "전번도 10번은 바꿨고..ㅋㅋ",
          "original_form": "전번도 10번은 바꿨고..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014532.1.23",
          "form": "나도 하나도 기억안남ㅎㅎ",
          "original_form": "나도 하나도 기억안남ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014532.1.24",
          "form": "다른사람도 다 그때의 나를 기억못해줬으면 좋겠다 흑흑",
          "original_form": "다른사람도 다 그때의 나를 기억못해줬으면 좋겠다 흑흑",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014532.1.25",
          "form": "근데 난 한결같아.. 미남형이야.. ㅋㅋㅋ",
          "original_form": "근데 난 한결같아.. 미남형이야.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014532.1.26",
          "form": "흐음",
          "original_form": "흐음",
          "speaker_id": "2",
