          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042832.1.3",
          "form": "아냐 3900원인가",
          "original_form": "아냐 3900원인가",
          "speaker_id": "2",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042832.1.4",
          "form": "비싸네",
          "original_form": "비싸네",
          "speaker_id": "1",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042832.1.5",
          "form": "아니네 3500이네",
          "original_form": "아니네 3500이네",
          "speaker_id": "2",
          "time": "20210907 09:46"
        },
        {
          "id": "MDRW2100042832.1.6",
          "form": "먹어야지",
          "original_form": "먹어야지",
          "speaker_id": "1",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042832.1.7",
          "form": "낼먹을까",
          "original_form": "낼먹을까",
          "speaker_id": "1",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042832.1.8",
          "form": "ㅇㅇ ㄱㄱ 맛있어",
          "original_form": "ㅇㅇ ㄱㄱ 맛있어",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042832.1.9",
          "form": "약간 다른맛인데 그래도 조음",
          "original_form": "약간 다른맛인데 그래도 조음",
          "speaker_id": "2",
          "time": "20210907 09:47"
        },
        {
          "id": "MDRW2100042832.1.10",
          "form": "건더기는",
          "original_form": "건더기는",
          "speaker_id": "1",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042832.1.11",
          "form": "많냐고?",
          "original_form": "많냐고?",
          "speaker_id": "2",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042832.1.12",
          "form": "좀있어 비슷",
          "original_form": "좀있어 비슷",
          "speaker_id": "2",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042832.1.13",
          "form": "근데 자몽 슬라이스 넣어줌 빨대로 꾹꾹눌러서 즙을 짜주자",
          "original_form": "근데 자몽 슬라이스 넣어줌 빨대로 꾹꾹눌러서 즙을 짜주자",
          "speaker_id": "2",
          "time": "20210907 09:48"
        },
        {
          "id": "MDRW2100042832.1.14",
          "form": "헐 겁나맘에들어",
          "original_form": "헐 겁나맘에들어",
          "speaker_id": "1",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042832.1.15",
          "form": "퓨레로만드나보네",
          "original_form": "퓨레로만드나보네",
          "speaker_id": "1",
          "time": "20210907 09:49"
        },
        {
          "id": "MDRW2100042832.1.16",
          "form": "ㅇㅇ 조아",
          "original_form": "ㅇㅇ 조아",
          "speaker_id": "2",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042832.1.17",
          "form": "나 거기 감자빵 한번 먹어보고싶다",
          "original_form": "나 거기 감자빵 한번 먹어보고싶다",
          "speaker_id": "2",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042832.1.18",
          "form": "시럽 덜달게되너",
          "original_form": "시럽 덜달게되너",
          "speaker_id": "1",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100042832.1.19",
          "form": "감자빵 먹어봤어",
          "original_form": "감자빵 먹어봤어",
          "speaker_id": "1",
          "time": "20210907 09:51"
        },
        {
          "id": "MDRW2100042832.1.20",
          "form": "요청사항에 적어바 근데 물을 더타도 됨",
          "original_form": "요청사항에 적어바 근데 물을 더타도 됨",
          "speaker_id": "2",
          "time": "20210907 09:51"
        },
        {
          "id": "MDRW2100042832.1.21",
          "form": "메가꺼말고",
          "original_form": "메가꺼말고",
          "speaker_id": "1",
          "time": "20210907 09:51"
        },
        {
