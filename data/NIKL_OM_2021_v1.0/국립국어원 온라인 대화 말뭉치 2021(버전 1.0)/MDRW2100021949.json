      "utterance": [
        {
          "id": "MDRW2100021949.1.1",
          "form": "그치 친구들이랑 가면 더 재밌지",
          "original_form": "그치 친구들이랑 가면 더 재밌지",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021949.1.2",
          "form": "친구들이랑 간지가 2년도 넘어서",
          "original_form": "친구들이랑 간지가 2년도 넘어서",
          "speaker_id": "1",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021949.1.3",
          "form": "꽤 오래 됐네?",
          "original_form": "꽤 오래 됐네?",
          "speaker_id": "2",
          "time": "20210829 00:06"
        },
        {
          "id": "MDRW2100021949.1.4",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021949.1.5",
          "form": "코로나가 얼른 끝나야 될 텐데",
          "original_form": "코로나가 얼른 끝나야 될 텐데",
          "speaker_id": "2",
          "time": "20210829 00:07"
        },
        {
          "id": "MDRW2100021949.1.6",
          "form": "코로나가 빨리 끝나야 갈텐데",
          "original_form": "코로나가 빨리 끝나야 갈텐데",
          "speaker_id": "1",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021949.1.7",
          "form": "그러니까 여행 가고 싶은 곳 많은데",
          "original_form": "그러니까 여행 가고 싶은 곳 많은데",
          "speaker_id": "2",
          "time": "20210829 00:08"
        },
        {
          "id": "MDRW2100021949.1.8",
          "form": "여행은 대충 정하면 되고 ㅎㅎ",
          "original_form": "여행은 대충 정하면 되고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:09"
        },
        {
          "id": "MDRW2100021949.1.9",
          "form": "사실 어디든 좋아",
          "original_form": "사실 어디든 좋아",
          "speaker_id": "2",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100021949.1.10",
          "form": "그건 그렇지",
          "original_form": "그건 그렇지",
          "speaker_id": "1",
          "time": "20210829 00:10"
        },
        {
          "id": "MDRW2100021949.1.11",
          "form": "국내 여행이라도 가고 싶다",
          "original_form": "국내 여행이라도 가고 싶다",
          "speaker_id": "2",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021949.1.12",
          "form": "인정",
          "original_form": "인정",
          "speaker_id": "1",
          "time": "20210829 00:11"
        },
        {
          "id": "MDRW2100021949.1.13",
          "form": "국내 여행가면 어디 가고 싶어?",
          "original_form": "국내 여행가면 어디 가고 싶어?",
          "speaker_id": "2",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021949.1.14",
          "form": "제주도",
          "original_form": "제주도",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021949.1.15",
          "form": "말고는 없어?",
          "original_form": "말고는 없어?",
          "speaker_id": "2",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021949.1.16",
          "form": "제주도 한 번도 안가봐서",
          "original_form": "제주도 한 번도 안가봐서",
          "speaker_id": "1",
          "time": "20210829 00:12"
        },
        {
          "id": "MDRW2100021949.1.17",
          "form": "그럼 제주도 다음으로 가고 싶은 곳은?",
          "original_form": "그럼 제주도 다음으로 가고 싶은 곳은?",
          "speaker_id": "2",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100021949.1.18",
          "form": "강릉?",
          "original_form": "강릉?",
          "speaker_id": "1",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100021949.1.19",
          "form": "강릉 좋았어?",
          "original_form": "강릉 좋았어?",
          "speaker_id": "2",
          "time": "20210829 00:13"
        },
        {
          "id": "MDRW2100021949.1.20",
          "form": "괜찮았어",
          "original_form": "괜찮았어",
          "speaker_id": "1",
          "time": "20210829 00:14"
        },
        {
          "id": "MDRW2100021949.1.21",
          "form": "여행 가보고 싶은 곳 있어?",
          "original_form": "여행 가보고 싶은 곳 있어?",
          "speaker_id": "1",
