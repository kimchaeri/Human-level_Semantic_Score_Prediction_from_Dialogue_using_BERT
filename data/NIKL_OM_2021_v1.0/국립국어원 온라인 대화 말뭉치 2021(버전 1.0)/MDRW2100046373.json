      "utterance": [
        {
          "id": "MDRW2100046373.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211019 17:23"
        },
        {
          "id": "MDRW2100046373.1.2",
          "form": "안녕하세요~ 요즘 오징어게임이 대세인데 보셨나요?",
          "original_form": "안녕하세요~ 요즘 오징어게임이 대세인데 보셨나요?",
          "speaker_id": "2",
          "time": "20211019 17:24"
        },
        {
          "id": "MDRW2100046373.1.3",
          "form": "처음부터끝까지는 못봤고",
          "original_form": "처음부터끝까지는 못봤고",
          "speaker_id": "1",
          "time": "20211019 17:25"
        },
        {
          "id": "MDRW2100046373.1.4",
          "form": "유튜브로 요약된걸로 결말까지 봤습니다",
          "original_form": "유튜브로 요약된걸로 결말까지 봤습니다",
          "speaker_id": "1",
          "time": "20211019 17:26"
        },
        {
          "id": "MDRW2100046373.1.5",
          "form": "오 ㅋㅋ 어떠셨나요 저는 넷플로 봤는데 꽤 재밌더라구요",
          "original_form": "오 ㅋㅋ 어떠셨나요 저는 넷플로 봤는데 꽤 재밌더라구요",
          "speaker_id": "2",
          "time": "20211019 17:27"
        },
        {
          "id": "MDRW2100046373.1.6",
          "form": "저도 되게 재미있게봤어요",
          "original_form": "저도 되게 재미있게봤어요",
          "speaker_id": "1",
          "time": "20211019 17:34"
        },
        {
          "id": "MDRW2100046373.1.7",
          "form": "어떤 장면이 가장 인상깊으신가요",
          "original_form": "어떤 장면이 가장 인상깊으신가요",
          "speaker_id": "2",
          "time": "20211019 17:36"
        },
        {
          "id": "MDRW2100046373.1.8",
          "form": "할아버지랑 이정재랑 구슬로 내기할때 할아버지가 치매인척 다알고있었던 부분이요",
          "original_form": "할아버지랑 이정재랑 구슬로 내기할때 할아버지가 치매인척 다알고있었던 부분이요",
          "speaker_id": "1",
          "time": "20211019 17:36"
        },
        {
          "id": "MDRW2100046373.1.9",
          "form": "가장 유명한 장면이네요..",
          "original_form": "가장 유명한 장면이네요..",
          "speaker_id": "2",
          "time": "20211019 17:39"
        },
        {
          "id": "MDRW2100046373.1.10",
          "form": "결말 알고 다시보면 복선이 되게 많더라구요",
          "original_form": "결말 알고 다시보면 복선이 되게 많더라구요",
          "speaker_id": "2",
          "time": "20211019 17:39"
        },
        {
          "id": "MDRW2100046373.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20211019 17:40"
        },
        {
          "id": "MDRW2100046373.1.12",
          "form": "한번더 봐도 재미있을것같아요",
          "original_form": "한번더 봐도 재미있을것같아요",
          "speaker_id": "1",
          "time": "20211019 17:40"
        },
        {
          "id": "MDRW2100046373.1.13",
          "form": "요약본으로 보셨으니까 본편으로 보시면 더 재밌을거에요",
          "original_form": "요약본으로 보셨으니까 본편으로 보시면 더 재밌을거에요",
          "speaker_id": "2",
          "time": "20211019 17:41"
        },
        {
          "id": "MDRW2100046373.1.14",
          "form": "안그래도",
          "original_form": "안그래도",
          "speaker_id": "1",
          "time": "20211019 17:45"
        },
        {
          "id": "MDRW2100046373.1.15",
          "form": "넷플릭스 있어서 나중에 다시 보려구요!",
          "original_form": "넷플릭스 있어서 나중에 다시 보려구요!",
          "speaker_id": "1",
          "time": "20211019 17:46"
        },
        {
          "id": "MDRW2100046373.1.16",
          "form": "넷플 오리지널 콘텐츠는 다 재밌으니 다른 것도 추천드려요 ㅎㅎ",
          "original_form": "넷플 오리지널 콘텐츠는 다 재밌으니 다른 것도 추천드려요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211019 17:46"
        }
      ]
