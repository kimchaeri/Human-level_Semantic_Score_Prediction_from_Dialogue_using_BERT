      "utterance": [
        {
          "id": "MDRW2100000909.1.1",
          "form": "요즘 날씨 이상하지않아요?",
          "original_form": "요즘 날씨 이상하지않아요?",
          "speaker_id": "2",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000909.1.2",
          "form": "여기는 어제 갑자기 막 먹구름도 끼구",
          "original_form": "여기는 어제 갑자기 막 먹구름도 끼구",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000909.1.3",
          "form": "비도 잠깐 내리고 ㅠㅠ",
          "original_form": "비도 잠깐 내리고 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 20:32"
        },
        {
          "id": "MDRW2100000909.1.4",
          "form": "맞아요 날씨가 너무 오락가락해요",
          "original_form": "맞아요 날씨가 너무 오락가락해요",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000909.1.5",
          "form": "비도 뜬금없이 내리구요,,",
          "original_form": "비도 뜬금없이 내리구요,,",
          "speaker_id": "1",
          "time": "20210819 20:34"
        },
        {
          "id": "MDRW2100000909.1.6",
          "form": "그쵸 ㅠㅋㅋ 오늘은 또 소나기가 온다더니",
          "original_form": "그쵸 ㅠㅋㅋ 오늘은 또 소나기가 온다더니",
          "speaker_id": "2",
          "time": "20210819 20:36"
        },
        {
          "id": "MDRW2100000909.1.7",
          "form": "결국 덥기만하고 안왔네여..",
          "original_form": "결국 덥기만하고 안왔네여..",
          "speaker_id": "2",
          "time": "20210819 20:37"
        },
        {
          "id": "MDRW2100000909.1.8",
          "form": "그러니까요!! 오늘 엄청 덥더라구요",
          "original_form": "그러니까요!! 오늘 엄청 덥더라구요",
          "speaker_id": "1",
          "time": "20210819 20:38"
        },
        {
          "id": "MDRW2100000909.1.9",
          "form": "요즘 시원해졌다고 생각했는데 역시나 ㅠ",
          "original_form": "요즘 시원해졌다고 생각했는데 역시나 ㅠ",
          "speaker_id": "1",
          "time": "20210819 20:39"
        },
        {
          "id": "MDRW2100000909.1.10",
          "form": "진짜 아침밤으론 시원해졌는데",
          "original_form": "진짜 아침밤으론 시원해졌는데",
          "speaker_id": "2",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000909.1.11",
          "form": "낮엔 아직까지 더워요",
          "original_form": "낮엔 아직까지 더워요",
          "speaker_id": "2",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000909.1.12",
          "form": "이러다 곧 겨울오겟죠..",
          "original_form": "이러다 곧 겨울오겟죠..",
          "speaker_id": "2",
          "time": "20210819 20:42"
        },
        {
          "id": "MDRW2100000909.1.13",
          "form": "네... 여름하고 겨울 중 뭘 더 좋아하세요?",
          "original_form": "네... 여름하고 겨울 중 뭘 더 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000909.1.14",
          "form": "전 그나마 겨울이요 ㅎ",
          "original_form": "전 그나마 겨울이요 ㅎ",
          "speaker_id": "1",
          "time": "20210819 20:43"
        },
        {
          "id": "MDRW2100000909.1.15",
          "form": "흠 저도 겨울이요!",
          "original_form": "흠 저도 겨울이요!",
          "speaker_id": "2",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000909.1.16",
          "form": "겨울 그 찬공기냄새 좋아해요 ㅎㅎ",
          "original_form": "겨울 그 찬공기냄새 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 20:47"
        },
        {
          "id": "MDRW2100000909.1.17",
          "form": "어 저두요 옷에서 나는 공기요!!!@",
          "original_form": "어 저두요 옷에서 나는 공기요!!!@",
          "speaker_id": "1",
          "time": "20210819 20:49"
        },
        {
          "id": "MDRW2100000909.1.18",
          "form": "여름엔 땀냄새가 ...ㅋㅋ",
          "original_form": "여름엔 땀냄새가 ...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000909.1.19",
          "form": "여름은 그래서 싫어요 ㅎㅎ ㅎㅎㅎ",
          "original_form": "여름은 그래서 싫어요 ㅎㅎ ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 20:51"
        },
        {
          "id": "MDRW2100000909.1.20",
          "form": "맞아요 뭔가 찝찝한기분 ..",
          "original_form": "맞아요 뭔가 찝찝한기분 ..",
          "speaker_id": "2",
          "time": "20210819 20:52"
        },
        {
          "id": "MDRW2100000909.1.21",
          "form": "지금도 겨울이 얼른 오면 좋겠어요 ㅎ",
