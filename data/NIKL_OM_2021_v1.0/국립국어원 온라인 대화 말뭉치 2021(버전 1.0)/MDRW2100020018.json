      "utterance": [
        {
          "id": "MDRW2100020018.1.1",
          "form": "ㅎㅎ 학교생활이네요ㅎ",
          "original_form": "ㅎㅎ 학교생활이네요ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020018.1.2",
          "form": "저 그때 제 딸하고 책읽고 있었는데ㅎㅎㅎ",
          "original_form": "저 그때 제 딸하고 책읽고 있었는데ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020018.1.3",
          "form": "어이가없어서ㅎㅎ",
          "original_form": "어이가없어서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020018.1.4",
          "form": "ㄱㄱ요ㅎ",
          "original_form": "ㄱㄱ요ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:18"
        },
        {
          "id": "MDRW2100020018.1.5",
          "form": "학교때 그립네요 ㅎㅎ",
          "original_form": "학교때 그립네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:19"
        },
        {
          "id": "MDRW2100020018.1.6",
          "form": "name1님은 리즈시절이 언제에요?",
          "original_form": "&name1&님은 리즈시절이 언제에요?",
          "speaker_id": "2",
          "time": "20210828 00:19"
        },
        {
          "id": "MDRW2100020018.1.7",
          "form": "어느 학창시절때?",
          "original_form": "어느 학창시절때?",
          "speaker_id": "2",
          "time": "20210828 00:19"
        },
        {
          "id": "MDRW2100020018.1.8",
          "form": "음 전 20살이겟죠 ㅎㅎ",
          "original_form": "음 전 20살이겟죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100020018.1.9",
          "form": "에고ㅎ 엄청났겟네요ㅎㅎ",
          "original_form": "에고ㅎ 엄청났겟네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100020018.1.10",
          "form": "아뇨 전 별루 ㅎㅎㅎ",
          "original_form": "아뇨 전 별루 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100020018.1.11",
          "form": "name1님이 생각해더 이뻤음요?ㅎ",
          "original_form": "&name1&님이 생각해더 이뻤음요?ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100020018.1.12",
          "form": "에이ㅎ 별루라고 하시니깐 이쁘셨나보네ㅎ",
          "original_form": "에이ㅎ 별루라고 하시니깐 이쁘셨나보네ㅎ",
          "speaker_id": "2",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100020018.1.13",
          "form": "대학교 때 미팅못해봣내요 ㅋㅋ",
          "original_form": "대학교 때 미팅못해봣내요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020018.1.14",
          "form": "저 과대 였는데 주섬만 엄청 했어요 ㅎㅎ",
          "original_form": "저 과대 였는데 주섬만 엄청 했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020018.1.15",
          "form": "주선",
          "original_form": "주선",
          "speaker_id": "1",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020018.1.16",
