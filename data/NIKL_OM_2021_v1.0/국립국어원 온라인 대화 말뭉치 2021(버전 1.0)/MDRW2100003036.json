      "utterance": [
        {
          "id": "MDRW2100003036.1.1",
          "form": "요즘 힘들지?",
          "original_form": "요즘 힘들지?",
          "speaker_id": "1",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003036.1.2",
          "form": "종교에 귀의할 생각은 없어",
          "original_form": "종교에 귀의할 생각은 없어",
          "speaker_id": "1",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003036.1.3",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210820 17:45"
        },
        {
          "id": "MDRW2100003036.1.4",
          "form": "아니 아무래도 나는",
          "original_form": "아니 아무래도 나는",
          "speaker_id": "2",
          "time": "20210820 18:42"
        },
        {
          "id": "MDRW2100003036.1.5",
          "form": "내눈에 보이지않으면 안믿어서",
          "original_form": "내눈에 보이지않으면 안믿어서",
          "speaker_id": "2",
          "time": "20210820 18:42"
        },
        {
          "id": "MDRW2100003036.1.6",
          "form": "넌 그럴 생각이 있니?",
          "original_form": "넌 그럴 생각이 있니?",
          "speaker_id": "2",
          "time": "20210820 18:42"
        },
        {
          "id": "MDRW2100003036.1.7",
          "form": "나도 잘 모르겠어.",
          "original_form": "나도 잘 모르겠어.",
          "speaker_id": "1",
          "time": "20210820 18:43"
        },
        {
          "id": "MDRW2100003036.1.8",
          "form": "종교행사에 참여한 적 있니?",
          "original_form": "종교행사에 참여한 적 있니?",
          "speaker_id": "1",
          "time": "20210820 18:43"
        },
        {
          "id": "MDRW2100003036.1.9",
          "form": "어릴때 친구따라서 크리스마스에 교회간적있어",
          "original_form": "어릴때 친구따라서 크리스마스에 교회간적있어",
          "speaker_id": "2",
          "time": "20210820 20:19"
        },
        {
          "id": "MDRW2100003036.1.10",
          "form": "맛있는거주고 문상준다고해서 따라갔었지",
          "original_form": "맛있는거주고 문상준다고해서 따라갔었지",
          "speaker_id": "2",
          "time": "20210820 20:20"
        },
        {
          "id": "MDRW2100003036.1.11",
          "form": "뭐 먹었어?",
          "original_form": "뭐 먹었어?",
          "speaker_id": "1",
          "time": "20210820 20:25"
        },
        {
          "id": "MDRW2100003036.1.12",
          "form": "그때 초코케이크 먹었던 것 같아",
          "original_form": "그때 초코케이크 먹었던 것 같아",
          "speaker_id": "2",
          "time": "20210820 20:28"
        },
        {
          "id": "MDRW2100003036.1.13",
          "form": "와 좋다",
          "original_form": "와 좋다",
          "speaker_id": "1",
          "time": "20210820 20:28"
        },
        {
          "id": "MDRW2100003036.1.14",
          "form": "맛있었겠다",
          "original_form": "맛있었겠다",
          "speaker_id": "1",
          "time": "20210820 20:28"
        },
        {
          "id": "MDRW2100003036.1.15",
          "form": "근데 교회에서 돈 걷어갔어",
          "original_form": "근데 교회에서 돈 걷어갔어",
          "speaker_id": "2",
          "time": "20210820 20:28"
        },
        {
          "id": "MDRW2100003036.1.16",
          "form": "이런.",
          "original_form": "이런.",
          "speaker_id": "1",
          "time": "20210820 20:28"
        },
        {
          "id": "MDRW2100003036.1.17",
          "form": "초등학생이었는데 천원이나 냈잖니",
          "original_form": "초등학생이었는데 천원이나 냈잖니",
          "speaker_id": "2",
          "time": "20210820 20:28"
        },
        {
          "id": "MDRW2100003036.1.18",
          "form": "오백원 낶어?",
          "original_form": "오백원 낶어?",
          "speaker_id": "1",
          "time": "20210820 20:28"
        },
        {
          "id": "MDRW2100003036.1.19",
          "form": "이런... 너무해",
          "original_form": "이런... 너무해",
          "speaker_id": "1",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003036.1.20",
          "form": "용돈이 적었을텐데",
          "original_form": "용돈이 적었을텐데",
          "speaker_id": "1",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003036.1.21",
          "form": "ㅠㅠ아마 그때 일주일에 천원이었을ㄲ야",
          "original_form": "ㅠㅠ아마 그때 일주일에 천원이었을ㄲ야",
          "speaker_id": "2",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003036.1.22",
          "form": "너는 가본적있니?",
          "original_form": "너는 가본적있니?",
          "speaker_id": "2",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003036.1.23",
          "form": "나도 있지",
          "original_form": "나도 있지",
          "speaker_id": "1",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003036.1.24",
          "form": "내가 생각나는건 어릴 때 성당에 갔을때야",
          "original_form": "내가 생각나는건 어릴 때 성당에 갔을때야",
          "speaker_id": "1",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003036.1.25",
          "form": "어쩌다 가봤니",
          "original_form": "어쩌다 가봤니",
          "speaker_id": "2",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003036.1.26",
          "form": "아하 모태신앙이었어?",
          "original_form": "아하 모태신앙이었어?",
          "speaker_id": "2",
          "time": "20210820 20:30"
        },
        {
          "id": "MDRW2100003036.1.27",
          "form": "우리집이ㅜ천주교였는데",
          "original_form": "우리집이ㅜ천주교였는데",
          "speaker_id": "1",
          "time": "20210820 20:30"
        },
        {
          "id": "MDRW2100003036.1.28",
          "form": "친구따라갔어",
          "original_form": "친구따라갔어",
          "speaker_id": "1",
          "time": "20210820 20:30"
        },
        {
          "id": "MDRW2100003036.1.29",
          "form": "흠 그럴수도있지",
          "original_form": "흠 그럴수도있지",
          "speaker_id": "2",
          "time": "20210820 20:30"
        }
      ]
    }
  ]
