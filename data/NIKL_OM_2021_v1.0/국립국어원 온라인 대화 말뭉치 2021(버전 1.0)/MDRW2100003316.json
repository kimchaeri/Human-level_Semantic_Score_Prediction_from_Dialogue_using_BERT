      "utterance": [
        {
          "id": "MDRW2100003316.1.1",
          "form": "여행 다니고 싶다",
          "original_form": "여행 다니고 싶다",
          "speaker_id": "1",
          "time": "20210820 21:40"
        },
        {
          "id": "MDRW2100003316.1.2",
          "form": "누나는 여행 어디 가고싶은곳 있어?",
          "original_form": "누나는 여행 어디 가고싶은곳 있어?",
          "speaker_id": "1",
          "time": "20210820 21:40"
        },
        {
          "id": "MDRW2100003316.1.3",
          "form": "음 사람들이 많지안고 아무도 방해안받는 그런곳으로 여행가고싶어",
          "original_form": "음 사람들이 많지안고 아무도 방해안받는 그런곳으로 여행가고싶어",
          "speaker_id": "2",
          "time": "20210820 21:41"
        },
        {
          "id": "MDRW2100003316.1.4",
          "form": "그러면 산이 좋긴하겠네",
          "original_form": "그러면 산이 좋긴하겠네",
          "speaker_id": "1",
          "time": "20210820 21:42"
        },
        {
          "id": "MDRW2100003316.1.5",
          "form": "아님 트래킹 할 수 있는 곳",
          "original_form": "아님 트래킹 할 수 있는 곳",
          "speaker_id": "1",
          "time": "20210820 21:42"
        },
        {
          "id": "MDRW2100003316.1.6",
          "form": "늪지나 그런곳도",
          "original_form": "늪지나 그런곳도",
          "speaker_id": "1",
          "time": "20210820 21:42"
        },
        {
          "id": "MDRW2100003316.1.7",
          "form": "좋을듯",
          "original_form": "좋을듯",
          "speaker_id": "1",
          "time": "20210820 21:42"
        },
        {
          "id": "MDRW2100003316.1.8",
          "form": "오르막길 별로 안좋아하는대",
          "original_form": "오르막길 별로 안좋아하는대",
          "speaker_id": "2",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003316.1.9",
          "form": "여행간다해도 날더더우니깐 시원한 바다? 멘션잡아서",
          "original_form": "여행간다해도 날더더우니깐 시원한 바다? 멘션잡아서",
          "speaker_id": "2",
          "time": "20210820 21:48"
        },
        {
          "id": "MDRW2100003316.1.10",
          "form": "같이 여행도가고",
          "original_form": "같이 여행도가고",
          "speaker_id": "2",
          "time": "20210820 21:49"
        },
        {
          "id": "MDRW2100003316.1.11",
          "form": "팬션도 좋지",
          "original_form": "팬션도 좋지",
          "speaker_id": "1",
          "time": "20210820 21:52"
        },
        {
          "id": "MDRW2100003316.1.12",
          "form": "오르막길이 별로면 공원도 좋겠네",
          "original_form": "오르막길이 별로면 공원도 좋겠네",
          "speaker_id": "1",
          "time": "20210820 21:53"
        },
        {
          "id": "MDRW2100003316.1.13",
          "form": "맞지 내꿈은 자전거를 타면서 여기저기 여행해보는거야",
          "original_form": "맞지 내꿈은 자전거를 타면서 여기저기 여행해보는거야",
          "speaker_id": "2",
          "time": "20210820 21:54"
        },
        {
          "id": "MDRW2100003316.1.14",
          "form": "전국일주 좋지!",
          "original_form": "전국일주 좋지!",
          "speaker_id": "1",
          "time": "20210820 21:57"
        },
        {
          "id": "MDRW2100003316.1.15",
          "form": "자전거 타고 전국일주 낭만넘친다 ㅠㅠ",
          "original_form": "자전거 타고 전국일주 낭만넘친다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210820 21:57"
        },
        {
          "id": "MDRW2100003316.1.16",
          "form": "그래서 자전거도하나 마련해놨거든 전국일주! 를하고싶지만.. 코로나때매.",
          "original_form": "그래서 자전거도하나 마련해놨거든 전국일주! 를하고싶지만.. 코로나때매.",
          "speaker_id": "2",
          "time": "20210820 22:07"
        },
        {
          "id": "MDRW2100003316.1.17",
          "form": "코로나 ㅠㅠ 짜증나지",
          "original_form": "코로나 ㅠㅠ 짜증나지",
          "speaker_id": "1",
          "time": "20210820 22:08"
        },
        {
          "id": "MDRW2100003316.1.18",
          "form": "여행가고싶다ㅏ",
          "original_form": "여행가고싶다ㅏ",
          "speaker_id": "2",
          "time": "20210823 07:45"
        },
        {
          "id": "MDRW2100003316.1.19",
          "form": "나도 여행가고싶다",
          "original_form": "나도 여행가고싶다",
          "speaker_id": "1",
          "time": "20210823 13:56"
        },
        {
          "id": "MDRW2100003316.1.20",
          "form": "머나먼 바다를 구경하고싶어",
          "original_form": "머나먼 바다를 구경하고싶어",
          "speaker_id": "1",
          "time": "20210823 13:56"
        },
        {
          "id": "MDRW2100003316.1.21",
          "form": "바다 좋지",
          "original_form": "바다 좋지",
          "speaker_id": "2",
          "time": "20210823 13:56"
        },
        {
          "id": "MDRW2100003316.1.22",
          "form": "여객선 타보고 싶음",
          "original_form": "여객선 타보고 싶음",
          "speaker_id": "1",
          "time": "20210823 13:56"
        },
        {
          "id": "MDRW2100003316.1.23",
          "form": "그안에서 즐길거 즐기면서",
          "original_form": "그안에서 즐길거 즐기면서",
          "speaker_id": "1",
          "time": "20210823 13:57"
        },
        {
          "id": "MDRW2100003316.1.24",
          "form": "바다구경하고싶어",
          "original_form": "바다구경하고싶어",
          "speaker_id": "1",
          "time": "20210823 13:57"
        }
      ]
    }
