      "utterance": [
        {
          "id": "MDRW2100006179.1.1",
          "form": "어... 갑자기 보스몬스터가?",
          "original_form": "어... 갑자기 보스몬스터가?",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100006179.1.2",
          "form": "ㅎㅎ아마 후배가 잘못한 걸거야ㅎㅎ",
          "original_form": "ㅎㅎ아마 후배가 잘못한 걸거야ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100006179.1.3",
          "form": "우리 인원 줄이라는데요ㅠ",
          "original_form": "우리 인원 줄이라는데요ㅠ",
          "speaker_id": "1",
          "time": "20210907 21:25"
        },
        {
          "id": "MDRW2100006179.1.4",
          "form": "이미 인력 줄어든 사람ㅜㅜ",
          "original_form": "이미 인력 줄어든 사람ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100006179.1.5",
          "form": "같이 머리띠 맬까요",
          "original_form": "같이 머리띠 맬까요",
          "speaker_id": "2",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100006179.1.6",
          "form": "2조2교대가",
          "original_form": "2조2교대가",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100006179.1.7",
          "form": "사람이 할 말입니까!!!",
          "original_form": "사람이 할 말입니까!!!",
          "speaker_id": "1",
          "time": "20210907 21:28"
        },
        {
          "id": "MDRW2100006179.1.8",
          "form": "? 돈을 많이 올려준답니까?",
          "original_form": "? 돈을 많이 올려준답니까?",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100006179.1.9",
          "form": "이 퇴행하는 업무형태는 뭐죠",
          "original_form": "이 퇴행하는 업무형태는 뭐죠",
          "speaker_id": "2",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100006179.1.10",
          "form": "아뇨?",
          "original_form": "아뇨?",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100006179.1.11",
          "form": "심지어 야간수당 빠지면",
          "original_form": "심지어 야간수당 빠지면",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100006179.1.12",
          "form": "월급 -입니다",
          "original_form": "월급 -입니다",
          "speaker_id": "1",
          "time": "20210907 21:33"
        },
        {
          "id": "MDRW2100006179.1.13",
          "form": "ㅋㅋㅋ 와 미춌나봐",
          "original_form": "ㅋㅋㅋ 와 미춌나봐",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006179.1.14",
          "form": "진짜 상상초월이네요",
          "original_form": "진짜 상상초월이네요",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100006179.1.15",
          "form": "널리 알려주세요!!!",
          "original_form": "널리 알려주세요!!!",
          "speaker_id": "1",
          "time": "20210907 21:41"
        },
        {
          "id": "MDRW2100006179.1.16",
          "form": "네네ㅜ",
          "original_form": "네네ㅜ",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100006179.1.17",
          "form": "진짜 위놈들은 뭘 생각하는지",
          "original_form": "진짜 위놈들은 뭘 생각하는지",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100006179.1.18",
          "form": "쥐어짜면서 돌아가야 속이 시원한가봐요",
          "original_form": "쥐어짜면서 돌아가야 속이 시원한가봐요",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006179.1.19",
          "form": "환장",
          "original_form": "환장",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006179.1.20",
          "form": "이걸 직원 탓을 하네요",
          "original_form": "이걸 직원 탓을 하네요",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006179.1.21",
          "form": "지들이 무기계약직 전환시키고",
          "original_form": "지들이 무기계약직 전환시키고",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006179.1.22",
          "form": "인건비 폭증시키고서는 ㅎㅎ",
          "original_form": "인건비 폭증시키고서는 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006179.1.23",
          "form": "아이고ㅜㅜ 님도 고생이 많으십니다",
          "original_form": "아이고ㅜㅜ 님도 고생이 많으십니다",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100006179.1.24",
          "form": "진짜 바뀜 안되는데",
          "original_form": "진짜 바뀜 안되는데",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100006179.1.25",
          "form": "힘내세요!",
          "original_form": "힘내세요!",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100006179.1.26",
          "form": "널리 퍼뜨려보겠음!",
          "original_form": "널리 퍼뜨려보겠음!",
          "speaker_id": "2",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100006179.1.27",
          "form": "아까 누가 내가 잘못했다고",
          "original_form": "아까 누가 내가 잘못했다고",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100006179.1.28",
          "form": "그랬어요 ㅎㅎ",
          "original_form": "그랬어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:00"
        },
        {
          "id": "MDRW2100006179.1.29",
          "form": "기억나지 않쥬",
          "original_form": "기억나지 않쥬",
          "speaker_id": "2",
          "time": "20210907 22:01"
        }
      ]
    }
