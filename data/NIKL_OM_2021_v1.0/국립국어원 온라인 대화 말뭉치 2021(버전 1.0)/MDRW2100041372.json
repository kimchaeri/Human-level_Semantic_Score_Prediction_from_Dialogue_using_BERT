      "utterance": [
        {
          "id": "MDRW2100041372.1.1",
          "form": "감사합니다ㅋㅋㅋㅋ",
          "original_form": "감사합니다ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100041372.1.2",
          "form": "근데 코로나가 끝나야 콘서트가 열릴텐데ㅋㅋㅋ",
          "original_form": "근데 코로나가 끝나야 콘서트가 열릴텐데ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:44"
        },
        {
          "id": "MDRW2100041372.1.3",
          "form": "큐ㅜㅠㅜ",
          "original_form": "큐ㅜㅠㅜ",
          "speaker_id": "1",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100041372.1.4",
          "form": "또어떤콘서트가보셨어요?",
          "original_form": "또어떤콘서트가보셨어요?",
          "speaker_id": "1",
          "time": "20210905 22:45"
        },
        {
          "id": "MDRW2100041372.1.5",
          "form": "저는 엔시티요",
          "original_form": "저는 엔시티요",
          "speaker_id": "2",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100041372.1.6",
          "form": "엔시티 아시나요?",
          "original_form": "엔시티 아시나요?",
          "speaker_id": "2",
          "time": "20210905 22:46"
        },
        {
          "id": "MDRW2100041372.1.7",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100041372.1.8",
          "form": "sm아이돌",
          "original_form": "sm아이돌",
          "speaker_id": "1",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100041372.1.9",
          "form": "와진짜티켓팅고수세요",
          "original_form": "와진짜티켓팅고수세요",
          "speaker_id": "1",
          "time": "20210905 22:52"
        },
        {
          "id": "MDRW2100041372.1.10",
          "form": "아니예요ㅋㅋㅋㅋ",
          "original_form": "아니예요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100041372.1.11",
          "form": "그럼 가보시고 싶은 공연 있으신가요??",
          "original_form": "그럼 가보시고 싶은 공연 있으신가요??",
          "speaker_id": "2",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100041372.1.12",
          "form": "음 대학로소극장연극",
          "original_form": "음 대학로소극장연극",
          "speaker_id": "1",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100041372.1.13",
          "form": "그런거코로나만아님",
          "original_form": "그런거코로나만아님",
          "speaker_id": "1",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100041372.1.14",
          "form": "가고싶어요ㅠ",
          "original_form": "가고싶어요ㅠ",
          "speaker_id": "1",
          "time": "20210905 22:53"
        },
        {
          "id": "MDRW2100041372.1.15",
          "form": "특별히 보고 싶은 연극작품이 있으신가요?",
          "original_form": "특별히 보고 싶은 연극작품이 있으신가요?",
          "speaker_id": "2",
          "time": "20210905 22:54"
        },
        {
          "id": "MDRW2100041372.1.16",
          "form": "음저는거기가서한번돌고",
          "original_form": "음저는거기가서한번돌고",
          "speaker_id": "1",
          "time": "20210905 22:56"
        },
        {
          "id": "MDRW2100041372.1.17",
          "form": "포스터맘에드는거봐요ㅎㅎ",
          "original_form": "포스터맘에드는거봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:56"
        },
        {
          "id": "MDRW2100041372.1.18",
          "form": "즉흥적으로 보고싶은 공연선택하시는 스타일이신가요?",
          "original_form": "즉흥적으로 보고싶은 공연선택하시는 스타일이신가요?",
          "speaker_id": "2",
          "time": "20210905 22:57"
        },
        {
          "id": "MDRW2100041372.1.19",
          "form": "네~가끔망할때도있는데",
          "original_form": "네~가끔망할때도있는데",
          "speaker_id": "1",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100041372.1.20",
          "form": "그럼그런대로봐요ㅎㅎ",
          "original_form": "그럼그런대로봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100041372.1.21",
          "form": "그럼 제일 재밌게 본 연극은 무엇인가요?",
          "original_form": "그럼 제일 재밌게 본 연극은 무엇인가요?",
          "speaker_id": "2",
          "time": "20210905 22:58"
        },
        {
          "id": "MDRW2100041372.1.22",
          "form": "음 옥탑방고양이? 그거재밌게봤어요",
          "original_form": "음 옥탑방고양이? 그거재밌게봤어요",
          "speaker_id": "1",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100041372.1.23",
          "form": "뮤지컬이나 연극보신적있으신가요?",
          "original_form": "뮤지컬이나 연극보신적있으신가요?",
          "speaker_id": "1",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100041372.1.24",
          "form": "저는 학교에서 보여준 것 말고는 본 적이 없어요ㅠㅠ",
          "original_form": "저는 학교에서 보여준 것 말고는 본 적이 없어요ㅠㅠ",
