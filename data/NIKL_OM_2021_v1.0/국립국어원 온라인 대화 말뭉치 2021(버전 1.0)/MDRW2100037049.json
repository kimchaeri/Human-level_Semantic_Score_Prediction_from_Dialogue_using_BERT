        },
        {
          "id": "MDRW2100037049.1.2",
          "form": "후보를 말해봐",
          "original_form": "후보를 말해봐",
          "speaker_id": "1",
          "time": "20210831 17:58"
        },
        {
          "id": "MDRW2100037049.1.3",
          "form": "일단 가을이니까 가디건",
          "original_form": "일단 가을이니까 가디건",
          "speaker_id": "2",
          "time": "20210831 17:58"
        },
        {
          "id": "MDRW2100037049.1.4",
          "form": "너 가디건 많지 않나",
          "original_form": "너 가디건 많지 않나",
          "speaker_id": "1",
          "time": "20210831 17:58"
        },
        {
          "id": "MDRW2100037049.1.5",
          "form": "옷은 많으면 많을수록 좋아",
          "original_form": "옷은 많으면 많을수록 좋아",
          "speaker_id": "2",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100037049.1.6",
          "form": "어떤 가디건",
          "original_form": "어떤 가디건",
          "speaker_id": "1",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100037049.1.7",
          "form": "하얀색  단추가 있는 거",
          "original_form": "하얀색  단추가 있는 거",
          "speaker_id": "2",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100037049.1.8",
          "form": "나는 가디건은 좀 크게 입는 게 좋아",
          "original_form": "나는 가디건은 좀 크게 입는 게 좋아",
          "speaker_id": "1",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100037049.1.9",
          "form": "나도 그렇게 생각해",
          "original_form": "나도 그렇게 생각해",
          "speaker_id": "2",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100037049.1.10",
          "form": "그리고 색깔이 어두운 게 이뻐",
          "original_form": "그리고 색깔이 어두운 게 이뻐",
          "speaker_id": "1",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100037049.1.11",
          "form": "활용도가 좋지",
          "original_form": "활용도가 좋지",
          "speaker_id": "2",
          "time": "20210831 17:59"
        },
        {
          "id": "MDRW2100037049.1.12",
          "form": "너는 하얀색 좋아하니까",
