      "utterance": [
        {
          "id": "MDRW2100042433.1.1",
          "form": "팔 반 선생님",
          "original_form": "팔 반 선생님",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042433.1.2",
          "form": "좋지 않아??",
          "original_form": "좋지 않아??",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042433.1.3",
          "form": "어 선생님 좋아요ㅋㅋㅋㅋ",
          "original_form": "어 선생님 좋아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042433.1.4",
          "form": "애들 방학 전에",
          "original_form": "애들 방학 전에",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042433.1.5",
          "form": "애들 잘하는 거다 상장 만들어서 보내주고",
          "original_form": "애들 잘하는 거다 상장 만들어서 보내주고",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042433.1.6",
          "form": "뭐 엄청 좋네",
          "original_form": "뭐 엄청 좋네",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042433.1.7",
          "form": "뭐 약속 잘 지키고 하면 선물도 엄청 줘요",
          "original_form": "뭐 약속 잘 지키고 하면 선물도 엄청 줘요",
          "speaker_id": "1",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042433.1.8",
          "form": "남자 선생님 진짜 친절하다",
          "original_form": "남자 선생님 진짜 친절하다",
          "speaker_id": "2",
          "time": "20210906 22:06"
        },
        {
          "id": "MDRW2100042433.1.9",
          "form": "오 대박",
          "original_form": "오 대박",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042433.1.10",
          "form": "선물도 준다고??",
          "original_form": "선물도 준다고??",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042433.1.11",
          "form": "그러니까요",
          "original_form": "그러니까요",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042433.1.12",
          "form": "엄청 섬세한 선생님이네",
          "original_form": "엄청 섬세한 선생님이네",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042433.1.13",
          "form": "앞에 애들은 추울 수 있다고 겉옷 좀 챙겨 보내라고",
          "original_form": "앞에 애들은 추울 수 있다고 겉옷 좀 챙겨 보내라고",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042433.1.14",
          "form": "뭐 애들 선풍기",
          "original_form": "뭐 애들 선풍기",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042433.1.15",
          "form": "아이들이 진짜 좋아하겠다",
          "original_form": "아이들이 진짜 좋아하겠다",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042433.1.16",
          "form": "화분 같은 것도 주고",
          "original_form": "화분 같은 것도 주고",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042433.1.17",
          "form": "방학 전에 계획표를",
          "original_form": "방학 전에 계획표를",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042433.1.18",
          "form": "아 우리도 화분은 뭐 주긴 했던데",
          "original_form": "아 우리도 화분은 뭐 주긴 했던데",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042433.1.19",
          "form": "게임기처럼 만들어 가지고",
          "original_form": "게임기처럼 만들어 가지고",
          "speaker_id": "1",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042433.1.20",
          "form": "그 약간 숙제 같은 느낌이었는데 선물이었나??ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그 약간 숙제 같은 느낌이었는데 선물이었나??ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:07"
        },
        {
          "id": "MDRW2100042433.1.21",
          "form": "name3가 엄청 좋아해요",
          "original_form": "&name3&가 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042433.1.22",
          "form": "선생님이 되게",
          "original_form": "선생님이 되게",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042433.1.23",
          "form": "우리는 선물ㅋㅋㅋㅋㅋ",
          "original_form": "우리는 선물ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042433.1.24",
          "form": "초등학교에 맞는 선생님인 거 같다",
          "original_form": "초등학교에 맞는 선생님인 거 같다",
          "speaker_id": "2",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042433.1.25",
          "form": "상담하는데",
          "original_form": "상담하는데",
          "speaker_id": "1",
          "time": "20210906 22:08"
        },
        {
          "id": "MDRW2100042433.1.26",
          "form": "우리 아파트에 사는 언니 아들",
          "original_form": "우리 아파트에 사는 언니 아들",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042433.1.27",
          "form": "지금 이학년이거든",
          "original_form": "지금 이학년이거든",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042433.1.28",
          "form": "근데 걔 일학년 때 선생님 진짜 무서웠대",
          "original_form": "근데 걔 일학년 때 선생님 진짜 무서웠대",
          "speaker_id": "2",
          "time": "20210906 22:13"
        },
        {
          "id": "MDRW2100042433.1.29",
          "form": "숙제 안하면 머리 막 때리고",
          "original_form": "숙제 안하면 머리 막 때리고",
          "speaker_id": "2",
          "time": "20210906 22:13"
        }
      ]
    }
