        {
          "id": "MDRW2100004225.1.1",
          "form": "차 얘기하면 들어주나",
          "original_form": "차 얘기하면 들어주나",
          "speaker_id": "1",
          "time": "20210821 15:40"
        },
        {
          "id": "MDRW2100004225.1.2",
          "form": "요즘 홈카페에 빠졌어",
          "original_form": "요즘 홈카페에 빠졌어",
          "speaker_id": "1",
          "time": "20210821 15:40"
        },
        {
          "id": "MDRW2100004225.1.3",
          "form": "홈카페",
          "original_form": "홈카페",
          "speaker_id": "2",
          "time": "20210821 15:40"
        },
        {
          "id": "MDRW2100004225.1.4",
          "form": "재밌어보인다",
          "original_form": "재밌어보인다",
          "speaker_id": "2",
          "time": "20210821 15:40"
        },
        {
          "id": "MDRW2100004225.1.5",
          "form": "사진도찍나",
          "original_form": "사진도찍나",
          "speaker_id": "2",
          "time": "20210821 15:40"
        },
        {
          "id": "MDRW2100004225.1.6",
          "form": "만들고 바로 먹지",
          "original_form": "만들고 바로 먹지",
          "speaker_id": "1",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004225.1.7",
          "form": "하긴 니는 먹는게 목적일거같긴하다",
          "original_form": "하긴 니는 먹는게 목적일거같긴하다",
          "speaker_id": "2",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004225.1.8",
          "form": "먹으려고 만든거야",
          "original_form": "먹으려고 만든거야",
          "speaker_id": "1",
          "time": "20210821 17:41"
        },
        {
          "id": "MDRW2100004225.1.9",
          "form": "사진찍을 시간없어",
          "original_form": "사진찍을 시간없어",
          "speaker_id": "1",
          "time": "20210821 17:41"
        },
        {
          "id": "MDRW2100004225.1.10",
          "form": "지금 나는",
          "original_form": "지금 나는",
          "speaker_id": "2",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004225.1.11",
          "form": "쇠고기주먹밥 먹고있다",
          "original_form": "쇠고기주먹밥 먹고있다",
          "speaker_id": "2",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004225.1.12",
          "form": "주먹밥 맛있겠다",
          "original_form": "주먹밥 맛있겠다",
          "speaker_id": "1",
          "time": "20210821 17:50"
        },
        {
          "id": "MDRW2100004225.1.13",
          "form": "맛있다",
          "original_form": "맛있다",
          "speaker_id": "2",
          "time": "20210821 17:51"
        },
        {
          "id": "MDRW2100004225.1.14",
          "form": "니는 뭐먹냐",
          "original_form": "니는 뭐먹냐",
          "speaker_id": "2",
          "time": "20210821 17:51"
        },
        {
          "id": "MDRW2100004225.1.15",
          "form": "탄산수",
          "original_form": "탄산수",
          "speaker_id": "1",
          "time": "20210821 17:52"
        },
        {
          "id": "MDRW2100004225.1.16",
          "form": "차 타서?",
          "original_form": "차 타서?",
          "speaker_id": "2",
          "time": "20210821 17:52"
        },
        {
          "id": "MDRW2100004225.1.17",
          "form": "우린게맞는건가",
          "original_form": "우린게맞는건가",
          "speaker_id": "2",
          "time": "20210821 17:52"
        },
        {
          "id": "MDRW2100004225.1.18",
          "form": "저번에 산 차 냉침해서",
          "original_form": "저번에 산 차 냉침해서",
          "speaker_id": "1",
          "time": "20210821 17:52"
        },
        {
          "id": "MDRW2100004225.1.19",
          "form": "탄거랑",
          "original_form": "탄거랑",
          "speaker_id": "1",
          "time": "20210821 17:53"
        },
        {
          "id": "MDRW2100004225.1.20",
          "form": "난 사이다가좋더라",
          "original_form": "난 사이다가좋더라",
          "speaker_id": "2",
          "time": "20210821 17:53"
        },
        {
          "id": "MDRW2100004225.1.21",
          "form": "크리스탈라이트 석류체리맛",
          "original_form": "크리스탈라이트 석류체리맛",
