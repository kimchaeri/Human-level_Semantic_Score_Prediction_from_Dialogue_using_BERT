        {
          "id": "MDRW2100018031.1.1",
          "form": "가족이랑 사세요",
          "original_form": "가족이랑 사세요",
          "speaker_id": "1",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100018031.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100018031.1.3",
          "form": "그럼자취생?",
          "original_form": "그럼자취생?",
          "speaker_id": "1",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100018031.1.4",
          "form": "넹ㅋㅋ",
          "original_form": "넹ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100018031.1.5",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "1",
          "time": "20210826 22:13"
        },
        {
          "id": "MDRW2100018031.1.6",
          "form": "혼자집안일하는 거 힘들죠",
          "original_form": "혼자집안일하는 거 힘들죠",
          "speaker_id": "2",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100018031.1.7",
          "form": "네ㅜㅜ요리도 싫고",
          "original_form": "네ㅜㅜ요리도 싫고",
          "speaker_id": "1",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100018031.1.8",
          "form": "저는 청소가 제일 싫어요",
          "original_form": "저는 청소가 제일 싫어요",
          "speaker_id": "2",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100018031.1.9",
          "form": "저는 청소 좋아해요",
          "original_form": "저는 청소 좋아해요",
          "speaker_id": "1",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100018031.1.10",
          "form": "네?",
          "original_form": "네?",
          "speaker_id": "2",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100018031.1.11",
          "form": "깔끔한게 취향이라...",
          "original_form": "깔끔한게 취향이라...",
          "speaker_id": "1",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100018031.1.12",
          "form": "저도 깔끔한건 좋은데",
          "original_form": "저도 깔끔한건 좋은데",
          "speaker_id": "2",
          "time": "20210826 22:14"
        },
        {
          "id": "MDRW2100018031.1.13",
          "form": "넘 귀찮아요 하나마나느낌이",
          "original_form": "넘 귀찮아요 하나마나느낌이",
          "speaker_id": "2",
          "time": "20210826 22:15"
        },
        {
          "id": "MDRW2100018031.1.14",
          "form": "그쵸ㅠㅠ 해도 티안나고 현상유지만되니",
