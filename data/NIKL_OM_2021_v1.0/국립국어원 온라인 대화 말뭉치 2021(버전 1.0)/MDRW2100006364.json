      "utterance": [
        {
          "id": "MDRW2100006364.1.1",
          "form": "새로하는 뭉찬2  보셨어요?ㅎ",
          "original_form": "새로하는 뭉찬2  보셨어요?ㅎ",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100006364.1.2",
          "form": "오디션중인데",
          "original_form": "오디션중인데",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100006364.1.3",
          "form": "네.  오디션하는거요",
          "original_form": "네.  오디션하는거요",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100006364.1.4",
          "form": "일요일을 마감하는 프로죠",
          "original_form": "일요일을 마감하는 프로죠",
          "speaker_id": "2",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100006364.1.5",
          "form": "신선하더라구요",
          "original_form": "신선하더라구요",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100006364.1.6",
          "form": "모르는 사람들인데 운동진짜 잘하는듯",
          "original_form": "모르는 사람들인데 운동진짜 잘하는듯",
          "speaker_id": "1",
          "time": "20210907 23:15"
        },
        {
          "id": "MDRW2100006364.1.7",
          "form": "진짜요. 다들 대단하더라구요",
          "original_form": "진짜요. 다들 대단하더라구요",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100006364.1.8",
          "form": "허민호? 그사람이랑 스켈레톤?",
          "original_form": "허민호? 그사람이랑 스켈레톤?",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100006364.1.9",
          "form": "모르던 종목도 나오더라구요",
          "original_form": "모르던 종목도 나오더라구요",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100006364.1.10",
          "form": "그사람 스피드장난아니던데",
          "original_form": "그사람 스피드장난아니던데",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100006364.1.11",
          "form": "카디바? 첨들어봤어요",
          "original_form": "카디바? 첨들어봤어요",
          "speaker_id": "2",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100006364.1.12",
          "form": "씨름선수 귀엽게생겼어요",
          "original_form": "씨름선수 귀엽게생겼어요",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100006364.1.13",
          "form": "저도 그건첨들어봐요",
          "original_form": "저도 그건첨들어봐요",
          "speaker_id": "1",
          "time": "20210907 23:16"
        },
        {
          "id": "MDRW2100006364.1.14",
          "form": "다들 잘됐음 좋겠어요",
          "original_form": "다들 잘됐음 좋겠어요",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100006364.1.15",
          "form": "힘들게 운동하는 사람들인데",
          "original_form": "힘들게 운동하는 사람들인데",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100006364.1.16",
          "form": "일욜날 본방이예요?",
          "original_form": "일욜날 본방이예요?",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100006364.1.17",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100006364.1.18",
          "form": "언제하는지 모르고 틀다나옴봐서ㅋ",
          "original_form": "언제하는지 모르고 틀다나옴봐서ㅋ",
          "speaker_id": "1",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100006364.1.19",
          "form": "9시쯤? jtbc",
          "original_form": "9시쯤? jtbc",
          "speaker_id": "2",
          "time": "20210907 23:17"
        },
        {
          "id": "MDRW2100006364.1.20",
          "form": "챙겨봐야겠어요",
          "original_form": "챙겨봐야겠어요",
          "speaker_id": "1",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100006364.1.21",
          "form": "이제 재미있을꺼같아요",
          "original_form": "이제 재미있을꺼같아요",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100006364.1.22",
          "form": "슈퍼밴드도 잼나더라구요",
          "original_form": "슈퍼밴드도 잼나더라구요",
          "speaker_id": "1",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100006364.1.23",
          "form": "같이연습하고 경기하고~",
          "original_form": "같이연습하고 경기하고~",
          "speaker_id": "2",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100006364.1.24",
          "form": "주로 jtbc를 틀어놔서 근가ㅋ",
          "original_form": "주로 jtbc를 틀어놔서 근가ㅋ",
          "speaker_id": "1",
          "time": "20210907 23:18"
        },
        {
          "id": "MDRW2100006364.1.25",
          "form": "전 그건 안봤어요",
