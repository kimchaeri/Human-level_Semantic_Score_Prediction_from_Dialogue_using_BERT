      "utterance": [
        {
          "id": "MDRW2100034008.1.1",
          "form": "요즘 사고싶은거 있으세요?",
          "original_form": "요즘 사고싶은거 있으세요?",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100034008.1.2",
          "form": "저는 요즘 플립3요ㅠㅠㅠ",
          "original_form": "저는 요즘 플립3요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100034008.1.3",
          "form": "근데 지금 다이어트 도시락 보고있어요",
          "original_form": "근데 지금 다이어트 도시락 보고있어요",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100034008.1.4",
          "form": "드셔보신거 있으세요?",
          "original_form": "드셔보신거 있으세요?",
          "speaker_id": "2",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100034008.1.5",
          "form": "헛 뭐 사셨나요",
          "original_form": "헛 뭐 사셨나요",
          "speaker_id": "1",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100034008.1.6",
          "form": "도시락은 먹어본적 없내요..",
          "original_form": "도시락은 먹어본적 없내요..",
          "speaker_id": "1",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100034008.1.7",
          "form": "그냥 빠바 샐러드나 샐러디 시켜서 가끔 먹었어요",
          "original_form": "그냥 빠바 샐러드나 샐러디 시켜서 가끔 먹었어요",
          "speaker_id": "1",
          "time": "20210826 11:56"
        },
        {
          "id": "MDRW2100034008.1.8",
          "form": "오오 샐러디 어떤게 맛나나요?!",
          "original_form": "오오 샐러디 어떤게 맛나나요?!",
          "speaker_id": "2",
          "time": "20210826 12:12"
        },
        {
          "id": "MDRW2100034008.1.9",
          "form": "샐러드는 치킨샐러드 무난했고",
          "original_form": "샐러드는 치킨샐러드 무난했고",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100034008.1.10",
          "form": "맥시칸 랩... 이건 살좀 찌겠지만 엄청맛잇어요...",
          "original_form": "맥시칸 랩... 이건 살좀 찌겠지만 엄청맛잇어요...",
          "speaker_id": "1",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100034008.1.11",
          "form": "쓰읍 .... 저녁에 그거 먹어야 겠어요",
          "original_form": "쓰읍 .... 저녁에 그거 먹어야 겠어요",
          "speaker_id": "2",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100034008.1.12",
          "form": "혹시 당근하시나요?",
          "original_form": "혹시 당근하시나요?",
          "speaker_id": "2",
          "time": "20210826 19:03"
        },
        {
          "id": "MDRW2100034008.1.13",
          "form": "아 당근 많이하죠 ㅋㅋㅋㅋ",
          "original_form": "아 당근 많이하죠 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100034008.1.14",
          "form": "저녁으로 드셨나요?",
          "original_form": "저녁으로 드셨나요?",
          "speaker_id": "1",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100034008.1.15",
          "form": "아뇨 아직..저녁을",
          "original_form": "아뇨 아직..저녁을",
          "speaker_id": "2",
          "time": "20210826 20:09"
        },
        {
          "id": "MDRW2100034008.1.16",
          "form": "점심을 6시에 먹어서 10시가 저녁이라..",
          "original_form": "점심을 6시에 먹어서 10시가 저녁이라..",
          "speaker_id": "2",
          "time": "20210826 20:09"
        },
        {
          "id": "MDRW2100034008.1.17",
          "form": "잠을..한..4시에 자야겠어요",
          "original_form": "잠을..한..4시에 자야겠어요",
          "speaker_id": "2",
          "time": "20210826 20:09"
        },
        {
          "id": "MDRW2100034008.1.18",
          "form": "어이쿠 무슨일이에요 ㅋㅋㅋㅋㅋ",
          "original_form": "어이쿠 무슨일이에요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100034008.1.19",
          "form": "늦잠 주무셨나요?",
          "original_form": "늦잠 주무셨나요?",
          "speaker_id": "1",
          "time": "20210826 21:47"
        },
        {
          "id": "MDRW2100034008.1.20",
          "form": "넼ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "넼ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100034008.1.21",
          "form": "6시에 잠들어서",
          "original_form": "6시에 잠들어서",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100034008.1.22",
          "form": "그래서 하..6시 50분에 알람 맞췄어요",
          "original_form": "그래서 하..6시 50분에 알람 맞췄어요",
          "speaker_id": "2",
          "time": "20210826 21:50"
        },
        {
          "id": "MDRW2100034008.1.23",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100034008.1.24",
          "form": "그래도 푹 주무셨음 된거죠",
          "original_form": "그래도 푹 주무셨음 된거죠",
          "speaker_id": "1",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100034008.1.25",
          "form": "웹소 보셨나요 ㅎㅎ",
          "original_form": "웹소 보셨나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 21:52"
        },
        {
          "id": "MDRW2100034008.1.26",
          "form": "엇 아녀",
          "original_form": "엇 아녀",
          "speaker_id": "2",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100034008.1.27",
          "form": "이거하느라",
          "original_form": "이거하느라",
          "speaker_id": "2",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100034008.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100034008.1.29",
          "form": "너무 잘맞는 사람 만나서 극악의 밸런스겜하느라곸ㅋㅋㅋㅋ",
          "original_form": "너무 잘맞는 사람 만나서 극악의 밸런스겜하느라곸ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 21:53"
        },
        {
          "id": "MDRW2100034008.1.30",
          "form": "진자 웃기다ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "진자 웃기다ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100034008.1.31",
          "form": "밸런스게임 재밋죠 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "밸런스게임 재밋죠 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 21:55"
        },
        {
          "id": "MDRW2100034008.1.32",
