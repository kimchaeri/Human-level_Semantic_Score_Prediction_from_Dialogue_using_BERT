      "utterance": [
        {
          "id": "MDRW2100011275.1.1",
          "form": "아르바이트해 보신 적 있으신가요",
          "original_form": "아르바이트해 보신 적 있으신가요",
          "speaker_id": "1",
          "time": "20210824 11:23"
        },
        {
          "id": "MDRW2100011275.1.2",
          "form": "네  편의점도해보고 많이 해봤어요",
          "original_form": "네  편의점도해보고 많이 해봤어요",
          "speaker_id": "2",
          "time": "20210824 11:24"
        },
        {
          "id": "MDRW2100011275.1.3",
          "form": "편의점 알바는 어땠나요",
          "original_form": "편의점 알바는 어땠나요",
          "speaker_id": "1",
          "time": "20210824 11:24"
        },
        {
          "id": "MDRW2100011275.1.4",
          "form": "편의점 편했어요~ 지역마다",
          "original_form": "편의점 편했어요~ 지역마다",
          "speaker_id": "2",
          "time": "20210824 11:25"
        },
        {
          "id": "MDRW2100011275.1.5",
          "form": "편차가 있을듯해요",
          "original_form": "편차가 있을듯해요",
          "speaker_id": "2",
          "time": "20210824 11:25"
        },
        {
          "id": "MDRW2100011275.1.6",
          "form": "또 다른 건 어떤 걸 해봤나요",
          "original_form": "또 다른 건 어떤 걸 해봤나요",
          "speaker_id": "1",
          "time": "20210824 11:26"
        },
        {
          "id": "MDRW2100011275.1.7",
          "form": "목소리 녹음 알바 이런것도 해봤어요ㅎㅎ",
          "original_form": "목소리 녹음 알바 이런것도 해봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:29"
        },
        {
          "id": "MDRW2100011275.1.8",
          "form": "목소리 녹음 알바도 있어요?  신기하네요",
          "original_form": "목소리 녹음 알바도 있어요?  신기하네요",
          "speaker_id": "1",
          "time": "20210824 11:29"
        },
        {
          "id": "MDRW2100011275.1.9",
          "form": "네네 신기했어요 저도ㅎㅎ",
          "original_form": "네네 신기했어요 저도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100011275.1.10",
          "form": "목소리 녹음 알바는 주로 어떤 걸 녹음해요",
          "original_form": "목소리 녹음 알바는 주로 어떤 걸 녹음해요",
          "speaker_id": "1",
          "time": "20210824 11:31"
        },
        {
          "id": "MDRW2100011275.1.11",
          "form": "음 인쇄물 그냥 따라 읽으라 했었어요ㅋㅋㅋ",
          "original_form": "음 인쇄물 그냥 따라 읽으라 했었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100011275.1.12",
          "form": "인쇄물만 따라 읽으면 된다니 간단하네요",
          "original_form": "인쇄물만 따라 읽으면 된다니 간단하네요",
          "speaker_id": "1",
          "time": "20210824 11:32"
        },
        {
          "id": "MDRW2100011275.1.13",
          "form": "네 꿀알바였어요ㅎㅎ",
          "original_form": "네 꿀알바였어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:34"
        },
        {
          "id": "MDRW2100011275.1.14",
          "form": "저도 기회가 되면 한번 해보고 싶네요",
          "original_form": "저도 기회가 되면 한번 해보고 싶네요",
          "speaker_id": "1",
          "time": "20210824 11:35"
        }
      ]
