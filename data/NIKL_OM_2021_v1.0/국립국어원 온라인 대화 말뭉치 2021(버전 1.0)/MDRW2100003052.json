      "utterance": [
        {
          "id": "MDRW2100003052.1.1",
          "form": "주로 어떤 게임이나 웹툰 보세요?",
          "original_form": "주로 어떤 게임이나 웹툰 보세요?",
          "speaker_id": "1",
          "time": "20210820 19:49"
        },
        {
          "id": "MDRW2100003052.1.2",
          "form": "전 게임은 안하고요",
          "original_form": "전 게임은 안하고요",
          "speaker_id": "2",
          "time": "20210820 19:49"
        },
        {
          "id": "MDRW2100003052.1.3",
          "form": "웹툰은 카카오에서 최신 뜨면 보는데",
          "original_form": "웹툰은 카카오에서 최신 뜨면 보는데",
          "speaker_id": "2",
          "time": "20210820 19:49"
        },
        {
          "id": "MDRW2100003052.1.4",
          "form": "소설 더 좋아해요",
          "original_form": "소설 더 좋아해요",
          "speaker_id": "2",
          "time": "20210820 19:50"
        },
        {
          "id": "MDRW2100003052.1.5",
          "form": "무슨 게임하세요?",
          "original_form": "무슨 게임하세요?",
          "speaker_id": "2",
          "time": "20210820 19:51"
        },
        {
          "id": "MDRW2100003052.1.6",
          "form": "모바일은 2048 이나 머리좋아지는 게임을 주로 해요. (문제풀기 등등)",
          "original_form": "모바일은 2048 이나 머리좋아지는 게임을 주로 해요. (문제풀기 등등)",
          "speaker_id": "1",
          "time": "20210820 19:52"
        },
        {
          "id": "MDRW2100003052.1.7",
          "form": "아 그런것도 있었군요",
          "original_form": "아 그런것도 있었군요",
          "speaker_id": "2",
          "time": "20210820 19:52"
        },
        {
          "id": "MDRW2100003052.1.8",
          "form": "모바일은 2048이라는 게임 주로 해요",
          "original_form": "모바일은 2048이라는 게임 주로 해요",
          "speaker_id": "1",
          "time": "20210820 19:52"
        },
        {
          "id": "MDRW2100003052.1.9",
          "form": "머리 좋아 지는거 솔깃 하네요",
          "original_form": "머리 좋아 지는거 솔깃 하네요",
          "speaker_id": "2",
          "time": "20210820 19:52"
        },
        {
          "id": "MDRW2100003052.1.10",
          "form": "무슨 게임인데요?",
          "original_form": "무슨 게임인데요?",
          "speaker_id": "2",
          "time": "20210820 19:52"
        },
        {
          "id": "MDRW2100003052.1.11",
          "form": "전 진짜 게임을 태어나서 그 같은모양 깨는거 만 해봤네요 ㅋㅌ",
          "original_form": "전 진짜 게임을 태어나서 그 같은모양 깨는거 만 해봤네요 ㅋㅌ",
          "speaker_id": "2",
          "time": "20210820 19:53"
        },
        {
          "id": "MDRW2100003052.1.12",
          "form": "brainilis라는 게임인데, 다양한 문제를 풀 수 있어서 재미있어요",
          "original_form": "brainilis라는 게임인데, 다양한 문제를 풀 수 있어서 재미있어요",
          "speaker_id": "1",
          "time": "20210820 19:53"
        },
        {
          "id": "MDRW2100003052.1.13",
          "form": "오 문제 잘 푸시나봐요",
          "original_form": "오 문제 잘 푸시나봐요",
          "speaker_id": "2",
          "time": "20210820 19:54"
        },
        {
          "id": "MDRW2100003052.1.14",
          "form": "전 문제 약해요 ㅋㅋ",
          "original_form": "전 문제 약해요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 19:54"
        },
        {
          "id": "MDRW2100003052.1.15",
          "form": "첨엔 어려웠는데 할 수록 조금씩 늘더라구요. name2.님도 심심할때 한번 해보세요~",
          "original_form": "첨엔 어려웠는데 할 수록 조금씩 늘더라구요. &name2&.님도 심심할때 한번 해보세요~",
          "speaker_id": "1",
          "time": "20210820 19:55"
        },
        {
          "id": "MDRW2100003052.1.16",
          "form": "네 늘수도 있군요 도전해 보겠습니다",
          "original_form": "네 늘수도 있군요 도전해 보겠습니다",
          "speaker_id": "2",
          "time": "20210820 19:55"
        },
        {
          "id": "MDRW2100003052.1.17",
          "form": "예전에 그 카드 맞추는건 저주 했었어요",
          "original_form": "예전에 그 카드 맞추는건 저주 했었어요",
          "speaker_id": "2",
          "time": "20210820 19:56"
        },
        {
          "id": "MDRW2100003052.1.18",
          "form": "자주 ㅋㅋ",
          "original_form": "자주 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 19:57"
        },
        {
          "id": "MDRW2100003052.1.19",
          "form": "카드맞추는 게임이름이 뭐에요?",
          "original_form": "카드맞추는 게임이름이 뭐에요?",
          "speaker_id": "1",
          "time": "20210820 19:57"
        },
        {
          "id": "MDRW2100003052.1.20",
          "form": "저주가 되버렸네요ㅋㅋ",
          "original_form": "저주가 되버렸네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 19:57"
        },
        {
          "id": "MDRW2100003052.1.21",
          "form": "갑자기 생각이 안나네요",
          "original_form": "갑자기 생각이 안나네요",
          "speaker_id": "2",
          "time": "20210820 19:57"
        },
        {
          "id": "MDRW2100003052.1.22",
          "form": "윈도우에 기본으로 깔려있던거요",
          "original_form": "윈도우에 기본으로 깔려있던거요",
          "speaker_id": "2",
          "time": "20210820 19:57"
        },
        {
          "id": "MDRW2100003052.1.23",
          "form": "지뢰게임이랑 같이요",
          "original_form": "지뢰게임이랑 같이요",
          "speaker_id": "2",
          "time": "20210820 19:58"
        },
        {
          "id": "MDRW2100003052.1.24",
          "form": "죄송해요 오래전이라 기억이 안나요",
          "original_form": "죄송해요 오래전이라 기억이 안나요",
          "speaker_id": "2",
          "time": "20210820 19:59"
        },
        {
          "id": "MDRW2100003052.1.25",
          "form": "혹시 solitaire 이지 않나요?",
          "original_form": "혹시 solitaire 이지 않나요?",
          "speaker_id": "1",
          "time": "20210820 19:59"
        },
        {
          "id": "MDRW2100003052.1.26",
          "form": "맞아요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 19:59"
        },
        {
          "id": "MDRW2100003052.1.27",
          "form": "참 예전에 사무실에서 몰래 했네요",
          "original_form": "참 예전에 사무실에서 몰래 했네요",
          "speaker_id": "2",
          "time": "20210820 20:00"
        },
        {
          "id": "MDRW2100003052.1.28",
          "form": "헉 2048 깜놀인데요",
          "original_form": "헉 2048 깜놀인데요",
          "speaker_id": "2",
          "time": "20210820 20:03"
        },
        {
          "id": "MDRW2100003052.1.29",
          "form": "고단수 게임 이네요",
          "original_form": "고단수 게임 이네요",
          "speaker_id": "2",
          "time": "20210820 20:03"
        },
        {
          "id": "MDRW2100003052.1.30",
          "form": "저 금방 네이버에서 봤는데 2048 보고 놀랐어요",
          "original_form": "저 금방 네이버에서 봤는데 2048 보고 놀랐어요",
          "speaker_id": "2",
          "time": "20210820 20:04"
        },
        {
          "id": "MDRW2100003052.1.31",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "1",
          "time": "20210820 20:16"
        },
        {
          "id": "MDRW2100003052.1.32",
          "form": "어려워 보여요",
          "original_form": "어려워 보여요",
          "speaker_id": "2",
          "time": "20210820 20:17"
        },
        {
          "id": "MDRW2100003052.1.33",
          "form": "정말 능력자 시네요 ㅎㅎ",
          "original_form": "정말 능력자 시네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 20:17"
        },
        {
          "id": "MDRW2100003052.1.34",
          "form": "부끄럽습니다. 2048도 처음에 어려웠었어요",
          "original_form": "부끄럽습니다. 2048도 처음에 어려웠었어요",
          "speaker_id": "1",
          "time": "20210820 20:19"
        },
        {
          "id": "MDRW2100003052.1.35",
          "form": "전 진입장벽 느끼고 하차했을거예요 ㅋㅋ",
          "original_form": "전 진입장벽 느끼고 하차했을거예요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:19"
        },
        {
          "id": "MDRW2100003052.1.36",
          "form": "저는 solitaire가 너무 어려워서 ㅋㅋ 아직 시작못했어요",
          "original_form": "저는 solitaire가 너무 어려워서 ㅋㅋ 아직 시작못했어요",
          "speaker_id": "1",
          "time": "20210820 20:20"
        },
        {
          "id": "MDRW2100003052.1.37",
          "form": "에이 잘하실거예요",
          "original_form": "에이 잘하실거예요",
          "speaker_id": "2",
          "time": "20210820 20:20"
        },
        {
          "id": "MDRW2100003052.1.38",
          "form": "요거 요령만 알면 금새 하더라구요",
          "original_form": "요거 요령만 알면 금새 하더라구요",
          "speaker_id": "2",
          "time": "20210820 20:20"
        },
        {
          "id": "MDRW2100003052.1.39",
          "form": "왠지 달인 되실듯",
          "original_form": "왠지 달인 되실듯",
          "speaker_id": "2",
          "time": "20210820 20:20"
        },
        {
          "id": "MDRW2100003052.1.40",
          "form": "에고 겸손의 말씀. 격려 고마와요",
          "original_form": "에고 겸손의 말씀. 격려 고마와요",
          "speaker_id": "1",
          "time": "20210820 20:21"
        },
        {
          "id": "MDRW2100003052.1.41",
          "form": "네 ㅎㅎ 고고하셔요",
          "original_form": "네 ㅎㅎ 고고하셔요",
          "speaker_id": "2",
          "time": "20210820 20:21"
        },
        {
          "id": "MDRW2100003052.1.42",
          "form": "네 한번 해볼게요 ㅎㅎ",
          "original_form": "네 한번 해볼게요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 20:22"
        },
        {
          "id": "MDRW2100003052.1.43",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 20:22"
        },
        {
          "id": "MDRW2100003052.1.44",
          "form": "저도 올만에 도전해서 해봐야겠네요 다까먹어서 되려나 몰라요",
          "original_form": "저도 올만에 도전해서 해봐야겠네요 다까먹어서 되려나 몰라요",
          "speaker_id": "2",
