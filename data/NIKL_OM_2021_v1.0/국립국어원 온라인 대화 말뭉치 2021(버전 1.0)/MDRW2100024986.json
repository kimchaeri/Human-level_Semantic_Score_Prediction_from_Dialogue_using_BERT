        },
        {
          "id": "MDRW2100024986.1.2",
          "form": "우와... 신기한색이네요 ㅋㄱㅋㅋㅋ",
          "original_form": "우와... 신기한색이네요 ㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:48"
        },
        {
          "id": "MDRW2100024986.1.3",
