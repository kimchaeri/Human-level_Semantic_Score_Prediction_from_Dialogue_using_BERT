        },
        {
          "id": "MDRW2100004158.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210821 15:19"
        },
        {
          "id": "MDRW2100004158.1.3",
          "form": "좋아하는 음식 있으신가요?",
          "original_form": "좋아하는 음식 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004158.1.4",
          "form": "전 치킨을 제일 좋아해요!",
          "original_form": "전 치킨을 제일 좋아해요!",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004158.1.5",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210821 15:20"
        },
        {
          "id": "MDRW2100004158.1.6",
          "form": "저는 피자요! 빵도 좋아하고 면도 좋아하고..",
          "original_form": "저는 피자요! 빵도 좋아하고 면도 좋아하고..",
          "speaker_id": "2",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004158.1.7",
          "form": "밀가루를 좋아하는 것 같아요",
          "original_form": "밀가루를 좋아하는 것 같아요",
          "speaker_id": "2",
          "time": "20210821 15:21"
        },
        {
          "id": "MDRW2100004158.1.8",
          "form": "밀가루 좋죠",
          "original_form": "밀가루 좋죠",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004158.1.9",
          "form": "저도 빵 좋아해요",
          "original_form": "저도 빵 좋아해요",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004158.1.10",
          "form": "10분전에 그 파리바게트",
          "original_form": "10분전에 그 파리바게트",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004158.1.11",
          "form": "연유브레드 먹었어요",
          "original_form": "연유브레드 먹었어요",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004158.1.12",
          "form": "일어나자마자!",
          "original_form": "일어나자마자!",
          "speaker_id": "1",
          "time": "20210821 15:23"
        },
        {
          "id": "MDRW2100004158.1.13",
          "form": "헐 연유브레드ㅜㅠ",
          "original_form": "헐 연유브레드ㅜㅠ",
          "speaker_id": "2",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004158.1.14",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "2",
          "time": "20210821 15:24"
        },
        {
          "id": "MDRW2100004158.1.15",
          "form": "치즈타르트도 좋아하고..",
          "original_form": "치즈타르트도 좋아하고..",
          "speaker_id": "2",
          "time": "20210821 15:25"
        },
        {
          "id": "MDRW2100004158.1.16",
          "form": "치즈타르트 좋죠 단짠단짠",
          "original_form": "치즈타르트 좋죠 단짠단짠",
          "speaker_id": "1",
          "time": "20210821 15:27"
        },
        {
          "id": "MDRW2100004158.1.17",
          "form": "어후 먹고싶다ㅠㅠ",
          "original_form": "어후 먹고싶다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004158.1.18",
          "form": "맛집탐방 자주 다니시나요?",
          "original_form": "맛집탐방 자주 다니시나요?",
          "speaker_id": "1",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004158.1.19",
          "form": "저는 가끔 다니는데",
          "original_form": "저는 가끔 다니는데",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004158.1.20",
          "form": "요즘은 코로나 때문에 잘 안 가요ㅠ",
          "original_form": "요즘은 코로나 때문에 잘 안 가요ㅠ",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004158.1.21",
          "form": "배달음식만 시키고..",
          "original_form": "배달음식만 시키고..",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004158.1.22",
          "form": "그쵸 저도 그래요ㅠㅜ",
          "original_form": "그쵸 저도 그래요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210821 15:40"
        },
        {
          "id": "MDRW2100004158.1.23",
          "form": "전 코로나 전에도 집순이라 맛집 잘안다녔는데",
          "original_form": "전 코로나 전에도 집순이라 맛집 잘안다녔는데",
          "speaker_id": "1",
          "time": "20210821 15:40"
        },
        {
          "id": "MDRW2100004158.1.24",
          "form": "요즘 배달음식 엄청 시켜먹게 되더라구요",
          "original_form": "요즘 배달음식 엄청 시켜먹게 되더라구요",
          "speaker_id": "1",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004158.1.25",
          "form": "그쵸.. 배민을 손에서 놓을 수가 없어요ㅋㅋㅋ",
          "original_form": "그쵸.. 배민을 손에서 놓을 수가 없어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 16:44"
        },
        {
          "id": "MDRW2100004158.1.26",
          "form": "맞아요 간편하게 배달 오구ㅜ",
          "original_form": "맞아요 간편하게 배달 오구ㅜ",
          "speaker_id": "1",
          "time": "20210821 16:49"
        },
        {
          "id": "MDRW2100004158.1.27",
          "form": "좋아하시는 배달 음식 있어요?",
          "original_form": "좋아하시는 배달 음식 있어요?",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004158.1.28",
          "form": "저는 떡볶이 좋아하는데",
          "original_form": "저는 떡볶이 좋아하는데",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004158.1.29",
          "form": "배달시키면 뒷처리가 넘 힘들더라구요ㅋㅋㅜㅜ",
          "original_form": "배달시키면 뒷처리가 넘 힘들더라구요ㅋㅋㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 16:53"
        },
        {
          "id": "MDRW2100004158.1.30",
          "form": "저 치킨이랑 족발 떡볶이 이케 자주 시켜먹어용",
          "original_form": "저 치킨이랑 족발 떡볶이 이케 자주 시켜먹어용",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004158.1.31",
          "form": "헐 맞아요 게다가 양도 엄청 많아가지고...",
          "original_form": "헐 맞아요 게다가 양도 엄청 많아가지고...",
          "speaker_id": "1",
          "time": "20210821 17:10"
        },
        {
          "id": "MDRW2100004158.1.32",
          "form": "ㅋㅋㅋㅋㅋ 맞아요 그래서 자주 1인분만 포장해와요",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요 그래서 자주 1인분만 포장해와요",
          "speaker_id": "2",
          "time": "20210821 17:30"
        },
        {
          "id": "MDRW2100004158.1.33",
          "form": "그게 훨씬 낫더라구요.. 배달팁 부담도 덜 되고",
          "original_form": "그게 훨씬 낫더라구요.. 배달팁 부담도 덜 되고",
          "speaker_id": "2",
          "time": "20210821 17:31"
        },
        {
          "id": "MDRW2100004158.1.34",
          "form": "헐 글쿤요 저는 재탕해서 계속 먹어왔었는데 나중에 그렇게 해볼게요",
          "original_form": "헐 글쿤요 저는 재탕해서 계속 먹어왔었는데 나중에 그렇게 해볼게요",
          "speaker_id": "1",
          "time": "20210821 17:32"
        }
      ]
