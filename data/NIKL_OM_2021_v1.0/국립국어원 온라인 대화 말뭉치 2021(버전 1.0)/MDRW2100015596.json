      "utterance": [
        {
          "id": "MDRW2100015596.1.1",
          "form": "영양제 챙겨드세요?",
          "original_form": "영양제 챙겨드세요?",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015596.1.2",
          "form": "넵ㅎ",
          "original_form": "넵ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015596.1.3",
          "form": "꼭챙겨먹어요",
          "original_form": "꼭챙겨먹어요",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015596.1.4",
          "form": "드시는거있으세요?",
          "original_form": "드시는거있으세요?",
          "speaker_id": "2",
          "time": "20210825 23:45"
        },
        {
          "id": "MDRW2100015596.1.5",
          "form": "비타민하고 루테인이요",
          "original_form": "비타민하고 루테인이요",
          "speaker_id": "1",
          "time": "20210825 23:46"
        },
        {
          "id": "MDRW2100015596.1.6",
          "form": "저도요 필수영양제,,.",
          "original_form": "저도요 필수영양제,,.",
          "speaker_id": "2",
          "time": "20210825 23:57"
        },
        {
          "id": "MDRW2100015596.1.7",
          "form": "유산균하구용ㅋㅋ",
          "original_form": "유산균하구용ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:58"
        },
        {
          "id": "MDRW2100015596.1.8",
          "form": "어 맞아요",
          "original_form": "어 맞아요",
          "speaker_id": "2",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100015596.1.9",
          "form": "다이어트도 하시나요",
          "original_form": "다이어트도 하시나요",
          "speaker_id": "2",
          "time": "20210826 13:03"
        },
        {
          "id": "MDRW2100015596.1.10",
          "form": "아니요 저는 알아서 찌고 빠져요 신경안써요 별님은요",
          "original_form": "아니요 저는 알아서 찌고 빠져요 신경안써요 별님은요",
          "speaker_id": "1",
          "time": "20210826 13:04"
        },
        {
          "id": "MDRW2100015596.1.11",
          "form": "저도 그냥 먹고싶은거 먹어요ㅋㅋ",
          "original_form": "저도 그냥 먹고싶은거 먹어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:05"
        },
        {
          "id": "MDRW2100015596.1.12",
          "form": "저는 겨울에 찌우고 여름에 빼요",
          "original_form": "저는 겨울에 찌우고 여름에 빼요",
          "speaker_id": "1",
          "time": "20210826 14:07"
        },
        {
          "id": "MDRW2100015596.1.13",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210826 14:15"
        },
        {
          "id": "MDRW2100015596.1.14",
          "form": "이제 찌울타이밍이네요",
          "original_form": "이제 찌울타이밍이네요",
          "speaker_id": "2",
          "time": "20210826 14:15"
        },
        {
          "id": "MDRW2100015596.1.15",
          "form": "네 근데 친구들이 그냥 추운돼지라고",
          "original_form": "네 근데 친구들이 그냥 추운돼지라고",
          "speaker_id": "1",
          "time": "20210826 14:24"
        },
        {
          "id": "MDRW2100015596.1.16",
          "form": "그러지말고 근육키우래요ㅎㅎ",
          "original_form": "그러지말고 근육키우래요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 14:24"
        },
        {
          "id": "MDRW2100015596.1.17",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 14:29"
        },
        {
          "id": "MDRW2100015596.1.18",
          "form": "아니 지방이 더 따뜻하잖아요",
          "original_form": "아니 지방이 더 따뜻하잖아요",
          "speaker_id": "2",
          "time": "20210826 14:29"
        },
        {
          "id": "MDRW2100015596.1.19",
          "form": "별님도 운동하세요",
          "original_form": "별님도 운동하세요",
          "speaker_id": "1",
          "time": "20210826 14:29"
        },
        {
          "id": "MDRW2100015596.1.20",
          "form": "가끔 홈트만해요",
          "original_form": "가끔 홈트만해요",
          "speaker_id": "2",
          "time": "20210826 14:29"
        }
      ]
