      "utterance": [
        {
          "id": "MDRW2100044491.1.1",
          "form": "안녕하세요 좋아하는 음 있나요?ㅎ",
          "original_form": "안녕하세요 좋아하는 음 있나요?ㅎ",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044491.1.2",
          "form": "음식이요!",
          "original_form": "음식이요!",
          "speaker_id": "1",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044491.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044491.1.4",
          "form": "네 저는 떡볶이 좋아해요",
          "original_form": "네 저는 떡볶이 좋아해요",
          "speaker_id": "2",
          "time": "20210907 22:53"
        },
        {
          "id": "MDRW2100044491.1.5",
          "form": "와 떡볶이 맛있죠 최애떡볶이집 있나요?",
          "original_form": "와 떡볶이 맛있죠 최애떡볶이집 있나요?",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044491.1.6",
          "form": "최애가 매번 달라져요 ㅋ",
          "original_form": "최애가 매번 달라져요 ㅋ",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044491.1.7",
          "form": "ㅋㅋ 저는 요즘 명랑핫도그 떡볶이가 맛있더라고요~",
          "original_form": "ㅋㅋ 저는 요즘 명랑핫도그 떡볶이가 맛있더라고요~",
          "speaker_id": "1",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044491.1.8",
          "form": "저는 쌀떡파라서 쌀떡파는걸 좋아하는데요",
          "original_form": "저는 쌀떡파라서 쌀떡파는걸 좋아하는데요",
          "speaker_id": "2",
          "time": "20210907 22:54"
        },
        {
          "id": "MDRW2100044491.1.9",
          "form": "와 쌀떡 쫄깃해서 맛있죠 ㅎ",
          "original_form": "와 쌀떡 쫄깃해서 맛있죠 ㅎ",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044491.1.10",
          "form": "밀떡도잘 먹어요 ㅋ",
          "original_form": "밀떡도잘 먹어요 ㅋ",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044491.1.11",
          "form": "명랑핫도그떡볶이도 시켜먹어봤어요",
          "original_form": "명랑핫도그떡볶이도 시켜먹어봤어요",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044491.1.12",
          "form": "ㅋㅋㅋ 떡볶이 좋아하는 분들은 그거 거의 안가리고 드시는 거 같아요",
          "original_form": "ㅋㅋㅋ 떡볶이 좋아하는 분들은 그거 거의 안가리고 드시는 거 같아요",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044491.1.13",
          "form": "거기 맛있지 않나요? ㅎㅎ",
          "original_form": "거기 맛있지 않나요? ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044491.1.14",
          "form": "네 맛있더라구요 ㅋ",
          "original_form": "네 맛있더라구요 ㅋ",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044491.1.15",
          "form": "핫도그랑 같이먹을수 있어서도 좋구요",
          "original_form": "핫도그랑 같이먹을수 있어서도 좋구요",
          "speaker_id": "2",
          "time": "20210907 22:55"
        },
        {
          "id": "MDRW2100044491.1.16",
          "form": "저는 감탄떡볶이도 좋아하고",
          "original_form": "저는 감탄떡볶이도 좋아하고",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044491.1.17",
          "form": "맞아요 그게 제일 좋더라고요",
          "original_form": "맞아요 그게 제일 좋더라고요",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044491.1.18",
          "form": "감탄 떡볶이 안 먹어봤는데 맛있어요?",
          "original_form": "감탄 떡볶이 안 먹어봤는데 맛있어요?",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044491.1.19",
          "form": "우리할매떡볶이도 좋아하고",
          "original_form": "우리할매떡볶이도 좋아하고",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044491.1.20",
          "form": "감탄도 맛있어요 쌀떡밀떡 다팔아요",
          "original_form": "감탄도 맛있어요 쌀떡밀떡 다팔아요",
          "speaker_id": "2",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044491.1.21",
          "form": "거기 떡이 안자른 통 가래떡 맞죠?",
          "original_form": "거기 떡이 안자른 통 가래떡 맞죠?",
          "speaker_id": "1",
          "time": "20210907 22:56"
        },
        {
          "id": "MDRW2100044491.1.22",
          "form": "우리할매떡볶이요 ㅎㅎ",
          "original_form": "우리할매떡볶이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044491.1.23",
          "form": "아하 거기는 두개 다 파능군요 먹어봐야겠네요 ㅎ",
          "original_form": "아하 거기는 두개 다 파능군요 먹어봐야겠네요 ㅎ",
          "speaker_id": "1",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044491.1.24",
          "form": "맞아요 ㅋ!!",
          "original_form": "맞아요 ㅋ!!",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044491.1.25",
          "form": "안자르고 먹는데 한입에 가득차게 먹으니 맛있더라구요",
          "original_form": "안자르고 먹는데 한입에 가득차게 먹으니 맛있더라구요",
          "speaker_id": "2",
          "time": "20210907 22:57"
        },
        {
          "id": "MDRW2100044491.1.26",
          "form": "아 진짜 맛있겠아요 저도 떡볶이 너무 좋아하고든요",
          "original_form": "아 진짜 맛있겠아요 저도 떡볶이 너무 좋아하고든요",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044491.1.27",
          "form": "최근엔 걸작떡볶이에서 먹었어요",
          "original_form": "최근엔 걸작떡볶이에서 먹었어요",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044491.1.28",
          "form": "여긴 치킨하고 같이 배달이요 ㅋ",
          "original_form": "여긴 치킨하고 같이 배달이요 ㅋ",
          "speaker_id": "2",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044491.1.29",
          "form": "걸작 떡볶이도 들어봤는데 안시켜봤어요 와 치킨도 맛 괭찮나요?!",
          "original_form": "걸작 떡볶이도 들어봤는데 안시켜봤어요 와 치킨도 맛 괭찮나요?!",
          "speaker_id": "1",
          "time": "20210907 22:58"
        },
        {
          "id": "MDRW2100044491.1.30",
          "form": "엄청은 아닌데 먹을만해요 ㅋ 소스도 여러개있어서 ㅋㅋ 취향대로 ㅋㅋ",
          "original_form": "엄청은 아닌데 먹을만해요 ㅋ 소스도 여러개있어서 ㅋㅋ 취향대로 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 22:59"
        },
        {
          "id": "MDRW2100044491.1.31",
          "form": "ㅋㅋㅋㅋ 소스가 여러개면 더 좋겠네요~!",
          "original_form": "ㅋㅋㅋㅋ 소스가 여러개면 더 좋겠네요~!",
          "speaker_id": "1",
          "time": "20210907 22:59"
        }
      ]
