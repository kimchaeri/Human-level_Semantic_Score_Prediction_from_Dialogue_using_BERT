        },
        {
          "id": "MDRW2100030060.1.2",
          "form": "동호회같은 거 하시나요?",
          "original_form": "동호회같은 거 하시나요?",
          "speaker_id": "1",
          "time": "20210903 20:52"
        },
        {
          "id": "MDRW2100030060.1.3",
          "form": "안녕하세요:)",
          "original_form": "안녕하세요:)",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100030060.1.4",
          "form": "동호회는 없네염",
          "original_form": "동호회는 없네염",
          "speaker_id": "2",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100030060.1.5",
          "form": "앗 그렇구나...사실 저두요...",
          "original_form": "앗 그렇구나...사실 저두요...",
          "speaker_id": "1",
          "time": "20210903 20:53"
        },
        {
          "id": "MDRW2100030060.1.6",
          "form": "대학생 때도 동아리두 안 했어요 집순이라...",
          "original_form": "대학생 때도 동아리두 안 했어요 집순이라...",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100030060.1.7",
          "form": "그럼 혹시 봉사같은 거 하시나용?",
          "original_form": "그럼 혹시 봉사같은 거 하시나용?",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100030060.1.8",
          "form": "것도 아니요....ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "것도 아니요....ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100030060.1.9",
          "form": "대신 봉사단체 후원은 했습니당ㅋㅋ",
          "original_form": "대신 봉사단체 후원은 했습니당ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100030060.1.10",
          "form": "우와 멋져요 뭐 후원하셨나요?",
          "original_form": "우와 멋져요 뭐 후원하셨나요?",
          "speaker_id": "1",
          "time": "20210903 20:54"
        },
        {
          "id": "MDRW2100030060.1.11",
          "form": "저는 월드비젼이요:)",
          "original_form": "저는 월드비젼이요:)",
          "speaker_id": "2",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100030060.1.12",
          "form": "우왕...멋져요. 저는 대학생 때 교육봉사나 조금 했던 거 같아요ㅋㅋㅋㅋㅋ...ㅠㅠ",
          "original_form": "우왕...멋져요. 저는 대학생 때 교육봉사나 조금 했던 거 같아요ㅋㅋㅋㅋㅋ...ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100030060.1.13",
          "form": "오 교육봉사!",
          "original_form": "오 교육봉사!",
          "speaker_id": "2",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100030060.1.14",
          "form": "과에서 했던건가요?",
          "original_form": "과에서 했던건가요?",
          "speaker_id": "2",
          "time": "20210903 20:58"
        },
        {
          "id": "MDRW2100030060.1.15",
          "form": "넵ㅋㅋㅋㅋㅋㅋ 졸업요건이어서요...",
          "original_form": "넵ㅋㅋㅋㅋㅋㅋ 졸업요건이어서요...",
          "speaker_id": "1",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100030060.1.16",
          "form": "ㅋㅋ얼마동안이나 하셨어여?",
          "original_form": "ㅋㅋ얼마동안이나 하셨어여?",
          "speaker_id": "2",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100030060.1.17",
          "form": "한....60시간 채워야했던 거 같아요",
          "original_form": "한....60시간 채워야했던 거 같아요",
          "speaker_id": "1",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100030060.1.18",
          "form": "지역 아동센터 3주간 출퇴근 했거든요ㅋㅋㅋㅋ",
          "original_form": "지역 아동센터 3주간 출퇴근 했거든요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100030060.1.19",
          "form": "3주면은\r\n아이들하고도 정들었겠어영",
          "original_form": "3주면은\r\n아이들하고도 정들었겠어영",
          "speaker_id": "2",
          "time": "20210903 21:01"
        },
        {
          "id": "MDRW2100030060.1.20",
          "form": "마자요ㅋㅋㅋㅋㅋ...근데 애들은 쿨하더라구요",
          "original_form": "마자요ㅋㅋㅋㅋㅋ...근데 애들은 쿨하더라구요",
          "speaker_id": "1",
          "time": "20210903 21:03"
        },
        {
          "id": "MDRW2100030060.1.21",
          "form": "사실 학원알바도 좀 했었는데",
          "original_form": "사실 학원알바도 좀 했었는데",
          "speaker_id": "1",
          "time": "20210903 21:03"
        },
        {
          "id": "MDRW2100030060.1.22",
          "form": "애들은 이별에 늘 쿨한 거 같아요ㅋㅋㅋㅋ",
          "original_form": "애들은 이별에 늘 쿨한 거 같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:03"
        },
        {
          "id": "MDRW2100030060.1.23",
          "form": "전 어릴때도\r\n쿨하지 못했는데",
          "original_form": "전 어릴때도\r\n쿨하지 못했는데",
          "speaker_id": "2",
          "time": "20210903 21:03"
        },
        {
          "id": "MDRW2100030060.1.24",
