      "utterance": [
        {
          "id": "MDRW2100030150.1.1",
          "form": "이브 정말 추천드려요",
          "original_form": "이브 정말 추천드려요",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030150.1.2",
          "form": "명작 쯔꾸르라고 하는 이유가 있는게임..",
          "original_form": "명작 쯔꾸르라고 하는 이유가 있는게임..",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030150.1.3",
          "form": "오~~~~",
          "original_form": "오~~~~",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030150.1.4",
          "form": "사실 궁금하긴 했어",
          "original_form": "사실 궁금하긴 했어",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030150.1.5",
          "form": "점프스케어는 일단 거의 없고",
          "original_form": "점프스케어는 일단 거의 없고",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030150.1.6",
          "form": "무서워서 여태 안핶지..",
          "original_form": "무서워서 여태 안핶지..",
          "speaker_id": "1",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030150.1.7",
          "form": "스토리가 죽여주고",
          "original_form": "스토리가 죽여주고",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030150.1.8",
          "form": "이브..무섭긴한데 내가보기엔 그렇게 무섭지는 않아요",
          "original_form": "이브..무섭긴한데 내가보기엔 그렇게 무섭지는 않아요",
          "speaker_id": "2",
          "time": "20210903 23:17"
        },
        {
          "id": "MDRW2100030150.1.9",
          "form": "쯔꾸르 특유의 시스템같은걸 정말 잘 살렸고",
          "original_form": "쯔꾸르 특유의 시스템같은걸 정말 잘 살렸고",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030150.1.10",
          "form": "주요장소가 미술관인데",
          "original_form": "주요장소가 미술관인데",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030150.1.11",
          "form": "와 미술관 좋아",
          "original_form": "와 미술관 좋아",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030150.1.12",
          "form": "액자? 어쩌고? 하던데",
          "original_form": "액자? 어쩌고? 하던데",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030150.1.13",
          "form": "그런데 이 미술관 분위기를 진짜",
          "original_form": "그런데 이 미술관 분위기를 진짜",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030150.1.14",
          "form": "잘만들었거든",
          "original_form": "잘만들었거든",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030150.1.15",
          "form": "웅웅 주여 스토리는",
          "original_form": "웅웅 주여 스토리는",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030150.1.16",
          "form": "오~~~~~",
          "original_form": "오~~~~~",
          "speaker_id": "1",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030150.1.17",
          "form": "주요스토리는",
          "original_form": "주요스토리는",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030150.1.18",
          "form": "이브가 엄마아빠 손잡고 '게르테나'라는 화가의 미술전에 갔는데",
          "original_form": "이브가 엄마아빠 손잡고 '게르테나'라는 화가의 미술전에 갔는데",
          "speaker_id": "2",
          "time": "20210903 23:18"
        },
        {
          "id": "MDRW2100030150.1.19",
          "form": "어느샌가 사람들이 전부 없어져버리고",
          "original_form": "어느샌가 사람들이 전부 없어져버리고",
          "speaker_id": "2",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030150.1.20",
          "form": "헐잠만 키워드 식재료야",
          "original_form": "헐잠만 키워드 식재료야",
          "speaker_id": "1",
          "time": "20210903 23:19"
        },
        {
          "id": "MDRW2100030150.1.21",
          "form": "현실세께와는 다른 .또다른 미술관에 갇혀서",
          "original_form": "현실세께와는 다른 .또다른 미술관에 갇혀서",
          "speaker_id": "2",
          "time": "20210903 23:19"
        }
