      "utterance": [
        {
          "id": "MDRW2100022742.1.1",
          "form": "식사는요?",
          "original_form": "식사는요?",
          "speaker_id": "1",
          "time": "20210829 17:33"
        },
        {
          "id": "MDRW2100022742.1.2",
          "form": "점심 먹었습니다.",
          "original_form": "점심 먹었습니다.",
          "speaker_id": "2",
          "time": "20210829 17:34"
        },
        {
          "id": "MDRW2100022742.1.3",
          "form": "메뉴는요?",
          "original_form": "메뉴는요?",
          "speaker_id": "1",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022742.1.4",
          "form": "이제 저녁먹어야할 시간이네요.",
          "original_form": "이제 저녁먹어야할 시간이네요.",
          "speaker_id": "2",
          "time": "20210829 17:35"
        },
        {
          "id": "MDRW2100022742.1.5",
          "form": "저녁은 뭐로 드실건가요?",
          "original_form": "저녁은 뭐로 드실건가요?",
          "speaker_id": "2",
          "time": "20210829 17:36"
        },
        {
          "id": "MDRW2100022742.1.6",
          "form": "만두요",
          "original_form": "만두요",
          "speaker_id": "1",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100022742.1.7",
          "form": "만두 맛있죠!",
          "original_form": "만두 맛있죠!",
          "speaker_id": "2",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100022742.1.8",
          "form": "아 참고로 점심에는 라면먹었습니다.",
          "original_form": "아 참고로 점심에는 라면먹었습니다.",
          "speaker_id": "2",
          "time": "20210829 17:37"
        },
        {
          "id": "MDRW2100022742.1.9",
          "form": "전 칼국수여",
          "original_form": "전 칼국수여",
          "speaker_id": "1",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022742.1.10",
          "form": "오 칼국수 맛있겠네요",
          "original_form": "오 칼국수 맛있겠네요",
          "speaker_id": "2",
          "time": "20210829 17:38"
        },
        {
          "id": "MDRW2100022742.1.11",
          "form": "바지락칼국수ㅛ",
          "original_form": "바지락칼국수ㅛ",
          "speaker_id": "1",
          "time": "20210829 17:39"
        },
        {
          "id": "MDRW2100022742.1.12",
          "form": "오오 바지락칼국수 국물맛이 일품이죠",
          "original_form": "오오 바지락칼국수 국물맛이 일품이죠",
          "speaker_id": "2",
          "time": "20210829 17:40"
        },
        {
          "id": "MDRW2100022742.1.13",
          "form": "아 그럼 저녁에는 만두 드실거라 했는데",
          "original_form": "아 그럼 저녁에는 만두 드실거라 했는데",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022742.1.14",
          "form": "name1님은 어떤 만두 좋아하세요?",
          "original_form": "&name1&님은 어떤 만두 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022742.1.15",
          "form": "군만두요",
          "original_form": "군만두요",
          "speaker_id": "1",
          "time": "20210829 17:41"
        },
        {
          "id": "MDRW2100022742.1.16",
          "form": "오 군만두 간단하고 맛있죠!",
          "original_form": "오 군만두 간단하고 맛있죠!",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
          "id": "MDRW2100022742.1.17",
          "form": "저는 번거롭지만 찐만두가 좋더라고요.",
          "original_form": "저는 번거롭지만 찐만두가 좋더라고요.",
          "speaker_id": "2",
          "time": "20210829 17:42"
        },
        {
