      "utterance": [
        {
          "id": "MDRW2100043199.1.1",
          "form": "제가 비혼주의엿는데",
          "original_form": "제가 비혼주의엿는데",
          "speaker_id": "2",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043199.1.2",
          "form": "그생각을 확 바꿔준 사람이에요",
          "original_form": "그생각을 확 바꿔준 사람이에요",
          "speaker_id": "2",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043199.1.3",
          "form": "아직 육아는 자신없어서 더 놀고 계획하기로햇어요",
          "original_form": "아직 육아는 자신없어서 더 놀고 계획하기로햇어요",
          "speaker_id": "2",
          "time": "20210907 15:38"
        },
        {
          "id": "MDRW2100043199.1.4",
          "form": "와 저는 그 확신이라는게 너무 궁금해요 ㅋㅋ 저도 이제 나이가있어서",
          "original_form": "와 저는 그 확신이라는게 너무 궁금해요 ㅋㅋ 저도 이제 나이가있어서",
          "speaker_id": "1",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100043199.1.5",
          "form": "딩크하고싶은데 동의해줄지도모르겠고요",
          "original_form": "딩크하고싶은데 동의해줄지도모르겠고요",
          "speaker_id": "1",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100043199.1.6",
          "form": "아 그게중요해요",
          "original_form": "아 그게중요해요",
          "speaker_id": "2",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100043199.1.7",
          "form": "남친이 딩크던 아니던 상관없다고",
          "original_form": "남친이 딩크던 아니던 상관없다고",
          "speaker_id": "2",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100043199.1.8",
          "form": "이야기많이하고",
          "original_form": "이야기많이하고",
          "speaker_id": "2",
          "time": "20210907 15:39"
        },
        {
          "id": "MDRW2100043199.1.9",
          "form": "제가하고싶은데로 하기로햇어요",
          "original_form": "제가하고싶은데로 하기로햇어요",
          "speaker_id": "2",
          "time": "20210907 15:40"
        },
        {
          "id": "MDRW2100043199.1.10",
          "form": "좋은사람이라서",
          "original_form": "좋은사람이라서",
          "speaker_id": "2",
          "time": "20210907 15:40"
        },
        {
          "id": "MDRW2100043199.1.11",
          "form": "이야기 많이 많이 나눠보세요 ㅎㅎ",
          "original_form": "이야기 많이 많이 나눠보세요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100043199.1.12",
          "form": "와 남친님이 배려를 많이해주시네요 ㅜㅜ 저도 좀 만나다가 슬슬 이야기꺼내봐야겠어요 그런쪽으로",
          "original_form": "와 남친님이 배려를 많이해주시네요 ㅜㅜ 저도 좀 만나다가 슬슬 이야기꺼내봐야겠어요 그런쪽으로",
          "speaker_id": "1",
          "time": "20210907 15:41"
        },
        {
          "id": "MDRW2100043199.1.13",
          "form": "네 엄청 엄청 많이해줘요",
          "original_form": "네 엄청 엄청 많이해줘요",
          "speaker_id": "2",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043199.1.14",
          "form": "확실히 나이가 나이인지라",
          "original_form": "확실히 나이가 나이인지라",
          "speaker_id": "2",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043199.1.15",
          "form": "그런 이야기 엄청 많이햇져",
          "original_form": "그런 이야기 엄청 많이햇져",
          "speaker_id": "2",
          "time": "20210907 15:42"
        },
        {
          "id": "MDRW2100043199.1.16",
          "form": "은근 세뇌를시킬까요...? 지금좋아죽을때",
          "original_form": "은근 세뇌를시킬까요...? 지금좋아죽을때",
          "speaker_id": "1",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100043199.1.17",
          "form": "ㅋㅋ세뇌보다\r\n원래 그런사람을 만나는게ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋ세뇌보다\r\n원래 그런사람을 만나는게ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100043199.1.18",
          "form": "주변친구들보면 좋아죽어서 결혼해도",
          "original_form": "주변친구들보면 좋아죽어서 결혼해도",
          "speaker_id": "2",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100043199.1.19",
          "form": "결혼은 현실이더라구요ㅜㅜㅜㅜㅜ",
          "original_form": "결혼은 현실이더라구요ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100043199.1.20",
          "form": "따흑",
          "original_form": "따흑",
          "speaker_id": "2",
          "time": "20210907 15:44"
        },
        {
          "id": "MDRW2100043199.1.21",
          "form": "그래서 다들 경제력보고 결혼하라고 하나봐요 ㅠㅠ",
          "original_form": "그래서 다들 경제력보고 결혼하라고 하나봐요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 16:00"
        },
        {
          "id": "MDRW2100043199.1.22",
          "form": "그것도 중요함요 !!! ㅎㅎ",
          "original_form": "그것도 중요함요 !!! ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 16:04"
        },
        {
          "id": "MDRW2100043199.1.23",
          "form": "그래도 역시..신뢰가최고겠죠",
          "original_form": "그래도 역시..신뢰가최고겠죠",
          "speaker_id": "1",
          "time": "20210907 16:06"
        },
        {
          "id": "MDRW2100043199.1.24",
          "form": "네 두루두루 중요하니 신중히 신중히!!!!",
          "original_form": "네 두루두루 중요하니 신중히 신중히!!!!",
          "speaker_id": "2",
          "time": "20210907 16:07"
        },
        {
          "id": "MDRW2100043199.1.25",
          "form": "와 준비하시려면힘들겠어요 ㅜㅜ",
          "original_form": "와 준비하시려면힘들겠어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210907 16:09"
        },
        {
          "id": "MDRW2100043199.1.26",
          "form": "최대한 간단하고 심플하게",
          "original_form": "최대한 간단하고 심플하게",
          "speaker_id": "2",
