      "utterance": [
        {
          "id": "MDRW2100020823.1.1",
          "form": "여행계획있으세요?",
          "original_form": "여행계획있으세요?",
          "speaker_id": "1",
          "time": "20210828 13:03"
        },
        {
          "id": "MDRW2100020823.1.2",
          "form": "지금 볼일겸 여행겸 가고있어요 ㅎ",
          "original_form": "지금 볼일겸 여행겸 가고있어요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 13:04"
        },
        {
          "id": "MDRW2100020823.1.3",
          "form": "세종으로요",
          "original_form": "세종으로요",
          "speaker_id": "2",
          "time": "20210828 13:04"
        },
        {
          "id": "MDRW2100020823.1.4",
          "form": "세종이요? 뭐 유명한곳 있어요? 잘몰라서..",
          "original_form": "세종이요? 뭐 유명한곳 있어요? 잘몰라서..",
          "speaker_id": "1",
          "time": "20210828 13:06"
        },
        {
          "id": "MDRW2100020823.1.5",
          "form": "아뇨 세종은 볼일있어서 가는데",
          "original_form": "아뇨 세종은 볼일있어서 가는데",
          "speaker_id": "2",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020823.1.6",
          "form": "간김에 거기에 가볼만한곳에 가보려구요",
          "original_form": "간김에 거기에 가볼만한곳에 가보려구요",
          "speaker_id": "2",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020823.1.7",
          "form": "국립식물원이랑",
          "original_form": "국립식물원이랑",
          "speaker_id": "2",
          "time": "20210828 13:07"
        },
        {
          "id": "MDRW2100020823.1.8",
          "form": "베어트리파크",
          "original_form": "베어트리파크",
          "speaker_id": "2",
          "time": "20210828 13:08"
        },
        {
          "id": "MDRW2100020823.1.9",
          "form": "국립식물원에 세종에 있어요? 첨 알앗네.. \n규모 커여?",
          "original_form": "국립식물원에 세종에 있어요? 첨 알앗네.. \n규모 커여?",
          "speaker_id": "1",
          "time": "20210828 13:09"
        },
        {
          "id": "MDRW2100020823.1.10",
          "form": "저도 안가봤는데",
          "original_form": "저도 안가봤는데",
          "speaker_id": "2",
          "time": "20210828 13:10"
        },
        {
          "id": "MDRW2100020823.1.11",
          "form": "큰가봐요 전기전동차?",
          "original_form": "큰가봐요 전기전동차?",
          "speaker_id": "2",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100020823.1.12",
          "form": "같은것도 있구요",
          "original_form": "같은것도 있구요",
          "speaker_id": "2",
          "time": "20210828 13:11"
        },
        {
          "id": "MDRW2100020823.1.13",
          "form": "오.. 가보고 싶네요... \n베어트리파크는? 곰나무공원이 뭐하는데에요?",
          "original_form": "오.. 가보고 싶네요... \n베어트리파크는? 곰나무공원이 뭐하는데에요?",
          "speaker_id": "1",
          "time": "20210828 13:12"
        },
        {
          "id": "MDRW2100020823.1.14",
          "form": "곰공원같아요\n애가 좋아할것같아서 가보려구요",
          "original_form": "곰공원같아요\n애가 좋아할것같아서 가보려구요",
          "speaker_id": "2",
          "time": "20210828 13:13"
        },
        {
          "id": "MDRW2100020823.1.15",
          "form": "그거 말곤 동물원이 안보여서요 ㅎ",
          "original_form": "그거 말곤 동물원이 안보여서요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020823.1.16",
          "form": "앙, 한국에도 곰이 있어요? ㅋㅋㅋ",
          "original_form": "앙, 한국에도 곰이 있어요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:14"
        },
        {
          "id": "MDRW2100020823.1.17",
          "form": "동물원엔 다 곰이 있는거아닌가요?ㅎ",
          "original_form": "동물원엔 다 곰이 있는거아닌가요?ㅎ",
          "speaker_id": "2",
          "time": "20210828 13:21"
        },
        {
          "id": "MDRW2100020823.1.18",
          "form": "가봐야겠지만 곰상태가 안좋다는 말도 있어요 ㅋ",
          "original_form": "가봐야겠지만 곰상태가 안좋다는 말도 있어요 ㅋ",
          "speaker_id": "2",
          "time": "20210828 13:22"
        },
        {
          "id": "MDRW2100020823.1.19",
          "form": "어디 여행계획있으세요?",
          "original_form": "어디 여행계획있으세요?",
          "speaker_id": "2",
          "time": "20210828 13:23"
        },
        {
          "id": "MDRW2100020823.1.20",
          "form": "저는 아직 넘 더워서.. 집콕이요..",
          "original_form": "저는 아직 넘 더워서.. 집콕이요..",
          "speaker_id": "1",
          "time": "20210828 13:23"
        },
        {
          "id": "MDRW2100020823.1.21",
          "form": "코로나라 눈치도 보이고..",
          "original_form": "코로나라 눈치도 보이고..",
          "speaker_id": "1",
          "time": "20210828 13:23"
        },
        {
          "id": "MDRW2100020823.1.22",
          "form": "코로나인데 저는 세종가는 고속도로 올리는데",
          "original_form": "코로나인데 저는 세종가는 고속도로 올리는데",
          "speaker_id": "2",
          "time": "20210828 13:24"
        },
        {
          "id": "MDRW2100020823.1.23",
          "form": "막히더라구요",
          "original_form": "막히더라구요",
          "speaker_id": "2",
          "time": "20210828 13:24"
        },
        {
          "id": "MDRW2100020823.1.24",
          "form": "많이들 다니시나봐요ㅜㅠ",
          "original_form": "많이들 다니시나봐요ㅜㅠ",
          "speaker_id": "2",
          "time": "20210828 13:24"
        },
        {
          "id": "MDRW2100020823.1.25",
          "form": "벌써 길이 막혀요?",
          "original_form": "벌써 길이 막혀요?",
          "speaker_id": "1",
          "time": "20210828 13:25"
        },
        {
          "id": "MDRW2100020823.1.26",
          "form": "네 고속도로로 가는길은 막혀요",
          "original_form": "네 고속도로로 가는길은 막혀요",
          "speaker_id": "2",
          "time": "20210828 13:27"
        },
        {
          "id": "MDRW2100020823.1.27",
          "form": "그럼 어디가보고싶으세요?",
          "original_form": "그럼 어디가보고싶으세요?",
          "speaker_id": "2",
          "time": "20210828 13:27"
        },
        {
          "id": "MDRW2100020823.1.28",
          "form": "코시국 끝나면요",
          "original_form": "코시국 끝나면요",
          "speaker_id": "2",
          "time": "20210828 13:27"
        },
        {
          "id": "MDRW2100020823.1.29",
          "form": "음.. 국내라면.. 설악산 종주요.. ㅋㅋ",
          "original_form": "음.. 국내라면.. 설악산 종주요.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 13:28"
        },
        {
          "id": "MDRW2100020823.1.30",
          "form": "와~~",
          "original_form": "와~~",
          "speaker_id": "2",
          "time": "20210828 13:28"
        },
        {
          "id": "MDRW2100020823.1.31",
          "form": "머찌세요",
          "original_form": "머찌세요",
          "speaker_id": "2",
          "time": "20210828 13:28"
        }
      ]
