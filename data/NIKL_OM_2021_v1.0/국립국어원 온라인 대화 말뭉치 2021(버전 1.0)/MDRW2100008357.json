        },
        {
          "id": "MDRW2100008357.1.23",
          "form": "계열이 비슷해요",
          "original_form": "계열이 비슷해요",
          "speaker_id": "1",
          "time": "20210822 15:41"
        },
        {
          "id": "MDRW2100008357.1.24",
          "form": "동숲 해보셨나요?",
          "original_form": "동숲 해보셨나요?",
