      "utterance": [
        {
          "id": "MDRW2100038493.1.1",
          "form": "오늘은 가을가을하게",
          "original_form": "오늘은 가을가을하게",
          "speaker_id": "2",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100038493.1.2",
          "form": "슬랙스와 티와 간절기겉옷을 입고나왓어요",
          "original_form": "슬랙스와 티와 간절기겉옷을 입고나왓어요",
          "speaker_id": "2",
          "time": "20210902 14:36"
        },
        {
          "id": "MDRW2100038493.1.3",
          "form": "패피 name1님의 오늘의 코디는오ㅓ",
          "original_form": "패피 &name1&님의 오늘의 코디는오ㅓ",
          "speaker_id": "2",
          "time": "20210902 14:50"
        },
        {
          "id": "MDRW2100038493.1.4",
          "form": "오 패피다",
          "original_form": "오 패피다",
          "speaker_id": "1",
          "time": "20210902 16:15"
        },
        {
          "id": "MDRW2100038493.1.5",
          "form": "저는작업복이요ㅋㄱㅋㅋ",
          "original_form": "저는작업복이요ㅋㄱㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:15"
        },
        {
          "id": "MDRW2100038493.1.6",
          "form": "비와서병원가기싫어졋네요",
          "original_form": "비와서병원가기싫어졋네요",
          "speaker_id": "1",
          "time": "20210902 16:15"
        },
        {
          "id": "MDRW2100038493.1.7",
          "form": "ㅋㅋㅋㅋ 병원 가셨나요",
          "original_form": "ㅋㅋㅋㅋ 병원 가셨나요",
          "speaker_id": "2",
          "time": "20210902 17:05"
        },
        {
          "id": "MDRW2100038493.1.8",
          "form": "안가고집에누웠어요",
          "original_form": "안가고집에누웠어요",
          "speaker_id": "1",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100038493.1.9",
          "form": "집도꿉꿉하네요",
          "original_form": "집도꿉꿉하네요",
          "speaker_id": "1",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100038493.1.10",
          "form": "집에선반팔반바지입나요?",
          "original_form": "집에선반팔반바지입나요?",
          "speaker_id": "1",
          "time": "20210902 17:07"
        },
        {
          "id": "MDRW2100038493.1.11",
          "form": "아니요 반팔원피쓰 입어용",
          "original_form": "아니요 반팔원피쓰 입어용",
          "speaker_id": "2",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100038493.1.12",
          "form": "비가 와요 집에 있길 잘하셨네요",
          "original_form": "비가 와요 집에 있길 잘하셨네요",
          "speaker_id": "2",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100038493.1.13",
          "form": "반팔원피스짱시원할듯해요ㅋㅋㄲ",
          "original_form": "반팔원피스짱시원할듯해요ㅋㅋㄲ",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100038493.1.14",
          "form": "근데누우면말려올라가지않나요원피스는",
          "original_form": "근데누우면말려올라가지않나요원피스는",
          "speaker_id": "1",
          "time": "20210902 17:35"
        },
        {
          "id": "MDRW2100038493.1.15",
          "form": "네 그래도 내방인데 뭐",
          "original_form": "네 그래도 내방인데 뭐",
          "speaker_id": "2",
          "time": "20210902 17:46"
        },
        {
          "id": "MDRW2100038493.1.16",
          "form": "그건맞아요 저처럼거실생활안하면뭐",
          "original_form": "그건맞아요 저처럼거실생활안하면뭐",
          "speaker_id": "1",
          "time": "20210902 17:47"
        },
        {
          "id": "MDRW2100038493.1.17",
          "form": "네넹 원피스가 짱이지요",
          "original_form": "네넹 원피스가 짱이지요",
          "speaker_id": "2",
          "time": "20210902 17:55"
        },
        {
          "id": "MDRW2100038493.1.18",
          "form": "시원햇는데  또 꿉꿉하네요",
          "original_form": "시원햇는데  또 꿉꿉하네요",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100038493.1.19",
          "form": "원피스짱이예요오",
          "original_form": "원피스짱이예요오",
          "speaker_id": "1",
          "time": "20210902 17:59"
        },
        {
          "id": "MDRW2100038493.1.20",
          "form": "그러게요 운동다녀오는데 비안와서 다행이에용",
          "original_form": "그러게요 운동다녀오는데 비안와서 다행이에용",
          "speaker_id": "2",
          "time": "20210902 19:28"
        },
        {
          "id": "MDRW2100038493.1.21",
          "form": "오진짜네요많이올거같았는데다행",
          "original_form": "오진짜네요많이올거같았는데다행",
          "speaker_id": "1",
          "time": "20210902 20:19"
        },
        {
          "id": "MDRW2100038493.1.22",
          "form": "교장쌤이 밤에만 쏟는다고 착한 비라고 햇어요",
          "original_form": "교장쌤이 밤에만 쏟는다고 착한 비라고 햇어요",
          "speaker_id": "2",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100038493.1.23",
          "form": "ㅋㅋㅋㅋㅋㄱ착한비ㅋㅋㄱㅋ",
