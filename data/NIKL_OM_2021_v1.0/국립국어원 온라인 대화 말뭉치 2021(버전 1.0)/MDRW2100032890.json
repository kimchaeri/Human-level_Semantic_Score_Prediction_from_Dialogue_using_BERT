      "utterance": [
        {
          "id": "MDRW2100032890.1.1",
          "form": "아니 어떤 20대그",
          "original_form": "아니 어떤 20대그",
          "speaker_id": "2",
          "time": "20210906 04:02"
        },
        {
          "id": "MDRW2100032890.1.2",
          "form": "우론차인가 생강차에 노른자를 올려",
          "original_form": "우론차인가 생강차에 노른자를 올려",
          "speaker_id": "2",
          "time": "20210906 04:02"
        },
        {
          "id": "MDRW2100032890.1.3",
          "form": "안 비려ㅠㅠ??",
          "original_form": "안 비려ㅠㅠ??",
          "speaker_id": "2",
          "time": "20210906 04:03"
        },
        {
          "id": "MDRW2100032890.1.4",
          "form": "사실은",
          "original_form": "사실은",
          "speaker_id": "1",
          "time": "20210906 04:03"
        },
        {
          "id": "MDRW2100032890.1.5",
          "form": "노른자는 안 올려봤어",
          "original_form": "노른자는 안 올려봤어",
          "speaker_id": "1",
          "time": "20210906 04:03"
        },
        {
          "id": "MDRW2100032890.1.6",
          "form": "허세였던거지",
          "original_form": "허세였던거지",
          "speaker_id": "1",
          "time": "20210906 04:03"
        },
        {
          "id": "MDRW2100032890.1.7",
          "form": "생강차 자체는 좋아해...",
          "original_form": "생강차 자체는 좋아해...",
          "speaker_id": "1",
          "time": "20210906 04:03"
        },
        {
          "id": "MDRW2100032890.1.8",
          "form": "어쩐지",
          "original_form": "어쩐지",
          "speaker_id": "2",
          "time": "20210906 04:03"
        },
        {
          "id": "MDRW2100032890.1.9",
          "form": "난생 처음듣는 조합이더라",
          "original_form": "난생 처음듣는 조합이더라",
          "speaker_id": "2",
          "time": "20210906 04:04"
        },
        {
          "id": "MDRW2100032890.1.10",
          "form": "ㅋㅋㅋ생강차 호불호가 갈릴 뿐이지",
          "original_form": "ㅋㅋㅋ생강차 호불호가 갈릴 뿐이지",
          "speaker_id": "2",
          "time": "20210906 04:04"
        },
        {
          "id": "MDRW2100032890.1.11",
          "form": "ㅋㅋㅋㅋㅋ어른스러워보이고싶었어",
          "original_form": "ㅋㅋㅋㅋㅋ어른스러워보이고싶었어",
          "speaker_id": "1",
          "time": "20210906 04:04"
        },
        {
          "id": "MDRW2100032890.1.12",
          "form": "좋아하는 사람은 좋아해",
          "original_form": "좋아하는 사람은 좋아해",
          "speaker_id": "2",
          "time": "20210906 04:04"
        },
        {
          "id": "MDRW2100032890.1.13",
          "form": "맞어 특히 감기걸릴때는",
          "original_form": "맞어 특히 감기걸릴때는",
          "speaker_id": "1",
          "time": "20210906 04:04"
        },
        {
          "id": "MDRW2100032890.1.14",
          "form": "ㅋㅋㅌㅌㅋㅋㅋ가오녀였군",
          "original_form": "ㅋㅋㅌㅌㅋㅋㅋ가오녀였군",
          "speaker_id": "2",
          "time": "20210906 04:04"
        },
        {
          "id": "MDRW2100032890.1.15",
          "form": "캬",
          "original_form": "캬",
          "speaker_id": "1",
          "time": "20210906 04:04"
        },
        {
          "id": "MDRW2100032890.1.16",
          "form": "아흑",
          "original_form": "아흑",
          "speaker_id": "2",
          "time": "20210906 04:04"
        },
        {
          "id": "MDRW2100032890.1.17",
