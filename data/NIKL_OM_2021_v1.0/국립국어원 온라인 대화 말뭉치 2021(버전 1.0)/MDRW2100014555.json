      "utterance": [
        {
          "id": "MDRW2100014555.1.1",
          "form": "그러니깐여",
          "original_form": "그러니깐여",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014555.1.2",
          "form": "그래도 뚱르 재밌었어영ㅋㅋ",
          "original_form": "그래도 뚱르 재밌었어영ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014555.1.3",
          "form": "최근에 정글크루즈 보셨다는데",
          "original_form": "최근에 정글크루즈 보셨다는데",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014555.1.4",
          "form": "그건 무슨 내용이예요????",
          "original_form": "그건 무슨 내용이예요????",
          "speaker_id": "2",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014555.1.5",
          "form": "진짜 끝까지 기대하면서 봤는데...다시 멋지게 나올거라고ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 끝까지 기대하면서 봤는데...다시 멋지게 나올거라고ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014555.1.6",
          "form": "쥬만지 보셨어요?",
          "original_form": "쥬만지 보셨어요?",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014555.1.7",
          "form": "네 완전 어렸을 때",
          "original_form": "네 완전 어렸을 때",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014555.1.8",
          "form": "봤죠",
          "original_form": "봤죠",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014555.1.9",
          "form": "유치원생때 본 듯",
          "original_form": "유치원생때 본 듯",
          "speaker_id": "2",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014555.1.10",
          "form": "그런 느낌이에요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그런 느낌이에요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014555.1.11",
          "form": "잃어버린 세계를 찾아서는요?",
          "original_form": "잃어버린 세계를 찾아서는요?",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014555.1.12",
          "form": "모험+액션!",
          "original_form": "모험+액션!",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014555.1.13",
          "form": "잃어버린 세계 제목만 들어본듯",
          "original_form": "잃어버린 세계 제목만 들어본듯",
          "speaker_id": "2",
          "time": "20210825 19:48"
        },
        {
          "id": "MDRW2100014555.1.14",
          "form": "속편들도 나오지 않았어여??",
          "original_form": "속편들도 나오지 않았어여??",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014555.1.15",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "2",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014555.1.16",
          "form": "네 맞아요!",
          "original_form": "네 맞아요!",
          "speaker_id": "1",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014555.1.17",
          "form": "그런 느낌이에요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그런 느낌이에요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014555.1.18",
          "form": "내셔널 트레져",
          "original_form": "내셔널 트레져",
          "speaker_id": "1",
          "time": "20210825 19:49"
        },
        {
          "id": "MDRW2100014555.1.19",
          "form": "내셔널 트레져도 12 둘다 봤는데",
          "original_form": "내셔널 트레져도 12 둘다 봤는데",
          "speaker_id": "2",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014555.1.20",
          "form": "너무 옛날 영화라 내용 기억도 안나네 ㅋㅋ",
          "original_form": "너무 옛날 영화라 내용 기억도 안나네 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014555.1.21",
          "form": "느낌만 기억하시면ㅋㅋㅋㅋㅋㅋ",
          "original_form": "느낌만 기억하시면ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014555.1.22",
          "form": "비슷한 결이라",
          "original_form": "비슷한 결이라",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014555.1.23",
          "form": "ㅋㅋㅋㅋ 네엡!",
          "original_form": "ㅋㅋㅋㅋ 네엡!",
          "speaker_id": "2",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014555.1.24",
          "form": "내셔널 트레져 봐야겠어요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "내셔널 트레져 봐야겠어요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014555.1.25",
          "form": "얘기하니까 보고 싶어졌어요",
          "original_form": "얘기하니까 보고 싶어졌어요",
          "speaker_id": "1",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014555.1.26",
          "form": "찐으로  영화 좋아하시는구낭ㅋㅋ",
          "original_form": "찐으로  영화 좋아하시는구낭ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014555.1.27",
          "form": "본 거 또 보는 것도 좋아해서요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "본 거 또 보는 것도 좋아해서요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014555.1.28",
          "form": "저도 영화나 드라마 자주보는데",
          "original_form": "저도 영화나 드라마 자주보는데",
          "speaker_id": "2",
          "time": "20210825 19:57"
        },
        {
          "id": "MDRW2100014555.1.29",
          "form": "두 번씩 찾아보진 않아서",
          "original_form": "두 번씩 찾아보진 않아서",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014555.1.30",
          "form": "그래두 가끔 tv에서 재밌는거 나오면 그냥 보는듯한 ㅋㅋ",
          "original_form": "그래두 가끔 tv에서 재밌는거 나오면 그냥 보는듯한 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014555.1.31",
          "form": "그런 느낌!?!?",
          "original_form": "그런 느낌!?!?",
          "speaker_id": "2",
          "time": "20210825 19:59"
        },
        {
          "id": "MDRW2100014555.1.32",
