      "utterance": [
        {
          "id": "MDRW2100036482.1.1",
          "form": "아니아니 왜??",
          "original_form": "아니아니 왜??",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036482.1.2",
          "form": "나 오랜만에 빽다방 어플을 깔았더니 아아쿠폰을 주더라",
          "original_form": "나 오랜만에 빽다방 어플을 깔았더니 아아쿠폰을 주더라",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036482.1.3",
          "form": "흠 우리집에서 가까운편이긴해",
          "original_form": "흠 우리집에서 가까운편이긴해",
          "speaker_id": "2",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036482.1.4",
          "form": "공짜로 마셨어!",
          "original_form": "공짜로 마셨어!",
          "speaker_id": "1",
          "time": "20210830 23:40"
        },
        {
          "id": "MDRW2100036482.1.5",
          "form": "우와 새로 가입하면 주는거야?",
          "original_form": "우와 새로 가입하면 주는거야?",
          "speaker_id": "2",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036482.1.6",
          "form": "너도 어플을 깔아봐",
          "original_form": "너도 어플을 깔아봐",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036482.1.7",
          "form": "음료에거라도 절약하자",
          "original_form": "음료에거라도 절약하자",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036482.1.8",
          "form": "음 아니 난 오랜만에 접속한거엮어",
          "original_form": "음 아니 난 오랜만에 접속한거엮어",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036482.1.9",
          "form": "좋았어.",
          "original_form": "좋았어.",
          "speaker_id": "2",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036482.1.10",
          "form": "랜덤으로 주는 거 같아",
          "original_form": "랜덤으로 주는 거 같아",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036482.1.11",
          "form": "빽다방 커피 무난해",
          "original_form": "빽다방 커피 무난해",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036482.1.12",
          "form": "근데 카페인은 많대",
          "original_form": "근데 카페인은 많대",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036482.1.13",
          "form": "빽다방 커피는 막이 어땟어?",
          "original_form": "빽다방 커피는 막이 어땟어?",
          "speaker_id": "2",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036482.1.14",
          "form": "앗 내 마음을 읽었구나",
          "original_form": "앗 내 마음을 읽었구나",
          "speaker_id": "2",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036482.1.15",
          "form": "너무 많이 마셔서 심장두근대는 일은 없도록 해.",
          "original_form": "너무 많이 마셔서 심장두근대는 일은 없도록 해.",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036482.1.16",
          "form": "무난무난해",
          "original_form": "무난무난해",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036482.1.17",
          "form": "그건 양이 많은편이지?",
          "original_form": "그건 양이 많은편이지?",
          "speaker_id": "2",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036482.1.18",
          "form": "대신 저렴해",
          "original_form": "대신 저렴해",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036482.1.19",
          "form": "응! 좋아",
          "original_form": "응! 좋아",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036482.1.20",
          "form": "근데 양이 너무 맘ㅎ아",
          "original_form": "근데 양이 너무 맘ㅎ아",
          "speaker_id": "1",
          "time": "20210830 23:41"
        },
        {
          "id": "MDRW2100036482.1.21",
          "form": "나눠 마셔도 배탈안날까?",
          "original_form": "나눠 마셔도 배탈안날까?",
          "speaker_id": "2",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036482.1.22",
          "form": "적당하면좋겠어",
          "original_form": "적당하면좋겠어",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036482.1.23",
          "form": "당연하지",
          "original_form": "당연하지",
          "speaker_id": "1",
          "time": "20210830 23:42"
        },
        {
          "id": "MDRW2100036482.1.24",
          "form": "냉장고를 깨끗하게 유지해.",
          "original_form": "냉장고를 깨끗하게 유지해.",
          "speaker_id": "1",
          "time": "20210830 23:42"
        }
      ]
    }
