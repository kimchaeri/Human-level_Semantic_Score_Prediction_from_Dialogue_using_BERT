      "utterance": [
        {
          "id": "MDRW2100046057.1.1",
          "form": "디즈니는 노래가 너무 좋아요 ㅋㅋㅋㅋ",
          "original_form": "디즈니는 노래가 너무 좋아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046057.1.2",
          "form": "저 겨울왕국 1 봤었어요!!",
          "original_form": "저 겨울왕국 1 봤었어요!!",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046057.1.3",
          "form": "미녀와야수도 너무 보고싶네요",
          "original_form": "미녀와야수도 너무 보고싶네요",
          "speaker_id": "1",
          "time": "20210908 09:56"
        },
        {
          "id": "MDRW2100046057.1.4",
          "form": "저도 1만 봣어요 !!!",
          "original_form": "저도 1만 봣어요 !!!",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046057.1.5",
          "form": "마블 좋아하세요",
          "original_form": "마블 좋아하세요",
          "speaker_id": "2",
          "time": "20210908 09:57"
        },
        {
          "id": "MDRW2100046057.1.6",
          "form": "마블 ㅈㅐ밌긴한데",
          "original_form": "마블 ㅈㅐ밌긴한데",
          "speaker_id": "1",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100046057.1.7",
          "form": "제가 시리즈물을 잘 못봐서..",
          "original_form": "제가 시리즈물을 잘 못봐서..",
          "speaker_id": "1",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100046057.1.8",
          "form": "시리즈 안챙기고 가끔 봐요!",
          "original_form": "시리즈 안챙기고 가끔 봐요!",
          "speaker_id": "1",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100046057.1.9",
          "form": "오 저두요 저는 시리즈물 봐도",
          "original_form": "오 저두요 저는 시리즈물 봐도",
          "speaker_id": "2",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100046057.1.10",
          "form": "개봉하면 전 내용을 까먹어서 ㅋㅋ",
          "original_form": "개봉하면 전 내용을 까먹어서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 09:58"
        },
        {
          "id": "MDRW2100046057.1.11",
          "form": "앜ㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:00"
        },
        {
          "id": "MDRW2100046057.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210908 10:00"
        },
        {
          "id": "MDRW2100046057.1.13",
          "form": "저는 그리고 챙겨보는걸 잘 못해요 ㅋㅋ",
          "original_form": "저는 그리고 챙겨보는걸 잘 못해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:00"
        },
        {
          "id": "MDRW2100046057.1.14",
          "form": "ㅋㅋㅋㅋ 근데 앞에꺼 안봐도 재밋드라구요",
          "original_form": "ㅋㅋㅋㅋ 근데 앞에꺼 안봐도 재밋드라구요",
          "speaker_id": "2",
          "time": "20210908 10:02"
        },
        {
          "id": "MDRW2100046057.1.15",
          "form": "맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046057.1.16",
          "form": "저는 메이즈러너 1 너무 재밌게봤는데",
          "original_form": "저는 메이즈러너 1 너무 재밌게봤는데",
          "speaker_id": "1",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046057.1.17",
          "form": "2부터는 못봤어요 ..",
          "original_form": "2부터는 못봤어요 ..",
          "speaker_id": "1",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046057.1.18",
          "form": "오오 그거 마니 들어봣어요",
          "original_form": "오오 그거 마니 들어봣어요",
          "speaker_id": "2",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046057.1.19",
          "form": "옛날 드라마인가요?",
          "original_form": "옛날 드라마인가요?",
          "speaker_id": "2",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046057.1.20",
          "form": "진짜 재밌었어요",
          "original_form": "진짜 재밌었어요",
          "speaker_id": "1",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046057.1.21",
          "form": "아뇨 외국 영화에요!",
          "original_form": "아뇨 외국 영화에요!",
          "speaker_id": "1",
          "time": "20210908 10:04"
        },
        {
          "id": "MDRW2100046057.1.22",
          "form": "한 6~7년 전에 개봉했었어요",
          "original_form": "한 6~7년 전에 개봉했었어요",
          "speaker_id": "1",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100046057.1.23",
          "form": "오오 영화군요!!!",
          "original_form": "오오 영화군요!!!",
          "speaker_id": "2",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100046057.1.24",
          "form": "그러면 정주행 할만 할거같아욬ㅋ",
          "original_form": "그러면 정주행 할만 할거같아욬ㅋ",
          "speaker_id": "2",
          "time": "20210908 10:05"
        },
        {
          "id": "MDRW2100046057.1.25",
          "form": "네네 ㅎㅎ 영화에요",
          "original_form": "네네 ㅎㅎ 영화에요",
