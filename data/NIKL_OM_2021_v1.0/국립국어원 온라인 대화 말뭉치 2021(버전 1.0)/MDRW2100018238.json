      "utterance": [
        {
          "id": "MDRW2100018238.1.1",
          "form": "날씨가 많이 선선해졌네요",
          "original_form": "날씨가 많이 선선해졌네요",
          "speaker_id": "1",
          "time": "20210826 23:20"
        },
        {
          "id": "MDRW2100018238.1.2",
          "form": "그러게요 그래도 오늘 집에모기나왔어요",
          "original_form": "그러게요 그래도 오늘 집에모기나왔어요",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018238.1.3",
          "form": "ㅜㅜ",
          "original_form": "ㅜㅜ",
          "speaker_id": "2",
          "time": "20210826 23:25"
        },
        {
          "id": "MDRW2100018238.1.4",
          "form": "앗.... 더워서 없어졌다던 모기가...",
          "original_form": "앗.... 더워서 없어졌다던 모기가...",
          "speaker_id": "1",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018238.1.5",
          "form": "이제 슬슬 나오나보네요",
          "original_form": "이제 슬슬 나오나보네요",
          "speaker_id": "1",
          "time": "20210826 23:31"
        },
        {
          "id": "MDRW2100018238.1.6",
          "form": "어떤계절 제일 좋아하세요?",
          "original_form": "어떤계절 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 23:34"
        },
        {
          "id": "MDRW2100018238.1.7",
          "form": "가을이요!!",
          "original_form": "가을이요!!",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018238.1.8",
          "form": "곧 옵니다용",
          "original_form": "곧 옵니다용",
          "speaker_id": "1",
          "time": "20210826 23:38"
        },
        {
          "id": "MDRW2100018238.1.9",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018238.1.10",
          "form": "저 동생이랑 같이사는데",
          "original_form": "저 동생이랑 같이사는데",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018238.1.11",
          "form": "곧 여동생옷 뺏어입을수있네요",
          "original_form": "곧 여동생옷 뺏어입을수있네요",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018238.1.12",
          "form": "가을 왜 좋아하세요?",
          "original_form": "가을 왜 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 23:39"
        },
        {
          "id": "MDRW2100018238.1.13",
          "form": "선선한 게 좋더라고요",
          "original_form": "선선한 게 좋더라고요",
          "speaker_id": "1",
          "time": "20210827 00:08"
        },
        {
          "id": "MDRW2100018238.1.14",
          "form": "겨울이 다가오는 그 겨울내음도 조금씩 있고",
          "original_form": "겨울이 다가오는 그 겨울내음도 조금씩 있고",
          "speaker_id": "1",
          "time": "20210827 00:09"
        },
        {
          "id": "MDRW2100018238.1.15",
          "form": "바람이 찬 게 상쾌하고요",
          "original_form": "바람이 찬 게 상쾌하고요",
          "speaker_id": "1",
          "time": "20210827 00:09"
        },
        {
          "id": "MDRW2100018238.1.16",
          "form": "햇볕도 따갑고~~",
          "original_form": "햇볕도 따갑고~~",
          "speaker_id": "1",
          "time": "20210827 00:09"
        },
        {
          "id": "MDRW2100018238.1.17",
          "form": "참 풍요로운 계절이에여",
          "original_form": "참 풍요로운 계절이에여",
          "speaker_id": "1",
          "time": "20210827 00:09"
        },
        {
          "id": "MDRW2100018238.1.18",
          "form": "제 생일도 있고요",
          "original_form": "제 생일도 있고요",
          "speaker_id": "1",
          "time": "20210827 00:09"
        },
        {
          "id": "MDRW2100018238.1.19",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210827 04:21"
        },
        {
          "id": "MDRW2100018238.1.20",
          "form": "햇볕이 따갑군요",
          "original_form": "햇볕이 따갑군요",
          "speaker_id": "2",
          "time": "20210827 04:21"
        },
        {
          "id": "MDRW2100018238.1.21",
          "form": "이번겨울엔 그걸한번 느껴봐야겠어요!",
          "original_form": "이번겨울엔 그걸한번 느껴봐야겠어요!",
          "speaker_id": "2",
          "time": "20210827 04:22"
        },
        {
          "id": "MDRW2100018238.1.22",
          "form": "이번 가을에요.. 새벽이라 정신없었네욬ㅋ",
          "original_form": "이번 가을에요.. 새벽이라 정신없었네욬ㅋ",
          "speaker_id": "2",
          "time": "20210827 05:30"
        },
        {
          "id": "MDRW2100018238.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:26"
        },
        {
          "id": "MDRW2100018238.1.24",
          "form": "겨울 오기전에",
          "original_form": "겨울 오기전에",
          "speaker_id": "1",
          "time": "20210827 08:26"
        },
        {
          "id": "MDRW2100018238.1.25",
          "form": "가을 즐겨야 한다고요,,,",
          "original_form": "가을 즐겨야 한다고요,,,",
          "speaker_id": "1",
          "time": "20210827 08:26"
        },
        {
          "id": "MDRW2100018238.1.26",
          "form": "그러게요 겨울이 넘 무서워요",
          "original_form": "그러게요 겨울이 넘 무서워요",
          "speaker_id": "2",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018238.1.27",
          "form": "작년에 코로나 겨울이 넘 우울했어요 ㅋㅋㅋ",
          "original_form": "작년에 코로나 겨울이 넘 우울했어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018238.1.28",
          "form": "자연 색깔이 무채색인게 그렇더라고요",
          "original_form": "자연 색깔이 무채색인게 그렇더라고요",
          "speaker_id": "2",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018238.1.29",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018238.1.30",
          "form": "그쳐 허허벌판 ㅠㅠ",
          "original_form": "그쳐 허허벌판 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018238.1.31",
          "form": "삭막해요",
          "original_form": "삭막해요",
          "speaker_id": "1",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018238.1.32",
          "form": "근데 가을도 그런 면에선",
          "original_form": "근데 가을도 그런 면에선",
          "speaker_id": "1",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018238.1.33",
          "form": "거의 갈색밖에 없어서",
          "original_form": "거의 갈색밖에 없어서",
          "speaker_id": "1",
          "time": "20210827 08:41"
        },
        {
          "id": "MDRW2100018238.1.34",
          "form": "재미가없어요",
          "original_form": "재미가없어요",
          "speaker_id": "1",
          "time": "20210827 08:41"
        },
        {
          "id": "MDRW2100018238.1.35",
          "form": "아 그런가요",
          "original_form": "아 그런가요",
          "speaker_id": "2",
          "time": "20210827 08:51"
        },
        {
          "id": "MDRW2100018238.1.36",
          "form": "저원래 겨울이최애였는데 코로나땜에 바뀌었슴다",
          "original_form": "저원래 겨울이최애였는데 코로나땜에 바뀌었슴다",
          "speaker_id": "2",
          "time": "20210827 08:51"
        },
        {
          "id": "MDRW2100018238.1.37",
          "form": "이제 봄여름 채도있는 계절이좋아요ㅜ",
          "original_form": "이제 봄여름 채도있는 계절이좋아요ㅜ",
          "speaker_id": "2",
          "time": "20210827 08:52"
        },
        {
          "id": "MDRW2100018238.1.38",
          "form": "앗 그쵸",
          "original_form": "앗 그쵸",
          "speaker_id": "1",
          "time": "20210827 08:55"
        },
        {
          "id": "MDRW2100018238.1.39",
          "form": "나이들수록",
          "original_form": "나이들수록",
          "speaker_id": "1",
          "time": "20210827 08:55"
        },
        {
          "id": "MDRW2100018238.1.40",
          "form": "채도가 좋지않나요?",
          "original_form": "채도가 좋지않나요?",
          "speaker_id": "1",
          "time": "20210827 08:55"
        },
        {
          "id": "MDRW2100018238.1.41",
          "form": "5060갬성?ㅋㅋㅋㅋㅋ",
          "original_form": "5060갬성?ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:55"
        },
        {
          "id": "MDRW2100018238.1.42",
          "form": "어우 맞아요",
          "original_form": "어우 맞아요",
          "speaker_id": "2",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018238.1.43",
          "form": "취향바뀌는것도 신기해요 ㅋㅋㅋㅋ",
          "original_form": "취향바뀌는것도 신기해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018238.1.44",
          "form": "이걸 내가 체감하는게 넘 신기해욬ㅋㅋ",
          "original_form": "이걸 내가 체감하는게 넘 신기해욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:46"
        },
        {
          "id": "MDRW2100018238.1.45",
          "form": "저도요 ㅋㅋㅋㅌㅋ",
          "original_form": "저도요 ㅋㅋㅋㅌㅋ",
          "speaker_id": "1",
          "time": "20210827 11:59"
        },
        {
          "id": "MDRW2100018238.1.46",
          "form": "저는 안 그럴 줄 알았는데 말이져...",
          "original_form": "저는 안 그럴 줄 알았는데 말이져...",
          "speaker_id": "1",
          "time": "20210827 11:59"
        },
        {
          "id": "MDRW2100018238.1.47",
          "form": "저 언제 늙었나 싶은 게..",
          "original_form": "저 언제 늙었나 싶은 게..",
          "speaker_id": "1",
          "time": "20210827 11:59"
        },
        {
          "id": "MDRW2100018238.1.48",
          "form": "저 이어폰 없으명 슈퍼도 안갔는데",
          "original_form": "저 이어폰 없으명 슈퍼도 안갔는데",
          "speaker_id": "1",
          "time": "20210827 12:00"
        },
        {
          "id": "MDRW2100018238.1.49",
          "form": "출퇴근 잘만 하더라고요... 음악없이도....",
          "original_form": "출퇴근 잘만 하더라고요... 음악없이도....",
          "speaker_id": "1",
