      "utterance": [
        {
          "id": "MDRW2100022415.1.1",
          "form": "보시는 티비프로그램 있으세요?",
          "original_form": "보시는 티비프로그램 있으세요?",
          "speaker_id": "1",
          "time": "20210829 13:42"
        },
        {
          "id": "MDRW2100022415.1.2",
          "form": "네 저는 드라마 위주로 봐요 ㅎㅎ",
          "original_form": "네 저는 드라마 위주로 봐요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022415.1.3",
          "form": "오 지금 보시는거있으세요?",
          "original_form": "오 지금 보시는거있으세요?",
          "speaker_id": "1",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022415.1.4",
          "form": "지금은 비밀의 숲보고 있어요 ㅋㅋ",
          "original_form": "지금은 비밀의 숲보고 있어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100022415.1.5",
          "form": "제가 좀 트렌트가 늦어서 ㅎㅎ",
          "original_form": "제가 좀 트렌트가 늦어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100022415.1.6",
          "form": "그거 추리 같은거죠? 재밌어요?",
          "original_form": "그거 추리 같은거죠? 재밌어요?",
          "speaker_id": "1",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100022415.1.7",
          "form": "명작이라하더라고요",
          "original_form": "명작이라하더라고요",
          "speaker_id": "1",
          "time": "20210829 13:44"
        },
        {
          "id": "MDRW2100022415.1.8",
          "form": "네 추리 맞아요",
          "original_form": "네 추리 맞아요",
          "speaker_id": "2",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100022415.1.9",
          "form": "검사가 살인사건 추적하는 내용이에요",
          "original_form": "검사가 살인사건 추적하는 내용이에요",
          "speaker_id": "2",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100022415.1.10",
          "form": "누구나와요??",
          "original_form": "누구나와요??",
          "speaker_id": "1",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100022415.1.11",
          "form": "오 추리물 좋아하는데!!",
          "original_form": "오 추리물 좋아하는데!!",
          "speaker_id": "1",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100022415.1.12",
          "form": "조승우씨하고 배두나씨가 주연이고",
          "original_form": "조승우씨하고 배두나씨가 주연이고",
          "speaker_id": "2",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100022415.1.13",
          "form": "와 연기는 보장됐네요",
          "original_form": "와 연기는 보장됐네요",
          "speaker_id": "1",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100022415.1.14",
          "form": "아 그래 신혜선씨도 나와요",
          "original_form": "아 그래 신혜선씨도 나와요",
          "speaker_id": "2",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100022415.1.15",
          "form": "맞아요 배우는 뭐 이미 탑클라스죠 ㅋㅋㅋ",
          "original_form": "맞아요 배우는 뭐 이미 탑클라스죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100022415.1.16",
          "form": "재밌으니까 한번 보시는 걸 추천드려요!",
          "original_form": "재밌으니까 한번 보시는 걸 추천드려요!",
          "speaker_id": "2",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100022415.1.17",
          "form": "오 ㅋㅋ넷플에 있을까요?",
          "original_form": "오 ㅋㅋ넷플에 있을까요?",
          "speaker_id": "1",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022415.1.18",
          "form": "네 넷플에도 있고",
          "original_form": "네 넷플에도 있고",
          "speaker_id": "2",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022415.1.19",
          "form": "그거 뭐더라?",
          "original_form": "그거 뭐더라?",
          "speaker_id": "2",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022415.1.20",
          "form": "CJ에서 굴리는 플랫폼이...",
          "original_form": "CJ에서 굴리는 플랫폼이...",
          "speaker_id": "2",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100022415.1.21",
          "form": "티빙인가",
          "original_form": "티빙인가",
          "speaker_id": "2",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100022415.1.22",
          "form": "아 티빙 티빙 거기애서도 볼 수 잇어요",
          "original_form": "아 티빙 티빙 거기애서도 볼 수 잇어요",
          "speaker_id": "2",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100022415.1.23",
          "form": "오 티빙 네이버멤버십 해서볼수있는데",
          "original_form": "오 티빙 네이버멤버십 해서볼수있는데",
          "speaker_id": "1",
          "time": "20210829 13:48"
        },
        {
          "id": "MDRW2100022415.1.24",
          "form": "봐야겠네요",
          "original_form": "봐야겠네요",
          "speaker_id": "1",
          "time": "20210829 13:48"
        }
      ]
