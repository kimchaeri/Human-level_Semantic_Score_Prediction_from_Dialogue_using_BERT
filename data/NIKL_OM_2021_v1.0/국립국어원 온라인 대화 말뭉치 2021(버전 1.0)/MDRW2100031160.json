      "utterance": [
        {
          "id": "MDRW2100031160.1.1",
          "form": "연애중이세요?",
          "original_form": "연애중이세요?",
          "speaker_id": "1",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100031160.1.2",
          "form": "ㅋㅋ아뇨ㅋㅋ",
          "original_form": "ㅋㅋ아뇨ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:40"
        },
        {
          "id": "MDRW2100031160.1.3",
          "form": "연애중이신가요?",
          "original_form": "연애중이신가요?",
          "speaker_id": "2",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100031160.1.4",
          "form": "넵 저는 하고있습니다",
          "original_form": "넵 저는 하고있습니다",
          "speaker_id": "1",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100031160.1.5",
          "form": "소개팅은 안하세요?",
          "original_form": "소개팅은 안하세요?",
          "speaker_id": "1",
          "time": "20210904 17:41"
        },
        {
          "id": "MDRW2100031160.1.6",
          "form": "가끔하는데 인연이 없어서ㅋㅋ",
          "original_form": "가끔하는데 인연이 없어서ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:44"
        },
        {
          "id": "MDRW2100031160.1.7",
          "form": "저도 소개팅으로 만났거든요",
          "original_form": "저도 소개팅으로 만났거든요",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100031160.1.8",
          "form": "몇년 사귀셨어요?",
          "original_form": "몇년 사귀셨어요?",
          "speaker_id": "2",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100031160.1.9",
          "form": "5년?",
          "original_form": "5년?",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100031160.1.10",
          "form": "비혼주의신가요?",
          "original_form": "비혼주의신가요?",
          "speaker_id": "2",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100031160.1.11",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100031160.1.12",
          "form": "결혼해야죠 ㅎㅎ",
          "original_form": "결혼해야죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100031160.1.13",
          "form": "ㅋㅋ제 지인은 거의 십년째 연애만",
          "original_form": "ㅋㅋ제 지인은 거의 십년째 연애만",
          "speaker_id": "2",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100031160.1.14",
          "form": "헛",
          "original_form": "헛",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031160.1.15",
          "form": "10년이요?",
          "original_form": "10년이요?",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031160.1.16",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031160.1.17",
          "form": "ㅋㅋ 사연이 있는거같긴한데 개인사라... ㅋㅋ",
          "original_form": "ㅋㅋ 사연이 있는거같긴한데 개인사라... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031160.1.18",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100031160.1.19",
          "form": "ㅋㅋ 진짜 이제 가족이겠어요!",
          "original_form": "ㅋㅋ 진짜 이제 가족이겠어요!",
          "speaker_id": "2",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100031160.1.20",
          "form": "이제 결혼해야죠 하고 있는데 코로나땜에",
          "original_form": "이제 결혼해야죠 하고 있는데 코로나땜에",
          "speaker_id": "1",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100031160.1.21",
          "form": "참....",
          "original_form": "참....",
          "speaker_id": "1",
          "time": "20210904 17:51"
        },
        {
          "id": "MDRW2100031160.1.22",
          "form": "ㅜ코로나때매 연애 2년추가",
          "original_form": "ㅜ코로나때매 연애 2년추가",
          "speaker_id": "2",
          "time": "20210904 17:56"
        },
        {
          "id": "MDRW2100031160.1.23",
          "form": "진짜ㅜㅜ",
          "original_form": "진짜ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 17:57"
        },
        {
          "id": "MDRW2100031160.1.24",
          "form": "코로나시러요",
          "original_form": "코로나시러요",
          "speaker_id": "1",
          "time": "20210904 17:57"
        },
        {
          "id": "MDRW2100031160.1.25",
          "form": "사촌동생 1년 미루면서 동거하다 작년에 그냥 식올렸어요",
          "original_form": "사촌동생 1년 미루면서 동거하다 작년에 그냥 식올렸어요",
          "speaker_id": "2",
          "time": "20210904 17:57"
        }
