      "utterance": [
        {
          "id": "MDRW2100036088.1.1",
          "form": "저는 하이라이트로 바꿔서요 매일 닦아요..",
          "original_form": "저는 하이라이트로 바꿔서요 매일 닦아요..",
          "speaker_id": "2",
          "time": "20210830 16:02"
        },
        {
          "id": "MDRW2100036088.1.2",
          "form": "하이라이트는 어떤가요?",
          "original_form": "하이라이트는 어떤가요?",
          "speaker_id": "1",
          "time": "20210830 16:03"
        },
        {
          "id": "MDRW2100036088.1.3",
          "form": "가스레인지랑 비교하면 어떠세요?",
          "original_form": "가스레인지랑 비교하면 어떠세요?",
          "speaker_id": "1",
          "time": "20210830 16:03"
        },
        {
          "id": "MDRW2100036088.1.4",
          "form": "저는 바꾸길 잘했어요.. 좋아요",
          "original_form": "저는 바꾸길 잘했어요.. 좋아요",
          "speaker_id": "2",
          "time": "20210830 16:04"
        },
        {
          "id": "MDRW2100036088.1.5",
          "form": "근데 요즘 인덕션이 더 좋다고 그래서.. 좀 흔들려요",
          "original_form": "근데 요즘 인덕션이 더 좋다고 그래서.. 좀 흔들려요",
          "speaker_id": "2",
          "time": "20210830 16:05"
        },
        {
          "id": "MDRW2100036088.1.6",
          "form": "아 그래용?",
          "original_form": "아 그래용?",
          "speaker_id": "1",
          "time": "20210830 16:05"
        },
        {
          "id": "MDRW2100036088.1.7",
          "form": "인덕션으로 바꿔야하나 하구요",
          "original_form": "인덕션으로 바꿔야하나 하구요",
          "speaker_id": "2",
          "time": "20210830 16:05"
        },
        {
          "id": "MDRW2100036088.1.8",
          "form": "저는 수건도 가끔 삶거든요",
          "original_form": "저는 수건도 가끔 삶거든요",
          "speaker_id": "1",
          "time": "20210830 16:05"
        },
        {
          "id": "MDRW2100036088.1.9",
          "form": "소독되라구",
          "original_form": "소독되라구",
          "speaker_id": "1",
          "time": "20210830 16:05"
        },
        {
          "id": "MDRW2100036088.1.10",
          "form": "근데 가스레인지 쓰니까 뭔가 좀 답답해요",
          "original_form": "근데 가스레인지 쓰니까 뭔가 좀 답답해요",
          "speaker_id": "1",
          "time": "20210830 16:05"
        },
        {
          "id": "MDRW2100036088.1.11",
          "form": "하이라이트는 시간이 좀 걸려요",
          "original_form": "하이라이트는 시간이 좀 걸려요",
          "speaker_id": "2",
          "time": "20210830 16:05"
        },
        {
          "id": "MDRW2100036088.1.12",
          "form": "인덕션은 엄청 빨리 달궈진대요",
          "original_form": "인덕션은 엄청 빨리 달궈진대요",
          "speaker_id": "2",
          "time": "20210830 16:05"
        },
        {
          "id": "MDRW2100036088.1.13",
          "form": "그리고 손 데일 일도 없대요",
          "original_form": "그리고 손 데일 일도 없대요",
          "speaker_id": "2",
          "time": "20210830 16:05"
        },
        {
          "id": "MDRW2100036088.1.14",
          "form": "단점은 인덕션용 냄비가 따로 있다는거",
          "original_form": "단점은 인덕션용 냄비가 따로 있다는거",
          "speaker_id": "2",
          "time": "20210830 16:06"
        },
        {
          "id": "MDRW2100036088.1.15",
          "form": "아 진짜요? 안뜨겁다니 무슨 원리인지 신기하네요ㅋㅋ",
          "original_form": "아 진짜요? 안뜨겁다니 무슨 원리인지 신기하네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:06"
        },
        {
          "id": "MDRW2100036088.1.16",
          "form": "가전하나 잘만 바꾸면",
          "original_form": "가전하나 잘만 바꾸면",
          "speaker_id": "1",
          "time": "20210830 16:06"
        },
        {
          "id": "MDRW2100036088.1.17",
          "form": "집안일하기가 수월해질것같은데...",
          "original_form": "집안일하기가 수월해질것같은데...",
          "speaker_id": "1",
          "time": "20210830 16:06"
        },
        {
          "id": "MDRW2100036088.1.18",
          "form": "손을 대도 안뜨겁다네요.. 그래서 고양이가 실수로 켜도 불 안난다고",
          "original_form": "손을 대도 안뜨겁다네요.. 그래서 고양이가 실수로 켜도 불 안난다고",
          "speaker_id": "2",
          "time": "20210830 16:18"
        },
        {
          "id": "MDRW2100036088.1.19",
          "form": "신세계네요ㅋㅋㅋ",
          "original_form": "신세계네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:19"
        },
        {
          "id": "MDRW2100036088.1.20",
          "form": "아 여름에 불앞에서 수건삶는거 너무 힘들었는데ㅠ",
          "original_form": "아 여름에 불앞에서 수건삶는거 너무 힘들었는데ㅠ",
          "speaker_id": "1",
          "time": "20210830 16:19"
        },
        {
          "id": "MDRW2100036088.1.21",
          "form": "네.. 전기로 하는건데.. 덥지도 않나봐요.. 제가 안써봐서 모르겠지만",
          "original_form": "네.. 전기로 하는건데.. 덥지도 않나봐요.. 제가 안써봐서 모르겠지만",
          "speaker_id": "2",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100036088.1.22",
          "form": "좀 흥미가  생겼어요.. 나중에 바꾸게 되면 인덕션 해볼라구요",
          "original_form": "좀 흥미가  생겼어요.. 나중에 바꾸게 되면 인덕션 해볼라구요",
          "speaker_id": "2",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100036088.1.23",
          "form": "좋은 정보 감사해용ㅋㅋㅋ",
          "original_form": "좋은 정보 감사해용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100036088.1.24",
          "form": "저도 인덕션으로 바꿔볼까봐요...",
          "original_form": "저도 인덕션으로 바꿔볼까봐요...",
          "speaker_id": "1",
          "time": "20210830 16:25"
        },
        {
          "id": "MDRW2100036088.1.25",
          "form": "이 좋은걸 모르고 살다니",
          "original_form": "이 좋은걸 모르고 살다니",
          "speaker_id": "1",
          "time": "20210830 16:26"
        },
        {
          "id": "MDRW2100036088.1.26",
          "form": "네~ 일단 인기인건 확실하구요.. 그래도 이리저리 잘 알아보고 사네요.. 전 직접 구매해서 써 본게 아니라 들은 이야기라서요",
          "original_form": "네~ 일단 인기인건 확실하구요.. 그래도 이리저리 잘 알아보고 사네요.. 전 직접 구매해서 써 본게 아니라 들은 이야기라서요",
          "speaker_id": "2",
          "time": "20210830 16:32"
        },
        {
          "id": "MDRW2100036088.1.27",
          "form": "네네 말씀 감사해용ㅎㅎ",
          "original_form": "네네 말씀 감사해용ㅎㅎ",
