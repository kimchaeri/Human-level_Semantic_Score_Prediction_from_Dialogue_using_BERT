      "utterance": [
        {
          "id": "MDRW2100016856.1.1",
          "form": "더블포크쉬림프콘치즈라고 먹어봤는데 반해서 그거만 먹어요 ㅋㅋㅋ",
          "original_form": "더블포크쉬림프콘치즈라고 먹어봤는데 반해서 그거만 먹어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:54"
        },
        {
          "id": "MDRW2100016856.1.2",
          "form": "첨들어요 도미노껀가요?",
          "original_form": "첨들어요 도미노껀가요?",
          "speaker_id": "2",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016856.1.3",
          "form": "반올림피자에요! 최근에 알게 됐는데 맛있더라구요",
          "original_form": "반올림피자에요! 최근에 알게 됐는데 맛있더라구요",
          "speaker_id": "1",
          "time": "20210826 16:09"
        },
        {
          "id": "MDRW2100016856.1.4",
          "form": "아 들어봤어요. 다행히 저희 지역에도 있네요",
          "original_form": "아 들어봤어요. 다행히 저희 지역에도 있네요",
          "speaker_id": "2",
          "time": "20210826 16:13"
        },
        {
          "id": "MDRW2100016856.1.5",
          "form": "거긴 도우가 얇고 토핑이랑 치즈가 도톰해서 좋더라구요",
          "original_form": "거긴 도우가 얇고 토핑이랑 치즈가 도톰해서 좋더라구요",
          "speaker_id": "1",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016856.1.6",
          "form": "오 딱 제 스탈이에요 저도 치즈 두툼한거 좋아해요",
          "original_form": "오 딱 제 스탈이에요 저도 치즈 두툼한거 좋아해요",
          "speaker_id": "2",
          "time": "20210826 16:14"
        },
        {
          "id": "MDRW2100016856.1.7",
          "form": "저랑 비슷하시네요 ㅋㅋㅋ",
          "original_form": "저랑 비슷하시네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016856.1.8",
          "form": "토핑은 어떤거 좋아하세요?",
          "original_form": "토핑은 어떤거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 16:15"
        },
        {
          "id": "MDRW2100016856.1.9",
          "form": "토핑은 주로 고기나 포테이토 올라간거 많이 먹는거:같아요",
          "original_form": "토핑은 주로 고기나 포테이토 올라간거 많이 먹는거:같아요",
          "speaker_id": "2",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016856.1.10",
          "form": "포테이토가 사람들이 젤 좋아하더라구여",
          "original_form": "포테이토가 사람들이 젤 좋아하더라구여",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016856.1.11",
          "form": "소스는 갈릭소스ㅋㅋ",
          "original_form": "소스는 갈릭소스ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016856.1.12",
          "form": "제일 무난하죠ㅎㅎ",
          "original_form": "제일 무난하죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016856.1.13",
          "form": "저도 고기들어간거 제일 좋아해여 무조건 고기들은거로 시켜요 ㅋㅋㅋ",
          "original_form": "저도 고기들어간거 제일 좋아해여 무조건 고기들은거로 시켜요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016856.1.14",
          "form": "갈릭소스 추가 필수죠 ㅠㅠ",
          "original_form": "갈릭소스 추가 필수죠 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 16:16"
        },
        {
          "id": "MDRW2100016856.1.15",
          "form": "저도 육식파에요",
          "original_form": "저도 육식파에요",
          "speaker_id": "2",
          "time": "20210826 16:17"
        },
        {
          "id": "MDRW2100016856.1.16",
          "form": "어떤 고기 좋아하세요?",
          "original_form": "어떤 고기 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 16:17"
        }
      ]
