      "utterance": [
        {
          "id": "MDRW2100040575.1.1",
          "form": "세탁기에 한번빨앗는데 티셔츠",
          "original_form": "세탁기에 한번빨앗는데 티셔츠",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040575.1.2",
          "form": "저 거기서 옷 몇 벙 샀는데",
          "original_form": "저 거기서 옷 몇 벙 샀는데",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040575.1.3",
          "form": "앜ㅋㅋㅋ 맞아요 저도",
          "original_form": "앜ㅋㅋㅋ 맞아요 저도",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040575.1.4",
          "form": "늘어났어요....",
          "original_form": "늘어났어요....",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040575.1.5",
          "form": "실 겁나 막 엄청 풀어자고",
          "original_form": "실 겁나 막 엄청 풀어자고",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040575.1.6",
          "form": "그래도 그냥 입고 다니긴 하는데(?)",
          "original_form": "그래도 그냥 입고 다니긴 하는데(?)",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040575.1.7",
          "form": "퀄리티가 절대네버",
          "original_form": "퀄리티가 절대네버",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040575.1.8",
          "form": "그 가격아님",
          "original_form": "그 가격아님",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040575.1.9",
          "form": "왜냐면 입을 옷 없어서",
          "original_form": "왜냐면 입을 옷 없어서",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040575.1.10",
          "form": "맞아욬ㅋㅋㅋ제친구도 잘 입고는다니는데",
          "original_form": "맞아욬ㅋㅋㅋ제친구도 잘 입고는다니는데",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040575.1.11",
          "form": "같은 생각 하고 계시는군요",
          "original_form": "같은 생각 하고 계시는군요",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040575.1.12",
          "form": "그 가격은 정말",
          "original_form": "그 가격은 정말",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040575.1.13",
          "form": "도대체",
          "original_form": "도대체",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040575.1.14",
          "form": "그리고 디자인이랑",
          "original_form": "그리고 디자인이랑",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040575.1.15",
          "form": "핏이 너무",
          "original_form": "핏이 너무",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040575.1.16",
          "form": "ㅋㅋㅋㅋㅋ네 두세번 시즌마다 몇십만원씩 사던 앤데",
          "original_form": "ㅋㅋㅋㅋㅋ네 두세번 시즌마다 몇십만원씩 사던 앤데",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040575.1.17",
          "form": "요즘은 안사요",
          "original_form": "요즘은 안사요",
          "speaker_id": "2",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040575.1.18",
          "form": "그들 위주라서",
          "original_form": "그들 위주라서",
          "speaker_id": "1",
          "time": "20210904 22:50"
        },
        {
          "id": "MDRW2100040575.1.19",
          "form": "맞아요 작고 마른 사람들",
          "original_form": "맞아요 작고 마른 사람들",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040575.1.20",
          "form": "절대못닙음ㅋㅋㅋㅋ",
          "original_form": "절대못닙음ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040575.1.21",
          "form": "그춐ㅋㅋㅋㅋ 아니 근데 다른 분들도 그러지 않을까요?",
          "original_form": "그춐ㅋㅋㅋㅋ 아니 근데 다른 분들도 그러지 않을까요?",
          "speaker_id": "1",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040575.1.22",
          "form": "여남공용으 ㅣ 문제점이에요 그건",
          "original_form": "여남공용으 ㅣ 문제점이에요 그건",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040575.1.23",
          "form": "옷이 너무커요",
          "original_form": "옷이 너무커요",
          "speaker_id": "2",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040575.1.24",
          "form": "저희처럼 느끼는 분들이",
          "original_form": "저희처럼 느끼는 분들이",
          "speaker_id": "1",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040575.1.25",
          "form": "많을거같은대",
          "original_form": "많을거같은대",
          "speaker_id": "1",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040575.1.26",
          "form": "맞아요 저도",
          "original_form": "맞아요 저도",
          "speaker_id": "1",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040575.1.27",
          "form": "거기서 산 맨투맨 5만원인가",
          "original_form": "거기서 산 맨투맨 5만원인가",
          "speaker_id": "1",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040575.1.28",
          "form": "넘게 주고 샀는데",
          "original_form": "넘게 주고 샀는데",
          "speaker_id": "1",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040575.1.29",
          "form": "너무 핏이 안예뻐서",
          "original_form": "너무 핏이 안예뻐서",
          "speaker_id": "1",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040575.1.30",
          "form": "왠... 뒷허리에 알 수 없는 그림 그려져있고",
          "original_form": "왠... 뒷허리에 알 수 없는 그림 그려져있고",
          "speaker_id": "1",
          "time": "20210904 22:51"
        },
        {
          "id": "MDRW2100040575.1.31",
          "form": "저도 사고싶엇는데 그냥 포기햇어요",
          "original_form": "저도 사고싶엇는데 그냥 포기햇어요",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040575.1.32",
          "form": "저희 영상중에 룩북잇는데",
          "original_form": "저희 영상중에 룩북잇는데",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100040575.1.33",
          "form": "거기에 아인이랑 저의 옷핏차이",
          "original_form": "거기에 아인이랑 저의 옷핏차이",
          "speaker_id": "2",
          "time": "20210904 22:52"
        }
      ]
    }
