      "utterance": [
        {
          "id": "MDRW2100042052.1.1",
          "form": "헉 네네 좋아해요",
          "original_form": "헉 네네 좋아해요",
          "speaker_id": "2",
          "time": "20210906 19:11"
        },
        {
          "id": "MDRW2100042052.1.2",
          "form": "저녁 드셧나요??",
          "original_form": "저녁 드셧나요??",
          "speaker_id": "1",
          "time": "20210906 19:13"
        },
        {
          "id": "MDRW2100042052.1.3",
          "form": "아니요! 드셨어요?",
          "original_form": "아니요! 드셨어요?",
          "speaker_id": "2",
          "time": "20210906 19:14"
        },
        {
          "id": "MDRW2100042052.1.4",
          "form": "아직이요..!!! 머먹을지 고민이예요",
          "original_form": "아직이요..!!! 머먹을지 고민이예요",
          "speaker_id": "1",
          "time": "20210906 19:14"
        },
        {
          "id": "MDRW2100042052.1.5",
          "form": "헉 뭐 드셨나요!",
          "original_form": "헉 뭐 드셨나요!",
          "speaker_id": "2",
          "time": "20210907 07:47"
        },
        {
          "id": "MDRW2100042052.1.6",
          "form": "비빔면에 볶음밥 먹어ㅛ어용 ㅋㅋㅋㅋ",
          "original_form": "비빔면에 볶음밥 먹어ㅛ어용 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 09:16"
        },
        {
          "id": "MDRW2100042052.1.7",
          "form": "벌써 아침이네요 ㅠㅠ",
          "original_form": "벌써 아침이네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 09:17"
        },
        {
          "id": "MDRW2100042052.1.8",
          "form": "식사하셧어요?",
          "original_form": "식사하셧어요?",
          "speaker_id": "1",
          "time": "20210907 09:17"
        },
        {
          "id": "MDRW2100042052.1.9",
          "form": "네네! 와 맛있었겠네요 저는 오늘 점심 돈가스 먹었어요",
          "original_form": "네네! 와 맛있었겠네요 저는 오늘 점심 돈가스 먹었어요",
          "speaker_id": "2",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100042052.1.10",
          "form": "이제 퇴근해서 저녁 먹으려구요ㅋㅋㅋ",
          "original_form": "이제 퇴근해서 저녁 먹으려구요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:59"
        },
        {
          "id": "MDRW2100042052.1.11",
          "form": "오오 돈가스 !!",
          "original_form": "오오 돈가스 !!",
          "speaker_id": "1",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100042052.1.12",
          "form": "저녁 어떤거 드세요?",
          "original_form": "저녁 어떤거 드세요?",
          "speaker_id": "1",
          "time": "20210907 19:00"
        },
        {
          "id": "MDRW2100042052.1.13",
          "form": "아직 잘 모르겠어요!! 집 가는 길이라",
          "original_form": "아직 잘 모르겠어요!! 집 가는 길이라",
          "speaker_id": "2",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100042052.1.14",
          "form": "오 저는 김치찌개에 밥 먹을려구용!!",
          "original_form": "오 저는 김치찌개에 밥 먹을려구용!!",
          "speaker_id": "1",
          "time": "20210907 19:02"
        },
        {
          "id": "MDRW2100042052.1.15",
          "form": "와 김치찌개 너무 좋죠 참치 넣는 거 좋아하시나요?",
          "original_form": "와 김치찌개 너무 좋죠 참치 넣는 거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100042052.1.16",
          "form": "네네!!! 오 맞아요 참치 들어갓어여 ㅋㅋ",
          "original_form": "네네!!! 오 맞아요 참치 들어갓어여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 19:03"
        },
        {
          "id": "MDRW2100042052.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ참치 김치찌개 맛있어서 저도 좋아해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ참치 김치찌개 맛있어서 저도 좋아해요",
