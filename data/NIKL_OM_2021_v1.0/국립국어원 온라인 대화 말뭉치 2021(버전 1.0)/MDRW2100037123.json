      "utterance": [
        {
          "id": "MDRW2100037123.1.1",
          "form": "김치랑 초콜릿 냉면 메밀국수 티라미수케익 등등이요ㅋㅋ",
          "original_form": "김치랑 초콜릿 냉면 메밀국수 티라미수케익 등등이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:13"
        },
        {
          "id": "MDRW2100037123.1.2",
          "form": "오 많이 이용했군요",
          "original_form": "오 많이 이용했군요",
          "speaker_id": "1",
          "time": "20210831 20:14"
        },
        {
          "id": "MDRW2100037123.1.3",
          "form": "뭐가 제일",
          "original_form": "뭐가 제일",
          "speaker_id": "1",
          "time": "20210831 20:14"
        },
        {
          "id": "MDRW2100037123.1.4",
          "form": "마음에 드셨나요",
          "original_form": "마음에 드셨나요",
          "speaker_id": "1",
          "time": "20210831 20:14"
        },
        {
          "id": "MDRW2100037123.1.5",
          "form": "메밀국수요ㅋㅋ여기서밖에 안팔더라구요",
          "original_form": "메밀국수요ㅋㅋ여기서밖에 안팔더라구요",
          "speaker_id": "2",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100037123.1.6",
          "form": "제가 좋아하는 메밀국수를요",
          "original_form": "제가 좋아하는 메밀국수를요",
          "speaker_id": "2",
          "time": "20210831 20:51"
        },
        {
          "id": "MDRW2100037123.1.7",
          "form": "까악 !!!!!!",
          "original_form": "까악 !!!!!!",
          "speaker_id": "1",
          "time": "20210831 20:52"
        },
        {
          "id": "MDRW2100037123.1.8",
          "form": "거기 메밀 맛있나요",
          "original_form": "거기 메밀 맛있나요",
          "speaker_id": "1",
          "time": "20210831 20:52"
        },
        {
          "id": "MDRW2100037123.1.9",
          "form": "네ㅋㅋ엄청 오래된 메밀국수집인데 미슐랭받았다고 들었어요",
          "original_form": "네ㅋㅋ엄청 오래된 메밀국수집인데 미슐랭받았다고 들었어요",
          "speaker_id": "2",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100037123.1.10",
          "form": "저희 외증조할아버지도 맛있다고 가셨던 곳이었대요ㅋㅋ",
          "original_form": "저희 외증조할아버지도 맛있다고 가셨던 곳이었대요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:00"
        },
        {
          "id": "MDRW2100037123.1.11",
          "form": "허그.ㅡ...",
          "original_form": "허그.ㅡ...",
          "speaker_id": "1",
          "time": "20210831 21:01"
        },
        {
          "id": "MDRW2100037123.1.12",
          "form": "가게 이름이 뭐죠",
          "original_form": "가게 이름이 뭐죠",
          "speaker_id": "1",
          "time": "20210831 21:01"
        },
        {
          "id": "MDRW2100037123.1.13",
          "form": "저 메밀 좋아해요",
          "original_form": "저 메밀 좋아해요",
          "speaker_id": "1",
          "time": "20210831 21:01"
        },
        {
          "id": "MDRW2100037123.1.14",
          "form": "광화문미진이라고 혹시 아세요?",
          "original_form": "광화문미진이라고 혹시 아세요?",
          "speaker_id": "2",
          "time": "20210831 21:01"
        },
        {
          "id": "MDRW2100037123.1.15",
          "form": "오 아니요",
          "original_form": "오 아니요",
          "speaker_id": "1",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100037123.1.16",
          "form": "체인점일까요??",
          "original_form": "체인점일까요??",
          "speaker_id": "1",
          "time": "20210901 01:20"
        },
        {
          "id": "MDRW2100037123.1.17",
          "form": "체인 몇군데 있네용",
          "original_form": "체인 몇군데 있네용",
          "speaker_id": "2",
          "time": "20210901 12:56"
        },
        {
          "id": "MDRW2100037123.1.18",
          "form": "근데 처음 듣는 거 보니",
          "original_form": "근데 처음 듣는 거 보니",
          "speaker_id": "1",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100037123.1.19",
          "form": "저희쪽에는",
          "original_form": "저희쪽에는",
          "speaker_id": "1",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100037123.1.20",
          "form": "없응 거 같아요..",
          "original_form": "없응 거 같아요..",
          "speaker_id": "1",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100037123.1.21",
          "form": "아숩",
          "original_form": "아숩",
          "speaker_id": "1",
          "time": "20210901 13:02"
        },
        {
          "id": "MDRW2100037123.1.22",
          "form": "ㅜ 마켓컬리에서라도 한 번 시켜먹어보세요ㅎ",
          "original_form": "ㅜ 마켓컬리에서라도 한 번 시켜먹어보세요ㅎ",
          "speaker_id": "2",
          "time": "20210901 13:03"
        },
        {
          "id": "MDRW2100037123.1.23",
          "form": "그래야겠어요ㅋㅋㅋㅋㅋ",
          "original_form": "그래야겠어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 14:37"
        },
        {
          "id": "MDRW2100037123.1.24",
          "form": "진미집? 여기도 유명해요",
          "original_form": "진미집? 여기도 유명해요",
          "speaker_id": "1",
          "time": "20210901 14:37"
        },
        {
          "id": "MDRW2100037123.1.25",
          "form": "전 배민라이브로 시켜먹어봤거든요",
          "original_form": "전 배민라이브로 시켜먹어봤거든요",
          "speaker_id": "1",
          "time": "20210901 14:38"
        },
        {
          "id": "MDRW2100037123.1.26",
          "form": "찾아보니 백반기행에 나왔던 집이네요!",
          "original_form": "찾아보니 백반기행에 나왔던 집이네요!",
          "speaker_id": "2",
          "time": "20210901 14:55"
        },
        {
          "id": "MDRW2100037123.1.27",
          "form": "오 그랴요?",
          "original_form": "오 그랴요?",
          "speaker_id": "1",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100037123.1.28",
          "form": "쨋든 여기도 괜탆아요!!",
          "original_form": "쨋든 여기도 괜탆아요!!",
          "speaker_id": "1",
          "time": "20210901 15:29"
        },
        {
          "id": "MDRW2100037123.1.29",
          "form": "그쪽으로 여행가게되면 가봐야겠어요!",
          "original_form": "그쪽으로 여행가게되면 가봐야겠어요!",
          "speaker_id": "2",
          "time": "20210901 15:33"
        }
      ]
    }
  ]
