      "utterance": [
        {
          "id": "MDRW2100043650.1.1",
          "form": "ㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋ그리고",
          "original_form": "ㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋ그리고",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043650.1.2",
          "form": "칼국수는 안그래도 국물 엄청 되직한편인데",
          "original_form": "칼국수는 안그래도 국물 엄청 되직한편인데",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043650.1.3",
          "form": "들깨가루까지 넣으면 너무 퍽퍽해",
          "original_form": "들깨가루까지 넣으면 너무 퍽퍽해",
          "speaker_id": "2",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043650.1.4",
          "form": "ㅇㅇ 거의 들깨죽 느낌이긴 해",
          "original_form": "ㅇㅇ 거의 들깨죽 느낌이긴 해",
          "speaker_id": "1",
          "time": "20210907 19:51"
        },
        {
          "id": "MDRW2100043650.1.5",
          "form": "아 비빔막국수 먹을까",
          "original_form": "아 비빔막국수 먹을까",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043650.1.6",
          "form": "그건 국물이 아니잖아ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그건 국물이 아니잖아ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043650.1.7",
          "form": "ㅋㅋㅋㅋㅋ 글킨 한데",
          "original_form": "ㅋㅋㅋㅋㅋ 글킨 한데",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043650.1.8",
          "form": "이집이 비빔 맛집이라네",
          "original_form": "이집이 비빔 맛집이라네",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043650.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043650.1.10",
          "form": "근데 비빔 먹으면",
          "original_form": "근데 비빔 먹으면",
          "speaker_id": "2",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100043650.1.11",
          "form": "늘 국물이 아쉬워서",
          "original_form": "늘 국물이 아쉬워서",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043650.1.12",
          "form": "맞아 그렇긴 해....",
          "original_form": "맞아 그렇긴 해....",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043650.1.13",
          "form": "국물을 먹는다고 비빔이 아쉽진",
          "original_form": "국물을 먹는다고 비빔이 아쉽진",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043650.1.14",
          "form": "않단말이지....",
          "original_form": "않단말이지....",
          "speaker_id": "2",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043650.1.15",
          "form": "ㅇㅇ 그래서 고민임...",
          "original_form": "ㅇㅇ 그래서 고민임...",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043650.1.16",
          "form": "국물은 김치로도 충분한데",
          "original_form": "국물은 김치로도 충분한데",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043650.1.17",
          "form": "비빔은 국물이 땡겨",
          "original_form": "비빔은 국물이 땡겨",
          "speaker_id": "1",
          "time": "20210907 19:53"
        },
        {
          "id": "MDRW2100043650.1.18",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043650.1.19",
          "form": "그치만 그거 맛집이면",
          "original_form": "그치만 그거 맛집이면",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043650.1.20",
          "form": "그거부터 먹어보긴 해야겠지?",
          "original_form": "그거부터 먹어보긴 해야겠지?",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043650.1.21",
          "form": "사실 만두랑 메밀 맛집이라는데",
          "original_form": "사실 만두랑 메밀 맛집이라는데",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043650.1.22",
          "form": "메밀이 뜨거운건 식감이 별로거든",
          "original_form": "메밀이 뜨거운건 식감이 별로거든",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043650.1.23",
          "form": "오그럼",
          "original_form": "오그럼",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043650.1.24",
          "form": "메밀지짐만두 있나?",
          "original_form": "메밀지짐만두 있나?",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043650.1.25",
          "form": "그건 없어....",
          "original_form": "그건 없어....",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043650.1.26",
          "form": "그런거 진짜 맛있는데",
          "original_form": "그런거 진짜 맛있는데",
          "speaker_id": "2",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043650.1.27",
          "form": "찐만두뿐임",
          "original_form": "찐만두뿐임",
          "speaker_id": "1",
          "time": "20210907 19:54"
        },
        {
          "id": "MDRW2100043650.1.28",
          "form": "쳇.....",
          "original_form": "쳇.....",
          "speaker_id": "2",
