      "utterance": [
        {
          "id": "MDRW2100010050.1.1",
          "form": "엉 아빠 일어나서 출출하다고 파이드시고",
          "original_form": "엉 아빠 일어나서 출출하다고 파이드시고",
          "speaker_id": "2",
          "time": "20210823 13:34"
        },
        {
          "id": "MDRW2100010050.1.2",
          "form": "점심은 안차려 먹고?",
          "original_form": "점심은 안차려 먹고?",
          "speaker_id": "1",
          "time": "20210823 13:35"
        },
        {
          "id": "MDRW2100010050.1.3",
          "form": "축 처져있긴 한데 나쁘진 않은듯",
          "original_form": "축 처져있긴 한데 나쁘진 않은듯",
          "speaker_id": "2",
          "time": "20210823 13:35"
        },
        {
          "id": "MDRW2100010050.1.4",
          "form": "어 나도 아침겸 점심으로 넘겼어",
          "original_form": "어 나도 아침겸 점심으로 넘겼어",
          "speaker_id": "2",
          "time": "20210823 13:35"
        },
        {
          "id": "MDRW2100010050.1.5",
          "form": "응 오늘 식사 챙기기는 소홀하겠군 서로",
          "original_form": "응 오늘 식사 챙기기는 소홀하겠군 서로",
          "speaker_id": "1",
          "time": "20210823 13:35"
        },
        {
          "id": "MDRW2100010050.1.6",
          "form": "엄마는 따로 연락 없지?",
          "original_form": "엄마는 따로 연락 없지?",
          "speaker_id": "1",
          "time": "20210823 13:35"
        },
        {
          "id": "MDRW2100010050.1.7",
          "form": "응 아까 아빠전화벨 울리긴 했는데",
          "original_form": "응 아까 아빠전화벨 울리긴 했는데",
          "speaker_id": "2",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100010050.1.8",
          "form": "그게엄만지는 잘 모르겠네",
          "original_form": "그게엄만지는 잘 모르겠네",
          "speaker_id": "2",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100010050.1.9",
          "form": "아빠걱정돼서 연락한겨?",
          "original_form": "아빠걱정돼서 연락한겨?",
          "speaker_id": "2",
          "time": "20210823 13:36"
        },
        {
          "id": "MDRW2100010050.1.10",
          "form": "응 겸사겸사",
          "original_form": "응 겸사겸사",
          "speaker_id": "1",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100010050.1.11",
          "form": "오전에 정신없다가 오후 되니 가족들 안부가 궁금해서",
          "original_form": "오전에 정신없다가 오후 되니 가족들 안부가 궁금해서",
          "speaker_id": "1",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100010050.1.12",
          "form": "가족걱정에 여념이 없구먼",
          "original_form": "가족걱정에 여념이 없구먼",
          "speaker_id": "2",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100010050.1.13",
          "form": "날씨 이런데 넌 괜찮아?",
          "original_form": "날씨 이런데 넌 괜찮아?",
          "speaker_id": "2",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100010050.1.14",
          "form": "그럼 가족에 대한 사랑이 넘치지",
          "original_form": "그럼 가족에 대한 사랑이 넘치지",
          "speaker_id": "1",
          "time": "20210823 13:37"
        },
        {
          "id": "MDRW2100010050.1.15",
          "form": "응 난 괜찮아",
          "original_form": "응 난 괜찮아",
          "speaker_id": "1",
          "time": "20210823 13:38"
        },
        {
          "id": "MDRW2100010050.1.16",
          "form": "가족들이 비오고 저기압이면 상태가 안좋아서",
          "original_form": "가족들이 비오고 저기압이면 상태가 안좋아서",
          "speaker_id": "2",
          "time": "20210823 13:38"
        },
        {
          "id": "MDRW2100010050.1.17",
          "form": "응 다들 그렇지",
          "original_form": "응 다들 그렇지",
          "speaker_id": "1",
          "time": "20210823 13:38"
        },
        {
          "id": "MDRW2100010050.1.18",
          "form": "엄마 회사일은 잘 되고 있으려나",
          "original_form": "엄마 회사일은 잘 되고 있으려나",
          "speaker_id": "2",
          "time": "20210823 13:38"
        },
        {
          "id": "MDRW2100010050.1.19",
          "form": "어제 저녁에도 막 연락하고 그랬는디",
          "original_form": "어제 저녁에도 막 연락하고 그랬는디",
          "speaker_id": "2",
          "time": "20210823 13:38"
        },
        {
          "id": "MDRW2100010050.1.20",
          "form": "그러게 엄마가 오늘 출근하면서도 한 걱정했는데",
          "original_form": "그러게 엄마가 오늘 출근하면서도 한 걱정했는데",
          "speaker_id": "1",
          "time": "20210823 13:38"
        },
        {
          "id": "MDRW2100010050.1.21",
          "form": "가족들한테 연락할 겨를도 없을겨",
          "original_form": "가족들한테 연락할 겨를도 없을겨",
          "speaker_id": "1",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100010050.1.22",
          "form": "이따 퇴근하면 얘기들을 수 있겠지",
          "original_form": "이따 퇴근하면 얘기들을 수 있겠지",
          "speaker_id": "1",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100010050.1.23",
          "form": "하긴 아침에 둘이 같이 나갔지",
          "original_form": "하긴 아침에 둘이 같이 나갔지",
          "speaker_id": "2",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100010050.1.24",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100010050.1.25",
          "form": "가족들 다 무사히 하루 보내고 만나면 좋겠네",
          "original_form": "가족들 다 무사히 하루 보내고 만나면 좋겠네",
          "speaker_id": "2",
          "time": "20210823 13:39"
        },
        {
          "id": "MDRW2100010050.1.26",
          "form": "오늘은 집안일 하지 말고 푹 쉬어~",
          "original_form": "오늘은 집안일 하지 말고 푹 쉬어~",
          "speaker_id": "1",
          "time": "20210823 13:39"
        }
      ]
    }
  ]
