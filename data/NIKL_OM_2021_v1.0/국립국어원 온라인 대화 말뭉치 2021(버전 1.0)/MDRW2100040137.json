      "utterance": [
        {
          "id": "MDRW2100040137.1.1",
          "form": "여자애들고 조용하고 잘 맞춰주는남자애좋아해요",
          "original_form": "여자애들고 조용하고 잘 맞춰주는남자애좋아해요",
          "speaker_id": "2",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100040137.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100040137.1.3",
          "form": "저희 엄마는 우리 애들을 못봐주셔요 ㅠ 너무 힘들다고요 ㅠ",
          "original_form": "저희 엄마는 우리 애들을 못봐주셔요 ㅠ 너무 힘들다고요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100040137.1.4",
          "form": "딸들은편하죠",
          "original_form": "딸들은편하죠",
          "speaker_id": "2",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100040137.1.5",
          "form": "손가는게 별로없어서",
          "original_form": "손가는게 별로없어서",
          "speaker_id": "2",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100040137.1.6",
          "form": "울 엄마는 딸만 둘 키웠거든요",
          "original_form": "울 엄마는 딸만 둘 키웠거든요",
          "speaker_id": "1",
          "time": "20210904 11:56"
        },
        {
          "id": "MDRW2100040137.1.7",
          "form": "우리 애들이 이렇게 심난한거 보고 니가 애를 잘못 키웠다는 둥 ㅠ",
          "original_form": "우리 애들이 이렇게 심난한거 보고 니가 애를 잘못 키웠다는 둥 ㅠ",
          "speaker_id": "1",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100040137.1.8",
          "form": "우리 애들은 남자치곤 얌전한 쪽에 속하는것 같은데요...",
          "original_form": "우리 애들은 남자치곤 얌전한 쪽에 속하는것 같은데요...",
          "speaker_id": "1",
          "time": "20210904 11:57"
        },
        {
          "id": "MDRW2100040137.1.9",
          "form": "엄마가그런말씀하시면 은근기분상하죠",
          "original_form": "엄마가그런말씀하시면 은근기분상하죠",
          "speaker_id": "2",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100040137.1.10",
          "form": "맞아요 ㅠ",
          "original_form": "맞아요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 11:58"
        },
        {
          "id": "MDRW2100040137.1.11",
          "form": "전 나름 고생고생 하면서 키우고 있는데 말이죠 ㅠ",
          "original_form": "전 나름 고생고생 하면서 키우고 있는데 말이죠 ㅠ",
          "speaker_id": "1",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100040137.1.12",
          "form": "애 키우길때 양가 도움 받으셨나요?",
          "original_form": "애 키우길때 양가 도움 받으셨나요?",
          "speaker_id": "1",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100040137.1.13",
          "form": "아들둘이라 더그런건데",
          "original_form": "아들둘이라 더그런건데",
          "speaker_id": "2",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100040137.1.14",
          "form": "도와주지않으면서 잔소리아닌 잔소리를하시면",
          "original_form": "도와주지않으면서 잔소리아닌 잔소리를하시면",
          "speaker_id": "2",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100040137.1.15",
          "form": "친정 조금",
          "original_form": "친정 조금",
          "speaker_id": "2",
          "time": "20210904 11:59"
        },
        {
          "id": "MDRW2100040137.1.16",
          "form": "저도그때 짜증났어요",
          "original_form": "저도그때 짜증났어요",
          "speaker_id": "2",
          "time": "20210904 12:00"
        },
        {
          "id": "MDRW2100040137.1.17",
          "form": "아.. 다 그렇군요 ㅠ",
          "original_form": "아.. 다 그렇군요 ㅠ",
          "speaker_id": "1",
          "time": "20210904 12:26"
        },
        {
          "id": "MDRW2100040137.1.18",
          "form": "맞아요 도와주지도 않으면서 잔소리만 ㅠ",
          "original_form": "맞아요 도와주지도 않으면서 잔소리만 ㅠ",
          "speaker_id": "1",
          "time": "20210904 12:26"
        },
        {
          "id": "MDRW2100040137.1.19",
          "form": "나중에 저는 안그럴꺼라고 다짐했네요",
          "original_form": "나중에 저는 안그럴꺼라고 다짐했네요",
          "speaker_id": "2",
          "time": "20210904 12:27"
        },
        {
          "id": "MDRW2100040137.1.20",
          "form": "애키우는것도 힘든데 엄마눈치까지",
          "original_form": "애키우는것도 힘든데 엄마눈치까지",
          "speaker_id": "2",
          "time": "20210904 12:27"
        },
        {
          "id": "MDRW2100040137.1.21",
          "form": "맞아요 저도요 ㅠ 물론 며느리가 저에겐 애를 안맡기겠지만요 ㅎㅎ",
          "original_form": "맞아요 저도요 ㅠ 물론 며느리가 저에겐 애를 안맡기겠지만요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 12:32"
        },
        {
          "id": "MDRW2100040137.1.22",
          "form": "며느리는 더불편할듯",
          "original_form": "며느리는 더불편할듯",
          "speaker_id": "2",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100040137.1.23",
          "form": "어찌보면 멀리사는게 좋을것같아요",
          "original_form": "어찌보면 멀리사는게 좋을것같아요",
          "speaker_id": "2",
          "time": "20210904 12:35"
        },
        {
          "id": "MDRW2100040137.1.24",
          "form": "이럴땐 아들만 둘인것도 나쁘지 않은것 같아요 ㅋㅋ",
          "original_form": "이럴땐 아들만 둘인것도 나쁘지 않은것 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100040137.1.25",
          "form": "황혼육아 걱정이 없고 ㅋ",
          "original_form": "황혼육아 걱정이 없고 ㅋ",
          "speaker_id": "1",
          "time": "20210904 12:37"
        },
        {
          "id": "MDRW2100040137.1.26",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210904 12:40"
        },
        {
          "id": "MDRW2100040137.1.27",
          "form": "딸들 안봐주면 서운해할듯해요",
          "original_form": "딸들 안봐주면 서운해할듯해요",
          "speaker_id": "2",
          "time": "20210904 12:41"
        }
      ]
    }
