      "utterance": [
        {
          "id": "MDRW2100025770.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 16:51"
        },
        {
          "id": "MDRW2100025770.1.2",
          "form": "좋아하는 음식이 뭔가요?",
          "original_form": "좋아하는 음식이 뭔가요?",
          "speaker_id": "2",
          "time": "20210901 17:20"
        },
        {
          "id": "MDRW2100025770.1.3",
          "form": "전 고기류 좋아해요",
          "original_form": "전 고기류 좋아해요",
          "speaker_id": "1",
          "time": "20210901 17:38"
        },
        {
          "id": "MDRW2100025770.1.4",
          "form": "오 저는 고기보다는 해산물 파",
          "original_form": "오 저는 고기보다는 해산물 파",
          "speaker_id": "2",
          "time": "20210901 17:44"
        },
        {
          "id": "MDRW2100025770.1.5",
          "form": "어떤 해산물을 좋아하세요?",
          "original_form": "어떤 해산물을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 17:45"
        },
        {
          "id": "MDRW2100025770.1.6",
          "form": "새우를 제일 좋아해요",
          "original_form": "새우를 제일 좋아해요",
          "speaker_id": "2",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025770.1.7",
          "form": "새우좋아하시나요",
          "original_form": "새우좋아하시나요",
          "speaker_id": "2",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025770.1.8",
          "form": "저는 비린걸 싫어해서",
          "original_form": "저는 비린걸 싫어해서",
          "speaker_id": "1",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025770.1.9",
          "form": "새우류는 익히면 맛있게 먹어요",
          "original_form": "새우류는 익히면 맛있게 먹어요",
          "speaker_id": "1",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025770.1.10",
          "form": "생새우 이런건 못먹겠더라구여",
          "original_form": "생새우 이런건 못먹겠더라구여",
          "speaker_id": "1",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100025770.1.11",
          "form": "오 그러면 못드시는 해산물이 많겠네요",
          "original_form": "오 그러면 못드시는 해산물이 많겠네요",
          "speaker_id": "2",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100025770.1.12",
          "form": "고기는 뭘 제일 좋아하셔요?",
          "original_form": "고기는 뭘 제일 좋아하셔요?",
          "speaker_id": "2",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100025770.1.13",
          "form": "저는 돼지고기류가 가장 맛난거같아요",
          "original_form": "저는 돼지고기류가 가장 맛난거같아요",
          "speaker_id": "1",
          "time": "20210901 17:50"
        },
        {
          "id": "MDRW2100025770.1.14",
          "form": "저는 요즘은 수육 좋아해요",
          "original_form": "저는 요즘은 수육 좋아해요",
          "speaker_id": "2",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100025770.1.15",
          "form": "저는 목살 구워먹는거 좋아해요",
          "original_form": "저는 목살 구워먹는거 좋아해요",
          "speaker_id": "1",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100025770.1.16",
          "form": "너무 기름 진 건 안좋아하시는군요 ㅋㅋ",
          "original_form": "너무 기름 진 건 안좋아하시는군요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:57"
        },
        {
          "id": "MDRW2100025770.1.17",
          "form": "삼겹살도 좋아해요 ㅋㅋ",
          "original_form": "삼겹살도 좋아해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100025770.1.18",
          "form": "사실 주로 군것질하는걸 좋아해요",
