      "utterance": [
        {
          "id": "MDRW2100039713.1.1",
          "form": "영화를 언제볼지 고민이되네요",
          "original_form": "영화를 언제볼지 고민이되네요",
          "speaker_id": "1",
          "time": "20210903 20:50"
        },
        {
          "id": "MDRW2100039713.1.2",
          "form": "그러게요 고민해보세요",
          "original_form": "그러게요 고민해보세요",
          "speaker_id": "2",
          "time": "20210903 20:57"
        },
        {
          "id": "MDRW2100039713.1.3",
          "form": "적으몈서 할려니 머리가 안돌아가요",
          "original_form": "적으몈서 할려니 머리가 안돌아가요",
          "speaker_id": "1",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100039713.1.4",
          "form": "전화는 안대나요",
          "original_form": "전화는 안대나요",
          "speaker_id": "1",
          "time": "20210903 20:59"
        },
        {
          "id": "MDRW2100039713.1.5",
          "form": "뭘적어요?전화는안돼요",
          "original_form": "뭘적어요?전화는안돼요",
          "speaker_id": "2",
          "time": "20210903 21:41"
        },
        {
          "id": "MDRW2100039713.1.6",
          "form": "너무하시네",
          "original_form": "너무하시네",
          "speaker_id": "1",
          "time": "20210903 21:43"
        },
        {
          "id": "MDRW2100039713.1.7",
          "form": "맞아요 잠이오려고해요",
          "original_form": "맞아요 잠이오려고해요",
          "speaker_id": "2",
          "time": "20210903 21:58"
        },
        {
          "id": "MDRW2100039713.1.8",
          "form": "나빠요 선생님",
          "original_form": "나빠요 선생님",
          "speaker_id": "1",
          "time": "20210903 21:58"
        },
        {
          "id": "MDRW2100039713.1.9",
          "form": "포켓몬 잠만보",
          "original_form": "포켓몬 잠만보",
          "speaker_id": "1",
          "time": "20210903 22:42"
        },
        {
          "id": "MDRW2100039713.1.10",
          "form": "잠만보 옛날 별명이었는데ㅋ",
          "original_form": "잠만보 옛날 별명이었는데ㅋ",
          "speaker_id": "2",
          "time": "20210904 08:32"
        },
        {
          "id": "MDRW2100039713.1.11",
          "form": "제가 잠만보네요",
          "original_form": "제가 잠만보네요",
          "speaker_id": "1",
          "time": "20210904 11:55"
        },
        {
          "id": "MDRW2100039713.1.12",
          "form": "점심뉴스 시간에 일났어요",
          "original_form": "점심뉴스 시간에 일났어요",
          "speaker_id": "1",
          "time": "20210904 11:55"
        },
        {
          "id": "MDRW2100039713.1.13",
          "form": "잘하셨어요",
          "original_form": "잘하셨어요",
          "speaker_id": "2",
          "time": "20210904 13:36"
        },
        {
          "id": "MDRW2100039713.1.14",
          "form": "그때만나면 되겠네요",
          "original_form": "그때만나면 되겠네요",
          "speaker_id": "1",
          "time": "20210904 13:44"
        },
        {
          "id": "MDRW2100039713.1.15",
          "form": "머 보고 계신가요",
          "original_form": "머 보고 계신가요",
          "speaker_id": "1",
          "time": "20210906 14:37"
        },
        {
          "id": "MDRW2100039713.1.16",
          "form": "머머 보세요",
          "original_form": "머머 보세요",
          "speaker_id": "1",
          "time": "20210906 15:11"
        },
        {
          "id": "MDRW2100039713.1.17",
          "form": "어제축구보셨나요?레바논과의 경기요",
          "original_form": "어제축구보셨나요?레바논과의 경기요",
          "speaker_id": "2",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100039713.1.18",
          "form": "아니요 1대영이엿던가요?",
          "original_form": "아니요 1대영이엿던가요?",
          "speaker_id": "1",
          "time": "20210908 08:41"
        },
        {
          "id": "MDRW2100039713.1.19",
          "form": "네 다행히 이겼어요",
          "original_form": "네 다행히 이겼어요",
          "speaker_id": "2",
          "time": "20210908 08:42"
        },
        {
          "id": "MDRW2100039713.1.20",
          "form": "재밌던데요",
          "original_form": "재밌던데요",
          "speaker_id": "2",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100039713.1.21",
          "form": "오 손흥민 나왔나요",
          "original_form": "오 손흥민 나왔나요",
          "speaker_id": "1",
          "time": "20210908 08:43"
        },
        {
          "id": "MDRW2100039713.1.22",
          "form": "아니요 안나왔어요",
          "original_form": "아니요 안나왔어요",
          "speaker_id": "2",
          "time": "20210908 08:44"
        },
        {
          "id": "MDRW2100039713.1.23",
          "form": "헐 왜요",
          "original_form": "헐 왜요",
          "speaker_id": "1",
          "time": "20210908 08:45"
        },
        {
          "id": "MDRW2100039713.1.24",
          "form": "저번에는 본거 같은데",
          "original_form": "저번에는 본거 같은데",
          "speaker_id": "1",
          "time": "20210908 08:46"
        },
        {
          "id": "MDRW2100039713.1.25",
          "form": "종아리 부상이라고하네요ㄷㄷ",
          "original_form": "종아리 부상이라고하네요ㄷㄷ",
          "speaker_id": "2",
          "time": "20210908 08:46"
        },
        {
          "id": "MDRW2100039713.1.26",
          "form": "아 ㄷㄷ 저번에 부상이 있었던가요?",
          "original_form": "아 ㄷㄷ 저번에 부상이 있었던가요?",
