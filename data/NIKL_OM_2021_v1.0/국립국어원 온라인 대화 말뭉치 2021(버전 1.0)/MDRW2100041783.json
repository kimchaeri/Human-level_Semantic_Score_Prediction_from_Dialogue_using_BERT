      "utterance": [
        {
          "id": "MDRW2100041783.1.1",
          "form": "왜이래잠이없노ㅠ",
          "original_form": "왜이래잠이없노ㅠ",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041783.1.2",
          "form": "이집이나 저집이나",
          "original_form": "이집이나 저집이나",
          "speaker_id": "2",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041783.1.3",
          "form": "ㅋㅋㅋㅋㅋ그러니깐은 아오",
          "original_form": "ㅋㅋㅋㅋㅋ그러니깐은 아오",
          "speaker_id": "1",
          "time": "20210906 15:45"
        },
        {
          "id": "MDRW2100041783.1.4",
          "form": "둘째는근데 낮잠을  마니자고",
          "original_form": "둘째는근데 낮잠을  마니자고",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041783.1.5",
          "form": "여튼 잠 좀 자라 ㅋㅋ 나는 이제 조금만 쉬다가 ㅋㅋ 둘째를 맞이해야지",
          "original_form": "여튼 잠 좀 자라 ㅋㅋ 나는 이제 조금만 쉬다가 ㅋㅋ 둘째를 맞이해야지",
          "speaker_id": "1",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041783.1.6",
          "form": "응ㅋㅋ 근데나안잘듯",
          "original_form": "응ㅋㅋ 근데나안잘듯",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041783.1.7",
          "form": "꼭 자야지하면 안와",
          "original_form": "꼭 자야지하면 안와",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041783.1.8",
          "form": "아하 ㅋㅋ 그렇구만 ㅋㅋ",
          "original_form": "아하 ㅋㅋ 그렇구만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041783.1.9",
          "form": "맞어 맞어",
          "original_form": "맞어 맞어",
          "speaker_id": "1",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041783.1.10",
          "form": "ㅋㅋ 그래서 못자고",
          "original_form": "ㅋㅋ 그래서 못자고",
          "speaker_id": "2",
          "time": "20210906 15:46"
        },
        {
          "id": "MDRW2100041783.1.11",
          "form": "나는 오늘 저녁 먹고 일찍 잘것같은 예감이다",
