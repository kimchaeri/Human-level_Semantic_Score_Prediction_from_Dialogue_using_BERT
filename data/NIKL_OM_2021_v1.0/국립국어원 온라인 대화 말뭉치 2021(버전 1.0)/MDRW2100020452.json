        },
        {
          "id": "MDRW2100020452.1.27",
          "form": "집안일하라고 남겨둔 빅픽쳐아닐까요ㅡ",
          "original_form": "집안일하라고 남겨둔 빅픽쳐아닐까요ㅡ",
          "speaker_id": "1",
          "time": "20210828 11:26"
        },
        {
          "id": "MDRW2100020452.1.28",
