      "utterance": [
        {
          "id": "MDRW2100020452.1.1",
          "form": "어라 일찍 일어나셨네요",
          "original_form": "어라 일찍 일어나셨네요",
          "speaker_id": "2",
          "time": "20210828 06:15"
        },
        {
          "id": "MDRW2100020452.1.2",
          "form": "아니왜이렇게일찍일어나셨어요?",
          "original_form": "아니왜이렇게일찍일어나셨어요?",
          "speaker_id": "1",
          "time": "20210828 06:15"
        },
        {
          "id": "MDRW2100020452.1.3",
          "form": "다른가족분들도일어나셨나요?",
          "original_form": "다른가족분들도일어나셨나요?",
          "speaker_id": "1",
          "time": "20210828 06:15"
        },
        {
          "id": "MDRW2100020452.1.4",
          "form": "일찍 잤더니 일찍 깨서 ㅋㅋㅋ근데 금방 다시 잒어뇨",
          "original_form": "일찍 잤더니 일찍 깨서 ㅋㅋㅋ근데 금방 다시 잒어뇨",
          "speaker_id": "2",
          "time": "20210828 08:14"
        },
        {
          "id": "MDRW2100020452.1.5",
          "form": "ㅋㅋㅋㄱㅋㄲ개피곤해요 오늘 주말이라 모해요 \r\n집안일해야해요오",
          "original_form": "ㅋㅋㅋㄱㅋㄲ개피곤해요 오늘 주말이라 모해요 \r\n집안일해야해요오",
          "speaker_id": "1",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100020452.1.6",
          "form": "집안일 뭐하세요",
          "original_form": "집안일 뭐하세요",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020452.1.7",
          "form": "전 제 방에서 공기청정기로 작동하고 잇어요",
          "original_form": "전 제 방에서 공기청정기로 작동하고 잇어요",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020452.1.8",
          "form": "산소마시고 이산화탄소 내뿜어요",
          "original_form": "산소마시고 이산화탄소 내뿜어요",
          "speaker_id": "2",
          "time": "20210828 11:23"
        },
        {
          "id": "MDRW2100020452.1.9",
          "form": "우와\r\n아\r\n순간 공기청정기 작동하는줄알았어요",
          "original_form": "우와\r\n아\r\n순간 공기청정기 작동하는줄알았어요",
          "speaker_id": "1",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020452.1.10",
          "form": "ㅋㄱㅈㄱㅈㅋㅈㄱㅈㅋㅈㄱㄷㄱ",
          "original_form": "ㅋㄱㅈㄱㅈㅋㅈㄱㅈㅋㅈㄱㄷㄱ",
          "speaker_id": "1",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020452.1.11",
          "form": "빨래돌리고 청소해야해요",
          "original_form": "빨래돌리고 청소해야해요",
          "speaker_id": "1",
          "time": "20210828 11:24"
        },
        {
          "id": "MDRW2100020452.1.12",
          "form": "ㅋㅋㅋㅋ맞다 나도 빨래해야해요",
          "original_form": "ㅋㅋㅋㅋ맞다 나도 빨래해야해요",
          "speaker_id": "2",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020452.1.13",
          "form": "옷수선도 맡기러 가야하고",
          "original_form": "옷수선도 맡기러 가야하고",
          "speaker_id": "2",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020452.1.14",
          "form": "집안일많죠?",
          "original_form": "집안일많죠?",
          "speaker_id": "1",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020452.1.15",
          "form": "할머니 생신인데 5인이상 못모여서",
          "original_form": "할머니 생신인데 5인이상 못모여서",
          "speaker_id": "2",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020452.1.16",
          "form": "헙",
          "original_form": "헙",
          "speaker_id": "1",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020452.1.17",
          "form": "할머니 아빠 엄마 동생만 식사하러가요",
          "original_form": "할머니 아빠 엄마 동생만 식사하러가요",
          "speaker_id": "2",
          "time": "20210828 11:25"
        },
        {
          "id": "MDRW2100020452.1.18",
          "form": "어떡해요",
          "original_form": "어떡해요",
          "speaker_id": "1",
          "time": "20210828 11:25"
        },
        {
