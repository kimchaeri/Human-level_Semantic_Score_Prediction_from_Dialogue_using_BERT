      "utterance": [
        {
          "id": "MDRW2100024513.1.1",
          "form": "어디를 가장 최근 여행하셨나요?",
          "original_form": "어디를 가장 최근 여행하셨나요?",
          "speaker_id": "1",
          "time": "20210831 12:37"
        },
        {
          "id": "MDRW2100024513.1.2",
          "form": "부산갔다왔어요 name1님은요?",
          "original_form": "부산갔다왔어요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 12:37"
        },
        {
          "id": "MDRW2100024513.1.3",
          "form": "어 저는 지금 부산이에요",
          "original_form": "어 저는 지금 부산이에요",
          "speaker_id": "1",
          "time": "20210831 12:39"
        },
        {
          "id": "MDRW2100024513.1.4",
          "form": "휴가 마지막일",
          "original_form": "휴가 마지막일",
          "speaker_id": "1",
          "time": "20210831 12:39"
        },
        {
          "id": "MDRW2100024513.1.5",
          "form": "아 부산 여행오신거에요?",
          "original_form": "아 부산 여행오신거에요?",
          "speaker_id": "2",
          "time": "20210831 12:39"
        },
        {
          "id": "MDRW2100024513.1.6",
          "form": "휴가오셔서 이런걸 왜하세요 ㅋㅋ",
          "original_form": "휴가오셔서 이런걸 왜하세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:39"
        },
        {
          "id": "MDRW2100024513.1.7",
          "form": "이거하면서 소소하게 시간보내는거죠",
          "original_form": "이거하면서 소소하게 시간보내는거죠",
          "speaker_id": "1",
          "time": "20210831 12:40"
        },
        {
          "id": "MDRW2100024513.1.8",
          "form": "어디어디 보셨어요?",
          "original_form": "어디어디 보셨어요?",
          "speaker_id": "2",
          "time": "20210831 12:41"
        },
        {
          "id": "MDRW2100024513.1.9",
          "form": "꼬막쌈밥집, 거대갈비, 아데초이  이렇게 세군대가고 나머지 시간은",
          "original_form": "꼬막쌈밥집, 거대갈비, 아데초이  이렇게 세군대가고 나머지 시간은",
          "speaker_id": "1",
          "time": "20210831 12:47"
        },
        {
          "id": "MDRW2100024513.1.10",
          "form": "호텔에만 있었어요",
          "original_form": "호텔에만 있었어요",
          "speaker_id": "1",
          "time": "20210831 12:48"
        },
        {
          "id": "MDRW2100024513.1.11",
          "form": "부산 처음이세요?",
          "original_form": "부산 처음이세요?",
          "speaker_id": "2",
          "time": "20210831 12:48"
        },
        {
          "id": "MDRW2100024513.1.12",
          "form": "아뇨 부산 자주 갔어요",
          "original_form": "아뇨 부산 자주 갔어요",
          "speaker_id": "1",
          "time": "20210831 12:53"
        },
        {
          "id": "MDRW2100024513.1.13",
          "form": "집이 울산이라 종종 가요",
          "original_form": "집이 울산이라 종종 가요",
          "speaker_id": "1",
          "time": "20210831 12:53"
        },
        {
          "id": "MDRW2100024513.1.14",
          "form": "아 근데 휴가가신거군요",
          "original_form": "아 근데 휴가가신거군요",
          "speaker_id": "2",
          "time": "20210831 12:54"
        },
        {
          "id": "MDRW2100024513.1.15",
          "form": "넵 맞아요",
          "original_form": "넵 맞아요",
          "speaker_id": "1",
          "time": "20210831 13:29"
        },
        {
          "id": "MDRW2100024513.1.16",
          "form": "name2님은 부산 어디 다녀오셨어요?",
          "original_form": "&name2&님은 부산 어디 다녀오셨어요?",
          "speaker_id": "1",
          "time": "20210831 13:29"
        },
        {
          "id": "MDRW2100024513.1.17",
          "form": "기억이안나네요",
          "original_form": "기억이안나네요",
