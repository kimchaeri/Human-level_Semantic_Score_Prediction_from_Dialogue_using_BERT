      "utterance": [
        {
          "id": "MDRW2100028487.1.1",
          "form": "안녕하세요 최근 읽으신 책 있나요",
          "original_form": "안녕하세요 최근 읽으신 책 있나요",
          "speaker_id": "1",
          "time": "20210902 20:20"
        },
        {
          "id": "MDRW2100028487.1.2",
          "form": "책은 인터넷 도서관에서 고양이에 관한거 빌려봤었어요",
          "original_form": "책은 인터넷 도서관에서 고양이에 관한거 빌려봤었어요",
          "speaker_id": "2",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028487.1.3",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028487.1.4",
          "form": "오 인터넷 도서관에서 책 대여가 가능해요?",
          "original_form": "오 인터넷 도서관에서 책 대여가 가능해요?",
          "speaker_id": "1",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028487.1.5",
          "form": "저는 한강의 채식주의자가 최근이에요",
          "original_form": "저는 한강의 채식주의자가 최근이에요",
          "speaker_id": "1",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028487.1.6",
          "form": "네네 신세계에요",
          "original_form": "네네 신세계에요",
          "speaker_id": "2",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028487.1.7",
          "form": "저도 얼마전에 알았어요",
          "original_form": "저도 얼마전에 알았어요",
          "speaker_id": "2",
          "time": "20210902 20:21"
        },
        {
          "id": "MDRW2100028487.1.8",
          "form": "오 들어보긴했어요",
          "original_form": "오 들어보긴했어요",
          "speaker_id": "2",
          "time": "20210902 20:22"
        },
        {
          "id": "MDRW2100028487.1.9",
          "form": "인터넬 도서돤 신기해요",
          "original_form": "인터넬 도서돤 신기해요",
          "speaker_id": "1",
          "time": "20210902 20:24"
        },
        {
          "id": "MDRW2100028487.1.10",
          "form": "저는 추리소설 해리포터 이런거 좋아해요 ㅎㅎ",
          "original_form": "저는 추리소설 해리포터 이런거 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100028487.1.11",
          "form": "오 해리포터 오랜만이에요",
          "original_form": "오 해리포터 오랜만이에요",
          "speaker_id": "1",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028487.1.12",
          "form": "추천 소설 있나요",
          "original_form": "추천 소설 있나요",
          "speaker_id": "2",
          "time": "20210902 20:27"
        },
        {
          "id": "MDRW2100028487.1.13",
          "form": "달러구트 백화점이용",
          "original_form": "달러구트 백화점이용",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028487.1.14",
          "form": "전 안읽어봤지맘 같이읽어요",
          "original_form": "전 안읽어봤지맘 같이읽어요",
          "speaker_id": "1",
          "time": "20210902 20:28"
        },
        {
          "id": "MDRW2100028487.1.15",
          "form": "오 그거 맨날 상위권이 더라구요",
          "original_form": "오 그거 맨날 상위권이 더라구요",
          "speaker_id": "2",
          "time": "20210902 20:29"
        },
        {
          "id": "MDRW2100028487.1.16",
          "form": "맞아용",
          "original_form": "맞아용",
          "speaker_id": "1",
          "time": "20210902 20:37"
        },
        {
          "id": "MDRW2100028487.1.17",
          "form": "좋아하는 작가님 있으신가요?",
          "original_form": "좋아하는 작가님 있으신가요?",
          "speaker_id": "2",
          "time": "20210902 20:39"
        },
        {
          "id": "MDRW2100028487.1.18",
          "form": "저는 이슬아작가 공지영 작가 박웅현 작가 좋아해요",
          "original_form": "저는 이슬아작가 공지영 작가 박웅현 작가 좋아해요",
          "speaker_id": "1",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100028487.1.19",
          "form": "샤오루님은요",
          "original_form": "샤오루님은요",
          "speaker_id": "1",
          "time": "20210902 20:40"
        },
        {
          "id": "MDRW2100028487.1.20",
          "form": "오 저 도가니 영화보고 책도 샀었는데",
          "original_form": "오 저 도가니 영화보고 책도 샀었는데",
          "speaker_id": "2",
          "time": "20210902 20:41"
        },
        {
          "id": "MDRW2100028487.1.21",
          "form": "아 도가니!",
          "original_form": "아 도가니!",
          "speaker_id": "1",
          "time": "20210902 20:41"
        }
      ]
