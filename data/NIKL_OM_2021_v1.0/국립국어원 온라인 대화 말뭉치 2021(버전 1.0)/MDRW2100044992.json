      "utterance": [
        {
          "id": "MDRW2100044992.1.1",
          "form": "악 쌀 들어가요?",
          "original_form": "악 쌀 들어가요?",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044992.1.2",
          "form": "네!! 들어가유",
          "original_form": "네!! 들어가유",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044992.1.3",
          "form": "아앗 고기 야채 쌀",
          "original_form": "아앗 고기 야채 쌀",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044992.1.4",
          "form": "정답은?",
          "original_form": "정답은?",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044992.1.5",
          "form": "혹시 컨디션이 안 좋으셨나요?",
          "original_form": "혹시 컨디션이 안 좋으셨나요?",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044992.1.6",
          "form": "앜 컨디션죠습니다",
          "original_form": "앜 컨디션죠습니다",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044992.1.7",
          "form": "ㅋㅋㅋㅋㅌㅌㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅌㅌㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044992.1.8",
          "form": "아픈 사람이 주로 먹는 메뉴인가요?",
          "original_form": "아픈 사람이 주로 먹는 메뉴인가요?",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044992.1.9",
          "form": "죽은 진짜 죽을것같은상황아니먼 안먹어여",
          "original_form": "죽은 진짜 죽을것같은상황아니먼 안먹어여",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044992.1.10",
          "form": "헐 진짜요?",
          "original_form": "헐 진짜요?",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044992.1.11",
          "form": "아니욬ㅋㅋㅋ",
          "original_form": "아니욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044992.1.12",
          "form": "악 생각 간파당함",
          "original_form": "악 생각 간파당함",
          "speaker_id": "2",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100044992.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044992.1.14",
          "form": "아 뭘까요",
          "original_form": "아 뭘까요",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044992.1.15",
          "form": "끓이는 요리인가요?",
          "original_form": "끓이는 요리인가요?",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044992.1.16",
          "form": "아니욥!!",
          "original_form": "아니욥!!",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044992.1.17",
          "form": "간편한음식이랍니다",
          "original_form": "간편한음식이랍니다",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044992.1.18",
          "form": "편의점에도있어요",
          "original_form": "편의점에도있어요",
          "speaker_id": "1",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044992.1.19",
          "form": "힌트 드릴까여",
          "original_form": "힌트 드릴까여",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044992.1.20",
          "form": "전자렌지에 데우기도 하나요!",
          "original_form": "전자렌지에 데우기도 하나요!",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044992.1.21",
          "form": "아뇨 아직이요!!!!!ㅋㅋㅋㅋㅋ",
          "original_form": "아뇨 아직이요!!!!!ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044992.1.22",
          "form": "네에!!!",
          "original_form": "네에!!!",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044992.1.23",
          "form": "데피면 더 맛있죠",
          "original_form": "데피면 더 맛있죠",
          "speaker_id": "1",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044992.1.24",
          "form": "오기생긴다 제가 맞추고 말것이야요",
          "original_form": "오기생긴다 제가 맞추고 말것이야요",
