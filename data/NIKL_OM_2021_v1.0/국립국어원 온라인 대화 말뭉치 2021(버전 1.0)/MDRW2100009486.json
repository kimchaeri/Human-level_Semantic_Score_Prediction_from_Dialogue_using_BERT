      "utterance": [
        {
          "id": "MDRW2100009486.1.1",
          "form": "아 좋아해여!",
          "original_form": "아 좋아해여!",
          "speaker_id": "2",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009486.1.2",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210822 23:50"
        },
        {
          "id": "MDRW2100009486.1.3",
          "form": "저도요 ! 영화관가서 관람하는거 좋아해요",
          "original_form": "저도요 ! 영화관가서 관람하는거 좋아해요",
          "speaker_id": "1",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009486.1.4",
          "form": "최근에 보신 영화있나요?",
          "original_form": "최근에 보신 영화있나요?",
          "speaker_id": "1",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100009486.1.5",
          "form": "영화관가서 관람한건..",
          "original_form": "영화관가서 관람한건..",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009486.1.6",
          "form": "그 디즈니영화 크루엘라요!",
          "original_form": "그 디즈니영화 크루엘라요!",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009486.1.7",
          "form": "오 .. 저도 봤어요!",
          "original_form": "오 .. 저도 봤어요!",
          "speaker_id": "1",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009486.1.8",
          "form": "진짜 재밌게봤는데 어떠셨어요?",
          "original_form": "진짜 재밌게봤는데 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100009486.1.9",
          "form": "저도 재밌게봣어요!",
          "original_form": "저도 재밌게봣어요!",
          "speaker_id": "2",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009486.1.10",
          "form": "옷보는 쟈미가 쏠쏠하더라고요!!",
          "original_form": "옷보는 쟈미가 쏠쏠하더라고요!!",
          "speaker_id": "2",
          "time": "20210822 23:54"
        },
        {
          "id": "MDRW2100009486.1.11",
          "form": "맞아요 연기를 너무 잘하더라구요..",
          "original_form": "맞아요 연기를 너무 잘하더라구요..",
          "speaker_id": "1",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009486.1.12",
          "form": "강아지도 너무 귀여웠어요ㅎㅎ",
          "original_form": "강아지도 너무 귀여웠어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:55"
        },
        {
          "id": "MDRW2100009486.1.13",
          "form": "아 맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 06:58"
        },
        {
          "id": "MDRW2100009486.1.14",
          "form": "우리애 자란다 잘한다 하면소 봣던 ㅋㅋㅋㅋㅌㅋ",
          "original_form": "우리애 자란다 잘한다 하면소 봣던 ㅋㅋㅋㅋㅌㅋ",
          "speaker_id": "2",
          "time": "20210823 06:58"
        },
        {
          "id": "MDRW2100009486.1.15",
          "form": "name1님은 또 뭐보샸나요?",
          "original_form": "&name1&님은 또 뭐보샸나요?",
          "speaker_id": "2",
          "time": "20210823 06:59"
        },
        {
          "id": "MDRW2100009486.1.16",
          "form": "요즘 영화관가는걸 좋아해서 최근영화는 거의 다본 것 같네요",
          "original_form": "요즘 영화관가는걸 좋아해서 최근영화는 거의 다본 것 같네요",
          "speaker_id": "1",
          "time": "20210823 11:57"
        },
        {
          "id": "MDRW2100009486.1.17",
          "form": "프리가이랑 올드 재밌게봤어요!",
          "original_form": "프리가이랑 올드 재밌게봤어요!",
          "speaker_id": "1",
          "time": "20210823 11:57"
        },
        {
          "id": "MDRW2100009486.1.18",
          "form": "오 무슨내용인가요!",
          "original_form": "오 무슨내용인가요!",
          "speaker_id": "2",
          "time": "20210823 14:09"
        },
        {
          "id": "MDRW2100009486.1.19",
          "form": "프리가이는 게임npc가 자아를 찾는 내용이고 올드는 특정장소에서만 시간이 엄청 빠르게 가는 내용이에요!",
          "original_form": "프리가이는 게임npc가 자아를 찾는 내용이고 올드는 특정장소에서만 시간이 엄청 빠르게 가는 내용이에요!",
          "speaker_id": "1",
          "time": "20210823 14:12"
        },
        {
          "id": "MDRW2100009486.1.20",
          "form": "둘다 소재가 신선해서 굉장히 재밌더라구요",
          "original_form": "둘다 소재가 신선해서 굉장히 재밌더라구요",
          "speaker_id": "1",
          "time": "20210823 14:12"
        },
        {
          "id": "MDRW2100009486.1.21",
          "form": "진짜 신선하네요!!",
          "original_form": "진짜 신선하네요!!",
          "speaker_id": "2",
          "time": "20210823 14:23"
        },
        {
          "id": "MDRW2100009486.1.22",
          "form": "특히 프리가이 되게 재밌을것ㄱ타요",
          "original_form": "특히 프리가이 되게 재밌을것ㄱ타요",
          "speaker_id": "2",
          "time": "20210823 14:23"
        },
        {
          "id": "MDRW2100009486.1.23",
          "form": "슬쩍 메모해두기 ㅋㅋ",
          "original_form": "슬쩍 메모해두기 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 14:23"
        },
        {
