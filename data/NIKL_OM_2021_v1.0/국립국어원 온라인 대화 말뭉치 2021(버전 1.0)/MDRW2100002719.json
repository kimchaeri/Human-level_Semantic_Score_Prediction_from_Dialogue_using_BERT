      "utterance": [
        {
          "id": "MDRW2100002719.1.1",
          "form": "날씨가 제멋대로네",
          "original_form": "날씨가 제멋대로네",
          "speaker_id": "2",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002719.1.2",
          "form": "비왔다가 해떴다가 오락가락",
          "original_form": "비왔다가 해떴다가 오락가락",
          "speaker_id": "1",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002719.1.3",
          "form": "우산 안 가져왔는데 ㅎㅎ",
          "original_form": "우산 안 가져왔는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:00"
        },
        {
          "id": "MDRW2100002719.1.4",
          "form": "퇴근할 때는 안오겠지",
          "original_form": "퇴근할 때는 안오겠지",
          "speaker_id": "1",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002719.1.5",
          "form": "요즘 갑자기 먹구름 끼고 비오고 하더만",
          "original_form": "요즘 갑자기 먹구름 끼고 비오고 하더만",
          "speaker_id": "2",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002719.1.6",
          "form": "날씨를 예측할 수가 없어",
          "original_form": "날씨를 예측할 수가 없어",
          "speaker_id": "1",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002719.1.7",
          "form": "그래서 올해 유난히 무지개도 많이 떴잖아",
          "original_form": "그래서 올해 유난히 무지개도 많이 떴잖아",
          "speaker_id": "2",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002719.1.8",
          "form": "맞아 그것도 쌍무지개 ㅎㅎ",
          "original_form": "맞아 그것도 쌍무지개 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:01"
        },
        {
          "id": "MDRW2100002719.1.9",
          "form": "사진 찍어 뒀는데 보기 드문 광경이라 ㅎㅎ",
          "original_form": "사진 찍어 뒀는데 보기 드문 광경이라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002719.1.10",
          "form": "나도 ㅋㅋ",
          "original_form": "나도 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002719.1.11",
          "form": "비 좀 마르면 운동하러 가야겠다",
          "original_form": "비 좀 마르면 운동하러 가야겠다",
          "speaker_id": "1",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002719.1.12",
          "form": "줄넘기?",
          "original_form": "줄넘기?",
          "speaker_id": "2",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002719.1.13",
          "form": "ㅇㅇ 나 말고 애들 ㅎㅎ",
          "original_form": "ㅇㅇ 나 말고 애들 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002719.1.14",
          "form": "ㅇㅇ 키크는데 좋다니까 열심히 시켜",
          "original_form": "ㅇㅇ 키크는데 좋다니까 열심히 시켜",
          "speaker_id": "2",
          "time": "20210820 15:02"
        },
        {
          "id": "MDRW2100002719.1.15",
          "form": "웅 비가 더 안오길 바래야지",
          "original_form": "웅 비가 더 안오길 바래야지",
