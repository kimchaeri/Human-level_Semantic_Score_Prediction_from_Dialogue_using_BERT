      "utterance": [
        {
          "id": "MDRW2100002200.1.1",
          "form": "말이 늦으니 자꾸 못알아들어서 화를냈어요",
          "original_form": "말이 늦으니 자꾸 못알아들어서 화를냈어요",
          "speaker_id": "1",
          "time": "20210820 00:27"
        },
        {
          "id": "MDRW2100002200.1.2",
          "form": "기다려줘야한다고는 하더라구요",
          "original_form": "기다려줘야한다고는 하더라구요",
          "speaker_id": "2",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002200.1.3",
          "form": "그러다 보니 저도 힘들고 근데 지금은 너무 말잘해서 힘드네요 ㅡㅡ",
          "original_form": "그러다 보니 저도 힘들고 근데 지금은 너무 말잘해서 힘드네요 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002200.1.4",
          "form": "이래도 힘들고 저래도 힘들고",
          "original_form": "이래도 힘들고 저래도 힘들고",
          "speaker_id": "2",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002200.1.5",
          "form": "네 29개월까진 괜찮은것 같아요 그이후엔 좀치료해야하고요",
          "original_form": "네 29개월까진 괜찮은것 같아요 그이후엔 좀치료해야하고요",
          "speaker_id": "1",
          "time": "20210820 00:28"
        },
        {
          "id": "MDRW2100002200.1.6",
          "form": "네 크면 안힘들줄 알았어요",
          "original_form": "네 크면 안힘들줄 알았어요",
          "speaker_id": "1",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002200.1.7",
          "form": "그냥 계속 힘들져?ㅜㅜ",
          "original_form": "그냥 계속 힘들져?ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 00:29"
        },
        {
          "id": "MDRW2100002200.1.8",
          "form": "네 크면 사춘기 때문에 더힘들어요",
          "original_form": "네 크면 사춘기 때문에 더힘들어요",
          "speaker_id": "1",
          "time": "20210820 00:30"
        },
        {
          "id": "MDRW2100002200.1.9",
          "form": "엄마말에 반박에 자기방에서 안나와요",
          "original_form": "엄마말에 반박에 자기방에서 안나와요",
          "speaker_id": "1",
          "time": "20210820 00:31"
        },
        {
          "id": "MDRW2100002200.1.10",
          "form": "중2병도 무섭다던데",
          "original_form": "중2병도 무섭다던데",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002200.1.11",
          "form": "저 아는분 문짝을 떼어냈어요",
          "original_form": "저 아는분 문짝을 떼어냈어요",
          "speaker_id": "1",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002200.1.12",
          "form": "엄청 무서워요",
          "original_form": "엄청 무서워요",
          "speaker_id": "1",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002200.1.13",
          "form": "다른 인격이 나와요",
          "original_form": "다른 인격이 나와요",
          "speaker_id": "1",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002200.1.14",
          "form": "오.. 무섭네요",
          "original_form": "오.. 무섭네요",
          "speaker_id": "2",
          "time": "20210820 00:32"
        },
        {
          "id": "MDRW2100002200.1.15",
          "form": "네 정말 싫어요 저 딸이 겁나네요 2년후 중2",
          "original_form": "네 정말 싫어요 저 딸이 겁나네요 2년후 중2",
          "speaker_id": "1",
          "time": "20210820 00:33"
        },
        {
          "id": "MDRW2100002200.1.16",
          "form": "잘넘어갈수 있을거에요!",
          "original_form": "잘넘어갈수 있을거에요!",
          "speaker_id": "2",
          "time": "20210820 00:34"
        },
        {
          "id": "MDRW2100002200.1.17",
          "form": "지금봐선 벌써 중2같아요",
          "original_form": "지금봐선 벌써 중2같아요",
          "speaker_id": "1",
          "time": "20210820 00:34"
        },
        {
          "id": "MDRW2100002200.1.18",
          "form": "지금 그러고 중2때 그냥 지나갈수도있어요ㅜㅜ",
          "original_form": "지금 그러고 중2때 그냥 지나갈수도있어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210820 00:35"
        },
        {
          "id": "MDRW2100002200.1.19",
          "form": "그러길 바래요 제가 너무 오냐 오냐 키웠더니 후회중요",
          "original_form": "그러길 바래요 제가 너무 오냐 오냐 키웠더니 후회중요",
          "speaker_id": "1",
          "time": "20210820 00:36"
        },
        {
          "id": "MDRW2100002200.1.20",
          "form": "크면 엄마가 눈치 본다더니 제가 떡그래요",
          "original_form": "크면 엄마가 눈치 본다더니 제가 떡그래요",
          "speaker_id": "1",
          "time": "20210820 00:37"
        },
        {
          "id": "MDRW2100002200.1.21",
          "form": "혼낼때는 딱 혼나봐야하나봐요",
          "original_form": "혼낼때는 딱 혼나봐야하나봐요",
          "speaker_id": "2",
          "time": "20210820 00:38"
        },
        {
          "id": "MDRW2100002200.1.22",
          "form": "맞아요 그걸 못했네요 제가 혼나요",
          "original_form": "맞아요 그걸 못했네요 제가 혼나요",
          "speaker_id": "1",
          "time": "20210820 00:38"
        },
        {
          "id": "MDRW2100002200.1.23",
          "form": "이미 안무서운 엄마로 알아서 그런가봐요",
          "original_form": "이미 안무서운 엄마로 알아서 그런가봐요",
          "speaker_id": "2",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002200.1.24",
          "form": "네 안거죠",
          "original_form": "네 안거죠",
          "speaker_id": "1",
          "time": "20210820 00:39"
        },
        {
          "id": "MDRW2100002200.1.25",
          "form": "저는 벌써부터 기싸움중이에요",
          "original_form": "저는 벌써부터 기싸움중이에요",
          "speaker_id": "2",
          "time": "20210820 00:40"
        },
        {
          "id": "MDRW2100002200.1.26",
          "form": "시작이죠",
          "original_form": "시작이죠",
          "speaker_id": "1",
          "time": "20210820 00:40"
        },
        {
          "id": "MDRW2100002200.1.27",
          "form": "전 그때부터 졌네요",
          "original_form": "전 그때부터 졌네요",
          "speaker_id": "1",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002200.1.28",
          "form": "전 아직 이기고는 잇는데",
          "original_form": "전 아직 이기고는 잇는데",
          "speaker_id": "2",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002200.1.29",
          "form": "오 대단하세요",
          "original_form": "오 대단하세요",
          "speaker_id": "1",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002200.1.30",
          "form": "자꾸 얄밉게 굴어요 애가..",
          "original_form": "자꾸 얄밉게 굴어요 애가..",
          "speaker_id": "2",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002200.1.31",
          "form": "얄미울때죠 ㅋㅋ",
          "original_form": "얄미울때죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002200.1.32",
          "form": "그냥 울어버리구요ㅋㅋ",
          "original_form": "그냥 울어버리구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:41"
        },
        {
          "id": "MDRW2100002200.1.33",
          "form": "오 고단수네요",
          "original_form": "오 고단수네요",
          "speaker_id": "1",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002200.1.34",
          "form": "세상을 알아요",
          "original_form": "세상을 알아요",
          "speaker_id": "1",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002200.1.35",
          "form": "눈치싸움 중이에요",
          "original_form": "눈치싸움 중이에요",
          "speaker_id": "2",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002200.1.36",
          "form": "꼭 이기셔요 ㅎㅎ",
          "original_form": "꼭 이기셔요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002200.1.37",
          "form": "울면 되는줄알아서 울게 두기도 하는데",
          "original_form": "울면 되는줄알아서 울게 두기도 하는데",
          "speaker_id": "2",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002200.1.38",
          "form": "아 그렇죠 저도 그랬어요",
          "original_form": "아 그렇죠 저도 그랬어요",
          "speaker_id": "1",
          "time": "20210820 00:42"
        },
        {
          "id": "MDRW2100002200.1.39",
          "form": "어른들이 다 봐주니까 버릇이 나빠져요",
          "original_form": "어른들이 다 봐주니까 버릇이 나빠져요",
          "speaker_id": "2",
          "time": "20210820 00:43"
        }
      ]
    }
  ]
