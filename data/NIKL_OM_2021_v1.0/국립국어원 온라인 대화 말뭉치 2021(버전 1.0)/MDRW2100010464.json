      "utterance": [
        {
          "id": "MDRW2100010464.1.1",
          "form": "장어를 낮에 먹었는데도 왜 배가 고프죠",
          "original_form": "장어를 낮에 먹었는데도 왜 배가 고프죠",
          "speaker_id": "1",
          "time": "20210823 18:35"
        },
        {
          "id": "MDRW2100010464.1.2",
          "form": "불합리하네요 ㅋㅋ",
          "original_form": "불합리하네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 18:35"
        },
        {
          "id": "MDRW2100010464.1.3",
          "form": "에이 시간이 넘 많이 지났잖아요",
          "original_form": "에이 시간이 넘 많이 지났잖아요",
          "speaker_id": "2",
          "time": "20210823 18:35"
        },
        {
          "id": "MDRW2100010464.1.4",
          "form": "저녁 시간인데",
          "original_form": "저녁 시간인데",
          "speaker_id": "2",
          "time": "20210823 18:35"
        },
        {
          "id": "MDRW2100010464.1.5",
          "form": "당연히 저녁 먹어야 생각 안나죠",
          "original_form": "당연히 저녁 먹어야 생각 안나죠",
          "speaker_id": "2",
          "time": "20210823 18:35"
        },
        {
          "id": "MDRW2100010464.1.6",
          "form": "메뉴가 넘 저녁 부실해요",
          "original_form": "메뉴가 넘 저녁 부실해요",
          "speaker_id": "2",
          "time": "20210823 18:35"
        },
        {
          "id": "MDRW2100010464.1.7",
          "form": "그러지 말고",
          "original_form": "그러지 말고",
          "speaker_id": "2",
          "time": "20210823 18:35"
        },
        {
          "id": "MDRW2100010464.1.8",
          "form": "샐러드 쉐이크 하세요",
          "original_form": "샐러드 쉐이크 하세요",
          "speaker_id": "2",
          "time": "20210823 18:36"
        },
        {
          "id": "MDRW2100010464.1.9",
          "form": "안그래도 집에가서",
          "original_form": "안그래도 집에가서",
          "speaker_id": "1",
          "time": "20210823 18:36"
        },
        {
          "id": "MDRW2100010464.1.10",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210823 18:36"
        },
        {
          "id": "MDRW2100010464.1.11",
          "form": "쉐이크말고 뭐도 같이 먹게요",
          "original_form": "쉐이크말고 뭐도 같이 먹게요",
          "speaker_id": "1",
          "time": "20210823 18:36"
        },
        {
          "id": "MDRW2100010464.1.12",
          "form": "그래얃해요",
          "original_form": "그래얃해요",
          "speaker_id": "2",
          "time": "20210823 18:37"
        },
        {
          "id": "MDRW2100010464.1.13",
          "form": "ㅋㅋ 집에 뭐가있더라",
          "original_form": "ㅋㅋ 집에 뭐가있더라",
          "speaker_id": "1",
          "time": "20210823 18:37"
        },
        {
          "id": "MDRW2100010464.1.14",
          "form": "전 가끔 gs25",
          "original_form": "전 가끔 gs25",
          "speaker_id": "2",
          "time": "20210823 18:37"
        },
        {
          "id": "MDRW2100010464.1.15",
          "form": "샐러드 사서 때운곤해요",
          "original_form": "샐러드 사서 때운곤해요",
          "speaker_id": "2",
          "time": "20210823 18:37"
        },
        {
          "id": "MDRW2100010464.1.16",
          "form": "어우 아까 아아 1리터 진한맛 괜히마셨나봐요",
          "original_form": "어우 아까 아아 1리터 진한맛 괜히마셨나봐요",
          "speaker_id": "1",
          "time": "20210823 18:37"
        },
        {
          "id": "MDRW2100010464.1.17",
          "form": "왜요",
          "original_form": "왜요",
          "speaker_id": "2",
          "time": "20210823 18:37"
        },
        {
          "id": "MDRW2100010464.1.18",
          "form": "배 부르시죠?",
          "original_form": "배 부르시죠?",
          "speaker_id": "2",
          "time": "20210823 18:38"
        },
        {
          "id": "MDRW2100010464.1.19",
          "form": "물배만차고",
          "original_form": "물배만차고",
          "speaker_id": "2",
          "time": "20210823 18:38"
        },
        {
          "id": "MDRW2100010464.1.20",
          "form": "속은 허하고",
          "original_form": "속은 허하고",
          "speaker_id": "2",
          "time": "20210823 18:38"
        },
        {
          "id": "MDRW2100010464.1.21",
          "form": "배아프세요?",
          "original_form": "배아프세요?",
          "speaker_id": "2",
          "time": "20210823 18:38"
        },
        {
          "id": "MDRW2100010464.1.22",
          "form": "뭔가 허한데",
          "original_form": "뭔가 허한데",
          "speaker_id": "1",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100010464.1.23",
          "form": "아 그렇죠 물배만차요",
          "original_form": "아 그렇죠 물배만차요",
          "speaker_id": "2",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100010464.1.24",
          "form": "너무 급 진한 카페인이들어간거같아요",
          "original_form": "너무 급 진한 카페인이들어간거같아요",
          "speaker_id": "1",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100010464.1.25",
          "form": "속은비고",
          "original_form": "속은비고",
          "speaker_id": "2",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100010464.1.26",
          "form": "약간 속이 벌렁벌렁한달까",
          "original_form": "약간 속이 벌렁벌렁한달까",
          "speaker_id": "1",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100010464.1.27",
          "form": "그럼 안좋아요 ...카페인 과다네요",
          "original_form": "그럼 안좋아요 ...카페인 과다네요",
          "speaker_id": "2",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100010464.1.28",
          "form": "울렁한 느낌들어요",
          "original_form": "울렁한 느낌들어요",
          "speaker_id": "1",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100010464.1.29",
          "form": "증상이 딱 그러네요",
          "original_form": "증상이 딱 그러네요",
          "speaker_id": "2",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100010464.1.30",
          "form": "ㅋㅋ 얼른집에가서 뭐든 먹어야겠어요",
          "original_form": "ㅋㅋ 얼른집에가서 뭐든 먹어야겠어요",
          "speaker_id": "1",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100010464.1.31",
          "form": "안되겠어요 저녁은 좀 드셔요",
          "original_form": "안되겠어요 저녁은 좀 드셔요",
          "speaker_id": "2",
          "time": "20210823 18:40"
        }
      ]
    }
