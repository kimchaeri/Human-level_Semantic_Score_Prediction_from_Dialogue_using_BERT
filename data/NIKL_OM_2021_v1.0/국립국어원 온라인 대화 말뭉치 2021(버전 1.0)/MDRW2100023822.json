      "utterance": [
        {
          "id": "MDRW2100023822.1.1",
          "form": "아니요 다른게임들은 잼병이라서요ㅠ",
          "original_form": "아니요 다른게임들은 잼병이라서요ㅠ",
          "speaker_id": "2",
          "time": "20210830 18:08"
        },
        {
          "id": "MDRW2100023822.1.2",
          "form": "좋아하는게임있으세요?",
          "original_form": "좋아하는게임있으세요?",
          "speaker_id": "2",
          "time": "20210830 18:08"
        },
        {
          "id": "MDRW2100023822.1.3",
          "form": "저는 오버워치나 스팀게임 스위치 게임 번갈아가면서 해요",
          "original_form": "저는 오버워치나 스팀게임 스위치 게임 번갈아가면서 해요",
          "speaker_id": "1",
          "time": "20210830 18:14"
        },
        {
          "id": "MDRW2100023822.1.4",
          "form": "오 스팀게임은 뭐하세요?",
          "original_form": "오 스팀게임은 뭐하세요?",
          "speaker_id": "2",
          "time": "20210830 18:16"
        },
        {
          "id": "MDRW2100023822.1.5",
          "form": "친구들이랑 스타듀밸리나 오버쿡드도 하구요",
          "original_form": "친구들이랑 스타듀밸리나 오버쿡드도 하구요",
          "speaker_id": "1",
          "time": "20210830 18:16"
        },
        {
          "id": "MDRW2100023822.1.6",
          "form": "혼자서는 레데리나 위쳐같은거 했어요",
          "original_form": "혼자서는 레데리나 위쳐같은거 했어요",
          "speaker_id": "1",
          "time": "20210830 18:16"
        },
        {
          "id": "MDRW2100023822.1.7",
          "form": "오 재밋겠네요!",
          "original_form": "오 재밋겠네요!",
          "speaker_id": "2",
          "time": "20210830 18:20"
        },
        {
          "id": "MDRW2100023822.1.8",
          "form": "재밌어요",
          "original_form": "재밌어요",
          "speaker_id": "1",
          "time": "20210830 18:22"
        },
        {
          "id": "MDRW2100023822.1.9",
          "form": "스팀게임들은 안하시나요?",
          "original_form": "스팀게임들은 안하시나요?",
          "speaker_id": "1",
          "time": "20210830 18:22"
        },
        {
          "id": "MDRW2100023822.1.10",
          "form": "스팀 어몽어스만해봤어요",
          "original_form": "스팀 어몽어스만해봤어요",
          "speaker_id": "2",
          "time": "20210830 18:34"
        },
        {
          "id": "MDRW2100023822.1.11",
          "form": "어몽어스 친구들이랑 하셨어요?",
          "original_form": "어몽어스 친구들이랑 하셨어요?",
          "speaker_id": "1",
          "time": "20210830 19:57"
        },
        {
          "id": "MDRW2100023822.1.12",
          "form": "네 친구들이랑 했어요",
          "original_form": "네 친구들이랑 했어요",
          "speaker_id": "2",
          "time": "20210830 20:08"
        },
        {
          "id": "MDRW2100023822.1.13",
          "form": "콘솔게임은 하시나요?",
          "original_form": "콘솔게임은 하시나요?",
          "speaker_id": "1",
          "time": "20210830 20:11"
        },
        {
          "id": "MDRW2100023822.1.14",
          "form": "아뇨 한번도 못해봤어요",
          "original_form": "아뇨 한번도 못해봤어요",
          "speaker_id": "2",
          "time": "20210830 20:13"
        },
        {
          "id": "MDRW2100023822.1.15",
          "form": "사고싶은 콘솔은 있으세요?",
          "original_form": "사고싶은 콘솔은 있으세요?",
          "speaker_id": "1",
          "time": "20210830 20:14"
        },
        {
          "id": "MDRW2100023822.1.16",
