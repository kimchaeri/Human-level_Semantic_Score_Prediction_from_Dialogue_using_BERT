        },
        {
          "id": "MDRW2100030280.1.2",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "2",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030280.1.3",
          "form": "개강하셨나요",
          "original_form": "개강하셨나요",
          "speaker_id": "1",
          "time": "20210903 23:28"
        },
        {
          "id": "MDRW2100030280.1.4",
          "form": "맞습니다 저는 개강한대학생입니다",
          "original_form": "맞습니다 저는 개강한대학생입니다",
          "speaker_id": "2",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030280.1.5",
          "form": "1학년이신가봐요",
          "original_form": "1학년이신가봐요",
          "speaker_id": "1",
          "time": "20210903 23:30"
        },
        {
          "id": "MDRW2100030280.1.6",
          "form": "2학년입니다",
          "original_form": "2학년입니다",
          "speaker_id": "2",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030280.1.7",
          "form": "헉 2학년이면",
          "original_form": "헉 2학년이면",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030280.1.8",
          "form": "코로나학번이시군요",
          "original_form": "코로나학번이시군요",
          "speaker_id": "1",
          "time": "20210903 23:31"
        },
        {
          "id": "MDRW2100030280.1.9",
          "form": "네ㅜㅜ 화나요",
          "original_form": "네ㅜㅜ 화나요",
          "speaker_id": "2",
          "time": "20210903 23:32"
        },
        {
          "id": "MDRW2100030280.1.10",
          "form": "저도 어떻게보면 코로나학번이라..",
          "original_form": "저도 어떻게보면 코로나학번이라..",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030280.1.11",
          "form": "전 1학년 다니고 군복학하니 터졌거든요",
          "original_form": "전 1학년 다니고 군복학하니 터졌거든요",
          "speaker_id": "1",
          "time": "20210903 23:33"
        },
        {
          "id": "MDRW2100030280.1.12",
          "form": "와...",
          "original_form": "와...",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030280.1.13",
          "form": "그럼 지금 몇학년이세요?",
          "original_form": "그럼 지금 몇학년이세요?",
          "speaker_id": "2",
          "time": "20210903 23:34"
        },
        {
          "id": "MDRW2100030280.1.14",
          "form": "저는 지금 3학년이에요",
          "original_form": "저는 지금 3학년이에요",
          "speaker_id": "1",
          "time": "20210903 23:37"
        },
        {
          "id": "MDRW2100030280.1.15",
          "form": "헉 멋있다... 계속 대면하실 때 학교다니셨겠네요",
          "original_form": "헉 멋있다... 계속 대면하실 때 학교다니셨겠네요",
          "speaker_id": "2",
          "time": "20210903 23:42"
        },
        {
          "id": "MDRW2100030280.1.16",
          "form": "1학년때만 다녔어서 저도 별거없어여..ㅋㅋㅋ",
          "original_form": "1학년때만 다녔어서 저도 별거없어여..ㅋㅋㅋ",
