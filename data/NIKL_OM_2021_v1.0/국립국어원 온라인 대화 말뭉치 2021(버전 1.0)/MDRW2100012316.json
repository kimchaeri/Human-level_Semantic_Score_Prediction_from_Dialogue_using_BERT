      "utterance": [
        {
          "id": "MDRW2100012316.1.1",
          "form": "오늘 무엇을 먹었나요?",
          "original_form": "오늘 무엇을 먹었나요?",
          "speaker_id": "1",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100012316.1.2",
          "form": "오늘 볶음밥을 먹었어요",
          "original_form": "오늘 볶음밥을 먹었어요",
          "speaker_id": "2",
          "time": "20210824 21:11"
        },
        {
          "id": "MDRW2100012316.1.3",
          "form": "너는 뭐 먹었어요",
          "original_form": "너는 뭐 먹었어요",
          "speaker_id": "2",
          "time": "20210824 21:12"
        },
        {
          "id": "MDRW2100012316.1.4",
          "form": "오... 저는 미역국 먹었답니다",
          "original_form": "오... 저는 미역국 먹었답니다",
          "speaker_id": "1",
          "time": "20210824 21:12"
        },
        {
          "id": "MDRW2100012316.1.5",
          "form": "오 미역국 맛있겠다",
          "original_form": "오 미역국 맛있겠다",
          "speaker_id": "2",
          "time": "20210824 21:12"
        },
        {
          "id": "MDRW2100012316.1.6",
          "form": "내일은 마라탕 먹을거야 나는",
          "original_form": "내일은 마라탕 먹을거야 나는",
          "speaker_id": "2",
          "time": "20210824 21:13"
        },
        {
          "id": "MDRW2100012316.1.7",
          "form": "와? 어디 마라탕 먹을겁니까",
          "original_form": "와? 어디 마라탕 먹을겁니까",
          "speaker_id": "1",
          "time": "20210824 21:13"
        },
        {
          "id": "MDRW2100012316.1.8",
          "form": "명동 라화쿵푸요",
          "original_form": "명동 라화쿵푸요",
          "speaker_id": "2",
          "time": "20210824 21:14"
        },
        {
          "id": "MDRW2100012316.1.9",
          "form": "넘 떨려요",
          "original_form": "넘 떨려요",
          "speaker_id": "2",
          "time": "20210824 21:14"
        },
        {
          "id": "MDRW2100012316.1.10",
          "form": "카페도 걀거야",
          "original_form": "카페도 걀거야",
          "speaker_id": "2",
          "time": "20210824 21:14"
        },
        {
          "id": "MDRW2100012316.1.11",
          "form": "나 요즘 마라탕에 꽂혔어",
          "original_form": "나 요즘 마라탕에 꽂혔어",
          "speaker_id": "2",
          "time": "20210824 21:14"
        },
        {
          "id": "MDRW2100012316.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 명실상부 라화쿵푸...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 명실상부 라화쿵푸...",
          "speaker_id": "1",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012316.1.13",
          "form": "카페는 어디 가시나요",
          "original_form": "카페는 어디 가시나요",
          "speaker_id": "1",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012316.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012316.1.15",
          "form": "모르겠러요",
          "original_form": "모르겠러요",
          "speaker_id": "2",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012316.1.16",
          "form": "명동 카페 잘 모릅니다요ㅠ",
          "original_form": "명동 카페 잘 모릅니다요ㅠ",
          "speaker_id": "2",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012316.1.17",
          "form": "추천해드려요?",
          "original_form": "추천해드려요?",
          "speaker_id": "1",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012316.1.18",
          "form": "네네 어디가 조아요",
          "original_form": "네네 어디가 조아요",
          "speaker_id": "2",
          "time": "20210824 21:15"
        },
        {
          "id": "MDRW2100012316.1.19",
          "form": "저도 잘은 모릅니다만...",
          "original_form": "저도 잘은 모릅니다만...",
          "speaker_id": "1",
          "time": "20210824 21:16"
        },
        {
          "id": "MDRW2100012316.1.20",
          "form": "저는 니생각이라는 카페 좋아합니다",
          "original_form": "저는 니생각이라는 카페 좋아합니다",
          "speaker_id": "1",
          "time": "20210824 21:16"
        },
        {
          "id": "MDRW2100012316.1.21",
          "form": "니생각이라는 카페라",
          "original_form": "니생각이라는 카페라",
          "speaker_id": "2",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100012316.1.22",
          "form": "감성적이군요",
          "original_form": "감성적이군요",
          "speaker_id": "2",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100012316.1.23",
          "form": "네이버에 쳐봐야지",
          "original_form": "네이버에 쳐봐야지",
          "speaker_id": "2",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100012316.1.24",
          "form": "뭐가 맛있엉ㅅ?",
          "original_form": "뭐가 맛있엉ㅅ?",
          "speaker_id": "2",
          "time": "20210824 21:17"
        },
        {
          "id": "MDRW2100012316.1.25",
          "form": "거기 쑥이 메인이라",
          "original_form": "거기 쑥이 메인이라",
          "speaker_id": "1",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100012316.1.26",
          "form": "우와 뭐야 가격 너무 착한데?",
          "original_form": "우와 뭐야 가격 너무 착한데?",
          "speaker_id": "2",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100012316.1.27",
          "form": "넴",
          "original_form": "넴",
          "speaker_id": "2",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100012316.1.28",
          "form": "쑥관련된 메뉴 추천합니다",
          "original_form": "쑥관련된 메뉴 추천합니다",
          "speaker_id": "1",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100012316.1.29",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100012316.1.30",
          "form": "쑥라떼? 좋았음",
          "original_form": "쑥라떼? 좋았음",
          "speaker_id": "1",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100012316.1.31",
          "form": "달달하진 않죠?",
          "original_form": "달달하진 않죠?",
          "speaker_id": "2",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100012316.1.32",
          "form": "엄... 제가 달게 먹어서...",
          "original_form": "엄... 제가 달게 먹어서...",
          "speaker_id": "1",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100012316.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100012316.1.34",
          "form": "ㅋㅋㅋㅋㅋㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100012316.1.35",
          "form": "기대된다",
          "original_form": "기대된다",
          "speaker_id": "2",
          "time": "20210824 21:18"
        },
        {
          "id": "MDRW2100012316.1.36",
          "form": "제 입맛에는 그리 달지 않았습니다...",
          "original_form": "제 입맛에는 그리 달지 않았습니다...",
          "speaker_id": "1",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100012316.1.37",
          "form": "호호. ..",
          "original_form": "호호. ..",
          "speaker_id": "2",
          "time": "20210824 21:19"
        },
        {
          "id": "MDRW2100012316.1.38",
          "form": "오케이이...",
          "original_form": "오케이이...",
          "speaker_id": "2",
          "time": "20210824 21:19"
        }
      ]
    }
