      "utterance": [
        {
          "id": "MDRW2100000379.1.1",
          "form": "코로나로 공연 못본지 오래네요.",
          "original_form": "코로나로 공연 못본지 오래네요.",
          "speaker_id": "1",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000379.1.2",
          "form": "그러게 말이에요.",
          "original_form": "그러게 말이에요.",
          "speaker_id": "2",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000379.1.3",
          "form": "저 올 초에 김선호 나오는 연극 티켓팅 실패했어요!",
          "original_form": "저 올 초에 김선호 나오는 연극 티켓팅 실패했어요!",
          "speaker_id": "2",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000379.1.4",
          "form": "쓰린 기억",
          "original_form": "쓰린 기억",
          "speaker_id": "2",
          "time": "20210819 15:43"
        },
        {
          "id": "MDRW2100000379.1.5",
          "form": "name1는 공연 좋아하세요?",
          "original_form": "&name1&는 공연 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 15:44"
        },
        {
          "id": "MDRW2100000379.1.6",
          "form": "많이 즐기지 않지만, 뮤지컬은 종종 챙겨 갑니다.",
          "original_form": "많이 즐기지 않지만, 뮤지컬은 종종 챙겨 갑니다.",
          "speaker_id": "1",
          "time": "20210819 15:44"
        },
        {
          "id": "MDRW2100000379.1.7",
          "form": "연극 좋아하시나봐요",
          "original_form": "연극 좋아하시나봐요",
          "speaker_id": "1",
          "time": "20210819 15:44"
        },
        {
          "id": "MDRW2100000379.1.8",
          "form": "아주 좋아하는 건 아니지만 그래도 기회가 있을 때 종종 보러 갔어요.",
          "original_form": "아주 좋아하는 건 아니지만 그래도 기회가 있을 때 종종 보러 갔어요.",
          "speaker_id": "2",
          "time": "20210819 15:45"
        },
        {
          "id": "MDRW2100000379.1.9",
          "form": "뮤지컬 좋아하시는구나",
          "original_form": "뮤지컬 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210819 15:45"
        },
        {
          "id": "MDRW2100000379.1.10",
          "form": "저 마지막으로 본 게 위키드였어요",
          "original_form": "저 마지막으로 본 게 위키드였어요",
          "speaker_id": "2",
          "time": "20210819 15:46"
        },
        {
          "id": "MDRW2100000379.1.11",
          "form": "위키드는 본 적이 없네요ㅠ 배우는 누구 좋아하세요?",
          "original_form": "위키드는 본 적이 없네요ㅠ 배우는 누구 좋아하세요?",
          "speaker_id": "1",
          "time": "20210819 15:46"
        },
        {
          "id": "MDRW2100000379.1.12",
          "form": "선호 씨...",
          "original_form": "선호 씨...",
          "speaker_id": "2",
          "time": "20210819 15:48"
        },
        {
          "id": "MDRW2100000379.1.13",
          "form": "사실 연극이나 뮤지컬 배우 잘 몰라요!",
          "original_form": "사실 연극이나 뮤지컬 배우 잘 몰라요!",
          "speaker_id": "2",
          "time": "20210819 15:48"
        },
        {
          "id": "MDRW2100000379.1.14",
          "form": "영화 좋아하세요?",
          "original_form": "영화 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 15:48"
        },
        {
          "id": "MDRW2100000379.1.15",
          "form": "네 영화는 정말 좋아한답니다.",
          "original_form": "네 영화는 정말 좋아한답니다.",
          "speaker_id": "1",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000379.1.16",
          "form": "그런데 요즘엔 극장이 아니라 유튜브로 사서 봐요",
          "original_form": "그런데 요즘엔 극장이 아니라 유튜브로 사서 봐요",
          "speaker_id": "1",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000379.1.17",
          "form": "오!!",
          "original_form": "오!!",
          "speaker_id": "2",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000379.1.18",
          "form": "아하 저도 계속 극장에 못 가다가 최근에",
          "original_form": "아하 저도 계속 극장에 못 가다가 최근에",
          "speaker_id": "2",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000379.1.19",
          "form": "정글크루즈 보러 갔었는데",
          "original_form": "정글크루즈 보러 갔었는데",
          "speaker_id": "2",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000379.1.20",
          "form": "진짜 재밌게 봤어요!",
          "original_form": "진짜 재밌게 봤어요!",
          "speaker_id": "2",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000379.1.21",
          "form": "드웨인 존슨 좋아하거든요",
          "original_form": "드웨인 존슨 좋아하거든요",
          "speaker_id": "2",
          "time": "20210819 15:50"
        },
        {
          "id": "MDRW2100000379.1.22",
          "form": "오 좋은 영화추천 감사해요.",
          "original_form": "오 좋은 영화추천 감사해요.",
          "speaker_id": "1",
          "time": "20210819 15:51"
        },
        {
          "id": "MDRW2100000379.1.23",
          "form": "어떤 장르 좋아하세요?",
          "original_form": "어떤 장르 좋아하세요?",
          "speaker_id": "2",
          "time": "20210819 15:51"
        },
        {
          "id": "MDRW2100000379.1.24",
          "form": "와 멋지시죠",
          "original_form": "와 멋지시죠",
          "speaker_id": "1",
          "time": "20210819 15:51"
        },
        {
          "id": "MDRW2100000379.1.25",
          "form": "전 드라마요.",
          "original_form": "전 드라마요.",
          "speaker_id": "1",
          "time": "20210819 15:51"
        },
        {
          "id": "MDRW2100000379.1.26",
          "form": "오 인생영화가 궁금해요",
          "original_form": "오 인생영화가 궁금해요",
          "speaker_id": "2",
          "time": "20210819 15:52"
        },
        {
          "id": "MDRW2100000379.1.27",
          "form": "ㅎㅎ 그게 참 꼽기가 어려워요.",
          "original_form": "ㅎㅎ 그게 참 꼽기가 어려워요.",
          "speaker_id": "1",
          "time": "20210819 15:52"
        },
        {
          "id": "MDRW2100000379.1.28",
          "form": "전 라라랜드 좋아합니다ㅎㅎ",
          "original_form": "전 라라랜드 좋아합니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210819 15:52"
        },
        {
          "id": "MDRW2100000379.1.29",
          "form": "오 음악이 있는 영화 좋아하시는구나",
          "original_form": "오 음악이 있는 영화 좋아하시는구나",
          "speaker_id": "2",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000379.1.30",
          "form": "위대한 쇼맨 보셨어요?",
          "original_form": "위대한 쇼맨 보셨어요?",
          "speaker_id": "2",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000379.1.31",
          "form": "와 그거 극장에서 보다가",
          "original_form": "와 그거 극장에서 보다가",
          "speaker_id": "1",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000379.1.32",
          "form": "시작부터 울었어요",
          "original_form": "시작부터 울었어요",
          "speaker_id": "1",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000379.1.33",
          "form": "으어어 보셨구나",
          "original_form": "으어어 보셨구나",
          "speaker_id": "2",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000379.1.34",
          "form": "진짜 너무너무 좋았어요",
          "original_form": "진짜 너무너무 좋았어요",
          "speaker_id": "2",
          "time": "20210819 15:53"
        },
        {
          "id": "MDRW2100000379.1.35",
          "form": "으어어어 근데 의외로 사람들이 몰라서ㅋㅋ",
          "original_form": "으어어어 근데 의외로 사람들이 몰라서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000379.1.36",
          "form": "워어어어어 그 ost만 알고ㅋㅋ",
          "original_form": "워어어어어 그 ost만 알고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 15:54"
        },
        {
          "id": "MDRW2100000379.1.37",
          "form": "맞아요. 아는 사람만 아는 영화!",
          "original_form": "맞아요. 아는 사람만 아는 영화!",
          "speaker_id": "2",
          "time": "20210819 15:55"
        },
        {
          "id": "MDRW2100000379.1.38",
          "form": "그래서 저도 여기저기 추천 많이 해요",
          "original_form": "그래서 저도 여기저기 추천 많이 해요",
          "speaker_id": "2",
          "time": "20210819 15:55"
        },
        {
          "id": "MDRW2100000379.1.39",
          "form": "저는 좀비영화도 좋아해요!",
          "original_form": "저는 좀비영화도 좋아해요!",
          "speaker_id": "2",
          "time": "20210819 15:55"
        },
        {
          "id": "MDRW2100000379.1.40",
          "form": "작가 허지웅은 좀비영화 마니아던데",
          "original_form": "작가 허지웅은 좀비영화 마니아던데",
          "speaker_id": "1",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000379.1.41",
          "form": "전 만화책으로만ㅠ",
          "original_form": "전 만화책으로만ㅠ",
          "speaker_id": "1",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000379.1.42",
          "form": "아 초단편 영화중에 좀비영화 좋은 거 있는데",
          "original_form": "아 초단편 영화중에 좀비영화 좋은 거 있는데",
          "speaker_id": "1",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000379.1.43",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000379.1.44",
          "form": "알려주세요!",
          "original_form": "알려주세요!",
          "speaker_id": "2",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000379.1.45",
          "form": "저는 상업적인 좀비영화 좋아해요",
          "original_form": "저는 상업적인 좀비영화 좋아해요",
          "speaker_id": "2",
          "time": "20210819 15:56"
        },
        {
          "id": "MDRW2100000379.1.46",
          "form": "레지던트 이블같은?",
          "original_form": "레지던트 이블같은?",
