      "utterance": [
        {
          "id": "MDRW2100020593.1.1",
          "form": "하 못고르겠어요",
          "original_form": "하 못고르겠어요",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020593.1.2",
          "form": "엄외 민초 아봉 피스타 슈팅스타 자모카 초나숲 이런거요?",
          "original_form": "엄외 민초 아봉 피스타 슈팅스타 자모카 초나숲 이런거요?",
          "speaker_id": "1",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020593.1.3",
          "form": "아니 name1님이 골라주셨네요",
          "original_form": "아니 &name1&님이 골라주셨네요",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020593.1.4",
          "form": "맞아요 거의 그런류로 돌려요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 거의 그런류로 돌려요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020593.1.5",
          "form": "그래 사실 31가지 맛중에 6가지만 고르라는게",
          "original_form": "그래 사실 31가지 맛중에 6가지만 고르라는게",
          "speaker_id": "1",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020593.1.6",
          "form": "잔인한거지",
          "original_form": "잔인한거지",
          "speaker_id": "1",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020593.1.7",
          "form": "슈팅스타 엄마 최애라서 늘 삽니다",
          "original_form": "슈팅스타 엄마 최애라서 늘 삽니다",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020593.1.8",
          "form": "물론 난 한가지만 담지만",
          "original_form": "물론 난 한가지만 담지만",
          "speaker_id": "1",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020593.1.9",
          "form": "입안에서 튀기는 걸 사야해요",
          "original_form": "입안에서 튀기는 걸 사야해요",
          "speaker_id": "2",
          "time": "20210828 10:14"
        },
        {
          "id": "MDRW2100020593.1.10",
          "form": "맞아요 진짜 광기...",
          "original_form": "맞아요 진짜 광기...",
          "speaker_id": "2",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020593.1.11",
          "form": "ㅋㅋㅋㅋ 슈팅스타 맛있더라구요",
          "original_form": "ㅋㅋㅋㅋ 슈팅스타 맛있더라구요",
          "speaker_id": "1",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020593.1.12",
          "form": "name1님의 세번째 픽은 뭔가요",
          "original_form": "&name1&님의 세번째 픽은 뭔가요",
          "speaker_id": "2",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020593.1.13",
          "form": "초나숲입니다",
          "original_form": "초나숲입니다",
          "speaker_id": "1",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020593.1.14",
          "form": "그쵸 맛있죠",
          "original_form": "그쵸 맛있죠",
          "speaker_id": "2",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020593.1.15",
          "form": "그린티와 초코 그리고 초코볼의 조합",
          "original_form": "그린티와 초코 그리고 초코볼의 조합",
          "speaker_id": "1",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020593.1.16",
          "form": "하 뭘 좀 아시는 군요",
          "original_form": "하 뭘 좀 아시는 군요",
          "speaker_id": "2",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020593.1.17",
          "form": "그린티..!",
          "original_form": "그린티..!",
          "speaker_id": "2",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020593.1.18",
          "form": "근데 요즘 아봉도 맛있더라구요",
          "original_form": "근데 요즘 아봉도 맛있더라구요",
          "speaker_id": "1",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020593.1.19",
          "form": "그린티를 왜 잊은건지",
          "original_form": "그린티를 왜 잊은건지",
          "speaker_id": "2",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020593.1.20",
          "form": "한때 제 최애였는데",
          "original_form": "한때 제 최애였는데",
          "speaker_id": "2",
          "time": "20210828 10:15"
        },
        {
          "id": "MDRW2100020593.1.21",
          "form": "맞아요 아봉이랑 비슷한거 접때 잠시 나왔었는데 사라졌더라구요 아쉽..ㅠ",
          "original_form": "맞아요 아봉이랑 비슷한거 접때 잠시 나왔었는데 사라졌더라구요 아쉽..ㅠ",
          "speaker_id": "2",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020593.1.22",
          "form": "그게 더 맛있었는데..",
          "original_form": "그게 더 맛있었는데..",
          "speaker_id": "2",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020593.1.23",
          "form": "아몬드 봉봉봉요?",
          "original_form": "아몬드 봉봉봉요?",
          "speaker_id": "1",
          "time": "20210828 10:16"
        },
        {
          "id": "MDRW2100020593.1.24",
          "form": "아닌가",
          "original_form": "아닌가",
