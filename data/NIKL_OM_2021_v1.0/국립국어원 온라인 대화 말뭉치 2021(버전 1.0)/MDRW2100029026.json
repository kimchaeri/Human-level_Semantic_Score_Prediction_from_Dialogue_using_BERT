      "utterance": [
        {
          "id": "MDRW2100029026.1.1",
          "form": "주로 어떤 동물 동영상 보세요?",
          "original_form": "주로 어떤 동물 동영상 보세요?",
          "speaker_id": "1",
          "time": "20210902 23:22"
        },
        {
          "id": "MDRW2100029026.1.2",
          "form": "고양이나 파충류요!!",
          "original_form": "고양이나 파충류요!!",
          "speaker_id": "2",
          "time": "20210903 00:10"
        },
        {
          "id": "MDRW2100029026.1.3",
          "form": "오 파충류요?",
          "original_form": "오 파충류요?",
          "speaker_id": "1",
          "time": "20210903 00:17"
        },
        {
          "id": "MDRW2100029026.1.4",
          "form": "네 다흑이라고 파충류나 벌레를 주로 다루는 채널이 있어요!",
          "original_form": "네 다흑이라고 파충류나 벌레를 주로 다루는 채널이 있어요!",
          "speaker_id": "2",
          "time": "20210903 13:42"
        },
        {
          "id": "MDRW2100029026.1.5",
          "form": "오 신기해요",
          "original_form": "오 신기해요",
          "speaker_id": "1",
          "time": "20210903 13:43"
        },
        {
          "id": "MDRW2100029026.1.6",
          "form": "파충류눈 괜찮은데 벌레는 별론 것 같아요",
          "original_form": "파충류눈 괜찮은데 벌레는 별론 것 같아요",
          "speaker_id": "1",
          "time": "20210903 13:43"
        },
        {
          "id": "MDRW2100029026.1.7",
          "form": "마자요 벌레는 좀 징그럽긴 한데 꽤 친근하게 다뤄주는 편이라 그래도 재밌게 보고 있어요!",
          "original_form": "마자요 벌레는 좀 징그럽긴 한데 꽤 친근하게 다뤄주는 편이라 그래도 재밌게 보고 있어요!",
          "speaker_id": "2",
          "time": "20210903 13:44"
        },
        {
          "id": "MDRW2100029026.1.8",
          "form": "유튜버 분이 덜 징그럽게 찍나봐요 ㅎㅎ",
          "original_form": "유튜버 분이 덜 징그럽게 찍나봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 13:46"
        },
        {
          "id": "MDRW2100029026.1.9",
          "form": "나중에 키우고 싶은 동물이 있으세요?",
          "original_form": "나중에 키우고 싶은 동물이 있으세요?",
          "speaker_id": "1",
          "time": "20210903 13:46"
        },
        {
          "id": "MDRW2100029026.1.10",
          "form": "음 고양이를 키우고 싶어요",
          "original_form": "음 고양이를 키우고 싶어요",
          "speaker_id": "2",
          "time": "20210903 13:47"
        },
        {
          "id": "MDRW2100029026.1.11",
          "form": "고양이도 귀엽죠 저는 대형견 키우고 싶어요 리트리버 같은거로요",
          "original_form": "고양이도 귀엽죠 저는 대형견 키우고 싶어요 리트리버 같은거로요",
          "speaker_id": "1",
          "time": "20210903 13:49"
        },
        {
          "id": "MDRW2100029026.1.12",
          "form": "리트리버 귀엽죠 ㅋㅋ 순하게 생겼고 인절미 같고..",
          "original_form": "리트리버 귀엽죠 ㅋㅋ 순하게 생겼고 인절미 같고..",
          "speaker_id": "2",
          "time": "20210903 13:50"
        },
        {
          "id": "MDRW2100029026.1.13",
          "form": "그쵸 넘 귀엽고 뭐라고하죠? 웃상? 웃는 얼굴처럼 생겨서 넘 좋아요",
          "original_form": "그쵸 넘 귀엽고 뭐라고하죠? 웃상? 웃는 얼굴처럼 생겨서 넘 좋아요",
          "speaker_id": "1",
          "time": "20210903 13:52"
        },
        {
          "id": "MDRW2100029026.1.14",
          "form": "맞아요 ㅋㅌㅋㅋ 보는 사람이 푸근해지는 얼굴...",
          "original_form": "맞아요 ㅋㅌㅋㅋ 보는 사람이 푸근해지는 얼굴...",
          "speaker_id": "2",
          "time": "20210903 13:53"
        },
        {
          "id": "MDRW2100029026.1.15",
          "form": "근데 제가 대형견의 체력을 못 따라갈 것 같아서... 그게 너무 문제에요...ㅋㅋㅋㅋㅋ",
          "original_form": "근데 제가 대형견의 체력을 못 따라갈 것 같아서... 그게 너무 문제에요...ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 13:54"
        }
      ]
