      "utterance": [
        {
          "id": "MDRW2100038230.1.1",
          "form": "그러게 이쁘네",
          "original_form": "그러게 이쁘네",
          "speaker_id": "2",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100038230.1.2",
          "form": "2호점 첫 개시인가봐다",
          "original_form": "2호점 첫 개시인가봐다",
          "speaker_id": "1",
          "time": "20210902 01:17"
        },
        {
          "id": "MDRW2100038230.1.3",
          "form": "다시 태어난 윤식당 자막 ㅋㅋ",
          "original_form": "다시 태어난 윤식당 자막 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038230.1.4",
          "form": "저기 나오는 선베드에 누워 있기만 해도 좋겠다",
          "original_form": "저기 나오는 선베드에 누워 있기만 해도 좋겠다",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038230.1.5",
          "form": "응 완전 좋겠다",
          "original_form": "응 완전 좋겠다",
          "speaker_id": "2",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038230.1.6",
          "form": "하루만에 꾸몄는데도 잘 꾸몄네",
          "original_form": "하루만에 꾸몄는데도 잘 꾸몄네",
          "speaker_id": "1",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038230.1.7",
          "form": "그러게 대단하다",
          "original_form": "그러게 대단하다",
          "speaker_id": "2",
          "time": "20210902 01:18"
        },
        {
          "id": "MDRW2100038230.1.8",
          "form": "2호점 첫영업을 잘해야할텐데",
          "original_form": "2호점 첫영업을 잘해야할텐데",
          "speaker_id": "1",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100038230.1.9",
          "form": "질할꺼야",
          "original_form": "질할꺼야",
          "speaker_id": "2",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100038230.1.10",
          "form": "소품으로 아기자기하게 잘 꾸몄네",
          "original_form": "소품으로 아기자기하게 잘 꾸몄네",
          "speaker_id": "1",
          "time": "20210902 01:19"
        },
        {
          "id": "MDRW2100038230.1.11",
          "form": "윤여정 저 선글라스 비싸더라",
          "original_form": "윤여정 저 선글라스 비싸더라",
          "speaker_id": "2",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100038230.1.12",
          "form": "엉 안경이랑 선클라스 같이 있는건데 나두 갖고 싶어 검색하니 ㅋㅋ",
          "original_form": "엉 안경이랑 선클라스 같이 있는건데 나두 갖고 싶어 검색하니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100038230.1.13",
          "form": "몇백만원이었지",
          "original_form": "몇백만원이었지",
          "speaker_id": "2",
          "time": "20210902 01:20"
        },
        {
          "id": "MDRW2100038230.1.14",
          "form": "응 헉 거렸다",
          "original_form": "응 헉 거렸다",
          "speaker_id": "1",
          "time": "20210902 01:21"
        },
        {
          "id": "MDRW2100038230.1.15",
          "form": "신구할배 알바 하는거 너무 귀엽다",
          "original_form": "신구할배 알바 하는거 너무 귀엽다",
