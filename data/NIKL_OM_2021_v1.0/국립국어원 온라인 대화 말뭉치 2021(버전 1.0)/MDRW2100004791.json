      "utterance": [
        {
          "id": "MDRW2100004791.1.1",
          "form": "혹시 놀면뭐하니 보시나요?",
          "original_form": "혹시 놀면뭐하니 보시나요?",
          "speaker_id": "1",
          "time": "20210821 18:49"
        },
        {
          "id": "MDRW2100004791.1.2",
          "form": "지금하나요?",
          "original_form": "지금하나요?",
          "speaker_id": "2",
          "time": "20210821 18:50"
        },
        {
          "id": "MDRW2100004791.1.3",
          "form": "슬의생틀어놨는데",
          "original_form": "슬의생틀어놨는데",
          "speaker_id": "2",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004791.1.4",
          "form": "네네 오늘도 결방인줄 알았는데 하네요?ㅎㅎ",
          "original_form": "네네 오늘도 결방인줄 알았는데 하네요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004791.1.5",
          "form": "방금 채널돌렸어요ㅎ",
          "original_form": "방금 채널돌렸어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 18:51"
        },
        {
          "id": "MDRW2100004791.1.6",
          "form": "오늘은 올림픽특집인가봐요ㅎㅎ",
          "original_form": "오늘은 올림픽특집인가봐요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004791.1.7",
          "form": "그런거같아요 예고에 신유빈 선수나오던데",
          "original_form": "그런거같아요 예고에 신유빈 선수나오던데",
          "speaker_id": "2",
          "time": "20210821 18:53"
        },
        {
          "id": "MDRW2100004791.1.8",
          "form": "그러게요ㅎㅎ 머리묶은모습만 보다가 풀고나오니 못알아봤어요ㅎㅎ",
          "original_form": "그러게요ㅎㅎ 머리묶은모습만 보다가 풀고나오니 못알아봤어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 18:54"
        },
        {
          "id": "MDRW2100004791.1.9",
          "form": "저도요ㅎ 유빈양 옛날에  무도에 나왔었나봐요",
          "original_form": "저도요ㅎ 유빈양 옛날에  무도에 나왔었나봐요",
          "speaker_id": "2",
          "time": "20210821 18:57"
        },
        {
          "id": "MDRW2100004791.1.10",
          "form": "그러게요 몰랐었네요ㅋㅋ",
          "original_form": "그러게요 몰랐었네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:58"
        },
        {
          "id": "MDRW2100004791.1.11",
          "form": "요즘 올림픽선수들 예능에서 자주보이더라구요 ㅋㅋ",
          "original_form": "요즘 올림픽선수들 예능에서 자주보이더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:59"
        },
        {
          "id": "MDRW2100004791.1.12",
          "form": "한철이니 나올수있을때 나오려나봐요ㅎ",
          "original_form": "한철이니 나올수있을때 나오려나봐요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:00"
        },
        {
          "id": "MDRW2100004791.1.13",
          "form": "그죠 근데 펜싱선수들이 예능마다 나와서 같은얘기만하니 좀 지겹더라구요ㅠ 다양하게 나왔으면 ㅎㅎ",
          "original_form": "그죠 근데 펜싱선수들이 예능마다 나와서 같은얘기만하니 좀 지겹더라구요ㅠ 다양하게 나왔으면 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:01"
        },
        {
          "id": "MDRW2100004791.1.14",
          "form": "저도 동감요ㅎ",
          "original_form": "저도 동감요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004791.1.15",
          "form": "첨하는얘기처럼 하는데 다똑같아요",
          "original_form": "첨하는얘기처럼 하는데 다똑같아요",
          "speaker_id": "2",
          "time": "20210821 19:02"
        },
        {
          "id": "MDRW2100004791.1.16",
          "form": "그죠^^ 유퀴드에 럭비선수들 나온것도 좋더라구요^^",
          "original_form": "그죠^^ 유퀴드에 럭비선수들 나온것도 좋더라구요^^",
          "speaker_id": "1",
          "time": "20210821 19:04"
        },
        {
          "id": "MDRW2100004791.1.17",
          "form": "아까 재방봤어요",
          "original_form": "아까 재방봤어요",
          "speaker_id": "2",
          "time": "20210821 19:05"
        },
        {
          "id": "MDRW2100004791.1.18",
          "form": "안창림선수도 귀엽던데요",
          "original_form": "안창림선수도 귀엽던데요",
          "speaker_id": "2",
          "time": "20210821 19:06"
        },
        {
          "id": "MDRW2100004791.1.19",
          "form": "안창림선수는 라디오스타에서 더 입담이 최고였어요 아버지얘기할때 넘 웃기고 귀엽더라구요ㅋㅋ",
          "original_form": "안창림선수는 라디오스타에서 더 입담이 최고였어요 아버지얘기할때 넘 웃기고 귀엽더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004791.1.20",
          "form": "아 거기에 나왔어요? 재방 봐야겠네요ㅎ",
          "original_form": "아 거기에 나왔어요? 재방 봐야겠네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:12"
        },
        {
          "id": "MDRW2100004791.1.21",
          "form": "네 넘웃겼는데ㅋㅋ 혼자봐서 아쉽더라구요ㅋㅋ",
          "original_form": "네 넘웃겼는데ㅋㅋ 혼자봐서 아쉽더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 19:13"
        },
        {
          "id": "MDRW2100004791.1.22",
          "form": "꼭 봐야겠군요ㅎ",
          "original_form": "꼭 봐야겠군요ㅎ",
