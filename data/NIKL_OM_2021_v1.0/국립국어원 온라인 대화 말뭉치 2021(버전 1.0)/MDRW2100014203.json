        {
          "id": "MDRW2100014203.1.1",
          "form": "아 배고파 ㅎㅎ",
          "original_form": "아 배고파 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014203.1.2",
          "form": "퇴근 시간 거의 다 됐네~",
          "original_form": "퇴근 시간 거의 다 됐네~",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014203.1.3",
          "form": "오늘 저녁엔 뭐 먹어?",
          "original_form": "오늘 저녁엔 뭐 먹어?",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014203.1.4",
          "form": "아직 안정했는데 ㅎㅎ",
          "original_form": "아직 안정했는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014203.1.5",
          "form": "삼양라면 어때?",
          "original_form": "삼양라면 어때?",
          "speaker_id": "2",
          "time": "20210825 17:44"
        },
        {
          "id": "MDRW2100014203.1.6",
          "form": "크~ 라면에 김치 얹어 먹으면 맛있겠다",
          "original_form": "크~ 라면에 김치 얹어 먹으면 맛있겠다",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014203.1.7",
          "form": "5개 끓여서 나눠먹자 ㅎㅎ",
          "original_form": "5개 끓여서 나눠먹자 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014203.1.8",
          "form": "그랴 실컷 먹어보자",
          "original_form": "그랴 실컷 먹어보자",
          "speaker_id": "1",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014203.1.9",
          "form": "밥도 말아먹고 ㅎㅎ",
          "original_form": "밥도 말아먹고 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 17:45"
        },
        {
          "id": "MDRW2100014203.1.10",
          "form": "삼양라면 안매워서 애들도 잘 먹을듯",
          "original_form": "삼양라면 안매워서 애들도 잘 먹을듯",
