      "utterance": [
        {
          "id": "MDRW2100028289.1.1",
          "form": "사실 요즘 슬슬 요리가 귀찮아지긴 했습니다만..",
          "original_form": "사실 요즘 슬슬 요리가 귀찮아지긴 했습니다만..",
          "speaker_id": "1",
          "time": "20210902 18:56"
        },
        {
          "id": "MDRW2100028289.1.2",
          "form": "근데 직접 조리해서 먹는게",
          "original_form": "근데 직접 조리해서 먹는게",
          "speaker_id": "1",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100028289.1.3",
          "form": "자존감에 좋대요",
          "original_form": "자존감에 좋대요",
          "speaker_id": "1",
          "time": "20210902 19:12"
        },
        {
          "id": "MDRW2100028289.1.4",
          "form": "지금 야근 중이라 자아존중감 바닥",
          "original_form": "지금 야근 중이라 자아존중감 바닥",
          "speaker_id": "2",
          "time": "20210902 19:40"
        },
        {
          "id": "MDRW2100028289.1.5",
          "form": "앗...",
          "original_form": "앗...",
          "speaker_id": "1",
          "time": "20210902 19:46"
        },
        {
          "id": "MDRW2100028289.1.6",
          "form": "조리해 먹는 것보다 배달 음식이 맛있죠",
          "original_form": "조리해 먹는 것보다 배달 음식이 맛있죠",
          "speaker_id": "2",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100028289.1.7",
          "form": "근데 사실",
          "original_form": "근데 사실",
          "speaker_id": "1",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028289.1.8",
          "form": "저도 차마 더 맛있다고는 못해서",
          "original_form": "저도 차마 더 맛있다고는 못해서",
          "speaker_id": "1",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028289.1.9",
          "form": "자존감 얘기를...",
          "original_form": "자존감 얘기를...",
          "speaker_id": "1",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028289.1.10",
          "form": "네...",
          "original_form": "네...",
          "speaker_id": "1",
          "time": "20210902 20:04"
        },
        {
          "id": "MDRW2100028289.1.11",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "1",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100028289.1.12",
          "form": "된장이 생각보다 쓸 데가",
          "original_form": "된장이 생각보다 쓸 데가",
          "speaker_id": "1",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100028289.1.13",
          "form": "많더라고요?",
          "original_form": "많더라고요?",
          "speaker_id": "1",
          "time": "20210902 20:05"
        },
        {
          "id": "MDRW2100028289.1.14",
          "form": "된장 어디에 쓰는데요",
          "original_form": "된장 어디에 쓰는데요",
          "speaker_id": "2",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028289.1.15",
          "form": "수육할때도 넣고",
          "original_form": "수육할때도 넣고",
          "speaker_id": "1",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028289.1.16",
          "form": "찌개할 때도 넣고",
          "original_form": "찌개할 때도 넣고",
          "speaker_id": "1",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028289.1.17",
          "form": "하다못해 라면 할때도",
          "original_form": "하다못해 라면 할때도",
          "speaker_id": "1",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028289.1.18",
          "form": "마늘이랑 같이 넣으면",
          "original_form": "마늘이랑 같이 넣으면",
          "speaker_id": "1",
          "time": "20210902 20:06"
        },
        {
          "id": "MDRW2100028289.1.19",
          "form": "맛이 깊어짐요",
          "original_form": "맛이 깊어짐요",
          "speaker_id": "1",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028289.1.20",
          "form": "물론 그냥 된장 요리할 때도 쓰고...",
          "original_form": "물론 그냥 된장 요리할 때도 쓰고...",
          "speaker_id": "1",
          "time": "20210902 20:07"
        },
        {
          "id": "MDRW2100028289.1.21",
          "form": "잘 해 드시네요",
          "original_form": "잘 해 드시네요",
          "speaker_id": "2",
          "time": "20210902 20:07"
        }
      ]
