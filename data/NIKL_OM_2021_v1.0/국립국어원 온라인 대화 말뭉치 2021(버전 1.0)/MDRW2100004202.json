      "utterance": [
        {
          "id": "MDRW2100004202.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210821 15:28"
        },
        {
          "id": "MDRW2100004202.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004202.1.3",
          "form": "그쪽은 비 오나요?",
          "original_form": "그쪽은 비 오나요?",
          "speaker_id": "2",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004202.1.4",
          "form": "여기는 그쳤는데",
          "original_form": "여기는 그쳤는데",
          "speaker_id": "2",
          "time": "20210821 15:29"
        },
        {
          "id": "MDRW2100004202.1.5",
          "form": "아침까지 오다가 지금은 그쳤어요",
          "original_form": "아침까지 오다가 지금은 그쳤어요",
          "speaker_id": "1",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004202.1.6",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004202.1.7",
          "form": "아까 정말 엄청많이와서",
          "original_form": "아까 정말 엄청많이와서",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004202.1.8",
          "form": "잠깐 나갔다가 다 젖었잖아요",
          "original_form": "잠깐 나갔다가 다 젖었잖아요",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004202.1.9",
          "form": "그래도 날씨는 선선해져서",
          "original_form": "그래도 날씨는 선선해져서",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004202.1.10",
          "form": "좋은거 같아요 ㅎ",
          "original_form": "좋은거 같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 15:30"
        },
        {
          "id": "MDRW2100004202.1.11",
          "form": "맞아요 너무 더웠는데 시원해져서 좋아요!",
          "original_form": "맞아요 너무 더웠는데 시원해져서 좋아요!",
          "speaker_id": "1",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004202.1.12",
          "form": "딱 가을 넘어갔다고",
          "original_form": "딱 가을 넘어갔다고",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004202.1.13",
          "form": "시원해지는게 너무 신기해요 ㅎㅎㅎ",
          "original_form": "시원해지는게 너무 신기해요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004202.1.14",
          "form": "맞아요!ㅎㅎ",
          "original_form": "맞아요!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004202.1.15",
          "form": "이제 슬슬 가을 옷 꺼내야하는건..",
          "original_form": "이제 슬슬 가을 옷 꺼내야하는건..",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004202.1.16",
          "form": "일이지만..",
          "original_form": "일이지만..",
          "speaker_id": "2",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004202.1.17",
          "form": "이번 여름이 너무 더워서..",
          "original_form": "이번 여름이 너무 더워서..",
          "speaker_id": "1",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004202.1.18",
          "form": "가을이 기다려졌었어요",
          "original_form": "가을이 기다려졌었어요",
          "speaker_id": "1",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004202.1.19",
          "form": "그쵸..",
          "original_form": "그쵸..",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004202.1.20",
          "form": "나갈일도 별로 없었는데도",
          "original_form": "나갈일도 별로 없었는데도",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004202.1.21",
          "form": "집안에서 너무 힘들었던 ;;",
          "original_form": "집안에서 너무 힘들었던 ;;",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004202.1.22",
          "form": "무슨 계절 좋아하세요?",
          "original_form": "무슨 계절 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004202.1.23",
          "form": "저는 더위를 많이 타서 겨울을 좋아해요!",
          "original_form": "저는 더위를 많이 타서 겨울을 좋아해요!",
          "speaker_id": "1",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004202.1.24",
          "form": "겨울 좋죠",
          "original_form": "겨울 좋죠",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004202.1.25",
          "form": "저도 겨울 좋아해요",
          "original_form": "저도 겨울 좋아해요",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004202.1.26",
          "form": "추위는 싫지만 전기장판에서 귤까먹는거 너무 좋아요",
          "original_form": "추위는 싫지만 전기장판에서 귤까먹는거 너무 좋아요",
          "speaker_id": "2",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004202.1.27",
          "form": "저도 겨울에 귤 까먹는거 너무 좋아해요",
          "original_form": "저도 겨울에 귤 까먹는거 너무 좋아해요",
          "speaker_id": "1",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004202.1.28",
          "form": "군고구마나 붕어빵도 겨울 별미로..ㅎㅎ",
          "original_form": "군고구마나 붕어빵도 겨울 별미로..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004202.1.29",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004202.1.30",
          "form": "맛있는 음식의 계절!",
          "original_form": "맛있는 음식의 계절!",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004202.1.31",
          "form": "뜨끈한거 먹으면 사르르 녹아요 ㅎ",
          "original_form": "뜨끈한거 먹으면 사르르 녹아요 ㅎ",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004202.1.32",
          "form": "뭔가 올해는",
          "original_form": "뭔가 올해는",
          "speaker_id": "2",
          "time": "20210821 15:35"
        },
        {
          "id": "MDRW2100004202.1.33",
          "form": "겨울이 빨리 올 것 같네요",
          "original_form": "겨울이 빨리 올 것 같네요",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004202.1.34",
          "form": "너무 좋아요",
          "original_form": "너무 좋아요",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004202.1.35",
          "form": "얼른 겨울이 왔으면 좋겠어요",
          "original_form": "얼른 겨울이 왔으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004202.1.36",
          "form": "진짜 좋아하시나 보네요",
          "original_form": "진짜 좋아하시나 보네요",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004202.1.37",
          "form": "진심이 느껴졌어요 ㅋㅋㅋㅋ",
          "original_form": "진심이 느껴졌어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:36"
        },
        {
          "id": "MDRW2100004202.1.38",
          "form": "저는 가을 봄 중간 계절을 너무 좋아해서",
          "original_form": "저는 가을 봄 중간 계절을 너무 좋아해서",
          "speaker_id": "2",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004202.1.39",
          "form": "천찬히 와줬으면 하는 바람..",
          "original_form": "천찬히 와줬으면 하는 바람..",
          "speaker_id": "2",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004202.1.40",
          "form": "하지만 여름보다는 겨울이 2000배 좋앙ㅅ",
          "original_form": "하지만 여름보다는 겨울이 2000배 좋앙ㅅ",
          "speaker_id": "2",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004202.1.41",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004202.1.42",
          "form": "여름은...너무 사람을 지치게 하는 거 같아요",
          "original_form": "여름은...너무 사람을 지치게 하는 거 같아요",
          "speaker_id": "1",
          "time": "20210821 15:37"
        },
        {
          "id": "MDRW2100004202.1.43",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004202.1.44",
          "form": "사람이 녹아내리는...",
          "original_form": "사람이 녹아내리는...",
          "speaker_id": "2",
          "time": "20210821 15:38"
        },
        {
          "id": "MDRW2100004202.1.45",
          "form": "휴가도 못가서",
          "original_form": "휴가도 못가서",
          "speaker_id": "2",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100004202.1.46",
          "form": "슬프기만 할 따름이에요",
          "original_form": "슬프기만 할 따름이에요",
          "speaker_id": "2",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100004202.1.47",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "1",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100004202.1.48",
          "form": "원래는 바다에 들어가거나 계곡이라도...가는데",
          "original_form": "원래는 바다에 들어가거나 계곡이라도...가는데",
          "speaker_id": "1",
          "time": "20210821 15:39"
        },
        {
          "id": "MDRW2100004202.1.49",
          "form": "여름은 휴가의 계절인데",
          "original_form": "여름은 휴가의 계절인데",
          "speaker_id": "2",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004202.1.50",
          "form": "너무 슬펐어요",
          "original_form": "너무 슬펐어요",
          "speaker_id": "2",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004202.1.51",
          "form": "물 만나고",
          "original_form": "물 만나고",
          "speaker_id": "2",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004202.1.52",
          "form": "이 더위를 싸악 씻겨줘야하는데",
          "original_form": "이 더위를 싸악 씻겨줘야하는데",
          "speaker_id": "2",
          "time": "20210821 15:41"
        },
        {
          "id": "MDRW2100004202.1.53",
          "form": "맞아요...진짜...얼른 휴가를 갈 수 있었으면 좋겠네요",
          "original_form": "맞아요...진짜...얼른 휴가를 갈 수 있었으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210821 15:42"
        }
      ]
