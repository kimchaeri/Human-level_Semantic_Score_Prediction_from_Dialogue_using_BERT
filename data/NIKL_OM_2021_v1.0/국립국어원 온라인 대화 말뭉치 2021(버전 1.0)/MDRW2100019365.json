      "utterance": [
        {
          "id": "MDRW2100019365.1.1",
          "form": "네네 이게 원래 품질관리를 많이 해줘야하는 품종인데",
          "original_form": "네네 이게 원래 품질관리를 많이 해줘야하는 품종인데",
          "speaker_id": "2",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019365.1.2",
          "form": "재배농가가 유행타고 확 늘어나면서",
          "original_form": "재배농가가 유행타고 확 늘어나면서",
          "speaker_id": "2",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019365.1.3",
          "form": "그냥 키우기만 한다고 하더라구요 ㅠ",
          "original_form": "그냥 키우기만 한다고 하더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019365.1.4",
          "form": "저렴해진건 좋은데 아쉬워요 ㅠ",
          "original_form": "저렴해진건 좋은데 아쉬워요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 18:22"
        },
        {
          "id": "MDRW2100019365.1.5",
          "form": "맞아요 처음 나왔을때 가격은 비쌌지만",
          "original_form": "맞아요 처음 나왔을때 가격은 비쌌지만",
          "speaker_id": "1",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100019365.1.6",
          "form": "진짜 달아서 맛있었는데 ㅠㅠ",
          "original_form": "진짜 달아서 맛있었는데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100019365.1.7",
          "form": "그쵸 그리고 약간 망고향?이 나서 신기햇거든요",
          "original_form": "그쵸 그리고 약간 망고향?이 나서 신기햇거든요",
          "speaker_id": "2",
          "time": "20210827 18:24"
        },
        {
          "id": "MDRW2100019365.1.8",
          "form": "아 맞아요!",
          "original_form": "아 맞아요!",
          "speaker_id": "1",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019365.1.9",
          "form": "식감도 좋았어요 ㅎㅎ",
          "original_form": "식감도 좋았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019365.1.10",
          "form": "거봉스러운 느낌 ㅋㅋ",
          "original_form": "거봉스러운 느낌 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019365.1.11",
          "form": "이제는 그냥 달고 씨앗 없어서 편한 맛에 먹는거같아요 ㅋㅋㅋ",
          "original_form": "이제는 그냥 달고 씨앗 없어서 편한 맛에 먹는거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:26"
        },
        {
          "id": "MDRW2100019365.1.12",
          "form": "맞아요 ㄱㅋ 가격도 한송이에 막 몇만원 하고 그랬었지않나요",
          "original_form": "맞아요 ㄱㅋ 가격도 한송이에 막 몇만원 하고 그랬었지않나요",
          "speaker_id": "1",
          "time": "20210827 18:27"
        },
        {
          "id": "MDRW2100019365.1.13",
          "form": "네네 ㅋㅋㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019365.1.14",
          "form": "한송이에 2~3만원정도 했던거같아요",
          "original_form": "한송이에 2~3만원정도 했던거같아요",
          "speaker_id": "2",
          "time": "20210827 18:28"
        },
        {
          "id": "MDRW2100019365.1.15",
          "form": "진짜 너무 비싸긴 했네요 ㅋㅋ",
          "original_form": "진짜 너무 비싸긴 했네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:29"
        },
        {
          "id": "MDRW2100019365.1.16",
          "form": "그 정도면 복숭아 한박스 가격인데 ㅋㅋ",
          "original_form": "그 정도면 복숭아 한박스 가격인데 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019365.1.17",
          "form": "그쵸 ㅋㅋㅋㅋㅋ 그리구 마트에서 잘 팔지도 않고",
          "original_form": "그쵸 ㅋㅋㅋㅋㅋ 그리구 마트에서 잘 팔지도 않고",
          "speaker_id": "2",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019365.1.18",
          "form": "맞아요 ㅋㅋ 사실 그 한창 유행한후로 먹은적이 거의 없네요 ㅋㅋ",
          "original_form": "맞아요 ㅋㅋ 사실 그 한창 유행한후로 먹은적이 거의 없네요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:32"
        },
        {
          "id": "MDRW2100019365.1.19",
          "form": "요즘은 복숭아철이라 복숭아가 낫죠 ㅋㅋㅋ",
          "original_form": "요즘은 복숭아철이라 복숭아가 낫죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100019365.1.20",
          "form": "딱복 좋아하시나요?",
          "original_form": "딱복 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100019365.1.21",
          "form": "네 ! 저는 사실 복숭아라면 가리지않아요 ㅋㅋ",
          "original_form": "네 ! 저는 사실 복숭아라면 가리지않아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100019365.1.22",
          "form": "약간 딱복과 물복 사이를 젤 좋아하지만 ㅋㅋ",
          "original_form": "약간 딱복과 물복 사이를 젤 좋아하지만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:38"
        },
        {
          "id": "MDRW2100019365.1.23",
          "form": "저는 딱복파여서 딱복 끝물인 요즘 너무 슬퍼요 ㅠ",
          "original_form": "저는 딱복파여서 딱복 끝물인 요즘 너무 슬퍼요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 18:39"
        }
      ]
    }
