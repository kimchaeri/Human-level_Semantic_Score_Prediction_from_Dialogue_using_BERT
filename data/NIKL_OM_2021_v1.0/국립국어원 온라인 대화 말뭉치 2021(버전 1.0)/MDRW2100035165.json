      "utterance": [
        {
          "id": "MDRW2100035165.1.1",
          "form": "있어요! 아주 오래전에",
          "original_form": "있어요! 아주 오래전에",
          "speaker_id": "2",
          "time": "20210829 09:22"
        },
        {
          "id": "MDRW2100035165.1.2",
          "form": "저도 한 5년전에 겨드랑이했는데",
          "original_form": "저도 한 5년전에 겨드랑이했는데",
          "speaker_id": "1",
          "time": "20210829 09:22"
        },
        {
          "id": "MDRW2100035165.1.3",
          "form": "10회하라고할때 할걸 귀찮아서 5회하고 관뒀거든요ㅠ",
          "original_form": "10회하라고할때 할걸 귀찮아서 5회하고 관뒀거든요ㅠ",
          "speaker_id": "1",
          "time": "20210829 09:23"
        },
        {
          "id": "MDRW2100035165.1.4",
          "form": "가격은 어느정도였나요?",
          "original_form": "가격은 어느정도였나요?",
          "speaker_id": "2",
          "time": "20210829 09:23"
        },
        {
          "id": "MDRW2100035165.1.5",
          "form": "그럼 현재는 안나나요?",
          "original_form": "그럼 현재는 안나나요?",
          "speaker_id": "2",
          "time": "20210829 09:23"
        },
        {
          "id": "MDRW2100035165.1.6",
          "form": "그때 좀 공장형 피부과였어서",
          "original_form": "그때 좀 공장형 피부과였어서",
          "speaker_id": "1",
          "time": "20210829 09:23"
        },
        {
          "id": "MDRW2100035165.1.7",
          "form": "5회에 2만3천원인가 되게 싸게 주고했던것같아용",
          "original_form": "5회에 2만3천원인가 되게 싸게 주고했던것같아용",
          "speaker_id": "1",
          "time": "20210829 09:24"
        },
        {
          "id": "MDRW2100035165.1.8",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210829 09:24"
        },
        {
          "id": "MDRW2100035165.1.9",
          "form": "어머 엄청 싸넹6",
          "original_form": "어머 엄청 싸넹6",
          "speaker_id": "2",
          "time": "20210829 09:24"
        },
        {
          "id": "MDRW2100035165.1.10",
          "form": "싸네요",
          "original_form": "싸네요",
          "speaker_id": "2",
          "time": "20210829 09:24"
        },
        {
          "id": "MDRW2100035165.1.11",
          "form": "저는 10년 더 되가서.. 비쌌던 기억이.. 그리고 지금 다시 나요 ㅠㅠ",
          "original_form": "저는 10년 더 되가서.. 비쌌던 기억이.. 그리고 지금 다시 나요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 09:24"
        },
        {
          "id": "MDRW2100035165.1.12",
          "form": "그죵ㅋㅋㅋ 차비랑 비슷하게 들겠더라구요ㅋㅋㅋ",
          "original_form": "그죵ㅋㅋㅋ 차비랑 비슷하게 들겠더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 09:24"
        },
        {
          "id": "MDRW2100035165.1.13",
          "form": "아하ㅠㅠ 저도",
          "original_form": "아하ㅠㅠ 저도",
          "speaker_id": "1",
          "time": "20210829 09:24"
        },
        {
          "id": "MDRW2100035165.1.14",
          "form": "예전만큼 안나지는 않아용",
          "original_form": "예전만큼 안나지는 않아용",
          "speaker_id": "1",
          "time": "20210829 09:24"
        },
        {
          "id": "MDRW2100035165.1.15",
          "form": "듬섬듬성 나요...",
          "original_form": "듬섬듬성 나요...",
          "speaker_id": "1",
          "time": "20210829 09:24"
        },
        {
          "id": "MDRW2100035165.1.16",
          "form": "부위는 겨드랑이만 하셨나요,",
          "original_form": "부위는 겨드랑이만 하셨나요,",
          "speaker_id": "2",
          "time": "20210829 09:24"
        },
        {
          "id": "MDRW2100035165.1.17",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210829 09:25"
        },
        {
          "id": "MDRW2100035165.1.18",
          "form": "그래서 가정용 제모기를 하나 장만할까 고민중이에용",
          "original_form": "그래서 가정용 제모기를 하나 장만할까 고민중이에용",
          "speaker_id": "1",
          "time": "20210829 09:25"
        },
        {
          "id": "MDRW2100035165.1.19",
          "form": "저는 종아리랑 겨드랑이요 ㅋ",
          "original_form": "저는 종아리랑 겨드랑이요 ㅋ",
          "speaker_id": "2",
          "time": "20210829 09:25"
        },
        {
          "id": "MDRW2100035165.1.20",
          "form": "피부과가서 제모하고 관리받는게",
          "original_form": "피부과가서 제모하고 관리받는게",
          "speaker_id": "1",
          "time": "20210829 09:26"
        },
        {
          "id": "MDRW2100035165.1.21",
          "form": "귀찮더라구용",
          "original_form": "귀찮더라구용",
          "speaker_id": "1",
          "time": "20210829 09:26"
        },
        {
          "id": "MDRW2100035165.1.22",
          "form": "잘 사용할까요? 아프면 안하게 될 것 같기도 ㅋ",
          "original_form": "잘 사용할까요? 아프면 안하게 될 것 같기도 ㅋ",
          "speaker_id": "2",
          "time": "20210829 09:26"
        },
        {
          "id": "MDRW2100035165.1.23",
          "form": "그래서 걱정이에요ㅠㅠ",
          "original_form": "그래서 걱정이에요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 09:26"
        },
        {
          "id": "MDRW2100035165.1.24",
          "form": "기껏 사놓고 피부과 다닐까봐...ㅋㅋ",
          "original_form": "기껏 사놓고 피부과 다닐까봐...ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 09:26"
        },
        {
          "id": "MDRW2100035165.1.25",
          "form": "인중 제모는 엄청 아프다고 하더라구요 ㅋ",
          "original_form": "인중 제모는 엄청 아프다고 하더라구요 ㅋ",
          "speaker_id": "2",
          "time": "20210829 09:28"
        }
      ]
    }
