      "utterance": [
        {
          "id": "MDRW2100019143.1.1",
          "form": "name2님 종교 있으신가요",
          "original_form": "&name2&님 종교 있으신가요",
          "speaker_id": "1",
          "time": "20210827 15:37"
        },
        {
          "id": "MDRW2100019143.1.2",
          "form": "아니요 저는 무교에요",
          "original_form": "아니요 저는 무교에요",
          "speaker_id": "2",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019143.1.3",
          "form": "name1님은 종교가 있으신가요?",
          "original_form": "&name1&님은 종교가 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 15:42"
        },
        {
          "id": "MDRW2100019143.1.4",
          "form": "저도 무교에요",
          "original_form": "저도 무교에요",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019143.1.5",
          "form": "근데 모태신앙ㅋㅋ",
          "original_form": "근데 모태신앙ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:43"
        },
        {
          "id": "MDRW2100019143.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋ모태신앙인데 어떻게 무교일수있어요?!",
          "original_form": "ㅋㅋㅋㅋㅋㅋ모태신앙인데 어떻게 무교일수있어요?!",
          "speaker_id": "2",
          "time": "20210827 17:00"
        },
        {
          "id": "MDRW2100019143.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋ 그렇게됏어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 그렇게됏어요",
          "speaker_id": "1",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100019143.1.8",
          "form": "근데 전 불교 궁금함",
          "original_form": "근데 전 불교 궁금함",
          "speaker_id": "1",
          "time": "20210827 17:37"
        },
        {
          "id": "MDRW2100019143.1.9",
          "form": "저두요 제 남편이 불교신자에요",
          "original_form": "저두요 제 남편이 불교신자에요",
          "speaker_id": "2",
          "time": "20210827 17:42"
        },
        {
          "id": "MDRW2100019143.1.10",
          "form": "아 진짜요? 결혼하셨나봐요",
          "original_form": "아 진짜요? 결혼하셨나봐요",
          "speaker_id": "1",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100019143.1.11",
          "form": "네 근데 군대갔어요 지금",
          "original_form": "네 근데 군대갔어요 지금",
          "speaker_id": "2",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100019143.1.12",
          "form": "그치만 어쨌든 저는 무교랍니다",
          "original_form": "그치만 어쨌든 저는 무교랍니다",
          "speaker_id": "2",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100019143.1.13",
          "form": "아이고 ? 결혼후 군대를 ?",
          "original_form": "아이고 ? 결혼후 군대를 ?",
          "speaker_id": "1",
          "time": "20210827 18:08"
        },
        {
          "id": "MDRW2100019143.1.14",
          "form": "불자님도참....",
          "original_form": "불자님도참....",
          "speaker_id": "1",
          "time": "20210827 18:09"
        },
        {
          "id": "MDRW2100019143.1.15",
          "form": "그러니까요 흑흑.. 그치만 믿음이 중요하죠... 후후",
          "original_form": "그러니까요 흑흑.. 그치만 믿음이 중요하죠... 후후",
          "speaker_id": "2",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100019143.1.16",
          "form": "name1님 모태신앙은 어떤종교신가요?",
          "original_form": "&name1&님 모태신앙은 어떤종교신가요?",
          "speaker_id": "2",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100019143.1.17",
          "form": "천주교입니당",
          "original_form": "천주교입니당",
          "speaker_id": "1",
          "time": "20210827 19:00"
        },
        {
          "id": "MDRW2100019143.1.18",
          "form": "갑자기저도 나가볼까 하는생각이들어요",
          "original_form": "갑자기저도 나가볼까 하는생각이들어요",
          "speaker_id": "1",
          "time": "20210827 19:01"
        },
        {
          "id": "MDRW2100019143.1.19",
          "form": "오 천주교 좋죠. 바빠서 못가신건가요? ㅠ",
          "original_form": "오 천주교 좋죠. 바빠서 못가신건가요? ㅠ",
          "speaker_id": "2",
          "time": "20210827 19:11"
        },
        {
          "id": "MDRW2100019143.1.20",
          "form": "아뇨 귀찮아서 ㅎ 잘생긴 신자들 있는데면 가고싶어요",
          "original_form": "아뇨 귀찮아서 ㅎ 잘생긴 신자들 있는데면 가고싶어요",
