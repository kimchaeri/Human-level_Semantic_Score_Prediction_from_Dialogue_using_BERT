      "utterance": [
        {
          "id": "MDRW2100022895.1.1",
          "form": "두리안..? 냄새 진짜 못견딤",
          "original_form": "두리안..? 냄새 진짜 못견딤",
          "speaker_id": "2",
          "time": "20210829 18:54"
        },
        {
          "id": "MDRW2100022895.1.2",
          "form": "아 안먹어봤는데 냄새 별로야?",
          "original_form": "아 안먹어봤는데 냄새 별로야?",
          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022895.1.3",
          "form": "어 진짜 구리구리해...",
          "original_form": "어 진짜 구리구리해...",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022895.1.4",
          "form": "무슨냄새랑 비슷해? 청국장?",
          "original_form": "무슨냄새랑 비슷해? 청국장?",
          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022895.1.5",
          "form": "아니 약간 방구냄새...?",
          "original_form": "아니 약간 방구냄새...?",
          "speaker_id": "2",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022895.1.6",
          "form": "방구냄새?ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "방구냄새?ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:55"
        },
        {
          "id": "MDRW2100022895.1.7",
          "form": "엉 좀...청국장이나 된장류의 짠내나는 구리구리가 아니라 좀 속 안좋은 구리구리 냄새야",
          "original_form": "엉 좀...청국장이나 된장류의 짠내나는 구리구리가 아니라 좀 속 안좋은 구리구리 냄새야",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022895.1.8",
          "form": "헐...나는 감이 싫어",
          "original_form": "헐...나는 감이 싫어",
          "speaker_id": "1",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022895.1.9",
          "form": "근데 우리 아빠는 맛있다고 좋아하더라ㅋㅋㅋ 나는 별로야... 오? 감 왜?? 감 맛있는디...",
          "original_form": "근데 우리 아빠는 맛있다고 좋아하더라ㅋㅋㅋ 나는 별로야... 오? 감 왜?? 감 맛있는디...",
          "speaker_id": "2",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022895.1.10",
          "form": "감은 과즙이 정말 없어",
          "original_form": "감은 과즙이 정말 없어",
          "speaker_id": "1",
          "time": "20210829 18:56"
        },
        {
          "id": "MDRW2100022895.1.11",
          "form": "홍시같은건?",
          "original_form": "홍시같은건?",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022895.1.12",
          "form": "홍시는 콧물같아 약간 굴이랑 비슷..",
          "original_form": "홍시는 콧물같아 약간 굴이랑 비슷..",
          "speaker_id": "1",
          "time": "20210829 18:57"
        },
        {
          "id": "MDRW2100022895.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ난 굴도 좋아해서...홍시도 좋아....곶감도 좋아해",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ난 굴도 좋아해서...홍시도 좋아....곶감도 좋아해",
          "speaker_id": "2",
          "time": "20210829 18:57"
        },
