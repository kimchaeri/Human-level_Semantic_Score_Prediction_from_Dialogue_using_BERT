      "utterance": [
        {
          "id": "MDRW2100017106.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017106.1.2",
          "form": "안녕하세요! 오늘저녁 아직 안드셨죠?",
          "original_form": "안녕하세요! 오늘저녁 아직 안드셨죠?",
          "speaker_id": "2",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017106.1.3",
          "form": "뭐 드실 예정이신가요?",
          "original_form": "뭐 드실 예정이신가요?",
          "speaker_id": "2",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017106.1.4",
          "form": "아직이요!",
          "original_form": "아직이요!",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017106.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210826 17:38"
        },
        {
          "id": "MDRW2100017106.1.6",
          "form": "전 아직 안정했지만 집에있는 미역국이랑 밥먹을것같아요ㅋㅋㅋㅋㅋ",
          "original_form": "전 아직 안정했지만 집에있는 미역국이랑 밥먹을것같아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017106.1.7",
          "form": "치킨도먹고픈데",
          "original_form": "치킨도먹고픈데",
          "speaker_id": "2",
          "time": "20210826 17:41"
        },
        {
          "id": "MDRW2100017106.1.8",
          "form": "좀더 고민해봐야죠",
          "original_form": "좀더 고민해봐야죠",
          "speaker_id": "2",
          "time": "20210826 17:42"
        },
        {
          "id": "MDRW2100017106.1.9",
          "form": "와 치킨으로",
          "original_form": "와 치킨으로",
          "speaker_id": "1",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017106.1.10",
          "form": "갑시다..",
          "original_form": "갑시다..",
          "speaker_id": "1",
          "time": "20210826 17:50"
        },
        {
          "id": "MDRW2100017106.1.11",
          "form": "둘다 아니고 중국집으로 시켰어요ㅠㅠ 지금부터 한시간의 기다림이...",
          "original_form": "둘다 아니고 중국집으로 시켰어요ㅠㅠ 지금부터 한시간의 기다림이...",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017106.1.12",
          "form": "저녁정해지셨나요?",
          "original_form": "저녁정해지셨나요?",
          "speaker_id": "2",
          "time": "20210826 18:22"
        },
        {
          "id": "MDRW2100017106.1.13",
          "form": "오 중국집도",
          "original_form": "오 중국집도",
          "speaker_id": "1",
          "time": "20210826 18:28"
        },
        {
          "id": "MDRW2100017106.1.14",
          "form": "너무맛있잖아요 저는아직이요",
          "original_form": "너무맛있잖아요 저는아직이요",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017106.1.15",
          "form": "짬뽕vs짜장?",
          "original_form": "짬뽕vs짜장?",
          "speaker_id": "1",
          "time": "20210826 18:29"
        },
        {
          "id": "MDRW2100017106.1.16",
          "form": "저는 오늘은 짜장이요! 평소에는 야끼우동파에욬ㅋㅋㅋㅋㅋ",
          "original_form": "저는 오늘은 짜장이요! 평소에는 야끼우동파에욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017106.1.17",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 18:43"
        },
        {
          "id": "MDRW2100017106.1.18",
          "form": "ㅋㅋㅋㅋ 전 짬뽕파입니다",
          "original_form": "ㅋㅋㅋㅋ 전 짬뽕파입니다",
          "speaker_id": "1",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100017106.1.19",
          "form": "야끼우동너무좋죠",
          "original_form": "야끼우동너무좋죠",
          "speaker_id": "1",
          "time": "20210826 18:46"
        },
        {
          "id": "MDRW2100017106.1.20",
          "form": "짬뽕도 맛있어요 뭘고르기 힘들죠",
          "original_form": "짬뽕도 맛있어요 뭘고르기 힘들죠",
          "speaker_id": "2",
          "time": "20210826 18:51"
        },
        {
          "id": "MDRW2100017106.1.21",
          "form": "탕수육은 찍먹파? 부먹파?",
          "original_form": "탕수육은 찍먹파? 부먹파?",
          "speaker_id": "2",
          "time": "20210826 18:52"
        },
        {
          "id": "MDRW2100017106.1.22",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100017106.1.23",
          "form": "찍먹입니다!!!!",
          "original_form": "찍먹입니다!!!!",
          "speaker_id": "1",
          "time": "20210826 18:57"
        },
        {
          "id": "MDRW2100017106.1.24",
          "form": "헐 저두요! 부먹은 눅눅해져서 바삭바삭함이 사라지는게 싫어요ㅠㅠ",
          "original_form": "헐 저두요! 부먹은 눅눅해져서 바삭바삭함이 사라지는게 싫어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 19:54"
        },
        {
          "id": "MDRW2100017106.1.25",
          "form": "오늘 먹은 중국집은 성공적이었어욬ㅋㅋㅋㅋ 첨시켜봤는데 굿",
          "original_form": "오늘 먹은 중국집은 성공적이었어욬ㅋㅋㅋㅋ 첨시켜봤는데 굿",
          "speaker_id": "2",
          "time": "20210826 19:56"
        },
        {
          "id": "MDRW2100017106.1.26",
          "form": "오 굿이네요ㅋㅋㅋ",
          "original_form": "오 굿이네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:12"
        },
        {
          "id": "MDRW2100017106.1.27",
          "form": "중국집성공쉽지얗은데",
          "original_form": "중국집성공쉽지얗은데",
