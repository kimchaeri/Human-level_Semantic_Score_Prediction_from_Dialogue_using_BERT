      "utterance": [
        {
          "id": "MDRW2100029220.1.1",
          "form": "ㅋㅋㅋㅋ 로제떡볶이도 신전 맛있던데요?",
          "original_form": "ㅋㅋㅋㅋ 로제떡볶이도 신전 맛있던데요?",
          "speaker_id": "1",
          "time": "20210903 00:24"
        },
        {
          "id": "MDRW2100029220.1.2",
          "form": "다이어트엔 최악이겠지만여...",
          "original_form": "다이어트엔 최악이겠지만여...",
          "speaker_id": "1",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029220.1.3",
          "form": "와 그건 안먹어봤는데 ㅋㅋㅋ",
          "original_form": "와 그건 안먹어봤는데 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029220.1.4",
          "form": "운동 하시나요",
          "original_form": "운동 하시나요",
          "speaker_id": "2",
          "time": "20210903 00:25"
        },
        {
          "id": "MDRW2100029220.1.5",
          "form": "운동은 숨쉬기만 하고 있어요... 운동 좋아하세요?",
          "original_form": "운동은 숨쉬기만 하고 있어요... 운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 00:28"
        },
        {
          "id": "MDRW2100029220.1.6",
          "form": "네..pt 하고잇고",
          "original_form": "네..pt 하고잇고",
          "speaker_id": "2",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029220.1.7",
          "form": "그전엔 필라테스도 했어요",
          "original_form": "그전엔 필라테스도 했어요",
          "speaker_id": "2",
          "time": "20210903 00:30"
        },
        {
          "id": "MDRW2100029220.1.8",
          "form": "와 부지런하시네요!!",
          "original_form": "와 부지런하시네요!!",
          "speaker_id": "1",
          "time": "20210903 00:31"
        },
        {
          "id": "MDRW2100029220.1.9",
          "form": "저도 필라테스는 해보고 싶었어요",
          "original_form": "저도 필라테스는 해보고 싶었어요",
          "speaker_id": "1",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029220.1.10",
          "form": "완전 추천!!! 내일 아침에.pt해야해서 슬슬 자려구요",
          "original_form": "완전 추천!!! 내일 아침에.pt해야해서 슬슬 자려구요",
          "speaker_id": "2",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029220.1.11",
          "form": "너무 즐거운 대화인데 내일 마저 연락드려도",
          "original_form": "너무 즐거운 대화인데 내일 마저 연락드려도",
          "speaker_id": "2",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029220.1.12",
          "form": "될까요",
          "original_form": "될까요",
          "speaker_id": "2",
          "time": "20210903 00:32"
        },
        {
          "id": "MDRW2100029220.1.13",
          "form": "ㅋㅋㅋ 네 주무셔야죠 굿밤되세요",
          "original_form": "ㅋㅋㅋ 네 주무셔야죠 굿밤되세요",
          "speaker_id": "1",
          "time": "20210903 00:33"
        },
        {
          "id": "MDRW2100029220.1.14",
          "form": "네 감사합니다. 잘주무셔용",
          "original_form": "네 감사합니다. 잘주무셔용",
          "speaker_id": "2",
          "time": "20210903 00:34"
        },
        {
          "id": "MDRW2100029220.1.15",
          "form": "영양제 챙겨 드시나요",
          "original_form": "영양제 챙겨 드시나요",
          "speaker_id": "2",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029220.1.16",
          "form": "비타민디는 먹고 있어요 name2은 드시나요?",
          "original_form": "비타민디는 먹고 있어요 &name2&은 드시나요?",
          "speaker_id": "1",
          "time": "20210903 19:26"
        },
        {
          "id": "MDRW2100029220.1.17",
          "form": "비타민씨랑 유산균이요 ㅋㅋ 비타민 디는 좀 효과가 있던가요",
          "original_form": "비타민씨랑 유산균이요 ㅋㅋ 비타민 디는 좀 효과가 있던가요",
          "speaker_id": "2",
          "time": "20210903 21:16"
        },
        {
          "id": "MDRW2100029220.1.18",
          "form": "음 효과가 있는걸까요 기력이 조금 더 생겼으면 좋겠는데 ㅋㅋㅋㅋㅋ",
          "original_form": "음 효과가 있는걸까요 기력이 조금 더 생겼으면 좋겠는데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100029220.1.19",
          "form": "안먹는것보다는 낫겠지 하면서 먹고있어요",
          "original_form": "안먹는것보다는 낫겠지 하면서 먹고있어요",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100029220.1.20",
          "form": "아 유산균은 어떤 제품을 먹어야 될지를 몰라서 못먹고 있어요",
          "original_form": "아 유산균은 어떤 제품을 먹어야 될지를 몰라서 못먹고 있어요",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100029220.1.21",
