      "utterance": [
        {
          "id": "MDRW2100008354.1.1",
          "form": "요즘 방학인가요?",
          "original_form": "요즘 방학인가요?",
          "speaker_id": "2",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008354.1.2",
          "form": "학생이세요?ㅎ 전직장인",
          "original_form": "학생이세요?ㅎ 전직장인",
          "speaker_id": "1",
          "time": "20210822 15:25"
        },
        {
          "id": "MDRW2100008354.1.3",
          "form": "요즘 학교 수업하나요?",
          "original_form": "요즘 학교 수업하나요?",
          "speaker_id": "1",
          "time": "20210822 15:26"
        },
        {
          "id": "MDRW2100008354.1.4",
          "form": "저는 요새 쉬어요 계약직하다가 코로나로 짤리고",
          "original_form": "저는 요새 쉬어요 계약직하다가 코로나로 짤리고",
          "speaker_id": "2",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008354.1.5",
          "form": "요새 온라인수업하려나 어떠려나",
          "original_form": "요새 온라인수업하려나 어떠려나",
          "speaker_id": "2",
          "time": "20210822 15:28"
        },
        {
          "id": "MDRW2100008354.1.6",
          "form": "졸업하신지 얼마나 되셨어요?ㅋ",
          "original_form": "졸업하신지 얼마나 되셨어요?ㅋ",
          "speaker_id": "1",
          "time": "20210822 15:29"
        },
        {
          "id": "MDRW2100008354.1.7",
          "form": "대학 졸업한지 한참 됐네요",
          "original_form": "대학 졸업한지 한참 됐네요",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008354.1.8",
          "form": "요즘은 차라리 학생때가 좋았다싶어요 ㅋ",
          "original_form": "요즘은 차라리 학생때가 좋았다싶어요 ㅋ",
          "speaker_id": "2",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008354.1.9",
          "form": "아ㅎ 대학생활은 즐거우셨나요?",
          "original_form": "아ㅎ 대학생활은 즐거우셨나요?",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008354.1.10",
          "form": "동아리활동 해보셨어요?",
          "original_form": "동아리활동 해보셨어요?",
          "speaker_id": "1",
          "time": "20210822 15:33"
        },
        {
          "id": "MDRW2100008354.1.11",
          "form": "음 고등학교 때까진했는데 대학땐 안했어요",
          "original_form": "음 고등학교 때까진했는데 대학땐 안했어요",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008354.1.12",
          "form": "지금생각하면그거도 다 경험인데 싶어요",
          "original_form": "지금생각하면그거도 다 경험인데 싶어요",
          "speaker_id": "2",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008354.1.13",
          "form": "무슨 동아리 해보셨어요?",
          "original_form": "무슨 동아리 해보셨어요?",
          "speaker_id": "1",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008354.1.14",
          "form": "저도 고딩때만 방과후 활동 이런것만 해봤네요",
          "original_form": "저도 고딩때만 방과후 활동 이런것만 해봤네요",
          "speaker_id": "1",
          "time": "20210822 15:37"
        },
        {
          "id": "MDRW2100008354.1.15",
          "form": "저는 그냥 독서부?",
          "original_form": "저는 그냥 독서부?",
          "speaker_id": "2",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008354.1.16",
          "form": "와 너무 옛날이라 기억이 거의 안나요 ㅋㅋ",
          "original_form": "와 너무 옛날이라 기억이 거의 안나요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:43"
        },
        {
          "id": "MDRW2100008354.1.17",
          "form": "오 책좋아하셨어요?ㅎ",
          "original_form": "오 책좋아하셨어요?ㅎ",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008354.1.18",
          "form": "전 퀼트공예 ㅎ",
          "original_form": "전 퀼트공예 ㅎ",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008354.1.19",
          "form": "바늘질해서 인형도 만들고 했는데",
          "original_form": "바늘질해서 인형도 만들고 했는데",
          "speaker_id": "1",
          "time": "20210822 15:44"
        },
        {
          "id": "MDRW2100008354.1.20",
          "form": "전 책좋아했어요",
          "original_form": "전 책좋아했어요",
          "speaker_id": "2",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008354.1.21",
          "form": "한참 꿈 많은10대때는 작가나 사서를 꿈 꿨 ㅋㅋ",
          "original_form": "한참 꿈 많은10대때는 작가나 사서를 꿈 꿨 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 15:51"
        },
        {
          "id": "MDRW2100008354.1.22",
          "form": "오ㅎ 작가 멋있네요",
          "original_form": "오ㅎ 작가 멋있네요",
          "speaker_id": "1",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008354.1.23",
          "form": "요즘은 다들 학교를 못가니",
          "original_form": "요즘은 다들 학교를 못가니",
          "speaker_id": "1",
          "time": "20210822 15:55"
        },
        {
          "id": "MDRW2100008354.1.24",
          "form": "친구들이랑 노는것도 다추억인데 그러질못할듯요",
          "original_form": "친구들이랑 노는것도 다추억인데 그러질못할듯요",
          "speaker_id": "1",
          "time": "20210822 15:56"
        },
        {
          "id": "MDRW2100008354.1.25",
          "form": "대학생들도 안타깝죠 뭔가 엠티 이런거도 못갈거고 흠",
          "original_form": "대학생들도 안타깝죠 뭔가 엠티 이런거도 못갈거고 흠",
          "speaker_id": "2",
          "time": "20210822 15:59"
        },
        {
          "id": "MDRW2100008354.1.26",
          "form": "그니까요 엠티도 잼난데",
          "original_form": "그니까요 엠티도 잼난데",
          "speaker_id": "1",
          "time": "20210822 16:00"
        },
        {
          "id": "MDRW2100008354.1.27",
          "form": "그때  아님 할수없는일이라 좀 안타까워요",
          "original_form": "그때  아님 할수없는일이라 좀 안타까워요",
          "speaker_id": "1",
          "time": "20210822 16:00"
        }
      ]
    }
