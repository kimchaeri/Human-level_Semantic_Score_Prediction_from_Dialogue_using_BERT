      "utterance": [
        {
          "id": "MDRW2100038684.1.1",
          "form": "주로 뭐 시켜드세요?!",
          "original_form": "주로 뭐 시켜드세요?!",
          "speaker_id": "2",
          "time": "20210902 19:27"
        },
        {
          "id": "MDRW2100038684.1.2",
          "form": "저 치킨 좋아해요!!!",
          "original_form": "저 치킨 좋아해요!!!",
          "speaker_id": "1",
          "time": "20210902 19:37"
        },
        {
          "id": "MDRW2100038684.1.3",
          "form": "헐헐 어디 브랜드요!!!!!!!!!!!",
          "original_form": "헐헐 어디 브랜드요!!!!!!!!!!!",
          "speaker_id": "2",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100038684.1.4",
          "form": "굽네랑 교촌이요 !!!",
          "original_form": "굽네랑 교촌이요 !!!",
          "speaker_id": "1",
          "time": "20210902 21:40"
        },
        {
          "id": "MDRW2100038684.1.5",
          "form": "저도 굽네 좋아해요! 고추바사삭",
          "original_form": "저도 굽네 좋아해요! 고추바사삭",
          "speaker_id": "2",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100038684.1.6",
          "form": "헐 저두 바사삭 굽네 최애에요 !",
          "original_form": "헐 저두 바사삭 굽네 최애에요 !",
          "speaker_id": "1",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100038684.1.7",
          "form": "소스도 맛집이잖아여 ㅜㅜ",
          "original_form": "소스도 맛집이잖아여 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100038684.1.8",
          "form": "오마이갓 그거 소스 두개씩 시켜서 왕창 찍어먹어야",
          "original_form": "오마이갓 그거 소스 두개씩 시켜서 왕창 찍어먹어야",
          "speaker_id": "2",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100038684.1.9",
          "form": "헐 저 그말하고 잇엇어요",
          "original_form": "헐 저 그말하고 잇엇어요",
          "speaker_id": "2",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100038684.1.10",
          "form": "역시 !!!!",
          "original_form": "역시 !!!!",
          "speaker_id": "1",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100038684.1.11",
          "form": "하 진짜 소스추가 배우신분",
          "original_form": "하 진짜 소스추가 배우신분",
          "speaker_id": "1",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100038684.1.12",
          "form": "마블링 한번 고블링 한번",
          "original_form": "마블링 한번 고블링 한번",
          "speaker_id": "1",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100038684.1.13",
          "form": "해줘야한다구엿",
          "original_form": "해줘야한다구엿",
          "speaker_id": "1",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100038684.1.14",
          "form": "오마이갓 당근이죠! 전 약간 마블링 좀 더 좋아하는 편이에요",
          "original_form": "오마이갓 당근이죠! 전 약간 마블링 좀 더 좋아하는 편이에요",
          "speaker_id": "2",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038684.1.15",
          "form": "name1님은요!?",
          "original_form": "&name1&님은요!?",
          "speaker_id": "2",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038684.1.16",
          "form": "저두여 !!!!",
          "original_form": "저두여 !!!!",
          "speaker_id": "1",
          "time": "20210902 22:01"
        },
        {
          "id": "MDRW2100038684.1.17",
          "form": "항상 마블링은 다먹구",
          "original_form": "항상 마블링은 다먹구",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038684.1.18",
          "form": "고블링은 조금 남아여 ㅎㅎㅎ",
          "original_form": "고블링은 조금 남아여 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:02"
        },
        {
          "id": "MDRW2100038684.1.19",
          "form": "앗 음식 취향 정말 찰떡이구만요 저희 ㅎㅎ",
          "original_form": "앗 음식 취향 정말 찰떡이구만요 저희 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100038684.1.20",
          "form": "저는 푸라닭도 좋아해요!",
          "original_form": "저는 푸라닭도 좋아해요!",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100038684.1.21",
          "form": "아시나요!?",
          "original_form": "아시나요!?",
          "speaker_id": "2",
          "time": "20210902 22:04"
        },
        {
          "id": "MDRW2100038684.1.22",
          "form": "헐 거기 고 추 마요 사랑합니다 저 !!!!",
          "original_form": "헐 거기 고 추 마요 사랑합니다 저 !!!!",
          "speaker_id": "1",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038684.1.23",
          "form": "헐랭 저도요 저 그거랑",
          "original_form": "헐랭 저도요 저 그거랑",
          "speaker_id": "2",
          "time": "20210902 22:06"
        },
        {
          "id": "MDRW2100038684.1.24",
