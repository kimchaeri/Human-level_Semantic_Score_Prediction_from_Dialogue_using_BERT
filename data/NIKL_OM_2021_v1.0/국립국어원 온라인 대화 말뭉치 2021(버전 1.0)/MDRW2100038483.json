      "utterance": [
        {
          "id": "MDRW2100038483.1.1",
          "form": "헐 전 아직도 소녀시대 좋아해요!",
          "original_form": "헐 전 아직도 소녀시대 좋아해요!",
          "speaker_id": "1",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100038483.1.2",
          "form": "태연 솔로곡 다 챙겨듣거든요!",
          "original_form": "태연 솔로곡 다 챙겨듣거든요!",
          "speaker_id": "1",
          "time": "20210902 14:24"
        },
        {
          "id": "MDRW2100038483.1.3",
          "form": "오 태연은 진짜",
          "original_form": "오 태연은 진짜",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100038483.1.4",
          "form": "누구나 좋아할 아티스트예요 그쵸?",
          "original_form": "누구나 좋아할 아티스트예요 그쵸?",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100038483.1.5",
          "form": "저는 에프엑스도 진짜 좋아했어요!!!",
          "original_form": "저는 에프엑스도 진짜 좋아했어요!!!",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100038483.1.6",
          "form": "지금도 좋아하지만....",
          "original_form": "지금도 좋아하지만....",
          "speaker_id": "2",
          "time": "20210902 14:25"
        },
        {
          "id": "MDRW2100038483.1.7",
          "form": "헐헐 저도요! 저 그 일렉트릭 쇼크도 좋아했고",
          "original_form": "헐헐 저도요! 저 그 일렉트릭 쇼크도 좋아했고",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038483.1.8",
          "form": "네네네!!!!",
          "original_form": "네네네!!!!",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038483.1.9",
          "form": "포 월즈도 ㅠ 완전 좋아해ㅛ어요@!",
          "original_form": "포 월즈도 ㅠ 완전 좋아해ㅛ어요@!",
          "speaker_id": "1",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038483.1.10",
          "form": "핑크텦 진짜명반이죠",
          "original_form": "핑크텦 진짜명반이죠",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038483.1.11",
          "form": "맞아요 진짜 하나하나 다 명반이죠!!!!",
          "original_form": "맞아요 진짜 하나하나 다 명반이죠!!!!",
          "speaker_id": "2",
          "time": "20210902 14:27"
        },
        {
          "id": "MDRW2100038483.1.12",
          "form": "맞아요!! 그래서 지금 활동 안 하는 거 너무 아쉬워요 ㅠ.^",
          "original_form": "맞아요!! 그래서 지금 활동 안 하는 거 너무 아쉬워요 ㅠ.^",
          "speaker_id": "1",
          "time": "20210902 14:28"
        },
        {
          "id": "MDRW2100038483.1.13",
          "form": "맞아요... 진짜 역대급 아이돌 그룹이었는데 ㅠㅠ",
          "original_form": "맞아요... 진짜 역대급 아이돌 그룹이었는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100038483.1.14",
          "form": "저는 이상하게",
          "original_form": "저는 이상하게",
          "speaker_id": "2",
          "time": "20210902 15:09"
        },
        {
          "id": "MDRW2100038483.1.15",
          "form": "에스파는 정이 안가더라구요?!",
          "original_form": "에스파는 정이 안가더라구요?!",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100038483.1.16",
          "form": "노래는 좋은데 ....!!!!",
          "original_form": "노래는 좋은데 ....!!!!",
          "speaker_id": "2",
          "time": "20210902 15:10"
        },
        {
          "id": "MDRW2100038483.1.17",
          "form": "저도 노래만 들어요!!!!!!!!!!!",
          "original_form": "저도 노래만 들어요!!!!!!!!!!!",
          "speaker_id": "1",
          "time": "20210902 15:58"
        },
        {
          "id": "MDRW2100038483.1.18",
          "form": "뭔가 제 취향이 아닌 사람들?!?!",
          "original_form": "뭔가 제 취향이 아닌 사람들?!?!",
