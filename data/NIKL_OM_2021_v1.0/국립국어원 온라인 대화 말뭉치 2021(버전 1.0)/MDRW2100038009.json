      "utterance": [
        {
          "id": "MDRW2100038009.1.1",
          "form": "나 무슨 꽁 트 보는 줄",
          "original_form": "나 무슨 꽁 트 보는 줄",
          "speaker_id": "2",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100038009.1.2",
          "form": "진짜 그게 무슨 의미 고ㅋㅋㅋㅋㅋㅋ",
          "original_form": "진짜 그게 무슨 의미 고ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100038009.1.3",
          "form": "ㅋㅋㅋㅋ꽁트 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ꽁트 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:17"
        },
        {
          "id": "MDRW2100038009.1.4",
          "form": "맞어 근데 코로나때문에 어쩔수없는거지만 ㅋㅋ",
          "original_form": "맞어 근데 코로나때문에 어쩔수없는거지만 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100038009.1.5",
          "form": "리코더를 안불면서 배우는건 한계가 있지",
          "original_form": "리코더를 안불면서 배우는건 한계가 있지",
          "speaker_id": "1",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100038009.1.6",
          "form": "그래 소리를 모르는데",
          "original_form": "그래 소리를 모르는데",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100038009.1.7",
          "form": "이게 맞는지 아닌지 어떻게 알아",
          "original_form": "이게 맞는지 아닌지 어떻게 알아",
          "speaker_id": "2",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100038009.1.8",
          "form": "그러니깐 ㅠㅠ ㅋㅋㅋ",
          "original_form": "그러니깐 ㅠㅠ ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:18"
        },
        {
          "id": "MDRW2100038009.1.9",
          "form": "만족하면서 보내야 하는데",
          "original_form": "만족하면서 보내야 하는데",
          "speaker_id": "2",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100038009.1.10",
          "form": "잘 안 되는구먼",
          "original_form": "잘 안 되는구먼",
          "speaker_id": "2",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100038009.1.11",
          "form": "그렇다이 ㅠㅠ ㅋㅋ",
          "original_form": "그렇다이 ㅠㅠ ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100038009.1.12",
          "form": "하나 맘에 안들면 신경쓰이긴 하지",
          "original_form": "하나 맘에 안들면 신경쓰이긴 하지",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100038009.1.13",
          "form": "그러니까~",
          "original_form": "그러니까~",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100038009.1.14",
          "form": "다른데는 연주하는 거 동영상도 찍어 보내준다는데",
          "original_form": "다른데는 연주하는 거 동영상도 찍어 보내준다는데",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100038009.1.15",
          "form": "맞어 맞어 ㅠ",
          "original_form": "맞어 맞어 ㅠ",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100038009.1.16",
          "form": "와 그런것도 없구나 ㅠㅠ",
          "original_form": "와 그런것도 없구나 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100038009.1.17",
          "form": "내가 피아노를 모르니",
          "original_form": "내가 피아노를 모르니",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100038009.1.18",
          "form": "응응",
          "original_form": "응응",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100038009.1.19",
          "form": "잘하고 있는지도 모르겠음",
          "original_form": "잘하고 있는지도 모르겠음",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100038009.1.20",
          "form": "근데 피아노 학원 어느정도 다니면은 그렇게 해주긴하던데",
          "original_form": "근데 피아노 학원 어느정도 다니면은 그렇게 해주긴하던데",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100038009.1.21",
          "form": "지안이 옛날에 피아노다녔을때 동영상찍어서 보내주더라구",
          "original_form": "지안이 옛날에 피아노다녔을때 동영상찍어서 보내주더라구",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100038009.1.22",
          "form": "그러니까~",
          "original_form": "그러니까~",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100038009.1.23",
          "form": "이게 집에서 하는 거랑은 좀 다를 텐데",
          "original_form": "이게 집에서 하는 거랑은 좀 다를 텐데",
          "speaker_id": "2",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100038009.1.24",
          "form": "전혀 모르겠음",
          "original_form": "전혀 모르겠음",
          "speaker_id": "2",
          "time": "20210901 22:24"
        }
      ]
    }
