      "utterance": [
        {
          "id": "MDRW2100005076.1.1",
          "form": "오늘 비오던데",
          "original_form": "오늘 비오던데",
          "speaker_id": "1",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005076.1.2",
          "form": "깜놀했자나 장마도아니고",
          "original_form": "깜놀했자나 장마도아니고",
          "speaker_id": "2",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005076.1.3",
          "form": "근데 다 그치고는 쨍쨍하더라",
          "original_form": "근데 다 그치고는 쨍쨍하더라",
          "speaker_id": "2",
          "time": "20210821 20:20"
        },
        {
          "id": "MDRW2100005076.1.4",
          "form": "비오는 거 좋아해?",
          "original_form": "비오는 거 좋아해?",
          "speaker_id": "1",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005076.1.5",
          "form": "나는 실내에서 비오는 소리 듣는 걸 좋아해",
          "original_form": "나는 실내에서 비오는 소리 듣는 걸 좋아해",
          "speaker_id": "1",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005076.1.6",
          "form": "보는거만 좋아해..ㅎ",
          "original_form": "보는거만 좋아해..ㅎ",
          "speaker_id": "2",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005076.1.7",
          "form": "나돜ㅋㅋ 비오는날 밖에 나가는건 싫어",
          "original_form": "나돜ㅋㅋ 비오는날 밖에 나가는건 싫어",
          "speaker_id": "2",
          "time": "20210821 20:21"
        },
        {
          "id": "MDRW2100005076.1.8",
          "form": "비오는날 드라이브해봤어?",
          "original_form": "비오는날 드라이브해봤어?",
          "speaker_id": "2",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005076.1.9",
          "form": "노래들으면서 창문에 빗물 토독토독하는 거 보면 마음이 포근포긍해져",
          "original_form": "노래들으면서 창문에 빗물 토독토독하는 거 보면 마음이 포근포긍해져",
          "speaker_id": "2",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005076.1.10",
          "form": "맞아맞아 어둑어둑할 때 비오는 소리 너무 좋아",
          "original_form": "맞아맞아 어둑어둑할 때 비오는 소리 너무 좋아",
          "speaker_id": "1",
          "time": "20210821 20:22"
        },
        {
          "id": "MDRW2100005076.1.11",
          "form": "좀 선선해진 걸 보니까 진짜 가을인가봐",
          "original_form": "좀 선선해진 걸 보니까 진짜 가을인가봐",
          "speaker_id": "1",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005076.1.12",
          "form": "맞아 요새는 밤에 춥더라",
          "original_form": "맞아 요새는 밤에 춥더라",
          "speaker_id": "2",
          "time": "20210821 20:23"
        },
        {
          "id": "MDRW2100005076.1.13",
          "form": "근데 낮엔 더워서 어느장단에맞춰야할지 선풍기 켰다가껐다가 하는즁",
          "original_form": "근데 낮엔 더워서 어느장단에맞춰야할지 선풍기 켰다가껐다가 하는즁",
          "speaker_id": "2",
          "time": "20210821 20:24"
        },
        {
          "id": "MDRW2100005076.1.14",
          "form": "ㅋㅋㅋ맞아맞아",
          "original_form": "ㅋㅋㅋ맞아맞아",
          "speaker_id": "1",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005076.1.15",
          "form": "무슨 계절 좋아해?",
          "original_form": "무슨 계절 좋아해?",
          "speaker_id": "1",
          "time": "20210821 20:26"
        },
        {
          "id": "MDRW2100005076.1.16",
          "form": "봄!",
          "original_form": "봄!",
          "speaker_id": "2",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005076.1.17",
          "form": "분홍분홍 연두연두하면서 살짝 쌀쌀한 날씨",
          "original_form": "분홍분홍 연두연두하면서 살짝 쌀쌀한 날씨",
          "speaker_id": "2",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005076.1.18",
          "form": "봄 아침엔 선선한데 낮에는 따뜻하고 꽃도 피고 포근한 계절",
          "original_form": "봄 아침엔 선선한데 낮에는 따뜻하고 꽃도 피고 포근한 계절",
          "speaker_id": "1",
          "time": "20210821 20:27"
        },
        {
          "id": "MDRW2100005076.1.19",
          "form": "나는 여름 겨울이 좋아 극단적이지",
          "original_form": "나는 여름 겨울이 좋아 극단적이지",
          "speaker_id": "1",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005076.1.20",
          "form": "ㅋㅋㅋㅋㅋ개웃기다 중간이 없네",
          "original_form": "ㅋㅋㅋㅋㅋ개웃기다 중간이 없네",
          "speaker_id": "2",
          "time": "20210821 20:28"
        },
        {
          "id": "MDRW2100005076.1.21",
          "form": "내일도 비소식 있네",
          "original_form": "내일도 비소식 있네",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005076.1.22",
          "form": "난 집에 있을거라 상관없지만ㅎㅋㅋㅋ",
          "original_form": "난 집에 있을거라 상관없지만ㅎㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:29"
        },
        {
          "id": "MDRW2100005076.1.23",
          "form": "나도나도 집순이이기도 한데 코로나라 더 나갈 일이없다",
          "original_form": "나도나도 집순이이기도 한데 코로나라 더 나갈 일이없다",
          "speaker_id": "2",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005076.1.24",
          "form": "이번주 내내 비오는 거 보니까 가을장마인가봐",
          "original_form": "이번주 내내 비오는 거 보니까 가을장마인가봐",
          "speaker_id": "1",
          "time": "20210821 20:31"
        },
        {
          "id": "MDRW2100005076.1.25",
          "form": "가을장마라는게 있는지 오늘알았넼ㅋㅋㅋ",
          "original_form": "가을장마라는게 있는지 오늘알았넼ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:32"
        },
        {
          "id": "MDRW2100005076.1.26",
          "form": "창문 열어놨더니 바람 많이 분다",
          "original_form": "창문 열어놨더니 바람 많이 분다",
          "speaker_id": "1",
          "time": "20210821 20:34"
        },
        {
          "id": "MDRW2100005076.1.27",
          "form": "그러고보니까 이제 매미도 거의 사라진거같어",
          "original_form": "그러고보니까 이제 매미도 거의 사라진거같어",
          "speaker_id": "2",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005076.1.28",
          "form": "창문 열어놓으면 항상 매미소리났었는데",
          "original_form": "창문 열어놓으면 항상 매미소리났었는데",
          "speaker_id": "2",
          "time": "20210821 20:35"
        },
        {
          "id": "MDRW2100005076.1.29",
          "form": "오 그러게 매미소리가 안 들리네",
          "original_form": "오 그러게 매미소리가 안 들리네",
          "speaker_id": "1",
          "time": "20210821 20:36"
        }
      ]
