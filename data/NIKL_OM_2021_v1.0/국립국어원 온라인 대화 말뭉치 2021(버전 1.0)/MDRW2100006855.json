      "utterance": [
        {
          "id": "MDRW2100006855.1.1",
          "form": "오늘 점심은 뭐 드실 예정인가요",
          "original_form": "오늘 점심은 뭐 드실 예정인가요",
          "speaker_id": "1",
          "time": "20210908 10:49"
        },
        {
          "id": "MDRW2100006855.1.2",
          "form": "남은 국과 반찬이요 ㅎㅎ",
          "original_form": "남은 국과 반찬이요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100006855.1.3",
          "form": "아 집에서 드시는군요",
          "original_form": "아 집에서 드시는군요",
          "speaker_id": "1",
          "time": "20210908 10:50"
        },
        {
          "id": "MDRW2100006855.1.4",
          "form": "네 파이팅님은요",
          "original_form": "네 파이팅님은요",
          "speaker_id": "2",
          "time": "20210908 10:52"
        },
        {
          "id": "MDRW2100006855.1.5",
          "form": "저는 회사인데 도시락을 싸요",
          "original_form": "저는 회사인데 도시락을 싸요",
          "speaker_id": "1",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100006855.1.6",
          "form": "아 구내식당이 없나요,ㅣ?",
          "original_form": "아 구내식당이 없나요,ㅣ?",
          "speaker_id": "2",
          "time": "20210908 10:53"
        },
        {
          "id": "MDRW2100006855.1.7",
          "form": "있어요 그런데 제가 밥을 빨리 못먹어서 불편하더라고요",
          "original_form": "있어요 그런데 제가 밥을 빨리 못먹어서 불편하더라고요",
          "speaker_id": "1",
          "time": "20210908 10:54"
        },
        {
          "id": "MDRW2100006855.1.8",
          "form": "점심시간은 1시간인가요",
          "original_form": "점심시간은 1시간인가요",
          "speaker_id": "2",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100006855.1.9",
          "form": "네 한시간이에요",
          "original_form": "네 한시간이에요",
          "speaker_id": "1",
          "time": "20210908 10:55"
        },
        {
          "id": "MDRW2100006855.1.10",
          "form": "탕비실에서 빵이나 과일 떡 등으로 먹어요",
          "original_form": "탕비실에서 빵이나 과일 떡 등으로 먹어요",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100006855.1.11",
          "form": "그래서 저녁에 폭식을 하는것같기도 ㅎㅎ",
          "original_form": "그래서 저녁에 폭식을 하는것같기도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 10:56"
        },
        {
          "id": "MDRW2100006855.1.12",
          "form": "그러시겠어요 간단히 먹다보면...",
          "original_form": "그러시겠어요 간단히 먹다보면...",
          "speaker_id": "2",
          "time": "20210908 10:59"
        },
        {
          "id": "MDRW2100006855.1.13",
          "form": "네 오늘은 출근과 동시에",
          "original_form": "네 오늘은 출근과 동시에",
          "speaker_id": "1",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100006855.1.14",
          "form": "퇴근하면 떡볶이 먹겠다고 다짐을 ㅋㅋ",
          "original_form": "퇴근하면 떡볶이 먹겠다고 다짐을 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100006855.1.15",
          "form": "떡볶이 좋아하시나요",
          "original_form": "떡볶이 좋아하시나요",
          "speaker_id": "2",
          "time": "20210908 11:00"
        },
        {
          "id": "MDRW2100006855.1.16",
          "form": "네 로제보다 그냥 기본을 엄청 좋아해요",
          "original_form": "네 로제보다 그냥 기본을 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210908 11:01"
        },
        {
          "id": "MDRW2100006855.1.17",
          "form": "로제 궁금하더라구요",
          "original_form": "로제 궁금하더라구요",
          "speaker_id": "2",
          "time": "20210908 11:02"
        },
        {
          "id": "MDRW2100006855.1.18",
          "form": "그냥 로제에 매운맛 더한덧?",
          "original_form": "그냥 로제에 매운맛 더한덧?",
