      "utterance": [
        {
          "id": "MDRW2100041610.1.1",
          "form": "내일 언니 온다더나??",
          "original_form": "내일 언니 온다더나??",
          "speaker_id": "2",
          "time": "20210906 12:40"
        },
        {
          "id": "MDRW2100041610.1.2",
          "form": "볶음밥 좋네 ㅎㅎ",
          "original_form": "볶음밥 좋네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 12:40"
        },
        {
          "id": "MDRW2100041610.1.3",
          "form": "유가네 맛있겠다",
          "original_form": "유가네 맛있겠다",
          "speaker_id": "2",
          "time": "20210906 12:40"
        },
        {
          "id": "MDRW2100041610.1.4",
          "form": "지금물어보지뭐ㅋ",
          "original_form": "지금물어보지뭐ㅋ",
          "speaker_id": "1",
          "time": "20210906 12:42"
        },
        {
          "id": "MDRW2100041610.1.5",
          "form": "옹 ㅎㅎ",
          "original_form": "옹 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 13:01"
        },
        {
          "id": "MDRW2100041610.1.6",
          "form": "좋다 ㅎㅎ",
          "original_form": "좋다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 13:01"
        },
        {
          "id": "MDRW2100041610.1.7",
          "form": "얍얍 언니 바쁜가보다 ㅋ",
          "original_form": "얍얍 언니 바쁜가보다 ㅋ",
          "speaker_id": "1",
          "time": "20210906 13:02"
        },
        {
          "id": "MDRW2100041610.1.8",
          "form": "언니 나 밥 먹고 톡할게 ㅋ",
          "original_form": "언니 나 밥 먹고 톡할게 ㅋ",
          "speaker_id": "1",
          "time": "20210906 13:02"
        },
        {
          "id": "MDRW2100041610.1.9",
          "form": "옹 언니 공부한다고 바쁜갑다",
          "original_form": "옹 언니 공부한다고 바쁜갑다",
          "speaker_id": "2",
          "time": "20210906 13:02"
        },
        {
          "id": "MDRW2100041610.1.10",
          "form": "그랴 맛있게 무\r\nㅎㅎ",
          "original_form": "그랴 맛있게 무\r\nㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 13:02"
        },
        {
          "id": "MDRW2100041610.1.11",
          "form": "알겠어이 ㅋㅋ",
          "original_form": "알겠어이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 13:02"
        },
        {
          "id": "MDRW2100041610.1.12",
          "form": "먹었나??",
          "original_form": "먹었나??",
          "speaker_id": "2",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100041610.1.13",
          "form": "응 먹었당 ㅋㅋ 배부르다잉",
          "original_form": "응 먹었당 ㅋㅋ 배부르다잉",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100041610.1.14",
          "form": "핑퐁 커피 address 앞에 새로 생겼는데",
          "original_form": "핑퐁 커피 &address& 앞에 새로 생겼는데",
          "speaker_id": "2",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100041610.1.15",
          "form": "핑퐁 라떼라고 맛있어",
          "original_form": "핑퐁 라떼라고 맛있어",
          "speaker_id": "2",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100041610.1.16",
          "form": "그럼 내일 세 개만 사서 가까? 아이스로??",
          "original_form": "그럼 내일 세 개만 사서 가까? 아이스로??",
          "speaker_id": "2",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100041610.1.17",
          "form": "응응 아이스로",
          "original_form": "응응 아이스로",
          "speaker_id": "1",
          "time": "20210906 14:27"
        },
        {
          "id": "MDRW2100041610.1.18",
          "form": "엉 알았음 ㅎㅎ",
          "original_form": "엉 알았음 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100041610.1.19",
          "form": "오늘 저녁 뭐 먹지??",
          "original_form": "오늘 저녁 뭐 먹지??",
          "speaker_id": "2",
          "time": "20210906 14:28"
        },
        {
          "id": "MDRW2100041610.1.20",
          "form": "오늘 저녁 ㅋㅋ 나는 곰국",
          "original_form": "오늘 저녁 ㅋㅋ 나는 곰국",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100041610.1.21",
          "form": "콩나물무침에 두부조림을 하려구 ㅋㅋ",
          "original_form": "콩나물무침에 두부조림을 하려구 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100041610.1.22",
          "form": "요리 잘하네ㅋㅋㅋㅋㅋ",
          "original_form": "요리 잘하네ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100041610.1.23",
          "form": "두부조림도 하고ㅋㅋㅋ",
          "original_form": "두부조림도 하고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 14:29"
        },
        {
          "id": "MDRW2100041610.1.24",
