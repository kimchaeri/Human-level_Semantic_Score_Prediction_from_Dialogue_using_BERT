      "utterance": [
        {
          "id": "MDRW2100009554.1.1",
          "form": "담배를 끊어야되는데...",
          "original_form": "담배를 끊어야되는데...",
          "speaker_id": "1",
          "time": "20210823 00:32"
        },
        {
          "id": "MDRW2100009554.1.2",
          "form": "와 애국자시넹ㆍ",
          "original_form": "와 애국자시넹ㆍ",
          "speaker_id": "2",
          "time": "20210823 00:33"
        },
        {
          "id": "MDRW2100009554.1.3",
          "form": "와 애국자시네요",
          "original_form": "와 애국자시네요",
          "speaker_id": "2",
          "time": "20210823 00:34"
        },
        {
          "id": "MDRW2100009554.1.4",
          "form": "뭔가 저의 다이어트 같은건가요",
          "original_form": "뭔가 저의 다이어트 같은건가요",
          "speaker_id": "2",
          "time": "20210823 00:34"
        },
        {
          "id": "MDRW2100009554.1.5",
          "form": "다이어트는 평생의 목표지만",
          "original_form": "다이어트는 평생의 목표지만",
          "speaker_id": "1",
          "time": "20210823 00:35"
        },
        {
          "id": "MDRW2100009554.1.6",
          "form": "저는 담배는 끊을 자신이 없어요ㅜ",
          "original_form": "저는 담배는 끊을 자신이 없어요ㅜ",
          "speaker_id": "1",
          "time": "20210823 00:35"
        },
        {
          "id": "MDRW2100009554.1.7",
          "form": "건강이 안 좋아지는 거 같은 기분ㅜ",
          "original_form": "건강이 안 좋아지는 거 같은 기분ㅜ",
          "speaker_id": "1",
          "time": "20210823 00:36"
        },
        {
          "id": "MDRW2100009554.1.8",
          "form": "음 종류를 바꿔보시거나 줄여보시는건요",
          "original_form": "음 종류를 바꿔보시거나 줄여보시는건요",
          "speaker_id": "2",
          "time": "20210823 00:36"
        },
        {
          "id": "MDRW2100009554.1.9",
          "form": "전담이긴 한데 건강에 안 좋긴 매한가지겠죠",
          "original_form": "전담이긴 한데 건강에 안 좋긴 매한가지겠죠",
          "speaker_id": "1",
          "time": "20210823 00:37"
        },
        {
          "id": "MDRW2100009554.1.10",
          "form": "금연캠프 같은건 오바일까요",
          "original_form": "금연캠프 같은건 오바일까요",
          "speaker_id": "2",
          "time": "20210823 00:38"
        },
        {
          "id": "MDRW2100009554.1.11",
          "form": "오늘 현수막 본거 같은데",
          "original_form": "오늘 현수막 본거 같은데",
          "speaker_id": "2",
          "time": "20210823 00:38"
        },
        {
          "id": "MDRW2100009554.1.12",
          "form": "안 할래요ㅋ 그냥 건강에 자신이 없어졌어요ㅋ 30대 오면서ㅋ",
          "original_form": "안 할래요ㅋ 그냥 건강에 자신이 없어졌어요ㅋ 30대 오면서ㅋ",
          "speaker_id": "1",
          "time": "20210823 00:39"
        },
        {
          "id": "MDRW2100009554.1.13",
          "form": "음 그것도 개인의 선택이죠",
          "original_form": "음 그것도 개인의 선택이죠",
          "speaker_id": "2",
          "time": "20210823 00:40"
        },
        {
          "id": "MDRW2100009554.1.14",
          "form": "다이어트는 잘 되시나요?",
          "original_form": "다이어트는 잘 되시나요?",
          "speaker_id": "1",
          "time": "20210823 00:41"
        },
        {
          "id": "MDRW2100009554.1.15",
          "form": "근데 나이 더먹고 길게 오래 돈많이 쓰면서 괴로울까봐 걱정이초",
          "original_form": "근데 나이 더먹고 길게 오래 돈많이 쓰면서 괴로울까봐 걱정이초",
          "speaker_id": "2",
          "time": "20210823 00:41"
        },
        {
          "id": "MDRW2100009554.1.16",
          "form": "걱정이쵸",
          "original_form": "걱정이쵸",
          "speaker_id": "2",
          "time": "20210823 00:41"
        },
        {
          "id": "MDRW2100009554.1.17",
          "form": "그렇죠 건강 걱정이 돈 걱정이죠 결국ㅜ",
          "original_form": "그렇죠 건강 걱정이 돈 걱정이죠 결국ㅜ",
          "speaker_id": "1",
          "time": "20210823 00:41"
        },
        {
          "id": "MDRW2100009554.1.18",
          "form": "아프게 오래사는 것도 지옥이고",
          "original_form": "아프게 오래사는 것도 지옥이고",
          "speaker_id": "1",
          "time": "20210823 00:42"
        },
        {
          "id": "MDRW2100009554.1.19",
          "form": "그니까요ㅋㅋ난시원시원하게 하고픈거 다하고 딱 60에 시원하게 가게 해주슈 노고통 온니 자연사로 오케이?",
          "original_form": "그니까요ㅋㅋ난시원시원하게 하고픈거 다하고 딱 60에 시원하게 가게 해주슈 노고통 온니 자연사로 오케이?",
          "speaker_id": "2",
          "time": "20210823 00:44"
        },
        {
          "id": "MDRW2100009554.1.20",
          "form": "이게안되니까요 허헛",
          "original_form": "이게안되니까요 허헛",
          "speaker_id": "2",
          "time": "20210823 00:44"
        },
        {
          "id": "MDRW2100009554.1.21",
          "form": "저도 그런마인드였는데.. 나이는 칠십ㅋ",
          "original_form": "저도 그런마인드였는데.. 나이는 칠십ㅋ",
          "speaker_id": "1",
          "time": "20210823 07:27"
        },
        {
          "id": "MDRW2100009554.1.22",
          "form": "생각하는게 비슷하신듯해요",
          "original_form": "생각하는게 비슷하신듯해요",
          "speaker_id": "1",
          "time": "20210823 07:27"
        },
        {
          "id": "MDRW2100009554.1.23",
          "form": "사실 늙어서 고생 안하려고 다들 건강 관리하시는듯해요ㅜ",
          "original_form": "사실 늙어서 고생 안하려고 다들 건강 관리하시는듯해요ㅜ",
          "speaker_id": "2",
          "time": "20210823 09:44"
        },
        {
          "id": "MDRW2100009554.1.24",
          "form": "ㅋㅋ휴가끝나고 출근했더니 정신없네요ㅜ",
          "original_form": "ㅋㅋ휴가끝나고 출근했더니 정신없네요ㅜ",
          "speaker_id": "1",
          "time": "20210823 09:55"
        }
      ]
    }
  ]
