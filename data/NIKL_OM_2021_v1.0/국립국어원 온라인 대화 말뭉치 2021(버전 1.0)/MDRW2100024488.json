      "utterance": [
        {
          "id": "MDRW2100024488.1.1",
          "form": "명절 두번 여름휴가 연말 이렇게 4번",
          "original_form": "명절 두번 여름휴가 연말 이렇게 4번",
          "speaker_id": "2",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100024488.1.2",
          "form": "상여 4번이요? 명절2번에 휴가비 2번인가",
          "original_form": "상여 4번이요? 명절2번에 휴가비 2번인가",
          "speaker_id": "1",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100024488.1.3",
          "form": "오,,,그래도 백씩은 주나봐요",
          "original_form": "오,,,그래도 백씩은 주나봐요",
          "speaker_id": "1",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100024488.1.4",
          "form": "직급마다 다른가요?",
          "original_form": "직급마다 다른가요?",
          "speaker_id": "1",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100024488.1.5",
          "form": "그리고 추가로 가끔씩 더 나오기도 하는데",
          "original_form": "그리고 추가로 가끔씩 더 나오기도 하는데",
          "speaker_id": "2",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100024488.1.6",
          "form": "음... 기본급에 따라 다르죠",
          "original_form": "음... 기본급에 따라 다르죠",
          "speaker_id": "2",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100024488.1.7",
          "form": "기본급에 30프로 지급합니다~ 이런식",
          "original_form": "기본급에 30프로 지급합니다~ 이런식",
          "speaker_id": "2",
          "time": "20210831 11:31"
        },
        {
          "id": "MDRW2100024488.1.8",
          "form": "연봉이 억이겟는데요",
          "original_form": "연봉이 억이겟는데요",
          "speaker_id": "1",
          "time": "20210831 11:32"
        },
        {
          "id": "MDRW2100024488.1.9",
          "form": "억이면 흙수저 드립도 안치죠",
          "original_form": "억이면 흙수저 드립도 안치죠",
          "speaker_id": "2",
          "time": "20210831 11:32"
        },
        {
          "id": "MDRW2100024488.1.10",
          "form": "요번엔 바빴다고 휴가전에 한번 더 주심",
          "original_form": "요번엔 바빴다고 휴가전에 한번 더 주심",
          "speaker_id": "2",
          "time": "20210831 11:32"
        },
        {
          "id": "MDRW2100024488.1.11",
          "form": "좀",
          "original_form": "좀",
          "speaker_id": "2",
          "time": "20210831 11:32"
        },
        {
          "id": "MDRW2100024488.1.12",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210831 11:32"
        },
        {
          "id": "MDRW2100024488.1.13",
          "form": "좀 세.게",
          "original_form": "좀 세.게",
          "speaker_id": "2",
          "time": "20210831 11:32"
        },
        {
          "id": "MDRW2100024488.1.14",
          "form": "8년 다니면서 세번짼가ㅋㅋ +@는",
          "original_form": "8년 다니면서 세번짼가ㅋㅋ +@는",
          "speaker_id": "2",
          "time": "20210831 11:33"
        },
        {
          "id": "MDRW2100024488.1.15",
          "form": "뭐 암튼 그래요",
          "original_form": "뭐 암튼 그래요",
          "speaker_id": "2",
          "time": "20210831 11:33"
        },
        {
          "id": "MDRW2100024488.1.16",
          "form": "보통 오래 다니면 기본급이 높으니",
          "original_form": "보통 오래 다니면 기본급이 높으니",
          "speaker_id": "2",
          "time": "20210831 11:33"
        },
        {
          "id": "MDRW2100024488.1.17",
          "form": "더 많이 받아가죠 아무래도",
          "original_form": "더 많이 받아가죠 아무래도",
          "speaker_id": "2",
          "time": "20210831 11:34"
        },
        {
          "id": "MDRW2100024488.1.18",
          "form": "부럽다,,,",
          "original_form": "부럽다,,,",
          "speaker_id": "1",
          "time": "20210831 11:34"
        },
        {
          "id": "MDRW2100024488.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:34"
        },
        {
          "id": "MDRW2100024488.1.20",
          "form": "저는 언제쯤 그렇게 받아볼까요",
          "original_form": "저는 언제쯤 그렇게 받아볼까요",
          "speaker_id": "1",
          "time": "20210831 11:34"
        },
        {
          "id": "MDRW2100024488.1.21",
          "form": "직업자체가 돈을 많이벌수잇는직업이 아니라 ㅋㅋㅋㅋㅋ",
          "original_form": "직업자체가 돈을 많이벌수잇는직업이 아니라 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:34"
        },
        {
          "id": "MDRW2100024488.1.22",
          "form": "고생은 고생대로 한다던데...",
          "original_form": "고생은 고생대로 한다던데...",
          "speaker_id": "2",
          "time": "20210831 11:35"
        },
        {
          "id": "MDRW2100024488.1.23",
          "form": "국가 지원도 없죠?...",
          "original_form": "국가 지원도 없죠?...",
          "speaker_id": "2",
          "time": "20210831 11:35"
        },
        {
          "id": "MDRW2100024488.1.24",
          "form": "뭔지원이요?",
          "original_form": "뭔지원이요?",
          "speaker_id": "1",
          "time": "20210831 11:35"
        },
        {
          "id": "MDRW2100024488.1.25",
          "form": "예를들면 중소기업 청년지원 이런거",
          "original_form": "예를들면 중소기업 청년지원 이런거",
          "speaker_id": "2",
          "time": "20210831 11:35"
        },
        {
          "id": "MDRW2100024488.1.26",
          "form": "국가지원은 뭐말하는거지",
          "original_form": "국가지원은 뭐말하는거지",
          "speaker_id": "1",
          "time": "20210831 11:35"
        },
        {
          "id": "MDRW2100024488.1.27",
          "form": "없어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "없어요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:35"
        },
        {
          "id": "MDRW2100024488.1.28",
          "form": "중소기업 다니는친구들은 적금? 그거많이하던데",
          "original_form": "중소기업 다니는친구들은 적금? 그거많이하던데",
          "speaker_id": "1",
          "time": "20210831 11:36"
        },
        {
          "id": "MDRW2100024488.1.29",
          "form": "요새 애들 다 하는",
          "original_form": "요새 애들 다 하는",
          "speaker_id": "2",
          "time": "20210831 11:36"
        },
        {
          "id": "MDRW2100024488.1.30",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210831 11:36"
        },
        {
          "id": "MDRW2100024488.1.31",
          "form": "그거 꿀인데",
          "original_form": "그거 꿀인데",
          "speaker_id": "2",
          "time": "20210831 11:36"
        },
        {
          "id": "MDRW2100024488.1.32",
          "form": "ㅠ 중소기업으로 안들어가요",
          "original_form": "ㅠ 중소기업으로 안들어가요",
          "speaker_id": "1",
          "time": "20210831 11:36"
        },
        {
          "id": "MDRW2100024488.1.33",
          "form": "국가보조금으로 운영되는곳이라 그런가 그런건 하나도없음",
          "original_form": "국가보조금으로 운영되는곳이라 그런가 그런건 하나도없음",
          "speaker_id": "1",
          "time": "20210831 11:36"
        },
        {
          "id": "MDRW2100024488.1.34",
          "form": "교통비도 지원해주고 그러는데...",
          "original_form": "교통비도 지원해주고 그러는데...",
          "speaker_id": "2",
          "time": "20210831 11:36"
        },
        {
          "id": "MDRW2100024488.1.35",
          "form": "부럽긴하드라구여 ㅋㅋㅋㅋ 그래서어제 대기업에 사회복지사 뽑는지 검색해봤는데",
          "original_form": "부럽긴하드라구여 ㅋㅋㅋㅋ 그래서어제 대기업에 사회복지사 뽑는지 검색해봤는데",
          "speaker_id": "1",
          "time": "20210831 11:37"
        },
        {
          "id": "MDRW2100024488.1.36",
          "form": "도둑놈.들이 많아가지고 ㅡㅡ",
          "original_form": "도둑놈.들이 많아가지고 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210831 11:37"
        },
        {
          "id": "MDRW2100024488.1.37",
          "form": "삼성 어린이집에서 뽑길래",
          "original_form": "삼성 어린이집에서 뽑길래",
          "speaker_id": "1",
          "time": "20210831 11:37"
        },
        {
          "id": "MDRW2100024488.1.38",
          "form": "국회의원이 8천 이렇게 받아가는게 이해가 안댐",
          "original_form": "국회의원이 8천 이렇게 받아가는게 이해가 안댐",
          "speaker_id": "2",
          "time": "20210831 11:37"
        },
        {
          "id": "MDRW2100024488.1.39",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210831 11:37"
        },
        {
          "id": "MDRW2100024488.1.40",
          "form": "근데 또 그런데가기가~~ㅋㅋㅋㅋㅋㅋ",
          "original_form": "근데 또 그런데가기가~~ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:38"
        },
        {
          "id": "MDRW2100024488.1.41",
          "form": "지금이렇게 편히일하는데",
          "original_form": "지금이렇게 편히일하는데",
          "speaker_id": "1",
          "time": "20210831 11:38"
        },
        {
          "id": "MDRW2100024488.1.42",
          "form": "돈이 달라지면 마음이 바뀌실수도..",
          "original_form": "돈이 달라지면 마음이 바뀌실수도..",
          "speaker_id": "2",
          "time": "20210831 11:38"
        },
        {
          "id": "MDRW2100024488.1.43",
          "form": "그런곳가면 진ㄴ짜 개고생할듯요",
          "original_form": "그런곳가면 진ㄴ짜 개고생할듯요",
          "speaker_id": "1",
          "time": "20210831 11:38"
        },
        {
          "id": "MDRW2100024488.1.44",
          "form": "돈을 많이주는이유가 잇을거같은데 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "돈을 많이주는이유가 잇을거같은데 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:39"
        },
        {
          "id": "MDRW2100024488.1.45",
          "form": "대기업은 진짜 지원해봐요",
          "original_form": "대기업은 진짜 지원해봐요",
          "speaker_id": "2",
          "time": "20210831 11:39"
        },
        {
          "id": "MDRW2100024488.1.46",
          "form": "나중엔 가고 싶어도 못감",
          "original_form": "나중엔 가고 싶어도 못감",
          "speaker_id": "2",
          "time": "20210831 11:39"
        },
        {
          "id": "MDRW2100024488.1.47",
          "form": "아직 조건이 안됩니다",
          "original_form": "아직 조건이 안됩니다",
          "speaker_id": "1",
          "time": "20210831 11:39"
        },
        {
          "id": "MDRW2100024488.1.48",
          "form": "아?",
          "original_form": "아?",
          "speaker_id": "2",
          "time": "20210831 11:39"
        },
        {
          "id": "MDRW2100024488.1.49",
          "form": "공부를 더해야함",
          "original_form": "공부를 더해야함",
          "speaker_id": "1",
          "time": "20210831 11:39"
        },
        {
          "id": "MDRW2100024488.1.50",
          "form": "일하고와서 펜 잡는거 빡신뎅..",
          "original_form": "일하고와서 펜 잡는거 빡신뎅..",
          "speaker_id": "2",
          "time": "20210831 11:40"
        },
        {
          "id": "MDRW2100024488.1.51",
          "form": "합격률도 30프로라",
          "original_form": "합격률도 30프로라",
          "speaker_id": "1",
          "time": "20210831 11:43"
        },
        {
          "id": "MDRW2100024488.1.52",
          "form": "기대도안함 ㅋㅋㅋㅋ",
          "original_form": "기대도안함 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:43"
        },
        {
          "id": "MDRW2100024488.1.53",
          "form": "기사에요? 뭐이리 빡셈",
          "original_form": "기사에요? 뭐이리 빡셈",
          "speaker_id": "2",
          "time": "20210831 11:44"
        },
        {
          "id": "MDRW2100024488.1.54",
          "form": "과락도있고 ㅋㅋㅋㅋ",
          "original_form": "과락도있고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 11:46"
        },
        {
          "id": "MDRW2100024488.1.55",
