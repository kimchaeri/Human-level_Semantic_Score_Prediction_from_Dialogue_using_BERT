      "utterance": [
        {
          "id": "MDRW2100001936.1.1",
          "form": "가장 최근에 구매한 물건은 무엇인가요?",
          "original_form": "가장 최근에 구매한 물건은 무엇인가요?",
          "speaker_id": "1",
          "time": "20210819 22:21"
        },
        {
          "id": "MDRW2100001936.1.2",
          "form": "노트북이요ㅎㅎ",
          "original_form": "노트북이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001936.1.3",
          "form": "헐 저돜ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "헐 저돜ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001936.1.4",
          "form": "쿠팡에서 카드할인 잔뜩하길래 노트북 샀어요",
          "original_form": "쿠팡에서 카드할인 잔뜩하길래 노트북 샀어요",
          "speaker_id": "1",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001936.1.5",
          "form": "다음에뭐 또 구매하실 계획잇으세요?",
          "original_form": "다음에뭐 또 구매하실 계획잇으세요?",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001936.1.6",
          "form": "저 갤럭시 왓치 사고 싶어요ㅠㅜ 친구들이 사는 거 보니까 엄청 좋아 보이더라고요ㅋㅋㅋ",
          "original_form": "저 갤럭시 왓치 사고 싶어요ㅠㅜ 친구들이 사는 거 보니까 엄청 좋아 보이더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001936.1.7",
          "form": "아 그거 좋아보이더라구요",
          "original_form": "아 그거 좋아보이더라구요",
          "speaker_id": "1",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001936.1.8",
          "form": "저는 플립이 넘 갖고싶어요",
          "original_form": "저는 플립이 넘 갖고싶어요",
          "speaker_id": "1",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001936.1.9",
          "form": "근데 모든게 다 애플제품이라ㅜ 바꾸기가 쉽지않네요",
          "original_form": "근데 모든게 다 애플제품이라ㅜ 바꾸기가 쉽지않네요",
          "speaker_id": "1",
          "time": "20210819 22:44"
        },
        {
          "id": "MDRW2100001936.1.10",
          "form": "맞아요ㅠㅠ이번에 진짜 플립 너무 이쁘더라고요ㅠㅜㅜ",
          "original_form": "맞아요ㅠㅠ이번에 진짜 플립 너무 이쁘더라고요ㅠㅜㅜ",
          "speaker_id": "2",
          "time": "20210819 22:48"
        },
        {
          "id": "MDRW2100001936.1.11",
          "form": "맞아요 애플서 갤럭시로 바꾸기는 쉽지 않죠ㅠ",
          "original_form": "맞아요 애플서 갤럭시로 바꾸기는 쉽지 않죠ㅠ",
          "speaker_id": "2",
          "time": "20210819 22:49"
        },
        {
          "id": "MDRW2100001936.1.12",
          "form": "지금 5만원 이내로 선물을 받을 수 있다면 받고 싶은 선물 있으새요?",
          "original_form": "지금 5만원 이내로 선물을 받을 수 있다면 받고 싶은 선물 있으새요?",
          "speaker_id": "1",
          "time": "20210819 22:51"
        },
        {
          "id": "MDRW2100001936.1.13",
          "form": "5만원이요?? 5만원은 아니지만ㅋㅋㅋ저 요새 버즈 사고 싶어요ㅠㅜ 유선 이어폰 쓰는데 불편하더라고요ㅋㅋㅋ",
          "original_form": "5만원이요?? 5만원은 아니지만ㅋㅋㅋ저 요새 버즈 사고 싶어요ㅠㅜ 유선 이어폰 쓰는데 불편하더라고요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:52"
        },
        {
          "id": "MDRW2100001936.1.14",
          "form": "와 아직까지 유선 이어폰 쓰는 멋진 분이 계시네요",
          "original_form": "와 아직까지 유선 이어폰 쓰는 멋진 분이 계시네요",
          "speaker_id": "1",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100001936.1.15",
          "form": "버즈 어떤걸로요?",
          "original_form": "버즈 어떤걸로요?",
          "speaker_id": "1",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100001936.1.16",
          "form": "콩같이 생긴거??",
          "original_form": "콩같이 생긴거??",
          "speaker_id": "1",
          "time": "20210819 22:54"
        },
        {
          "id": "MDRW2100001936.1.17",
          "form": "예 제가 바로 그 멋진 여성이죠ㅋㅋㅋ",
          "original_form": "예 제가 바로 그 멋진 여성이죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 22:55"
        },
        {
          "id": "MDRW2100001936.1.18",
          "form": "버즈 프로로요!",
          "original_form": "버즈 프로로요!",
          "speaker_id": "2",
          "time": "20210819 22:55"
        },
        {
          "id": "MDRW2100001936.1.19",
          "form": "근데 편하긴 한데 충전 안돼잇을때마다",
          "original_form": "근데 편하긴 한데 충전 안돼잇을때마다",
          "speaker_id": "1",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100001936.1.20",
          "form": "극대노 하게되더라고요",
          "original_form": "극대노 하게되더라고요",
          "speaker_id": "1",
          "time": "20210819 22:56"
        },
        {
          "id": "MDRW2100001936.1.21",
          "form": "가방에 유선 꼭 챙기게되는 나날들..",
          "original_form": "가방에 유선 꼭 챙기게되는 나날들..",
          "speaker_id": "1",
          "time": "20210819 22:57"
        },
        {
          "id": "MDRW2100001936.1.22",
          "form": "오 무선이어폰 사용하세요?",
          "original_form": "오 무선이어폰 사용하세요?",
          "speaker_id": "2",
          "time": "20210819 22:57"
        },
        {
          "id": "MDRW2100001936.1.23",
          "form": "저 요새 무선이어폰이 너무 사고 싶어요ㅠㅜ",
          "original_form": "저 요새 무선이어폰이 너무 사고 싶어요ㅠㅜ",
          "speaker_id": "2",
          "time": "20210819 22:57"
        },
        {
          "id": "MDRW2100001936.1.24",
          "form": "저는 에어팟 프로써요! 노이즈 캔슬링은 짱인데",
          "original_form": "저는 에어팟 프로써요! 노이즈 캔슬링은 짱인데",
          "speaker_id": "1",
          "time": "20210819 22:59"
        },
        {
          "id": "MDRW2100001936.1.25",
          "form": "충전이 진짜 귀찮아요",
          "original_form": "충전이 진짜 귀찮아요",
          "speaker_id": "1",
          "time": "20210819 22:59"
        }
      ]
