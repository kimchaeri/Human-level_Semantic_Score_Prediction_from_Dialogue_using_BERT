      "utterance": [
        {
          "id": "MDRW2100038065.1.1",
          "form": "반려동물 있으세요?",
          "original_form": "반려동물 있으세요?",
          "speaker_id": "1",
          "time": "20210901 22:55"
        },
        {
          "id": "MDRW2100038065.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210901 22:56"
        },
        {
          "id": "MDRW2100038065.1.3",
          "form": "전 금붕어 키워요",
          "original_form": "전 금붕어 키워요",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038065.1.4",
          "form": "귀요미에요 이름이",
          "original_form": "귀요미에요 이름이",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038065.1.5",
          "form": "ㅎㅎ 귀엽네요",
          "original_form": "ㅎㅎ 귀엽네요",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038065.1.6",
          "form": "손 많리 안가요?",
          "original_form": "손 많리 안가요?",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038065.1.7",
          "form": "네네 금붕어는 키우기 쉬워요",
          "original_form": "네네 금붕어는 키우기 쉬워요",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038065.1.8",
          "form": "울애가 지은 이름이에욬ㅋㅋㅋ",
          "original_form": "울애가 지은 이름이에욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100038065.1.9",
          "form": "어멋 ㅎㅎㅎ 더 귀엽네요 그럼 ㅋ",
          "original_form": "어멋 ㅎㅎㅎ 더 귀엽네요 그럼 ㅋ",
          "speaker_id": "2",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100038065.1.10",
          "form": "물멍하는 재미가 있어요 물고기보고있음 좋아요",
          "original_form": "물멍하는 재미가 있어요 물고기보고있음 좋아요",
          "speaker_id": "1",
          "time": "20210901 23:02"
        },
        {
          "id": "MDRW2100038065.1.11",
          "form": "그래서 반려동물 키우나봐요",
          "original_form": "그래서 반려동물 키우나봐요",
          "speaker_id": "1",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100038065.1.12",
          "form": "물멍  해보고 싳네요",
          "original_form": "물멍  해보고 싳네요",
          "speaker_id": "2",
          "time": "20210901 23:19"
        },
        {
          "id": "MDRW2100038065.1.13",
          "form": "네네.전 아쿠아리움가는거 좋아해요 스노우쿨링 도",
          "original_form": "네네.전 아쿠아리움가는거 좋아해요 스노우쿨링 도",
          "speaker_id": "1",
          "time": "20210901 23:20"
        },
        {
          "id": "MDRW2100038065.1.14",
          "form": "강아지 알러지 있으세요?",
          "original_form": "강아지 알러지 있으세요?",
          "speaker_id": "1",
          "time": "20210902 15:23"
        },
        {
          "id": "MDRW2100038065.1.15",
          "form": "아뇨 없어여",
          "original_form": "아뇨 없어여",
          "speaker_id": "2",
          "time": "20210902 21:08"
        },
        {
          "id": "MDRW2100038065.1.16",
          "form": "반려동물 키워본적없으세요?",
          "original_form": "반려동물 키워본적없으세요?",
          "speaker_id": "1",
          "time": "20210902 22:20"
        },
        {
          "id": "MDRW2100038065.1.17",
          "form": "네 강아지요",
          "original_form": "네 강아지요",
          "speaker_id": "2",
          "time": "20210902 22:26"
        },
        {
          "id": "MDRW2100038065.1.18",
          "form": "오 무슨 종이요?",
          "original_form": "오 무슨 종이요?",
