      "utterance": [
        {
          "id": "MDRW2100042731.1.1",
          "form": "또 비네ㅡㅡ;;;",
          "original_form": "또 비네ㅡㅡ;;;",
          "speaker_id": "2",
          "time": "20210907 08:39"
        },
        {
          "id": "MDRW2100042731.1.2",
          "form": "아들 출동했어요",
          "original_form": "아들 출동했어요",
          "speaker_id": "1",
          "time": "20210907 08:39"
        },
        {
          "id": "MDRW2100042731.1.3",
          "form": "난 그래도 기부니가 좋다",
          "original_form": "난 그래도 기부니가 좋다",
          "speaker_id": "2",
          "time": "20210907 08:39"
        },
        {
          "id": "MDRW2100042731.1.4",
          "form": "밖에 또 비 와요??",
          "original_form": "밖에 또 비 와요??",
          "speaker_id": "1",
          "time": "20210907 08:39"
        },
        {
          "id": "MDRW2100042731.1.5",
          "form": "휴무라ㅋ",
          "original_form": "휴무라ㅋ",
          "speaker_id": "2",
          "time": "20210907 08:39"
        },
        {
          "id": "MDRW2100042731.1.6",
          "form": "오늘 놀러 가서ㅋㅋㅋ",
          "original_form": "오늘 놀러 가서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:39"
        },
        {
          "id": "MDRW2100042731.1.7",
          "form": "촉촉한데?",
          "original_form": "촉촉한데?",
          "speaker_id": "2",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100042731.1.8",
          "form": "밖이",
          "original_form": "밖이",
          "speaker_id": "2",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100042731.1.9",
          "form": "나 아까 나갔다 왔는데 비 안오던데??",
          "original_form": "나 아까 나갔다 왔는데 비 안오던데??",
          "speaker_id": "1",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100042731.1.10",
          "form": "새벽에 온 거 아니에요ㅋㅋㅋ",
          "original_form": "새벽에 온 거 아니에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100042731.1.11",
          "form": "그렇군ㅋㅋ",
          "original_form": "그렇군ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100042731.1.12",
          "form": "넌 정말부지런하구나",
          "original_form": "넌 정말부지런하구나",
          "speaker_id": "2",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100042731.1.13",
          "form": "밖에도나갔다오고ㅋ",
          "original_form": "밖에도나갔다오고ㅋ",
          "speaker_id": "2",
          "time": "20210907 08:40"
        },
        {
          "id": "MDRW2100042731.1.14",
          "form": "난 둘 다 보냈어요ㅋㅋㅋㅋ",
          "original_form": "난 둘 다 보냈어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:41"
        },
        {
          "id": "MDRW2100042731.1.15",
          "form": "둘째는 얼 집 차는 태워 보내줘야죠",
          "original_form": "둘째는 얼 집 차는 태워 보내줘야죠",
          "speaker_id": "1",
          "time": "20210907 08:41"
        },
        {
          "id": "MDRW2100042731.1.16",
          "form": "아하ㅋㅋ",
          "original_form": "아하ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:41"
        },
        {
          "id": "MDRW2100042731.1.17",
          "form": "그래 쏴리ㅋㅋ",
          "original_form": "그래 쏴리ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:41"
        },
        {
          "id": "MDRW2100042731.1.18",
          "form": "안 그럼 언니 안 나가요 나도ㅋㅋㅋ",
          "original_form": "안 그럼 언니 안 나가요 나도ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 08:41"
        },
        {
          "id": "MDRW2100042731.1.19",
          "form": "ㅋㅋㅋ 내가 노란차랑 안녕하고나서 삶의질이 더 올라갔다ㅋ",
          "original_form": "ㅋㅋㅋ 내가 노란차랑 안녕하고나서 삶의질이 더 올라갔다ㅋ",
          "speaker_id": "2",
          "time": "20210907 08:41"
        },
        {
          "id": "MDRW2100042731.1.20",
          "form": "데리러가고 데리러오고ㅋㅋ",
          "original_form": "데리러가고 데리러오고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 08:41"
        },
        {
          "id": "MDRW2100042731.1.21",
          "form": "맞죠 기다리는 시간도 막",
          "original_form": "맞죠 기다리는 시간도 막",
          "speaker_id": "1",
          "time": "20210907 08:42"
        },
        {
          "id": "MDRW2100042731.1.22",
          "form": "기다리는 그 시간이 너무 아까워요~",
          "original_form": "기다리는 그 시간이 너무 아까워요~",
          "speaker_id": "1",
          "time": "20210907 08:42"
        }
      ]
