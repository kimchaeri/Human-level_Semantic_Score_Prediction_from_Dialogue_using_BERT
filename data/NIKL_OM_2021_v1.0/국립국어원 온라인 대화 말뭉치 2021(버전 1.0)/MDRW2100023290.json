      "utterance": [
        {
          "id": "MDRW2100023290.1.1",
          "form": "혹시 다이어트 하세요?",
          "original_form": "혹시 다이어트 하세요?",
          "speaker_id": "1",
          "time": "20210829 23:43"
        },
        {
          "id": "MDRW2100023290.1.2",
          "form": "다이어트 하시나요?",
          "original_form": "다이어트 하시나요?",
          "speaker_id": "1",
          "time": "20210829 23:43"
        },
        {
          "id": "MDRW2100023290.1.3",
          "form": "네하고있어여있",
          "original_form": "네하고있어여있",
          "speaker_id": "2",
          "time": "20210829 23:43"
        },
        {
          "id": "MDRW2100023290.1.4",
          "form": "지금 5키로뺐어요",
          "original_form": "지금 5키로뺐어요",
          "speaker_id": "2",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100023290.1.5",
          "form": "다이어트하세요?",
          "original_form": "다이어트하세요?",
          "speaker_id": "2",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100023290.1.6",
          "form": "오 대단하세요",
          "original_form": "오 대단하세요",
          "speaker_id": "1",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100023290.1.7",
          "form": "전 맨날 입다이어터예요ㅜ",
          "original_form": "전 맨날 입다이어터예요ㅜ",
          "speaker_id": "1",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100023290.1.8",
          "form": "지방흡입도 했는데 할게못됩니다",
          "original_form": "지방흡입도 했는데 할게못됩니다",
          "speaker_id": "2",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100023290.1.9",
          "form": "무슨 운동하세요?",
          "original_form": "무슨 운동하세요?",
          "speaker_id": "1",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100023290.1.10",
          "form": "많이아파요ㅠㅠ",
          "original_form": "많이아파요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100023290.1.11",
          "form": "저는 강아지랑산책다니고 홈트해요",
          "original_form": "저는 강아지랑산책다니고 홈트해요",
          "speaker_id": "2",
          "time": "20210829 23:44"
        },
        {
          "id": "MDRW2100023290.1.12",
          "form": "홈트 좋죠",
          "original_form": "홈트 좋죠",
          "speaker_id": "1",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023290.1.13",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "2",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023290.1.14",
          "form": "유튜브로보고하고있어요",
          "original_form": "유튜브로보고하고있어요",
          "speaker_id": "2",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023290.1.15",
          "form": "네저도 저녁에",
          "original_form": "네저도 저녁에",
          "speaker_id": "1",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023290.1.16",
          "form": "조금씩 나가서 걸어요",
          "original_form": "조금씩 나가서 걸어요",
          "speaker_id": "1",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023290.1.17",
          "form": "빨리걷기정도 ㅎㅎ",
          "original_form": "빨리걷기정도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023290.1.18",
          "form": "좋네요감량하셨어요?",
          "original_form": "좋네요감량하셨어요?",
          "speaker_id": "2",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023290.1.19",
          "form": "다이어트 관한음식도 해드시나요?",
          "original_form": "다이어트 관한음식도 해드시나요?",
          "speaker_id": "2",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023290.1.20",
          "form": "아뇨 감량은 안되고 현상유지중이예요ㅜ",
          "original_form": "아뇨 감량은 안되고 현상유지중이예요ㅜ",
          "speaker_id": "1",
          "time": "20210829 23:45"
        },
        {
          "id": "MDRW2100023290.1.21",
          "form": "음식은 그냥 먹고",
          "original_form": "음식은 그냥 먹고",
          "speaker_id": "1",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100023290.1.22",
          "form": "야식은 안먹어요",
          "original_form": "야식은 안먹어요",
          "speaker_id": "1",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100023290.1.23",
          "form": "음식도 해드세요?",
          "original_form": "음식도 해드세요?",
          "speaker_id": "1",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100023290.1.24",
          "form": "체력이좋아지면되는거아닐까요 건강하려고운동하는거잖아요",
          "original_form": "체력이좋아지면되는거아닐까요 건강하려고운동하는거잖아요",
          "speaker_id": "2",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100023290.1.25",
          "form": "네저는닭가슴살먹어요",
          "original_form": "네저는닭가슴살먹어요",
          "speaker_id": "2",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100023290.1.26",
          "form": "아 대단하시네요",
          "original_form": "아 대단하시네요",
          "speaker_id": "1",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100023290.1.27",
          "form": "샐러드도건강을위해먹고있어요",
          "original_form": "샐러드도건강을위해먹고있어요",
          "speaker_id": "2",
          "time": "20210829 23:46"
        },
        {
          "id": "MDRW2100023290.1.28",
          "form": "음식 참는거 진짜힘든데",
          "original_form": "음식 참는거 진짜힘든데",
          "speaker_id": "1",
          "time": "20210829 23:46"
        }
      ]
