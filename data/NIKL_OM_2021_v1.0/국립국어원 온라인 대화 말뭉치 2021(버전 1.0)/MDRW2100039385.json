      "utterance": [
        {
          "id": "MDRW2100039385.1.1",
          "form": "마침 저 점저 먹으려던 참이에요",
          "original_form": "마침 저 점저 먹으려던 참이에요",
          "speaker_id": "2",
          "time": "20210903 16:57"
        },
        {
          "id": "MDRW2100039385.1.2",
          "form": "맛있는 거 드세요 전 단호박죽 사먹으려구",
          "original_form": "맛있는 거 드세요 전 단호박죽 사먹으려구",
          "speaker_id": "1",
          "time": "20210903 16:58"
        },
        {
          "id": "MDRW2100039385.1.3",
          "form": "단백질 만땅 어쩌구 닭 퍽퍽살 먹어요",
          "original_form": "단백질 만땅 어쩌구 닭 퍽퍽살 먹어요",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100039385.1.4",
          "form": "단호박죽과 고등어백반중 고민하구있어요",
          "original_form": "단호박죽과 고등어백반중 고민하구있어요",
          "speaker_id": "1",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100039385.1.5",
          "form": "단호박죽이 레토르트라면 고등어요",
          "original_form": "단호박죽이 레토르트라면 고등어요",
          "speaker_id": "2",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100039385.1.6",
          "form": "단백질 채워야하는구나.. 아뇨 본죽 단호박죽이야",
          "original_form": "단백질 채워야하는구나.. 아뇨 본죽 단호박죽이야",
          "speaker_id": "1",
          "time": "20210903 16:59"
        },
        {
          "id": "MDRW2100039385.1.7",
          "form": "근데 그 집이 바퀴나오던 거기.",
          "original_form": "근데 그 집이 바퀴나오던 거기.",
          "speaker_id": "1",
          "time": "20210903 17:00"
        },
        {
          "id": "MDRW2100039385.1.8",
          "form": "고등어요.",
          "original_form": "고등어요.",
          "speaker_id": "2",
          "time": "20210903 17:00"
        },
        {
          "id": "MDRW2100039385.1.9",
          "form": "집 앞 맛집이지만... 네 그 고등어구이가 바퀴나와요",
          "original_form": "집 앞 맛집이지만... 네 그 고등어구이가 바퀴나와요",
          "speaker_id": "1",
          "time": "20210903 17:00"
        },
        {
          "id": "MDRW2100039385.1.10",
          "form": "아. 본죽집이 바퀴가 아니구나",
          "original_form": "아. 본죽집이 바퀴가 아니구나",
          "speaker_id": "2",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100039385.1.11",
          "form": "그럼 죽머거",
          "original_form": "그럼 죽머거",
          "speaker_id": "2",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100039385.1.12",
          "form": "전 컵누들 매콤찜닭맛 먹어요",
          "original_form": "전 컵누들 매콤찜닭맛 먹어요",
          "speaker_id": "2",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100039385.1.13",
          "form": "궁금해서",
          "original_form": "궁금해서",
          "speaker_id": "2",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100039385.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 저번에 말했는데ㅠ 조아 죽먹고 계란이나 왕창 삶아둘까나",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 저번에 말했는데ㅠ 조아 죽먹고 계란이나 왕창 삶아둘까나",
          "speaker_id": "1",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100039385.1.15",
          "form": "설거지하고 삶아서 조림만들까 싶음",
          "original_form": "설거지하고 삶아서 조림만들까 싶음",
          "speaker_id": "1",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100039385.1.16",
          "form": "당신의 장건강을 소중히 여겨주세요 맵지않을까",
          "original_form": "당신의 장건강을 소중히 여겨주세요 맵지않을까",
          "speaker_id": "1",
          "time": "20210903 17:01"
        },
        {
          "id": "MDRW2100039385.1.17",
          "form": "간장소스같길래요",
          "original_form": "간장소스같길래요",
          "speaker_id": "2",
          "time": "20210903 17:02"
        },
        {
          "id": "MDRW2100039385.1.18",
          "form": "간장이 매워봤자...",
          "original_form": "간장이 매워봤자...",
          "speaker_id": "2",
          "time": "20210903 17:02"
        },
        {
          "id": "MDRW2100039385.1.19",
          "form": "매운 간장치킨 먹어본적없구나 당신....",
          "original_form": "매운 간장치킨 먹어본적없구나 당신....",
          "speaker_id": "1",
          "time": "20210903 17:02"
        },
        {
          "id": "MDRW2100039385.1.20",
          "form": "아닌가 제가 매운걸 못먹는건가 몰겠구만",
          "original_form": "아닌가 제가 매운걸 못먹는건가 몰겠구만",
          "speaker_id": "1",
          "time": "20210903 17:02"
        },
        {
          "id": "MDRW2100039385.1.21",
          "form": "하튼 저도 저녁먹어야겠어요 역시 단호박죽 먹어야지",
          "original_form": "하튼 저도 저녁먹어야겠어요 역시 단호박죽 먹어야지",
          "speaker_id": "1",
          "time": "20210903 17:02"
        },
        {
          "id": "MDRW2100039385.1.22",
          "form": "저도 매운거 못먹는데 요즘 간장치킨도 맵게 나오나요?",
          "original_form": "저도 매운거 못먹는데 요즘 간장치킨도 맵게 나오나요?",
          "speaker_id": "2",
          "time": "20210903 17:03"
        },
        {
          "id": "MDRW2100039385.1.23",
          "form": "사람들은 매운거에 미쳤어",
          "original_form": "사람들은 매운거에 미쳤어",
          "speaker_id": "2",
          "time": "20210903 17:03"
        }
      ]
    }
