      "utterance": [
        {
          "id": "MDRW2100042015.1.1",
          "form": "안녕하세요 저녁 드셨나요?",
          "original_form": "안녕하세요 저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210906 18:36"
        },
        {
          "id": "MDRW2100042015.1.2",
          "form": "네 이제 퇴근이라 집가서 먹으려고요",
          "original_form": "네 이제 퇴근이라 집가서 먹으려고요",
          "speaker_id": "2",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100042015.1.3",
          "form": "뭐 드실지 정하셨나요??",
          "original_form": "뭐 드실지 정하셨나요??",
          "speaker_id": "1",
          "time": "20210906 18:38"
        },
        {
          "id": "MDRW2100042015.1.4",
          "form": "아마 부침개?? ㅋㅋ",
          "original_form": "아마 부침개?? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100042015.1.5",
          "form": "뭐드시나요??",
          "original_form": "뭐드시나요??",
          "speaker_id": "2",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100042015.1.6",
          "form": "저는 피자 먹을거 같아요ㅎㅎ",
          "original_form": "저는 피자 먹을거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100042015.1.7",
          "form": "피자 좋아하시나요?",
          "original_form": "피자 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210906 18:42"
        },
        {
          "id": "MDRW2100042015.1.8",
          "form": "아 너무 좋아하죠ㅜㅠ",
          "original_form": "아 너무 좋아하죠ㅜㅠ",
          "speaker_id": "2",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100042015.1.9",
          "form": "어디꺼 드세요?",
          "original_form": "어디꺼 드세요?",
          "speaker_id": "2",
          "time": "20210906 18:45"
        },
        {
          "id": "MDRW2100042015.1.10",
          "form": "피자마루 피자 먹으려구요@",
          "original_form": "피자마루 피자 먹으려구요@",
          "speaker_id": "1",
          "time": "20210906 18:50"
        },
        {
          "id": "MDRW2100042015.1.11",
          "form": "혹시 피자마루 드셔보셨나요?",
          "original_form": "혹시 피자마루 드셔보셨나요?",
          "speaker_id": "1",
          "time": "20210906 18:50"
        },
        {
          "id": "MDRW2100042015.1.12",
          "form": "맛있다던데요! 전 치킨마루 자주 먹어요!",
          "original_form": "맛있다던데요! 전 치킨마루 자주 먹어요!",
          "speaker_id": "2",
          "time": "20210906 19:00"
        },
        {
          "id": "MDRW2100042015.1.13",
          "form": "네 피자마루 맛있어요ㅎㅎ",
          "original_form": "네 피자마루 맛있어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100042015.1.14",
          "form": "치킨마루 맛있나요?!",
          "original_form": "치킨마루 맛있나요?!",
          "speaker_id": "1",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100042015.1.15",
          "form": "안먹어봤어요!",
          "original_form": "안먹어봤어요!",
          "speaker_id": "1",
          "time": "20210906 19:16"
        },
        {
          "id": "MDRW2100042015.1.16",
          "form": "치킨마루 가성비최고예요ㅋㅋ",
          "original_form": "치킨마루 가성비최고예요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:21"
        },
        {
          "id": "MDRW2100042015.1.17",
          "form": "바사칸치킨",
          "original_form": "바사칸치킨",
          "speaker_id": "2",
          "time": "20210906 19:21"
        },
        {
          "id": "MDRW2100042015.1.18",
          "form": "오 사진 찾아봤는데 진짜 맛있어보이네요ㅠㅠㅠ",
          "original_form": "오 사진 찾아봤는데 진짜 맛있어보이네요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 19:30"
        },
        {
          "id": "MDRW2100042015.1.19",
          "form": "가장 좋아하는 치킨인가요??",
          "original_form": "가장 좋아하는 치킨인가요??",
          "speaker_id": "1",
          "time": "20210906 19:31"
        },
        {
          "id": "MDRW2100042015.1.20",
          "form": "집가까이에 있어서 단골이에요ㅋㅋ",
          "original_form": "집가까이에 있어서 단골이에요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:32"
        },
        {
          "id": "MDRW2100042015.1.21",
          "form": "역시 가까운게 최곤거 같아요",
          "original_form": "역시 가까운게 최곤거 같아요",
          "speaker_id": "1",
          "time": "20210906 19:39"
        },
        {
          "id": "MDRW2100042015.1.22",
          "form": "웬만큼 맛있으면!!!",
