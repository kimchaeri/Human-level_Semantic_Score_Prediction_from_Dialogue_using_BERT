        },
        {
          "id": "MDRW2100018439.1.2",
          "form": "저는 안키우지만 저희 본가는 반려동물 키워요",
          "original_form": "저는 안키우지만 저희 본가는 반려동물 키워요",
          "speaker_id": "1",
          "time": "20210827 01:54"
        },
        {
          "id": "MDRW2100018439.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210827 01:54"
        },
        {
          "id": "MDRW2100018439.1.4",
          "form": "두둠댕댕이라고",
          "original_form": "두둠댕댕이라고",
          "speaker_id": "1",
          "time": "20210827 01:54"
        },
        {
          "id": "MDRW2100018439.1.5",
          "form": "오 어떻게생겻어요??",
          "original_form": "오 어떻게생겻어요??",
          "speaker_id": "2",
          "time": "20210827 01:55"
        },
        {
          "id": "MDRW2100018439.1.6",
          "form": "이름이에요 귀엽죠?",
          "original_form": "이름이에요 귀엽죠?",
          "speaker_id": "1",
          "time": "20210827 01:55"
        },
        {
          "id": "MDRW2100018439.1.7",
          "form": "특이하네요",
          "original_form": "특이하네요",
          "speaker_id": "2",
          "time": "20210827 01:55"
        },
        {
          "id": "MDRW2100018439.1.8",
          "form": "잘해주고싶게 생겼어요",
          "original_form": "잘해주고싶게 생겼어요",
          "speaker_id": "1",
          "time": "20210827 01:56"
        },
        {
          "id": "MDRW2100018439.1.9",
          "form": "오 그렇군요.. 종이 뭔가요",
          "original_form": "오 그렇군요.. 종이 뭔가요",
          "speaker_id": "2",
          "time": "20210827 01:56"
        },
        {
          "id": "MDRW2100018439.1.10",
          "form": "댕댕이요",
          "original_form": "댕댕이요",
          "speaker_id": "1",
          "time": "20210827 01:56"
        },
        {
          "id": "MDRW2100018439.1.11",
          "form": "그게 뭐죠?",
          "original_form": "그게 뭐죠?",
          "speaker_id": "2",
          "time": "20210827 01:56"
        },
        {
          "id": "MDRW2100018439.1.12",
          "form": "댕댕이는 강아지에요",
          "original_form": "댕댕이는 강아지에요",
          "speaker_id": "1",
          "time": "20210827 01:56"
        },
        {
          "id": "MDRW2100018439.1.13",
          "form": "와 그렇군요.. 처음들어봐요",
