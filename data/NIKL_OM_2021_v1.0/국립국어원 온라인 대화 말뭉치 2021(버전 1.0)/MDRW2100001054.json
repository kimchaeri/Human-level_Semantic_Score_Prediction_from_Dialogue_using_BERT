      "utterance": [
        {
          "id": "MDRW2100001054.1.1",
          "form": "오늘 날씨 진짜 더운거같아요",
          "original_form": "오늘 날씨 진짜 더운거같아요",
          "speaker_id": "1",
          "time": "20210722 21:16"
        },
        {
          "id": "MDRW2100001054.1.2",
          "form": "저 지금 제주도인데 여기도 너무더워요 ㅜ",
          "original_form": "저 지금 제주도인데 여기도 너무더워요 ㅜ",
          "speaker_id": "1",
          "time": "20210722 21:16"
        },
        {
          "id": "MDRW2100001054.1.3",
          "form": "헐 제주도 많이더워요?",
          "original_form": "헐 제주도 많이더워요?",
          "speaker_id": "2",
          "time": "20210722 21:16"
        },
        {
          "id": "MDRW2100001054.1.4",
          "form": "네 완전 더워요 ㅠㅠ",
          "original_form": "네 완전 더워요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210722 21:16"
        },
        {
          "id": "MDRW2100001054.1.5",
          "form": "땀이 삐질삐질",
          "original_form": "땀이 삐질삐질",
          "speaker_id": "1",
          "time": "20210722 21:17"
        },
        {
          "id": "MDRW2100001054.1.6",
          "form": "오늘 나가지는 않았는데 어제 나가니까 그늘아니면 죽겠던데요 ㅋㅋ",
          "original_form": "오늘 나가지는 않았는데 어제 나가니까 그늘아니면 죽겠던데요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210722 21:17"
        },
        {
          "id": "MDRW2100001054.1.7",
          "form": "그니까요 ;; 머리위에 에어컨같은거 있었으면 좋겠어요 ㅋㅋ",
          "original_form": "그니까요 ;; 머리위에 에어컨같은거 있었으면 좋겠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210722 21:17"
        },
        {
          "id": "MDRW2100001054.1.8",
          "form": "역시 여름에는 집이 짱인거 같습니다 ㅋㅋ",
          "original_form": "역시 여름에는 집이 짱인거 같습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210722 21:17"
        },
        {
          "id": "MDRW2100001054.1.9",
          "form": "근데 가게같은 곳 들어가면 또 추워요",
          "original_form": "근데 가게같은 곳 들어가면 또 추워요",
          "speaker_id": "2",
          "time": "20210722 21:18"
        },
        {
          "id": "MDRW2100001054.1.10",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210722 21:19"
        },
        {
          "id": "MDRW2100001054.1.11",
          "form": "완전 에어컨 빵빵하게 틀어놔서",
          "original_form": "완전 에어컨 빵빵하게 틀어놔서",
          "speaker_id": "1",
          "time": "20210722 21:19"
        },
        {
          "id": "MDRW2100001054.1.12",
          "form": "여름에는 집이짱 하지만 학원을 가야해서 나가면 찜통...",
          "original_form": "여름에는 집이짱 하지만 학원을 가야해서 나가면 찜통...",
          "speaker_id": "2",
          "time": "20210722 21:20"
        },
        {
          "id": "MDRW2100001054.1.13",
          "form": "진짜 나가고 싶지않아욤",
          "original_form": "진짜 나가고 싶지않아욤",
          "speaker_id": "1",
          "time": "20210722 21:21"
        },
        {
          "id": "MDRW2100001054.1.14",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210722 21:21"
        },
        {
          "id": "MDRW2100001054.1.15",
          "form": "저는 가을이 제일좋아요><",
          "original_form": "저는 가을이 제일좋아요><",
          "speaker_id": "1",
          "time": "20210722 21:22"
        },
        {
          "id": "MDRW2100001054.1.16",
          "form": "봄, 가을이 짱이죠",
          "original_form": "봄, 가을이 짱이죠",
          "speaker_id": "2",
          "time": "20210722 21:22"
        },
        {
          "id": "MDRW2100001054.1.17",
          "form": "하지만 짧다는게 함정...",
          "original_form": "하지만 짧다는게 함정...",
          "speaker_id": "2",
          "time": "20210722 21:22"
        },
        {
          "id": "MDRW2100001054.1.18",
          "form": "너무 짧죠 ㅜ",
          "original_form": "너무 짧죠 ㅜ",
          "speaker_id": "1",
          "time": "20210722 21:23"
        }
      ]
