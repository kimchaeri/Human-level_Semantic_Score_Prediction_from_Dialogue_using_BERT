      "utterance": [
        {
          "id": "MDRW2100041125.1.1",
          "form": "게임 하시나요?",
          "original_form": "게임 하시나요?",
          "speaker_id": "1",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100041125.1.2",
          "form": "모바일겜만요",
          "original_form": "모바일겜만요",
          "speaker_id": "2",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100041125.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100041125.1.4",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100041125.1.5",
          "form": "어떤 게임 하세요?",
          "original_form": "어떤 게임 하세요?",
          "speaker_id": "1",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100041125.1.6",
          "form": "혼자할수있는거 주로해요",
          "original_form": "혼자할수있는거 주로해요",
          "speaker_id": "2",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100041125.1.7",
          "form": "간단하고",
          "original_form": "간단하고",
          "speaker_id": "2",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100041125.1.8",
          "form": "요즘에는 퍼즐깨기? 같은거 하고있어요",
          "original_form": "요즘에는 퍼즐깨기? 같은거 하고있어요",
          "speaker_id": "2",
          "time": "20210905 17:36"
        },
        {
          "id": "MDRW2100041125.1.9",
          "form": "저도 퍼즐깨는 게임 좋아해요",
          "original_form": "저도 퍼즐깨는 게임 좋아해요",
          "speaker_id": "1",
          "time": "20210905 17:36"
        },
        {
          "id": "MDRW2100041125.1.10",
          "form": "꿈의 집 아시나요?",
          "original_form": "꿈의 집 아시나요?",
          "speaker_id": "1",
          "time": "20210905 17:36"
        },
        {
          "id": "MDRW2100041125.1.11",
          "form": "들어본거같은데",
          "original_form": "들어본거같은데",
          "speaker_id": "2",
          "time": "20210905 17:37"
        },
        {
          "id": "MDRW2100041125.1.12",
          "form": "기억이 안나네요 ㅎ",
          "original_form": "기억이 안나네요 ㅎ",
          "speaker_id": "2",
          "time": "20210905 17:37"
        },
        {
          "id": "MDRW2100041125.1.13",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "2",
          "time": "20210905 17:37"
        },
        {
          "id": "MDRW2100041125.1.14",
          "form": "네! 퍼즐 깨고 집도 꾸밀 수 있어서",
          "original_form": "네! 퍼즐 깨고 집도 꾸밀 수 있어서",
          "speaker_id": "1",
          "time": "20210905 17:37"
        },
        {
          "id": "MDRW2100041125.1.15",
          "form": "쉽게 안 질리더라고요",
          "original_form": "쉽게 안 질리더라고요",
          "speaker_id": "1",
          "time": "20210905 17:37"
        },
        {
          "id": "MDRW2100041125.1.16",
          "form": "저는 꾸미는게임은 오히려 쉽게 질리더라구요",
          "original_form": "저는 꾸미는게임은 오히려 쉽게 질리더라구요",
          "speaker_id": "2",
          "time": "20210905 17:38"
        },
        {
          "id": "MDRW2100041125.1.17",
          "form": "그래서 퍼즐깨기만",
          "original_form": "그래서 퍼즐깨기만",
          "speaker_id": "2",
          "time": "20210905 17:38"
        },
        {
          "id": "MDRW2100041125.1.18",
          "form": "그럼 쿠킹덤 같은 게임도 별로 안 좋아하시겠네요..!",
          "original_form": "그럼 쿠킹덤 같은 게임도 별로 안 좋아하시겠네요..!",
          "speaker_id": "1",
          "time": "20210905 17:38"
        },
        {
          "id": "MDRW2100041125.1.19",
          "form": "그 쿠키 달리는 게임이죠?",
          "original_form": "그 쿠키 달리는 게임이죠?",
          "speaker_id": "2",
          "time": "20210905 17:39"
        },
        {
          "id": "MDRW2100041125.1.20",
          "form": "그건 쿠키런! 이고요",
          "original_form": "그건 쿠키런! 이고요",
          "speaker_id": "1",
          "time": "20210905 17:39"
        },
        {
          "id": "MDRW2100041125.1.21",
          "form": "쿠킹덤은 쿠키 달리는 걸로 왕국 꾸미는 게임이에요!!!",
          "original_form": "쿠킹덤은 쿠키 달리는 걸로 왕국 꾸미는 게임이에요!!!",
          "speaker_id": "1",
          "time": "20210905 17:39"
        },
        {
          "id": "MDRW2100041125.1.22",
          "form": "아 ㅋㄲㅋ  안해봤어요",
          "original_form": "아 ㅋㄲㅋ  안해봤어요",
          "speaker_id": "2",
          "time": "20210905 17:39"
        },
        {
          "id": "MDRW2100041125.1.23",
          "form": "꾸미는게임 좋아하세요?",
          "original_form": "꾸미는게임 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 17:40"
        },
        {
          "id": "MDRW2100041125.1.24",
          "form": "네 전 아기자기한 게임 좋아하는 편이에요",
          "original_form": "네 전 아기자기한 게임 좋아하는 편이에요",
          "speaker_id": "1",
          "time": "20210905 17:40"
        }
      ]
