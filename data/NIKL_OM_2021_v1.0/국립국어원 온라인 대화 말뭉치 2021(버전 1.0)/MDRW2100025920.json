      "utterance": [
        {
          "id": "MDRW2100025920.1.1",
          "form": "안녕하세요?최근 사신 물건 있으세요?",
          "original_form": "안녕하세요?최근 사신 물건 있으세요?",
          "speaker_id": "1",
          "time": "20210901 18:00"
        },
        {
          "id": "MDRW2100025920.1.2",
          "form": "최근에 버즈프로 질렀습니다!",
          "original_form": "최근에 버즈프로 질렀습니다!",
          "speaker_id": "2",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100025920.1.3",
          "form": "사고싶은 물건 있으세요?",
          "original_form": "사고싶은 물건 있으세요?",
          "speaker_id": "2",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100025920.1.4",
          "form": "오ㅋㅋ버즈프로ㅋㅋ",
          "original_form": "오ㅋㅋ버즈프로ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100025920.1.5",
          "form": "좋다ㅋ",
          "original_form": "좋다ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:01"
        },
        {
          "id": "MDRW2100025920.1.6",
          "form": "저는 최근이사를 앞두고 있어서요ㅋㅋ",
          "original_form": "저는 최근이사를 앞두고 있어서요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025920.1.7",
          "form": "악 새로 구매하실건가용?ㅎㅎ",
          "original_form": "악 새로 구매하실건가용?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025920.1.8",
          "form": "ㅋㅋ네ㅋㅋ살게 너무 많아여ㅋ",
          "original_form": "ㅋㅋ네ㅋㅋ살게 너무 많아여ㅋ",
          "speaker_id": "1",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025920.1.9",
          "form": "저희도 이사가야해서 에어컨 팔았어요!",
          "original_form": "저희도 이사가야해서 에어컨 팔았어요!",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025920.1.10",
          "form": "어떤 거 사실거예용",
          "original_form": "어떤 거 사실거예용",
          "speaker_id": "2",
          "time": "20210901 18:02"
        },
        {
          "id": "MDRW2100025920.1.11",
          "form": "오ㅋㅋ이사할때 중고로 파셨군요",
          "original_form": "오ㅋㅋ이사할때 중고로 파셨군요",
          "speaker_id": "1",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100025920.1.12",
          "form": "당근에서 파셨나요?",
          "original_form": "당근에서 파셨나요?",
          "speaker_id": "1",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100025920.1.13",
          "form": "네네 이사가는 집에 에어컨이 잇더라구요 당근마켓에서 팔앗어요",
          "original_form": "네네 이사가는 집에 에어컨이 잇더라구요 당근마켓에서 팔앗어요",
          "speaker_id": "2",
          "time": "20210901 18:03"
        },
        {
          "id": "MDRW2100025920.1.14",
          "form": "ㅋㅋ저는 진짜 다 사요ㅋㅋ에어컨도 사고",
          "original_form": "ㅋㅋ저는 진짜 다 사요ㅋㅋ에어컨도 사고",
          "speaker_id": "1",
          "time": "20210901 18:04"
        },
        {
          "id": "MDRW2100025920.1.15",
          "form": "가전 가구 다 사요ㅋㅋ",
          "original_form": "가전 가구 다 사요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:04"
        },
        {
          "id": "MDRW2100025920.1.16",
          "form": "우와 다 새걸로 사시는거군요 부러워요",
          "original_form": "우와 다 새걸로 사시는거군요 부러워요",
          "speaker_id": "2",
          "time": "20210901 18:04"
        },
        {
          "id": "MDRW2100025920.1.17",
          "form": "엘지 오브제 시리즈 삽니다ㅋㅋ",
          "original_form": "엘지 오브제 시리즈 삽니다ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 18:05"
        },
        {
          "id": "MDRW2100025920.1.18",
          "form": "즐겁지만 텅장ㅠㅠ",
          "original_form": "즐겁지만 텅장ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 18:05"
        },
        {
          "id": "MDRW2100025920.1.19",
          "form": "우와 이쁘던데 ㅠ저는 저작년에 비스포크로 샀는데 오브제 너무 이쁘더라요 그 시리즈",
          "original_form": "우와 이쁘던데 ㅠ저는 저작년에 비스포크로 샀는데 오브제 너무 이쁘더라요 그 시리즈",
          "speaker_id": "2",
          "time": "20210901 18:05"
        },
        {
          "id": "MDRW2100025920.1.20",
          "form": "네ㅋㅋ이쁘긴해요ㅋㄱㅋ6갤할부 질렀네용ㅋㅋ",
