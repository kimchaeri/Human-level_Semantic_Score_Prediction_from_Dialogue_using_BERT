      "utterance": [
        {
          "id": "MDRW2100036255.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210830 20:15"
        },
        {
          "id": "MDRW2100036255.1.2",
          "form": "안녕하세용",
          "original_form": "안녕하세용",
          "speaker_id": "2",
          "time": "20210830 20:16"
        },
        {
          "id": "MDRW2100036255.1.3",
          "form": "코로나 백신 얘기 어떠세요?",
          "original_form": "코로나 백신 얘기 어떠세요?",
          "speaker_id": "1",
          "time": "20210830 20:17"
        },
        {
          "id": "MDRW2100036255.1.4",
          "form": "좋아요 그렇잖아도 고민중이었어요ㅜㅜ 백신접종하셧어요?",
          "original_form": "좋아요 그렇잖아도 고민중이었어요ㅜㅜ 백신접종하셧어요?",
          "speaker_id": "2",
          "time": "20210830 20:18"
        },
        {
          "id": "MDRW2100036255.1.5",
          "form": "아니오9월 예약인데",
          "original_form": "아니오9월 예약인데",
          "speaker_id": "1",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100036255.1.6",
          "form": "추석 전주에 맞아요 ㅋㅋ",
          "original_form": "추석 전주에 맞아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100036255.1.7",
          "form": "그러시군요 혹시 무섭지는 않으세요?",
          "original_form": "그러시군요 혹시 무섭지는 않으세요?",
          "speaker_id": "2",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100036255.1.8",
          "form": "하도 부작용사례 얘기가 많아서",
          "original_form": "하도 부작용사례 얘기가 많아서",
          "speaker_id": "2",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100036255.1.9",
          "form": "제ㅔ 주변선 생리 관련 부작용이 좀 있는데",
          "original_form": "제ㅔ 주변선 생리 관련 부작용이 좀 있는데",
          "speaker_id": "1",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100036255.1.10",
          "form": "친구들이 이걸 보고하려고 했는데 너무 가볍고 이러니깐 다들 잘 상대를 안해줬어요",
          "original_form": "친구들이 이걸 보고하려고 했는데 너무 가볍고 이러니깐 다들 잘 상대를 안해줬어요",
          "speaker_id": "1",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100036255.1.11",
          "form": "글서 질본서 한국은 생리관련 부작용 보고가 없다고 발표한거 보고",
          "original_form": "글서 질본서 한국은 생리관련 부작용 보고가 없다고 발표한거 보고",
          "speaker_id": "1",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100036255.1.12",
          "form": "깊은 불신에 빠졌습니다",
          "original_form": "깊은 불신에 빠졌습니다",
          "speaker_id": "1",
          "time": "20210830 20:20"
        },
        {
          "id": "MDRW2100036255.1.13",
          "form": "그쵸 저도용",
          "original_form": "그쵸 저도용",
          "speaker_id": "2",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100036255.1.14",
          "form": "믿을 수가 없습니다ㅜㅡ",
          "original_form": "믿을 수가 없습니다ㅜㅡ",
          "speaker_id": "2",
          "time": "20210830 20:21"
        },
        {
          "id": "MDRW2100036255.1.15",
          "form": "이게 항체유지지속기간이",
          "original_form": "이게 항체유지지속기간이",
          "speaker_id": "2",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100036255.1.16",
          "form": "정확히 어느정도인지도 모르니까",
          "original_form": "정확히 어느정도인지도 모르니까",
          "speaker_id": "2",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100036255.1.17",
          "form": "맞으라니깐 맞긴 맞는데 ㅋㅋㅋ",
          "original_form": "맞으라니깐 맞긴 맞는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:22"
        },
        {
          "id": "MDRW2100036255.1.18",
          "form": "맞았다 잘못될 경우 국가가 도와줄지 확신이 안서는 그런 상태에요",
          "original_form": "맞았다 잘못될 경우 국가가 도와줄지 확신이 안서는 그런 상태에요",
          "speaker_id": "1",
          "time": "20210830 20:23"
        },
        {
          "id": "MDRW2100036255.1.19",
          "form": "애초에 제약사들도 확신이 없을 것 같아요",
          "original_form": "애초에 제약사들도 확신이 없을 것 같아요",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100036255.1.20",
          "form": "이게 유래없는 비상사태라",
          "original_form": "이게 유래없는 비상사태라",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100036255.1.21",
          "form": "임상단계들 확 축소하고 비상승인 내준거니까",
          "original_form": "임상단계들 확 축소하고 비상승인 내준거니까",
          "speaker_id": "2",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100036255.1.22",
          "form": "임산부도 맞으라고 cdc도 권고는 하는데",
          "original_form": "임산부도 맞으라고 cdc도 권고는 하는데",
          "speaker_id": "1",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100036255.1.23",
          "form": "유산확률도 안나왔죠 ㅋㅋ",
          "original_form": "유산확률도 안나왔죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:24"
        },
        {
          "id": "MDRW2100036255.1.24",
          "form": "그르게나 말이에요",
          "original_form": "그르게나 말이에요",
          "speaker_id": "2",
          "time": "20210830 20:25"
        },
        {
          "id": "MDRW2100036255.1.25",
          "form": "진짜 세계가 어쩌다 질병하나에 이렇게ㅜㅜ",
          "original_form": "진짜 세계가 어쩌다 질병하나에 이렇게ㅜㅜ",
          "speaker_id": "2",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100036255.1.26",
          "form": "생리 부작용은 제 주변에도 많아서",
          "original_form": "생리 부작용은 제 주변에도 많아서",
          "speaker_id": "1",
          "time": "20210830 20:26"
        },
        {
          "id": "MDRW2100036255.1.27",
          "form": "제가 임산부라면 안맞았을거에요",
          "original_form": "제가 임산부라면 안맞았을거에요",
          "speaker_id": "1",
          "time": "20210830 20:26"
        }
      ]
    }
