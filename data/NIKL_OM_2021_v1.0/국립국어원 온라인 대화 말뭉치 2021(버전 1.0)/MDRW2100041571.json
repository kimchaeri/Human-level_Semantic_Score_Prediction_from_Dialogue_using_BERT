      "utterance": [
        {
          "id": "MDRW2100041571.1.1",
          "form": "으악 그렇군요!!!",
          "original_form": "으악 그렇군요!!!",
          "speaker_id": "2",
          "time": "20210906 11:30"
        },
        {
          "id": "MDRW2100041571.1.2",
          "form": "그 업무를 맡은 사람이 name1님뿐이신가요?!",
          "original_form": "그 업무를 맡은 사람이 &name1&님뿐이신가요?!",
          "speaker_id": "2",
          "time": "20210906 11:30"
        },
        {
          "id": "MDRW2100041571.1.3",
          "form": "뭔가 한 분이라더 더 있으면",
          "original_form": "뭔가 한 분이라더 더 있으면",
          "speaker_id": "2",
          "time": "20210906 11:30"
        },
        {
          "id": "MDRW2100041571.1.4",
          "form": "name1님 안계시는 동안,, 해결이 될텐데,,",
          "original_form": "&name1&님 안계시는 동안,, 해결이 될텐데,,",
          "speaker_id": "2",
          "time": "20210906 11:30"
        },
        {
          "id": "MDRW2100041571.1.5",
          "form": "ㅋㅋㅋㅋ아녀요 9명이나 잇답니다 저희팀은",
          "original_form": "ㅋㅋㅋㅋ아녀요 9명이나 잇답니다 저희팀은",
          "speaker_id": "1",
          "time": "20210906 12:30"
        },
        {
          "id": "MDRW2100041571.1.6",
          "form": "그치만 전화가 많이오다보니",
          "original_form": "그치만 전화가 많이오다보니",
          "speaker_id": "1",
          "time": "20210906 12:33"
        },
        {
          "id": "MDRW2100041571.1.7",
          "form": "많아도 바빠여",
          "original_form": "많아도 바빠여",
          "speaker_id": "1",
          "time": "20210906 12:33"
        },
        {
          "id": "MDRW2100041571.1.8",
          "form": "허걱 그렇군요",
          "original_form": "허걱 그렇군요",
          "speaker_id": "2",
          "time": "20210906 12:34"
        },
        {
          "id": "MDRW2100041571.1.9",
          "form": "지금은 점심시간이겠어요,,,",
          "original_form": "지금은 점심시간이겠어요,,,",
          "speaker_id": "2",
          "time": "20210906 12:34"
        },
        {
          "id": "MDRW2100041571.1.10",
          "form": "인간들 전화 좀 즉당히 해라~!",
          "original_form": "인간들 전화 좀 즉당히 해라~!",
          "speaker_id": "2",
          "time": "20210906 12:35"
        },
        {
          "id": "MDRW2100041571.1.11",
          "form": "맞아요 저희는 11시 40분부터 점심시간이랍니다",
          "original_form": "맞아요 저희는 11시 40분부터 점심시간이랍니다",
          "speaker_id": "1",
          "time": "20210906 12:44"
        },
        {
          "id": "MDRW2100041571.1.12",
          "form": "아앗 그러면 1시부터 슬슬 다시 업무 복귀하시나요ㅠㅠ",
          "original_form": "아앗 그러면 1시부터 슬슬 다시 업무 복귀하시나요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 12:47"
        },
        {
          "id": "MDRW2100041571.1.13",
          "form": "도시락 드셨으려나요?!",
          "original_form": "도시락 드셨으려나요?!",
          "speaker_id": "2",
          "time": "20210906 12:48"
        },
        {
          "id": "MDRW2100041571.1.14",
          "form": "네 맞아요 한시땡해서 바로 일시작 ㅠㅠ",
          "original_form": "네 맞아요 한시땡해서 바로 일시작 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100041571.1.15",
          "form": "네 저 집에서 볶음밥 싸와서 먹었어요",
          "original_form": "네 저 집에서 볶음밥 싸와서 먹었어요",
          "speaker_id": "1",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100041571.1.16",
          "form": "name2님도 도시락 드셨나요?",
          "original_form": "&name2&님도 도시락 드셨나요?",
          "speaker_id": "1",
          "time": "20210906 14:11"
        },
        {
          "id": "MDRW2100041571.1.17",
          "form": "네네! 어머니가 사오신 그 볶음밥이군요~!",
          "original_form": "네네! 어머니가 사오신 그 볶음밥이군요~!",
          "speaker_id": "2",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100041571.1.18",
          "form": "아주 바쁘셨겠어요 8ㅅ8",
          "original_form": "아주 바쁘셨겠어요 8ㅅ8",
          "speaker_id": "2",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100041571.1.19",
          "form": "저는 오늘 하루가 짧은 기분이에요 ㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "저는 오늘 하루가 짧은 기분이에요 ㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100041571.1.20",
          "form": "새똥 때문에 늦게 출근해서,,,",
          "original_form": "새똥 때문에 늦게 출근해서,,,",
          "speaker_id": "2",
          "time": "20210906 14:15"
        },
        {
          "id": "MDRW2100041571.1.21",
          "form": "ㅋㅋㅋㅋ살짝..개이득인 기분...?ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ살짝..개이득인 기분...?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100041571.1.22",
          "form": "아 오늘 전화가 정말 많이 와서 하 정신앖어요",
          "original_form": "아 오늘 전화가 정말 많이 와서 하 정신앖어요",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100041571.1.23",
          "form": "지금까지 일한 날중에 가장 바쁜날같아요 ㅋㅋㅋㅋ 하필 오늘 3명이나 빠져서..",
          "original_form": "지금까지 일한 날중에 가장 바쁜날같아요 ㅋㅋㅋㅋ 하필 오늘 3명이나 빠져서..",
          "speaker_id": "1",
          "time": "20210906 14:26"
        },
        {
          "id": "MDRW2100041571.1.24",
          "form": "으앗 그렇군요",
          "original_form": "으앗 그렇군요",
          "speaker_id": "2",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100041571.1.25",
          "form": "오늘 좀 그런 날인가봐요 !!!",
          "original_form": "오늘 좀 그런 날인가봐요 !!!",
          "speaker_id": "2",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041571.1.26",
          "form": "저도 오늘 후배가 안왔는데...!!!",
          "original_form": "저도 오늘 후배가 안왔는데...!!!",
          "speaker_id": "2",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041571.1.27",
          "form": "name1님  퇴근은 몇 시에 하세요~~~?",
          "original_form": "&name1&님  퇴근은 몇 시에 하세요~~~?",
          "speaker_id": "2",
          "time": "20210906 15:14"
        },
        {
          "id": "MDRW2100041571.1.28",
          "form": "저는 딱 9투6 입니다.",
          "original_form": "저는 딱 9투6 입니다.",
          "speaker_id": "1",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041571.1.29",
          "form": "6시 칼퇴를 위해 맨날 드릉드릉하죠 ㅋㅋㅋㅋ 6시 땡 하자마자 달려나가요",
          "original_form": "6시 칼퇴를 위해 맨날 드릉드릉하죠 ㅋㅋㅋㅋ 6시 땡 하자마자 달려나가요",
          "speaker_id": "1",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041571.1.30",
          "form": "그래도 집가면 7시 반이라는..",
          "original_form": "그래도 집가면 7시 반이라는..",
          "speaker_id": "1",
          "time": "20210906 15:16"
        },
        {
          "id": "MDRW2100041571.1.31",
          "form": "앗 진짜 뭔지 알아요 6시에 퇴근해도 집에 가면 7시 30분 ㅠㅠ",
          "original_form": "앗 진짜 뭔지 알아요 6시에 퇴근해도 집에 가면 7시 30분 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 15:24"
        },
        {
          "id": "MDRW2100041571.1.32",
          "form": "달팽이처럼 집 옮겨다니고 싶어요...",
          "original_form": "달팽이처럼 집 옮겨다니고 싶어요...",
          "speaker_id": "2",
          "time": "20210906 15:24"
        },
        {
          "id": "MDRW2100041571.1.33",
          "form": "엄마 아빠랑 계속 같이 살고싶은데",
          "original_form": "엄마 아빠랑 계속 같이 살고싶은데",
          "speaker_id": "2",
          "time": "20210906 15:24"
        },
        {
          "id": "MDRW2100041571.1.34",
          "form": "저는 취업하면 높은 확률로 다른 데로 가야해서 슬퍼요ㅠ",
          "original_form": "저는 취업하면 높은 확률로 다른 데로 가야해서 슬퍼요ㅠ",
          "speaker_id": "2",
          "time": "20210906 15:25"
        },
        {
          "id": "MDRW2100041571.1.35",
          "form": "헉 왜요?",
          "original_form": "헉 왜요?",
          "speaker_id": "1",
          "time": "20210906 15:25"
        },
