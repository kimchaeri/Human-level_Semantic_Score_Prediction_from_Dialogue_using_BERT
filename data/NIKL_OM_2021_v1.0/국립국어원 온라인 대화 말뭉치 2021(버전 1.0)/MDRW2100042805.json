      "utterance": [
        {
          "id": "MDRW2100042805.1.1",
          "form": "라떼는",
          "original_form": "라떼는",
          "speaker_id": "2",
          "time": "20210907 09:22"
        },
        {
          "id": "MDRW2100042805.1.2",
          "form": "출산휴가만 썼는데",
          "original_form": "출산휴가만 썼는데",
          "speaker_id": "2",
          "time": "20210907 09:22"
        },
        {
          "id": "MDRW2100042805.1.3",
          "form": "라떼는 핑퐁에서",
          "original_form": "라떼는 핑퐁에서",
          "speaker_id": "1",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042805.1.4",
          "form": "요새 애들은 맨날 휴직 한다면서",
          "original_form": "요새 애들은 맨날 휴직 한다면서",
          "speaker_id": "2",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042805.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042805.1.6",
          "form": "찾아 먹을 수 있는 건 다 찾아 먹어야지",
          "original_form": "찾아 먹을 수 있는 건 다 찾아 먹어야지",
          "speaker_id": "1",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042805.1.7",
          "form": "빨리 핑 본 커피 마시고 싶다",
          "original_form": "빨리 핑 본 커피 마시고 싶다",
          "speaker_id": "2",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042805.1.8",
          "form": "요즘은 눈치 보면 아무것도 못함",
          "original_form": "요즘은 눈치 보면 아무것도 못함",
          "speaker_id": "1",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042805.1.9",
          "form": "그래 어차피",
          "original_form": "그래 어차피",
          "speaker_id": "2",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042805.1.10",
          "form": "월급 지들이 주는 거 아니면서",
          "original_form": "월급 지들이 주는 거 아니면서",
          "speaker_id": "1",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042805.1.11",
          "form": "내가 돈 안 받고",
          "original_form": "내가 돈 안 받고",
          "speaker_id": "2",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042805.1.12",
          "form": "내가 쉬겠다는 건데",
          "original_form": "내가 쉬겠다는 건데",
          "speaker_id": "2",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042805.1.13",
          "form": "돈은 나라에서 나오는데",
          "original_form": "돈은 나라에서 나오는데",
          "speaker_id": "1",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042805.1.14",
          "form": "딴지 걸면 안 되지",
          "original_form": "딴지 걸면 안 되지",
          "speaker_id": "2",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042805.1.15",
          "form": "그래 지가 내 돈 주는 것도 아니면서",
          "original_form": "그래 지가 내 돈 주는 것도 아니면서",
          "speaker_id": "2",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042805.1.16",
          "form": "맞아요 받을 수 있는 거 다 받아야 돼",
          "original_form": "맞아요 받을 수 있는 거 다 받아야 돼",
          "speaker_id": "1",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042805.1.17",
          "form": "그래도 요새는",
          "original_form": "그래도 요새는",
          "speaker_id": "2",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042805.1.18",
          "form": "시대가 많이 바껴서",
          "original_form": "시대가 많이 바껴서",
          "speaker_id": "2",
          "time": "20210907 09:23"
        },
        {
          "id": "MDRW2100042805.1.19",
          "form": "지들이 못해서 배 아픈 거임",
          "original_form": "지들이 못해서 배 아픈 거임",
          "speaker_id": "1",
          "time": "20210907 09:23"
        }
