      "utterance": [
        {
          "id": "MDRW2100013070.1.1",
          "form": "그니까 그럼 돈 받고 하는데",
          "original_form": "그니까 그럼 돈 받고 하는데",
          "speaker_id": "2",
          "time": "20210825 12:09"
        },
        {
          "id": "MDRW2100013070.1.2",
          "form": "뮤지컬을",
          "original_form": "뮤지컬을",
          "speaker_id": "1",
          "time": "20210825 12:09"
        },
        {
          "id": "MDRW2100013070.1.3",
          "form": "너무쉽게생각하는경향이있어",
          "original_form": "너무쉽게생각하는경향이있어",
          "speaker_id": "1",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100013070.1.4",
          "form": "노래도하고연기도해야해서",
          "original_form": "노래도하고연기도해야해서",
          "speaker_id": "1",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100013070.1.5",
          "form": "어려운데",
          "original_form": "어려운데",
          "speaker_id": "1",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100013070.1.6",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100013070.1.7",
          "form": "둘 다 잘해야 하는데",
          "original_form": "둘 다 잘해야 하는데",
          "speaker_id": "2",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100013070.1.8",
          "form": "둘중에하나만잘하거나",
          "original_form": "둘중에하나만잘하거나",
          "speaker_id": "1",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100013070.1.9",
          "form": "연기도 썩 잘 못하면서",
          "original_form": "연기도 썩 잘 못하면서",
          "speaker_id": "2",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100013070.1.10",
          "form": "둘다어느정도만",
          "original_form": "둘다어느정도만",
          "speaker_id": "1",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100013070.1.11",
          "form": "더ㅣ면",
          "original_form": "더ㅣ면",
          "speaker_id": "1",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100013070.1.12",
          "form": "괜찮다고생각하나보ㅓ",
          "original_form": "괜찮다고생각하나보ㅓ",
          "speaker_id": "1",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100013070.1.13",
          "form": "ㅇㅈㅇㅈ",
          "original_form": "ㅇㅈㅇㅈ",
          "speaker_id": "2",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100013070.1.14",
          "form": "진심",
          "original_form": "진심",
          "speaker_id": "1",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100013070.1.15",
          "form": "뮤지컬을 하고 싶으면 아예 앙상블부터 시작하던가",
          "original_form": "뮤지컬을 하고 싶으면 아예 앙상블부터 시작하던가",
          "speaker_id": "2",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100013070.1.16",
          "form": "ㅇㅇㅇ",
          "original_form": "ㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210825 12:11"
        },
