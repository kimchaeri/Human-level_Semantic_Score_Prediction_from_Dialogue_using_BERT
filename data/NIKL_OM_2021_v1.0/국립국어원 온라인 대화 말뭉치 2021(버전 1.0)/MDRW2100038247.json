        {
          "id": "MDRW2100038247.1.5",
          "form": "저도 그 소식 들었어요ㅎㅎ",
          "original_form": "저도 그 소식 들었어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 02:41"
        },
        {
          "id": "MDRW2100038247.1.6",
          "form": "오 어디서 들으셨죠?",
