        {
          "id": "MDRW2100047289.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 08:26"
        },
        {
          "id": "MDRW2100047289.1.2",
          "form": "네 여행 좋아하죠",
          "original_form": "네 여행 좋아하죠",
          "speaker_id": "2",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100047289.1.3",
          "form": "좋아하시나요?",
          "original_form": "좋아하시나요?",
          "speaker_id": "2",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100047289.1.4",
          "form": "저도용 ㅎㅎ 바다나 산 어디가 더좋으세요",
          "original_form": "저도용 ㅎㅎ 바다나 산 어디가 더좋으세요",
          "speaker_id": "1",
          "time": "20210826 08:28"
        },
        {
          "id": "MDRW2100047289.1.5",
          "form": "전 바다요",
          "original_form": "전 바다요",
          "speaker_id": "2",
          "time": "20210826 08:42"
        },
        {
          "id": "MDRW2100047289.1.6",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210826 08:42"
        },
        {
          "id": "MDRW2100047289.1.7",
          "form": "저도 아직은 바다용 ㅎㅎ",
          "original_form": "저도 아직은 바다용 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 09:53"
        },
        {
          "id": "MDRW2100047289.1.8",
          "form": "ㅋㅋ 산도 좋지만",
          "original_form": "ㅋㅋ 산도 좋지만",
          "speaker_id": "2",
          "time": "20210826 09:55"
        },
        {
          "id": "MDRW2100047289.1.9",
          "form": "벌레가 싫어서요",
          "original_form": "벌레가 싫어서요",
          "speaker_id": "2",
          "time": "20210826 09:55"
        },
        {
          "id": "MDRW2100047289.1.10",
          "form": "맞아요 언제어디서 튀어나올지몰라요 ㅠㅋㅋ",
          "original_form": "맞아요 언제어디서 튀어나올지몰라요 ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:23"
        },
        {
          "id": "MDRW2100047289.1.11",
          "form": "ㅋㅋ 진짜 무서워요",
          "original_form": "ㅋㅋ 진짜 무서워요",
          "speaker_id": "2",
          "time": "20210826 11:26"
        },
        {
          "id": "MDRW2100047289.1.12",
          "form": "회좋아하세용? 동해가서 회먹고싶네요",
          "original_form": "회좋아하세용? 동해가서 회먹고싶네요",
          "speaker_id": "1",
          "time": "20210826 11:40"
        },
        {
          "id": "MDRW2100047289.1.13",
          "form": "네 저 회 짱 좋아해요",
          "original_form": "네 저 회 짱 좋아해요",
          "speaker_id": "2",
          "time": "20210826 12:09"
        },
        {
          "id": "MDRW2100047289.1.14",
          "form": "오 숙회는용?",
          "original_form": "오 숙회는용?",
          "speaker_id": "1",
          "time": "20210826 14:58"
        },
        {
          "id": "MDRW2100047289.1.15",
          "form": "숙회는 안먹어봤어요ㅋㅋㅋㅋ",
          "original_form": "숙회는 안먹어봤어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:01"
        },
        {
          "id": "MDRW2100047289.1.16",
