      "utterance": [
        {
          "id": "MDRW2100037188.1.1",
          "form": "초코 과자에요",
          "original_form": "초코 과자에요",
          "speaker_id": "2",
          "time": "20210831 21:23"
        },
        {
          "id": "MDRW2100037188.1.2",
          "form": "빈츠의 업그레이드 버전 느낌?ㅋㅋ",
          "original_form": "빈츠의 업그레이드 버전 느낌?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037188.1.3",
          "form": "랑드샤..",
          "original_form": "랑드샤..",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037188.1.4",
          "form": "아아~ 그렇군요!",
          "original_form": "아아~ 그렇군요!",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037188.1.5",
          "form": "해외과자? ㅎㅎㅎ",
          "original_form": "해외과자? ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037188.1.6",
          "form": "저는 이름은 처음들어봐욤",
          "original_form": "저는 이름은 처음들어봐욤",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037188.1.7",
          "form": "실제로 보면 알지는 모르겠어요 ㅎㅎ",
          "original_form": "실제로 보면 알지는 모르겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037188.1.8",
          "form": "양은 좀 적긴 한데 맛있더라구요. 나온지 얼마 안 된 것 같아요",
          "original_form": "양은 좀 적긴 한데 맛있더라구요. 나온지 얼마 안 된 것 같아요",
          "speaker_id": "2",
          "time": "20210831 21:24"
        },
        {
          "id": "MDRW2100037188.1.9",
          "form": "롯데였던 거 같아요",
          "original_form": "롯데였던 거 같아요",
          "speaker_id": "2",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100037188.1.10",
          "form": "아~~",
          "original_form": "아~~",
          "speaker_id": "1",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100037188.1.11",
          "form": "다음에 장보러갈때 한번 찾아봐야겠어요 ㅎㅎ",
          "original_form": "다음에 장보러갈때 한번 찾아봐야겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:25"
        },
        {
          "id": "MDRW2100037188.1.12",
          "form": "abc초코쿠키도 맛있었는데 랑드샤가 더 맛있더라구요 ㅎㅎ",
          "original_form": "abc초코쿠키도 맛있었는데 랑드샤가 더 맛있더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100037188.1.13",
          "form": "오호!",
          "original_form": "오호!",
          "speaker_id": "1",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100037188.1.14",
          "form": "저도 지금 초코과자만 사왔어요 ㅋㅋㅋ",
          "original_form": "저도 지금 초코과자만 사왔어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:27"
        },
        {
          "id": "MDRW2100037188.1.15",
          "form": "최근들어 안먹던 과자를 찾게되네여... 뭐지ㅜㅜ ㅎㅎ",
          "original_form": "최근들어 안먹던 과자를 찾게되네여... 뭐지ㅜㅜ ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100037188.1.16",
          "form": "저도 어릴 때보다 과자 더 찾게 되더라구요ㅠ",
          "original_form": "저도 어릴 때보다 과자 더 찾게 되더라구요ㅠ",
          "speaker_id": "2",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100037188.1.17",
          "form": "입이 심심하기도 하고..",
          "original_form": "입이 심심하기도 하고..",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100037188.1.18",
          "form": "다들 비슷하네여 ㅎㅎ",
          "original_form": "다들 비슷하네여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100037188.1.19",
          "form": "그러고보니 요즘 새콤달콤을 못먹었네ㅜㅜ 갑자기 생각났어요 ㅋㅋㅋㅋ",
          "original_form": "그러고보니 요즘 새콤달콤을 못먹었네ㅜㅜ 갑자기 생각났어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100037188.1.20",
          "form": "아 전 몇 년 전부터 껌 엄청 씹어요",
          "original_form": "아 전 몇 년 전부터 껌 엄청 씹어요",
          "speaker_id": "2",
          "time": "20210831 21:34"
        },
        {
          "id": "MDRW2100037188.1.21",
          "form": "집에 껌이 떨어지지 않게 하고 있어요 ㅎㅎ",
          "original_form": "집에 껌이 떨어지지 않게 하고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100037188.1.22",
          "form": "오!! 껌 진짜 씹는맛있죠!",
          "original_form": "오!! 껌 진짜 씹는맛있죠!",
          "speaker_id": "1",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100037188.1.23",
          "form": "저도 껌 좋아해요! 자일리톨 같은 ㅎㅎ통으로 사다가",
          "original_form": "저도 껌 좋아해요! 자일리톨 같은 ㅎㅎ통으로 사다가",
          "speaker_id": "1",
          "time": "20210831 21:35"
        },
        {
          "id": "MDRW2100037188.1.24",
          "form": "자일리톨 리필 파는 거 계속 사오고 있어요ㅎㅎ",
          "original_form": "자일리톨 리필 파는 거 계속 사오고 있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 21:36"
        },
        {
          "id": "MDRW2100037188.1.25",
          "form": "아아 ㅋㅋㅋㅋ",
          "original_form": "아아 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100037188.1.26",
          "form": "그거 납작하게 된거요!",
          "original_form": "그거 납작하게 된거요!",
          "speaker_id": "1",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100037188.1.27",
          "form": "기본 초록색도 샀다가 연두색도 샀다가 분홍색도 사고..ㅋㅋㅋ",
          "original_form": "기본 초록색도 샀다가 연두색도 샀다가 분홍색도 사고..ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100037188.1.28",
          "form": "어떤 색 잘 드세요?",
          "original_form": "어떤 색 잘 드세요?",
