      "utterance": [
        {
          "id": "MDRW2100037370.1.1",
          "form": "저녁은 드셨어요 ?",
          "original_form": "저녁은 드셨어요 ?",
          "speaker_id": "1",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037370.1.2",
          "form": "네 집밥 먹었어요",
          "original_form": "네 집밥 먹었어요",
          "speaker_id": "2",
          "time": "20210831 22:57"
        },
        {
          "id": "MDRW2100037370.1.3",
          "form": "name1님도 드셨어요?",
          "original_form": "&name1&님도 드셨어요?",
          "speaker_id": "2",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037370.1.4",
          "form": "네 ! 저는 빵이요 ㅎㅎ",
          "original_form": "네 ! 저는 빵이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 22:58"
        },
        {
          "id": "MDRW2100037370.1.5",
          "form": "어떤 빵 드셨나요?",
          "original_form": "어떤 빵 드셨나요?",
          "speaker_id": "2",
          "time": "20210831 23:02"
        },
        {
          "id": "MDRW2100037370.1.6",
          "form": "마늘바게트 먹었어요 !",
          "original_form": "마늘바게트 먹었어요 !",
          "speaker_id": "1",
          "time": "20210831 23:03"
        },
        {
          "id": "MDRW2100037370.1.7",
          "form": "맛있었겠네요 갑자기 저도 바게트 먹고 싶어요",
          "original_form": "맛있었겠네요 갑자기 저도 바게트 먹고 싶어요",
          "speaker_id": "2",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100037370.1.8",
          "form": "안에 명란젓 들어가있는 바게트 먹어본 적 있는데 되게 맛있었어요",
          "original_form": "안에 명란젓 들어가있는 바게트 먹어본 적 있는데 되게 맛있었어요",
          "speaker_id": "2",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100037370.1.9",
          "form": "아아 맛있어요 !!!",
          "original_form": "아아 맛있어요 !!!",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100037370.1.10",
          "form": "저희집 근처 빵집에도 있는데",
          "original_form": "저희집 근처 빵집에도 있는데",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100037370.1.11",
          "form": "대표메뉴더라구요 ㅎㅎ",
          "original_form": "대표메뉴더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100037370.1.12",
          "form": "오 대표메뉴일만 하죠!!",
          "original_form": "오 대표메뉴일만 하죠!!",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037370.1.13",
          "form": "근처 빵집에 팔다니 부러워요ㅋㅋㅋ",
          "original_form": "근처 빵집에 팔다니 부러워요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:06"
        },
        {
          "id": "MDRW2100037370.1.14",
          "form": "저도 거기에서 첨 먹어보구",
          "original_form": "저도 거기에서 첨 먹어보구",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100037370.1.15",
          "form": "신기했어요 ㅋㅋ 생각보다 맛있어서요 !",
          "original_form": "신기했어요 ㅋㅋ 생각보다 맛있어서요 !",
          "speaker_id": "1",
          "time": "20210831 23:08"
        },
        {
          "id": "MDRW2100037370.1.16",
          "form": "맞아요 생각보다 맛있죠ㅋㅋ",
          "original_form": "맞아요 생각보다 맛있죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100037370.1.17",
          "form": "앙버터는 좋아하세요?",
          "original_form": "앙버터는 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 23:09"
        },
        {
          "id": "MDRW2100037370.1.18",
          "form": "좋아해요 !!!",
          "original_form": "좋아해요 !!!",
          "speaker_id": "1",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100037370.1.19",
          "form": "근데 잘하는집이 잘 없어서 ㅜㅜ",
          "original_form": "근데 잘하는집이 잘 없어서 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210831 23:10"
        },
        {
          "id": "MDRW2100037370.1.20",
          "form": "크루아상 앙버터가 증말 맛있는데 저도 주변에 파는데가 없네요",
          "original_form": "크루아상 앙버터가 증말 맛있는데 저도 주변에 파는데가 없네요",
          "speaker_id": "2",
          "time": "20210831 23:11"
        },
        {
          "id": "MDRW2100037370.1.21",
          "form": "맞죠 ㅠㅠ 뭔가 담백해서 좋아해요 ㅎㅎ",
          "original_form": "맞죠 ㅠㅠ 뭔가 담백해서 좋아해요 ㅎㅎ",
