      "utterance": [
        {
          "id": "MDRW2100004819.1.1",
          "form": "군대 가세요??",
          "original_form": "군대 가세요??",
          "speaker_id": "1",
          "time": "20210821 20:36"
        },
        {
          "id": "MDRW2100004819.1.2",
          "form": "아니요 그냥 공부하려고요",
          "original_form": "아니요 그냥 공부하려고요",
          "speaker_id": "2",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100004819.1.3",
          "form": "여자에요",
          "original_form": "여자에요",
          "speaker_id": "2",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100004819.1.4",
          "form": "악.ㅋㅋㅋㅋ",
          "original_form": "악.ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100004819.1.5",
          "form": "저는 남편이 군대 다녀왔거든요..",
          "original_form": "저는 남편이 군대 다녀왔거든요..",
          "speaker_id": "1",
          "time": "20210821 20:59"
        },
        {
          "id": "MDRW2100004819.1.6",
          "form": "젊을때 2년을 나라에 희생한다는게.. 참 크더라고요",
          "original_form": "젊을때 2년을 나라에 희생한다는게.. 참 크더라고요",
          "speaker_id": "1",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100004819.1.7",
          "form": "지금은 1년반이죠??",
          "original_form": "지금은 1년반이죠??",
          "speaker_id": "1",
          "time": "20210821 21:00"
        },
        {
          "id": "MDRW2100004819.1.8",
          "form": "네 그런 거 같아요",
          "original_form": "네 그런 거 같아요",
          "speaker_id": "2",
          "time": "20210821 21:01"
        },
        {
          "id": "MDRW2100004819.1.9",
          "form": "남자 형제가 동생들밖에 없어서 군대가 좀 멀게 느껴져요",
          "original_form": "남자 형제가 동생들밖에 없어서 군대가 좀 멀게 느껴져요",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100004819.1.10",
          "form": "남동생도 아직 군대가려면 멀어서요",
          "original_form": "남동생도 아직 군대가려면 멀어서요",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100004819.1.11",
          "form": "그래도 지인이 군대 간다고 하면 걱정이 될 거 같아요",
          "original_form": "그래도 지인이 군대 간다고 하면 걱정이 될 거 같아요",
          "speaker_id": "2",
          "time": "20210821 21:02"
        },
        {
          "id": "MDRW2100004819.1.12",
          "form": "저는 비금 남동생 군대가있거든요",
          "original_form": "저는 비금 남동생 군대가있거든요",
          "speaker_id": "1",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100004819.1.13",
          "form": "지금..",
          "original_form": "지금..",
          "speaker_id": "1",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100004819.1.14",
          "form": "예전보다는 좋아져서 자긴 지낼만 하다고 하더라고요.",
          "original_form": "예전보다는 좋아져서 자긴 지낼만 하다고 하더라고요.",
          "speaker_id": "1",
          "time": "20210821 21:05"
        },
        {
          "id": "MDRW2100004819.1.15",
          "form": "그래도 다행이네요",
          "original_form": "그래도 다행이네요",
          "speaker_id": "2",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100004819.1.16",
          "form": "핸드폰 사용할 수 있다던데 연락 자주 되나요?",
          "original_form": "핸드폰 사용할 수 있다던데 연락 자주 되나요?",
          "speaker_id": "2",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100004819.1.17",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "1",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100004819.1.18",
          "form": "저녁시간에 문자정도는 가능하더라고요!",
          "original_form": "저녁시간에 문자정도는 가능하더라고요!",
          "speaker_id": "1",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100004819.1.19",
          "form": "평소엔 연락 잘 안 하는데 동생 군대가면 연락 자주할 거 같아요",
          "original_form": "평소엔 연락 잘 안 하는데 동생 군대가면 연락 자주할 거 같아요",
          "speaker_id": "2",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100004819.1.20",
          "form": "뭐 거의 저도 생사확인...",
          "original_form": "뭐 거의 저도 생사확인...",
          "speaker_id": "1",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100004819.1.21",
          "form": "아픈곳 없는지 정도 물어보는거 같아요",
          "original_form": "아픈곳 없는지 정도 물어보는거 같아요",
          "speaker_id": "1",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100004819.1.22",
          "form": "코로나도 걱정이네요",
          "original_form": "코로나도 걱정이네요",
          "speaker_id": "2",
          "time": "20210821 21:16"
        },
        {
          "id": "MDRW2100004819.1.23",
          "form": "군대는 단체 생활하니까",
          "original_form": "군대는 단체 생활하니까",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100004819.1.24",
          "form": "그쵸.. 그래서 벌써 2차까지 다 접종했더라고요.",
          "original_form": "그쵸.. 그래서 벌써 2차까지 다 접종했더라고요.",
          "speaker_id": "1",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100004819.1.25",
          "form": "휴가도 엄청 제한되고.",
          "original_form": "휴가도 엄청 제한되고.",
          "speaker_id": "1",
          "time": "20210821 21:19"
        },
        {
          "id": "MDRW2100004819.1.26",
          "form": "고생이 많으시네요ㅠㅠ",
          "original_form": "고생이 많으시네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 21:20"
        },
        {
          "id": "MDRW2100004819.1.27",
          "form": "이번에 그래서 첫휴가 나왔는데..",
          "original_form": "이번에 그래서 첫휴가 나왔는데..",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100004819.1.28",
          "form": "붙여나올수 있는 휴가 다 붙여서",
          "original_form": "붙여나올수 있는 휴가 다 붙여서",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100004819.1.29",
          "form": "2주넘게 나왔더라고요",
          "original_form": "2주넘게 나왔더라고요",
          "speaker_id": "1",
          "time": "20210821 21:21"
        },
        {
          "id": "MDRW2100004819.1.30",
          "form": "오랜만에 보셨겠어요",
          "original_form": "오랜만에 보셨겠어요",
          "speaker_id": "2",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100004819.1.31",
          "form": "건강하게 전역하셨으면 좋겠네요!",
          "original_form": "건강하게 전역하셨으면 좋겠네요!",
          "speaker_id": "2",
          "time": "20210821 21:22"
        },
        {
          "id": "MDRW2100004819.1.32",
          "form": "제발요.ㅠㅠㅠ",
          "original_form": "제발요.ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 21:23"
        }
      ]
    }
