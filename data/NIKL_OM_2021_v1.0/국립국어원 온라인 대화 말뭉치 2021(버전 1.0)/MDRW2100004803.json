      "utterance": [
        {
          "id": "MDRW2100004803.1.1",
          "form": "그거 최고지?",
          "original_form": "그거 최고지?",
          "speaker_id": "2",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004803.1.2",
          "form": "나도 잘 쓰고있어",
          "original_form": "나도 잘 쓰고있어",
          "speaker_id": "2",
          "time": "20210821 18:37"
        },
        {
          "id": "MDRW2100004803.1.3",
          "form": "그야말로 돈이 아깝지 않은 소비",
          "original_form": "그야말로 돈이 아깝지 않은 소비",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004803.1.4",
          "form": "백화점 가고싶어",
          "original_form": "백화점 가고싶어",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004803.1.5",
          "form": "아이쇼핑이라도 하고싶어",
          "original_form": "아이쇼핑이라도 하고싶어",
          "speaker_id": "2",
          "time": "20210821 18:38"
        },
        {
          "id": "MDRW2100004803.1.6",
          "form": "하늘에서 100만원이 갑자기 떨어지면 뭐 사고싶어?",
          "original_form": "하늘에서 100만원이 갑자기 떨어지면 뭐 사고싶어?",
          "speaker_id": "2",
          "time": "20210821 18:39"
        },
        {
          "id": "MDRW2100004803.1.7",
          "form": "100만원 너무 적어ㅠ",
          "original_form": "100만원 너무 적어ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004803.1.8",
          "form": "그럼 얼마?",
          "original_form": "그럼 얼마?",
          "speaker_id": "2",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004803.1.9",
          "form": "10만원짜리 10개사면 100만원이잖아ㅠ",
          "original_form": "10만원짜리 10개사면 100만원이잖아ㅠ",
          "speaker_id": "1",
          "time": "20210821 18:40"
        },
        {
          "id": "MDRW2100004803.1.10",
          "form": "맞는말이긴 한데....",
          "original_form": "맞는말이긴 한데....",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004803.1.11",
          "form": "적어도 10억은돼야지",
          "original_form": "적어도 10억은돼야지",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004803.1.12",
          "form": "왤케 웃기지",
          "original_form": "왤케 웃기지",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004803.1.13",
          "form": "아파트를 사지",
          "original_form": "아파트를 사지",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004803.1.14",
          "form": "그 돈 있음 아파트 쇼핑",
          "original_form": "그 돈 있음 아파트 쇼핑",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004803.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004803.1.16",
          "form": "아!",
          "original_form": "아!",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004803.1.17",
          "form": "내가 할 말이야",
          "original_form": "내가 할 말이야",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004803.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004803.1.19",
          "form": "아파트 \"쇼핑\"",
          "original_form": "아파트 \"쇼핑\"",
          "speaker_id": "2",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004803.1.20",
          "form": "아 너무 좋다",
          "original_form": "아 너무 좋다",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004803.1.21",
          "form": "아파트 쇼핑ㅠㅠ",
          "original_form": "아파트 쇼핑ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 18:41"
        },
        {
          "id": "MDRW2100004803.1.22",
          "form": "진작 이 주제할걸ㅋㅋ",
          "original_form": "진작 이 주제할걸ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:42"
        },
        {
          "id": "MDRW2100004803.1.23",
          "form": "술술 나오는 구만ㅋㅋ",
