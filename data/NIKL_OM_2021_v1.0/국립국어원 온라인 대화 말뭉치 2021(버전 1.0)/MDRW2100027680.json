      "utterance": [
        {
          "id": "MDRW2100027680.1.1",
          "form": "법원직은",
          "original_form": "법원직은",
          "speaker_id": "2",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027680.1.2",
          "form": "과목이 좀 많아요.. 8과목...",
          "original_form": "과목이 좀 많아요.. 8과목...",
          "speaker_id": "2",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027680.1.3",
          "form": "그래도 제가 보기엔 일행보단 차라리 나을 것 같은게",
          "original_form": "그래도 제가 보기엔 일행보단 차라리 나을 것 같은게",
          "speaker_id": "2",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027680.1.4",
          "form": "헉.....",
          "original_form": "헉.....",
          "speaker_id": "1",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027680.1.5",
          "form": "지엽적인 문제는 안 나온다더라고요",
          "original_form": "지엽적인 문제는 안 나온다더라고요",
          "speaker_id": "2",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027680.1.6",
          "form": "일행은 5과목인데 시험시간도 더 길겠네요?",
          "original_form": "일행은 5과목인데 시험시간도 더 길겠네요?",
          "speaker_id": "1",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027680.1.7",
          "form": "일행 합격하신 분들이 제일 신기해요",
          "original_form": "일행 합격하신 분들이 제일 신기해요",
          "speaker_id": "2",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027680.1.8",
          "form": "아예 다른 시험이라고 봐야되겠네요",
          "original_form": "아예 다른 시험이라고 봐야되겠네요",
          "speaker_id": "1",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027680.1.9",
          "form": "ㅋㅋㅋ맞아요 한국사특히!!!",
          "original_form": "ㅋㅋㅋ맞아요 한국사특히!!!",
          "speaker_id": "1",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027680.1.10",
          "form": "일행은 시험시간이 어떻게 되죠??",
          "original_form": "일행은 시험시간이 어떻게 되죠??",
          "speaker_id": "2",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027680.1.11",
          "form": "저희는 10시부터 4시까지 쳐요ㅎㅎㅎ",
          "original_form": "저희는 10시부터 4시까지 쳐요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 14:12"
        },
        {
          "id": "MDRW2100027680.1.12",
          "form": "1,2교시 나눠서",
          "original_form": "1,2교시 나눠서",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027680.1.13",
          "form": "일행우 걍 100분이던데",
          "original_form": "일행우 걍 100분이던데",
          "speaker_id": "1",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027680.1.14",
          "form": "한국사 문제 쪼잔하게 낸 것들 보면 진짜 어이가 없더라구요",
          "original_form": "한국사 문제 쪼잔하게 낸 것들 보면 진짜 어이가 없더라구요",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027680.1.15",
          "form": "법원직은 뭔가 전문적이네요",
          "original_form": "법원직은 뭔가 전문적이네요",
          "speaker_id": "1",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027680.1.16",
          "form": "와 그렇구나",
          "original_form": "와 그렇구나",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027680.1.17",
          "form": "ㅋㅋㅋ맞아요 이걸 맞추라고 낸건지",
          "original_form": "ㅋㅋㅋ맞아요 이걸 맞추라고 낸건지",
          "speaker_id": "1",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027680.1.18",
          "form": "주위에 유난히 9급된애들이많아요 그쪽과라서...",
          "original_form": "주위에 유난히 9급된애들이많아요 그쪽과라서...",
          "speaker_id": "1",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027680.1.19",
          "form": "노골적이죠 자 너를 떨어뜨릴거니까 풀어봐라! 하고",
          "original_form": "노골적이죠 자 너를 떨어뜨릴거니까 풀어봐라! 하고",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027680.1.20",
          "form": "6개월만에도 붙더라구요",
          "original_form": "6개월만에도 붙더라구요",
          "speaker_id": "1",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027680.1.21",
          "form": "와 대단하네요",
          "original_form": "와 대단하네요",
          "speaker_id": "2",
          "time": "20210902 14:13"
        },
        {
          "id": "MDRW2100027680.1.22",
          "form": "행정관련 학과 나오셨어요?",
          "original_form": "행정관련 학과 나오셨어요?",
          "speaker_id": "2",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027680.1.23",
          "form": "ㅋㅋㅋ맞아요 걍 이문제는 틀리고가자^^이런느낌?",
          "original_form": "ㅋㅋㅋ맞아요 걍 이문제는 틀리고가자^^이런느낌?",
          "speaker_id": "1",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027680.1.24",
          "form": "네네 본전공은 그쪽이에요!",
          "original_form": "네네 본전공은 그쪽이에요!",
          "speaker_id": "1",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027680.1.25",
          "form": "아하 그렇구나",
          "original_form": "아하 그렇구나",
          "speaker_id": "2",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027680.1.26",
          "form": "대단한 사람들 많네요",
          "original_form": "대단한 사람들 많네요",
          "speaker_id": "2",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027680.1.27",
          "form": "아무튼 법원직이",
          "original_form": "아무튼 법원직이",
          "speaker_id": "1",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027680.1.28",
          "form": "되고나면 훨씬",
          "original_form": "되고나면 훨씬",
          "speaker_id": "1",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027680.1.29",
          "form": "이득이 많겠어요!!!",
          "original_form": "이득이 많겠어요!!!",
          "speaker_id": "1",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027680.1.30",
          "form": "행정관련 학과면 회계하면서도 동시에 장점이 있겠네요ㅎㅎㅎ",
          "original_form": "행정관련 학과면 회계하면서도 동시에 장점이 있겠네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027680.1.31",
          "form": "화이팅입니닷",
          "original_form": "화이팅입니닷",
          "speaker_id": "1",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027680.1.32",
          "form": "네 그걸 노립니다",
          "original_form": "네 그걸 노립니다",
          "speaker_id": "2",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027680.1.33",
          "form": "감사해욧ㅜㅠ",
          "original_form": "감사해욧ㅜㅠ",
          "speaker_id": "2",
          "time": "20210902 14:14"
        },
        {
          "id": "MDRW2100027680.1.34",
          "form": "ㅋㅋㅋㅋ사실 둘이 연관은 없어요",
          "original_form": "ㅋㅋㅋㅋ사실 둘이 연관은 없어요",
          "speaker_id": "1",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027680.1.35",
          "form": "법원직은 시험이",
          "original_form": "법원직은 시험이",
          "speaker_id": "1",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027680.1.36",
          "form": "전공 살려서 못 가도",
          "original_form": "전공 살려서 못 가도",
          "speaker_id": "2",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027680.1.37",
          "form": "내년인가요?",
          "original_form": "내년인가요?",
          "speaker_id": "1",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027680.1.38",
          "form": "사실 생각해보면 은근히 써먹을 곳이 있더라고요",
          "original_form": "사실 생각해보면 은근히 써먹을 곳이 있더라고요",
          "speaker_id": "2",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027680.1.39",
          "form": "넵 내년 7월이예요",
          "original_form": "넵 내년 7월이예요",
          "speaker_id": "2",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027680.1.40",
          "form": "원래 2월 말에 치는 시험인데",
          "original_form": "원래 2월 말에 치는 시험인데",
          "speaker_id": "2",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027680.1.41",
          "form": "이번에 바껴서 7월애 치더라고요",
          "original_form": "이번에 바껴서 7월애 치더라고요",
          "speaker_id": "2",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027680.1.42",
          "form": "복수전공으로 경영햇어서 지금은 거기 소속감이 더 크네요ㅋㅋㅋ",
          "original_form": "복수전공으로 경영햇어서 지금은 거기 소속감이 더 크네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027680.1.43",
          "form": "오 일년에 딱 한번인가봐요",
          "original_form": "오 일년에 딱 한번인가봐요",
          "speaker_id": "1",
          "time": "20210902 14:15"
        },
        {
          "id": "MDRW2100027680.1.44",
          "form": "국가직 지방직 없이...?",
          "original_form": "국가직 지방직 없이...?",
          "speaker_id": "1",
          "time": "20210902 14:16"
        },
        {
          "id": "MDRW2100027680.1.45",
          "form": "name2은 잘되실거예요!!",
          "original_form": "&name2&은 잘되실거예요!!",
          "speaker_id": "1",
          "time": "20210902 14:16"
        },
