      "utterance": [
        {
          "id": "MDRW2100007213.1.1",
          "form": "저 이번에 주말에",
          "original_form": "저 이번에 주말에",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007213.1.2",
          "form": "언니들이랑 놀면서 배달음식 엄청시켰는데",
          "original_form": "언니들이랑 놀면서 배달음식 엄청시켰는데",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007213.1.3",
          "form": "삼겹살 구워주는 집에",
          "original_form": "삼겹살 구워주는 집에",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007213.1.4",
          "form": "삼겹살을 시켰거든요",
          "original_form": "삼겹살을 시켰거든요",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007213.1.5",
          "form": "근데 시킨 그람수가 걍 1키로인데",
          "original_form": "근데 시킨 그람수가 걍 1키로인데",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007213.1.6",
          "form": "고기가 너무 적어서 깜짝놀랐어요...",
          "original_form": "고기가 너무 적어서 깜짝놀랐어요...",
          "speaker_id": "2",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007213.1.7",
          "form": "아 거기 대형 체인점 말하는 거야?",
          "original_form": "아 거기 대형 체인점 말하는 거야?",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007213.1.8",
          "form": "체인점이 맞으면 나도 거기서",
          "original_form": "체인점이 맞으면 나도 거기서",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007213.1.9",
          "form": "여러번 시켜봤는데",
          "original_form": "여러번 시켜봤는데",
          "speaker_id": "1",
          "time": "20210822 00:54"
        },
        {
          "id": "MDRW2100007213.1.10",
          "form": "점바점이 심하더라",
          "original_form": "점바점이 심하더라",
          "speaker_id": "1",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007213.1.11",
          "form": "체인점인지 아닌지 모르겠는데",
          "original_form": "체인점인지 아닌지 모르겠는데",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007213.1.12",
          "form": "1키로 시켰으면",
          "original_form": "1키로 시켰으면",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007213.1.13",
          "form": "아니 실제 무게 1키로까진 안바라거든요",
          "original_form": "아니 실제 무게 1키로까진 안바라거든요",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007213.1.14",
          "form": "구우면 줄어드는거 알아서",
          "original_form": "구우면 줄어드는거 알아서",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007213.1.15",
          "form": "근데 내용물에 마늘 버섯 이런거 다 있으면서도",
          "original_form": "근데 내용물에 마늘 버섯 이런거 다 있으면서도",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007213.1.16",
          "form": "고기 갯수가 너무 적은 거예요...",
          "original_form": "고기 갯수가 너무 적은 거예요...",
          "speaker_id": "2",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007213.1.17",
          "form": "파채만 많이오고...",
          "original_form": "파채만 많이오고...",
          "speaker_id": "2",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007213.1.18",
          "form": "완전 꽝인 곳이었네",
          "original_form": "완전 꽝인 곳이었네",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007213.1.19",
          "form": "우리는 600그램에",
          "original_form": "우리는 600그램에",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007213.1.20",
          "form": "3명이 배터지게 먹었는데",
          "original_form": "3명이 배터지게 먹었는데",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007213.1.21",
          "form": "눈에 보이게 고기량이 적으면 화나지",
          "original_form": "눈에 보이게 고기량이 적으면 화나지",
          "speaker_id": "1",
          "time": "20210822 00:56"
        },
        {
          "id": "MDRW2100007213.1.22",
          "form": "1키로 시키면 솔직히",
          "original_form": "1키로 시키면 솔직히",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007213.1.23",
          "form": "4명이 먹고 배부르다못해",
          "original_form": "4명이 먹고 배부르다못해",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007213.1.24",
          "form": "남아야되잖아요",
          "original_form": "남아야되잖아요",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007213.1.25",
          "form": "심지어 그 넷중 둘이 0.5인분 먹는 사람들이라",
          "original_form": "심지어 그 넷중 둘이 0.5인분 먹는 사람들이라",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007213.1.26",
          "form": "실질적으로 3명 먹는 양으로 먹는데도",
          "original_form": "실질적으로 3명 먹는 양으로 먹는데도",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007213.1.27",
          "form": "부족했잖아요;;;",
          "original_form": "부족했잖아요;;;",
          "speaker_id": "2",
          "time": "20210822 00:57"
        },
        {
          "id": "MDRW2100007213.1.28",
          "form": "삼겹살 1인분은 200g으로 파는데",
          "original_form": "삼겹살 1인분은 200g으로 파는데",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007213.1.29",
          "form": "단순계산으로 5인분인걸",
          "original_form": "단순계산으로 5인분인걸",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007213.1.30",
          "form": "모자랐다니",
          "original_form": "모자랐다니",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007213.1.31",
          "form": "너무하다",
          "original_form": "너무하다",
          "speaker_id": "1",
          "time": "20210822 00:58"
        },
        {
          "id": "MDRW2100007213.1.32",
          "form": "그니까요 ㅠㅠ",
          "original_form": "그니까요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007213.1.33",
          "form": "근데 그래도 리뷰를 나쁘게 쓰긴 좀 그렇잖아요",
          "original_form": "근데 그래도 리뷰를 나쁘게 쓰긴 좀 그렇잖아요",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007213.1.34",
          "form": "그래서 그냥 맛있어요 ^^ 라고",
          "original_form": "그래서 그냥 맛있어요 ^^ 라고",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007213.1.35",
          "form": "엄청 짧게 남겼어요",
          "original_form": "엄청 짧게 남겼어요",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007213.1.36",
          "form": "저 원래 리뷰 2줄이상 써주는 편인데...ㅎㅎ",
          "original_form": "저 원래 리뷰 2줄이상 써주는 편인데...ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007213.1.37",
          "form": "ㅋㅋㅋㅋㅋ 와 그래도 리뷰를 매번 쓰는구나",
          "original_form": "ㅋㅋㅋㅋㅋ 와 그래도 리뷰를 매번 쓰는구나",
          "speaker_id": "1",
          "time": "20210822 01:04"
        },
        {
          "id": "MDRW2100007213.1.38",
          "form": "나는 리뷰 이벤트때에나 부지런하게 쓰고",
          "original_form": "나는 리뷰 이벤트때에나 부지런하게 쓰고",
          "speaker_id": "1",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007213.1.39",
          "form": "보통은 잘 안써서 ㅋㅋ",
          "original_form": "보통은 잘 안써서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007213.1.40",
          "form": "저도 리뷰 이벤트때만 써요 그런데",
          "original_form": "저도 리뷰 이벤트때만 써요 그런데",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007213.1.41",
          "form": "배달음식 요새는 리뷰이벤트 안하는 곳 없잖아요",
          "original_form": "배달음식 요새는 리뷰이벤트 안하는 곳 없잖아요",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007213.1.42",
          "form": "항상 리뷰이벤트 있나 확인하고 참여합니다 저는...",
          "original_form": "항상 리뷰이벤트 있나 확인하고 참여합니다 저는...",
          "speaker_id": "2",
          "time": "20210822 01:05"
        },
        {
          "id": "MDRW2100007213.1.43",
          "form": "ㅋㅋㅋㅋ 10곳중에 9곳은 한다고 봐야지",
          "original_form": "ㅋㅋㅋㅋ 10곳중에 9곳은 한다고 봐야지",
          "speaker_id": "1",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007213.1.44",
          "form": "하는 곳은 필참!",
          "original_form": "하는 곳은 필참!",
          "speaker_id": "1",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100007213.1.45",
          "form": "자주 주문하는 마라탕집 사장님도",
          "original_form": "자주 주문하는 마라탕집 사장님도",
          "speaker_id": "1",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007213.1.46",
          "form": "이벤트 해주시면 좋을텐데",
          "original_form": "이벤트 해주시면 좋을텐데",
          "speaker_id": "1",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100007213.1.47",
          "form": "오 거긴 이벤트없어요?",
          "original_form": "오 거긴 이벤트없어요?",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007213.1.48",
          "form": "저 마라탕 아직도 한번도 안먹어봤어요",
          "original_form": "저 마라탕 아직도 한번도 안먹어봤어요",
          "speaker_id": "2",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100007213.1.49",
          "form": "어 정말로?",
          "original_form": "어 정말로?",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007213.1.50",
          "form": "매운거 좋아하면 좋아할텐데",
          "original_form": "매운거 좋아하면 좋아할텐데",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007213.1.51",
          "form": "위장이 약하면 비추야",
          "original_form": "위장이 약하면 비추야",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100007213.1.52",
          "form": "그럼 비추  ...",
          "original_form": "그럼 비추  ...",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007213.1.53",
          "form": "마라탕말고 그거 뭐지 꿔바로우인가",
          "original_form": "마라탕말고 그거 뭐지 꿔바로우인가",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007213.1.54",
          "form": "그거는 먹어보고 싶긴해요",
          "original_form": "그거는 먹어보고 싶긴해요",
          "speaker_id": "2",
          "time": "20210822 01:14"
        },
        {
          "id": "MDRW2100007213.1.55",
          "form": "쫌득한 탕수육이라고 보면 될까?",
          "original_form": "쫌득한 탕수육이라고 보면 될까?",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007213.1.56",
          "form": "달콤한 튀김 싫어하면",
          "original_form": "달콤한 튀김 싫어하면",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007213.1.57",
          "form": "별로일 수도 있어",
          "original_form": "별로일 수도 있어",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100007213.1.58",
          "form": "전 좋아해요",
          "original_form": "전 좋아해요",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007213.1.59",
          "form": "달콤한튀김 좋아",
          "original_form": "달콤한튀김 좋아",
          "speaker_id": "2",
          "time": "20210822 01:21"
        },
        {
          "id": "MDRW2100007213.1.60",
          "form": "어 그래? 그럼 한번 쯤 먹을만 할거야",
          "original_form": "어 그래? 그럼 한번 쯤 먹을만 할거야",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007213.1.61",
          "form": "기회되면 친구랑 먹어봐 ㅋㅋ",
          "original_form": "기회되면 친구랑 먹어봐 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007213.1.62",
          "form": "기회되면 먹어봐야징",
          "original_form": "기회되면 먹어봐야징",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
