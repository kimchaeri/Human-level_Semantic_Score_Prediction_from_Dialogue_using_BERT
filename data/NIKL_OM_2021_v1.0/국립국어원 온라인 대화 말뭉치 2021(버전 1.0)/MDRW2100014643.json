      "utterance": [
        {
          "id": "MDRW2100014643.1.1",
          "form": "어색함 못견디는 쪽이 지는 게임=소갸팅",
          "original_form": "어색함 못견디는 쪽이 지는 게임=소갸팅",
          "speaker_id": "1",
          "time": "20210825 20:05"
        },
        {
          "id": "MDRW2100014643.1.2",
          "form": "앜ㅋㅋㅋㅋㄱㅋㄱㅋ진짜로요",
          "original_form": "앜ㅋㅋㅋㅋㄱㅋㄱㅋ진짜로요",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014643.1.3",
          "form": "전 항상 패배....^^",
          "original_form": "전 항상 패배....^^",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014643.1.4",
          "form": "휴 그래서 그 이후로는 소개팅은 절대안나기뤃ㅎ",
          "original_form": "휴 그래서 그 이후로는 소개팅은 절대안나기뤃ㅎ",
          "speaker_id": "2",
          "time": "20210825 20:07"
        },
        {
          "id": "MDRW2100014643.1.5",
          "form": "저도욬ㅋㅋㅋㅋ",
          "original_form": "저도욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014643.1.6",
          "form": "헛소리 작렬해요",
          "original_form": "헛소리 작렬해요",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014643.1.7",
          "form": "하핫 코끼리 똥은 물에 뜬다네요~",
          "original_form": "하핫 코끼리 똥은 물에 뜬다네요~",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014643.1.8",
          "form": "이런소리까지 나와요",
          "original_form": "이런소리까지 나와요",
          "speaker_id": "1",
          "time": "20210825 20:10"
        },
        {
          "id": "MDRW2100014643.1.9",
          "form": "아닠ㅋㅋㄱㅋㄱㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아닠ㅋㅋㄱㅋㄱㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014643.1.10",
          "form": "진짜 헛소리하게되는거",
          "original_form": "진짜 헛소리하게되는거",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014643.1.11",
          "form": "완전공감이여요ㅠㅠ",
          "original_form": "완전공감이여요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014643.1.12",
          "form": "휴 소개팅해서 잘되는분들",
          "original_form": "휴 소개팅해서 잘되는분들",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014643.1.13",
          "form": "넘넘 신기할지경ㅋㄱㅋ",
          "original_form": "넘넘 신기할지경ㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210825 20:11"
        },
        {
          "id": "MDRW2100014643.1.14",
          "form": "그니까욬ㅋㅋㅋ",
          "original_form": "그니까욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014643.1.15",
          "form": "선다방 보셨나요",
          "original_form": "선다방 보셨나요",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014643.1.16",
          "form": "그 소개팅하는 프로그럄ㅋㅋㅋㅋㅋ",
          "original_form": "그 소개팅하는 프로그럄ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014643.1.17",
          "form": "거기서 잘되시는 분들 넘 부러웜ㅅ어욬ㅋㅋㅋ",
          "original_form": "거기서 잘되시는 분들 넘 부러웜ㅅ어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:12"
        },
        {
          "id": "MDRW2100014643.1.18",
          "form": "오 네네 봤어요ㅋㅋㄱㅋㄱㅋ",
          "original_form": "오 네네 봤어요ㅋㅋㄱㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014643.1.19",
          "form": "근데 다들 뭔가 선남선녀에",
          "original_form": "근데 다들 뭔가 선남선녀에",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014643.1.20",
          "form": "직업이나 능력도 좋으시고 그래서",
          "original_form": "직업이나 능력도 좋으시고 그래서",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014643.1.21",
          "form": "그림같은 커플...?",
          "original_form": "그림같은 커플...?",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014643.1.22",
          "form": "그런거 보면 대리설렘 충족돼서 재밌어요",
          "original_form": "그런거 보면 대리설렘 충족돼서 재밌어요",
          "speaker_id": "2",
          "time": "20210825 20:14"
        },
        {
          "id": "MDRW2100014643.1.23",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014643.1.24",
          "form": "그리고 다들 조곤조곤하셔서",
          "original_form": "그리고 다들 조곤조곤하셔서",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014643.1.25",
          "form": "응원하게 됐었어요",
          "original_form": "응원하게 됐었어요",
          "speaker_id": "1",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014643.1.26",
          "form": "ㅋㅋㅋㅋㄱ맞아용 다들 수줍수줍 그런느낌!!",
          "original_form": "ㅋㅋㅋㅋㄱ맞아용 다들 수줍수줍 그런느낌!!",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014643.1.27",
          "form": "요즘은 연애 관련 예능도 엄청 많아지는것같아요",
          "original_form": "요즘은 연애 관련 예능도 엄청 많아지는것같아요",
          "speaker_id": "2",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014643.1.28",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014643.1.29",
          "form": "다들..하고싶은데 못만나서 그런가 코로나땜시",
          "original_form": "다들..하고싶은데 못만나서 그런가 코로나땜시",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014643.1.30",
          "form": "근데 만날사람은 만나더라고요",
          "original_form": "근데 만날사람은 만나더라고요",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014643.1.31",
          "form": "ㅋㅋㅋㅋㄲ맞아요 그런게 신기하다니까요",
          "original_form": "ㅋㅋㅋㅋㄲ맞아요 그런게 신기하다니까요",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014643.1.32",
          "form": "다들 어디서 그렇게 만나는건지,,,!",
          "original_form": "다들 어디서 그렇게 만나는건지,,,!",
          "speaker_id": "2",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014643.1.33",
          "form": "제가 아는 동생은",
          "original_form": "제가 아는 동생은",
          "speaker_id": "1",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014643.1.34",
          "form": "그 마스크 벗은 모습도 안보고 사겼다고 하더라고욬ㅋㅋㅋㅋ",
          "original_form": "그 마스크 벗은 모습도 안보고 사겼다고 하더라고욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014643.1.35",
          "form": "헉ㅋㅋㅋㅋ정말요?ㅋㅋㄱㄱㅋㄱㅋ",
          "original_form": "헉ㅋㅋㅋㅋ정말요?ㅋㅋㄱㄱㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014643.1.36",
          "form": "눈만보고도",
          "original_form": "눈만보고도",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014643.1.37",
          "form": "딱 느낌이 온건갘ㅋㄱㄱㅋ",
          "original_form": "딱 느낌이 온건갘ㅋㄱㄱㅋ",
          "speaker_id": "2",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014643.1.38",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "2",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014643.1.39",
          "form": "긍까욬ㅋㅋㅋㅋ 사귀고 마스크 벗은모습 첨봤대욬ㅋㅋㅋㅋ",
          "original_form": "긍까욬ㅋㅋㅋㅋ 사귀고 마스크 벗은모습 첨봤대욬ㅋㅋㅋㅋ",
