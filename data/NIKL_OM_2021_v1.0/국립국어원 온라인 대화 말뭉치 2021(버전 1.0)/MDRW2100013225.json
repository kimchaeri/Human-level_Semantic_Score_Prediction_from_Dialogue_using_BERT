      "utterance": [
        {
          "id": "MDRW2100013225.1.1",
          "form": "군대 다녀오셧나요?",
          "original_form": "군대 다녀오셧나요?",
          "speaker_id": "1",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100013225.1.2",
          "form": "넵넵",
          "original_form": "넵넵",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100013225.1.3",
          "form": "육군",
          "original_form": "육군",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100013225.1.4",
          "form": "다녀오셨어요?",
          "original_form": "다녀오셨어요?",
          "speaker_id": "2",
          "time": "20210825 13:35"
        },
        {
          "id": "MDRW2100013225.1.5",
          "form": "전 의경이요.. ㅋㅋㅋ \r\n예비군은요?",
          "original_form": "전 의경이요.. ㅋㅋㅋ \r\n예비군은요?",
          "speaker_id": "1",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100013225.1.6",
          "form": "저 민방위에요ㅎㅎ",
          "original_form": "저 민방위에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 13:36"
        },
        {
          "id": "MDRW2100013225.1.7",
          "form": "의경 힘드나요?",
          "original_form": "의경 힘드나요?",
          "speaker_id": "2",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100013225.1.8",
          "form": "전 옛날에 다녀와서.. \r\n가혹행위가 심했죠...",
          "original_form": "전 옛날에 다녀와서.. \r\n가혹행위가 심했죠...",
          "speaker_id": "1",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100013225.1.9",
          "form": "데모 막는거도 힘들고..",
          "original_form": "데모 막는거도 힘들고..",
          "speaker_id": "1",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100013225.1.10",
          "form": "아 그렇구나",
          "original_form": "아 그렇구나",
          "speaker_id": "2",
          "time": "20210825 13:37"
        },
        {
          "id": "MDRW2100013225.1.11",
          "form": "의경은 진짜 실전이겠다",
          "original_form": "의경은 진짜 실전이겠다",
          "speaker_id": "2",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100013225.1.12",
          "form": "가혹행위는 빨리 사라져야죠",
          "original_form": "가혹행위는 빨리 사라져야죠",
          "speaker_id": "2",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100013225.1.13",
          "form": "연락하고 지내는 동기나 선후임 있나요?",
          "original_form": "연락하고 지내는 동기나 선후임 있나요?",
          "speaker_id": "2",
          "time": "20210825 13:38"
        },
        {
          "id": "MDRW2100013225.1.14",
          "form": "당연이..",
          "original_form": "당연이..",
          "speaker_id": "1",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100013225.1.15",
          "form": "없죠.. ㅋㅋㅋㅋ \r\n의경은 서로 친해질수가 없죠..",
          "original_form": "없죠.. ㅋㅋㅋㅋ \r\n의경은 서로 친해질수가 없죠..",
          "speaker_id": "1",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100013225.1.16",
          "form": "육군은 끈끈하죠?",
          "original_form": "육군은 끈끈하죠?",
          "speaker_id": "1",
          "time": "20210825 13:39"
        },
        {
          "id": "MDRW2100013225.1.17",
          "form": "아뇨 안그래요",
          "original_form": "아뇨 안그래요",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013225.1.18",
          "form": "전역하고 후임 한명이 같은 회사다녀서",
          "original_form": "전역하고 후임 한명이 같은 회사다녀서",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013225.1.19",
          "form": "연락하고 지냈는데",
          "original_form": "연락하고 지냈는데",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013225.1.20",
          "form": "이직하고는 연락안해요",
          "original_form": "이직하고는 연락안해요",
          "speaker_id": "2",
          "time": "20210825 13:42"
        },
        {
          "id": "MDRW2100013225.1.21",
          "form": "어디서 근무하셨어요?",
          "original_form": "어디서 근무하셨어요?",
          "speaker_id": "2",
          "time": "20210825 13:43"
        },
        {
          "id": "MDRW2100013225.1.22",
          "form": "전 affiliation이요.. \r\n근데 의경시스템이 전국단위라.. \r\n일터지면 전국 다다니죠..",
          "original_form": "전 &affiliation&이요.. \r\n근데 의경시스템이 전국단위라.. \r\n일터지면 전국 다다니죠..",
          "speaker_id": "1",
          "time": "20210825 13:47"
        },
        {
          "id": "MDRW2100013225.1.23",
          "form": "아 그래요",
          "original_form": "아 그래요",
          "speaker_id": "2",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013225.1.24",
          "form": "저는 affiliation에서 복무했는데",
          "original_form": "저는 &affiliation&에서 복무했는데",
          "speaker_id": "2",
          "time": "20210825 13:51"
        },
        {
          "id": "MDRW2100013225.1.25",
          "form": "주변에 아무것도 없었죠",
          "original_form": "주변에 아무것도 없었죠",
          "speaker_id": "2",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013225.1.26",
          "form": "affiliation이면 affiliation 밑이요?",
          "original_form": "&affiliation&이면 &affiliation& 밑이요?",
          "speaker_id": "1",
          "time": "20210825 13:52"
        },
        {
          "id": "MDRW2100013225.1.27",
          "form": "네 affiliation 근처에요",
          "original_form": "네 &affiliation& 근처에요",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013225.1.28",
          "form": "아시네요",
          "original_form": "아시네요",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013225.1.29",
          "form": "군생활 좋은 추익 있나요?",
          "original_form": "군생활 좋은 추익 있나요?",
          "speaker_id": "2",
          "time": "20210825 13:53"
        },
        {
          "id": "MDRW2100013225.1.30",
          "form": "ㅋㅋㅋ 추억... \r\n음주단속하던거일까요? ㅋㅋ \r\n별로 좋은기억이..",
          "original_form": "ㅋㅋㅋ 추억... \r\n음주단속하던거일까요? ㅋㅋ \r\n별로 좋은기억이..",
          "speaker_id": "1",
          "time": "20210825 13:55"
        }
      ]
