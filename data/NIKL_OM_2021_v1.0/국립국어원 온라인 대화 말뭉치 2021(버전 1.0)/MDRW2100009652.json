      "utterance": [
        {
          "id": "MDRW2100009652.1.1",
          "form": "그러고보니 곧 개강시즌이네요!",
          "original_form": "그러고보니 곧 개강시즌이네요!",
          "speaker_id": "2",
          "time": "20210823 02:27"
        },
        {
          "id": "MDRW2100009652.1.2",
          "form": "오 그렇네요!",
          "original_form": "오 그렇네요!",
          "speaker_id": "1",
          "time": "20210823 02:28"
        },
        {
          "id": "MDRW2100009652.1.3",
          "form": "다들 비대면수업하겠죠?",
          "original_form": "다들 비대면수업하겠죠?",
          "speaker_id": "1",
          "time": "20210823 02:28"
        },
        {
          "id": "MDRW2100009652.1.4",
          "form": "그럴거같아요..! 이놈의 코로나ㅜ",
          "original_form": "그럴거같아요..! 이놈의 코로나ㅜ",
          "speaker_id": "2",
          "time": "20210823 02:29"
        },
        {
          "id": "MDRW2100009652.1.5",
          "form": "ㅋㅋㅋ큐ㅠㅜㅜ신입생들 학교도 못가구",
          "original_form": "ㅋㅋㅋ큐ㅠㅜㅜ신입생들 학교도 못가구",
          "speaker_id": "1",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009652.1.6",
          "form": "슬플 것 같아요...",
          "original_form": "슬플 것 같아요...",
          "speaker_id": "1",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009652.1.7",
          "form": "ㅋㅋ큐ㅠ",
          "original_form": "ㅋㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009652.1.8",
          "form": "학교 다니시면서",
          "original_form": "학교 다니시면서",
          "speaker_id": "2",
          "time": "20210823 02:30"
        },
        {
          "id": "MDRW2100009652.1.9",
          "form": "즐거웠던 추억 같은 거 있으세요?",
          "original_form": "즐거웠던 추억 같은 거 있으세요?",
          "speaker_id": "2",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009652.1.10",
          "form": "과에서 단체로 여행간게 재밌었어요ㅎㅎ",
          "original_form": "과에서 단체로 여행간게 재밌었어요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 02:31"
        },
        {
          "id": "MDRW2100009652.1.11",
          "form": "동아리같은거 해보셨나요?",
          "original_form": "동아리같은거 해보셨나요?",
          "speaker_id": "1",
          "time": "20210823 02:32"
        },
        {
          "id": "MDRW2100009652.1.12",
          "form": "아뇨ㅠㅠname1님은요??",
          "original_form": "아뇨ㅠㅠ&name1&님은요??",
          "speaker_id": "2",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009652.1.13",
          "form": "저도 안해봐가지고ㅜㅜ",
          "original_form": "저도 안해봐가지고ㅜㅜ",
          "speaker_id": "1",
          "time": "20210823 02:33"
        },
        {
          "id": "MDRW2100009652.1.14",
          "form": "그럼 학교 다니시면서 재밌게 들은 수업 있으신가요??",
          "original_form": "그럼 학교 다니시면서 재밌게 들은 수업 있으신가요??",
          "speaker_id": "2",
          "time": "20210823 02:35"
        },
        {
          "id": "MDRW2100009652.1.15",
          "form": "아 전 마케팅수업 재밌었어요!",
          "original_form": "아 전 마케팅수업 재밌었어요!",
          "speaker_id": "1",
          "time": "20210823 02:36"
        },
        {
          "id": "MDRW2100009652.1.16",
          "form": "어떤수업이 제일 힘드셨어요?",
          "original_form": "어떤수업이 제일 힘드셨어요?",
          "speaker_id": "1",
          "time": "20210823 02:37"
        },
        {
          "id": "MDRW2100009652.1.17",
          "form": "찬반토론수업이 힘들었어요..",
          "original_form": "찬반토론수업이 힘들었어요..",
          "speaker_id": "2",
          "time": "20210823 02:38"
        },
        {
          "id": "MDRW2100009652.1.18",
          "form": "헉 공감이에요",
          "original_form": "헉 공감이에요",
          "speaker_id": "1",
          "time": "20210823 02:38"
        },
        {
          "id": "MDRW2100009652.1.19",
          "form": "토론수업진짜 싫어요ㅠ",
          "original_form": "토론수업진짜 싫어요ㅠ",
          "speaker_id": "1",
          "time": "20210823 02:38"
        },
        {
          "id": "MDRW2100009652.1.20",
          "form": "진짜 매주 준비하는데 탈모 오는 줄 알았어요",
          "original_form": "진짜 매주 준비하는데 탈모 오는 줄 알았어요",
          "speaker_id": "2",
          "time": "20210823 02:40"
