      "utterance": [
        {
          "id": "MDRW2100033758.1.1",
          "form": "중식 좋아하세요?",
          "original_form": "중식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 00:42"
        },
        {
          "id": "MDRW2100033758.1.2",
          "form": "네 name1님은요?",
          "original_form": "네 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 00:43"
        },
        {
          "id": "MDRW2100033758.1.3",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 00:51"
        },
        {
          "id": "MDRW2100033758.1.4",
          "form": "짬뽕vs짜장?",
          "original_form": "짬뽕vs짜장?",
          "speaker_id": "1",
          "time": "20210825 00:51"
        },
        {
          "id": "MDRW2100033758.1.5",
          "form": "저는 짬뽕이요 name1님은요?",
          "original_form": "저는 짬뽕이요 &name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 01:00"
        },
        {
          "id": "MDRW2100033758.1.6",
          "form": "저랑 같네요ㅋㅋㅋㄱ",
          "original_form": "저랑 같네요ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100033758.1.7",
          "form": "전 국물을 포기하지못한답니다",
          "original_form": "전 국물을 포기하지못한답니다",
          "speaker_id": "1",
          "time": "20210825 01:02"
        },
        {
          "id": "MDRW2100033758.1.8",
          "form": "맞아요 전 어릴 땐 짜장이 좋았는데 커서는 국물있고 매콤한 짬뽕이 좋아지더라고요",
          "original_form": "맞아요 전 어릴 땐 짜장이 좋았는데 커서는 국물있고 매콤한 짬뽕이 좋아지더라고요",
          "speaker_id": "2",
          "time": "20210825 01:04"
        },
        {
          "id": "MDRW2100033758.1.9",
          "form": "ㅋㅋㅋㅋ 저랑같네요",
          "original_form": "ㅋㅋㅋㅋ 저랑같네요",
          "speaker_id": "1",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100033758.1.10",
          "form": "루트도요",
          "original_form": "루트도요",
          "speaker_id": "1",
          "time": "20210825 01:09"
        },
        {
          "id": "MDRW2100033758.1.11",
          "form": "아무래도 어렸을 땐 짜장의 단맛을 좋아했던 것 같아요",
          "original_form": "아무래도 어렸을 땐 짜장의 단맛을 좋아했던 것 같아요",
          "speaker_id": "2",
          "time": "20210825 01:12"
        },
        {
          "id": "MDRW2100033758.1.12",
          "form": "저도 크고나니",
          "original_form": "저도 크고나니",
          "speaker_id": "1",
          "time": "20210825 01:18"
        },
        {
          "id": "MDRW2100033758.1.13",
          "form": "짜장도 그냥은 안먹고",
          "original_form": "짜장도 그냥은 안먹고",
          "speaker_id": "1",
          "time": "20210825 01:18"
        },
        {
          "id": "MDRW2100033758.1.14",
          "form": "간짜장만 먹어요ㅋㅋㅋ",
          "original_form": "간짜장만 먹어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:18"
        },
        {
          "id": "MDRW2100033758.1.15",
          "form": "간짜장은 더 자극적인가요?",
          "original_form": "간짜장은 더 자극적인가요?",
          "speaker_id": "2",
          "time": "20210825 01:18"
        },
        {
          "id": "MDRW2100033758.1.16",
          "form": "제가알기리ㅡ",
          "original_form": "제가알기리ㅡ",
          "speaker_id": "1",
          "time": "20210825 01:21"
        },
        {
          "id": "MDRW2100033758.1.17",
          "form": "알기로",
          "original_form": "알기로",
          "speaker_id": "1",
          "time": "20210825 01:21"
        },
        {
          "id": "MDRW2100033758.1.18",
          "form": "간짜장은 그자리서 주문들어오면 바로볶는다더라고요",
          "original_form": "간짜장은 그자리서 주문들어오면 바로볶는다더라고요",
          "speaker_id": "1",
          "time": "20210825 01:22"
        },
        {
          "id": "MDRW2100033758.1.19",
          "form": "아 그렇군요 그럼 더 맛있겠네요",
          "original_form": "아 그렇군요 그럼 더 맛있겠네요",
          "speaker_id": "2",
          "time": "20210825 01:23"
        },
        {
          "id": "MDRW2100033758.1.20",
          "form": "맞아요ㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:27"
        },
        {
          "id": "MDRW2100033758.1.21",
          "form": "느낌상 좀 다르더라고요",
          "original_form": "느낌상 좀 다르더라고요",
          "speaker_id": "1",
          "time": "20210825 01:27"
        }
      ]
