      "utterance": [
        {
          "id": "MDRW2100010943.1.1",
          "form": "반려동물 기르세요?",
          "original_form": "반려동물 기르세요?",
          "speaker_id": "1",
          "time": "20210823 23:09"
        },
        {
          "id": "MDRW2100010943.1.2",
          "form": "아니요 못길러요ㅠㅠ",
          "original_form": "아니요 못길러요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 23:19"
        },
        {
          "id": "MDRW2100010943.1.3",
          "form": "헉 왜요",
          "original_form": "헉 왜요",
          "speaker_id": "1",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100010943.1.4",
          "form": "알러지 있으세요?",
          "original_form": "알러지 있으세요?",
          "speaker_id": "1",
          "time": "20210823 23:22"
        },
        {
          "id": "MDRW2100010943.1.5",
          "form": "집이 반대해서요",
          "original_form": "집이 반대해서요",
          "speaker_id": "2",
          "time": "20210823 23:28"
        },
        {
          "id": "MDRW2100010943.1.6",
          "form": "그래서 언젠가 독립하면 키우려고요!",
          "original_form": "그래서 언젠가 독립하면 키우려고요!",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010943.1.7",
          "form": "저도 지금은 없는데",
          "original_form": "저도 지금은 없는데",
          "speaker_id": "1",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100010943.1.8",
          "form": "예전에 키웠고 나중에 또 키울생각이 있어요",
          "original_form": "예전에 키웠고 나중에 또 키울생각이 있어요",
          "speaker_id": "1",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010943.1.9",
          "form": "유기견을 입양하고싶더라구요",
          "original_form": "유기견을 입양하고싶더라구요",
          "speaker_id": "1",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100010943.1.10",
          "form": "오 대단하세요",
          "original_form": "오 대단하세요",
          "speaker_id": "2",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100010943.1.11",
          "form": "저도 유기동물을 키울 생각인데",
          "original_form": "저도 유기동물을 키울 생각인데",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010943.1.12",
          "form": "파충류를 키워보려고요",
          "original_form": "파충류를 키워보려고요",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100010943.1.13",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010943.1.14",
          "form": "어떤 동물이요 ?!",
          "original_form": "어떤 동물이요 ?!",
          "speaker_id": "1",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010943.1.15",
          "form": "뱀이나 도마뱀 종류로요",
          "original_form": "뱀이나 도마뱀 종류로요",
          "speaker_id": "2",
          "time": "20210823 23:33"
        },
        {
          "id": "MDRW2100010943.1.16",
          "form": "파충류들이 반려수요는 있는데 환경이 너무 열악하더라고요ㅠ",
          "original_form": "파충류들이 반려수요는 있는데 환경이 너무 열악하더라고요ㅠ",
          "speaker_id": "2",
          "time": "20210823 23:34"
        },
        {
          "id": "MDRW2100010943.1.17",
          "form": "거기다 개나 고양이보다 상대적으로 관심이 적다보니 아무렇게나 버려지거나 죽는 경우가 많아서",
          "original_form": "거기다 개나 고양이보다 상대적으로 관심이 적다보니 아무렇게나 버려지거나 죽는 경우가 많아서",
          "speaker_id": "2",
          "time": "20210823 23:35"
        },
        {
          "id": "MDRW2100010943.1.18",
          "form": "알게됐으니 나라도 돌봐야겠다는 생각에",
          "original_form": "알게됐으니 나라도 돌봐야겠다는 생각에",
          "speaker_id": "2",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100010943.1.19",
          "form": "파충류를 반려동물로 들이겠다고 결심하게 됐습니다!",
          "original_form": "파충류를 반려동물로 들이겠다고 결심하게 됐습니다!",
          "speaker_id": "2",
          "time": "20210823 23:36"
        },
        {
          "id": "MDRW2100010943.1.20",
          "form": "와 진짜 너무 좋고 예쁜 마음이에요",
          "original_form": "와 진짜 너무 좋고 예쁜 마음이에요",
          "speaker_id": "1",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100010943.1.21",
          "form": "사랑 받아야할 아이들이 버려지는게 진짜 마음이 아파요..",
          "original_form": "사랑 받아야할 아이들이 버려지는게 진짜 마음이 아파요..",
          "speaker_id": "1",
          "time": "20210823 23:37"
        },
        {
          "id": "MDRW2100010943.1.22",
          "form": "조금만 더 책임감을 가지고 데려오면 좋을텐데",
          "original_form": "조금만 더 책임감을 가지고 데려오면 좋을텐데",
          "speaker_id": "1",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100010943.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100010943.1.24",
          "form": "전 유기견 보호소 봉사도 갔는데 진짜 맘이 아프더라구요",
          "original_form": "전 유기견 보호소 봉사도 갔는데 진짜 맘이 아프더라구요",
          "speaker_id": "1",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100010943.1.25",
          "form": "맞아요 애들이 사랑을 갈구하는게 보여요ㅠㅠㅠ",
          "original_form": "맞아요 애들이 사랑을 갈구하는게 보여요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100010943.1.26",
          "form": "저도 친구따라 갔는데 너무 안타깝더라고요ㅠㅠ",
          "original_form": "저도 친구따라 갔는데 너무 안타깝더라고요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 23:38"
        },
        {
          "id": "MDRW2100010943.1.27",
          "form": "ㅜㅜ 여건이 되면 당장이라도 데려오고 싶었는데",
          "original_form": "ㅜㅜ 여건이 되면 당장이라도 데려오고 싶었는데",
          "speaker_id": "1",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100010943.1.28",
          "form": "충분히 책임질수있을때 데려와야지 라는 마음으로",
          "original_form": "충분히 책임질수있을때 데려와야지 라는 마음으로",
          "speaker_id": "1",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100010943.1.29",
          "form": "통장에 총알을 열심히.. 채우려고해요 ㅋㅋㅋ",
          "original_form": "통장에 총알을 열심히.. 채우려고해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:40"
        },
        {
          "id": "MDRW2100010943.1.30",
          "form": "ㅋㅋㅋㅋ저도 적금 만들었어요",
          "original_form": "ㅋㅋㅋㅋ저도 적금 만들었어요",
          "speaker_id": "2",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100010943.1.31",
          "form": "파충류가 온습도를 따져서 필요한 장비가 은근 많더라고요",
          "original_form": "파충류가 온습도를 따져서 필요한 장비가 은근 많더라고요",
          "speaker_id": "2",
          "time": "20210823 23:42"
        },
        {
          "id": "MDRW2100010943.1.32",
          "form": "그래도 훗날을 기약하니 더 열심히 살게 되는거 같기도 해서",
          "original_form": "그래도 훗날을 기약하니 더 열심히 살게 되는거 같기도 해서",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100010943.1.33",
          "form": "준비기간도 좋은 거 같아요",
          "original_form": "준비기간도 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210823 23:44"
        },
        {
          "id": "MDRW2100010943.1.34",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100010943.1.35",
          "form": "주변에 아직 파충류 키우는 사람을 직접 본젇은 없는데",
          "original_form": "주변에 아직 파충류 키우는 사람을 직접 본젇은 없는데",
          "speaker_id": "1",
          "time": "20210823 23:55"
        },
        {
          "id": "MDRW2100010943.1.36",
          "form": "어디선가 도마뱀 뱀 이런 반려동물을",
          "original_form": "어디선가 도마뱀 뱀 이런 반려동물을",
          "speaker_id": "1",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100010943.1.37",
          "form": "사진 찍은거 올린걸봤는데 진짜 귀엽더라구요 ㅋㅋㅋ",
          "original_form": "사진 찍은거 올린걸봤는데 진짜 귀엽더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100010943.1.38",
          "form": "아 잘몰라서 사람들이 매력을 모르는거구나 싶었어요",
          "original_form": "아 잘몰라서 사람들이 매력을 모르는거구나 싶었어요",
          "speaker_id": "1",
          "time": "20210823 23:56"
        },
        {
          "id": "MDRW2100010943.1.39",
          "form": "맞아 반려뱀은 눈이 똘망똘망하구",
          "original_form": "맞아 반려뱀은 눈이 똘망똘망하구",
          "speaker_id": "2",
          "time": "20210823 23:59"
        },
        {
          "id": "MDRW2100010943.1.40",
          "form": "도마뱀은 똑똑해서 애교도 부리거든요!",
          "original_form": "도마뱀은 똑똑해서 애교도 부리거든요!",
          "speaker_id": "2",
          "time": "20210823 23:59"
        },
        {
          "id": "MDRW2100010943.1.41",
          "form": "너무 귀여워요!!!",
          "original_form": "너무 귀여워요!!!",
          "speaker_id": "2",
          "time": "20210823 23:59"
        },
        {
          "id": "MDRW2100010943.1.42",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100010943.1.43",
          "form": "게코도 종이 엄청 많던데",
          "original_form": "게코도 종이 엄청 많던데",
          "speaker_id": "1",
          "time": "20210824 00:00"
        },
        {
          "id": "MDRW2100010943.1.44",
          "form": "속눈썹도 귀엽더라구요 ㅋㅋㅋㅋ",
          "original_form": "속눈썹도 귀엽더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 00:01"
        },
        {
          "id": "MDRW2100010943.1.45",
          "form": "맞아요 그리고 탈피때가 되면",
          "original_form": "맞아요 그리고 탈피때가 되면",
          "speaker_id": "2",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100010943.1.46",
          "form": "얼굴부터 탈피가 서서히 되는데",
          "original_form": "얼굴부터 탈피가 서서히 되는데",
          "speaker_id": "2",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100010943.1.47",
          "form": "그게 꼭 후드티 입은거 같아서",
          "original_form": "그게 꼭 후드티 입은거 같아서",
          "speaker_id": "2",
          "time": "20210824 00:02"
        },
        {
          "id": "MDRW2100010943.1.48",
          "form": "그것도 너무 귀엽죠 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그것도 너무 귀엽죠 ㅋㅋㅋㅋㅋㅋㅋ",
