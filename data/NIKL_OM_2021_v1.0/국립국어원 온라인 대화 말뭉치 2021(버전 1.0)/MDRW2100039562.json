      "utterance": [
        {
          "id": "MDRW2100039562.1.1",
          "form": "별거 다하네요",
          "original_form": "별거 다하네요",
          "speaker_id": "2",
          "time": "20210903 19:08"
        },
        {
          "id": "MDRW2100039562.1.2",
          "form": "살인은 기본이고",
          "original_form": "살인은 기본이고",
          "speaker_id": "1",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100039562.1.3",
          "form": "네 어디까지 가죠?",
          "original_form": "네 어디까지 가죠?",
          "speaker_id": "2",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100039562.1.4",
          "form": "네 ㅋㅋ 부부간 성폭력에",
          "original_form": "네 ㅋㅋ 부부간 성폭력에",
          "speaker_id": "1",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100039562.1.5",
          "form": "흠.. 네...",
          "original_form": "흠.. 네...",
          "speaker_id": "2",
          "time": "20210903 19:09"
        },
        {
          "id": "MDRW2100039562.1.6",
          "form": "옥상에서 뛰어내리는거까지..",
          "original_form": "옥상에서 뛰어내리는거까지..",
          "speaker_id": "1",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039562.1.7",
          "form": "안나오는게 없네요",
          "original_form": "안나오는게 없네요",
          "speaker_id": "2",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039562.1.8",
          "form": "네 내용은 재밌는데",
          "original_form": "네 내용은 재밌는데",
          "speaker_id": "1",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039562.1.9",
          "form": "곁가지가 넘 불편해요",
          "original_form": "곁가지가 넘 불편해요",
          "speaker_id": "1",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039562.1.10",
          "form": "네 그렇게까지 내보내야 하는지..",
          "original_form": "네 그렇게까지 내보내야 하는지..",
          "speaker_id": "2",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039562.1.11",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039562.1.12",
          "form": "아 박시후도 나와요",
          "original_form": "아 박시후도 나와요",
          "speaker_id": "1",
          "time": "20210903 19:10"
        },
        {
          "id": "MDRW2100039562.1.13",
          "form": "아 그 사람 나오는군요",
          "original_form": "아 그 사람 나오는군요",
          "speaker_id": "2",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039562.1.14",
          "form": "네 원래 남주인데",
          "original_form": "네 원래 남주인데",
          "speaker_id": "1",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039562.1.15",
          "form": "점점 산으로 가는 듯한..",
          "original_form": "점점 산으로 가는 듯한..",
          "speaker_id": "1",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039562.1.16",
          "form": "전 안 볼래요",
          "original_form": "전 안 볼래요",
          "speaker_id": "2",
          "time": "20210903 19:11"
        },
        {
          "id": "MDRW2100039562.1.17",
          "form": "아니 저 드라마 바벨 보다가",
          "original_form": "아니 저 드라마 바벨 보다가",
          "speaker_id": "1",
          "time": "20210903 19:32"
        },
        {
          "id": "MDRW2100039562.1.18",
          "form": "지금 또 에바여서",
          "original_form": "지금 또 에바여서",
          "speaker_id": "1",
          "time": "20210903 19:32"
        },
        {
          "id": "MDRW2100039562.1.19",
          "form": "뛰어왔어요",
          "original_form": "뛰어왔어요",
          "speaker_id": "1",
          "time": "20210903 19:32"
        }
      ]
