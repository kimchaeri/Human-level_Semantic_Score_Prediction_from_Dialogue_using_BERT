      "utterance": [
        {
          "id": "MDRW2100035087.1.1",
          "form": "안녕하세용 어제 뭐 드셨어용?",
          "original_form": "안녕하세용 어제 뭐 드셨어용?",
          "speaker_id": "1",
          "time": "20210829 02:24"
        },
        {
          "id": "MDRW2100035087.1.2",
          "form": "안녕하세요 어제 치즈돈가스랑 쫄면 떡볶이먹었어요!",
          "original_form": "안녕하세요 어제 치즈돈가스랑 쫄면 떡볶이먹었어요!",
          "speaker_id": "2",
          "time": "20210829 02:24"
        },
        {
          "id": "MDRW2100035087.1.3",
          "form": "헉ㅠㅠ 맛잘알이시네요ㅋㅋㅋㅋ",
          "original_form": "헉ㅠㅠ 맛잘알이시네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:24"
        },
        {
          "id": "MDRW2100035087.1.4",
          "form": "저는 김치부침개 먹었어요!",
          "original_form": "저는 김치부침개 먹었어요!",
          "speaker_id": "1",
          "time": "20210829 02:24"
        },
        {
          "id": "MDRW2100035087.1.5",
          "form": "와! 직접하셨나요?",
          "original_form": "와! 직접하셨나요?",
          "speaker_id": "2",
          "time": "20210829 02:24"
        },
        {
          "id": "MDRW2100035087.1.6",
          "form": "전배달 ㅠㅠ ㅋㅋ",
          "original_form": "전배달 ㅠㅠ ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100035087.1.7",
          "form": "네네ㅋㅋㅋ 아하",
          "original_form": "네네ㅋㅋㅋ 아하",
          "speaker_id": "1",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100035087.1.8",
          "form": "치즈돈까스 배달해도 치즈 안굳어요?",
          "original_form": "치즈돈까스 배달해도 치즈 안굳어요?",
          "speaker_id": "1",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100035087.1.9",
          "form": "그쪽동네는 비가왔었나요?",
          "original_form": "그쪽동네는 비가왔었나요?",
          "speaker_id": "2",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100035087.1.10",
          "form": "하루종일 집에 있어서 정확하진 않지만",
          "original_form": "하루종일 집에 있어서 정확하진 않지만",
          "speaker_id": "1",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100035087.1.11",
          "form": "처음시켜본곳이엿는데",
          "original_form": "처음시켜본곳이엿는데",
          "speaker_id": "2",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100035087.1.12",
          "form": "그런것같아요!",
          "original_form": "그런것같아요!",
          "speaker_id": "1",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100035087.1.13",
          "form": "그래서 부침개가 땡겨서ㅋㅋㅋ",
          "original_form": "그래서 부침개가 땡겨서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:25"
        },
        {
          "id": "MDRW2100035087.1.14",
          "form": "처음엔 그래도 괜찮다가 먹다보니 굳긴하더라구요! 치즈롤가스였어요!",
          "original_form": "처음엔 그래도 괜찮다가 먹다보니 굳긴하더라구요! 치즈롤가스였어요!",
          "speaker_id": "2",
          "time": "20210829 02:26"
        },
        {
          "id": "MDRW2100035087.1.15",
          "form": "요즘 배달음식 너무 잘돼있어서 저도 종종 시켜먹어용ㅋㅋㅋ",
          "original_form": "요즘 배달음식 너무 잘돼있어서 저도 종종 시켜먹어용ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:26"
        },
        {
          "id": "MDRW2100035087.1.16",
          "form": "치즈롤 와아",
          "original_form": "치즈롤 와아",
          "speaker_id": "1",
          "time": "20210829 02:26"
        },
        {
          "id": "MDRW2100035087.1.17",
          "form": "근데 주문하고 30분도 안되서 와서 완성하자마자엄청빨리배달이온거같아요 ㅎ",
          "original_form": "근데 주문하고 30분도 안되서 와서 완성하자마자엄청빨리배달이온거같아요 ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:26"
        },
        {
          "id": "MDRW2100035087.1.18",
          "form": "아아 그러네용",
          "original_form": "아아 그러네용",
          "speaker_id": "1",
          "time": "20210829 02:26"
        },
        {
          "id": "MDRW2100035087.1.19",
          "form": "매장도 가까운곳은아닌고같았는데!!",
          "original_form": "매장도 가까운곳은아닌고같았는데!!",
          "speaker_id": "2",
          "time": "20210829 02:26"
        },
        {
          "id": "MDRW2100035087.1.20",
          "form": "요즘 배달료가 많이 비싸죵?ㅋㅋㅋㅌ",
          "original_form": "요즘 배달료가 많이 비싸죵?ㅋㅋㅋㅌ",
          "speaker_id": "1",
          "time": "20210829 02:27"
        },
        {
          "id": "MDRW2100035087.1.21",
          "form": "착착착 ㅎㅎ 이른저녁이라 5시쯤",
          "original_form": "착착착 ㅎㅎ 이른저녁이라 5시쯤",
          "speaker_id": "2",
          "time": "20210829 02:27"
        },
        {
          "id": "MDRW2100035087.1.22",
          "form": "그래서 배달료 아까워서 더 시켜요ㅋㅋ",
          "original_form": "그래서 배달료 아까워서 더 시켜요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:27"
        },
        {
          "id": "MDRW2100035087.1.23",
          "form": "맞아요ㅠ원래없던곳도 생겼더라구요ㅠㅠ",
          "original_form": "맞아요ㅠ원래없던곳도 생겼더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 02:27"
        },
        {
          "id": "MDRW2100035087.1.24",
          "form": "마자요 ㅎㅎㅎㅎㅎ",
          "original_form": "마자요 ㅎㅎㅎㅎㅎ",
