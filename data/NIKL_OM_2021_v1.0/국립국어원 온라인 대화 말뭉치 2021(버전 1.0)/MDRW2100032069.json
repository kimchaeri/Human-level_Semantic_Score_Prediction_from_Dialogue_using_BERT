      "utterance": [
        {
          "id": "MDRW2100032069.1.1",
          "form": "역시 음식 이야기가 짱이죠",
          "original_form": "역시 음식 이야기가 짱이죠",
          "speaker_id": "2",
          "time": "20210905 11:51"
        },
        {
          "id": "MDRW2100032069.1.2",
          "form": "방탄에서 하는 말인데요 혹시 방탄 커피 먹어 보셨나요",
          "original_form": "방탄에서 하는 말인데요 혹시 방탄 커피 먹어 보셨나요",
          "speaker_id": "1",
          "time": "20210905 11:51"
        },
        {
          "id": "MDRW2100032069.1.3",
          "form": "살 빼려고 했죠",
          "original_form": "살 빼려고 했죠",
          "speaker_id": "2",
          "time": "20210905 11:51"
        },
        {
          "id": "MDRW2100032069.1.4",
          "form": "방탄 커피 너무 궁금해요 어떤 맛이죠",
          "original_form": "방탄 커피 너무 궁금해요 어떤 맛이죠",
          "speaker_id": "1",
          "time": "20210905 11:51"
        },
        {
          "id": "MDRW2100032069.1.5",
          "form": "포만감이 있더라고요",
          "original_form": "포만감이 있더라고요",
          "speaker_id": "2",
          "time": "20210905 11:51"
        },
        {
          "id": "MDRW2100032069.1.6",
          "form": "거기에 오일 들어가는 건가요",
          "original_form": "거기에 오일 들어가는 건가요",
          "speaker_id": "1",
          "time": "20210905 11:51"
        },
        {
          "id": "MDRW2100032069.1.7",
          "form": "호불호가 심하다고 들었거든요",
          "original_form": "호불호가 심하다고 들었거든요",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100032069.1.8",
          "form": "버터 커피 코코넛 오일",
          "original_form": "버터 커피 코코넛 오일",
          "speaker_id": "2",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100032069.1.9",
          "form": "좋아하는 사람은 엄청 좋아하는데 싫어하는 사람은 못 먹더라고요",
          "original_form": "좋아하는 사람은 엄청 좋아하는데 싫어하는 사람은 못 먹더라고요",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100032069.1.10",
          "form": "근데 생각보다는 맛이 고소해요",
          "original_form": "근데 생각보다는 맛이 고소해요",
          "speaker_id": "2",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100032069.1.11",
          "form": "직접 만들어서 드신 거예요 사서 드신 거예요",
          "original_form": "직접 만들어서 드신 거예요 사서 드신 거예요",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100032069.1.12",
          "form": "하지만 그것만 마시니까 마음이 허전하다고 할까요",
          "original_form": "하지만 그것만 마시니까 마음이 허전하다고 할까요",
          "speaker_id": "2",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100032069.1.13",
          "form": "뭔가 더 먹어야 될 거 같은 느낌",
          "original_form": "뭔가 더 먹어야 될 거 같은 느낌",
          "speaker_id": "2",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100032069.1.14",
          "form": "아까 분명 다이어트 용이라고 들었던 거 같은데요",
          "original_form": "아까 분명 다이어트 용이라고 들었던 거 같은데요",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100032069.1.15",
          "form": "일할 때는 아침에 귀찮으니까 그걸 먹었어요",
          "original_form": "일할 때는 아침에 귀찮으니까 그걸 먹었어요",
          "speaker_id": "2",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100032069.1.16",
          "form": "포만감이 큰가요 저는 포만감 보다는 맛이 궁금하네요",
          "original_form": "포만감이 큰가요 저는 포만감 보다는 맛이 궁금하네요",
          "speaker_id": "1",
          "time": "20210905 11:52"
        },
        {
          "id": "MDRW2100032069.1.17",
          "form": "베트남에서는 코코넛 오일 버터 흔하거든요",
          "original_form": "베트남에서는 코코넛 오일 버터 흔하거든요",
          "speaker_id": "2",
          "time": "20210905 11:53"
        },
        {
          "id": "MDRW2100032069.1.18",
          "form": "역시 먹는 거 얘기하는게 짱입니다",
          "original_form": "역시 먹는 거 얘기하는게 짱입니다",
          "speaker_id": "1",
