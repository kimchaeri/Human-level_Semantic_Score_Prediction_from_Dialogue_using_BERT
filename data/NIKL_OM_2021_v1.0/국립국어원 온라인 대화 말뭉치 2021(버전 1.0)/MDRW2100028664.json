        },
        {
          "id": "MDRW2100028664.1.2",
          "form": "불매각이네",
          "original_form": "불매각이네",
          "speaker_id": "1",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028664.1.3",
          "form": "암튼 그래서 ㅇㅇ픽",
          "original_form": "암튼 그래서 ㅇㅇ픽",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028664.1.4",
          "form": "이러면서 유투버얼굴걸리는거에",
          "original_form": "이러면서 유투버얼굴걸리는거에",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028664.1.5",
          "form": "걔이름써있더라고",
          "original_form": "걔이름써있더라고",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028664.1.6",
          "form": "오늘 그래서 난리남",
          "original_form": "오늘 그래서 난리남",
          "speaker_id": "2",
          "time": "20210902 21:15"
        },
        {
          "id": "MDRW2100028664.1.7",
          "form": "아니 회사는 무슨 정신으로.... 모델을 그런 사람으로",
          "original_form": "아니 회사는 무슨 정신으로.... 모델을 그런 사람으로",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028664.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ화장품 팔기 싫은가",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ화장품 팔기 싫은가",
          "speaker_id": "1",
          "time": "20210902 21:16"
        },
        {
          "id": "MDRW2100028664.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ진짜..... ㄹㅇ 요샌 유투ㅂ가 더 영향력이으니까 화장품은...",
          "original_form": "ㅋㅋㅋㅋㅋㅋ진짜..... ㄹㅇ 요샌 유투ㅂ가 더 영향력이으니까 화장품은...",
          "speaker_id": "2",
          "time": "20210902 21:17"
        },
        {
          "id": "MDRW2100028664.1.10",
          "form": "근데 대기업이 그런것도 안보고 모델을 시키나....",
          "original_form": "근데 대기업이 그런것도 안보고 모델을 시키나....",
          "speaker_id": "2",
          "time": "20210902 21:18"
        },
        {
          "id": "MDRW2100028664.1.11",
          "form": "진짜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ보통 커뮤도 아닉ᆢ",
          "original_form": "진짜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ보통 커뮤도 아닉ᆢ",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028664.1.12",
          "form": "아니고",
          "original_form": "아니고",
          "speaker_id": "1",
          "time": "20210902 21:19"
        },
        {
          "id": "MDRW2100028664.1.13",
          "form": "1베를 모델로ㅜㅜ",
          "original_form": "1베를 모델로ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
          "id": "MDRW2100028664.1.14",
          "form": "심지어 화장품 모델이라니",
          "original_form": "심지어 화장품 모델이라니",
          "speaker_id": "1",
          "time": "20210902 21:20"
        },
        {
