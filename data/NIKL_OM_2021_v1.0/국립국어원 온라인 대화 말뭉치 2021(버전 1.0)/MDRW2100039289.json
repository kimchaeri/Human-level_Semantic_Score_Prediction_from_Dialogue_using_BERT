      "utterance": [
        {
          "id": "MDRW2100039289.1.1",
          "form": "박스채로 사네 마네 하고",
          "original_form": "박스채로 사네 마네 하고",
          "speaker_id": "1",
          "time": "20210903 15:43"
        },
        {
          "id": "MDRW2100039289.1.2",
          "form": "그렇게까지 맛있었나?? 하고 생각해보면",
          "original_form": "그렇게까지 맛있었나?? 하고 생각해보면",
          "speaker_id": "2",
          "time": "20210903 15:47"
        },
        {
          "id": "MDRW2100039289.1.3",
          "form": "음.. 센세이션하긴 했죠",
          "original_form": "음.. 센세이션하긴 했죠",
          "speaker_id": "2",
          "time": "20210903 15:47"
        },
        {
          "id": "MDRW2100039289.1.4",
          "form": "허니버터 아몬드 여전히 좋아합니다",
          "original_form": "허니버터 아몬드 여전히 좋아합니다",
          "speaker_id": "2",
          "time": "20210903 15:48"
        },
        {
          "id": "MDRW2100039289.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:48"
        },
        {
          "id": "MDRW2100039289.1.6",
          "form": "맞아요 맛은 인정이에요",
          "original_form": "맞아요 맛은 인정이에요",
          "speaker_id": "1",
          "time": "20210903 15:49"
        },
        {
          "id": "MDRW2100039289.1.7",
          "form": "요즘은 밖에 못 나가서 그런지 어디서 줄 서서 샀단 얘기 안 들리네요",
          "original_form": "요즘은 밖에 못 나가서 그런지 어디서 줄 서서 샀단 얘기 안 들리네요",
          "speaker_id": "1",
          "time": "20210903 15:49"
        },
        {
          "id": "MDRW2100039289.1.8",
          "form": "큽... 그냥 이제는 편하게 사는? 편의점에 잘 있더라구요ㅋㅋ",
          "original_form": "큽... 그냥 이제는 편하게 사는? 편의점에 잘 있더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100039289.1.9",
          "form": "결국 공장 증설 했댔어요!",
          "original_form": "결국 공장 증설 했댔어요!",
          "speaker_id": "2",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100039289.1.10",
          "form": "공급이 늘어났군요ㅋㅋㅋㅋㅋ",
          "original_form": "공급이 늘어났군요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100039289.1.11",
          "form": "매출 보니까 그럴만하긴 했어요",
          "original_form": "매출 보니까 그럴만하긴 했어요",
          "speaker_id": "1",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100039289.1.12",
          "form": "해태의 빛나는 라이징 상품..ㅋㅋㅋㅋ",
          "original_form": "해태의 빛나는 라이징 상품..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100039289.1.13",
          "form": "얘기하니까 오랜만에 먹고싶어지는거 같아요",
          "original_form": "얘기하니까 오랜만에 먹고싶어지는거 같아요",
          "speaker_id": "2",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100039289.1.14",
          "form": "집갈때 한봉지 사가야지",
          "original_form": "집갈때 한봉지 사가야지",
          "speaker_id": "2",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100039289.1.15",
          "form": "감자칩 좋아하세요?",
          "original_form": "감자칩 좋아하세요?",
          "speaker_id": "1",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100039289.1.16",
          "form": "감자칩종류에서는 감자칩>스윙칩>>>>프링글스",
          "original_form": "감자칩종류에서는 감자칩>스윙칩>>>>프링글스",
          "speaker_id": "2",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100039289.1.17",
          "form": "로 좋아합니다",
          "original_form": "로 좋아합니다",
          "speaker_id": "2",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100039289.1.18",
          "form": "저도 감자칩 젤 좋아해요",
          "original_form": "저도 감자칩 젤 좋아해요",
          "speaker_id": "1",
          "time": "20210903 15:54"
        },
        {
          "id": "MDRW2100039289.1.19",
          "form": "그리고 눈을감자 좋아했는데",
          "original_form": "그리고 눈을감자 좋아했는데",
          "speaker_id": "1",
          "time": "20210903 15:54"
        },
        {
          "id": "MDRW2100039289.1.20",
          "form": "요즘 안 보이더라구요",
          "original_form": "요즘 안 보이더라구요",
          "speaker_id": "1",
          "time": "20210903 15:54"
        },
        {
          "id": "MDRW2100039289.1.21",
          "form": "앗 걔는 뭐더라.. 진짜 감자맛 나는!",
          "original_form": "앗 걔는 뭐더라.. 진짜 감자맛 나는!",
          "speaker_id": "2",
          "time": "20210903 15:56"
        },
        {
          "id": "MDRW2100039289.1.22",
          "form": "걔 맞죠? 감자칩은 약간 소금맛으로 먹는 기분이구",
          "original_form": "걔 맞죠? 감자칩은 약간 소금맛으로 먹는 기분이구",
          "speaker_id": "2",
          "time": "20210903 15:56"
        },
        {
          "id": "MDRW2100039289.1.23",
          "form": "정확히는 칩 모양은 아니긴 해요 빼빼로처럼 생겼어요",
          "original_form": "정확히는 칩 모양은 아니긴 해요 빼빼로처럼 생겼어요",
          "speaker_id": "1",
          "time": "20210903 15:57"
        },
        {
          "id": "MDRW2100039289.1.24",
          "form": "음.. 빼빼로 모양은 구운감자밖에 몰라요",
          "original_form": "음.. 빼빼로 모양은 구운감자밖에 몰라요",
          "speaker_id": "2",
          "time": "20210903 16:00"
        },
        {
          "id": "MDRW2100039289.1.25",
          "form": "아직도 못먹어본 과자가 있다니",
          "original_form": "아직도 못먹어본 과자가 있다니",
          "speaker_id": "2",
          "time": "20210903 16:00"
        },
        {
          "id": "MDRW2100039289.1.26",
          "form": "신제품 나오면 찾아 드시나봐요",
          "original_form": "신제품 나오면 찾아 드시나봐요",
          "speaker_id": "1",
          "time": "20210903 16:01"
        },
        {
          "id": "MDRW2100039289.1.27",
          "form": "제 기억이 가물거려서 이름이 틀렸을 수도 있어요",
          "original_form": "제 기억이 가물거려서 이름이 틀렸을 수도 있어요",
          "speaker_id": "1",
          "time": "20210903 16:01"
        },
        {
          "id": "MDRW2100039289.1.28",
          "form": "그런 수준은 아닌데 그냥.. 처음보는 과자면",
          "original_form": "그런 수준은 아닌데 그냥.. 처음보는 과자면",
          "speaker_id": "2",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100039289.1.29",
          "form": "한번은 집어보는?",
          "original_form": "한번은 집어보는?",
          "speaker_id": "2",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100039289.1.30",
          "form": "단 포장에서 너무 싸구려티가 나면 안집는 모땐사람입니다",
          "original_form": "단 포장에서 너무 싸구려티가 나면 안집는 모땐사람입니다",
          "speaker_id": "2",
          "time": "20210903 16:09"
        },
        {
          "id": "MDRW2100039289.1.31",
          "form": "전 과자는 먹는 것만 먹게 되더라구요",
          "original_form": "전 과자는 먹는 것만 먹게 되더라구요",
          "speaker_id": "1",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100039289.1.32",
          "form": "음식은 이거저거 시키는데....",
          "original_form": "음식은 이거저거 시키는데....",
          "speaker_id": "1",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100039289.1.33",
          "form": "인증된.. 안정된 맛..",
          "original_form": "인증된.. 안정된 맛..",
          "speaker_id": "2",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100039289.1.34",
          "form": "최근에 친구가 사와서 콰삭칩? 사왔는데 맛있었어요",
          "original_form": "최근에 친구가 사와서 콰삭칩? 사왔는데 맛있었어요",
          "speaker_id": "1",
          "time": "20210903 16:15"
        },
        {
          "id": "MDRW2100039289.1.35",
          "form": "저.. 과자알못인걸로..ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "저.. 과자알못인걸로..ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100039289.1.36",
          "form": "걔는 뭐에요?!?!",
          "original_form": "걔는 뭐에요?!?!",
          "speaker_id": "2",
          "time": "20210903 16:23"
        },
        {
          "id": "MDRW2100039289.1.37",
          "form": "포카칩 종류인지 콜라보인지 그런애 같았어요",
          "original_form": "포카칩 종류인지 콜라보인지 그런애 같았어요",
          "speaker_id": "1",
          "time": "20210903 16:25"
        },
        {
          "id": "MDRW2100039289.1.38",
          "form": "근데 포카칩보다 얇은 것 같고 더 구깃구깃하게 있는? 모양이에요",
          "original_form": "근데 포카칩보다 얇은 것 같고 더 구깃구깃하게 있는? 모양이에요",
          "speaker_id": "1",
          "time": "20210903 16:25"
        },
        {
          "id": "MDRW2100039289.1.39",
          "form": "세상에 과자가 너무 많다",
          "original_form": "세상에 과자가 너무 많다",
          "speaker_id": "2",
          "time": "20210903 16:29"
        },
        {
          "id": "MDRW2100039289.1.40",
          "form": "전 최근에 본 신제품?과자는",
          "original_form": "전 최근에 본 신제품?과자는",
          "speaker_id": "2",
          "time": "20210903 16:29"
        },
        {
