      "utterance": [
        {
          "id": "MDRW2100021995.1.1",
          "form": "츄츄족은 대체 뭘까",
          "original_form": "츄츄족은 대체 뭘까",
          "speaker_id": "2",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021995.1.2",
          "form": "얘네가",
          "original_form": "얘네가",
          "speaker_id": "2",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021995.1.3",
          "form": "그걸 생각 안했을것같진 않은데",
          "original_form": "그걸 생각 안했을것같진 않은데",
          "speaker_id": "2",
          "time": "20210829 00:31"
        },
        {
          "id": "MDRW2100021995.1.4",
          "form": "츄츄족은",
          "original_form": "츄츄족은",
          "speaker_id": "1",
          "time": "20210829 00:34"
        },
        {
          "id": "MDRW2100021995.1.5",
          "form": "멸망한 문명의 사람들",
          "original_form": "멸망한 문명의 사람들",
          "speaker_id": "1",
          "time": "20210829 00:34"
        },
        {
          "id": "MDRW2100021995.1.6",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021995.1.7",
          "form": "애완....?",
          "original_form": "애완....?",
          "speaker_id": "2",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021995.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021995.1.9",
          "form": "걍",
          "original_form": "걍",
          "speaker_id": "1",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021995.1.10",
          "form": "우쿠라고만 나와서",
          "original_form": "우쿠라고만 나와서",
          "speaker_id": "1",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021995.1.11",
          "form": "노예...?",
          "original_form": "노예...?",
          "speaker_id": "2",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021995.1.12",
          "form": "애완인간.....?",
          "original_form": "애완인간.....?",
          "speaker_id": "2",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021995.1.13",
          "form": "애완동물인지 호위기사인지는 모름",
          "original_form": "애완동물인지 호위기사인지는 모름",
          "speaker_id": "1",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021995.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021995.1.15",
          "form": "ㅋㅋㅋㄱㄲㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㄱㄲㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021995.1.16",
          "form": "잰장",
          "original_form": "잰장",
          "speaker_id": "2",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021995.1.17",
          "form": "어렵다 증말",
          "original_form": "어렵다 증말",
          "speaker_id": "2",
          "time": "20210829 00:35"
        },
        {
          "id": "MDRW2100021995.1.18",
          "form": "타타라스나 진행하고 있는데",
          "original_form": "타타라스나 진행하고 있는데",
          "speaker_id": "2",
          "time": "20210829 14:54"
        },
        {
          "id": "MDRW2100021995.1.19",
          "form": "아직까진 별게 없네",
          "original_form": "아직까진 별게 없네",
          "speaker_id": "2",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100021995.1.20",
          "form": "걍 비콘 설치하고왔어",
          "original_form": "걍 비콘 설치하고왔어",
          "speaker_id": "2",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100021995.1.21",
          "form": "걍",
          "original_form": "걍",
          "speaker_id": "2",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100021995.1.22",
          "form": "1.우인단이 또",
          "original_form": "1.우인단이 또",
          "speaker_id": "2",
          "time": "20210829 14:55"
        },
        {
          "id": "MDRW2100021995.1.23",
          "form": "2.근데 범인이 우인단이라 그런가 안 음침하고 걍 평범하게 음모를 꾸미는 우인단",
          "original_form": "2.근데 범인이 우인단이라 그런가 안 음침하고 걍 평범하게 음모를 꾸미는 우인단",
          "speaker_id": "2",
          "time": "20210829 14:56"
        },
        {
          "id": "MDRW2100021995.1.24",
          "form": "정도?",
          "original_form": "정도?",
          "speaker_id": "2",
          "time": "20210829 14:56"
        },
        {
          "id": "MDRW2100021995.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅇㅇ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅇㅇ",
          "speaker_id": "1",
          "time": "20210829 15:03"
        },
        {
          "id": "MDRW2100021995.1.26",
          "form": "솔짇히",
          "original_form": "솔짇히",
          "speaker_id": "1",
          "time": "20210829 15:03"
        },
        {
          "id": "MDRW2100021995.1.27",
          "form": "너무 우인단이 범인이라",
          "original_form": "너무 우인단이 범인이라",
          "speaker_id": "1",
          "time": "20210829 15:03"
        },
        {
          "id": "MDRW2100021995.1.28",
          "form": "새로운 맛이 없을 정도...",
          "original_form": "새로운 맛이 없을 정도...",
          "speaker_id": "1",
          "time": "20210829 15:03"
        },
        {
          "id": "MDRW2100021995.1.29",
          "form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래도",
          "original_form": "ㅇㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그래도",
          "speaker_id": "2",
          "time": "20210829 15:03"
        },
        {
          "id": "MDRW2100021995.1.30",
          "form": "이나즈마 자체 스토리보단",
          "original_form": "이나즈마 자체 스토리보단",
          "speaker_id": "2",
          "time": "20210829 15:03"
        },
        {
          "id": "MDRW2100021995.1.31",
          "form": "덜 음침해",
          "original_form": "덜 음침해",
          "speaker_id": "2",
          "time": "20210829 15:03"
        },
        {
          "id": "MDRW2100021995.1.32",
          "form": "그렇긴 하지",
          "original_form": "그렇긴 하지",
          "speaker_id": "1",
          "time": "20210829 15:03"
        },
        {
          "id": "MDRW2100021995.1.33",
          "form": "그리고 우인단이 주인공 입장에서나 악역이지",
          "original_form": "그리고 우인단이 주인공 입장에서나 악역이지",
          "speaker_id": "1",
          "time": "20210829 15:04"
        },
        {
          "id": "MDRW2100021995.1.34",
          "form": "걍 평범하게 남의나라 망가뜨리고싶은 우인단?",
          "original_form": "걍 평범하게 남의나라 망가뜨리고싶은 우인단?",
          "speaker_id": "2",
          "time": "20210829 15:04"
        },
        {
          "id": "MDRW2100021995.1.35",
          "form": "게임 전체 스토리를 생각하면 착한 애들이던데",
          "original_form": "게임 전체 스토리를 생각하면 착한 애들이던데",
          "speaker_id": "1",
          "time": "20210829 15:04"
        },
        {
          "id": "MDRW2100021995.1.36",
          "form": "잉",
          "original_form": "잉",
          "speaker_id": "2",
          "time": "20210829 15:04"
        },
        {
          "id": "MDRW2100021995.1.37",
          "form": "나중에 수습을 어케 하려고 그러나 몰라",
          "original_form": "나중에 수습을 어케 하려고 그러나 몰라",
          "speaker_id": "1",
          "time": "20210829 15:04"
        },
        {
          "id": "MDRW2100021995.1.38",
          "form": "그래??",
          "original_form": "그래??",
          "speaker_id": "2",
          "time": "20210829 15:04"
        },
        {
          "id": "MDRW2100021995.1.39",
          "form": "ㅇㅇ 굳이 선과 악을 따지자면",
          "original_form": "ㅇㅇ 굳이 선과 악을 따지자면",
          "speaker_id": "1",
          "time": "20210829 15:04"
        },
        {
          "id": "MDRW2100021995.1.40",
          "form": "정의로운 쪽이야",
          "original_form": "정의로운 쪽이야",
          "speaker_id": "1",
          "time": "20210829 15:04"
        },
        {
          "id": "MDRW2100021995.1.41",
