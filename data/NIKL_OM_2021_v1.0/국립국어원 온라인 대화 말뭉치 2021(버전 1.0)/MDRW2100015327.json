      "utterance": [
        {
          "id": "MDRW2100015327.1.1",
          "form": "name2님은 운동 좋아하세요?",
          "original_form": "&name2&님은 운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015327.1.2",
          "form": "저는 과체중이라 해야되는데 안하고있어요...",
          "original_form": "저는 과체중이라 해야되는데 안하고있어요...",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015327.1.3",
          "form": "운동 좋아해야되는데 절교했어요ㅜㅜ",
          "original_form": "운동 좋아해야되는데 절교했어요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:40"
        },
        {
          "id": "MDRW2100015327.1.4",
          "form": "전 최근까지 달리기 했는데 좋더라고요 기분 환기되어서요",
          "original_form": "전 최근까지 달리기 했는데 좋더라고요 기분 환기되어서요",
          "speaker_id": "1",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015327.1.5",
          "form": "와 달리기진짜 멋진데요!!",
          "original_form": "와 달리기진짜 멋진데요!!",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015327.1.6",
          "form": "달리기하면 기분좋아지는 호르몬도 나온다는데",
          "original_form": "달리기하면 기분좋아지는 호르몬도 나온다는데",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015327.1.7",
          "form": "첫시작을 못하네요ㅜㅜ",
          "original_form": "첫시작을 못하네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:41"
        },
        {
          "id": "MDRW2100015327.1.8",
          "form": "런데이 어플로 한번 해보세요",
          "original_form": "런데이 어플로 한번 해보세요",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015327.1.9",
          "form": "초보자가 하기 딱 좋아요",
          "original_form": "초보자가 하기 딱 좋아요",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015327.1.10",
          "form": "아 저 그어플 들어봤어요",
          "original_form": "아 저 그어플 들어봤어요",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015327.1.11",
          "form": "혹시 핸드폰 암웨어? 그것도 차시나요?",
          "original_form": "혹시 핸드폰 암웨어? 그것도 차시나요?",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015327.1.12",
          "form": "그냥 주머니에 넣어도 잘 작동되나요?ㅎㅎ",
          "original_form": "그냥 주머니에 넣어도 잘 작동되나요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015327.1.13",
          "form": "네 워치랑도 연동 될 거예요",
          "original_form": "네 워치랑도 연동 될 거예요",
          "speaker_id": "1",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015327.1.14",
          "form": "앗 워치가 없어서 이렇게 또 워치살 구실이 생기네요ㅋ기",
          "original_form": "앗 워치가 없어서 이렇게 또 워치살 구실이 생기네요ㅋ기",
          "speaker_id": "2",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015327.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015327.1.16",
          "form": "맞아요 달리기하면 워치 사고 싶어져요",
          "original_form": "맞아요 달리기하면 워치 사고 싶어져요",
          "speaker_id": "1",
          "time": "20210825 22:46"
        },
        {
          "id": "MDRW2100015327.1.17",
          "form": "ㅋㅋㅋㅋㅋ 구매해도된다고 인정받은 느낌이에요",
          "original_form": "ㅋㅋㅋㅋㅋ 구매해도된다고 인정받은 느낌이에요",
          "speaker_id": "2",
          "time": "20210825 22:47"
        },
        {
          "id": "MDRW2100015327.1.18",
          "form": "ㅋㅋㅋ운동도 약간의 장비빨이 있어야죠!",
          "original_form": "ㅋㅋㅋ운동도 약간의 장비빨이 있어야죠!",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015327.1.19",
          "form": "사면 더 운동하고 싶어지지 않을까요?ㅋㅋ",
          "original_form": "사면 더 운동하고 싶어지지 않을까요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:48"
        },
        {
          "id": "MDRW2100015327.1.20",
          "form": "오 장비빨!!!맞아요",
          "original_form": "오 장비빨!!!맞아요",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015327.1.21",
          "form": "그리고 그렇게 산",
          "original_form": "그리고 그렇게 산",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015327.1.22",
          "form": "운동용 자전거랑..,요가매트랑...필라테스링을 보며",
          "original_form": "운동용 자전거랑..,요가매트랑...필라테스링을 보며",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015327.1.23",
          "form": "반성합니다..",
          "original_form": "반성합니다..",
          "speaker_id": "2",
          "time": "20210825 22:49"
        },
        {
          "id": "MDRW2100015327.1.24",
          "form": "아이고ㅋㅋㅋㅋ",
          "original_form": "아이고ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015327.1.25",
          "form": "그만 사셔야겠네요 ...",
          "original_form": "그만 사셔야겠네요 ...",
          "speaker_id": "1",
          "time": "20210825 22:50"
        },
        {
          "id": "MDRW2100015327.1.26",
          "form": "ㅜㅜㅜㅋㅋㅋㅋ하나씩 열심히할게요...ㅋㅋㅋ",
          "original_form": "ㅜㅜㅜㅋㅋㅋㅋ하나씩 열심히할게요...ㅋㅋㅋ",
          "speaker_id": "2",
