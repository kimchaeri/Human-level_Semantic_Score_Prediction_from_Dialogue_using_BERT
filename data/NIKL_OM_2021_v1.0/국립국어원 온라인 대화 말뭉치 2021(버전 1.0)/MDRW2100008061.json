      "utterance": [
        {
          "id": "MDRW2100008061.1.1",
          "form": "기본티는 인쇼로 많이 보세요? ㅋㅋㅋㅋ",
          "original_form": "기본티는 인쇼로 많이 보세요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:01"
        },
        {
          "id": "MDRW2100008061.1.2",
          "form": "그냥 마트가서 사요ㅎㅎ",
          "original_form": "그냥 마트가서 사요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100008061.1.3",
          "form": "가끔 인쇼도 보는데",
          "original_form": "가끔 인쇼도 보는데",
          "speaker_id": "2",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100008061.1.4",
          "form": "인쇼 볼바엔 도매시장에서 컬러별로 떼오는게 좋아서",
          "original_form": "인쇼 볼바엔 도매시장에서 컬러별로 떼오는게 좋아서",
          "speaker_id": "2",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100008061.1.5",
          "form": "이마트 이런데 입점한 옷가게요? ㅋㅋㅋ",
          "original_form": "이마트 이런데 입점한 옷가게요? ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:02"
        },
        {
          "id": "MDRW2100008061.1.6",
          "form": "도매시장 가거나",
          "original_form": "도매시장 가거나",
          "speaker_id": "2",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008061.1.7",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008061.1.8",
          "form": "저 자주 좋아해요",
          "original_form": "저 자주 좋아해요",
          "speaker_id": "2",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008061.1.9",
          "form": "데이즈 티셔츠도 가성비 갑",
          "original_form": "데이즈 티셔츠도 가성비 갑",
          "speaker_id": "2",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008061.1.10",
          "form": "자주에서 나오는 티셔츠들 다 가성비 좋아요",
          "original_form": "자주에서 나오는 티셔츠들 다 가성비 좋아요",
          "speaker_id": "2",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008061.1.11",
          "form": "70퍼 세일할때 컬러별로 쟁이기 좋아서ㅎ...",
          "original_form": "70퍼 세일할때 컬러별로 쟁이기 좋아서ㅎ...",
          "speaker_id": "2",
          "time": "20210822 13:03"
        },
        {
          "id": "MDRW2100008061.1.12",
          "form": "쇼핑 많이하는만큼 꼴에 가성비 챙기는편이에요",
          "original_form": "쇼핑 많이하는만큼 꼴에 가성비 챙기는편이에요",
          "speaker_id": "2",
          "time": "20210822 13:04"
        },
        {
          "id": "MDRW2100008061.1.13",
          "form": "자주 데이즈.. 거기서 부터ㅋㅋㅋㅋ기본티 시작해 봐야지ㅋㅋㅋㅋ",
          "original_form": "자주 데이즈.. 거기서 부터ㅋㅋㅋㅋ기본티 시작해 봐야지ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100008061.1.14",
          "form": "데이즈가 진짜 가성비 짱이고",
          "original_form": "데이즈가 진짜 가성비 짱이고",
          "speaker_id": "2",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100008061.1.15",
          "form": "ㅋㅋㅋ아주 큰 도움이었어요ㅋㅋㅋ계절도 바뀌고 뭐ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ아주 큰 도움이었어요ㅋㅋㅋ계절도 바뀌고 뭐ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100008061.1.16",
          "form": "자주는 기본흰티 3장 1팩짜리 있거든요?",
          "original_form": "자주는 기본흰티 3장 1팩짜리 있거든요?",
          "speaker_id": "2",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100008061.1.17",
          "form": "이거 좋아요",
          "original_form": "이거 좋아요",
          "speaker_id": "2",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100008061.1.18",
          "form": "저 한 10팩은 산듯",
          "original_form": "저 한 10팩은 산듯",
          "speaker_id": "2",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100008061.1.19",
          "form": "몇년째 기본템",
          "original_form": "몇년째 기본템",
          "speaker_id": "2",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100008061.1.20",
          "form": "흰티 반팔티 같은거 말씀이시죵",
          "original_form": "흰티 반팔티 같은거 말씀이시죵",
          "speaker_id": "1",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100008061.1.21",
          "form": "네네 반팔티나 긴팔티",
          "original_form": "네네 반팔티나 긴팔티",
          "speaker_id": "2",
          "time": "20210822 13:06"
        },
        {
          "id": "MDRW2100008061.1.22",
          "form": "ㅋㅋㅋㅋㅋ흰티 안에 받쳐입어야할땤ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ흰티 안에 받쳐입어야할땤ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:07"
        },
        {
          "id": "MDRW2100008061.1.23",
          "form": "그냥 이너용으로 입거나 잠옷용이요",
          "original_form": "그냥 이너용으로 입거나 잠옷용이요",
          "speaker_id": "2",
          "time": "20210822 13:07"
        },
        {
          "id": "MDRW2100008061.1.24",
          "form": "없어서 옷 한참고르기도 여러번인데 그래도 귀찮아 가지궄ㅋㅋㅋ",
          "original_form": "없어서 옷 한참고르기도 여러번인데 그래도 귀찮아 가지궄ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:07"
        },
        {
          "id": "MDRW2100008061.1.25",
          "form": "원단 진짜 좋은거써요",
          "original_form": "원단 진짜 좋은거써요",
          "speaker_id": "2",
          "time": "20210822 13:07"
        },
        {
          "id": "MDRW2100008061.1.26",
          "form": "이노무 귀차니즘ㅋㅋㅋ",
          "original_form": "이노무 귀차니즘ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:07"
        },
        {
          "id": "MDRW2100008061.1.27",
          "form": "오 그래요?",
          "original_form": "오 그래요?",
          "speaker_id": "1",
