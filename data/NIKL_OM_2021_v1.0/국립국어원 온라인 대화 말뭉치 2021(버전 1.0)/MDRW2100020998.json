      "utterance": [
        {
          "id": "MDRW2100020998.1.1",
          "form": "저녁드셨나요?",
          "original_form": "저녁드셨나요?",
          "speaker_id": "1",
          "time": "20210828 19:05"
        },
        {
          "id": "MDRW2100020998.1.2",
          "form": "네네 ㅋㅋㅋㅋ아까 볶음밥 해먹었어요",
          "original_form": "네네 ㅋㅋㅋㅋ아까 볶음밥 해먹었어요",
          "speaker_id": "2",
          "time": "20210828 19:43"
        },
        {
          "id": "MDRW2100020998.1.3",
          "form": "오~~ 저는 피자 먹었어요!",
          "original_form": "오~~ 저는 피자 먹었어요!",
          "speaker_id": "1",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100020998.1.4",
          "form": "배달음식 좋아하세요?",
          "original_form": "배달음식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100020998.1.5",
          "form": "네네 좋아해요 ㅋㅋㅋㅋㅋ저는 치킨 가장 좋아해요",
          "original_form": "네네 좋아해요 ㅋㅋㅋㅋㅋ저는 치킨 가장 좋아해요",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100020998.1.6",
          "form": "배달음식 최애메뉴가 뭔가요!?",
          "original_form": "배달음식 최애메뉴가 뭔가요!?",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100020998.1.7",
          "form": "요즘 닭발이요!",
          "original_form": "요즘 닭발이요!",
          "speaker_id": "1",
          "time": "20210828 19:53"
        },
        {
          "id": "MDRW2100020998.1.8",
          "form": "숯불 무뼈 닭발인데 그리 맛있더라구요",
          "original_form": "숯불 무뼈 닭발인데 그리 맛있더라구요",
          "speaker_id": "1",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100020998.1.9",
          "form": "아 닭발 저도 좋아해요 !!!!",
          "original_form": "아 닭발 저도 좋아해요 !!!!",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100020998.1.10",
          "form": "크 숯불 ㅠㅠ 맛있겟는데요",
          "original_form": "크 숯불 ㅠㅠ 맛있겟는데요",
          "speaker_id": "2",
          "time": "20210828 19:54"
        },
        {
          "id": "MDRW2100020998.1.11",
          "form": "!!!!최고에요",
          "original_form": "!!!!최고에요",
          "speaker_id": "1",
          "time": "20210828 20:02"
        },
        {
          "id": "MDRW2100020998.1.12",
          "form": "숯불이 얼마 없어서 더 귀하고 맛있더라구요",
          "original_form": "숯불이 얼마 없어서 더 귀하고 맛있더라구요",
          "speaker_id": "1",
          "time": "20210828 20:02"
        },
        {
          "id": "MDRW2100020998.1.13",
          "form": "맞아요 숯불 잘 없어요 그게 조리가 힘들다구하더라구요 ㅠㅠ",
          "original_form": "맞아요 숯불 잘 없어요 그게 조리가 힘들다구하더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100020998.1.14",
          "form": "저는 한신포차랑 실비집도 좋아해요!",
          "original_form": "저는 한신포차랑 실비집도 좋아해요!",
          "speaker_id": "2",
          "time": "20210828 20:03"
        },
        {
          "id": "MDRW2100020998.1.15",
          "form": "아 그래서 그런거군요ㅠ 왠지..",
          "original_form": "아 그래서 그런거군요ㅠ 왠지..",
          "speaker_id": "1",
          "time": "20210828 20:07"
        },
        {
          "id": "MDRW2100020998.1.16",
          "form": "한신포차! 저도 좋아해요~  집근처에 없는게 아쉬워요",
          "original_form": "한신포차! 저도 좋아해요~  집근처에 없는게 아쉬워요",
          "speaker_id": "1",
          "time": "20210828 20:08"
        },
        {
          "id": "MDRW2100020998.1.17",
          "form": "맞아요 ㅠㅠ 근처에 있으면 자주 포장해먹을텐데요",
          "original_form": "맞아요 ㅠㅠ 근처에 있으면 자주 포장해먹을텐데요",
          "speaker_id": "2",
          "time": "20210828 20:10"
        },
        {
          "id": "MDRW2100020998.1.18",
          "form": "매운것도 잘 드시나봐요!",
          "original_form": "매운것도 잘 드시나봐요!",
          "speaker_id": "2",
          "time": "20210828 20:10"
        },
        {
          "id": "MDRW2100020998.1.19",
          "form": "맞아요ㅜㅜ",
          "original_form": "맞아요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 20:14"
        },
        {
          "id": "MDRW2100020998.1.20",
          "form": "최대한 순한맛으로 먹고있어요!",
          "original_form": "최대한 순한맛으로 먹고있어요!",
          "speaker_id": "1",
          "time": "20210828 20:14"
        },
        {
          "id": "MDRW2100020998.1.21",
          "form": "요즘은 안매운것도 맵더라구요",
          "original_form": "요즘은 안매운것도 맵더라구요",
          "speaker_id": "1",
          "time": "20210828 20:14"
        },
        {
          "id": "MDRW2100020998.1.22",
          "form": "그렇군요ㅜ 아쉽겠어요",
          "original_form": "그렇군요ㅜ 아쉽겠어요",
          "speaker_id": "2",
          "time": "20210828 20:15"
        },
        {
          "id": "MDRW2100020998.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210828 20:20"
        },
        {
          "id": "MDRW2100020998.1.24",
          "form": "매운거 잘드세요?",
          "original_form": "매운거 잘드세요?",
          "speaker_id": "1",
          "time": "20210828 20:20"
        },
        {
          "id": "MDRW2100020998.1.25",
          "form": "네네 좋아하고 잘먹기두해요 ㅋㅋㅋㅋ",
          "original_form": "네네 좋아하고 잘먹기두해요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100020998.1.26",
          "form": "바른치킨 대세레드드셔보셨어요?",
          "original_form": "바른치킨 대세레드드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210828 20:24"
        },
        {
          "id": "MDRW2100020998.1.27",
          "form": "맛있게 매워요 추천드립니다 ㅋㅋㅋㅋ",
