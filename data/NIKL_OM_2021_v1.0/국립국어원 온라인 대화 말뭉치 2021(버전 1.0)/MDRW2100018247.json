      "utterance": [
        {
          "id": "MDRW2100018247.1.1",
          "form": "안녕하세요 반려동물 키우시나요?",
          "original_form": "안녕하세요 반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210826 23:21"
        },
        {
          "id": "MDRW2100018247.1.2",
          "form": "아니요 좋아는해요^^",
          "original_form": "아니요 좋아는해요^^",
          "speaker_id": "2",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018247.1.3",
          "form": "name1은요?",
          "original_form": "&name1&은요?",
          "speaker_id": "2",
          "time": "20210826 23:28"
        },
        {
          "id": "MDRW2100018247.1.4",
          "form": "저는 고양이 키운답니다",
          "original_form": "저는 고양이 키운답니다",
          "speaker_id": "1",
          "time": "20210827 02:05"
        },
        {
          "id": "MDRW2100018247.1.5",
          "form": "집사에요 하하하",
          "original_form": "집사에요 하하하",
          "speaker_id": "1",
          "time": "20210827 02:05"
        },
        {
          "id": "MDRW2100018247.1.6",
          "form": "글군요ㅎ 냥이 발 넘귀여운데",
          "original_form": "글군요ㅎ 냥이 발 넘귀여운데",
          "speaker_id": "2",
          "time": "20210827 02:08"
        },
        {
          "id": "MDRW2100018247.1.7",
          "form": "맞아요 젤리가 말랑해요",
          "original_form": "맞아요 젤리가 말랑해요",
          "speaker_id": "1",
          "time": "20210827 08:39"
        },
        {
          "id": "MDRW2100018247.1.8",
          "form": "우리 고양이는 새침해서 만지는거 많이 안 좋아하는데",
          "original_form": "우리 고양이는 새침해서 만지는거 많이 안 좋아하는데",
          "speaker_id": "1",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018247.1.9",
          "form": "저가 좀 만져서 툭 툭 때릴때면 말랑 젤리로 맞는답니다",
          "original_form": "저가 좀 만져서 툭 툭 때릴때면 말랑 젤리로 맞는답니다",
          "speaker_id": "1",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018247.1.10",
          "form": "하하하하하",
          "original_form": "하하하하하",
          "speaker_id": "1",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018247.1.11",
          "form": "넘 귀엽겠어요ㅎ 부모님이  유기묘 데려다키웠는데",
          "original_form": "넘 귀엽겠어요ㅎ 부모님이  유기묘 데려다키웠는데",
          "speaker_id": "2",
          "time": "20210827 15:27"
        },
        {
          "id": "MDRW2100018247.1.12",
          "form": "꼬랑지 흔들어줘~하면 살랑살랑 흔들었는데 나이먹어서 얼마전에 보내줬네요",
          "original_form": "꼬랑지 흔들어줘~하면 살랑살랑 흔들었는데 나이먹어서 얼마전에 보내줬네요",
          "speaker_id": "2",
          "time": "20210827 15:28"
        },
        {
          "id": "MDRW2100018247.1.13",
          "form": "하하 꼬랑지 흔들어줘 뭔가 웃기네요",
          "original_form": "하하 꼬랑지 흔들어줘 뭔가 웃기네요",
          "speaker_id": "1",
          "time": "20210827 22:34"
        },
        {
          "id": "MDRW2100018247.1.14",
          "form": "오 그러셨군요 오랜기간 많은 추억을 남겼네요. 고양이도 부모님도",
          "original_form": "오 그러셨군요 오랜기간 많은 추억을 남겼네요. 고양이도 부모님도",
          "speaker_id": "1",
          "time": "20210827 22:35"
        },
        {
          "id": "MDRW2100018247.1.15",
          "form": "키울땐좋은데 보내기가  맘아프죠",
          "original_form": "키울땐좋은데 보내기가  맘아프죠",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100018247.1.16",
          "form": "맞아요. 항상 같이 있을 것 같은데 한순간에 달라지니까 참 힘들죠",
          "original_form": "맞아요. 항상 같이 있을 것 같은데 한순간에 달라지니까 참 힘들죠",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100018247.1.17",
          "form": "생명을 키우는건 많은 책임이 필요한 것 같아요",
          "original_form": "생명을 키우는건 많은 책임이 필요한 것 같아요",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100018247.1.18",
          "form": "말로 표현할 수 없을정도로요",
          "original_form": "말로 표현할 수 없을정도로요",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100018247.1.19",
          "form": "맞아요 책임질거아님 첨부터 안키우는게 좋은데",
          "original_form": "맞아요 책임질거아님 첨부터 안키우는게 좋은데",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100018247.1.20",
          "form": "요즘 넘 버려지는 아이들이 많은거같아요",
          "original_form": "요즘 넘 버려지는 아이들이 많은거같아요",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100018247.1.21",
          "form": "그러니까요. 반려동물 관련법이 강화됐으면 좋겠어요",
          "original_form": "그러니까요. 반려동물 관련법이 강화됐으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210827 23:25"
        },
        {
          "id": "MDRW2100018247.1.22",
          "form": "학대도 처벌좀 강화했음 좋겠고",
          "original_form": "학대도 처벌좀 강화했음 좋겠고",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100018247.1.23",
          "form": "가끔 뉴스보면 끔찍하더라구요",
          "original_form": "가끔 뉴스보면 끔찍하더라구요",
          "speaker_id": "2",
          "time": "20210827 23:26"
        },
        {
          "id": "MDRW2100018247.1.24",
          "form": "그니까요 진짜 똑같이 느낄만한 법이 생겨야돼요",
          "original_form": "그니까요 진짜 똑같이 느낄만한 법이 생겨야돼요",
          "speaker_id": "1",
          "time": "20210828 00:06"
        }
      ]
