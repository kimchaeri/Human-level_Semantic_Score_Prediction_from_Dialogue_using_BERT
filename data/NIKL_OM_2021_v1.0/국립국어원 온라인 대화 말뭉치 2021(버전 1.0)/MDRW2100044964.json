      "utterance": [
        {
          "id": "MDRW2100044964.1.1",
          "form": "자꾸 오류...",
          "original_form": "자꾸 오류...",
          "speaker_id": "2",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044964.1.2",
          "form": "뭐가 나쁜말이었니 덕ㅈㅣㄹ?",
          "original_form": "뭐가 나쁜말이었니 덕ㅈㅣㄹ?",
          "speaker_id": "2",
          "time": "20210908 00:24"
        },
        {
          "id": "MDRW2100044964.1.3",
          "form": "그러게 ㅠ",
          "original_form": "그러게 ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044964.1.4",
          "form": "안가져서 문제야 ㅠ",
          "original_form": "안가져서 문제야 ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044964.1.5",
          "form": "왔당",
          "original_form": "왔당",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044964.1.6",
          "form": "소 작가님",
          "original_form": "소 작가님",
          "speaker_id": "2",
          "time": "20210908 00:25"
        },
        {
          "id": "MDRW2100044964.1.7",
          "form": "강연금 더 할말 없어요?",
          "original_form": "강연금 더 할말 없어요?",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044964.1.8",
          "form": "진짜루??",
          "original_form": "진짜루??",
          "speaker_id": "2",
          "time": "20210908 00:26"
        },
        {
          "id": "MDRW2100044964.1.9",
          "form": "진짜우 ㅠ",
          "original_form": "진짜우 ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044964.1.10",
          "form": "없어요?",
          "original_form": "없어요?",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044964.1.11",
          "form": "뒷 얘기 생각나면 눈치보지 말고",
          "original_form": "뒷 얘기 생각나면 눈치보지 말고",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044964.1.12",
          "form": "가져옵시다",
          "original_form": "가져옵시다",
          "speaker_id": "2",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100044964.1.13",
          "form": "요새 카카페에서 서비스 일시 중단했던데 ㅠ",
          "original_form": "요새 카카페에서 서비스 일시 중단했던데 ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100044964.1.14",
          "form": "엉? 왜?",
          "original_form": "엉? 왜?",
          "speaker_id": "2",
          "time": "20210908 00:30"
        },
        {
          "id": "MDRW2100044964.1.15",
          "form": "일시중지면 올라온거 까지만 볼수있는겨?",
          "original_form": "일시중지면 올라온거 까지만 볼수있는겨?",
          "speaker_id": "2",
          "time": "20210908 00:31"
        },
        {
          "id": "MDRW2100044964.1.16",
          "form": "아니 특정 편을 못봐",
          "original_form": "아니 특정 편을 못봐",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100044964.1.17",
          "form": "그건..",
          "original_form": "그건..",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044964.1.18",
          "form": "더 이상한데? ㅋㅋㅋㅋㅋ",
          "original_form": "더 이상한데? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100044964.1.19",
          "form": "그 최신 10편인가가 협약이 이상했나봐",
          "original_form": "그 최신 10편인가가 협약이 이상했나봐",
          "speaker_id": "1",
          "time": "20210908 00:34"
        },
        {
          "id": "MDRW2100044964.1.20",
          "form": "아하..",
          "original_form": "아하..",
          "speaker_id": "2",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100044964.1.21",
          "form": "잘 가다가 왜 그랬데",
          "original_form": "잘 가다가 왜 그랬데",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100044964.1.22",
          "form": "피토한다 ㅋㅋㅋ",
          "original_form": "피토한다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100044964.1.23",
          "form": "나도 보여달라고..",
          "original_form": "나도 보여달라고..",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100044964.1.24",
          "form": "왜 거기서 멈추는건데",
          "original_form": "왜 거기서 멈추는건데",
          "speaker_id": "1",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100044964.1.25",
          "form": "매우 화나는 일중 하나가",
          "original_form": "매우 화나는 일중 하나가",
          "speaker_id": "2",
          "time": "20210908 00:36"
        },
        {
          "id": "MDRW2100044964.1.26",
          "form": "말을 하다가 멈추는 것이고 ㅋㅋㅋ",
          "original_form": "말을 하다가 멈추는 것이고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100044964.1.27",
          "form": "연재를 멈추다닠",
          "original_form": "연재를 멈추다닠",
          "speaker_id": "2",
          "time": "20210908 00:37"
        },
        {
          "id": "MDRW2100044964.1.28",
          "form": "ㅋㅋㅋㅋㅋ 화날만 하죠?",
          "original_form": "ㅋㅋㅋㅋㅋ 화날만 하죠?",
          "speaker_id": "1",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100044964.1.29",
          "form": "yeeeeees..",
          "original_form": "yeeeeees..",
          "speaker_id": "2",
          "time": "20210908 00:39"
        }
      ]
    }
