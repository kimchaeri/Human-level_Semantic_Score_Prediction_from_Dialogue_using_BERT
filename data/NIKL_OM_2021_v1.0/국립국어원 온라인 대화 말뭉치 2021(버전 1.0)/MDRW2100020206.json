      "utterance": [
        {
          "id": "MDRW2100020206.1.1",
          "form": "저도 맛있을진 모르겠으나",
          "original_form": "저도 맛있을진 모르겠으나",
          "speaker_id": "2",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100020206.1.2",
          "form": "성신여대에 있는데에요",
          "original_form": "성신여대에 있는데에요",
          "speaker_id": "2",
          "time": "20210828 00:20"
        },
        {
          "id": "MDRW2100020206.1.3",
          "form": "오 도전해보고 맛있으면 꼭 알려주세요!!",
          "original_form": "오 도전해보고 맛있으면 꼭 알려주세요!!",
          "speaker_id": "1",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020206.1.4",
          "form": "저도 내일 노원역가서 뭐좀 사올까 고민중이에용",
          "original_form": "저도 내일 노원역가서 뭐좀 사올까 고민중이에용",
          "speaker_id": "1",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020206.1.5",
          "form": "알겠어",
          "original_form": "알겠어",
          "speaker_id": "2",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020206.1.6",
          "form": "노원역에",
          "original_form": "노원역에",
          "speaker_id": "2",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020206.1.7",
          "form": "요즘 무화과 나와서 무화과 디저트들도 나오나봐요!!",
          "original_form": "요즘 무화과 나와서 무화과 디저트들도 나오나봐요!!",
          "speaker_id": "1",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020206.1.8",
          "form": "찍어둔카페잇엇는데",
          "original_form": "찍어둔카페잇엇는데",
          "speaker_id": "2",
          "time": "20210828 00:21"
        },
        {
          "id": "MDRW2100020206.1.9",
          "form": "제가내일가는데도 무화과도 !! 판대요!!",
          "original_form": "제가내일가는데도 무화과도 !! 판대요!!",
          "speaker_id": "2",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020206.1.10",
          "form": "2개사올예정이엇는디!!",
          "original_form": "2개사올예정이엇는디!!",
          "speaker_id": "2",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020206.1.11",
          "form": "오옹ㅋㅋㅋ어디일까요 듀근듀군",
          "original_form": "오옹ㅋㅋㅋ어디일까요 듀근듀군",
          "speaker_id": "1",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020206.1.12",
          "form": "내일가는데는 모드에",
          "original_form": "내일가는데는 모드에",
          "speaker_id": "2",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020206.1.13",
          "form": "오오오오 대박!!! 복숭아 하나 무화과하나 진짜 딱이에요",
          "original_form": "오오오오 대박!!! 복숭아 하나 무화과하나 진짜 딱이에요",
          "speaker_id": "1",
          "time": "20210828 00:22"
        },
        {
          "id": "MDRW2100020206.1.14",
          "form": "인스타에 검색 고고",
          "original_form": "인스타에 검색 고고",
          "speaker_id": "2",
          "time": "20210828 00:22"
        },
        {
