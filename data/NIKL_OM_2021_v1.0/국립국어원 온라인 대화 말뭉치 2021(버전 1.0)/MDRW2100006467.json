      "utterance": [
        {
          "id": "MDRW2100006467.1.1",
          "form": "엄청 날려요...",
          "original_form": "엄청 날려요...",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100006467.1.2",
          "form": "고양이가 빗질을 혼자 많이하는데 그때도 많이 빠지고",
          "original_form": "고양이가 빗질을 혼자 많이하는데 그때도 많이 빠지고",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100006467.1.3",
          "form": "그냥 있어도 빠지고..쓰다듬어도 빠져요...",
          "original_form": "그냥 있어도 빠지고..쓰다듬어도 빠져요...",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100006467.1.4",
          "form": "헉....기관지에 안좋지않나요?",
          "original_form": "헉....기관지에 안좋지않나요?",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100006467.1.5",
          "form": "음 저희집은 크게 문제가 있진 않은 거 같아요!",
          "original_form": "음 저희집은 크게 문제가 있진 않은 거 같아요!",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100006467.1.6",
          "form": "알러지 있는 사람이 없어서",
          "original_form": "알러지 있는 사람이 없어서",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100006467.1.7",
          "form": "큰 문제는 없어요ㅎㅎ",
          "original_form": "큰 문제는 없어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100006467.1.8",
          "form": "그 그럼 캣타워도 있나요??",
          "original_form": "그 그럼 캣타워도 있나요??",
          "speaker_id": "1",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100006467.1.9",
          "form": "캣타워는 없어요ㅠㅠ",
          "original_form": "캣타워는 없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100006467.1.10",
          "form": "사주고싶긴 하지만 비용이 너무 들기도 하고 공간이 좁아서..",
          "original_form": "사주고싶긴 하지만 비용이 너무 들기도 하고 공간이 좁아서..",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100006467.1.11",
          "form": "헉ㅠㅜ 캣타워가 좀 신기하더라구요",
          "original_form": "헉ㅠㅜ 캣타워가 좀 신기하더라구요",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100006467.1.12",
          "form": "이쁘기두하고",
          "original_form": "이쁘기두하고",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100006467.1.13",
          "form": "그쵸 애기들도 좋아하기도 하더라구요",
          "original_form": "그쵸 애기들도 좋아하기도 하더라구요",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100006467.1.14",
          "form": "거북이는 어떤 사료? 간식? 줘요??",
          "original_form": "거북이는 어떤 사료? 간식? 줘요??",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100006467.1.15",
          "form": "그 말린새우랑 양배추 상추 먹어요",
          "original_form": "그 말린새우랑 양배추 상추 먹어요",
          "speaker_id": "1",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100006467.1.16",
          "form": "아하 신기하네용",
          "original_form": "아하 신기하네용",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006467.1.17",
          "form": "거북이는 한마리만 키우세요??",
          "original_form": "거북이는 한마리만 키우세요??",
          "speaker_id": "2",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006467.1.18",
          "form": "네 한마리 키우고있어요",
          "original_form": "네 한마리 키우고있어요",
          "speaker_id": "1",
          "time": "20210908 00:51"
        },
        {
          "id": "MDRW2100006467.1.19",
          "form": "거북이 어디서 키워요?? 수조이런데인가.?.?",
          "original_form": "거북이 어디서 키워요?? 수조이런데인가.?.?",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100006467.1.20",
          "form": "아 그렇겠네요,, 복잡할 거 같기두하고..",
          "original_form": "아 그렇겠네요,, 복잡할 거 같기두하고..",
          "speaker_id": "2",
          "time": "20210908 00:56"
        },
        {
          "id": "MDRW2100006467.1.21",
          "form": "맞아요ㅠㅜ돈도 은근 많이 깨져요....수조랑 여과기만해도",
          "original_form": "맞아요ㅠㅜ돈도 은근 많이 깨져요....수조랑 여과기만해도",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100006467.1.22",
          "form": "헉..그렇군여..",
          "original_form": "헉..그렇군여..",
          "speaker_id": "2",
          "time": "20210908 01:12"
        }
      ]
