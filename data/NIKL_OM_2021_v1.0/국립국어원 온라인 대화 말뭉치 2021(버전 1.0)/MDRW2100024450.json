      "utterance": [
        {
          "id": "MDRW2100024450.1.1",
          "form": "연애중이신가요 ㅎㅎ",
          "original_form": "연애중이신가요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 10:07"
        },
        {
          "id": "MDRW2100024450.1.2",
          "form": "네ㅎㅎ name1님은요??",
          "original_form": "네ㅎㅎ &name1&님은요??",
          "speaker_id": "2",
          "time": "20210831 10:08"
        },
        {
          "id": "MDRW2100024450.1.3",
          "form": "저는 결혼해서 연애를 하면 안되는 몸...",
          "original_form": "저는 결혼해서 연애를 하면 안되는 몸...",
          "speaker_id": "1",
          "time": "20210831 10:09"
        },
        {
          "id": "MDRW2100024450.1.4",
          "form": "아앗... 큰일날뻔했네요",
          "original_form": "아앗... 큰일날뻔했네요",
          "speaker_id": "2",
          "time": "20210831 10:11"
        },
        {
          "id": "MDRW2100024450.1.5",
          "form": "행복합니다 ㅎㅎ",
          "original_form": "행복합니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 10:13"
        },
        {
          "id": "MDRW2100024450.1.6",
          "form": "ㅋㅋ오래 연애하고 결혼하신건가용?",
          "original_form": "ㅋㅋ오래 연애하고 결혼하신건가용?",
          "speaker_id": "2",
          "time": "20210831 10:14"
        },
        {
          "id": "MDRW2100024450.1.7",
          "form": "1년정도 만나고 결혼준비했어요ㅋㅋ",
          "original_form": "1년정도 만나고 결혼준비했어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 10:16"
        },
        {
          "id": "MDRW2100024450.1.8",
          "form": "회사 동기 만나서 ㅎㅎ",
          "original_form": "회사 동기 만나서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 10:16"
        },
        {
          "id": "MDRW2100024450.1.9",
          "form": "우왕! 회사!",
          "original_form": "우왕! 회사!",
          "speaker_id": "2",
          "time": "20210831 10:17"
        },
        {
          "id": "MDRW2100024450.1.10",
          "form": "행복하시구 좋으시겠어용",
          "original_form": "행복하시구 좋으시겠어용",
          "speaker_id": "2",
          "time": "20210831 10:17"
        },
        {
          "id": "MDRW2100024450.1.11",
          "form": "ㅋㅋ장단점이 있긴하죠",
          "original_form": "ㅋㅋ장단점이 있긴하죠",
          "speaker_id": "1",
          "time": "20210831 10:19"
        },
        {
          "id": "MDRW2100024450.1.12",
          "form": "장점은 어떤거에요,?",
          "original_form": "장점은 어떤거에요,?",
          "speaker_id": "2",
          "time": "20210831 10:21"
        },
        {
          "id": "MDRW2100024450.1.13",
          "form": "음 안정적이죠?ㅎㅎ",
          "original_form": "음 안정적이죠?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 16:14"
        },
        {
          "id": "MDRW2100024450.1.14",
          "form": "아항!ㅋㅋ 좋네용",
          "original_form": "아항!ㅋㅋ 좋네용",
          "speaker_id": "2",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100024450.1.15",
          "form": "만남은 안정적인게 좋더라구요",
          "original_form": "만남은 안정적인게 좋더라구요",
          "speaker_id": "2",
          "time": "20210831 16:30"
        },
        {
          "id": "MDRW2100024450.1.16",
          "form": "근데 여자친구가 집에 놀러왔는데 안가요",
          "original_form": "근데 여자친구가 집에 놀러왔는데 안가요",
          "speaker_id": "1",
          "time": "20210831 16:33"
        },
        {
          "id": "MDRW2100024450.1.17",
          "form": "자유가 사라졌어요",
          "original_form": "자유가 사라졌어요",
          "speaker_id": "1",
          "time": "20210831 16:33"
        },
        {
          "id": "MDRW2100024450.1.18",
          "form": "아앗..!",
          "original_form": "아앗..!",
          "speaker_id": "2",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100024450.1.19",
          "form": "결혼은 그런거균요",
          "original_form": "결혼은 그런거균요",
          "speaker_id": "2",
          "time": "20210831 16:36"
        },
        {
          "id": "MDRW2100024450.1.20",
          "form": "ㅋㅋ 장점이 더 커요 그래두",
          "original_form": "ㅋㅋ 장점이 더 커요 그래두",
          "speaker_id": "1",
