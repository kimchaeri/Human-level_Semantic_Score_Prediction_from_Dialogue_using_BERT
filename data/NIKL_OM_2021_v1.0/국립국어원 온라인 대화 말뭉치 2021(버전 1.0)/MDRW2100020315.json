      "utterance": [
        {
          "id": "MDRW2100020315.1.1",
          "form": "나가서 만나지..",
          "original_form": "나가서 만나지..",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020315.1.2",
          "form": "글쿠만",
          "original_form": "글쿠만",
          "speaker_id": "1",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020315.1.3",
          "form": "근데",
          "original_form": "근데",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020315.1.4",
          "form": "가금",
          "original_form": "가금",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020315.1.5",
          "form": "내집생기면 근데 초반에라도",
          "original_form": "내집생기면 근데 초반에라도",
          "speaker_id": "1",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020315.1.6",
          "form": "삼촌이나ㅠ이모 갑자기 올때잇는데",
          "original_form": "삼촌이나ㅠ이모 갑자기 올때잇는데",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020315.1.7",
          "form": "집이 너무좁아서",
          "original_form": "집이 너무좁아서",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020315.1.8",
          "form": "집들이를 안하겠나",
          "original_form": "집들이를 안하겠나",
          "speaker_id": "1",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020315.1.9",
          "form": "오래못잇다 나감",
          "original_form": "오래못잇다 나감",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020315.1.10",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020315.1.11",
          "form": "아웃겨",
          "original_form": "아웃겨",
          "speaker_id": "1",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020315.1.12",
          "form": "근데 룸메잇으면",
          "original_form": "근데 룸메잇으면",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020315.1.13",
          "form": "좀..",
          "original_form": "좀..",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020315.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020315.1.15",
          "form": "굳이 안데려올거같은데",
          "original_form": "굳이 안데려올거같은데",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020315.1.16",
          "form": "나는..",
          "original_form": "나는..",
          "speaker_id": "2",
          "time": "20210828 01:19"
        },
        {
          "id": "MDRW2100020315.1.17",
          "form": "걍 밖에서 만나서 놀고말지..",
          "original_form": "걍 밖에서 만나서 놀고말지..",
          "speaker_id": "2",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020315.1.18",
          "form": "불편하잔어",
          "original_form": "불편하잔어",
          "speaker_id": "2",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020315.1.19",
          "form": "님은 데꼬오세요 걍",
          "original_form": "님은 데꼬오세요 걍",
          "speaker_id": "2",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020315.1.20",
          "form": "맞춰서 나갓다올래",
          "original_form": "맞춰서 나갓다올래",
          "speaker_id": "2",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020315.1.21",
          "form": "그럴수가",
          "original_form": "그럴수가",
          "speaker_id": "1",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020315.1.22",
          "form": "그래도막",
          "original_form": "그래도막",
          "speaker_id": "1",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020315.1.23",
          "form": "난 혼자사는거 아니면 집들이 안할래",
          "original_form": "난 혼자사는거 아니면 집들이 안할래",
          "speaker_id": "2",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020315.1.24",
          "form": "지인 자고간다거나",
          "original_form": "지인 자고간다거나",
          "speaker_id": "1",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020315.1.25",
          "form": "하는상황이잇을수도잇어",
          "original_form": "하는상황이잇을수도잇어",
          "speaker_id": "1",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020315.1.26",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020315.1.27",
          "form": "예기치못한다구",
          "original_form": "예기치못한다구",
          "speaker_id": "1",
          "time": "20210828 01:20"
        },
        {
          "id": "MDRW2100020315.1.28",
          "form": "아직은 상상 안간다",
          "original_form": "아직은 상상 안간다",
          "speaker_id": "2",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100020315.1.29",
          "form": "냐가친구집애서",
          "original_form": "냐가친구집애서",
          "speaker_id": "1",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100020315.1.30",
          "form": "잔적이많아서 그런가봐",
          "original_form": "잔적이많아서 그런가봐",
          "speaker_id": "1",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100020315.1.31",
          "form": "ㅎㅎ;",
          "original_form": "ㅎㅎ;",
          "speaker_id": "1",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100020315.1.32",
          "form": "오 글쿠나",
          "original_form": "오 글쿠나",
          "speaker_id": "2",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100020315.1.33",
          "form": "난 거의없음..",
          "original_form": "난 거의없음..",
          "speaker_id": "2",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100020315.1.34",
          "form": "집에 누구 데려온적도없거",
          "original_form": "집에 누구 데려온적도없거",
          "speaker_id": "2",
          "time": "20210828 01:21"
        },
        {
          "id": "MDRW2100020315.1.35",
          "form": "그래도 말을 잘하고",
          "original_form": "그래도 말을 잘하고",
          "speaker_id": "1",
          "time": "20210828 01:22"
        },
        {
          "id": "MDRW2100020315.1.36",
          "form": "조율한다면",
          "original_form": "조율한다면",
          "speaker_id": "1",
          "time": "20210828 01:22"
        },
        {
          "id": "MDRW2100020315.1.37",
          "form": "문제없을거같닫",
          "original_form": "문제없을거같닫",
          "speaker_id": "1",
          "time": "20210828 01:22"
        },
        {
          "id": "MDRW2100020315.1.38",
          "form": "ㄱㄹ",
          "original_form": "ㄱㄹ",
          "speaker_id": "2",
          "time": "20210828 01:22"
        }
      ]
    }
  ]
