      "utterance": [
        {
          "id": "MDRW2100020027.1.1",
          "form": "쇼핑 주로 어디서 하세요?",
          "original_form": "쇼핑 주로 어디서 하세요?",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020027.1.2",
          "form": "오! 저는 그냥 인터넷이나",
          "original_form": "오! 저는 그냥 인터넷이나",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020027.1.3",
          "form": "좀만 버스타고가면 백화점도 있어서",
          "original_form": "좀만 버스타고가면 백화점도 있어서",
          "speaker_id": "2",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020027.1.4",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "1",
          "time": "20210827 23:19"
        },
        {
          "id": "MDRW2100020027.1.5",
          "form": "가끔 거기가서 쇼핑해요",
          "original_form": "가끔 거기가서 쇼핑해요",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020027.1.6",
          "form": "보고 사는게 가장 좋긴하죠",
          "original_form": "보고 사는게 가장 좋긴하죠",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020027.1.7",
          "form": "맞아요 근데 요새 워낙",
          "original_form": "맞아요 근데 요새 워낙",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020027.1.8",
          "form": "온라인 쇼핑몰도 잘되어있어서",
          "original_form": "온라인 쇼핑몰도 잘되어있어서",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020027.1.9",
          "form": "맞아요 ㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020027.1.10",
          "form": "옛날보다 많이 편해진거같아요ㅋㅋㅋ",
          "original_form": "옛날보다 많이 편해진거같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020027.1.11",
          "form": "저는 거의 온라인으로 사는 거 같아요",
          "original_form": "저는 거의 온라인으로 사는 거 같아요",
          "speaker_id": "1",
          "time": "20210827 23:20"
        },
        {
          "id": "MDRW2100020027.1.12",
          "form": "주로 어떤거 구매하세요?",
          "original_form": "주로 어떤거 구매하세요?",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100020027.1.13",
          "form": "음 저는 음식이나 옷이요",
          "original_form": "음 저는 음식이나 옷이요",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100020027.1.14",
          "form": "오 저두요",
          "original_form": "오 저두요",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020027.1.15",
          "form": "원래 옷을 자주 안샀는데",
          "original_form": "원래 옷을 자주 안샀는데",
          "speaker_id": "2",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100020027.1.16",
          "form": "올해 잘 나가지도 않으면서 유난히 옷을 되게 많이 샀어요ㅋㅋㅋ",
          "original_form": "올해 잘 나가지도 않으면서 유난히 옷을 되게 많이 샀어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020027.1.17",
          "form": "저도요 ㅋㅋㅋㅋ",
          "original_form": "저도요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020027.1.18",
          "form": "그래서 전 사고나서 못입은 옷 한가득이에요",
          "original_form": "그래서 전 사고나서 못입은 옷 한가득이에요",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020027.1.19",
          "form": "저도 겨우 한번 입어본 옷도 많고..",
          "original_form": "저도 겨우 한번 입어본 옷도 많고..",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020027.1.20",
          "form": "아 ㅜㅜ",
          "original_form": "아 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020027.1.21",
          "form": "요새 또 밤엔 추워져서",
          "original_form": "요새 또 밤엔 추워져서",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100020027.1.22",
          "form": "가을 옷을 슬슬사야하나 생각중이에요",
          "original_form": "가을 옷을 슬슬사야하나 생각중이에요",
          "speaker_id": "2",
          "time": "20210827 23:24"
        }
      ]
