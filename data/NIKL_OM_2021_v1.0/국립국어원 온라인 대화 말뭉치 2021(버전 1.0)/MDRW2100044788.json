      "utterance": [
        {
          "id": "MDRW2100044788.1.1",
          "form": "이제 32부작 투깝스 찍네",
          "original_form": "이제 32부작 투깝스 찍네",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044788.1.2",
          "form": "투깝스?  기억이 안나요",
          "original_form": "투깝스?  기억이 안나요",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044788.1.3",
          "form": "정의감 잇는 강력게 형사와 여기자",
          "original_form": "정의감 잇는 강력게 형사와 여기자",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044788.1.4",
          "form": "여기자는 누구예요?",
          "original_form": "여기자는 누구예요?",
          "speaker_id": "1",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044788.1.5",
          "form": "조정석이 형사인데 사기꾼 영혼이 빙의됨",
          "original_form": "조정석이 형사인데 사기꾼 영혼이 빙의됨",
          "speaker_id": "2",
          "time": "20210907 23:45"
        },
        {
          "id": "MDRW2100044788.1.6",
          "form": "ㅋㅋ 또 얘기를 만드네",
          "original_form": "ㅋㅋ 또 얘기를 만드네",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044788.1.7",
          "form": "여기자는 혜리",
          "original_form": "여기자는 혜리",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044788.1.8",
          "form": "혜리?  얘도 은근 주인공 많이 했네요",
          "original_form": "혜리?  얘도 은근 주인공 많이 했네요",
          "speaker_id": "1",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044788.1.9",
          "form": "여기서 김선호가 나왓네",
          "original_form": "여기서 김선호가 나왓네",
          "speaker_id": "2",
          "time": "20210907 23:46"
        },
        {
          "id": "MDRW2100044788.1.10",
          "form": "어머 대세 김선호ㅋ",
          "original_form": "어머 대세 김선호ㅋ",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044788.1.11",
          "form": "김선호가 사기꾼 영혼 ㅋㅋ",
          "original_form": "김선호가 사기꾼 영혼 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044788.1.12",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "1",
          "time": "20210907 23:47"
        },
        {
          "id": "MDRW2100044788.1.13",
          "form": "엉 그리고 좀 쉬다가 다시 녹두꽃이라는 사극으로",
          "original_form": "엉 그리고 좀 쉬다가 다시 녹두꽃이라는 사극으로",
