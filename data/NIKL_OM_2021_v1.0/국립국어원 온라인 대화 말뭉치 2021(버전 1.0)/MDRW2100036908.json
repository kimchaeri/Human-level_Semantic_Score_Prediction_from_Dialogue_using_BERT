      "utterance": [
        {
          "id": "MDRW2100036908.1.1",
          "form": "아 저두 진영 하",
          "original_form": "아 저두 진영 하",
          "speaker_id": "2",
          "time": "20210831 14:46"
        },
        {
          "id": "MDRW2100036908.1.2",
          "form": "넘 설레게 생겼어여",
          "original_form": "넘 설레게 생겼어여",
          "speaker_id": "2",
          "time": "20210831 14:46"
        },
        {
          "id": "MDRW2100036908.1.3",
          "form": "저 근데 첨에 비포보구",
          "original_form": "저 근데 첨에 비포보구",
          "speaker_id": "2",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100036908.1.4",
          "form": "진영이랑 공찬이랑 구별 못했었어요 ㅋㅋㅋ",
          "original_form": "진영이랑 공찬이랑 구별 못했었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100036908.1.5",
          "form": "헐 근데 그 둘 닮았다고 생각하는 사람 많더라고요!!!???????",
          "original_form": "헐 근데 그 둘 닮았다고 생각하는 사람 많더라고요!!!???????",
          "speaker_id": "1",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100036908.1.6",
          "form": "제 친구들도 맨날 헷갈려햤어요!!!!!!!",
          "original_form": "제 친구들도 맨날 헷갈려햤어요!!!!!!!",
          "speaker_id": "1",
          "time": "20210831 14:47"
        },
        {
          "id": "MDRW2100036908.1.7",
          "form": "헐 진짜여???!",
          "original_form": "헐 진짜여???!",
          "speaker_id": "2",
          "time": "20210831 14:48"
        },
        {
          "id": "MDRW2100036908.1.8",
          "form": "저만 헷갈린게 아니구낰ㅋㅋ",
          "original_form": "저만 헷갈린게 아니구낰ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:49"
        },
        {
          "id": "MDRW2100036908.1.9",
          "form": "일단 둘 다 존잘이라서 더 헷갈린다구여!!",
          "original_form": "일단 둘 다 존잘이라서 더 헷갈린다구여!!",
          "speaker_id": "2",
          "time": "20210831 14:49"
        },
        {
          "id": "MDRW2100036908.1.10",
          "form": "맞어요 그쯤 대박 존잘들",
          "original_form": "맞어요 그쯤 대박 존잘들",
          "speaker_id": "1",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100036908.1.11",
          "form": "엘 진영 공찬....",
          "original_form": "엘 진영 공찬....",
          "speaker_id": "1",
          "time": "20210831 14:53"
        },
        {
          "id": "MDRW2100036908.1.12",
          "form": "아 헐",
          "original_form": "아 헐",
          "speaker_id": "2",
          "time": "20210831 14:54"
        },
        {
          "id": "MDRW2100036908.1.13",
          "form": "저의 인생 첫번째 최애 엘입니다.",
          "original_form": "저의 인생 첫번째 최애 엘입니다.",
          "speaker_id": "2",
          "time": "20210831 14:54"
        },
        {
          "id": "MDRW2100036908.1.14",
          "form": "인피니트 ...",
          "original_form": "인피니트 ...",
          "speaker_id": "2",
          "time": "20210831 14:55"
        },
        {
          "id": "MDRW2100036908.1.15",
          "form": "아 인피니트 ㄹㅇ 제 추억의 그룹",
          "original_form": "아 인피니트 ㄹㅇ 제 추억의 그룹",
          "speaker_id": "1",
          "time": "20210831 14:55"
        },
        {
          "id": "MDRW2100036908.1.16",
          "form": "우현한테 빠져버린..",
          "original_form": "우현한테 빠져버린..",
          "speaker_id": "1",
          "time": "20210831 14:55"
        },
        {
          "id": "MDRW2100036908.1.17",
          "form": "앗 우현이 그시절 조련갑 ..",
          "original_form": "앗 우현이 그시절 조련갑 ..",
          "speaker_id": "2",
          "time": "20210831 14:55"
        },
        {
          "id": "MDRW2100036908.1.18",
          "form": "하트의 원조 아니냐구여 ㅋㅋㅋ",
          "original_form": "하트의 원조 아니냐구여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:56"
        },
        {
          "id": "MDRW2100036908.1.19",
          "form": "아 그니까요... 정말... 정말 사랑했다",
          "original_form": "아 그니까요... 정말... 정말 사랑했다",
          "speaker_id": "1",
          "time": "20210831 14:56"
        },
        {
          "id": "MDRW2100036908.1.20",
          "form": "지금은 군대간 거 같던데..",
          "original_form": "지금은 군대간 거 같던데..",
          "speaker_id": "1",
          "time": "20210831 14:56"
        },
        {
          "id": "MDRW2100036908.1.21",
          "form": "제대했을껄요 !!",
          "original_form": "제대했을껄요 !!",
          "speaker_id": "2",
          "time": "20210831 14:57"
        },
        {
          "id": "MDRW2100036908.1.22",
          "form": "저 얼마전에 유튭에서 본거같아욬ㅋㅋㅋ",
          "original_form": "저 얼마전에 유튭에서 본거같아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 14:57"
        },
        {
          "id": "MDRW2100036908.1.23",
          "form": "헛 그렇구만요... 넘 구구구구구오빠라.. 신경을 ^,^..",
          "original_form": "헛 그렇구만요... 넘 구구구구구오빠라.. 신경을 ^,^..",
          "speaker_id": "1",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100036908.1.24",
          "form": "name2님 인피티느 최애곡.먼가요",
          "original_form": "&name2&님 인피티느 최애곡.먼가요",
          "speaker_id": "1",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100036908.1.25",
          "form": "전",
          "original_form": "전",
          "speaker_id": "2",
          "time": "20210831 15:01"
        },
        {
          "id": "MDRW2100036908.1.26",
          "form": "타이틀 중에서는 추격자여 ...",
          "original_form": "타이틀 중에서는 추격자여 ...",
          "speaker_id": "2",
          "time": "20210831 15:02"
        },
        {
          "id": "MDRW2100036908.1.27",
          "form": "수록곡은 틱톡 필소뱃 ...",
          "original_form": "수록곡은 틱톡 필소뱃 ...",
          "speaker_id": "2",
          "time": "20210831 15:02"
        },
        {
          "id": "MDRW2100036908.1.28",
          "form": "오마이갓 틱톡과",
          "original_form": "오마이갓 틱톡과",
          "speaker_id": "1",
          "time": "20210831 15:03"
        },
        {
          "id": "MDRW2100036908.1.29",
          "form": "픽소뱃을 아신다니",
          "original_form": "픽소뱃을 아신다니",
          "speaker_id": "1",
          "time": "20210831 15:03"
        },
        {
          "id": "MDRW2100036908.1.30",
          "form": "에바쌈바 저 주변에 수록곡 듣는 사람 없여셔",
          "original_form": "에바쌈바 저 주변에 수록곡 듣는 사람 없여셔",
          "speaker_id": "1",
          "time": "20210831 15:03"
        },
        {
          "id": "MDRW2100036908.1.31",
          "form": "맨날 눈물 흘렸단 말이에요!",
          "original_form": "맨날 눈물 흘렸단 말이에요!",
          "speaker_id": "1",
          "time": "20210831 15:03"
        },
        {
          "id": "MDRW2100036908.1.32",
          "form": "하",
          "original_form": "하",
          "speaker_id": "2",
          "time": "20210831 15:03"
        },
        {
          "id": "MDRW2100036908.1.33",
          "form": "수록곡이 근본인더 !!!",
          "original_form": "수록곡이 근본인더 !!!",
          "speaker_id": "2",
          "time": "20210831 15:03"
        },
        {
          "id": "MDRW2100036908.1.34",
