      "utterance": [
        {
          "id": "MDRW2100019555.1.1",
          "form": "맞아요 딱복은 좀 일찍 나오니깐",
          "original_form": "맞아요 딱복은 좀 일찍 나오니깐",
          "speaker_id": "1",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100019555.1.2",
          "form": "지금은 좀 철은 아니죠",
          "original_form": "지금은 좀 철은 아니죠",
          "speaker_id": "1",
          "time": "20210827 20:16"
        },
        {
          "id": "MDRW2100019555.1.3",
          "form": "딱복 게절은 살짝 지났죠",
          "original_form": "딱복 게절은 살짝 지났죠",
          "speaker_id": "1",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019555.1.4",
          "form": "그래도 8월까진 간간히 나오는데 살짝 덜단느낌",
          "original_form": "그래도 8월까진 간간히 나오는데 살짝 덜단느낌",
          "speaker_id": "1",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019555.1.5",
          "form": "맞아요 요즘은 물복 당도가 더 올라간거같아요",
          "original_form": "맞아요 요즘은 물복 당도가 더 올라간거같아요",
          "speaker_id": "2",
          "time": "20210827 20:17"
        },
        {
          "id": "MDRW2100019555.1.6",
          "form": "전 낙엽이나 꽃보다도 과일로 게절을 더 느낍니다 ㅋㅋ",
          "original_form": "전 낙엽이나 꽃보다도 과일로 게절을 더 느낍니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100019555.1.7",
          "form": "인기 많은 농원들은 7월달쯤에 예약이 다 끝나더라구요",
          "original_form": "인기 많은 농원들은 7월달쯤에 예약이 다 끝나더라구요",
          "speaker_id": "2",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100019555.1.8",
          "form": "우와 맛있는거 사드시는구나 전 청과시장서 대충 골라서 좀 평범한 맛이에요",
          "original_form": "우와 맛있는거 사드시는구나 전 청과시장서 대충 골라서 좀 평범한 맛이에요",
          "speaker_id": "1",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100019555.1.9",
          "form": "저도 보통은 마트에서 사먹어요 ㅋㅋㅋㅋ",
          "original_form": "저도 보통은 마트에서 사먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019555.1.10",
          "form": "꽃은 가로수 말고 거의 볼 일이 없고",
          "original_form": "꽃은 가로수 말고 거의 볼 일이 없고",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019555.1.11",
          "form": "낙엽도 그런데",
          "original_form": "낙엽도 그런데",
          "speaker_id": "2",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019555.1.12",
          "form": "과일은 식탁에서 자주 보니까요 ㅋㅋㅋㅋ",
          "original_form": "과일은 식탁에서 자주 보니까요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100019555.1.13",
          "form": "그리고 낙엽은 맛이없지만 과일은 맛과 향이 좋자나요 ㅎㅎㅎ",
          "original_form": "그리고 낙엽은 맛이없지만 과일은 맛과 향이 좋자나요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100019555.1.14",
          "form": "이제 가을이면 사과라고 보통 생각하지만 맛있는 사과가 나올때까진 좀 남았으니",
          "original_form": "이제 가을이면 사과라고 보통 생각하지만 맛있는 사과가 나올때까진 좀 남았으니",
          "speaker_id": "1",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100019555.1.15",
          "form": "그때까지 복숭아 잘 즐기심되겠네요",
          "original_form": "그때까지 복숭아 잘 즐기심되겠네요",
          "speaker_id": "1",
          "time": "20210827 20:20"
        },
        {
          "id": "MDRW2100019555.1.16",
          "form": "여름에 무화과도 맛있게 먹었는데",
          "original_form": "여름에 무화과도 맛있게 먹었는데",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019555.1.17",
          "form": "끝물이라 아쉬워요",
          "original_form": "끝물이라 아쉬워요",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019555.1.18",
          "form": "의외로 여름과일들이 다 맛있는거같아요",
          "original_form": "의외로 여름과일들이 다 맛있는거같아요",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019555.1.19",
          "form": "무화과는 가을까지 나오지 않나여?",
          "original_form": "무화과는 가을까지 나오지 않나여?",
          "speaker_id": "1",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019555.1.20",
          "form": "앗 그런가요??",
          "original_form": "앗 그런가요??",
          "speaker_id": "2",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019555.1.21",
          "form": "네 전 가을까지 좀 사머것어요",
          "original_form": "네 전 가을까지 좀 사머것어요",
          "speaker_id": "1",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019555.1.22",
          "form": "아 늦여름부터 시작이네요 ㅋㅋㅋㅋ",
          "original_form": "아 늦여름부터 시작이네요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019555.1.23",
          "form": "그럼 전 사과 기다리는동안 무화과 먹어야겠어ㅛ",
          "original_form": "그럼 전 사과 기다리는동안 무화과 먹어야겠어ㅛ",
          "speaker_id": "2",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019555.1.24",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100019555.1.25",
          "form": "저도 내일 무화과를 사야겠어요",
          "original_form": "저도 내일 무화과를 사야겠어요",
          "speaker_id": "1",
          "time": "20210827 20:25"
        }
      ]
    }
