      "utterance": [
        {
          "id": "MDRW2100020753.1.1",
          "form": "그럴수밖에 없깄네요",
          "original_form": "그럴수밖에 없깄네요",
          "speaker_id": "2",
          "time": "20210828 12:06"
        },
        {
          "id": "MDRW2100020753.1.2",
          "form": "예이!!",
          "original_form": "예이!!",
          "speaker_id": "2",
          "time": "20210828 12:06"
        },
        {
          "id": "MDRW2100020753.1.3",
          "form": "정말 배고팠어요",
          "original_form": "정말 배고팠어요",
          "speaker_id": "2",
          "time": "20210828 12:06"
        },
        {
          "id": "MDRW2100020753.1.4",
          "form": "ㅋㅋㅋㅋ맛있는 점심시간~",
          "original_form": "ㅋㅋㅋㅋ맛있는 점심시간~",
          "speaker_id": "1",
          "time": "20210828 12:06"
        },
        {
          "id": "MDRW2100020753.1.5",
          "form": "지금 에어프라이기에 치킨 돌려볼까 해요",
          "original_form": "지금 에어프라이기에 치킨 돌려볼까 해요",
          "speaker_id": "1",
          "time": "20210828 12:08"
        },
        {
          "id": "MDRW2100020753.1.6",
          "form": "에어프라이기 있으시군요",
          "original_form": "에어프라이기 있으시군요",
          "speaker_id": "2",
          "time": "20210828 12:22"
        },
        {
          "id": "MDRW2100020753.1.7",
          "form": "그럼 치킨 훨씬 맛있죠",
          "original_form": "그럼 치킨 훨씬 맛있죠",
          "speaker_id": "2",
          "time": "20210828 12:22"
        },
        {
          "id": "MDRW2100020753.1.8",
          "form": "예전엔 전자레인지에 돌려먹었는데",
          "original_form": "예전엔 전자레인지에 돌려먹었는데",
          "speaker_id": "2",
          "time": "20210828 12:22"
        },
        {
          "id": "MDRW2100020753.1.9",
          "form": "에아프라이어기는 바삭함까지 살아나서 너무 좋아요",
          "original_form": "에아프라이어기는 바삭함까지 살아나서 너무 좋아요",
          "speaker_id": "2",
          "time": "20210828 12:23"
        },
        {
          "id": "MDRW2100020753.1.10",
          "form": "맞아요 ㅋㅋ 에어프라이기는 막 눅눅했잖아요",
          "original_form": "맞아요 ㅋㅋ 에어프라이기는 막 눅눅했잖아요",
          "speaker_id": "1",
          "time": "20210828 12:24"
        },
        {
          "id": "MDRW2100020753.1.11",
          "form": "요새 에어프라이기로 조리하는 제품 많아져서 행복해요",
          "original_form": "요새 에어프라이기로 조리하는 제품 많아져서 행복해요",
          "speaker_id": "1",
          "time": "20210828 12:24"
        },
        {
          "id": "MDRW2100020753.1.12",
          "form": "맞아요 훨씬 간단하고",
          "original_form": "맞아요 훨씬 간단하고",
          "speaker_id": "2",
          "time": "20210828 12:27"
        },
        {
          "id": "MDRW2100020753.1.13",
          "form": "진짜 좋은 제품",
          "original_form": "진짜 좋은 제품",
          "speaker_id": "2",
          "time": "20210828 12:27"
        },
        {
          "id": "MDRW2100020753.1.14",
          "form": "집에서 요리하게 되더라구요",
          "original_form": "집에서 요리하게 되더라구요",
          "speaker_id": "2",
          "time": "20210828 12:27"
        },
        {
          "id": "MDRW2100020753.1.15",
          "form": "그냥 넣고 돌리기만하면 돼서ㅋㅋㅋ",
          "original_form": "그냥 넣고 돌리기만하면 돼서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:27"
        },
        {
          "id": "MDRW2100020753.1.16",
          "form": "요새 CJ에서 그런거 잘만드는 것 같아요",
          "original_form": "요새 CJ에서 그런거 잘만드는 것 같아요",
          "speaker_id": "1",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100020753.1.17",
          "form": "돈까스도 이젠 기름에 안튀겨도 맛있게 되더라구요~",
          "original_form": "돈까스도 이젠 기름에 안튀겨도 맛있게 되더라구요~",
          "speaker_id": "1",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100020753.1.18",
          "form": "치즈스틱도 !!",
          "original_form": "치즈스틱도 !!",
          "speaker_id": "1",
          "time": "20210828 12:28"
        },
        {
          "id": "MDRW2100020753.1.19",
          "form": "맞아요 치즈스틱",
          "original_form": "맞아요 치즈스틱",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020753.1.20",
          "form": "해시브라운도 맛있고",
          "original_form": "해시브라운도 맛있고",
          "speaker_id": "2",
          "time": "20210828 12:31"
        },
        {
          "id": "MDRW2100020753.1.21",
          "form": "닭꼬치",
          "original_form": "닭꼬치",
          "speaker_id": "2",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020753.1.22",
          "form": "노브랜드 닭꼬치 먹어보셨나요?",
          "original_form": "노브랜드 닭꼬치 먹어보셨나요?",
          "speaker_id": "2",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020753.1.23",
          "form": "그거 맛있더라구요",
          "original_form": "그거 맛있더라구요",
          "speaker_id": "2",
          "time": "20210828 12:32"
        },
        {
          "id": "MDRW2100020753.1.24",
          "form": "어 그런 것도 에어프라이기 돼요?",
          "original_form": "어 그런 것도 에어프라이기 돼요?",
          "speaker_id": "1",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020753.1.25",
          "form": "저 지금까지 닭꼬치는 직접 꼬챙이에 꿰어서 먹었어요",
          "original_form": "저 지금까지 닭꼬치는 직접 꼬챙이에 꿰어서 먹었어요",
          "speaker_id": "1",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020753.1.26",
          "form": "로스터리가 되는 에어프라이기여서 ㅋㅋ",
          "original_form": "로스터리가 되는 에어프라이기여서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020753.1.27",
          "form": "오 그렇군여",
          "original_form": "오 그렇군여",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020753.1.28",
          "form": "근데 그렇게 먹는게 더 맛있겠는데요",
          "original_form": "근데 그렇게 먹는게 더 맛있겠는데요",
          "speaker_id": "2",
          "time": "20210828 12:33"
        },
        {
          "id": "MDRW2100020753.1.29",
          "form": "에어프라이어기 기능으로도 되긴해요ㅋㅋㅋ",
          "original_form": "에어프라이어기 기능으로도 되긴해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020753.1.30",
          "form": "전 아직 자취를 안하지먄",
          "original_form": "전 아직 자취를 안하지먄",
          "speaker_id": "2",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020753.1.31",
          "form": "자취하게 되면 이것부터 챙기려고요",
          "original_form": "자취하게 되면 이것부터 챙기려고요",
          "speaker_id": "2",
          "time": "20210828 12:34"
        },
        {
          "id": "MDRW2100020753.1.32",
          "form": "꼭 큰거 사세요 큰거 큰거",
          "original_form": "꼭 큰거 사세요 큰거 큰거",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020753.1.33",
          "form": "안 그러면 나중에 생닭 같은 것도 못넣고 짜증나요",
          "original_form": "안 그러면 나중에 생닭 같은 것도 못넣고 짜증나요",
          "speaker_id": "1",
          "time": "20210828 12:35"
        },
        {
          "id": "MDRW2100020753.1.34",
          "form": "아 그렇겠네요",
          "original_form": "아 그렇겠네요",
          "speaker_id": "2",
          "time": "20210828 12:51"
        },
        {
          "id": "MDRW2100020753.1.35",
          "form": "잘라서 깨작깨작 먹어여하는 불상사가",
          "original_form": "잘라서 깨작깨작 먹어여하는 불상사가",
          "speaker_id": "2",
          "time": "20210828 12:52"
        },
        {
          "id": "MDRW2100020753.1.36",
          "form": "그걸 생각 못했어욬ㅋㅋㅋ",
          "original_form": "그걸 생각 못했어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 12:52"
        },
        {
          "id": "MDRW2100020753.1.37",
          "form": "친구들 자주 놀러오는 편이면..",
          "original_form": "친구들 자주 놀러오는 편이면..",
          "speaker_id": "1",
          "time": "20210828 12:54"
        },
        {
          "id": "MDRW2100020753.1.38",
          "form": "한꺼번에 하기도 어렵죠 ㅠ",
          "original_form": "한꺼번에 하기도 어렵죠 ㅠ",
          "speaker_id": "1",
          "time": "20210828 12:54"
        },
        {
          "id": "MDRW2100020753.1.39",
          "form": "전 지금 에어프라이기에 치킨이랑 치즈볼 했는데",
          "original_form": "전 지금 에어프라이기에 치킨이랑 치즈볼 했는데",
          "speaker_id": "1",
          "time": "20210828 12:55"
        },
        {
          "id": "MDRW2100020753.1.40",
          "form": "맛있엉 ㅛㅜㅜ",
          "original_form": "맛있엉 ㅛㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 12:55"
        },
        {
          "id": "MDRW2100020753.1.41",
          "form": "치킨에 치즈볼까지",
          "original_form": "치킨에 치즈볼까지",
          "speaker_id": "2",
          "time": "20210828 12:57"
        },
        {
          "id": "MDRW2100020753.1.42",
          "form": "제대로네요",
          "original_form": "제대로네요",
          "speaker_id": "2",
          "time": "20210828 12:57"
        },
        {
          "id": "MDRW2100020753.1.43",
          "form": "완전 에어프라이어기 혁명",
          "original_form": "완전 에어프라이어기 혁명",
          "speaker_id": "2",
          "time": "20210828 12:57"
        },
        {
          "id": "MDRW2100020753.1.44",
          "form": "어느 브랜드 치킨 시키셨어요?",
          "original_form": "어느 브랜드 치킨 시키셨어요?",
          "speaker_id": "2",
          "time": "20210828 12:57"
        },
        {
          "id": "MDRW2100020753.1.45",
          "form": "후참잘이라고 아세요?",
          "original_form": "후참잘이라고 아세요?",
          "speaker_id": "1",
          "time": "20210828 12:59"
        },
        {
          "id": "MDRW2100020753.1.46",
          "form": "치즈볼은 원래 집에 있던거에요 ㅋㅋ",
          "original_form": "치즈볼은 원래 집에 있던거에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 12:59"
        }
