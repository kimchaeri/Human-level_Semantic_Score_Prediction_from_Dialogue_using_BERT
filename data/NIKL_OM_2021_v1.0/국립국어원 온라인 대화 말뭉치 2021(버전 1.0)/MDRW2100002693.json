      "utterance": [
        {
          "id": "MDRW2100002693.1.1",
          "form": "엠비씨 전시회는 다름 지역에서해?",
          "original_form": "엠비씨 전시회는 다름 지역에서해?",
          "speaker_id": "1",
          "time": "20210820 15:10"
        },
        {
          "id": "MDRW2100002693.1.2",
          "form": "음? 엠비씨에서 게임 전시는 열리지 않아.",
          "original_form": "음? 엠비씨에서 게임 전시는 열리지 않아.",
          "speaker_id": "2",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002693.1.3",
          "form": "아하 내가 헷갈렸나봐",
          "original_form": "아하 내가 헷갈렸나봐",
          "speaker_id": "1",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002693.1.4",
          "form": "그랜드체이스 다시 할 수 있더라",
          "original_form": "그랜드체이스 다시 할 수 있더라",
          "speaker_id": "2",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002693.1.5",
          "form": "내가 좋아하던 게임. 기억나?",
          "original_form": "내가 좋아하던 게임. 기억나?",
          "speaker_id": "2",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002693.1.6",
          "form": "우와 나는 그거 이름만 알아",
          "original_form": "우와 나는 그거 이름만 알아",
          "speaker_id": "1",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002693.1.7",
          "form": "나 그거 레벨 높고 꽤 했었는데",
          "original_form": "나 그거 레벨 높고 꽤 했었는데",
          "speaker_id": "2",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002693.1.8",
          "form": "친구들과 꼬셔서 함께 하곤했어.",
          "original_form": "친구들과 꼬셔서 함께 하곤했어.",
          "speaker_id": "2",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002693.1.9",
          "form": "그럼 다시시작하면",
          "original_form": "그럼 다시시작하면",
          "speaker_id": "1",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002693.1.10",
          "form": "고등학생 때 몰애 게임을 하기도 했지.",
          "original_form": "고등학생 때 몰애 게임을 하기도 했지.",
          "speaker_id": "2",
          "time": "20210820 15:11"
        },
        {
          "id": "MDRW2100002693.1.11",
          "form": "레벨이 유지되는거야?",
          "original_form": "레벨이 유지되는거야?",
          "speaker_id": "1",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002693.1.12",
          "form": "그 게임 서버종료도ㅒㅅ다가",
          "original_form": "그 게임 서버종료도ㅒㅅ다가",
          "speaker_id": "2",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002693.1.13",
          "form": "다른 플랫폼에서 여는거라",
          "original_form": "다른 플랫폼에서 여는거라",
          "speaker_id": "2",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002693.1.14",
          "form": "그건 아마 안 될 거 같아.",
          "original_form": "그건 아마 안 될 거 같아.",
          "speaker_id": "2",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002693.1.15",
          "form": "ㅠㅠ슬푸네",
          "original_form": "ㅠㅠ슬푸네",
          "speaker_id": "1",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002693.1.16",
          "form": "나중에 스팀으로 그랜드체이스를 해봐야겠어",
          "original_form": "나중에 스팀으로 그랜드체이스를 해봐야겠어",
          "speaker_id": "2",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002693.1.17",
          "form": "그럼에도 다시 시작할꺼니?",
          "original_form": "그럼에도 다시 시작할꺼니?",
          "speaker_id": "1",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002693.1.18",
          "form": "그 나중이 언제가 될 지는 모르겠어",
          "original_form": "그 나중이 언제가 될 지는 모르겠어",
          "speaker_id": "2",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002693.1.19",
          "form": "일단 올해랑 내년은 아니야.",
          "original_form": "일단 올해랑 내년은 아니야.",
          "speaker_id": "2",
          "time": "20210820 15:12"
        },
        {
          "id": "MDRW2100002693.1.20",
          "form": "추억 회상에 좋을 것 같아",
          "original_form": "추억 회상에 좋을 것 같아",
          "speaker_id": "1",
          "time": "20210820 15:12"
        }
      ]
