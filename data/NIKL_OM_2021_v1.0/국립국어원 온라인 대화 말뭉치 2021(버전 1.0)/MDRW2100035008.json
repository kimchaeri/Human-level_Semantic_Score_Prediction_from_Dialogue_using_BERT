        },
        {
          "id": "MDRW2100035008.1.2",
          "form": "name2님네는 라면머있나요",
          "original_form": "&name2&님네는 라면머있나요",
          "speaker_id": "1",
          "time": "20210828 23:46"
        },
        {
          "id": "MDRW2100035008.1.3",
          "form": "지금은 너구리 열라면 진라면매운맛 짜파게티 이렇게있네요",
          "original_form": "지금은 너구리 열라면 진라면매운맛 짜파게티 이렇게있네요",
          "speaker_id": "2",
          "time": "20210829 00:00"
        },
        {
          "id": "MDRW2100035008.1.4",
          "form": "와 많다",
          "original_form": "와 많다",
          "speaker_id": "1",
          "time": "20210829 00:01"
        },
        {
          "id": "MDRW2100035008.1.5",
          "form": "든든하겠네요",
          "original_form": "든든하겠네요",
          "speaker_id": "1",
          "time": "20210829 00:01"
        },
        {
          "id": "MDRW2100035008.1.6",
          "form": "그런데 꼭 없는 라면 먹고싶어지고 그래요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그런데 꼭 없는 라면 먹고싶어지고 그래요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100035008.1.7",
          "form": "아까 꼬꼬면 얘기햇더니 꼬꼬면 먹고싶네요 ㅋㅋㅋㅋㅋ",
          "original_form": "아까 꼬꼬면 얘기햇더니 꼬꼬면 먹고싶네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 00:03"
        },
        {
          "id": "MDRW2100035008.1.8",
          "form": "ㅋㅋ쇼핑없어도 되겠어요 당분간",
          "original_form": "ㅋㅋ쇼핑없어도 되겠어요 당분간",
          "speaker_id": "1",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100035008.1.9",
          "form": "ㅋㅋ 아 또 쇼핑하셔야겠당 그럼",
          "original_form": "ㅋㅋ 아 또 쇼핑하셔야겠당 그럼",
          "speaker_id": "1",
          "time": "20210829 00:04"
        },
        {
          "id": "MDRW2100035008.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ꼭 집에없는게 먹고싶어지는 청개구리심리...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ꼭 집에없는게 먹고싶어지는 청개구리심리...",
          "speaker_id": "2",
