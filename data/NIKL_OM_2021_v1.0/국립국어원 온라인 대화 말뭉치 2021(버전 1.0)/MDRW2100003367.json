      "utterance": [
        {
          "id": "MDRW2100003367.1.1",
          "form": "한식 양식 뭐가 좋아",
          "original_form": "한식 양식 뭐가 좋아",
          "speaker_id": "1",
          "time": "20210820 22:14"
        },
        {
          "id": "MDRW2100003367.1.2",
          "form": "나는 양식",
          "original_form": "나는 양식",
          "speaker_id": "2",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003367.1.3",
          "form": "나는 한식 ㅋㅋㅋ",
          "original_form": "나는 한식 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003367.1.4",
          "form": "난 김치를 별로 안좋아해서",
          "original_form": "난 김치를 별로 안좋아해서",
          "speaker_id": "2",
          "time": "20210820 22:15"
        },
        {
          "id": "MDRW2100003367.1.5",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210820 22:16"
        },
        {
          "id": "MDRW2100003367.1.6",
          "form": "김치를 안좋아하구나",
          "original_form": "김치를 안좋아하구나",
          "speaker_id": "1",
          "time": "20210820 22:16"
        },
        {
          "id": "MDRW2100003367.1.7",
          "form": "외국에서도 김치 생각 안나더라고",
          "original_form": "외국에서도 김치 생각 안나더라고",
          "speaker_id": "2",
          "time": "20210820 22:16"
        },
        {
          "id": "MDRW2100003367.1.8",
          "form": "그럴 수 있지",
          "original_form": "그럴 수 있지",
          "speaker_id": "1",
          "time": "20210820 22:17"
        },
        {
          "id": "MDRW2100003367.1.9",
          "form": "한식도 맛있는 거 많긴 해",
          "original_form": "한식도 맛있는 거 많긴 해",
          "speaker_id": "2",
          "time": "20210820 22:17"
        },
        {
          "id": "MDRW2100003367.1.10",
          "form": "그지그지",
          "original_form": "그지그지",
          "speaker_id": "1",
          "time": "20210820 22:17"
        },
        {
          "id": "MDRW2100003367.1.11",
          "form": "중식 일식 프랑스식 뭐 안가리긴 해",
          "original_form": "중식 일식 프랑스식 뭐 안가리긴 해",
          "speaker_id": "2",
          "time": "20210820 22:17"
        },
        {
          "id": "MDRW2100003367.1.12",
          "form": "나도 음식 잘 안가려서",
          "original_form": "나도 음식 잘 안가려서",
          "speaker_id": "1",
          "time": "20210820 22:18"
        },
        {
          "id": "MDRW2100003367.1.13",
          "form": "ㅋㅋㅋ 다 잘먹ㄴ",
          "original_form": "ㅋㅋㅋ 다 잘먹ㄴ",
          "speaker_id": "2",
          "time": "20210820 22:18"
        },
        {
          "id": "MDRW2100003367.1.14",
          "form": "다 잘 먹는게 보기 좋지",
          "original_form": "다 잘 먹는게 보기 좋지",
          "speaker_id": "2",
          "time": "20210820 22:18"
        },
        {
          "id": "MDRW2100003367.1.15",
          "form": "너는 최애 음식이 뭔데",
          "original_form": "너는 최애 음식이 뭔데",
          "speaker_id": "1",
          "time": "20210820 22:19"
        },
        {
          "id": "MDRW2100003367.1.16",
          "form": "돈까스 ㅋㅋㅋ 최근에 바뀌었어",
          "original_form": "돈까스 ㅋㅋㅋ 최근에 바뀌었어",
          "speaker_id": "2",
          "time": "20210820 22:19"
        },
        {
          "id": "MDRW2100003367.1.17",
          "form": "돈까스 맛있지 나도 좋아해",
          "original_form": "돈까스 맛있지 나도 좋아해",
          "speaker_id": "2",
          "time": "20210820 22:20"
        }
      ]
