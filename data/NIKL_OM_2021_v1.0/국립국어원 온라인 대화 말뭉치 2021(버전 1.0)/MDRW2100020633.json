      "utterance": [
        {
          "id": "MDRW2100020633.1.1",
          "form": "안녕하세요~ 학교 다니시나요?",
          "original_form": "안녕하세요~ 학교 다니시나요?",
          "speaker_id": "1",
          "time": "20210828 10:37"
        },
        {
          "id": "MDRW2100020633.1.2",
          "form": "안녕하세요~ 네! 이제 마지막 학기네요ㅎㅎ",
          "original_form": "안녕하세요~ 네! 이제 마지막 학기네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:38"
        },
        {
          "id": "MDRW2100020633.1.3",
          "form": "와 저도 마지막 학기에요!",
          "original_form": "와 저도 마지막 학기에요!",
          "speaker_id": "1",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020633.1.4",
          "form": "코로나 때문에 강의 단톡방이 몇 개 생기더라고요.. 부담감이 하하",
          "original_form": "코로나 때문에 강의 단톡방이 몇 개 생기더라고요.. 부담감이 하하",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020633.1.5",
          "form": "아 저는 개강이라는 현실을 외면 중입니다..ㅎㅎㅎ",
          "original_form": "아 저는 개강이라는 현실을 외면 중입니다..ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020633.1.6",
          "form": "단톡방 아직 안 들어갔거든요..ㅎㅎㄹ",
          "original_form": "단톡방 아직 안 들어갔거든요..ㅎㅎㄹ",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020633.1.7",
          "form": "아 그렇군요!!",
          "original_form": "아 그렇군요!!",
          "speaker_id": "1",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020633.1.8",
          "form": "맞아요.. 집에서 최대한 뒹굴거리다가 가야겠어요",
          "original_form": "맞아요.. 집에서 최대한 뒹굴거리다가 가야겠어요",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020633.1.9",
          "form": "수강신청은 잘 하셨나요?",
          "original_form": "수강신청은 잘 하셨나요?",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020633.1.10",
          "form": "네! 4학년되니 수강신청이 쉽더라구요ㅋㅋㅋㅋ",
          "original_form": "네! 4학년되니 수강신청이 쉽더라구요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020633.1.11",
          "form": "올클했습니다!",
          "original_form": "올클했습니다!",
          "speaker_id": "2",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020633.1.12",
          "form": "name1님은요??",
          "original_form": "&name1&님은요??",
          "speaker_id": "2",
          "time": "20210828 10:46"
        },
        {
          "id": "MDRW2100020633.1.13",
          "form": "이야~ 축하합니다 저도 쉽게 후다닥 해서 편했네요 ㅎㅎ",
          "original_form": "이야~ 축하합니다 저도 쉽게 후다닥 해서 편했네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020633.1.14",
          "form": "공강은 있으신지요? 허허..",
          "original_form": "공강은 있으신지요? 허허..",
          "speaker_id": "1",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020633.1.15",
          "form": "하하.. 제가 복전을 늦게 해서...",
          "original_form": "하하.. 제가 복전을 늦게 해서...",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020633.1.16",
          "form": "공강은 없습니다...",
          "original_form": "공강은 없습니다...",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020633.1.17",
          "form": "4학년인데 공강이 없다는 사실이 슬프네요..하하",
          "original_form": "4학년인데 공강이 없다는 사실이 슬프네요..하하",
          "speaker_id": "2",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020633.1.18",
          "form": "아이고..아이고.. 그렇군요",
          "original_form": "아이고..아이고.. 그렇군요",
          "speaker_id": "1",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020633.1.19",
          "form": "휴학은 하셨었나요? 주변 친구들은 휴학 한 번씩 했어서요",
          "original_form": "휴학은 하셨었나요? 주변 친구들은 휴학 한 번씩 했어서요",
          "speaker_id": "1",
          "time": "20210828 10:49"
        },
        {
          "id": "MDRW2100020633.1.20",
          "form": "뭔가 학생이 끝나는게 싫다고 그러더군요 ㅋㅋ",
          "original_form": "뭔가 학생이 끝나는게 싫다고 그러더군요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020633.1.21",
          "form": "하... 저는 부모님이 휴학을 매우 싫어하셔서..",
          "original_form": "하... 저는 부모님이 휴학을 매우 싫어하셔서..",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020633.1.22",
          "form": "한번도 못했습니다..",
          "original_form": "한번도 못했습니다..",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020633.1.23",
          "form": "눈물나네요..ㅠ",
          "original_form": "눈물나네요..ㅠ",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020633.1.24",
          "form": "저기요 저도에요.. 하..",
          "original_form": "저기요 저도에요.. 하..",
          "speaker_id": "1",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020633.1.25",
          "form": "부럽더군요.. 휴학하고 놀러다니는 친구들",
          "original_form": "부럽더군요.. 휴학하고 놀러다니는 친구들",
          "speaker_id": "1",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020633.1.26",
          "form": "name1님도군요ㅠㅠㅠ",
          "original_form": "&name1&님도군요ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020633.1.27",
          "form": "휴학한번한다고 그렇게 큰일이 일어나지 않는데..",
          "original_form": "휴학한번한다고 그렇게 큰일이 일어나지 않는데..",
          "speaker_id": "2",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020633.1.28",
          "form": "참.. 눈물나네요ㅠ",
          "original_form": "참.. 눈물나네요ㅠ",
          "speaker_id": "2",
          "time": "20210828 10:52"
        },
        {
          "id": "MDRW2100020633.1.29",
          "form": "그러니까요..저는 이유를 물으니까 얼른 다니고 취직하라고 그러더라고요.",
          "original_form": "그러니까요..저는 이유를 물으니까 얼른 다니고 취직하라고 그러더라고요.",
          "speaker_id": "1",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020633.1.30",
          "form": "저도 돈은 빨리 벌고 싶은데 취직은 하기싫어요.. 누구나 그렇겠지만",
          "original_form": "저도 돈은 빨리 벌고 싶은데 취직은 하기싫어요.. 누구나 그렇겠지만",
          "speaker_id": "1",
          "time": "20210828 10:54"
        },
        {
          "id": "MDRW2100020633.1.31",
          "form": "맞아요..ㅎㅎㅎ 돈많은백수가 꿈이랍니다..하하하",
          "original_form": "맞아요..ㅎㅎㅎ 돈많은백수가 꿈이랍니다..하하하",
          "speaker_id": "2",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020633.1.32",
          "form": "학교 다니면서 동아리는 하셨나요??",
          "original_form": "학교 다니면서 동아리는 하셨나요??",
          "speaker_id": "2",
          "time": "20210828 10:55"
        },
        {
          "id": "MDRW2100020633.1.33",
          "form": "아니요.. 동아리 다 술만 먹는다고 해서 안했어요ㅋㅋㅋㅋㅋ술을 별로 안 좋아해서",
          "original_form": "아니요.. 동아리 다 술만 먹는다고 해서 안했어요ㅋㅋㅋㅋㅋ술을 별로 안 좋아해서",
          "speaker_id": "1",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020633.1.34",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100020633.1.35",
          "form": "아잇 저두욬ㅋㅋㅋㅋㅋ",
          "original_form": "아잇 저두욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020633.1.36",
          "form": "들어가고 싶은 동아리도 딱히 없었어요..",
          "original_form": "들어가고 싶은 동아리도 딱히 없었어요..",
          "speaker_id": "2",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020633.1.37",
          "form": "아잇 ㅋㅋㅋㅋ 잘 맞네요",
          "original_form": "아잇 ㅋㅋㅋㅋ 잘 맞네요",
          "speaker_id": "1",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100020633.1.38",
          "form": "졸업하고 뭐 계획있으신가요?..",
          "original_form": "졸업하고 뭐 계획있으신가요?..",
          "speaker_id": "1",
          "time": "20210828 11:01"
        },
        {
          "id": "MDRW2100020633.1.39",
          "form": "아무생각 없이 여행가고싶은데 또 취업은 해야해서..참",
          "original_form": "아무생각 없이 여행가고싶은데 또 취업은 해야해서..참",
