      "utterance": [
        {
          "id": "MDRW2100012208.1.1",
          "form": "다이어트 하고 계신가요?",
          "original_form": "다이어트 하고 계신가요?",
          "speaker_id": "1",
          "time": "20210824 20:19"
        },
        {
          "id": "MDRW2100012208.1.2",
          "form": "다이어트보다는",
          "original_form": "다이어트보다는",
          "speaker_id": "2",
          "time": "20210824 20:19"
        },
        {
          "id": "MDRW2100012208.1.3",
          "form": "건강을 계선하는중입니다",
          "original_form": "건강을 계선하는중입니다",
          "speaker_id": "2",
          "time": "20210824 20:19"
        },
        {
          "id": "MDRW2100012208.1.4",
          "form": "다이어트는 부수적인 효과지요!",
          "original_form": "다이어트는 부수적인 효과지요!",
          "speaker_id": "2",
          "time": "20210824 20:19"
        },
        {
          "id": "MDRW2100012208.1.5",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "1",
          "time": "20210824 20:21"
        },
        {
          "id": "MDRW2100012208.1.6",
          "form": "어떤식으로 하고 계신가요",
          "original_form": "어떤식으로 하고 계신가요",
          "speaker_id": "1",
          "time": "20210824 20:21"
        },
        {
          "id": "MDRW2100012208.1.7",
          "form": "야식을 끊고! 당이 많이 들어간 간식을 대폭 줄였어요",
          "original_form": "야식을 끊고! 당이 많이 들어간 간식을 대폭 줄였어요",
          "speaker_id": "2",
          "time": "20210824 20:22"
        },
        {
          "id": "MDRW2100012208.1.8",
          "form": "그르고 집에서 틈틈히 운동을 해주고있어요",
          "original_form": "그르고 집에서 틈틈히 운동을 해주고있어요",
          "speaker_id": "2",
          "time": "20210824 20:23"
        },
        {
          "id": "MDRW2100012208.1.9",
          "form": "오 실행하신진 얼마나 되셨어요?",
          "original_form": "오 실행하신진 얼마나 되셨어요?",
          "speaker_id": "1",
          "time": "20210824 20:24"
        },
        {
          "id": "MDRW2100012208.1.10",
          "form": "약 2주정도 된거같아요",
          "original_form": "약 2주정도 된거같아요",
          "speaker_id": "2",
          "time": "20210824 20:24"
        },
        {
          "id": "MDRW2100012208.1.11",
          "form": "효과는 어때요?",
          "original_form": "효과는 어때요?",
          "speaker_id": "1",
          "time": "20210824 20:25"
        },
        {
          "id": "MDRW2100012208.1.12",
          "form": "눈에 보이나용",
          "original_form": "눈에 보이나용",
          "speaker_id": "1",
          "time": "20210824 20:25"
        },
        {
          "id": "MDRW2100012208.1.13",
          "form": "확 눈에 보이는 효과는 아직인거같아요 그렇지만",
          "original_form": "확 눈에 보이는 효과는 아직인거같아요 그렇지만",
          "speaker_id": "2",
          "time": "20210824 20:26"
        },
        {
          "id": "MDRW2100012208.1.14",
          "form": "체지방이 줄엇다는것은 어느정도 느껴지는거 같아요",
          "original_form": "체지방이 줄엇다는것은 어느정도 느껴지는거 같아요",
          "speaker_id": "2",
          "time": "20210824 20:26"
        },
        {
          "id": "MDRW2100012208.1.15",
          "form": "뱃살이 덜 느껴진다거나 얼굴살이 빠진다던가 그런?",
          "original_form": "뱃살이 덜 느껴진다거나 얼굴살이 빠진다던가 그런?",
          "speaker_id": "1",
          "time": "20210824 20:31"
        },
        {
          "id": "MDRW2100012208.1.16",
          "form": "응!뱃살접히는느낌이",
          "original_form": "응!뱃살접히는느낌이",
          "speaker_id": "2",
          "time": "20210824 20:58"
        },
        {
          "id": "MDRW2100012208.1.17",
          "form": "예전보다 얇아졌달까",
          "original_form": "예전보다 얇아졌달까",
          "speaker_id": "2",
          "time": "20210824 20:59"
        },
        {
          "id": "MDRW2100012208.1.18",
          "form": "얼굴은?",
          "original_form": "얼굴은?",
          "speaker_id": "1",
          "time": "20210824 21:41"
        },
        {
          "id": "MDRW2100012208.1.19",
          "form": "얼굴은 아직 변화가 없는것같인",
          "original_form": "얼굴은 아직 변화가 없는것같인",
          "speaker_id": "2",
          "time": "20210824 21:43"
        },
        {
          "id": "MDRW2100012208.1.20",
          "form": "저는 다이어트하니까",
          "original_form": "저는 다이어트하니까",
          "speaker_id": "1",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100012208.1.21",
          "form": "얼굴살부터 빠지던데",
          "original_form": "얼굴살부터 빠지던데",
          "speaker_id": "1",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100012208.1.22",
          "form": "역시 사람마다 다른가봐영",
          "original_form": "역시 사람마다 다른가봐영",
          "speaker_id": "1",
          "time": "20210824 21:44"
        },
        {
          "id": "MDRW2100012208.1.23",
          "form": "저는 얼굴이 제일 나중에 빠지는거 같아요",
          "original_form": "저는 얼굴이 제일 나중에 빠지는거 같아요",
          "speaker_id": "2",
          "time": "20210824 21:45"
        },
        {
          "id": "MDRW2100012208.1.24",
          "form": "빠져도 동그란건 어쩔수 없나봐요ㅠ",
          "original_form": "빠져도 동그란건 어쩔수 없나봐요ㅠ",
          "speaker_id": "2",
          "time": "20210824 21:46"
        },
        {
          "id": "MDRW2100012208.1.25",
          "form": "아 그래서 닉네임이 name2세요?",
          "original_form": "아 그래서 닉네임이 &name2&세요?",
          "speaker_id": "1",
          "time": "20210824 21:50"
        },
        {
          "id": "MDRW2100012208.1.26",
          "form": "그건 아니에요ㅎ 그나저나 name1은 건강관리를 어떻게하시나요?",
          "original_form": "그건 아니에요ㅎ 그나저나 &name1&은 건강관리를 어떻게하시나요?",
          "speaker_id": "2",
          "time": "20210824 21:52"
        },
        {
          "id": "MDRW2100012208.1.27",
          "form": "저는 안하는데..그나마 최근에는",
          "original_form": "저는 안하는데..그나마 최근에는",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012208.1.28",
          "form": "건강식먹고 산책도 자주 하고 그랬네요",
          "original_form": "건강식먹고 산책도 자주 하고 그랬네요",
          "speaker_id": "1",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012208.1.29",
          "form": "산책 좋은거 같아요!",
          "original_form": "산책 좋은거 같아요!",
          "speaker_id": "2",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012208.1.30",
          "form": "어떻게 산책하시나요?",
          "original_form": "어떻게 산책하시나요?",
          "speaker_id": "2",
          "time": "20210824 21:56"
        },
        {
          "id": "MDRW2100012208.1.31",
          "form": "기냥 막 가요..안정하고",
          "original_form": "기냥 막 가요..안정하고",
          "speaker_id": "1",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012208.1.32",
          "form": "그러다 너무 힘들어지면",
          "original_form": "그러다 너무 힘들어지면",
          "speaker_id": "1",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012208.1.33",
          "form": "안되겠다 하고 돌아옵니다",
          "original_form": "안되겠다 하고 돌아옵니다",
          "speaker_id": "1",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100012208.1.34",
          "form": "코스를 정하고 해야하지 않을까요?",
          "original_form": "코스를 정하고 해야하지 않을까요?",
          "speaker_id": "2",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100012208.1.35",
          "form": "너무 무리해서 걸으면 그것만큼 피로해지는것도 있는것같아요",
          "original_form": "너무 무리해서 걸으면 그것만큼 피로해지는것도 있는것같아요",
          "speaker_id": "2",
          "time": "20210824 21:58"
        },
        {
          "id": "MDRW2100012208.1.36",
          "form": "코스를 정해놓고 걸으면",
          "original_form": "코스를 정해놓고 걸으면",
          "speaker_id": "1",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012208.1.37",
          "form": "이미 그 길을 아니까",
          "original_form": "이미 그 길을 아니까",
          "speaker_id": "1",
          "time": "20210824 22:02"
        },
        {
          "id": "MDRW2100012208.1.38",
          "form": "나중엔 질려요..",
          "original_form": "나중엔 질려요..",
          "speaker_id": "1",
          "time": "20210824 22:03"
        }
      ]
    }
