      "utterance": [
        {
          "id": "MDRW2100007901.1.1",
          "form": "안녕하세요 저녁 드셨나요",
          "original_form": "안녕하세요 저녁 드셨나요",
          "speaker_id": "1",
          "time": "20210902 20:26"
        },
        {
          "id": "MDRW2100007901.1.2",
          "form": "네 저녁드셨나요?",
          "original_form": "네 저녁드셨나요?",
          "speaker_id": "2",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100007901.1.3",
          "form": "먹었죠ㅎㅎ 떡볶이 먹었습니다",
          "original_form": "먹었죠ㅎㅎ 떡볶이 먹었습니다",
          "speaker_id": "1",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100007901.1.4",
          "form": "name2님은 뭐드셨나요",
          "original_form": "&name2&님은 뭐드셨나요",
          "speaker_id": "1",
          "time": "20210902 21:36"
        },
        {
          "id": "MDRW2100007901.1.5",
          "form": "저는 고기구워먹었어요!",
          "original_form": "저는 고기구워먹었어요!",
          "speaker_id": "2",
          "time": "20210902 21:41"
        },
        {
          "id": "MDRW2100007901.1.6",
          "form": "우와 맛있었겠어요",
          "original_form": "우와 맛있었겠어요",
          "speaker_id": "1",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100007901.1.7",
          "form": "정육점아저씨가 오늘 돼지앞다리살 추천해주셔서 사왔어요",
          "original_form": "정육점아저씨가 오늘 돼지앞다리살 추천해주셔서 사왔어요",
          "speaker_id": "2",
          "time": "20210902 21:44"
        },
        {
          "id": "MDRW2100007901.1.8",
          "form": "돼지 앞다리살은 별로 못먹어본거 같은데",
          "original_form": "돼지 앞다리살은 별로 못먹어본거 같은데",
          "speaker_id": "1",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100007901.1.9",
          "form": "맛있나요",
          "original_form": "맛있나요",
          "speaker_id": "1",
          "time": "20210902 21:46"
        },
        {
          "id": "MDRW2100007901.1.10",
          "form": "질좋을땐 싸고 맛있는거 같아요",
          "original_form": "질좋을땐 싸고 맛있는거 같아요",
          "speaker_id": "2",
          "time": "20210902 21:47"
        },
        {
          "id": "MDRW2100007901.1.11",
          "form": "아닐경우는 좀 질겨요",
          "original_form": "아닐경우는 좀 질겨요",
          "speaker_id": "2",
          "time": "20210902 21:47"
        },
        {
          "id": "MDRW2100007901.1.12",
          "form": "아 잘보고 사야겠네요",
          "original_form": "아 잘보고 사야겠네요",
          "speaker_id": "1",
          "time": "20210902 21:49"
        },
        {
          "id": "MDRW2100007901.1.13",
          "form": "정육점분들한테 오늘 뭐가 좋은지 물어보셔요!",
          "original_form": "정육점분들한테 오늘 뭐가 좋은지 물어보셔요!",
          "speaker_id": "2",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100007901.1.14",
          "form": "매번 마트에 나와 있는 것만 사서",
          "original_form": "매번 마트에 나와 있는 것만 사서",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100007901.1.15",
          "form": "따로 정육점 가볼생각은 못했네요",
          "original_form": "따로 정육점 가볼생각은 못했네요",
          "speaker_id": "1",
          "time": "20210902 21:52"
        },
        {
          "id": "MDRW2100007901.1.16",
          "form": "자취하다보니 재료를 자주 사야해서 마트 정육점아저씨랑 친해졌어요ㅋㅋㅋㅋ",
          "original_form": "자취하다보니 재료를 자주 사야해서 마트 정육점아저씨랑 친해졌어요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 21:55"
        },
        {
          "id": "MDRW2100007901.1.17",
          "form": "와 자취하시는군요",
          "original_form": "와 자취하시는군요",
          "speaker_id": "1",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100007901.1.18",
          "form": "그럼 장도 전부 직접 보시겠어요",
          "original_form": "그럼 장도 전부 직접 보시겠어요",
          "speaker_id": "1",
          "time": "20210902 21:58"
        },
        {
          "id": "MDRW2100007901.1.19",
          "form": "네 삼일에 한번은 봐요",
          "original_form": "네 삼일에 한번은 봐요",
          "speaker_id": "2",
          "time": "20210902 22:00"
        }
      ]
