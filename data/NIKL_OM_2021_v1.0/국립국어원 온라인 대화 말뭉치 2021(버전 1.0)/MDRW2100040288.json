      "utterance": [
        {
          "id": "MDRW2100040288.1.1",
          "form": "아침에 공차 갔다 왔어요",
          "original_form": "아침에 공차 갔다 왔어요",
          "speaker_id": "1",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100040288.1.2",
          "form": "우리 맛있는 거 먹으러 어디 어디 가기로 했어 줘",
          "original_form": "우리 맛있는 거 먹으러 어디 어디 가기로 했어 줘",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040288.1.3",
          "form": "저 오늘 아마스빈 엄청 땡겼는데 공차 맛있었겠다 내요",
          "original_form": "저 오늘 아마스빈 엄청 땡겼는데 공차 맛있었겠다 내요",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040288.1.4",
          "form": "저는 타피오카 펄 엄청 좋아해요",
          "original_form": "저는 타피오카 펄 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040288.1.5",
          "form": "심심이 하면서 공차 맛있는 메뉴 알려주셔서",
          "original_form": "심심이 하면서 공차 맛있는 메뉴 알려주셔서",
          "speaker_id": "1",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040288.1.6",
          "form": "다른 분 깨져요",
          "original_form": "다른 분 깨져요",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040288.1.7",
          "form": "민트 초코칩에 얼음 빼고 타피오카 펄 시럽 두 번 펌핑",
          "original_form": "민트 초코칩에 얼음 빼고 타피오카 펄 시럽 두 번 펌핑",
          "speaker_id": "1",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040288.1.8",
          "form": "저도 알려주세요",
          "original_form": "저도 알려주세요",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040288.1.9",
          "form": "주문 외워서 갔어요",
          "original_form": "주문 외워서 갔어요",
          "speaker_id": "1",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040288.1.10",
          "form": "아 저는 민트초코 안 먹습니다",
          "original_form": "아 저는 민트초코 안 먹습니다",
          "speaker_id": "2",
          "time": "20210904 15:31"
        },
        {
          "id": "MDRW2100040288.1.11",
          "form": "민초 반대파예요",
          "original_form": "민초 반대파예요",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040288.1.12",
          "form": "근데 알바가 얼음 넣어줘 서 얻어서 밍밍했어요",
          "original_form": "근데 알바가 얼음 넣어줘 서 얻어서 밍밍했어요",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040288.1.13",
          "form": "저런",
          "original_form": "저런",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040288.1.14",
          "form": "공차에 초당 옥수수도 나왔더라구요",
          "original_form": "공차에 초당 옥수수도 나왔더라구요",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040288.1.15",
          "form": "저는 아마스빈 타피오카 펄 엄청 좋아해요",
          "original_form": "저는 아마스빈 타피오카 펄 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040288.1.16",
          "form": "타피오 팍 타피오카 펄이 제일 맛있었어요",
          "original_form": "타피오 팍 타피오카 펄이 제일 맛있었어요",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040288.1.17",
          "form": "초당 옥수수 넌 대체 무슨 마십니까",
          "original_form": "초당 옥수수 넌 대체 무슨 마십니까",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040288.1.18",
          "form": "ㅋㅋㅋ 카피 옥 카 펄",
          "original_form": "ㅋㅋㅋ 카피 옥 카 펄",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040288.1.19",
          "form": "초당 옥수수는이라고 했는데 넌이라고 되네요",
          "original_form": "초당 옥수수는이라고 했는데 넌이라고 되네요",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040288.1.20",
          "form": "우리 발음이 좀 새나 봐요",
          "original_form": "우리 발음이 좀 새나 봐요",
          "speaker_id": "2",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040288.1.21",
          "form": "왠지 고소한 수프 같은 느낌",
          "original_form": "왠지 고소한 수프 같은 느낌",
          "speaker_id": "1",
          "time": "20210904 15:32"
        },
        {
          "id": "MDRW2100040288.1.22",
          "form": "닐까요",
          "original_form": "닐까요",
          "speaker_id": "1",
          "time": "20210904 15:33"
        },
        {
          "id": "MDRW2100040288.1.23",
          "form": "아 진짜 맛있겠네요 스프도 먹은지 오래 됐어요",
          "original_form": "아 진짜 맛있겠네요 스프도 먹은지 오래 됐어요",
          "speaker_id": "2",
          "time": "20210904 15:33"
        },
        {
          "id": "MDRW2100040288.1.24",
          "form": "재밌고 좋네요",
          "original_form": "재밌고 좋네요",
          "speaker_id": "1",
          "time": "20210904 15:33"
        },
        {
          "id": "MDRW2100040288.1.25",
          "form": "스프에 밥 말아서 김치랑 먹으면 진짜 맛있는데",
          "original_form": "스프에 밥 말아서 김치랑 먹으면 진짜 맛있는데",
          "speaker_id": "2",
          "time": "20210904 15:33"
        }
      ]
