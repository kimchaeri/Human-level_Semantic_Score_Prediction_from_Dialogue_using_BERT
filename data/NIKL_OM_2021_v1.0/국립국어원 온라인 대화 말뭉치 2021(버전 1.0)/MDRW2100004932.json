      "utterance": [
        {
          "id": "MDRW2100004932.1.1",
          "form": "아, 맞아. 너 싸이월드 했었어?",
          "original_form": "아, 맞아. 너 싸이월드 했었어?",
          "speaker_id": "1",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004932.1.2",
          "form": "아니 잘 안했었어",
          "original_form": "아니 잘 안했었어",
          "speaker_id": "2",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004932.1.3",
          "form": "난 초등학생 때 너도나도 하니까 덩달아 하나 만들긴 했는데",
          "original_form": "난 초등학생 때 너도나도 하니까 덩달아 하나 만들긴 했는데",
          "speaker_id": "1",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004932.1.4",
          "form": "넌?",
          "original_form": "넌?",
          "speaker_id": "2",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004932.1.5",
          "form": "만들긴 했어도 거의 안했어",
          "original_form": "만들긴 했어도 거의 안했어",
          "speaker_id": "1",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004932.1.6",
          "form": "난 어릴때부터 아싸여서,",
          "original_form": "난 어릴때부터 아싸여서,",
          "speaker_id": "1",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004932.1.7",
          "form": "교류할 사람들이 없었어",
          "original_form": "교류할 사람들이 없었어",
          "speaker_id": "1",
          "time": "20210821 19:23"
        },
        {
          "id": "MDRW2100004932.1.8",
          "form": "나도",
          "original_form": "나도",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004932.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004932.1.10",
          "form": "초딩때는 싸이월드보다는",
          "original_form": "초딩때는 싸이월드보다는",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004932.1.11",
          "form": "다음 카페 주록 쓴 것같아",
          "original_form": "다음 카페 주록 쓴 것같아",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004932.1.12",
          "form": "싸이월드 부활한대?",
          "original_form": "싸이월드 부활한대?",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004932.1.13",
          "form": "나 처음 암",
          "original_form": "나 처음 암",
          "speaker_id": "2",
          "time": "20210821 19:24"
        },
        {
          "id": "MDRW2100004932.1.14",
          "form": "몇달전에 말 나왔더라고",
          "original_form": "몇달전에 말 나왔더라고",
          "speaker_id": "1",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004932.1.15",
          "form": "나야 잘 안써서 괜찮은데",
          "original_form": "나야 잘 안써서 괜찮은데",
          "speaker_id": "1",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004932.1.16",
          "form": "어떤 사람들은 흑역사 돌아온다고 절규하더라고",
          "original_form": "어떤 사람들은 흑역사 돌아온다고 절규하더라고",
          "speaker_id": "1",
          "time": "20210821 19:25"
        },
        {
          "id": "MDRW2100004932.1.17",
          "form": "아 데이터 다 복원되서 부활하나보네",
          "original_form": "아 데이터 다 복원되서 부활하나보네",
          "speaker_id": "2",
          "time": "20210821 19:27"
        },
        {
          "id": "MDRW2100004932.1.18",
          "form": "응 그렇다고 들었어",
          "original_form": "응 그렇다고 들었어",
          "speaker_id": "1",
          "time": "20210821 19:28"
        },
        {
          "id": "MDRW2100004932.1.19",
          "form": "근데 어릴때도 잘 안썼는데",
          "original_form": "근데 어릴때도 잘 안썼는데",
          "speaker_id": "1",
          "time": "20210821 19:29"
        },
        {
          "id": "MDRW2100004932.1.20",
          "form": "나도 안 쓸듯",
          "original_form": "나도 안 쓸듯",
          "speaker_id": "2",
          "time": "20210821 19:29"
        },
        {
          "id": "MDRW2100004932.1.21",
          "form": "지금 부활한다고 성실히 쓸 것 같진 않아",
          "original_form": "지금 부활한다고 성실히 쓸 것 같진 않아",
          "speaker_id": "1",
          "time": "20210821 19:29"
        },
        {
          "id": "MDRW2100004932.1.22",
          "form": "그렇지?",
          "original_form": "그렇지?",
          "speaker_id": "1",
          "time": "20210821 19:29"
        },
        {
          "id": "MDRW2100004932.1.23",
          "form": "항상 쓰던 SNS만 쓸것같음",
          "original_form": "항상 쓰던 SNS만 쓸것같음",
          "speaker_id": "2",
          "time": "20210821 19:29"
        },
        {
          "id": "MDRW2100004932.1.24",
          "form": "다른 사람들도 별로 쓸 생각없어보이던데",
          "original_form": "다른 사람들도 별로 쓸 생각없어보이던데",
          "speaker_id": "2",
          "time": "20210821 19:30"
        },
        {
          "id": "MDRW2100004932.1.25",
          "form": "그치. 이미 한 물 가버린 느낌이야",
          "original_form": "그치. 이미 한 물 가버린 느낌이야",
          "speaker_id": "1",
          "time": "20210821 19:31"
        },
        {
          "id": "MDRW2100004932.1.26",
          "form": "이거 부활해도 얼마나 갈 수 있을지",
          "original_form": "이거 부활해도 얼마나 갈 수 있을지",
          "speaker_id": "1",
          "time": "20210821 19:31"
        },
        {
          "id": "MDRW2100004932.1.27",
          "form": "뭔가 차별점이 있어야 한다고 생각해",
          "original_form": "뭔가 차별점이 있어야 한다고 생각해",
          "speaker_id": "1",
          "time": "20210821 19:31"
        },
        {
          "id": "MDRW2100004932.1.28",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210821 19:31"
        }
      ]
    }
