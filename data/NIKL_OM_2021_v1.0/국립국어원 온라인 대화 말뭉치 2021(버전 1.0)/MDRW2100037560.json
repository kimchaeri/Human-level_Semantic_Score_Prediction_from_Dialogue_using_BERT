      "utterance": [
        {
          "id": "MDRW2100037560.1.1",
          "form": "배고프다",
          "original_form": "배고프다",
          "speaker_id": "2",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100037560.1.2",
          "form": "난 아침을 든든하게 먹었어요",
          "original_form": "난 아침을 든든하게 먹었어요",
          "speaker_id": "1",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100037560.1.3",
          "form": "다이어트를 하니까",
          "original_form": "다이어트를 하니까",
          "speaker_id": "1",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100037560.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100037560.1.5",
          "form": "점심은 못 드시나요",
          "original_form": "점심은 못 드시나요",
          "speaker_id": "1",
          "time": "20210901 11:28"
        },
        {
          "id": "MDRW2100037560.1.6",
          "form": "뭐 드시나요??ㅋㅋㅋ",
          "original_form": "뭐 드시나요??ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 11:29"
        },
        {
          "id": "MDRW2100037560.1.7",
          "form": "ㅋㅋㅋㅋㅋ 그놈의 다이어트",
          "original_form": "ㅋㅋㅋㅋㅋ 그놈의 다이어트",
          "speaker_id": "2",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100037560.1.8",
          "form": "나 짬뽕밥 먹었어",
          "original_form": "나 짬뽕밥 먹었어",
          "speaker_id": "2",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100037560.1.9",
          "form": "배부르고 좋구먼",
          "original_form": "배부르고 좋구먼",
          "speaker_id": "2",
          "time": "20210901 12:24"
        },
        {
          "id": "MDRW2100037560.1.10",
          "form": "오 맛있겠네요",
          "original_form": "오 맛있겠네요",
          "speaker_id": "1",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100037560.1.11",
          "form": "얼큰하게ㅋㅋㅋ",
          "original_form": "얼큰하게ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100037560.1.12",
          "form": "땀 줄줄 흘리면서 먹어야 되는데ㅋㅋ",
          "original_form": "땀 줄줄 흘리면서 먹어야 되는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:26"
        },
        {
          "id": "MDRW2100037560.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 땀은 1도 흐르지않음",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 땀은 1도 흐르지않음",
          "speaker_id": "2",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100037560.1.14",
          "form": "콧물이 흐를뿐 ㅋㅋㅋㅋ",
          "original_form": "콧물이 흐를뿐 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100037560.1.15",
          "form": "나 그래그래 콧물이난다",
          "original_form": "나 그래그래 콧물이난다",
          "speaker_id": "2",
          "time": "20210901 12:27"
        },
        {
          "id": "MDRW2100037560.1.16",
          "form": "아 웃겨ㅋ",
          "original_form": "아 웃겨ㅋ",
          "speaker_id": "1",
          "time": "20210901 12:32"
        },
        {
          "id": "MDRW2100037560.1.17",
          "form": "어디로든  발산은 해야됨ㅋㅋㅋㅋ",
          "original_form": "어디로든  발산은 해야됨ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 12:33"
        },
        {
          "id": "MDRW2100037560.1.18",
          "form": "ㅋㅋㅋㅋㄱ 왜하필 콧물이고",
          "original_form": "ㅋㅋㅋㅋㄱ 왜하필 콧물이고",
          "speaker_id": "2",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100037560.1.19",
          "form": "없어보이게\r\nㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "없어보이게\r\nㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100037560.1.20",
          "form": "맨날 휴지로 남 이마 닦을때",
          "original_form": "맨날 휴지로 남 이마 닦을때",
          "speaker_id": "2",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100037560.1.21",
          "form": "난 코쑤시고",
          "original_form": "난 코쑤시고",
          "speaker_id": "2",
          "time": "20210901 12:50"
        },
        {
          "id": "MDRW2100037560.1.22",
          "form": "일단 얼굴로는 뭐가 나오면  없어 보이지요ㅋㅋㅋ",
          "original_form": "일단 얼굴로는 뭐가 나오면  없어 보이지요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100037560.1.23",
          "form": "난 지금 인 중에 땀이 나서 마스크가 다 젖었어요",
          "original_form": "난 지금 인 중에 땀이 나서 마스크가 다 젖었어요",
          "speaker_id": "1",
          "time": "20210901 13:08"
        },
        {
          "id": "MDRW2100037560.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 13:12"
        },
        {
          "id": "MDRW2100037560.1.25",
          "form": "마스크가 젖을만큼이면",
          "original_form": "마스크가 젖을만큼이면",
          "speaker_id": "2",
          "time": "20210901 13:12"
        },
        {
          "id": "MDRW2100037560.1.26",
          "form": "뭐 인중이 뚫렸나??",
          "original_form": "뭐 인중이 뚫렸나??",
          "speaker_id": "2",
          "time": "20210901 13:12"
        },
        {
          "id": "MDRW2100037560.1.27",
          "form": "인중은 항상 젖어 있어요ㅋㅋㅋㅋㅋ",
          "original_form": "인중은 항상 젖어 있어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:18"
        },
        {
          "id": "MDRW2100037560.1.28",
          "form": "습해서 수염이 정말 잘 남ㅋㅋㅋㅋㅋ",
          "original_form": "습해서 수염이 정말 잘 남ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:18"
        },
        {
          "id": "MDRW2100037560.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:18"
        },
        {
          "id": "MDRW2100037560.1.30",
          "form": "미치겠다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "미치겠다 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210901 13:18"
        },
        {
          "id": "MDRW2100037560.1.31",
          "form": "잘자라노 ㅋㅋㅋㄱ",
          "original_form": "잘자라노 ㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210901 13:18"
        },
        {
          "id": "MDRW2100037560.1.32",
          "form": "나 지금 에어컨도 파워 냉방했는데ㅋㅋ",
          "original_form": "나 지금 에어컨도 파워 냉방했는데ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:18"
        },
        {
          "id": "MDRW2100037560.1.33",
          "form": "햇빛보게 마스크 좀 벗고있어라",
          "original_form": "햇빛보게 마스크 좀 벗고있어라",
          "speaker_id": "2",
          "time": "20210901 13:18"
        },
        {
          "id": "MDRW2100037560.1.34",
          "form": "인중 파워가 더 쌤ㅋㅋ",
          "original_form": "인중 파워가 더 쌤ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:18"
        },
        {
          "id": "MDRW2100037560.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 아놔 파워냉방",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 아놔 파워냉방",
          "speaker_id": "2",
          "time": "20210901 13:18"
        },
        {
          "id": "MDRW2100037560.1.36",
          "form": "전에도 추웠는데",
          "original_form": "전에도 추웠는데",
          "speaker_id": "2",
          "time": "20210901 13:19"
        },
        {
          "id": "MDRW2100037560.1.37",
          "form": "여름 옷 정리한다고 진짜 더 죽겠어요ㅋㅋㅋ",
          "original_form": "여름 옷 정리한다고 진짜 더 죽겠어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:19"
        },
        {
          "id": "MDRW2100037560.1.38",
          "form": "사장님 전기세 엄청 나가겠네",
          "original_form": "사장님 전기세 엄청 나가겠네",
          "speaker_id": "2",
          "time": "20210901 13:19"
        },
        {
          "id": "MDRW2100037560.1.39",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ 창고 들어가야되나",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㄱ 창고 들어가야되나",
