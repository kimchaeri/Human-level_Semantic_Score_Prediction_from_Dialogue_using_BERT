      "utterance": [
        {
          "id": "MDRW2100016845.1.1",
          "form": "저는 기본버거? 먹었던거같아요 치즈감튀랑",
          "original_form": "저는 기본버거? 먹었던거같아요 치즈감튀랑",
          "speaker_id": "1",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016845.1.2",
          "form": "거긴 치즈감튀도 맛잇어요",
          "original_form": "거긴 치즈감튀도 맛잇어요",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016845.1.3",
          "form": "요즘 매일 자극적인 음식만 먹어서",
          "original_form": "요즘 매일 자극적인 음식만 먹어서",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016845.1.4",
          "form": "좀 건강이 걱정되긴해뇨ㅋㅋㅋㅋㅋ",
          "original_form": "좀 건강이 걱정되긴해뇨ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:49"
        },
        {
          "id": "MDRW2100016845.1.5",
          "form": "그래도 맛있어서 행복하지 않나요? ㅋㅋㅋㅋ",
          "original_form": "그래도 맛있어서 행복하지 않나요? ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:51"
        },
        {
          "id": "MDRW2100016845.1.6",
          "form": "그건 당연하죠! ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그건 당연하죠! ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016845.1.7",
          "form": "전 이런 음식들 없으면 못 살아여.......",
          "original_form": "전 이런 음식들 없으면 못 살아여.......",
          "speaker_id": "2",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016845.1.8",
          "form": "맞아요 먹는 낙으로 살아요 저는",
          "original_form": "맞아요 먹는 낙으로 살아요 저는",
          "speaker_id": "1",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016845.1.9",
          "form": "저도요ㅋㅋ 오늘은 뭐 드실건가요?!?",
          "original_form": "저도요ㅋㅋ 오늘은 뭐 드실건가요?!?",
          "speaker_id": "2",
          "time": "20210826 15:57"
        },
        {
          "id": "MDRW2100016845.1.10",
          "form": "요새 다이어트 중이라서 ㅠㅠ",
          "original_form": "요새 다이어트 중이라서 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016845.1.11",
          "form": "고구마먹을 것 같아요 ㅠㅠ",
          "original_form": "고구마먹을 것 같아요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 15:58"
        },
        {
          "id": "MDRW2100016845.1.12",
          "form": "헉 그렇군요 저도 다이어트해야하는데 ㅜㅜ 오늘은 양배추 코우슬로라도 만들어 먹어야 겠어요",
          "original_form": "헉 그렇군요 저도 다이어트해야하는데 ㅜㅜ 오늘은 양배추 코우슬로라도 만들어 먹어야 겠어요",
          "speaker_id": "2",
          "time": "20210826 16:29"
        },
        {
          "id": "MDRW2100016845.1.13",
          "form": "전 말로만 몇 년 째 다이어터에요 ㅋㅋㅋㅋ",
          "original_form": "전 말로만 몇 년 째 다이어터에요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:30"
        },
        {
          "id": "MDRW2100016845.1.14",
          "form": "헉 저야말로...!!!! ㅎㅎㅎㅎㅎ",
          "original_form": "헉 저야말로...!!!! ㅎㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 16:31"
        },
        {
          "id": "MDRW2100016845.1.15",
          "form": "제가 그건가봐요ㅜ ㅋㅋㅋㅋㅋㅋ",
          "original_form": "제가 그건가봐요ㅜ ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016845.1.16",
          "form": "동지를 만나다니 반갑네요 ㅋㅋㅋ",
          "original_form": "동지를 만나다니 반갑네요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:32"
        },
        {
          "id": "MDRW2100016845.1.17",
          "form": "넴ㅎㅎㅎㅎㅎ 오늘은 고구마만 드시는건가요?!?",
          "original_form": "넴ㅎㅎㅎㅎㅎ 오늘은 고구마만 드시는건가요?!?",
