          "speaker_id": "2",
          "time": "20210821 19:05"
        },
        {
          "id": "MDRW2100004884.1.11",
          "form": "살짝이해는가는데",
          "original_form": "살짝이해는가는데",
          "speaker_id": "2",
          "time": "20210821 19:06"
        },
        {
          "id": "MDRW2100004884.1.12",
