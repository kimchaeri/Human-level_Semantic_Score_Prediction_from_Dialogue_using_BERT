        },
        {
          "id": "MDRW2100004884.1.2",
          "form": "마우스고장나서 살려했거든",
          "original_form": "마우스고장나서 살려했거든",
          "speaker_id": "1",
          "time": "20210821 19:04"
        },
        {
          "id": "MDRW2100004884.1.3",
          "form": "근데 친구성격이 고민많고그래서",
          "original_form": "근데 친구성격이 고민많고그래서",
          "speaker_id": "1",
          "time": "20210821 19:04"
        },
        {
          "id": "MDRW2100004884.1.4",
          "form": "나도사야하는디",
          "original_form": "나도사야하는디",
