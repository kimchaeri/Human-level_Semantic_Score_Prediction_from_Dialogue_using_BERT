      "utterance": [
        {
          "id": "MDRW2100041268.1.1",
          "form": "제주는 얼만큼 머무르시나요?",
          "original_form": "제주는 얼만큼 머무르시나요?",
          "speaker_id": "1",
          "time": "20210905 21:06"
        },
        {
          "id": "MDRW2100041268.1.2",
          "form": "아 내년 3월까지 예정입니당",
          "original_form": "아 내년 3월까지 예정입니당",
          "speaker_id": "2",
          "time": "20210905 21:08"
        },
        {
          "id": "MDRW2100041268.1.3",
          "form": "와 제주에서 가을-겨울-초봄까지 보내시겠네요",
          "original_form": "와 제주에서 가을-겨울-초봄까지 보내시겠네요",
          "speaker_id": "1",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041268.1.4",
          "form": "좋으시겠다",
          "original_form": "좋으시겠다",
          "speaker_id": "1",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041268.1.5",
          "form": "여름은 사람피해서 다니느라 ㅎㅎ",
          "original_form": "여름은 사람피해서 다니느라 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100041268.1.6",
          "form": "아 이제부터 제주생활을 누리셔요",
          "original_form": "아 이제부터 제주생활을 누리셔요",
          "speaker_id": "1",
          "time": "20210905 23:21"
        },
        {
          "id": "MDRW2100041268.1.7",
          "form": "네 본격적으로요 이제 오름위주로 가다가",
          "original_form": "네 본격적으로요 이제 오름위주로 가다가",
          "speaker_id": "2",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100041268.1.8",
          "form": "한라산이 목표예여",
          "original_form": "한라산이 목표예여",
          "speaker_id": "2",
          "time": "20210905 23:22"
        },
        {
          "id": "MDRW2100041268.1.9",
          "form": "오 오름과 한라산",
          "original_form": "오 오름과 한라산",
          "speaker_id": "1",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100041268.1.10",
          "form": "좋네요~",
          "original_form": "좋네요~",
          "speaker_id": "1",
          "time": "20210905 23:23"
        },
        {
          "id": "MDRW2100041268.1.11",
          "form": "해변보다는 이제 산으로 들로 가려구요",
          "original_form": "해변보다는 이제 산으로 들로 가려구요",
          "speaker_id": "2",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100041268.1.12",
          "form": "님은 여행계획 없으신가요",
          "original_form": "님은 여행계획 없으신가요",
          "speaker_id": "2",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100041268.1.13",
          "form": "요즘 날이 좋아서",
          "original_form": "요즘 날이 좋아서",
          "speaker_id": "1",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100041268.1.14",
          "form": "나들이 가고싶은데",
          "original_form": "나들이 가고싶은데",
          "speaker_id": "1",
          "time": "20210905 23:24"
        },
        {
          "id": "MDRW2100041268.1.15",
          "form": "수도권은 코로나가 너무 심각해서 망설여져요ㅜ",
          "original_form": "수도권은 코로나가 너무 심각해서 망설여져요ㅜ",
          "speaker_id": "1",
          "time": "20210905 23:25"
        },
        {
          "id": "MDRW2100041268.1.16",
          "form": "이제 백신 2차접종까지 해서 코로나 좀 잠잠해지면 가까운 곳이라도 잠깐씩 다녀올까 싶기는한데",
          "original_form": "이제 백신 2차접종까지 해서 코로나 좀 잠잠해지면 가까운 곳이라도 잠깐씩 다녀올까 싶기는한데",
          "speaker_id": "1",
          "time": "20210905 23:26"
        },
        {
          "id": "MDRW2100041268.1.17",
          "form": "집콕 계속 하셨군뇨",
          "original_form": "집콕 계속 하셨군뇨",
          "speaker_id": "2",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100041268.1.18",
          "form": "전 그래서 여행유튜브 많이 봤어요",
          "original_form": "전 그래서 여행유튜브 많이 봤어요",
          "speaker_id": "2",
          "time": "20210905 23:50"
        },
        {
          "id": "MDRW2100041268.1.19",
          "form": "맞아요 랜선 여행 많이 했어요 저도",
          "original_form": "맞아요 랜선 여행 많이 했어요 저도",
          "speaker_id": "1",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100041268.1.20",
          "form": "전 빠니보틀꺼 많이 봤어요",
          "original_form": "전 빠니보틀꺼 많이 봤어요",
          "speaker_id": "2",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100041268.1.21",
          "form": "저도 재작년에 인도에 있었어서",
          "original_form": "저도 재작년에 인도에 있었어서",
          "speaker_id": "2",
          "time": "20210905 23:53"
        },
        {
          "id": "MDRW2100041268.1.22",
          "form": "아 인도에도 계셨었군요",
          "original_form": "아 인도에도 계셨었군요",
          "speaker_id": "1",
          "time": "20210905 23:55"
        }
      ]
