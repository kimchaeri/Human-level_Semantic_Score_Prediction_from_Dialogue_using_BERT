      "utterance": [
        {
          "id": "MDRW2100012027.1.1",
          "form": "저는 점심때 고구마치즈돈까스가 나와서 좋았었습니당ㅋㅋ",
          "original_form": "저는 점심때 고구마치즈돈까스가 나와서 좋았었습니당ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:31"
        },
        {
          "id": "MDRW2100012027.1.2",
          "form": "존맛",
          "original_form": "존맛",
          "speaker_id": "1",
          "time": "20210824 18:32"
        },
        {
          "id": "MDRW2100012027.1.3",
          "form": "고구마치즈돈까스",
          "original_form": "고구마치즈돈까스",
          "speaker_id": "2",
          "time": "20210824 18:33"
        },
        {
          "id": "MDRW2100012027.1.4",
          "form": "최고존엄이네요 맛있는거 세 가지를 모아 튀긴",
          "original_form": "최고존엄이네요 맛있는거 세 가지를 모아 튀긴",
          "speaker_id": "2",
          "time": "20210824 18:33"
        },
        {
          "id": "MDRW2100012027.1.5",
          "form": "지존의 음식",
          "original_form": "지존의 음식",
          "speaker_id": "2",
          "time": "20210824 18:33"
        },
        {
          "id": "MDRW2100012027.1.6",
          "form": "네 ㅋㅋ거기다 제앞에서 똑 끊겨서",
          "original_form": "네 ㅋㅋ거기다 제앞에서 똑 끊겨서",
          "speaker_id": "1",
          "time": "20210824 18:34"
        },
        {
          "id": "MDRW2100012027.1.7",
          "form": "금방 튀긴거 먹었답니다",
          "original_form": "금방 튀긴거 먹었답니다",
          "speaker_id": "1",
          "time": "20210824 18:34"
        },
        {
          "id": "MDRW2100012027.1.8",
          "form": "핳핫",
          "original_form": "핳핫",
          "speaker_id": "1",
          "time": "20210824 18:34"
        },
        {
          "id": "MDRW2100012027.1.9",
          "form": "삼치구우셧나요",
          "original_form": "삼치구우셧나요",
          "speaker_id": "1",
          "time": "20210824 18:34"
        },
        {
          "id": "MDRW2100012027.1.10",
          "form": "와 갓튀긴 돈까스",
          "original_form": "와 갓튀긴 돈까스",
          "speaker_id": "2",
          "time": "20210824 18:34"
        },
        {
          "id": "MDRW2100012027.1.11",
          "form": "최.고.",
          "original_form": "최.고.",
          "speaker_id": "2",
          "time": "20210824 18:34"
        },
        {
          "id": "MDRW2100012027.1.12",
          "form": "네 그릴에 구우니 맛이 좋더라구요 살점이 두툼",
          "original_form": "네 그릴에 구우니 맛이 좋더라구요 살점이 두툼",
          "speaker_id": "2",
          "time": "20210824 18:35"
        },
        {
          "id": "MDRW2100012027.1.13",
          "form": "조금 기다렸지만 기다린보람이 있었쥬",
          "original_form": "조금 기다렸지만 기다린보람이 있었쥬",
          "speaker_id": "1",
          "time": "20210824 18:35"
        },
        {
          "id": "MDRW2100012027.1.14",
          "form": "제철삼치로 맛과 건강 챙기셨네요",
          "original_form": "제철삼치로 맛과 건강 챙기셨네요",
          "speaker_id": "1",
          "time": "20210824 18:36"
        },
        {
          "id": "MDRW2100012027.1.15",
          "form": "ㅋㅋ6시 내고향 멘트같아요 \"제철삼치로 맛과 건강 모두 챙기세요~\"",
          "original_form": "ㅋㅋ6시 내고향 멘트같아요 \"제철삼치로 맛과 건강 모두 챙기세요~\"",
          "speaker_id": "2",
          "time": "20210824 18:37"
        },
        {
          "id": "MDRW2100012027.1.16",
          "form": "그런데 정말 그랬습니다 배부르고 좋네요",
          "original_form": "그런데 정말 그랬습니다 배부르고 좋네요",
          "speaker_id": "2",
          "time": "20210824 18:37"
        },
        {
          "id": "MDRW2100012027.1.17",
          "form": "저녁은 뭐드셨어요?",
          "original_form": "저녁은 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210824 18:37"
        },
        {
          "id": "MDRW2100012027.1.18",
          "form": "ㅋㅋ이제 퇴근해가주구ㅠ",
          "original_form": "ㅋㅋ이제 퇴근해가주구ㅠ",
          "speaker_id": "1",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012027.1.19",
          "form": "셔틀에서는 동물의왕국 담에 6시 내고향 하거든요",
          "original_form": "셔틀에서는 동물의왕국 담에 6시 내고향 하거든요",
          "speaker_id": "1",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012027.1.20",
          "form": "그거 보면서 와서 말투가 이런가봐요",
          "original_form": "그거 보면서 와서 말투가 이런가봐요",
          "speaker_id": "1",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012027.1.21",
          "form": "얼른가서 먹어야죠ㅋㅋ",
          "original_form": "얼른가서 먹어야죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 18:39"
        },
        {
          "id": "MDRW2100012027.1.22",
          "form": "아하하 셔틀에서 틀어주는 프로그램들이 구수하네요",
          "original_form": "아하하 셔틀에서 틀어주는 프로그램들이 구수하네요",
          "speaker_id": "2",
          "time": "20210824 18:40"
        },
        {
          "id": "MDRW2100012027.1.23",
          "form": "오늘하루도 고생하셨어요 그래두 돈까스 맛있었겠어요",
          "original_form": "오늘하루도 고생하셨어요 그래두 돈까스 맛있었겠어요",
          "speaker_id": "2",
          "time": "20210824 18:41"
        },
        {
          "id": "MDRW2100012027.1.24",
          "form": "셔틀 기사님 공영방송 케백수만 고집하시는분..",
          "original_form": "셔틀 기사님 공영방송 케백수만 고집하시는분..",
          "speaker_id": "1",
          "time": "20210824 18:42"
        },
        {
          "id": "MDRW2100012027.1.25",
          "form": "ㅋㅋㅋ6시내고향에서도 지역마다 찾아가서 음식먹고 그런부분 재미있더라고요",
          "original_form": "ㅋㅋㅋ6시내고향에서도 지역마다 찾아가서 음식먹고 그런부분 재미있더라고요",
          "speaker_id": "2",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012027.1.26",
          "form": "토속적이고 작위적인 식사",
          "original_form": "토속적이고 작위적인 식사",
          "speaker_id": "2",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012027.1.27",
          "form": "네ㅋㅋ여섯시 내고향에서 맛있는거 많이나와서ㅋㅋ잘보고있습니다",
          "original_form": "네ㅋㅋ여섯시 내고향에서 맛있는거 많이나와서ㅋㅋ잘보고있습니다",
          "speaker_id": "1",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012027.1.28",
          "form": "은근 그런 프로그램 보면 먹고싶어져요",
          "original_form": "은근 그런 프로그램 보면 먹고싶어져요",
          "speaker_id": "2",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012027.1.29",
          "form": "분명 차려놓으면 안먹을 것 같은 메뉴도 맛있어뵈더라고요 ㅋㅋ",
          "original_form": "분명 차려놓으면 안먹을 것 같은 메뉴도 맛있어뵈더라고요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012027.1.30",
          "form": "ㅋㅋ 보는사람도 하는사람도 어색한데 음식은 어색 그런거 모르니깐~",
          "original_form": "ㅋㅋ 보는사람도 하는사람도 어색한데 음식은 어색 그런거 모르니깐~",
          "speaker_id": "1",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012027.1.31",
          "form": "ㅋㅋ 맞아요 시장에서는 눈길도 안주던거",
          "original_form": "ㅋㅋ 맞아요 시장에서는 눈길도 안주던거",
          "speaker_id": "1",
          "time": "20210824 18:44"
        },
        {
          "id": "MDRW2100012027.1.32",
          "form": "티비서 보면 넘 맛있겠고...",
          "original_form": "티비서 보면 넘 맛있겠고...",
          "speaker_id": "1",
          "time": "20210824 18:44"
        },
        {
          "id": "MDRW2100012027.1.33",
          "form": "아 ㅋㅋㅋㅋ 어색함을 모르는 음식 ㅋㅋㅋ 근데 가끔 아사이베리 밥 이런거 나오면",
          "original_form": "아 ㅋㅋㅋㅋ 어색함을 모르는 음식 ㅋㅋㅋ 근데 가끔 아사이베리 밥 이런거 나오면",
          "speaker_id": "2",
          "time": "20210824 18:45"
        },
        {
          "id": "MDRW2100012027.1.34",
          "form": "어색해했으면 좋겠어요",
          "original_form": "어색해했으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210824 18:45"
        },
        {
          "id": "MDRW2100012027.1.35",
          "form": "우리 거리를 두자",
          "original_form": "우리 거리를 두자",
          "speaker_id": "2",
          "time": "20210824 18:45"
        },
        {
          "id": "MDRW2100012027.1.36",
          "form": "ㅋㅋㅋㅋ전번에 메론김치나오던데요 조금 어색..",
          "original_form": "ㅋㅋㅋㅋ전번에 메론김치나오던데요 조금 어색..",
          "speaker_id": "1",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100012027.1.37",
          "form": "비싼 메론으로...김치를..?",
          "original_form": "비싼 메론으로...김치를..?",
          "speaker_id": "1",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100012027.1.38",
          "form": "아놔",
          "original_form": "아놔",
          "speaker_id": "2",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100012027.1.39",
          "form": "저번에 인터넷에서 한국인이 김치로 만들지 않은 식물은 없다 보긴 했는데",
          "original_form": "저번에 인터넷에서 한국인이 김치로 만들지 않은 식물은 없다 보긴 했는데",
          "speaker_id": "2",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100012027.1.40",
          "form": "메론.. 김치...",
          "original_form": "메론.. 김치...",
          "speaker_id": "2",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100012027.1.41",
          "form": "자기전에 생각날 거 같아요",
          "original_form": "자기전에 생각날 거 같아요",
          "speaker_id": "2",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100012027.1.42",
          "form": "메론김치...",
          "original_form": "메론김치...",
          "speaker_id": "2",
          "time": "20210824 18:47"
        }
      ]
