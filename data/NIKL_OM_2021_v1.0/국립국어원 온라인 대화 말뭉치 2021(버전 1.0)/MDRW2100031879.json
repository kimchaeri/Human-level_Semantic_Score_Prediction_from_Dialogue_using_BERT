      "utterance": [
        {
          "id": "MDRW2100031879.1.1",
          "form": "결혼할 계획이 없으시져?",
          "original_form": "결혼할 계획이 없으시져?",
          "speaker_id": "2",
          "time": "20210905 07:11"
        },
        {
          "id": "MDRW2100031879.1.2",
          "form": "네 지금 큰 마음이 안 생기는데요",
          "original_form": "네 지금 큰 마음이 안 생기는데요",
          "speaker_id": "1",
          "time": "20210905 07:11"
        },
        {
          "id": "MDRW2100031879.1.3",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210905 07:11"
        },
        {
          "id": "MDRW2100031879.1.4",
          "form": "안하시구",
          "original_form": "안하시구",
          "speaker_id": "2",
          "time": "20210905 07:11"
        },
        {
          "id": "MDRW2100031879.1.5",
          "form": "연애만 하시면되유",
          "original_form": "연애만 하시면되유",
          "speaker_id": "2",
          "time": "20210905 07:11"
        },
        {
          "id": "MDRW2100031879.1.6",
          "form": "연애도 귀찮아요",
          "original_form": "연애도 귀찮아요",
          "speaker_id": "1",
          "time": "20210905 07:12"
        },
        {
          "id": "MDRW2100031879.1.7",
          "form": "요즘 그런 커플많아요",
          "original_form": "요즘 그런 커플많아요",
          "speaker_id": "2",
          "time": "20210905 07:12"
        },
        {
          "id": "MDRW2100031879.1.8",
          "form": "그냥 저는 남자들이 예전부터 마음에 별로 안 들었어요",
          "original_form": "그냥 저는 남자들이 예전부터 마음에 별로 안 들었어요",
          "speaker_id": "1",
          "time": "20210905 07:12"
        },
        {
          "id": "MDRW2100031879.1.9",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210905 07:12"
        },
        {
          "id": "MDRW2100031879.1.10",
          "form": "궁금합니다",
          "original_form": "궁금합니다",
          "speaker_id": "2",
          "time": "20210905 07:12"
        },
        {
          "id": "MDRW2100031879.1.11",
          "form": "아버지 영향일까요?",
          "original_form": "아버지 영향일까요?",
          "speaker_id": "2",
          "time": "20210905 07:12"
        },
        {
          "id": "MDRW2100031879.1.12",
          "form": "그냥 계속 멘탈이 약한데 남자인 척 하면서",
          "original_form": "그냥 계속 멘탈이 약한데 남자인 척 하면서",
          "speaker_id": "1",
          "time": "20210905 07:12"
        },
        {
          "id": "MDRW2100031879.1.13",
          "form": "자기보다 잘난 안 잘랐는지 구별 없이 계속 나대고",
          "original_form": "자기보다 잘난 안 잘랐는지 구별 없이 계속 나대고",
          "speaker_id": "1",
          "time": "20210905 07:12"
        },
        {
          "id": "MDRW2100031879.1.14",
          "form": "쌘척하는군요",
          "original_form": "쌘척하는군요",
          "speaker_id": "2",
          "time": "20210905 07:12"
        },
        {
          "id": "MDRW2100031879.1.15",
          "form": "그런 걸 보기 좀 역겨워 하는 거 같아요",
          "original_form": "그런 걸 보기 좀 역겨워 하는 거 같아요",
          "speaker_id": "1",
          "time": "20210905 07:13"
        },
        {
          "id": "MDRW2100031879.1.16",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "2",
          "time": "20210905 07:13"
        },
        {
          "id": "MDRW2100031879.1.17",
          "form": "그런식으로 나대면",
          "original_form": "그런식으로 나대면",
          "speaker_id": "2",
          "time": "20210905 07:13"
        },
        {
          "id": "MDRW2100031879.1.18",
          "form": "남자인 저도",
          "original_form": "남자인 저도",
          "speaker_id": "2",
          "time": "20210905 07:13"
        },
        {
          "id": "MDRW2100031879.1.19",
          "form": "별로네요",
          "original_form": "별로네요",
          "speaker_id": "2",
          "time": "20210905 07:13"
        },
        {
          "id": "MDRW2100031879.1.20",
          "form": "그냥 여자나 남자나 인간이라는 점에선 똑같은 거 같은데",
          "original_form": "그냥 여자나 남자나 인간이라는 점에선 똑같은 거 같은데",
          "speaker_id": "1",
          "time": "20210905 07:14"
        },
        {
          "id": "MDRW2100031879.1.21",
          "form": "근데 내가 편하려면 남자에게 의존 하고 그래야 내 몸이 편해지는 거 같더라고요",
          "original_form": "근데 내가 편하려면 남자에게 의존 하고 그래야 내 몸이 편해지는 거 같더라고요",
          "speaker_id": "1",
          "time": "20210905 07:14"
        },
        {
          "id": "MDRW2100031879.1.22",
          "form": "똑같습니다",
          "original_form": "똑같습니다",
          "speaker_id": "2",
          "time": "20210905 07:14"
        },
        {
          "id": "MDRW2100031879.1.23",
          "form": "근데 그거 너무 똑같다고 하면 저를 고생시키는 이상한 남자들이랑 엮이는 거 같아서요",
          "original_form": "근데 그거 너무 똑같다고 하면 저를 고생시키는 이상한 남자들이랑 엮이는 거 같아서요",
          "speaker_id": "1",
          "time": "20210905 07:14"
        },
        {
          "id": "MDRW2100031879.1.24",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210905 07:14"
        },
        {
          "id": "MDRW2100031879.1.25",
          "form": "적당히 자기가 남자인척하는 그걸 이용해야 내 몸이 편해진다는 걸 알았어요",
          "original_form": "적당히 자기가 남자인척하는 그걸 이용해야 내 몸이 편해진다는 걸 알았어요",
          "speaker_id": "1",
          "time": "20210905 07:15"
        },
        {
          "id": "MDRW2100031879.1.26",
          "form": "오",
          "original_form": "오",
          "speaker_id": "2",
          "time": "20210905 07:15"
        },
        {
          "id": "MDRW2100031879.1.27",
          "form": "남자는 단순하니까요",
          "original_form": "남자는 단순하니까요",
          "speaker_id": "2",
          "time": "20210905 07:15"
        },
        {
          "id": "MDRW2100031879.1.28",
          "form": "물론 저도 단순세포임 ㅋㅋ",
          "original_form": "물론 저도 단순세포임 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 07:15"
        },
        {
          "id": "MDRW2100031879.1.29",
          "form": "근데 솔직히 그렇게 하면서까지",
          "original_form": "근데 솔직히 그렇게 하면서까지",
          "speaker_id": "1",
          "time": "20210905 07:15"
        },
        {
          "id": "MDRW2100031879.1.30",
          "form": "그 남자랑 사귀면서 시간 보내는게 시간 낭비 같아요",
          "original_form": "그 남자랑 사귀면서 시간 보내는게 시간 낭비 같아요",
          "speaker_id": "1",
          "time": "20210905 07:15"
        },
        {
          "id": "MDRW2100031879.1.31",
          "form": "그런남자랑은 인생이 낭비입니다",
          "original_form": "그런남자랑은 인생이 낭비입니다",
