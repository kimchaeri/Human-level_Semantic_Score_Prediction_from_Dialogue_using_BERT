      "utterance": [
        {
          "id": "MDRW2100031964.1.1",
          "form": "ㅋㅋㅋㅋ마자요",
          "original_form": "ㅋㅋㅋㅋ마자요",
          "speaker_id": "2",
          "time": "20210905 09:49"
        },
        {
          "id": "MDRW2100031964.1.2",
          "form": "제여친도 그러고",
          "original_form": "제여친도 그러고",
          "speaker_id": "1",
          "time": "20210905 09:49"
        },
        {
          "id": "MDRW2100031964.1.3",
          "form": "절대 교사 안만난다고",
          "original_form": "절대 교사 안만난다고",
          "speaker_id": "1",
          "time": "20210905 09:50"
        },
        {
          "id": "MDRW2100031964.1.4",
          "form": "남교사들은 여교사를 원한다던데",
          "original_form": "남교사들은 여교사를 원한다던데",
          "speaker_id": "1",
          "time": "20210905 09:50"
        },
        {
          "id": "MDRW2100031964.1.5",
          "form": "여교사는 안원한다는...",
          "original_form": "여교사는 안원한다는...",
          "speaker_id": "1",
          "time": "20210905 09:50"
        },
        {
          "id": "MDRW2100031964.1.6",
          "form": "그렇져 남교사들은 교사부부가 좀 괜찮은거같고 ㅋㅋㅋ",
          "original_form": "그렇져 남교사들은 교사부부가 좀 괜찮은거같고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100031964.1.7",
          "form": "학교에서 안그래도 저랑 동갑인 체육쌤있었는데",
          "original_form": "학교에서 안그래도 저랑 동갑인 체육쌤있었는데",
          "speaker_id": "2",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100031964.1.8",
          "form": "엮어주려는 사람이 있어서 절대 반대했어여ㅋㅋㅋㅋ",
          "original_form": "엮어주려는 사람이 있어서 절대 반대했어여ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100031964.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100031964.1.10",
          "form": "체육쌤 정말 훈남이긴 하나",
          "original_form": "체육쌤 정말 훈남이긴 하나",
          "speaker_id": "2",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100031964.1.11",
          "form": "거긴 그렇더라구요?",
          "original_form": "거긴 그렇더라구요?",
          "speaker_id": "1",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100031964.1.12",
          "form": "학교안 사람은 건드리진 않겠다",
          "original_form": "학교안 사람은 건드리진 않겠다",
          "speaker_id": "2",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100031964.1.13",
          "form": "제 여친도 저 만나기전에",
          "original_form": "제 여친도 저 만나기전에",
          "speaker_id": "1",
          "time": "20210905 09:51"
        },
        {
          "id": "MDRW2100031964.1.14",
          "form": "네 엮어주려는 심심한 선생님들 많아요ㅋㅋㅋㅋ",
          "original_form": "네 엮어주려는 심심한 선생님들 많아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100031964.1.15",
          "form": "교감이 불러서 남친없으면 자기 조카 만나보라고",
          "original_form": "교감이 불러서 남친없으면 자기 조카 만나보라고",
          "speaker_id": "1",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100031964.1.16",
          "form": "앜ㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100031964.1.17",
          "form": "새로운학교가면",
          "original_form": "새로운학교가면",
          "speaker_id": "1",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100031964.1.18",
          "form": "나이랑 남친여부부터 묻고",
          "original_form": "나이랑 남친여부부터 묻고",
          "speaker_id": "1",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100031964.1.19",
          "form": "막 엮을라고 한대요",
          "original_form": "막 엮을라고 한대요",
          "speaker_id": "1",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100031964.1.20",
          "form": "저희 교장쌤도 맨날 누구 소개는 못 시켜주면서 출장만 많이 보내서 미안하다고",
          "original_form": "저희 교장쌤도 맨날 누구 소개는 못 시켜주면서 출장만 많이 보내서 미안하다고",
          "speaker_id": "2",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100031964.1.21",
          "form": "저 볼때마다 안타까워했어요",
          "original_form": "저 볼때마다 안타까워했어요",
          "speaker_id": "2",
          "time": "20210905 09:53"
        },
        {
          "id": "MDRW2100031964.1.22",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:53"
        },
        {
          "id": "MDRW2100031964.1.23",
          "form": "사대생 절대 만날 생각 없다해놓고",
          "original_form": "사대생 절대 만날 생각 없다해놓고",
