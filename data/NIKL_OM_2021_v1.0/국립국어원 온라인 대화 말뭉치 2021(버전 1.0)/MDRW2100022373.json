      "utterance": [
        {
          "id": "MDRW2100022373.1.1",
          "form": "안녕하세요 반갑습니다",
          "original_form": "안녕하세요 반갑습니다",
          "speaker_id": "1",
          "time": "20210829 13:13"
        },
        {
          "id": "MDRW2100022373.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210829 13:17"
        },
        {
          "id": "MDRW2100022373.1.3",
          "form": "점심은 먹었나요?",
          "original_form": "점심은 먹었나요?",
          "speaker_id": "1",
          "time": "20210829 13:17"
        },
        {
          "id": "MDRW2100022373.1.4",
          "form": "라면에김밥먹었어요",
          "original_form": "라면에김밥먹었어요",
          "speaker_id": "2",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022373.1.5",
          "form": "제가 취준생이라 요즘 생활비 아껴야해서...",
          "original_form": "제가 취준생이라 요즘 생활비 아껴야해서...",
          "speaker_id": "2",
          "time": "20210829 13:43"
        },
        {
          "id": "MDRW2100022373.1.6",
          "form": "아이고 그렇군요",
          "original_form": "아이고 그렇군요",
          "speaker_id": "1",
          "time": "20210829 13:45"
        },
        {
          "id": "MDRW2100022373.1.7",
          "form": "좋은 곳에 꼭 합격하길 빌게요",
          "original_form": "좋은 곳에 꼭 합격하길 빌게요",
          "speaker_id": "1",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100022373.1.8",
          "form": "그래도 너무 라면만 먹으면 몸에 안좋을텐데요",
          "original_form": "그래도 너무 라면만 먹으면 몸에 안좋을텐데요",
          "speaker_id": "1",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100022373.1.9",
          "form": "감사합니다",
          "original_form": "감사합니다",
          "speaker_id": "2",
          "time": "20210829 13:46"
        },
        {
          "id": "MDRW2100022373.1.10",
          "form": "그래도 지금 버티면 좋은 시절이오겠죠",
          "original_form": "그래도 지금 버티면 좋은 시절이오겠죠",
          "speaker_id": "2",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022373.1.11",
          "form": "식사는 뭐드셨어요?",
          "original_form": "식사는 뭐드셨어요?",
          "speaker_id": "2",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022373.1.12",
          "form": "전 슬슬 커피사올려구요",
          "original_form": "전 슬슬 커피사올려구요",
          "speaker_id": "2",
          "time": "20210829 13:47"
        },
        {
          "id": "MDRW2100022373.1.13",
          "form": "저는 가자미구이에 밥 먹었어요",
          "original_form": "저는 가자미구이에 밥 먹었어요",
          "speaker_id": "1",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100022373.1.14",
          "form": "생선요리를 좋아한답니다",
          "original_form": "생선요리를 좋아한답니다",
          "speaker_id": "1",
          "time": "20210829 13:51"
        },
        {
          "id": "MDRW2100022373.1.15",
          "form": "아.. 저는 개인적으로 원래 생선요리를 별로 안좋아 해요",
          "original_form": "아.. 저는 개인적으로 원래 생선요리를 별로 안좋아 해요",
          "speaker_id": "2",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100022373.1.16",
          "form": "군대 때 트라우마가 너무 강해서",
          "original_form": "군대 때 트라우마가 너무 강해서",
          "speaker_id": "2",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100022373.1.17",
          "form": "연어회 참치회 이런거는 가끔 사주면 먹는데 일반 생선은 꺼리게 되더라구요 ㅎㅎ",
          "original_form": "연어회 참치회 이런거는 가끔 사주면 먹는데 일반 생선은 꺼리게 되더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100022373.1.18",
          "form": "아이고 트라우마때문에 그 맛있는 생선을 못먹는다니 안타깝네요",
          "original_form": "아이고 트라우마때문에 그 맛있는 생선을 못먹는다니 안타깝네요",
          "speaker_id": "1",
          "time": "20210829 14:02"
        },
        {
          "id": "MDRW2100022373.1.19",
          "form": "그래도 세상엔 맛있는 음식들이 많죠",
          "original_form": "그래도 세상엔 맛있는 음식들이 많죠",
          "speaker_id": "1",
          "time": "20210829 14:02"
        },
        {
          "id": "MDRW2100022373.1.20",
          "form": "가끔 명절에 할머니가 해주는 갈치구이 이런건 먹어요",
          "original_form": "가끔 명절에 할머니가 해주는 갈치구이 이런건 먹어요",
          "speaker_id": "2",
          "time": "20210829 14:02"
        },
        {
          "id": "MDRW2100022373.1.21",
          "form": "갈치구이 고급요리죠",
          "original_form": "갈치구이 고급요리죠",
          "speaker_id": "1",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100022373.1.22",
          "form": "그외에는 잘 안먹어요 고기가 더 좋아요 저는",
          "original_form": "그외에는 잘 안먹어요 고기가 더 좋아요 저는",
          "speaker_id": "2",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100022373.1.23",
          "form": "아주 안먹는건 아니네요",
          "original_form": "아주 안먹는건 아니네요",
          "speaker_id": "1",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100022373.1.24",
          "form": "커피는 혹시 좋아하시나요?",
          "original_form": "커피는 혹시 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100022373.1.25",
          "form": "저도 고기좋아한답니다",
          "original_form": "저도 고기좋아한답니다",
          "speaker_id": "1",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100022373.1.26",
          "form": "전 아메리카노 사왔어요",
          "original_form": "전 아메리카노 사왔어요",
          "speaker_id": "2",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100022373.1.27",
          "form": "커피는 좋아하는데 잠을 못자서 자제중이에요",
          "original_form": "커피는 좋아하는데 잠을 못자서 자제중이에요",
          "speaker_id": "1",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100022373.1.28",
          "form": "name2님은 자는데 영향없나봐요?",
          "original_form": "&name2&님은 자는데 영향없나봐요?",
          "speaker_id": "1",
          "time": "20210829 14:03"
        },
        {
          "id": "MDRW2100022373.1.29",
          "form": "잠을 못자면 엄청 피곤한데 힘드시겠어요",
          "original_form": "잠을 못자면 엄청 피곤한데 힘드시겠어요",
          "speaker_id": "2",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100022373.1.30",
          "form": "네 저는 거의 하루 두잔 마셔도 잘 자요",
          "original_form": "네 저는 거의 하루 두잔 마셔도 잘 자요",
          "speaker_id": "2",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100022373.1.31",
          "form": "좋은건지나쁜건지..",
          "original_form": "좋은건지나쁜건지..",
          "speaker_id": "2",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100022373.1.32",
          "form": "전 마시려면 아침일찍 마셔야 한답니다",
          "original_form": "전 마시려면 아침일찍 마셔야 한답니다",
          "speaker_id": "1",
          "time": "20210829 14:05"
        },
        {
          "id": "MDRW2100022373.1.33",
          "form": "잠설치면 다음날 너무 힘들어요",
          "original_form": "잠설치면 다음날 너무 힘들어요",
          "speaker_id": "1",
          "time": "20210829 14:05"
        },
        {
          "id": "MDRW2100022373.1.34",
          "form": "사람마다 음식이 몸에 맞고 안맞고 다 다르니 아쉽네요 ㅎㅎ",
          "original_form": "사람마다 음식이 몸에 맞고 안맞고 다 다르니 아쉽네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100022373.1.35",
          "form": "전 가끔 커피 맛있는 곳 찾아가는것도 좋아하거든요",
          "original_form": "전 가끔 커피 맛있는 곳 찾아가는것도 좋아하거든요",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100022373.1.36",
          "form": "맛집투어같은거도 좋아하시나요? 아님 주로 음식 해드시나요?",
          "original_form": "맛집투어같은거도 좋아하시나요? 아님 주로 음식 해드시나요?",
          "speaker_id": "2",
          "time": "20210829 14:08"
        },
        {
          "id": "MDRW2100022373.1.37",
          "form": "맛집을 일부러 찾아가지는 않아요",
          "original_form": "맛집을 일부러 찾아가지는 않아요",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100022373.1.38",
          "form": "가서 줄서는 것도 별루고",
          "original_form": "가서 줄서는 것도 별루고",
          "speaker_id": "1",
          "time": "20210829 14:11"
        },
        {
          "id": "MDRW2100022373.1.39",
          "form": "name2님은 좋아하나요?",
          "original_form": "&name2&님은 좋아하나요?",
