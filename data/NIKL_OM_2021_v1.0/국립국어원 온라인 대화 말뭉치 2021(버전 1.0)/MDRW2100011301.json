        },
        {
          "id": "MDRW2100011301.1.2",
          "form": "안녕하세요 누누님 계신곳 태풍 오나요",
          "original_form": "안녕하세요 누누님 계신곳 태풍 오나요",
          "speaker_id": "2",
          "time": "20210824 10:52"
        },
        {
          "id": "MDRW2100011301.1.3",
          "form": "태풍이요???",
          "original_form": "태풍이요???",
          "speaker_id": "1",
          "time": "20210824 11:00"
        },
        {
          "id": "MDRW2100011301.1.4",
          "form": "네네 오마이스 소멸했나요?",
          "original_form": "네네 오마이스 소멸했나요?",
          "speaker_id": "2",
          "time": "20210824 11:00"
        },
        {
          "id": "MDRW2100011301.1.5",
          "form": "태풍얘긴못들었는데",
          "original_form": "태풍얘긴못들었는데",
          "speaker_id": "1",
          "time": "20210824 11:07"
        },
        {
          "id": "MDRW2100011301.1.6",
          "form": "왔었나보네요?ㅠ",
          "original_form": "왔었나보네요?ㅠ",
          "speaker_id": "1",
          "time": "20210824 11:07"
        },
        {
          "id": "MDRW2100011301.1.7",
          "form": "네네 근데 서울은 비만조금오고 괜찮네요",
          "original_form": "네네 근데 서울은 비만조금오고 괜찮네요",
          "speaker_id": "2",
          "time": "20210824 11:09"
        },
        {
          "id": "MDRW2100011301.1.8",
          "form": "저도 서울이요ㅋㅋ",
          "original_form": "저도 서울이요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:16"
        },
        {
          "id": "MDRW2100011301.1.9",
          "form": "날씨가 부쩍 추워졌네요",
          "original_form": "날씨가 부쩍 추워졌네요",
          "speaker_id": "1",
          "time": "20210824 11:16"
        },
        {
          "id": "MDRW2100011301.1.10",
          "form": "이제 여름 드디어 끝인듯 하네요ㅎㅎ",
          "original_form": "이제 여름 드디어 끝인듯 하네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:17"
        },
        {
          "id": "MDRW2100011301.1.11",
          "form": "겨울vs여름?",
          "original_form": "겨울vs여름?",
          "speaker_id": "1",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100011301.1.12",
          "form": "겨울이 나은듯해요 껴입으면 되니까ㅎㅎ",
          "original_form": "겨울이 나은듯해요 껴입으면 되니까ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 11:22"
        },
        {
          "id": "MDRW2100011301.1.13",
          "form": "저도요ㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 11:26"
        },
        {
          "id": "MDRW2100011301.1.14",
          "form": "여름은 너무덥죠ㅠ",
          "original_form": "여름은 너무덥죠ㅠ",
          "speaker_id": "1",
          "time": "20210824 11:26"
        },
        {
          "id": "MDRW2100011301.1.15",
          "form": "봄 가을이 젤 좋아요~",
          "original_form": "봄 가을이 젤 좋아요~",
          "speaker_id": "2",
          "time": "20210824 11:28"
        },
        {
          "id": "MDRW2100011301.1.16",
          "form": "맞아요 근데",
          "original_form": "맞아요 근데",
          "speaker_id": "1",
          "time": "20210824 11:35"
        },
        {
