      "utterance": [
        {
          "id": "MDRW2100033572.1.1",
          "form": "반려동물 키우시나요?",
          "original_form": "반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210824 14:54"
        },
        {
          "id": "MDRW2100033572.1.2",
          "form": "아니요 부모님이 키우고계세요",
          "original_form": "아니요 부모님이 키우고계세요",
          "speaker_id": "2",
          "time": "20210824 14:54"
        },
        {
          "id": "MDRW2100033572.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100033572.1.4",
          "form": "어렸을대 닭 키운적있어요",
          "original_form": "어렸을대 닭 키운적있어요",
          "speaker_id": "1",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100033572.1.5",
          "form": "오 집에서요?ㅎ",
          "original_form": "오 집에서요?ㅎ",
          "speaker_id": "2",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100033572.1.6",
          "form": "병아리사다 키우신건가요?",
          "original_form": "병아리사다 키우신건가요?",
          "speaker_id": "2",
          "time": "20210824 14:55"
        },
        {
          "id": "MDRW2100033572.1.7",
          "form": "네ㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 14:56"
        },
        {
          "id": "MDRW2100033572.1.8",
          "form": "초등학교때 오백원주고 키웠습니다",
          "original_form": "초등학교때 오백원주고 키웠습니다",
          "speaker_id": "1",
          "time": "20210824 14:56"
        },
        {
          "id": "MDRW2100033572.1.9",
          "form": "애지중지..",
          "original_form": "애지중지..",
          "speaker_id": "1",
          "time": "20210824 14:56"
        },
        {
          "id": "MDRW2100033572.1.10",
          "form": "닭으로 키우기 쉽지않은데 대단하시네요",
          "original_form": "닭으로 키우기 쉽지않은데 대단하시네요",
          "speaker_id": "2",
          "time": "20210824 14:56"
        },
        {
          "id": "MDRW2100033572.1.11",
          "form": "어떤 동물키우세요?",
          "original_form": "어떤 동물키우세요?",
          "speaker_id": "1",
          "time": "20210824 14:57"
        },
        {
          "id": "MDRW2100033572.1.12",
          "form": "믹스견 키우는데 노령견이라 살날이 얼마안남았어요",
          "original_form": "믹스견 키우는데 노령견이라 살날이 얼마안남았어요",
          "speaker_id": "2",
          "time": "20210824 14:58"
        },
        {
          "id": "MDRW2100033572.1.13",
          "form": "아이고.. 오래살았으면 좋겠네요",
          "original_form": "아이고.. 오래살았으면 좋겠네요",
          "speaker_id": "1",
          "time": "20210824 14:59"
        },
        {
          "id": "MDRW2100033572.1.14",
          "form": "이미 16년 17년된거같아요",
          "original_form": "이미 16년 17년된거같아요",
          "speaker_id": "2",
          "time": "20210824 14:59"
        },
        {
          "id": "MDRW2100033572.1.15",
          "form": "대박 장수견이네요",
          "original_form": "대박 장수견이네요",
          "speaker_id": "1",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100033572.1.16",
          "form": "요즘 다 그렇게살더라구요",
          "original_form": "요즘 다 그렇게살더라구요",
          "speaker_id": "2",
          "time": "20210824 15:01"
        },
        {
          "id": "MDRW2100033572.1.17",
          "form": "시츄랑 말티즈도 있었는데 그정도살고 하늘나라로ㅠ",
          "original_form": "시츄랑 말티즈도 있었는데 그정도살고 하늘나라로ㅠ",
          "speaker_id": "2",
          "time": "20210824 15:02"
        },
        {
          "id": "MDRW2100033572.1.18",
          "form": "애견가이시군요",
          "original_form": "애견가이시군요",
          "speaker_id": "1",
          "time": "20210824 15:02"
        },
        {
          "id": "MDRW2100033572.1.19",
          "form": "전보는것만좋아해요",
          "original_form": "전보는것만좋아해요",
          "speaker_id": "2",
          "time": "20210824 15:03"
        }
      ]
