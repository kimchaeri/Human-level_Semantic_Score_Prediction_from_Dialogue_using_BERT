      "utterance": [
        {
          "id": "MDRW2100033185.1.1",
          "form": "연애 안 하는데 큰일이네요",
          "original_form": "연애 안 하는데 큰일이네요",
          "speaker_id": "2",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033185.1.2",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "1",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033185.1.3",
          "form": "하고싶은 욕심은 많은데!",
          "original_form": "하고싶은 욕심은 많은데!",
          "speaker_id": "1",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033185.1.4",
          "form": "재현아 어디서 뭐하니",
          "original_form": "재현아 어디서 뭐하니",
          "speaker_id": "1",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033185.1.5",
          "form": "욕심은 저도 많아요",
          "original_form": "욕심은 저도 많아요",
          "speaker_id": "2",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033185.1.6",
          "form": "아ㅋㅋㅋㅋㅋㅋㅈ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅈ",
          "speaker_id": "2",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033185.1.7",
          "form": "재현아..",
          "original_form": "재현아..",
          "speaker_id": "2",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033185.1.8",
          "form": "name2님한테 고백하지않고",
          "original_form": "&name2&님한테 고백하지않고",
          "speaker_id": "1",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033185.1.9",
          "form": "니가 잘못이 크다",
          "original_form": "니가 잘못이 크다",
          "speaker_id": "2",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033185.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033185.1.11",
          "form": "고백도 안하고",
          "original_form": "고백도 안하고",
          "speaker_id": "1",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033185.1.12",
          "form": "근데 제가 안 받앗을걸요..",
          "original_form": "근데 제가 안 받앗을걸요..",
          "speaker_id": "2",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033185.1.13",
          "form": "엩",
          "original_form": "엩",
          "speaker_id": "1",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033185.1.14",
          "form": "그그럼",
          "original_form": "그그럼",
          "speaker_id": "1",
          "time": "20210906 18:16"
        },
        {
          "id": "MDRW2100033185.1.15",
          "form": "키링연애를...",
          "original_form": "키링연애를...",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100033185.1.16",
          "form": "니가 나랑 무슨 연애를한다고그래!!! 그냥 우리.. 이대로 살자고.. 응?",
          "original_form": "니가 나랑 무슨 연애를한다고그래!!! 그냥 우리.. 이대로 살자고.. 응?",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100033185.1.17",
          "form": "마음에 안들어하겟지만요..",
          "original_form": "마음에 안들어하겟지만요..",
          "speaker_id": "2",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100033185.1.18",
          "form": "저는 와장창광흥창 연애를 하고싶어요",
          "original_form": "저는 와장창광흥창 연애를 하고싶어요",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100033185.1.19",
          "form": "우당탕탕 광흥창 하고",
          "original_form": "우당탕탕 광흥창 하고",
          "speaker_id": "1",
          "time": "20210906 18:17"
        },
        {
          "id": "MDRW2100033185.1.20",
          "form": "맨날 설거지 하다가 빡쳐서 접시 깨먹는....",
          "original_form": "맨날 설거지 하다가 빡쳐서 접시 깨먹는....",
          "speaker_id": "1",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100033185.1.21",
          "form": "유치찬란 연애 한번 해보고싶어요",
          "original_form": "유치찬란 연애 한번 해보고싶어요",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100033185.1.22",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100033185.1.23",
          "form": "재밌겠당",
          "original_form": "재밌겠당",
          "speaker_id": "1",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100033185.1.24",
          "form": "집앞에서 울고불고 싸우고",
          "original_form": "집앞에서 울고불고 싸우고",
          "speaker_id": "2",
          "time": "20210906 18:18"
        },
        {
          "id": "MDRW2100033185.1.25",
          "form": "재밌겠다..",
          "original_form": "재밌겠다..",
          "speaker_id": "2",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100033185.1.26",
          "form": "재밌겠다....",
          "original_form": "재밌겠다....",
          "speaker_id": "1",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100033185.1.27",
          "form": "근데 전 솔직히 왜그러는지 모르겠는데",
          "original_form": "근데 전 솔직히 왜그러는지 모르겠는데",
          "speaker_id": "1",
          "time": "20210906 18:19"
        },
        {
          "id": "MDRW2100033185.1.28",
          "form": "저는....... 재민이랑 연애를.......",
          "original_form": "저는....... 재민이랑 연애를.......",
          "speaker_id": "1",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100033185.1.29",
          "form": "하거싶은걸까?",
          "original_form": "하거싶은걸까?",
          "speaker_id": "1",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100033185.1.30",
          "form": "잘 모르겠어용",
          "original_form": "잘 모르겠어용",
          "speaker_id": "1",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100033185.1.31",
          "form": "으음..",
          "original_form": "으음..",
          "speaker_id": "2",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100033185.1.32",
          "form": "친구를 원하는거 아닐까요",
          "original_form": "친구를 원하는거 아닐까요",
          "speaker_id": "2",
          "time": "20210906 18:20"
        },
        {
          "id": "MDRW2100033185.1.33",
          "form": "그런걸까!",
          "original_form": "그런걸까!",
          "speaker_id": "1",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100033185.1.34",
          "form": "예시에 이상형이 있네요 저는 도경수인데",
          "original_form": "예시에 이상형이 있네요 저는 도경수인데",
          "speaker_id": "2",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100033185.1.35",
          "form": "근데 제가 좋아하는 사람이 생겨서 그럴수도",
          "original_form": "근데 제가 좋아하는 사람이 생겨서 그럴수도",
          "speaker_id": "1",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100033185.1.36",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "2",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100033185.1.37",
          "form": "저는......",
          "original_form": "저는......",
          "speaker_id": "1",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100033185.1.38",
          "form": "제 이상형은...",
          "original_form": "제 이상형은...",
          "speaker_id": "1",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100033185.1.39",
          "form": "좋아하는사람이라니..",
          "original_form": "좋아하는사람이라니..",
          "speaker_id": "2",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100033185.1.40",
          "form": "뭐지",
          "original_form": "뭐지",
          "speaker_id": "1",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100033185.1.41",
          "form": "귀여운 사람이 좋습니다",
          "original_form": "귀여운 사람이 좋습니다",
          "speaker_id": "1",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100033185.1.42",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210906 18:21"
        },
        {
          "id": "MDRW2100033185.1.43",
          "form": "강아지같이 귀여운 사람이 좋아요..",
          "original_form": "강아지같이 귀여운 사람이 좋아요..",
          "speaker_id": "2",
          "time": "20210906 18:22"
        },
