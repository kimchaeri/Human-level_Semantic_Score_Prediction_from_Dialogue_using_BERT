        },
        {
          "id": "MDRW2100040825.1.2",
          "form": "계란에 치즈 넣아?",
          "original_form": "계란에 치즈 넣아?",
          "speaker_id": "2",
          "time": "20210905 09:03"
        },
        {
          "id": "MDRW2100040825.1.3",
          "form": "감자할까싶네?",
          "original_form": "감자할까싶네?",
          "speaker_id": "1",
          "time": "20210905 09:04"
        },
        {
          "id": "MDRW2100040825.1.4",
          "form": "근데 아침에 엄미ㅏ 국 끓잏거같았는디",
          "original_form": "근데 아침에 엄미ㅏ 국 끓잏거같았는디",
          "speaker_id": "1",
          "time": "20210905 09:04"
        },
        {
          "id": "MDRW2100040825.1.5",
          "form": "좋은생각이야",
          "original_form": "좋은생각이야",
          "speaker_id": "2",
          "time": "20210905 09:04"
        },
        {
          "id": "MDRW2100040825.1.6",
          "form": "호박썰어서 할까",
          "original_form": "호박썰어서 할까",
          "speaker_id": "1",
          "time": "20210905 09:04"
        },
        {
          "id": "MDRW2100040825.1.7",
          "form": "지금하지ㅋㅋㅋㅋ",
          "original_form": "지금하지ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 09:04"
        },
        {
          "id": "MDRW2100040825.1.8",
          "form": "그래야겟네",
          "original_form": "그래야겟네",
          "speaker_id": "1",
          "time": "20210905 09:05"
        },
        {
          "id": "MDRW2100040825.1.9",
          "form": "단호박조림있어",
          "original_form": "단호박조림있어",
          "speaker_id": "2",
          "time": "20210905 09:05"
        },
        {
          "id": "MDRW2100040825.1.10",
          "form": "아 그거... 구제불능인디",
          "original_form": "아 그거... 구제불능인디",
          "speaker_id": "1",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100040825.1.11",
          "form": "냉장고?",
          "original_form": "냉장고?",
          "speaker_id": "1",
          "time": "20210905 09:06"
        },
        {
          "id": "MDRW2100040825.1.12",
          "form": "응?",
          "original_form": "응?",
          "speaker_id": "2",
          "time": "20210905 09:07"
        },
        {
          "id": "MDRW2100040825.1.13",
          "form": "어 며칠전에 한거 ㅜ 괜찮던데",
          "original_form": "어 며칠전에 한거 ㅜ 괜찮던데",
          "speaker_id": "2",
          "time": "20210905 09:08"
        },
        {
          "id": "MDRW2100040825.1.14",
          "form": "ㅋㅋ 근데 왜 안먹었어!",
          "original_form": "ㅋㅋ 근데 왜 안먹었어!",
          "speaker_id": "1",
          "time": "20210905 09:45"
        },
        {
          "id": "MDRW2100040825.1.15",
          "form": "있는줄 몰라서? 어제 정리하면서 보고 오늘 꺼낸거잖아",
          "original_form": "있는줄 몰라서? 어제 정리하면서 보고 오늘 꺼낸거잖아",
          "speaker_id": "2",
          "time": "20210905 09:52"
        },
        {
          "id": "MDRW2100040825.1.16",
          "form": "ㅋㅋ냉장고털어먹어야하는디",
