      "utterance": [
        {
          "id": "MDRW2100045854.1.1",
          "form": "여행가고싶은곳 있으세요?",
          "original_form": "여행가고싶은곳 있으세요?",
          "speaker_id": "1",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045854.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045854.1.3",
          "form": "네네 너무많아요",
          "original_form": "네네 너무많아요",
          "speaker_id": "2",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045854.1.4",
          "form": "어디가고싶으세요~?^^",
          "original_form": "어디가고싶으세요~?^^",
          "speaker_id": "1",
          "time": "20210908 08:05"
        },
        {
          "id": "MDRW2100045854.1.5",
          "form": "음 해외는 스위스요!!",
          "original_form": "음 해외는 스위스요!!",
          "speaker_id": "2",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045854.1.6",
          "form": "아하 ㅎㅎ",
          "original_form": "아하 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045854.1.7",
          "form": "너무 가보고 싶은 여행지였는데 코로나때문에 언제 갈 수 있을지 모르겠네요",
          "original_form": "너무 가보고 싶은 여행지였는데 코로나때문에 언제 갈 수 있을지 모르겠네요",
          "speaker_id": "2",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045854.1.8",
          "form": "가고싶은 여행지 있으세요?",
          "original_form": "가고싶은 여행지 있으세요?",
          "speaker_id": "2",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045854.1.9",
          "form": "스위스는 기차만 타도",
          "original_form": "스위스는 기차만 타도",
          "speaker_id": "1",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045854.1.10",
          "form": "풍경이 ....와우 에요",
          "original_form": "풍경이 ....와우 에요",
          "speaker_id": "1",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045854.1.11",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045854.1.12",
          "form": "저는 발리요",
          "original_form": "저는 발리요",
          "speaker_id": "1",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045854.1.13",
          "form": "헉 가보셨군요..",
          "original_form": "헉 가보셨군요..",
          "speaker_id": "2",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045854.1.14",
          "form": "너무너무 부러워요",
          "original_form": "너무너무 부러워요",
          "speaker_id": "2",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045854.1.15",
          "form": "발리도 되게 좋다더라구요!",
          "original_form": "발리도 되게 좋다더라구요!",
          "speaker_id": "2",
          "time": "20210908 08:06"
        },
        {
          "id": "MDRW2100045854.1.16",
          "form": "ㅎㅎ바다가 그리워요",
          "original_form": "ㅎㅎ바다가 그리워요",
          "speaker_id": "1",
          "time": "20210908 08:08"
        },
        {
          "id": "MDRW2100045854.1.17",
          "form": "스위스 얘기 조금만 들려주실 수 있을까요?",
          "original_form": "스위스 얘기 조금만 들려주실 수 있을까요?",
          "speaker_id": "2",
          "time": "20210908 08:09"
        },
        {
          "id": "MDRW2100045854.1.18",
          "form": "정말 너무 가보고싶은 여행지였거든요",
          "original_form": "정말 너무 가보고싶은 여행지였거든요",
          "speaker_id": "2",
          "time": "20210908 08:09"
        },
        {
          "id": "MDRW2100045854.1.19",
          "form": "ㅎㅎ 물가는 좀 많이 비쌌고..",
          "original_form": "ㅎㅎ 물가는 좀 많이 비쌌고..",
          "speaker_id": "1",
          "time": "20210908 08:10"
        },
        {
          "id": "MDRW2100045854.1.20",
          "form": "음식은 입에 안맞았고..",
          "original_form": "음식은 입에 안맞았고..",
          "speaker_id": "1",
          "time": "20210908 08:10"
        },
        {
          "id": "MDRW2100045854.1.21",
          "form": "풍경은 지상낙원이었어요 ㅎㅎ",
          "original_form": "풍경은 지상낙원이었어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:10"
        },
        {
          "id": "MDRW2100045854.1.22",
          "form": "헉 음식이 입에 안맞으셨나요?",
          "original_form": "헉 음식이 입에 안맞으셨나요?",
          "speaker_id": "2",
          "time": "20210908 08:11"
        },
        {
          "id": "MDRW2100045854.1.23",
          "form": "스위스 물가는 정말 .. 장난아니져..",
          "original_form": "스위스 물가는 정말 .. 장난아니져..",
          "speaker_id": "2",
          "time": "20210908 08:11"
        },
        {
          "id": "MDRW2100045854.1.24",
          "form": "기대했던 퐁듀가 실망이었어요 ㅋㅋㅋ",
          "original_form": "기대했던 퐁듀가 실망이었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 08:11"
        },
        {
          "id": "MDRW2100045854.1.25",
          "form": "헏 ㅠㅠ",
          "original_form": "헏 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 08:11"
        },
        {
          "id": "MDRW2100045854.1.26",
          "form": "뭔가 쓴맛이었어요 레알 찐치즈라 그런가",
          "original_form": "뭔가 쓴맛이었어요 레알 찐치즈라 그런가",
          "speaker_id": "1",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100045854.1.27",
          "form": "으악 쓴맛의 퐁듀라니",
          "original_form": "으악 쓴맛의 퐁듀라니",
          "speaker_id": "2",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100045854.1.28",
          "form": "기대했는데 실망이 더 크셨겠어요",
          "original_form": "기대했는데 실망이 더 크셨겠어요",
          "speaker_id": "2",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100045854.1.29",
          "form": "그치만 바라만봐도 배부른 풍경...ㅠ",
          "original_form": "그치만 바라만봐도 배부른 풍경...ㅠ",
          "speaker_id": "1",
          "time": "20210908 08:12"
        },
        {
          "id": "MDRW2100045854.1.30",
          "form": "그리워요 ㅎㅎㅎ",
          "original_form": "그리워요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 08:12"
        }
      ]
