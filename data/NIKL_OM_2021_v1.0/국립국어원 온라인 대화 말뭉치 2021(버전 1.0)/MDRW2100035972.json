      "utterance": [
        {
          "id": "MDRW2100035972.1.1",
          "form": "오늘",
          "original_form": "오늘",
          "speaker_id": "1",
          "time": "20210830 12:58"
        },
        {
          "id": "MDRW2100035972.1.2",
          "form": "반팔입으셨어요?",
          "original_form": "반팔입으셨어요?",
          "speaker_id": "1",
          "time": "20210830 12:58"
        },
        {
          "id": "MDRW2100035972.1.3",
          "form": "요즘 아침저녁으로 쌀쌀한데 긴팔 개시하셨나요?",
          "original_form": "요즘 아침저녁으로 쌀쌀한데 긴팔 개시하셨나요?",
          "speaker_id": "2",
          "time": "20210830 14:25"
        },
        {
          "id": "MDRW2100035972.1.4",
          "form": "넹 오늘부터 입고있는데",
          "original_form": "넹 오늘부터 입고있는데",
          "speaker_id": "1",
          "time": "20210830 14:25"
        },
        {
          "id": "MDRW2100035972.1.5",
          "form": "온도가 딱 좋고 좋은것같아용ㅎㅎ",
          "original_form": "온도가 딱 좋고 좋은것같아용ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 14:25"
        },
        {
          "id": "MDRW2100035972.1.6",
          "form": "저는 출퇴근 걸어다녀서 아직 반팔이에요 ㅋㅋ",
          "original_form": "저는 출퇴근 걸어다녀서 아직 반팔이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:26"
        },
        {
          "id": "MDRW2100035972.1.7",
          "form": "오늘 비온다던데 우산은 챙기셨어용?",
          "original_form": "오늘 비온다던데 우산은 챙기셨어용?",
          "speaker_id": "1",
          "time": "20210830 14:26"
        },
        {
          "id": "MDRW2100035972.1.8",
          "form": "네 우양산 들고다녀요",
          "original_form": "네 우양산 들고다녀요",
          "speaker_id": "2",
          "time": "20210830 14:30"
        },
        {
          "id": "MDRW2100035972.1.9",
          "form": "저도 우양산 유용하게 잘쓰고 있어요ㅋㅋㅋ",
          "original_form": "저도 우양산 유용하게 잘쓰고 있어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:30"
        },
        {
          "id": "MDRW2100035972.1.10",
          "form": "진짜 좋은데 항상 무게랑 크기 중에 고민이에요",
          "original_form": "진짜 좋은데 항상 무게랑 크기 중에 고민이에요",
          "speaker_id": "2",
          "time": "20210830 14:31"
        },
        {
          "id": "MDRW2100035972.1.11",
          "form": "맞아요ㅠㅠ 무게가 너무 가벼우면",
          "original_form": "맞아요ㅠㅠ 무게가 너무 가벼우면",
          "speaker_id": "1",
          "time": "20210830 14:31"
        },
        {
          "id": "MDRW2100035972.1.12",
          "form": "바람불때 위태위태하더라구요",
          "original_form": "바람불때 위태위태하더라구요",
          "speaker_id": "1",
          "time": "20210830 14:31"
        },
        {
          "id": "MDRW2100035972.1.13",
          "form": "맞아요 그렇다고 무거우면 양산용으로 쓸 때 귀찮아요",
          "original_form": "맞아요 그렇다고 무거우면 양산용으로 쓸 때 귀찮아요",
          "speaker_id": "2",
          "time": "20210830 14:32"
        },
        {
          "id": "MDRW2100035972.1.14",
          "form": "네네ㅋㅋㅋ 요즘 자외선이 너무 강해서",
          "original_form": "네네ㅋㅋㅋ 요즘 자외선이 너무 강해서",
          "speaker_id": "1",
          "time": "20210830 14:33"
        },
        {
          "id": "MDRW2100035972.1.15",
          "form": "양산 진짜 필수죠ㅠ",
          "original_form": "양산 진짜 필수죠ㅠ",
          "speaker_id": "1",
          "time": "20210830 14:33"
        },
        {
          "id": "MDRW2100035972.1.16",
          "form": "써보니까 안 썼던 때로 돌아갈 수 없더라고요 ㅋㅋㅋㅋ",
          "original_form": "써보니까 안 썼던 때로 돌아갈 수 없더라고요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100035972.1.17",
          "form": "맞아요 정수리 뜨겁고",
          "original_form": "맞아요 정수리 뜨겁고",
          "speaker_id": "1",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100035972.1.18",
          "form": "피부 열받고 이런거 못참겠더라구요ㅋㅋㅋ",
          "original_form": "피부 열받고 이런거 못참겠더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100035972.1.19",
          "form": "한여름에 잘썼어요",
          "original_form": "한여름에 잘썼어요",
