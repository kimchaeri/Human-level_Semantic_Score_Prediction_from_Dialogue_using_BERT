      "utterance": [
        {
          "id": "MDRW2100010470.1.1",
          "form": "이름만 들어봤어요!",
          "original_form": "이름만 들어봤어요!",
          "speaker_id": "1",
          "time": "20210823 18:39"
        },
        {
          "id": "MDRW2100010470.1.2",
          "form": "이번생은 가주가 되겠습니다 진짜 재미있어요!!!!!!",
          "original_form": "이번생은 가주가 되겠습니다 진짜 재미있어요!!!!!!",
          "speaker_id": "2",
          "time": "20210823 18:42"
        },
        {
          "id": "MDRW2100010470.1.3",
          "form": "저 뒤늦게 달리는데 너무",
          "original_form": "저 뒤늦게 달리는데 너무",
          "speaker_id": "2",
          "time": "20210823 18:42"
        },
        {
          "id": "MDRW2100010470.1.4",
          "form": "허어엉...허엉ㅇ어어..",
          "original_form": "허어엉...허엉ㅇ어어..",
          "speaker_id": "2",
          "time": "20210823 18:42"
        },
        {
          "id": "MDRW2100010470.1.5",
          "form": "시간 날 때 한번 달려볼게요",
          "original_form": "시간 날 때 한번 달려볼게요",
          "speaker_id": "1",
          "time": "20210823 18:43"
        },
        {
          "id": "MDRW2100010470.1.6",
          "form": "완결작이라 정주행하기 딱 좋네요ㅎㅎ",
          "original_form": "완결작이라 정주행하기 딱 좋네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 18:43"
        },
        {
          "id": "MDRW2100010470.1.7",
          "form": "앗.",
          "original_form": "앗.",
          "speaker_id": "2",
          "time": "20210823 18:44"
        },
        {
          "id": "MDRW2100010470.1.8",
          "form": "아쉽게도 오..완결이 아닙니다",
          "original_form": "아쉽게도 오..완결이 아닙니다",
          "speaker_id": "2",
          "time": "20210823 18:44"
        },
        {
          "id": "MDRW2100010470.1.9",
          "form": "그 외전이 나오고 있는데",
          "original_form": "그 외전이 나오고 있는데",
          "speaker_id": "2",
          "time": "20210823 18:44"
        },
        {
          "id": "MDRW2100010470.1.10",
          "form": "작가님이 8월달에 돌아오신다하고 안돌아오시더라고요",
          "original_form": "작가님이 8월달에 돌아오신다하고 안돌아오시더라고요",
          "speaker_id": "2",
          "time": "20210823 18:44"
        },
        {
          "id": "MDRW2100010470.1.11",
          "form": "저도 완결인줄 알고 달렸는데",
          "original_form": "저도 완결인줄 알고 달렸는데",
          "speaker_id": "2",
          "time": "20210823 18:45"
        },
        {
          "id": "MDRW2100010470.1.12",
          "form": "눈물이 났죠",
          "original_form": "눈물이 났죠",
          "speaker_id": "2",
          "time": "20210823 18:45"
        },
        {
          "id": "MDRW2100010470.1.13",
          "form": "아 정말요? 하 그래도 저는 원래",
          "original_form": "아 정말요? 하 그래도 저는 원래",
          "speaker_id": "1",
          "time": "20210823 18:47"
        },
        {
          "id": "MDRW2100010470.1.14",
          "form": "완결을 기다리는 편이 아니라 괜찮습니다",
          "original_form": "완결을 기다리는 편이 아니라 괜찮습니다",
          "speaker_id": "1",
          "time": "20210823 18:47"
        },
        {
          "id": "MDRW2100010470.1.15",
          "form": "실시간으로 달리는 독자분이셨군요",
          "original_form": "실시간으로 달리는 독자분이셨군요",
          "speaker_id": "2",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010470.1.16",
          "form": "참는 분들이 대단해요",
          "original_form": "참는 분들이 대단해요",
          "speaker_id": "1",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010470.1.17",
          "form": "저는 우선 구매하고 완결나면 달려요",
          "original_form": "저는 우선 구매하고 완결나면 달려요",
          "speaker_id": "2",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010470.1.18",
          "form": "크으",
          "original_form": "크으",
          "speaker_id": "1",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010470.1.19",
          "form": "구매 안하면 작가님들이 인기없다고 생각할까봐",
          "original_form": "구매 안하면 작가님들이 인기없다고 생각할까봐",
          "speaker_id": "2",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010470.1.20",
          "form": "젭알",
          "original_form": "젭알",
          "speaker_id": "2",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010470.1.21",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 18:48"
        },
        {
          "id": "MDRW2100010470.1.22",
          "form": "인기많은거 알아주셨으면 좋겠다고",
          "original_form": "인기많은거 알아주셨으면 좋겠다고",
          "speaker_id": "2",
          "time": "20210823 18:49"
        },
        {
          "id": "MDRW2100010470.1.23",
          "form": "생각해서",
          "original_form": "생각해서",
          "speaker_id": "2",
          "time": "20210823 18:49"
        },
        {
          "id": "MDRW2100010470.1.24",
          "form": "플랫폼들 수익구조가 정말 너무해요ㅠㅠ",
          "original_form": "플랫폼들 수익구조가 정말 너무해요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210823 18:49"
        },
        {
          "id": "MDRW2100010470.1.25",
          "form": "맞아요ㅜㅜㅜㅜ",
          "original_form": "맞아요ㅜㅜㅜㅜ",
          "speaker_id": "2",
