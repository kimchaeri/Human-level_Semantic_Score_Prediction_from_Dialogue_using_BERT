        },
        {
          "id": "MDRW2100033551.1.2",
          "form": "아니요~게임 안 해요...ㅎ",
          "original_form": "아니요~게임 안 해요...ㅎ",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100033551.1.3",
          "form": "name1님은 게임 좋아하시나요?",
          "original_form": "&name1&님은 게임 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210824 13:29"
        },
        {
          "id": "MDRW2100033551.1.4",
          "form": "저는 폰게임정도??ㅎㅎ",
          "original_form": "저는 폰게임정도??ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 13:34"
        },
        {
          "id": "MDRW2100033551.1.5",
          "form": "어떤 게임 하세요?",
          "original_form": "어떤 게임 하세요?",
          "speaker_id": "2",
          "time": "20210824 13:35"
        },
        {
          "id": "MDRW2100033551.1.6",
          "form": "카트라이더요ㅋㅋㅋ",
          "original_form": "카트라이더요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033551.1.7",
          "form": "아예안하시나요?",
          "original_form": "아예안하시나요?",
          "speaker_id": "1",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033551.1.8",
          "form": "네~저는 게임에는 취미가 없어요",
          "original_form": "네~저는 게임에는 취미가 없어요",
          "speaker_id": "2",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033551.1.9",
          "form": "카트라이더는 알아요",
          "original_form": "카트라이더는 알아요",
          "speaker_id": "2",
          "time": "20210824 23:09"
        },
        {
          "id": "MDRW2100033551.1.10",
          "form": "ㅋㅋㅋ 카트가 모바일로",
          "original_form": "ㅋㅋㅋ 카트가 모바일로",
          "speaker_id": "1",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100033551.1.11",
          "form": "있답니다",
          "original_form": "있답니다",
          "speaker_id": "1",
          "time": "20210824 23:32"
        },
        {
          "id": "MDRW2100033551.1.12",
          "form": "재밌을것 같아요~하신지 오래되셨어요?",
          "original_form": "재밌을것 같아요~하신지 오래되셨어요?",
          "speaker_id": "2",
          "time": "20210824 23:34"
        },
        {
          "id": "MDRW2100033551.1.13",
          "form": "음 한",
          "original_form": "음 한",
          "speaker_id": "1",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033551.1.14",
          "form": "6개월은 된것같네요ㅋㅋㅋ",
          "original_form": "6개월은 된것같네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100033551.1.15",
          "form": "오~꽤 되셨네요",
          "original_form": "오~꽤 되셨네요",
          "speaker_id": "2",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100033551.1.16",
          "form": "ㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋ맞아요",
          "speaker_id": "1",
          "time": "20210824 23:49"
        },
        {
          "id": "MDRW2100033551.1.17",
          "form": "게임은 그외는 저도 그닥이라",
          "original_form": "게임은 그외는 저도 그닥이라",
          "speaker_id": "1",
          "time": "20210824 23:50"
        },
        {
          "id": "MDRW2100033551.1.18",
