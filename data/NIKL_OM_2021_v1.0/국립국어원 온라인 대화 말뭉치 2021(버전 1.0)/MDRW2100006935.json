      "utterance": [
        {
          "id": "MDRW2100006935.1.1",
          "form": "여행갔던  기억 떠오르는거 있어?",
          "original_form": "여행갔던  기억 떠오르는거 있어?",
          "speaker_id": "1",
          "time": "20210822 01:06"
        },
        {
          "id": "MDRW2100006935.1.2",
          "form": "난 경주!",
          "original_form": "난 경주!",
          "speaker_id": "2",
          "time": "20210822 01:07"
        },
        {
          "id": "MDRW2100006935.1.3",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210822 01:08"
        },
        {
          "id": "MDRW2100006935.1.4",
          "form": "나는 여수!",
          "original_form": "나는 여수!",
          "speaker_id": "1",
          "time": "20210822 01:10"
        },
        {
          "id": "MDRW2100006935.1.5",
          "form": "쑥 아이스크림이 기억에  남지",
          "original_form": "쑥 아이스크림이 기억에  남지",
          "speaker_id": "1",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100006935.1.6",
          "form": "여수밤바다~ 도 봤니? ㅋㅋㅋ",
          "original_form": "여수밤바다~ 도 봤니? ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100006935.1.7",
          "form": "아크림 맛있겠당",
          "original_form": "아크림 맛있겠당",
          "speaker_id": "2",
          "time": "20210822 01:11"
        },
        {
          "id": "MDRW2100006935.1.8",
          "form": "그럼 여수 밤바다에서",
          "original_form": "그럼 여수 밤바다에서",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100006935.1.9",
          "form": "맥주마시고",
          "original_form": "맥주마시고",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100006935.1.10",
          "form": "다 했지",
          "original_form": "다 했지",
          "speaker_id": "1",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100006935.1.11",
          "form": "크흐 취한다",
          "original_form": "크흐 취한다",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100006935.1.12",
          "form": "나는 경주 야경이 너무 예뻤어",
          "original_form": "나는 경주 야경이 너무 예뻤어",
          "speaker_id": "2",
          "time": "20210822 01:12"
        },
        {
          "id": "MDRW2100006935.1.13",
          "form": "그 천문대 있는데서 야경 봤어?",
          "original_form": "그 천문대 있는데서 야경 봤어?",
          "speaker_id": "1",
          "time": "20210822 01:13"
        },
        {
          "id": "MDRW2100006935.1.14",
          "form": "숙소 테라스 야경이 너무 예뻤어",
          "original_form": "숙소 테라스 야경이 너무 예뻤어",
          "speaker_id": "2",
          "time": "20210822 01:17"
        },
        {
          "id": "MDRW2100006935.1.15",
          "form": "와 야경",
          "original_form": "와 야경",
          "speaker_id": "1",
          "time": "20210822 01:19"
        },
        {
          "id": "MDRW2100006935.1.16",
          "form": "멋있겠다",
          "original_form": "멋있겠다",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100006935.1.17",
          "form": "야경 보면서 해뜰 때까지 있을수 있음 난",
          "original_form": "야경 보면서 해뜰 때까지 있을수 있음 난",
          "speaker_id": "1",
          "time": "20210822 01:20"
        },
        {
          "id": "MDRW2100006935.1.18",
          "form": "ㅋㅋㅋㅋㅋ 밤 좋아하나보다",
          "original_form": "ㅋㅋㅋㅋㅋ 밤 좋아하나보다",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100006935.1.19",
          "form": "밤에 비오고 천둥치고 비바랖 부는거 좋아해",
          "original_form": "밤에 비오고 천둥치고 비바랖 부는거 좋아해",
          "speaker_id": "1",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100006935.1.20",
          "form": "ㅋㅋㅋㅋ 그럼 약간 바다나 산속으로 여행가야 되겠는걸",
          "original_form": "ㅋㅋㅋㅋ 그럼 약간 바다나 산속으로 여행가야 되겠는걸",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100006935.1.21",
          "form": "그런데에서 비오면 되게 힐링이야",
          "original_form": "그런데에서 비오면 되게 힐링이야",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100006935.1.22",
          "form": "비소리 들으면ㅅㅓ",
          "original_form": "비소리 들으면ㅅㅓ",
          "speaker_id": "1",
          "time": "20210822 01:31"
        },
        {
          "id": "MDRW2100006935.1.23",
          "form": "그땐  술 안먹어도 좋을 거 같아",
          "original_form": "그땐  술 안먹어도 좋을 거 같아",
          "speaker_id": "1",
          "time": "20210822 01:32"
        }
      ]
