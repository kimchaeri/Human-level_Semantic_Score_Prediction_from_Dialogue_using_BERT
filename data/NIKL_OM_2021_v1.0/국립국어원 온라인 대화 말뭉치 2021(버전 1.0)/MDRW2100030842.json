      "utterance": [
        {
          "id": "MDRW2100030842.1.1",
          "form": "시장 왔는데 아직 사람들 쫌 많긴 하네ㅋㅋ",
          "original_form": "시장 왔는데 아직 사람들 쫌 많긴 하네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:36"
        },
        {
          "id": "MDRW2100030842.1.2",
          "form": "헐 납작만두 사장님 안 나오심....",
          "original_form": "헐 납작만두 사장님 안 나오심....",
          "speaker_id": "2",
          "time": "20210904 14:38"
        },
        {
          "id": "MDRW2100030842.1.3",
          "form": "머선일이고 ㅋㅋㅋㅋ",
          "original_form": "머선일이고 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:40"
        },
        {
          "id": "MDRW2100030842.1.4",
          "form": "지금 막 나갈랬는데 ㅋㅋㅋㅋ",
          "original_form": "지금 막 나갈랬는데 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:40"
        },
        {
          "id": "MDRW2100030842.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 14:42"
        },
        {
          "id": "MDRW2100030842.1.6",
          "form": "마스크까지썼는데 ㅋㅋㅋ",
          "original_form": "마스크까지썼는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 14:42"
        },
        {
          "id": "MDRW2100030842.1.7",
          "form": "정보 고맙따...",
          "original_form": "정보 고맙따...",
          "speaker_id": "1",
          "time": "20210904 14:42"
        },
        {
          "id": "MDRW2100030842.1.8",
          "form": "응 나의 정보가 늦지않아서 다행이다....",
          "original_form": "응 나의 정보가 늦지않아서 다행이다....",
          "speaker_id": "2",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100030842.1.9",
          "form": "내 납작만두ㅜㅜ",
          "original_form": "내 납작만두ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 15:20"
        },
        {
          "id": "MDRW2100030842.1.10",
          "form": "다음 오일장을 기약하며..",
          "original_form": "다음 오일장을 기약하며..",
          "speaker_id": "2",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100030842.1.11",
          "form": "아직 날이 더워서 번데기는 안샀다ㅋㅋㅋ",
          "original_form": "아직 날이 더워서 번데기는 안샀다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:22"
        },
        {
          "id": "MDRW2100030842.1.12",
          "form": "다음에 꼭! ㅋㅋㅋㅋ",
          "original_form": "다음에 꼭! ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:24"
        },
        {
          "id": "MDRW2100030842.1.13",
          "form": "나도 야채비벼서 만두에 싸먹고싶었는디ㅠㅋㅋ",
          "original_form": "나도 야채비벼서 만두에 싸먹고싶었는디ㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100030842.1.14",
          "form": "꽈배기랑 순대 샀다ㅋㅋ",
          "original_form": "꽈배기랑 순대 샀다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:30"
        },
        {
          "id": "MDRW2100030842.1.15",
          "form": "아 맛있겠다",
          "original_form": "아 맛있겠다",
          "speaker_id": "1",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100030842.1.16",
          "form": "순대.. !",
          "original_form": "순대.. !",
          "speaker_id": "1",
          "time": "20210904 15:37"
        },
        {
          "id": "MDRW2100030842.1.17",
          "form": "은아먹거리에서 샀다ㅋㅋ",
          "original_form": "은아먹거리에서 샀다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 15:47"
        },
        {
          "id": "MDRW2100030842.1.18",
          "form": "거기가 진짜 저렴한 편인 듯 좋지 ㅋㅋ",
          "original_form": "거기가 진짜 저렴한 편인 듯 좋지 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:51"
        },
        {
          "id": "MDRW2100030842.1.19",
          "form": "응 양도 많이 주시더라ㅎㅎ",
          "original_form": "응 양도 많이 주시더라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 15:52"
        },
        {
          "id": "MDRW2100030842.1.20",
          "form": "순대...막장....",
          "original_form": "순대...막장....",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100030842.1.21",
          "form": "분식이 땡기는군",
          "original_form": "분식이 땡기는군",
          "speaker_id": "1",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100030842.1.22",
          "form": "지금 먹고있는데 내장도 맛있구먼ㅋㅋ",
          "original_form": "지금 먹고있는데 내장도 맛있구먼ㅋㅋ",
