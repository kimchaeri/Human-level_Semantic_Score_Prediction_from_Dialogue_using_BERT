      "utterance": [
        {
          "id": "MDRW2100014486.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014486.1.2",
          "form": "요즘 어떤 TV프로그램 보세요?",
          "original_form": "요즘 어떤 TV프로그램 보세요?",
          "speaker_id": "1",
          "time": "20210825 19:19"
        },
        {
          "id": "MDRW2100014486.1.3",
          "form": "유대석 나오는거 많이봐요.. \r\n식스센스 놀뭐",
          "original_form": "유대석 나오는거 많이봐요.. \r\n식스센스 놀뭐",
          "speaker_id": "2",
          "time": "20210825 19:22"
        },
        {
          "id": "MDRW2100014486.1.4",
          "form": "맞아요 다 재밌죠ㅋㅋㅋ",
          "original_form": "맞아요 다 재밌죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014486.1.5",
          "form": "유퀴즈도요",
          "original_form": "유퀴즈도요",
          "speaker_id": "1",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014486.1.6",
          "form": "드라마는요?",
          "original_form": "드라마는요?",
          "speaker_id": "1",
          "time": "20210825 19:29"
        },
        {
          "id": "MDRW2100014486.1.7",
          "form": "유퀴즈는 엑기스만요... \r\n드라마는 잘 몰라여.. \r\n뭐가 젤 잘나가나요?",
          "original_form": "유퀴즈는 엑기스만요... \r\n드라마는 잘 몰라여.. \r\n뭐가 젤 잘나가나요?",
          "speaker_id": "2",
          "time": "20210825 19:33"
        },
        {
          "id": "MDRW2100014486.1.8",
          "form": "음.. 요즘에는 슬의생?",
          "original_form": "음.. 요즘에는 슬의생?",
          "speaker_id": "1",
          "time": "20210825 19:34"
        },
        {
          "id": "MDRW2100014486.1.9",
          "form": "슬기로운 의사생활이요",
          "original_form": "슬기로운 의사생활이요",
          "speaker_id": "1",
          "time": "20210825 19:34"
        },
        {
          "id": "MDRW2100014486.1.10",
          "form": "요즘 핫해요 핫해",
          "original_form": "요즘 핫해요 핫해",
          "speaker_id": "1",
          "time": "20210825 19:34"
        },
        {
          "id": "MDRW2100014486.1.11",
          "form": "들어는 봤어요.. 의사가 연쇄살인마?",
          "original_form": "들어는 봤어요.. 의사가 연쇄살인마?",
          "speaker_id": "2",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100014486.1.12",
          "form": "아?",
          "original_form": "아?",
          "speaker_id": "1",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100014486.1.13",
          "form": "연쇄살인마요????",
          "original_form": "연쇄살인마요????",
          "speaker_id": "1",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100014486.1.14",
          "form": "아유 전혀요",
          "original_form": "아유 전혀요",
          "speaker_id": "1",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100014486.1.15",
          "form": "완전 따뜻하고 훈훈한 드라마예요",
          "original_form": "완전 따뜻하고 훈훈한 드라마예요",
          "speaker_id": "1",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100014486.1.16",
          "form": "ㅋㅋ 아.. 지금 영드 보는거랑 헷갈렷어요.. ㅋㅋ",
          "original_form": "ㅋㅋ 아.. 지금 영드 보는거랑 헷갈렷어요.. ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 19:35"
        },
        {
          "id": "MDRW2100014486.1.17",
          "form": "넷플릭스 하세여?",
          "original_form": "넷플릭스 하세여?",
          "speaker_id": "2",
          "time": "20210825 19:36"
        },
        {
          "id": "MDRW2100014486.1.18",
          "form": "네ㅋㅋㅋ넷플릭스도 보고 왓챠도 봐요",
          "original_form": "네ㅋㅋㅋ넷플릭스도 보고 왓챠도 봐요",
          "speaker_id": "1",
          "time": "20210825 19:37"
        },
        {
          "id": "MDRW2100014486.1.19",
          "form": "두개나 다하세요?",
          "original_form": "두개나 다하세요?",
          "speaker_id": "2",
          "time": "20210825 19:38"
        },
        {
          "id": "MDRW2100014486.1.20",
          "form": "네!",
          "original_form": "네!",
          "speaker_id": "1",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014486.1.21",
          "form": "주로 넷플릭스로는 영화보고 왓챠는 국내드라마?",
          "original_form": "주로 넷플릭스로는 영화보고 왓챠는 국내드라마?",
          "speaker_id": "1",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014486.1.22",
          "form": "옛날 작품들이 많더라고요",
          "original_form": "옛날 작품들이 많더라고요",
          "speaker_id": "1",
          "time": "20210825 19:39"
        },
        {
          "id": "MDRW2100014486.1.23",
          "form": "아.. 왓챠에 한드도 많아요? 영화만 있는줄..",
          "original_form": "아.. 왓챠에 한드도 많아요? 영화만 있는줄..",
          "speaker_id": "2",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100014486.1.24",
          "form": "한번도 안해봣어여..",
          "original_form": "한번도 안해봣어여..",
          "speaker_id": "2",
          "time": "20210825 19:40"
        },
        {
          "id": "MDRW2100014486.1.25",
          "form": "네 한국 드라마 진짜 많아요",
          "original_form": "네 한국 드라마 진짜 많아요",
          "speaker_id": "1",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100014486.1.26",
          "form": "추억을 곱씹으면서 보기에 딱이에요",
          "original_form": "추억을 곱씹으면서 보기에 딱이에요",
          "speaker_id": "1",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100014486.1.27",
          "form": "한드는 너무 길지 않아요? 막 30화짜리도 있고",
          "original_form": "한드는 너무 길지 않아요? 막 30화짜리도 있고",
          "speaker_id": "2",
          "time": "20210825 19:41"
        },
        {
          "id": "MDRW2100014486.1.28",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014486.1.29",
          "form": "짧은 건 10-15화",
          "original_form": "짧은 건 10-15화",
          "speaker_id": "1",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014486.1.30",
          "form": "긴 건 뭐 엄청 길고요",
          "original_form": "긴 건 뭐 엄청 길고요",
          "speaker_id": "1",
          "time": "20210825 19:43"
        },
        {
          "id": "MDRW2100014486.1.31",
          "form": "해리포터 조아하시나바여ㅡ",
          "original_form": "해리포터 조아하시나바여ㅡ",
          "speaker_id": "2",
          "time": "20210825 20:30"
        }
      ]
