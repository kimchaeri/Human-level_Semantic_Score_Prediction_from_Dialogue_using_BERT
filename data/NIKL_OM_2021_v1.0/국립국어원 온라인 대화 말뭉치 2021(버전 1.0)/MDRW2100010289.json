      "utterance": [
        {
          "id": "MDRW2100010289.1.1",
          "form": "분리수거하기 힘드네요",
          "original_form": "분리수거하기 힘드네요",
          "speaker_id": "1",
          "time": "20210823 16:28"
        },
        {
          "id": "MDRW2100010289.1.2",
          "form": "네 ㅎㅎ어찌나 빨리 쌓이는지",
          "original_form": "네 ㅎㅎ어찌나 빨리 쌓이는지",
          "speaker_id": "2",
          "time": "20210823 16:31"
        },
        {
          "id": "MDRW2100010289.1.3",
          "form": "코로나로 쓰레기가문제래요",
          "original_form": "코로나로 쓰레기가문제래요",
          "speaker_id": "1",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100010289.1.4",
          "form": "원래도 많이나왔지만ㅎㅎ",
          "original_form": "원래도 많이나왔지만ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100010289.1.5",
          "form": "맞아요 더 많아진거같아요 체감상으로도ㅎㅎ",
          "original_form": "맞아요 더 많아진거같아요 체감상으로도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:49"
        },
        {
          "id": "MDRW2100010289.1.6",
          "form": "택배를 안받고 쓰레기재활용하고 해야하는데",
          "original_form": "택배를 안받고 쓰레기재활용하고 해야하는데",
          "speaker_id": "1",
          "time": "20210823 16:51"
        },
        {
          "id": "MDRW2100010289.1.7",
          "form": "상품이 다양해진만큼 분리수거법도 다양해지지못해서 큰일이에요",
          "original_form": "상품이 다양해진만큼 분리수거법도 다양해지지못해서 큰일이에요",
          "speaker_id": "1",
          "time": "20210823 16:52"
        },
        {
          "id": "MDRW2100010289.1.8",
          "form": "맞아요 택배상자 크게 오는거보면 양심에 찔리고ㅠㅠ",
          "original_form": "맞아요 택배상자 크게 오는거보면 양심에 찔리고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210823 16:52"
        },
        {
          "id": "MDRW2100010289.1.9",
          "form": "필요할땐 또 없어요 그것이 ㅋㅋ",
          "original_form": "필요할땐 또 없어요 그것이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:54"
        },
        {
          "id": "MDRW2100010289.1.10",
          "form": "쌓아둘수도없고",
          "original_form": "쌓아둘수도없고",
          "speaker_id": "1",
          "time": "20210823 16:54"
        },
        {
          "id": "MDRW2100010289.1.11",
          "form": "맞아요ㅎㅎ진짜 필요할때 없어요ㅠㅠ 사서써야되고",
          "original_form": "맞아요ㅎㅎ진짜 필요할때 없어요ㅠㅠ 사서써야되고",
          "speaker_id": "2",
          "time": "20210823 16:55"
        },
        {
          "id": "MDRW2100010289.1.12",
          "form": "뽁뽁이두요 ㅋㅋ",
          "original_form": "뽁뽁이두요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 16:56"
        },
        {
          "id": "MDRW2100010289.1.13",
          "form": "맞아요ㅎㅎ진짜 짜증날때 많아요ㅋㅋ",
          "original_form": "맞아요ㅎㅎ진짜 짜증날때 많아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 16:56"
        },
        {
          "id": "MDRW2100010289.1.14",
          "form": "재활용하고싶어도 공간이 없어서 두기어렵네요",
          "original_form": "재활용하고싶어도 공간이 없어서 두기어렵네요",
          "speaker_id": "1",
          "time": "20210823 16:57"
        },
        {
          "id": "MDRW2100010289.1.15",
          "form": "집은좁고",
          "original_form": "집은좁고",
