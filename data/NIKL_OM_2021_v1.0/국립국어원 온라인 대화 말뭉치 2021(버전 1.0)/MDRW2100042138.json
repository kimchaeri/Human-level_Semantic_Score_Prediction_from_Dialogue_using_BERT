          "speaker_id": "1",
          "time": "20210906 20:09"
        },
        {
          "id": "MDRW2100042138.1.8",
          "form": "흠흠 이뿌네요ㅎㅎ",
          "original_form": "흠흠 이뿌네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 20:09"
        },
        {
          "id": "MDRW2100042138.1.9",
          "form": "구쵸 ㅋㅋㅋ",
          "original_form": "구쵸 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 20:11"
        },
        {
          "id": "MDRW2100042138.1.10",
          "form": "사과 먹을 때 짭짭 먹는거 넘 귀여워요",
          "original_form": "사과 먹을 때 짭짭 먹는거 넘 귀여워요",
          "speaker_id": "2",
          "time": "20210906 20:11"
        },
        {
          "id": "MDRW2100042138.1.11",
          "form": "ㅎㅎㅎㅎ사과좋아해요?",
          "original_form": "ㅎㅎㅎㅎ사과좋아해요?",
          "speaker_id": "1",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100042138.1.12",
          "form": "네네 조아해요",
          "original_form": "네네 조아해요",
          "speaker_id": "2",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100042138.1.13",
          "form": "친구강아지는고구마좋아하더라고요!",
          "original_form": "친구강아지는고구마좋아하더라고요!",
          "speaker_id": "1",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100042138.1.14",
          "form": "씹는 소리가 너무 귀여워요",
          "original_form": "씹는 소리가 너무 귀여워요",
          "speaker_id": "2",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100042138.1.15",
          "form": "고구마도 조아해요",
          "original_form": "고구마도 조아해요",
          "speaker_id": "2",
          "time": "20210906 20:12"
        },
        {
          "id": "MDRW2100042138.1.16",
          "form": "사랑스럽겠어요",
          "original_form": "사랑스럽겠어요",
          "speaker_id": "1",
          "time": "20210906 20:13"
        },
        {
          "id": "MDRW2100042138.1.17",
          "form": "산책도가시나요?",
          "original_form": "산책도가시나요?",
