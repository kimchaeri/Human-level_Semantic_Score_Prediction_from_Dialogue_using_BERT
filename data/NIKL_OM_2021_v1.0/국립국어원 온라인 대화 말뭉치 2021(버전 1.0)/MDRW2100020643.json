      "utterance": [
        {
          "id": "MDRW2100020643.1.1",
          "form": "그러게요 ... 병크 계속 터지면",
          "original_form": "그러게요 ... 병크 계속 터지면",
          "speaker_id": "2",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020643.1.2",
          "form": "왜 자꾸 논란의 중심에 서려고 하는지퓨",
          "original_form": "왜 자꾸 논란의 중심에 서려고 하는지퓨",
          "speaker_id": "1",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020643.1.3",
          "form": "뭔가 믿음이 떨어지는 ..",
          "original_form": "뭔가 믿음이 떨어지는 ..",
          "speaker_id": "2",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020643.1.4",
          "form": "입덕장벽이라고 불리더라구욬ㅋㅋㅋㅋㅋㅋ",
          "original_form": "입덕장벽이라고 불리더라구욬ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020643.1.5",
          "form": "하",
          "original_form": "하",
          "speaker_id": "1",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020643.1.6",
          "form": "아 미쳐요 ㅋㅋㅋㅋㅋㅋㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 미쳐요 ㅋㅋㅋㅋㅋㅋㅌㅌㅌㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020643.1.7",
          "form": "이쯤되면 누군지 아실거에욬ㅋㅋㅋㅋ",
          "original_form": "이쯤되면 누군지 아실거에욬ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020643.1.8",
          "form": "웃프네요 ...",
          "original_form": "웃프네요 ...",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020643.1.9",
          "form": "맞아요 제이....",
          "original_form": "맞아요 제이....",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020643.1.10",
          "form": "넼ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ ......",
          "original_form": "넼ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ ......",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020643.1.11",
          "form": "갑자기 그 얘기가 떠올라요",
          "original_form": "갑자기 그 얘기가 떠올라요",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020643.1.12",
          "form": "저는 오빠를 위해 열심히 사는데 오빠는 왜 막살아요",
          "original_form": "저는 오빠를 위해 열심히 사는데 오빠는 왜 막살아요",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020643.1.13",
          "form": "이 말이... 떠올라요ㅠㅠㅠㅋㅋㅋㅋ",
          "original_form": "이 말이... 떠올라요ㅠㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020643.1.14",
          "form": "엇 저 틔터에서 뭔가 본 것 같아요",
          "original_form": "엇 저 틔터에서 뭔가 본 것 같아요",
          "speaker_id": "2",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020643.1.15",
          "form": "진짜 그렇내요",
          "original_form": "진짜 그렇내요",
          "speaker_id": "2",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020643.1.16",
          "form": "저두요ㅠㅠㅋㅋㅋ",
          "original_form": "저두요ㅠㅠㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020643.1.17",
          "form": "팬없이 가수가 있기는 힘들잖아요 ..",
          "original_form": "팬없이 가수가 있기는 힘들잖아요 ..",
          "speaker_id": "2",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020643.1.18",
          "form": "김재환님은 아마 그런일 평생 없으실듯...",
          "original_form": "김재환님은 아마 그런일 평생 없으실듯...",
          "speaker_id": "1",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020643.1.19",
          "form": "바르고 성실한 이미지라",
          "original_form": "바르고 성실한 이미지라",
          "speaker_id": "1",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020643.1.20",
          "form": "이럴일은 없을것같아요ㅋㅋㅋ",
          "original_form": "이럴일은 없을것같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020643.1.21",
          "form": "제발 그랬으면 좋겠어요 .......... 근데 그 이미지가 전부가 아니라 ....",
          "original_form": "제발 그랬으면 좋겠어요 .......... 근데 그 이미지가 전부가 아니라 ....",
          "speaker_id": "2",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020643.1.22",
          "form": "제가 워너원 진짜 좋아했엇ㄱ는데",
          "original_form": "제가 워너원 진짜 좋아했엇ㄱ는데",
          "speaker_id": "2",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020643.1.23",
          "form": "박지훈 아세요 ??",
          "original_form": "박지훈 아세요 ??",
          "speaker_id": "2",
          "time": "20210828 10:44"
        },
        {
          "id": "MDRW2100020643.1.24",
          "form": "힝ㅠㅠㅠ 저도 그 뭐지 요하인가 그거 박스로 사다먹고 그랬는데ㅠㅠ",
          "original_form": "힝ㅠㅠㅠ 저도 그 뭐지 요하인가 그거 박스로 사다먹고 그랬는데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020643.1.25",
          "form": "알져 윙깅이",
          "original_form": "알져 윙깅이",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020643.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020643.1.27",
          "form": "그분도 병크 터졌었거든요 ... 진짜 그거보고 너무 충격받아서 ......",
          "original_form": "그분도 병크 터졌었거든요 ... 진짜 그거보고 너무 충격받아서 ......",
          "speaker_id": "2",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020643.1.28",
          "form": "아아....",
          "original_form": "아아....",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020643.1.29",
          "form": "지금 좀 잠잠해진 것 같기는 한데 ..",
          "original_form": "지금 좀 잠잠해진 것 같기는 한데 ..",
          "speaker_id": "2",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020643.1.30",
          "form": "여자친구...아....",
          "original_form": "여자친구...아....",
          "speaker_id": "1",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020643.1.31",
          "form": "요하잍ㅌㅌㅋㅋㅋㅋㅋㅋ 맞아요 넘나 추억이에요",
          "original_form": "요하잍ㅌㅌㅋㅋㅋㅋㅋㅋ 맞아요 넘나 추억이에요",
          "speaker_id": "2",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020643.1.32",
