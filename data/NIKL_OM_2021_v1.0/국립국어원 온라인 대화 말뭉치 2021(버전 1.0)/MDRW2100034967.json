      "utterance": [
        {
          "id": "MDRW2100034967.1.1",
          "form": "외국은못갈것같아요..",
          "original_form": "외국은못갈것같아요..",
          "speaker_id": "2",
          "time": "20210828 23:04"
        },
        {
          "id": "MDRW2100034967.1.2",
          "form": "분식못먹으면못살거같은데..!!!!!!",
          "original_form": "분식못먹으면못살거같은데..!!!!!!",
          "speaker_id": "2",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100034967.1.3",
          "form": "ㅎㅎ 먹을수는 있을거예요",
          "original_form": "ㅎㅎ 먹을수는 있을거예요",
          "speaker_id": "1",
          "time": "20210828 23:05"
        },
        {
          "id": "MDRW2100034967.1.4",
          "form": "기본 떡볶이 한접시 일이만원 내고...?",
          "original_form": "기본 떡볶이 한접시 일이만원 내고...?",
          "speaker_id": "1",
          "time": "20210828 23:06"
        },
        {
          "id": "MDRW2100034967.1.5",
          "form": "..일이만원이라니..@ㅁ@...!!!!!!!! 혹시 외국에도 많이 가보셨나요?!",
          "original_form": "..일이만원이라니..@ㅁ@...!!!!!!!! 혹시 외국에도 많이 가보셨나요?!",
          "speaker_id": "2",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100034967.1.6",
          "form": "아뇨ㅎㅎ 자주는 안 가봤죠",
          "original_form": "아뇨ㅎㅎ 자주는 안 가봤죠",
          "speaker_id": "1",
          "time": "20210828 23:07"
        },
        {
          "id": "MDRW2100034967.1.7",
          "form": "근데 외국 가면 한식 물가 비싸대서 그냥 현지음식 찾아 먹어요",
          "original_form": "근데 외국 가면 한식 물가 비싸대서 그냥 현지음식 찾아 먹어요",
          "speaker_id": "1",
          "time": "20210828 23:08"
        },
        {
          "id": "MDRW2100034967.1.8",
          "form": "ㅎㅎ 맞아요 자주가는게 아니라면 현지음식 먹는게 나을거 같아요!",
          "original_form": "ㅎㅎ 맞아요 자주가는게 아니라면 현지음식 먹는게 나을거 같아요!",
          "speaker_id": "2",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100034967.1.9",
          "form": "그러고보니 베트남인가 어디 떡볶이 전파한 사업가가 있는데",
          "original_form": "그러고보니 베트남인가 어디 떡볶이 전파한 사업가가 있는데",
          "speaker_id": "1",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100034967.1.10",
          "form": "거긴 물가가 싸서 괜찮을 듯ㅎ",
          "original_form": "거긴 물가가 싸서 괜찮을 듯ㅎ",
          "speaker_id": "1",
          "time": "20210828 23:09"
        },
        {
          "id": "MDRW2100034967.1.11",
          "form": "오 베트남에 떡볶이를 전파한 사업가가있나요? 대박 ㅇㅁㅇ",
          "original_form": "오 베트남에 떡볶이를 전파한 사업가가있나요? 대박 ㅇㅁㅇ",
          "speaker_id": "2",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100034967.1.12",
          "form": "새로운 사실을 알았네요 ㅋㅋㅋ",
          "original_form": "새로운 사실을 알았네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 23:10"
        },
        {
          "id": "MDRW2100034967.1.13",
          "form": "아 필리핀이네요",
          "original_form": "아 필리핀이네요",
          "speaker_id": "1",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034967.1.14",
          "form": "네네 그분이 김치시즈닝 만드신 분ㅎㅎ",
          "original_form": "네네 그분이 김치시즈닝 만드신 분ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034967.1.15",
          "form": "오 필리핀도신기한데요ㅇㅁㅇ?",
          "original_form": "오 필리핀도신기한데요ㅇㅁㅇ?",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034967.1.16",
          "form": "아!!!!",
          "original_form": "아!!!!",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034967.1.17",
          "form": "유퀴즈에서봤던그분이신가..!!",
          "original_form": "유퀴즈에서봤던그분이신가..!!",
          "speaker_id": "2",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034967.1.18",
          "form": "아마존서 일본시치미 제치고 외국조미료 1위라넹ㆍ",
          "original_form": "아마존서 일본시치미 제치고 외국조미료 1위라넹ㆍ",
          "speaker_id": "1",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034967.1.19",
          "form": "네네 그분이요",
          "original_form": "네네 그분이요",
          "speaker_id": "1",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034967.1.20",
          "form": "유퀴즈에도 나왔군요!",
          "original_form": "유퀴즈에도 나왔군요!",
          "speaker_id": "1",
          "time": "20210828 23:11"
        },
        {
          "id": "MDRW2100034967.1.21",
          "form": "전 인터뷰로 접했어요",
          "original_form": "전 인터뷰로 접했어요",
          "speaker_id": "1",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100034967.1.22",
          "form": "네네 그분 맞나봐요!  오오  대단하다고 생각했어요 ㅠㅁㅠ",
          "original_form": "네네 그분 맞나봐요!  오오  대단하다고 생각했어요 ㅠㅁㅠ",
          "speaker_id": "2",
          "time": "20210828 23:12"
        },
        {
          "id": "MDRW2100034967.1.23",
          "form": "정말 아이디어 실행력 다 대단한 것 같아요",
          "original_form": "정말 아이디어 실행력 다 대단한 것 같아요",
          "speaker_id": "1",
          "time": "20210828 23:13"
        },
        {
          "id": "MDRW2100034967.1.24",
          "form": "김치시즈닝 맛이 궁금해요ㅎ",
          "original_form": "김치시즈닝 맛이 궁금해요ㅎ",
          "speaker_id": "1",
          "time": "20210828 23:14"
        },
        {
          "id": "MDRW2100034967.1.25",
          "form": "그니까요 실행하기가 쉬운게아닌데..  저도 궁금해요 대체무슨맛이길래 그렇게인기가좋은지ㅎㅎ",
          "original_form": "그니까요 실행하기가 쉬운게아닌데..  저도 궁금해요 대체무슨맛이길래 그렇게인기가좋은지ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100034967.1.26",
          "form": "김치맛..김치맛 음..",
          "original_form": "김치맛..김치맛 음..",
          "speaker_id": "2",
          "time": "20210828 23:15"
        },
        {
          "id": "MDRW2100034967.1.27",
          "form": "아 태국인가는 김치국물만도 따로 판대요ㅎㅎ",
          "original_form": "아 태국인가는 김치국물만도 따로 판대요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 23:15"
        }
      ]
