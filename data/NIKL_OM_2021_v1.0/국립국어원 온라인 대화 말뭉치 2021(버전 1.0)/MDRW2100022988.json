        },
        {
          "id": "MDRW2100022988.1.2",
          "form": "스포츠스타좋아하세요?",
          "original_form": "스포츠스타좋아하세요?",
          "speaker_id": "1",
          "time": "20210829 20:05"
        },
        {
          "id": "MDRW2100022988.1.3",
          "form": "한때는 야구 되게좋아했어요 ㅎㅎ",
          "original_form": "한때는 야구 되게좋아했어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100022988.1.4",
          "form": "오ㅎㅎ 누구요?ㅎ",
          "original_form": "오ㅎㅎ 누구요?ㅎ",
          "speaker_id": "1",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100022988.1.5",
          "form": "야구엄청좋죠ㅎ",
          "original_form": "야구엄청좋죠ㅎ",
          "speaker_id": "1",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100022988.1.6",
          "form": "세대가 다른가?ㅎ 라떼는 이승엽 좋아했었는데ㅎㅎ",
          "original_form": "세대가 다른가?ㅎ 라떼는 이승엽 좋아했었는데ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 20:12"
        },
        {
          "id": "MDRW2100022988.1.7",
          "form": "저도 비슷한 세대 같사옵니다 ㅋㅋ",
          "original_form": "저도 비슷한 세대 같사옵니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:14"
        },
        {
          "id": "MDRW2100022988.1.8",
          "form": "그다지 다르지 않아욥 ㅋㅋㅋ",
          "original_form": "그다지 다르지 않아욥 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100022988.1.9",
          "form": "ㅎㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100022988.1.10",
          "form": "30대?ㅎ",
          "original_form": "30대?ㅎ",
          "speaker_id": "1",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100022988.1.11",
          "form": "전 중반이욤ㅎㅎㅎ",
          "original_form": "전 중반이욤ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100022988.1.12",
          "form": "선동렬나왔으면 나갈까? 생각했습니다ㅎㅎ",
          "original_form": "선동렬나왔으면 나갈까? 생각했습니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100022988.1.13",
          "form": "저도 30대중반이라서",
          "original_form": "저도 30대중반이라서",
          "speaker_id": "2",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100022988.1.14",
          "form": "음 양준혁세대군요?ㅋㅋㅋ",
          "original_form": "음 양준혁세대군요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 20:39"
        },
        {
          "id": "MDRW2100022988.1.15",
          "form": "오오ㅎㅎ",
          "original_form": "오오ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100022988.1.16",
          "form": "같은세대네요ㅎ",
          "original_form": "같은세대네요ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100022988.1.17",
          "form": "어떤팀 좋아하셨어요?",
          "original_form": "어떤팀 좋아하셨어요?",
          "speaker_id": "1",
          "time": "20210829 21:14"
        },
        {
          "id": "MDRW2100022988.1.18",
          "form": "저는 대구에서 학교를",
          "original_form": "저는 대구에서 학교를",
          "speaker_id": "2",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100022988.1.19",
          "form": "다녀서요",
          "original_form": "다녀서요",
          "speaker_id": "2",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100022988.1.20",
          "form": "삼성  라이온즈요",
          "original_form": "삼성  라이온즈요",
          "speaker_id": "2",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100022988.1.21",
          "form": "오오ㅎ 양준혁ㅎ",
          "original_form": "오오ㅎ 양준혁ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100022988.1.22",
          "form": "이승엽 ㅎ",
          "original_form": "이승엽 ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100022988.1.23",
          "form": "직관은요?",
          "original_form": "직관은요?",
          "speaker_id": "1",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100022988.1.24",
          "form": "학교다닐땐 보러다녔어요",
          "original_form": "학교다닐땐 보러다녔어요",
          "speaker_id": "2",
          "time": "20210829 21:21"
        },
        {
          "id": "MDRW2100022988.1.25",
          "form": "와 그게 언제야 ㄷㄷㄷ",
          "original_form": "와 그게 언제야 ㄷㄷㄷ",
          "speaker_id": "2",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100022988.1.26",
          "form": "오옹ㅎㅎㅎ",
          "original_form": "오옹ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100022988.1.27",
          "form": "추억돋겟네요~^^",
          "original_form": "추억돋겟네요~^^",
          "speaker_id": "1",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100022988.1.28",
          "form": "누구랑요?",
          "original_form": "누구랑요?",
          "speaker_id": "1",
          "time": "20210829 21:22"
        },
        {
          "id": "MDRW2100022988.1.29",
          "form": "혼자도가고",
          "original_form": "혼자도가고",
          "speaker_id": "2",
          "time": "20210829 21:23"
        },
        {
          "id": "MDRW2100022988.1.30",
          "form": "여자애들 우르르모여서가고",
          "original_form": "여자애들 우르르모여서가고",
          "speaker_id": "2",
          "time": "20210829 21:24"
        },
        {
          "id": "MDRW2100022988.1.31",
          "form": "오오ㅎ 그당시 남자친구는요ㅡ?",
          "original_form": "오오ㅎ 그당시 남자친구는요ㅡ?",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100022988.1.32",
          "form": "남자친구랑가면 재밌었을것같은데ㅎ",
          "original_form": "남자친구랑가면 재밌었을것같은데ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:25"
        },
        {
          "id": "MDRW2100022988.1.33",
          "form": "아하하",
          "original_form": "아하하",
          "speaker_id": "2",
          "time": "20210829 21:26"
        },
        {
          "id": "MDRW2100022988.1.34",
          "form": "그당시 꿈이 야구장같이 다닐 남자친구를 사귀는거였는데ㅜㅜㅜ",
          "original_form": "그당시 꿈이 야구장같이 다닐 남자친구를 사귀는거였는데ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 21:27"
        },
        {
          "id": "MDRW2100022988.1.35",
          "form": "아하하하",
          "original_form": "아하하하",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100022988.1.36",
          "form": "추억이ㅠ 갑자기 슬퍼지네요ㅠ",
          "original_form": "추억이ㅠ 갑자기 슬퍼지네요ㅠ",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100022988.1.37",
          "form": "그때 있었으면 좋았을텐데ㅠ",
          "original_form": "그때 있었으면 좋았을텐데ㅠ",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100022988.1.38",
          "form": "저도 야구장 데이트 하고싶네요ㅠㅍ",
          "original_form": "저도 야구장 데이트 하고싶네요ㅠㅍ",
          "speaker_id": "1",
          "time": "20210829 21:28"
        },
        {
          "id": "MDRW2100022988.1.39",
          "form": "그냥 야구만 열심히보고 ㅋㅋ 이루지못한 꿈이었죠",
          "original_form": "그냥 야구만 열심히보고 ㅋㅋ 이루지못한 꿈이었죠",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100022988.1.40",
          "form": "그때 감독이 선동열이었고 한참 양준혁 은퇴전 안타 2천개 달성 응원하고 그랬는데 말이죠 ㅋㅋ",
          "original_form": "그때 감독이 선동열이었고 한참 양준혁 은퇴전 안타 2천개 달성 응원하고 그랬는데 말이죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 21:35"
        },
        {
          "id": "MDRW2100022988.1.41",
          "form": "오오ㅎㅎ 생각나네여ㅎ",
          "original_form": "오오ㅎㅎ 생각나네여ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100022988.1.42",
          "form": "양준혁진짜 잘했는데요ㅎ",
          "original_form": "양준혁진짜 잘했는데요ㅎ",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100022988.1.43",
          "form": "야구만 열씸히 보시면 어뜩해요ㅠ",
          "original_form": "야구만 열씸히 보시면 어뜩해요ㅠ",
          "speaker_id": "1",
          "time": "20210829 21:36"
        },
        {
          "id": "MDRW2100022988.1.44",
          "form": "연애도 열씸히하셔야하는데",
          "original_form": "연애도 열씸히하셔야하는데",
