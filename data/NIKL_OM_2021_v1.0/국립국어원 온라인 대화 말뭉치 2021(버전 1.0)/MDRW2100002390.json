      "utterance": [
        {
          "id": "MDRW2100002390.1.1",
          "form": "너는 동물 키운적 있어?",
          "original_form": "너는 동물 키운적 있어?",
          "speaker_id": "1",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002390.1.2",
          "form": "응 나 예전에 고양이 키운적 있어",
          "original_form": "응 나 예전에 고양이 키운적 있어",
          "speaker_id": "2",
          "time": "20210820 11:02"
        },
        {
          "id": "MDRW2100002390.1.3",
          "form": "헐 부럽다",
          "original_form": "헐 부럽다",
          "speaker_id": "1",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002390.1.4",
          "form": "나도 고양이 키우고 싶어",
          "original_form": "나도 고양이 키우고 싶어",
          "speaker_id": "1",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002390.1.5",
          "form": "막상 고양이 키울때 흉터남고 그러던데 아프더라",
          "original_form": "막상 고양이 키울때 흉터남고 그러던데 아프더라",
          "speaker_id": "2",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002390.1.6",
          "form": "에공 힘든가보네",
          "original_form": "에공 힘든가보네",
          "speaker_id": "1",
          "time": "20210820 11:03"
        },
        {
          "id": "MDRW2100002390.1.7",
          "form": "고양이가 햘퀴어서 그런가?",
          "original_form": "고양이가 햘퀴어서 그런가?",
          "speaker_id": "1",
          "time": "20210820 11:04"
        },
        {
          "id": "MDRW2100002390.1.8",
          "form": "응 다리에 흉터남았어",
          "original_form": "응 다리에 흉터남았어",
          "speaker_id": "2",
          "time": "20210820 11:04"
        },
        {
          "id": "MDRW2100002390.1.9",
          "form": "ㅠㅠ 동물이란",
          "original_form": "ㅠㅠ 동물이란",
          "speaker_id": "1",
          "time": "20210820 11:04"
        },
        {
          "id": "MDRW2100002390.1.10",
          "form": "이런게 참 안좋네",
          "original_form": "이런게 참 안좋네",
          "speaker_id": "1",
          "time": "20210820 11:04"
        },
        {
          "id": "MDRW2100002390.1.11",
          "form": "그래도 귀여우니까 키우는거잖아",
          "original_form": "그래도 귀여우니까 키우는거잖아",
          "speaker_id": "2",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002390.1.12",
          "form": "하긴 그래",
          "original_form": "하긴 그래",
          "speaker_id": "1",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002390.1.13",
          "form": "고양이 키우면 정서적 안정이 있을것 같아",
          "original_form": "고양이 키우면 정서적 안정이 있을것 같아",
          "speaker_id": "1",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002390.1.14",
          "form": "나 우울할때 옆에 와서 위로해주던데",
          "original_form": "나 우울할때 옆에 와서 위로해주던데",
          "speaker_id": "2",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002390.1.15",
          "form": "진짜 귀엽더라",
          "original_form": "진짜 귀엽더라",
          "speaker_id": "2",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002390.1.16",
          "form": "우와 그거 좋네",
          "original_form": "우와 그거 좋네",
          "speaker_id": "1",
          "time": "20210820 11:05"
        },
        {
          "id": "MDRW2100002390.1.17",
          "form": "근데 평상시엔 사고뭉치야",
          "original_form": "근데 평상시엔 사고뭉치야",
          "speaker_id": "2",
          "time": "20210820 11:06"
        },
        {
          "id": "MDRW2100002390.1.18",
          "form": "고통 그자체",
          "original_form": "고통 그자체",
          "speaker_id": "2",
          "time": "20210820 11:06"
        },
        {
          "id": "MDRW2100002390.1.19",
          "form": "ㅋㅋ 다 부수고 다니려나",
          "original_form": "ㅋㅋ 다 부수고 다니려나",
          "speaker_id": "1",
          "time": "20210820 11:06"
        }
      ]
