      "utterance": [
        {
          "id": "MDRW2100003742.1.1",
          "form": "남편분이랑 어떻게 만나신거예요?",
          "original_form": "남편분이랑 어떻게 만나신거예요?",
          "speaker_id": "2",
          "time": "20210821 11:43"
        },
        {
          "id": "MDRW2100003742.1.2",
          "form": "소개받으셨어요?",
          "original_form": "소개받으셨어요?",
          "speaker_id": "2",
          "time": "20210821 11:43"
        },
        {
          "id": "MDRW2100003742.1.3",
          "form": "학교에서 만났었어요 도서관에서 만났었는데",
          "original_form": "학교에서 만났었어요 도서관에서 만났었는데",
          "speaker_id": "1",
          "time": "20210821 11:44"
        },
        {
          "id": "MDRW2100003742.1.4",
          "form": "아 자연스럽게 만나신거구나",
          "original_form": "아 자연스럽게 만나신거구나",
          "speaker_id": "2",
          "time": "20210821 11:44"
        },
        {
          "id": "MDRW2100003742.1.5",
          "form": "네~소개는 아니었어요",
          "original_form": "네~소개는 아니었어요",
          "speaker_id": "1",
          "time": "20210821 11:45"
        },
        {
          "id": "MDRW2100003742.1.6",
          "form": "학교에서 만나신거면 연애 오래하고 결혼하셨겠네요?",
          "original_form": "학교에서 만나신거면 연애 오래하고 결혼하셨겠네요?",
          "speaker_id": "2",
          "time": "20210821 11:45"
        },
        {
          "id": "MDRW2100003742.1.7",
          "form": "네~~6년이요!!",
          "original_form": "네~~6년이요!!",
          "speaker_id": "1",
          "time": "20210821 11:45"
        },
        {
          "id": "MDRW2100003742.1.8",
          "form": "6년+5년이면 벌써 11년인거죠?",
          "original_form": "6년+5년이면 벌써 11년인거죠?",
          "speaker_id": "2",
          "time": "20210821 11:45"
        },
        {
          "id": "MDRW2100003742.1.9",
          "form": "네~ㅠ 6년이나 한결같아서 이런 사람인줄 알았어요ㅠ",
          "original_form": "네~ㅠ 6년이나 한결같아서 이런 사람인줄 알았어요ㅠ",
          "speaker_id": "1",
          "time": "20210821 11:46"
        },
        {
          "id": "MDRW2100003742.1.10",
          "form": "연애6년동안 잘하다가 어떻게 결혼하고 바뀌지ㅜㄷㆍ",
          "original_form": "연애6년동안 잘하다가 어떻게 결혼하고 바뀌지ㅜㄷㆍ",
          "speaker_id": "2",
          "time": "20210821 11:46"
        },
        {
          "id": "MDRW2100003742.1.11",
          "form": "ㅜㅜ...",
          "original_form": "ㅜㅜ...",
          "speaker_id": "2",
          "time": "20210821 11:46"
        },
        {
          "id": "MDRW2100003742.1.12",
          "form": "근데 주변에 언니들도 다 그런 얘기 하는거같아요",
          "original_form": "근데 주변에 언니들도 다 그런 얘기 하는거같아요",
          "speaker_id": "2",
          "time": "20210821 11:46"
        },
        {
          "id": "MDRW2100003742.1.13",
          "form": "연애때랑 사람이 달라진다고",
          "original_form": "연애때랑 사람이 달라진다고",
          "speaker_id": "2",
          "time": "20210821 11:46"
        },
        {
          "id": "MDRW2100003742.1.14",
          "form": "근데 지금 생각해보면요",
          "original_form": "근데 지금 생각해보면요",
          "speaker_id": "1",
          "time": "20210821 11:46"
        },
        {
          "id": "MDRW2100003742.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 11:46"
        },
        {
          "id": "MDRW2100003742.1.16",
          "form": "결혼전에 남편네 가족을 만났던적이 있었는데",
          "original_form": "결혼전에 남편네 가족을 만났던적이 있었는데",
          "speaker_id": "1",
          "time": "20210821 11:47"
        },
        {
          "id": "MDRW2100003742.1.17",
          "form": "가족한테 좀 무례하게 행동을 하더라고요",
          "original_form": "가족한테 좀 무례하게 행동을 하더라고요",
          "speaker_id": "1",
          "time": "20210821 11:47"
        },
        {
          "id": "MDRW2100003742.1.18",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210821 11:47"
        },
        {
          "id": "MDRW2100003742.1.19",
          "form": "그게 본모습이었나보다",
          "original_form": "그게 본모습이었나보다",
          "speaker_id": "2",
          "time": "20210821 11:47"
        },
        {
          "id": "MDRW2100003742.1.20",
          "form": "결혼 전에는 가족한테는 저래도 저한테는 잘하니까",
          "original_form": "결혼 전에는 가족한테는 저래도 저한테는 잘하니까",
          "speaker_id": "1",
          "time": "20210821 11:48"
        },
        {
          "id": "MDRW2100003742.1.21",
          "form": "결혼하면 저한테만 잘하겠지 생각한건데",
          "original_form": "결혼하면 저한테만 잘하겠지 생각한건데",
          "speaker_id": "1",
          "time": "20210821 11:48"
        },
        {
          "id": "MDRW2100003742.1.22",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210821 11:48"
        },
        {
          "id": "MDRW2100003742.1.23",
          "form": "똑같나요?",
          "original_form": "똑같나요?",
          "speaker_id": "2",
          "time": "20210821 11:48"
        },
        {
          "id": "MDRW2100003742.1.24",
          "form": "가족되니?",
          "original_form": "가족되니?",
          "speaker_id": "2",
          "time": "20210821 11:48"
        },
        {
          "id": "MDRW2100003742.1.25",
          "form": "결혼하니 가족한테 하던걸 저한테 그대로 하는거에요!!",
          "original_form": "결혼하니 가족한테 하던걸 저한테 그대로 하는거에요!!",
          "speaker_id": "1",
          "time": "20210821 11:48"
        },
        {
          "id": "MDRW2100003742.1.26",
          "form": "헐!!!!!",
          "original_form": "헐!!!!!",
          "speaker_id": "2",
          "time": "20210821 11:48"
        },
        {
          "id": "MDRW2100003742.1.27",
          "form": "대박이네",
          "original_form": "대박이네",
          "speaker_id": "2",
          "time": "20210821 11:48"
        },
        {
          "id": "MDRW2100003742.1.28",
          "form": "이제는 제가 가족인거죠ㅠ",
          "original_form": "이제는 제가 가족인거죠ㅠ",
          "speaker_id": "1",
          "time": "20210821 11:49"
        },
        {
          "id": "MDRW2100003742.1.29",
          "form": "결혼이 뭐라고 6년을 다정한척 했을까요...",
          "original_form": "결혼이 뭐라고 6년을 다정한척 했을까요...",
          "speaker_id": "2",
          "time": "20210821 11:49"
        }
      ]
