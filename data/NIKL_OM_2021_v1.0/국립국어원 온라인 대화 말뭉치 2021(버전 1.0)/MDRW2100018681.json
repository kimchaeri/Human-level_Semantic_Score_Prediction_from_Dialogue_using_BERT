      "utterance": [
        {
          "id": "MDRW2100018681.1.1",
          "form": "비오나유?",
          "original_form": "비오나유?",
          "speaker_id": "1",
          "time": "20210827 10:38"
        },
        {
          "id": "MDRW2100018681.1.2",
          "form": "네 비옵니다",
          "original_form": "네 비옵니다",
          "speaker_id": "2",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100018681.1.3",
          "form": "갑자기\n쏟아지네요",
          "original_form": "갑자기\n쏟아지네요",
          "speaker_id": "2",
          "time": "20210827 10:43"
        },
        {
          "id": "MDRW2100018681.1.4",
          "form": "저희지역은 아직... \n충청이남에 비구름 엄청 많네요",
          "original_form": "저희지역은 아직... \n충청이남에 비구름 엄청 많네요",
          "speaker_id": "1",
          "time": "20210827 10:44"
        },
        {
          "id": "MDRW2100018681.1.5",
          "form": "충청도 이신가요??",
          "original_form": "충청도 이신가요??",
          "speaker_id": "2",
          "time": "20210827 10:44"
        },
        {
          "id": "MDRW2100018681.1.6",
          "form": "전 대구예요^^",
          "original_form": "전 대구예요^^",
          "speaker_id": "2",
          "time": "20210827 10:44"
        },
        {
          "id": "MDRW2100018681.1.7",
          "form": "주말에 비 소식이 없었는데 날씨가 또 바꼈네요",
          "original_form": "주말에 비 소식이 없었는데 날씨가 또 바꼈네요",
          "speaker_id": "2",
          "time": "20210827 10:44"
        },
        {
          "id": "MDRW2100018681.1.8",
          "form": "주구장창 비네요... \n장마때보다 에어컨 더 틀고있네요..",
          "original_form": "주구장창 비네요... \n장마때보다 에어컨 더 틀고있네요..",
          "speaker_id": "1",
          "time": "20210827 10:46"
        },
        {
          "id": "MDRW2100018681.1.9",
          "form": "네 너무 습하네요",
          "original_form": "네 너무 습하네요",
          "speaker_id": "2",
          "time": "20210827 10:51"
        },
        {
          "id": "MDRW2100018681.1.10",
          "form": "저도 에어컨 제습기다 틀고 있어요",
          "original_form": "저도 에어컨 제습기다 틀고 있어요",
          "speaker_id": "2",
          "time": "20210827 10:52"
        },
        {
          "id": "MDRW2100018681.1.11",
          "form": "대구는 더 습하겟네여?",
          "original_form": "대구는 더 습하겟네여?",
          "speaker_id": "1",
          "time": "20210827 10:52"
        },
        {
          "id": "MDRW2100018681.1.12",
          "form": "네 덥고 습하고 그렇습니다^^;;",
          "original_form": "네 덥고 습하고 그렇습니다^^;;",
          "speaker_id": "2",
          "time": "20210827 10:56"
        },
        {
          "id": "MDRW2100018681.1.13",
          "form": "일기예보가 비 안옴으로 바껴서 캠핑을 잡아놨는데",
          "original_form": "일기예보가 비 안옴으로 바껴서 캠핑을 잡아놨는데",
          "speaker_id": "2",
          "time": "20210827 10:56"
        },
        {
          "id": "MDRW2100018681.1.14",
          "form": "비가 와서 슬프네요 ㅠ",
          "original_form": "비가 와서 슬프네요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 10:56"
        },
        {
          "id": "MDRW2100018681.1.15",
          "form": "아..",
          "original_form": "아..",
          "speaker_id": "1",
          "time": "20210827 10:56"
        },
        {
          "id": "MDRW2100018681.1.16",
          "form": "비오면 캠핑어렺죠?",
          "original_form": "비오면 캠핑어렺죠?",
          "speaker_id": "1",
          "time": "20210827 10:57"
        },
        {
          "id": "MDRW2100018681.1.17",
          "form": "텐트가 젖으면 또 말려야되서 ㅠ \n비 안오길래 다시 예약잡은건데...",
          "original_form": "텐트가 젖으면 또 말려야되서 ㅠ \n비 안오길래 다시 예약잡은건데...",
          "speaker_id": "2",
          "time": "20210827 11:08"
        },
        {
          "id": "MDRW2100018681.1.18",
          "form": "일기예보에 당했어요 ㅠ",
          "original_form": "일기예보에 당했어요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 11:08"
        },
        {
          "id": "MDRW2100018681.1.19",
          "form": "아.. 뒷정리는 생각도 못해봤네요... \n가을이 시즌 피크인가요?",
          "original_form": "아.. 뒷정리는 생각도 못해봤네요... \n가을이 시즌 피크인가요?",
          "speaker_id": "1",
          "time": "20210827 11:09"
        },
        {
          "id": "MDRW2100018681.1.20",
          "form": "네 봄가을이 피크인거 같아요",
          "original_form": "네 봄가을이 피크인거 같아요",
          "speaker_id": "2",
          "time": "20210827 11:16"
        },
        {
          "id": "MDRW2100018681.1.21",
          "form": "전 추워서 동계 캠핑을 안하구요",
          "original_form": "전 추워서 동계 캠핑을 안하구요",
          "speaker_id": "2",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018681.1.22",
          "form": "겨울에는 난로를 피워야되는데 좀 위험해보여서요",
          "original_form": "겨울에는 난로를 피워야되는데 좀 위험해보여서요",
          "speaker_id": "2",
          "time": "20210827 11:17"
        },
        {
          "id": "MDRW2100018681.1.23",
          "form": "야외활동은 날씨 영향이크네요",
          "original_form": "야외활동은 날씨 영향이크네요",
          "speaker_id": "1",
          "time": "20210827 11:18"
        }
      ]
    }
