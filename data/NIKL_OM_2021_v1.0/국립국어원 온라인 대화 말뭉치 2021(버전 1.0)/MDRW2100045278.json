      "utterance": [
        {
          "id": "MDRW2100045278.1.1",
          "form": "네 든든하게 먹고싶어서요 ㅋㅋㅋㅋㅋ",
          "original_form": "네 든든하게 먹고싶어서요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:36"
        },
        {
          "id": "MDRW2100045278.1.2",
          "form": "만두 좋아하세요?",
          "original_form": "만두 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 01:37"
        },
        {
          "id": "MDRW2100045278.1.3",
          "form": "네 만두 좋아해요 님은요?",
          "original_form": "네 만두 좋아해요 님은요?",
          "speaker_id": "2",
          "time": "20210908 01:38"
        },
        {
          "id": "MDRW2100045278.1.4",
          "form": "저도 만두 좋아해요",
          "original_form": "저도 만두 좋아해요",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045278.1.5",
          "form": "고기 김치 뭘 좋아하세요?",
          "original_form": "고기 김치 뭘 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 01:40"
        },
        {
          "id": "MDRW2100045278.1.6",
          "form": "저는 고기요! 님은 어떤걸 좋아해요?",
          "original_form": "저는 고기요! 님은 어떤걸 좋아해요?",
          "speaker_id": "2",
          "time": "20210908 01:41"
        },
        {
          "id": "MDRW2100045278.1.7",
          "form": "저도 고기를 좀더 좋아해요 ㅋㅋㅋㅋ",
          "original_form": "저도 고기를 좀더 좋아해요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:42"
        },
        {
          "id": "MDRW2100045278.1.8",
          "form": "역시 기본이 최고에요",
          "original_form": "역시 기본이 최고에요",
          "speaker_id": "2",
          "time": "20210908 01:45"
        },
        {
          "id": "MDRW2100045278.1.9",
          "form": "냉동만두 어디꺼 드세요?",
          "original_form": "냉동만두 어디꺼 드세요?",
          "speaker_id": "1",
          "time": "20210908 01:46"
        },
        {
          "id": "MDRW2100045278.1.10",
          "form": "비비고요! 근데 제동생은 고향만두가 맛있대요",
          "original_form": "비비고요! 근데 제동생은 고향만두가 맛있대요",
          "speaker_id": "2",
          "time": "20210908 01:48"
        },
        {
          "id": "MDRW2100045278.1.11",
          "form": "저도 비비고 좋아해요",
          "original_form": "저도 비비고 좋아해요",
          "speaker_id": "1",
          "time": "20210908 01:50"
        },
        {
          "id": "MDRW2100045278.1.12",
          "form": "비비고가 만두로 매출 최고 찍는 이유가 있어요",
          "original_form": "비비고가 만두로 매출 최고 찍는 이유가 있어요",
          "speaker_id": "2",
          "time": "20210908 01:51"
        },
        {
          "id": "MDRW2100045278.1.13",
          "form": "고기만두에 청양고추가 들어잇어서 깔끔해요",
          "original_form": "고기만두에 청양고추가 들어잇어서 깔끔해요",
          "speaker_id": "1",
          "time": "20210908 01:52"
        },
        {
          "id": "MDRW2100045278.1.14",
          "form": "맞아요 딱 좋아요",
          "original_form": "맞아요 딱 좋아요",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045278.1.15",
          "form": "그리고 비비고는 다양해서 좋구요",
          "original_form": "그리고 비비고는 다양해서 좋구요",
          "speaker_id": "2",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045278.1.16",
          "form": "감자만두도 맛잇어요",
          "original_form": "감자만두도 맛잇어요",
          "speaker_id": "1",
          "time": "20210908 01:56"
        },
        {
          "id": "MDRW2100045278.1.17",
          "form": "비비고감자만두",
          "original_form": "비비고감자만두",
          "speaker_id": "1",
          "time": "20210908 01:56"
        }
      ]
