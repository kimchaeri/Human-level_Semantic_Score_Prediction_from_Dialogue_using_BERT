        {
          "id": "MDRW2100045600.1.2",
          "form": "넷플릭스 추전좀 해주라",
          "original_form": "넷플릭스 추전좀 해주라",
          "speaker_id": "1",
          "time": "20210908 04:02"
        },
        {
          "id": "MDRW2100045600.1.3",
          "form": "나 요즘 별나도 괜찮아 보는중",
          "original_form": "나 요즘 별나도 괜찮아 보는중",
          "speaker_id": "2",
          "time": "20210908 04:02"
        },
        {
          "id": "MDRW2100045600.1.4",
          "form": "오 그 드라마 유명하지",
          "original_form": "오 그 드라마 유명하지",
          "speaker_id": "1",
          "time": "20210908 04:02"
        },
        {
          "id": "MDRW2100045600.1.5",
          "form": "아 너도 봤어?",
          "original_form": "아 너도 봤어?",
          "speaker_id": "2",
          "time": "20210908 04:02"
        },
        {
          "id": "MDRW2100045600.1.6",
          "form": "ㅋㅋ아니 추천만 받고 안봤어",
          "original_form": "ㅋㅋ아니 추천만 받고 안봤어",
          "speaker_id": "1",
          "time": "20210908 04:02"
        },
        {
          "id": "MDRW2100045600.1.7",
          "form": "그거 주인공이 자폐증 앓고있는 얘기야",
          "original_form": "그거 주인공이 자폐증 앓고있는 얘기야",
          "speaker_id": "2",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100045600.1.8",
          "form": "아 그런 내용이구나",
          "original_form": "아 그런 내용이구나",
          "speaker_id": "1",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100045600.1.9",
          "form": "가족들이 되게 따뜻햬",
          "original_form": "가족들이 되게 따뜻햬",
          "speaker_id": "2",
          "time": "20210908 04:03"
        },
        {
          "id": "MDRW2100045600.1.10",
          "form": "어른들 얘기야?",
          "original_form": "어른들 얘기야?",
          "speaker_id": "1",
          "time": "20210908 04:04"
        },
        {
          "id": "MDRW2100045600.1.11",
          "form": "아 아니 고등학생부터 시작해",
          "original_form": "아 아니 고등학생부터 시작해",
          "speaker_id": "2",
          "time": "20210908 04:04"
        },
        {
          "id": "MDRW2100045600.1.12",
          "form": "시즌은 몇개야?",
          "original_form": "시즌은 몇개야?",
