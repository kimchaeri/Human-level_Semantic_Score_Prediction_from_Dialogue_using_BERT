      "utterance": [
        {
          "id": "MDRW2100038801.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100038801.1.2",
          "form": "손목은 약하시지만 손가락 힘은 센편인가봐요",
          "original_form": "손목은 약하시지만 손가락 힘은 센편인가봐요",
          "speaker_id": "2",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100038801.1.3",
          "form": "네 피아노를 오래 배워서인가",
          "original_form": "네 피아노를 오래 배워서인가",
          "speaker_id": "1",
          "time": "20210902 21:29"
        },
        {
          "id": "MDRW2100038801.1.4",
          "form": "그렇내요ㅋㅋㅋ",
          "original_form": "그렇내요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100038801.1.5",
          "form": "블투 키보드는 아무래도 컴퓨터 키보드에 비해서 약한거 같으니 조심하새요",
          "original_form": "블투 키보드는 아무래도 컴퓨터 키보드에 비해서 약한거 같으니 조심하새요",
          "speaker_id": "1",
          "time": "20210902 21:30"
        },
        {
          "id": "MDRW2100038801.1.6",
          "form": "네네 그럴게요!",
          "original_form": "네네 그럴게요!",
          "speaker_id": "2",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100038801.1.7",
          "form": "피아노 오래 배우셨어요?",
          "original_form": "피아노 오래 배우셨어요?",
          "speaker_id": "2",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100038801.1.8",
          "form": "그랬지만 안 한지 오래됐네요ㅎ",
          "original_form": "그랬지만 안 한지 오래됐네요ㅎ",
          "speaker_id": "1",
          "time": "20210902 21:31"
        },
        {
          "id": "MDRW2100038801.1.9",
          "form": "까먹었을듯햐요ㅎ",
          "original_form": "까먹었을듯햐요ㅎ",
          "speaker_id": "1",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100038801.1.10",
          "form": "언제 배우셨었나요?",
          "original_form": "언제 배우셨었나요?",
          "speaker_id": "2",
          "time": "20210902 21:32"
        },
        {
          "id": "MDRW2100038801.1.11",
          "form": "어... 5살때부턴가? 그래요 기억이 안나여ㅋㅋ",
          "original_form": "어... 5살때부턴가? 그래요 기억이 안나여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100038801.1.12",
          "form": "오 어릴때부터 배우셨네요",
          "original_form": "오 어릴때부터 배우셨네요",
          "speaker_id": "2",
          "time": "20210902 21:33"
        },
        {
          "id": "MDRW2100038801.1.13",
          "form": "전 초등학교때 배웠었어요",
          "original_form": "전 초등학교때 배웠었어요",
          "speaker_id": "2",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100038801.1.14",
          "form": "하지만 안 친지 오래돼서ㅎ",
          "original_form": "하지만 안 친지 오래돼서ㅎ",
          "speaker_id": "1",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100038801.1.15",
          "form": "오 글쿤요ㅎ",
          "original_form": "오 글쿤요ㅎ",
          "speaker_id": "1",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100038801.1.16",
          "form": "아직 피아노 즐기시나요?",
          "original_form": "아직 피아노 즐기시나요?",
          "speaker_id": "1",
          "time": "20210902 21:34"
        },
        {
          "id": "MDRW2100038801.1.17",
          "form": "저도 안 친지 오래되어서...",
          "original_form": "저도 안 친지 오래되어서...",
          "speaker_id": "2",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100038801.1.18",
          "form": "피아노 좋아는해요!",
          "original_form": "피아노 좋아는해요!",
          "speaker_id": "2",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100038801.1.19",
          "form": "그래서 사서 두고 다시 연주해보고 싶어요",
          "original_form": "그래서 사서 두고 다시 연주해보고 싶어요",
          "speaker_id": "2",
          "time": "20210902 21:35"
        },
        {
          "id": "MDRW2100038801.1.20",
          "form": "아 그러세요ㅎ 언젠가 다시 배워서 연주해보는거도 좋죠ㅎㅎ",
          "original_form": "아 그러세요ㅎ 언젠가 다시 배워서 연주해보는거도 좋죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 21:42"
        },
        {
          "id": "MDRW2100038801.1.21",
          "form": "그러고보니 저도 중고거래한적은 없어요ㅎ",
          "original_form": "그러고보니 저도 중고거래한적은 없어요ㅎ",
          "speaker_id": "1",
          "time": "20210902 21:43"
        },
        {
          "id": "MDRW2100038801.1.22",
          "form": "그럼 당근이나 중고나라 요런거 안 써보셨겠네요",
          "original_form": "그럼 당근이나 중고나라 요런거 안 써보셨겠네요",
          "speaker_id": "2",
          "time": "20210902 21:45"
        },
        {
          "id": "MDRW2100038801.1.23",
          "form": "네 저희 엄마가 낯선 사람이 오래 쓰던 중고 물건을 안 좋아하셔서 저까지 괜히 겁나는게 있어요ㅎ 어차피 미신이겠지만ㅎ",
          "original_form": "네 저희 엄마가 낯선 사람이 오래 쓰던 중고 물건을 안 좋아하셔서 저까지 괜히 겁나는게 있어요ㅎ 어차피 미신이겠지만ㅎ",
          "speaker_id": "1",
          "time": "20210902 21:51"
        },
        {
          "id": "MDRW2100038801.1.24",
          "form": "아 그러시군요  낯선사람이 쓰던 물건 조심스러운 느낌 알거같아요",
          "original_form": "아 그러시군요  낯선사람이 쓰던 물건 조심스러운 느낌 알거같아요",
          "speaker_id": "2",
          "time": "20210902 21:52"
        }
      ]
    }
