      "utterance": [
        {
          "id": "MDRW2100019085.1.1",
          "form": "요즘 슬의생 보는데 보시나요?",
          "original_form": "요즘 슬의생 보는데 보시나요?",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100019085.1.2",
          "form": "안봤어요",
          "original_form": "안봤어요",
          "speaker_id": "1",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019085.1.3",
          "form": "인기많은거같던데 ㅎㅎ",
          "original_form": "인기많은거같던데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 15:58"
        },
        {
          "id": "MDRW2100019085.1.4",
          "form": "저도 궁금해서 보고싶은데 시즌이더라구요",
          "original_form": "저도 궁금해서 보고싶은데 시즌이더라구요",
          "speaker_id": "2",
          "time": "20210827 18:07"
        },
        {
          "id": "MDRW2100019085.1.5",
          "form": "드라마 안본지오래됐어요",
          "original_form": "드라마 안본지오래됐어요",
          "speaker_id": "1",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019085.1.6",
          "form": "주로 미드랑 영화봐요",
          "original_form": "주로 미드랑 영화봐요",
          "speaker_id": "1",
          "time": "20210827 18:25"
        },
        {
          "id": "MDRW2100019085.1.7",
          "form": "오 미드 좋아하시나보네요",
          "original_form": "오 미드 좋아하시나보네요",
          "speaker_id": "2",
          "time": "20210827 18:30"
        },
        {
          "id": "MDRW2100019085.1.8",
          "form": "좋아해요ㅋㅋ",
          "original_form": "좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100019085.1.9",
          "form": "영화덕후이기도해요",
          "original_form": "영화덕후이기도해요",
          "speaker_id": "1",
          "time": "20210827 18:33"
        },
        {
          "id": "MDRW2100019085.1.10",
          "form": "영화 추천해주세요!",
          "original_form": "영화 추천해주세요!",
          "speaker_id": "2",
          "time": "20210827 18:34"
        },
        {
          "id": "MDRW2100019085.1.11",
          "form": "최근에본 모가디슈!!",
          "original_form": "최근에본 모가디슈!!",
          "speaker_id": "1",
          "time": "20210827 19:17"
        },
        {
          "id": "MDRW2100019085.1.12",
          "form": "재밌나요?",
          "original_form": "재밌나요?",
          "speaker_id": "2",
          "time": "20210827 19:39"
        },
        {
          "id": "MDRW2100019085.1.13",
          "form": "네 아프간터지기전에봣는데",
          "original_form": "네 아프간터지기전에봣는데",
          "speaker_id": "1",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100019085.1.14",
          "form": "ㅠㅠ그때나지금이나",
          "original_form": "ㅠㅠ그때나지금이나",
          "speaker_id": "1",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100019085.1.15",
          "form": "변한게없네요",
          "original_form": "변한게없네요",
          "speaker_id": "1",
          "time": "20210827 20:18"
        },
        {
          "id": "MDRW2100019085.1.16",
          "form": "강추해요",
          "original_form": "강추해요",
          "speaker_id": "1",
          "time": "20210827 20:19"
        },
        {
          "id": "MDRW2100019085.1.17",
          "form": "오오 보고싶네요",
          "original_form": "오오 보고싶네요",
          "speaker_id": "2",
          "time": "20210827 20:21"
        },
        {
          "id": "MDRW2100019085.1.18",
          "form": "평이 나뉘어서 어떤가했는데 강추라니 더 궁금합니다",
          "original_form": "평이 나뉘어서 어떤가했는데 강추라니 더 궁금합니다",
          "speaker_id": "2",
          "time": "20210827 20:22"
        },
        {
          "id": "MDRW2100019085.1.19",
          "form": "아프간 생각하면서 꼭 보시길 강추!",
          "original_form": "아프간 생각하면서 꼭 보시길 강추!",
          "speaker_id": "1",
          "time": "20210827 20:53"
        },
        {
          "id": "MDRW2100019085.1.20",
          "form": "유툽 주로 뭐 보세용?",
          "original_form": "유툽 주로 뭐 보세용?",
          "speaker_id": "1",
          "time": "20210827 20:54"
        },
        {
          "id": "MDRW2100019085.1.21",
          "form": "요즘엔 운동영상 보는거 같아요",
          "original_form": "요즘엔 운동영상 보는거 같아요",
          "speaker_id": "2",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019085.1.22",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100019085.1.23",
          "form": "저는 유툽은잘안봐요 ㅎㅎ",
          "original_form": "저는 유툽은잘안봐요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100019085.1.24",
          "form": "운동영상ㅜ",
          "original_form": "운동영상ㅜ",
          "speaker_id": "1",
          "time": "20210827 21:19"
        },
        {
          "id": "MDRW2100019085.1.25",
