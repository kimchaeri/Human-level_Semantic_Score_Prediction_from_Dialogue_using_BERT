      "utterance": [
        {
          "id": "MDRW2100007130.1.1",
          "form": "반려동물 있으세요?",
          "original_form": "반려동물 있으세요?",
          "speaker_id": "1",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007130.1.2",
          "form": "저는 없어요!",
          "original_form": "저는 없어요!",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007130.1.3",
          "form": "혹시 키우시나요?",
          "original_form": "혹시 키우시나요?",
          "speaker_id": "2",
          "time": "20210822 00:35"
        },
        {
          "id": "MDRW2100007130.1.4",
          "form": "동물 얘기 듣는 거 좋아해요!",
          "original_form": "동물 얘기 듣는 거 좋아해요!",
          "speaker_id": "2",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007130.1.5",
          "form": "저는 새를 키워요",
          "original_form": "저는 새를 키워요",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007130.1.6",
          "form": "앵무새!",
          "original_form": "앵무새!",
          "speaker_id": "1",
          "time": "20210822 00:36"
        },
        {
          "id": "MDRW2100007130.1.7",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "2",
          "time": "20210822 00:37"
        },
        {
          "id": "MDRW2100007130.1.8",
          "form": "앵무새 키우는 거 어렵지 않으신가요??",
          "original_form": "앵무새 키우는 거 어렵지 않으신가요??",
          "speaker_id": "2",
          "time": "20210822 00:38"
        },
        {
          "id": "MDRW2100007130.1.9",
          "form": "건강관리만 잘해주면 괜찮은거 같아요!",
          "original_form": "건강관리만 잘해주면 괜찮은거 같아요!",
          "speaker_id": "1",
          "time": "20210822 00:39"
        },
        {
          "id": "MDRW2100007130.1.10",
          "form": "새병원도 잘 없구 병원비가 엄청 비싸거든요!",
          "original_form": "새병원도 잘 없구 병원비가 엄청 비싸거든요!",
          "speaker_id": "1",
          "time": "20210822 00:40"
        },
        {
          "id": "MDRW2100007130.1.11",
          "form": "아 그러겠네요ㅠㅠ 아프지 말아야하는데!",
          "original_form": "아 그러겠네요ㅠㅠ 아프지 말아야하는데!",
          "speaker_id": "2",
          "time": "20210822 00:43"
        },
        {
          "id": "MDRW2100007130.1.12",
          "form": "앵무새가 말도 하나요? 되게 똑똑하다던데!",
          "original_form": "앵무새가 말도 하나요? 되게 똑똑하다던데!",
          "speaker_id": "2",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007130.1.13",
          "form": "ㅋㅋㅋㅋㅋ 저희집 애는 자기 이름이랑 엄마 아빠 정도 한답니다",
          "original_form": "ㅋㅋㅋㅋㅋ 저희집 애는 자기 이름이랑 엄마 아빠 정도 한답니다",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007130.1.14",
          "form": "하고 싶을 때만 해요 ㅋㅋ",
          "original_form": "하고 싶을 때만 해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:44"
        },
        {
          "id": "MDRW2100007130.1.15",
          "form": "그래도 너무 똑똑한데요??",
          "original_form": "그래도 너무 똑똑한데요??",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007130.1.16",
          "form": "귀여울 거 같아요!",
          "original_form": "귀여울 거 같아요!",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007130.1.17",
          "form": "크기가 얼마정도 될까요?",
          "original_form": "크기가 얼마정도 될까요?",
          "speaker_id": "2",
          "time": "20210822 00:47"
        },
        {
          "id": "MDRW2100007130.1.18",
          "form": "대형앵무새라 치와와? 정도 인거 같아요 ㅋㅋ",
          "original_form": "대형앵무새라 치와와? 정도 인거 같아요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007130.1.19",
          "form": "무서워하는 분들도 계시더라구요",
          "original_form": "무서워하는 분들도 계시더라구요",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007130.1.20",
          "form": "그래도 전 너무 예뻐요",
          "original_form": "그래도 전 너무 예뻐요",
          "speaker_id": "1",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007130.1.21",
          "form": "생각했던 거보다 크네요!",
          "original_form": "생각했던 거보다 크네요!",
          "speaker_id": "2",
          "time": "20210822 00:48"
        },
        {
          "id": "MDRW2100007130.1.22",
          "form": "그쵸 보통 자기 동생이나 자식처럼 생각하는데",
          "original_form": "그쵸 보통 자기 동생이나 자식처럼 생각하는데",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007130.1.23",
          "form": "얼마나 예쁘겠어요!",
          "original_form": "얼마나 예쁘겠어요!",
          "speaker_id": "2",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007130.1.24",
          "form": "ㅋㅋㅋㅋ 좋아하는 동물 있으신가요?",
          "original_form": "ㅋㅋㅋㅋ 좋아하는 동물 있으신가요?",
          "speaker_id": "1",
          "time": "20210822 00:49"
        },
        {
          "id": "MDRW2100007130.1.25",
          "form": "저 고양이도 좋아해요! 주변에 집사님들이 많아서",
          "original_form": "저 고양이도 좋아해요! 주변에 집사님들이 많아서",
          "speaker_id": "2",
          "time": "20210822 00:50"
        },
        {
          "id": "MDRW2100007130.1.26",
          "form": "고양이! 놀러가면 낮 안가리나요?",
          "original_form": "고양이! 놀러가면 낮 안가리나요?",
          "speaker_id": "1",
          "time": "20210822 00:52"
        },
        {
          "id": "MDRW2100007130.1.27",
          "form": "보통 겁내기는 해요 그래서 멀리서 지켜보기만 하는데",
          "original_form": "보통 겁내기는 해요 그래서 멀리서 지켜보기만 하는데",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007130.1.28",
          "form": "그래도 자주 보면 좀 익숙해지더라구요",
          "original_form": "그래도 자주 보면 좀 익숙해지더라구요",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007130.1.29",
          "form": "고양이 키우는 친구들은 다 옷에 고양이털 범벅되구",
          "original_form": "고양이 키우는 친구들은 다 옷에 고양이털 범벅되구",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007130.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 00:53"
        },
        {
          "id": "MDRW2100007130.1.31",
          "form": "기억하는구나~ 귀여워 ㅠ",
          "original_form": "기억하는구나~ 귀여워 ㅠ",
          "speaker_id": "1",
          "time": "20210822 00:55"
        },
        {
          "id": "MDRW2100007130.1.32",
          "form": "돌돌이 필수죠 ㅋㅋㅋ",
          "original_form": "돌돌이 필수죠 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 00:55"
        }
      ]
