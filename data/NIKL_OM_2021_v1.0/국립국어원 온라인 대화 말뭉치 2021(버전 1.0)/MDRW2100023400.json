      "utterance": [
        {
          "id": "MDRW2100023400.1.1",
          "form": "주무셔요ㅎ",
          "original_form": "주무셔요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:30"
        },
        {
          "id": "MDRW2100023400.1.2",
          "form": "내일출근하시려면 피곤하시겟다ㅎ",
          "original_form": "내일출근하시려면 피곤하시겟다ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:30"
        },
        {
          "id": "MDRW2100023400.1.3",
          "form": "아직요 2시에 자려규요",
          "original_form": "아직요 2시에 자려규요",
          "speaker_id": "1",
          "time": "20210830 01:30"
        },
        {
          "id": "MDRW2100023400.1.4",
          "form": "그럼 수다를떨어봐요ㅎㅎ",
          "original_form": "그럼 수다를떨어봐요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:31"
        },
        {
          "id": "MDRW2100023400.1.5",
          "form": "너무 제가 말이많죠?",
          "original_form": "너무 제가 말이많죠?",
          "speaker_id": "2",
          "time": "20210830 01:31"
        },
        {
          "id": "MDRW2100023400.1.6",
          "form": "근데 뭐 이시간엔 교구 만들면서 해서 괜찮아요",
          "original_form": "근데 뭐 이시간엔 교구 만들면서 해서 괜찮아요",
          "speaker_id": "1",
          "time": "20210830 01:31"
        },
        {
          "id": "MDRW2100023400.1.7",
          "form": "키보드워리어ㅎㅎ",
          "original_form": "키보드워리어ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:31"
        },
        {
          "id": "MDRW2100023400.1.8",
          "form": "아뇨아눂 ㅎㅎ",
          "original_form": "아뇨아눂 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:31"
        },
        {
          "id": "MDRW2100023400.1.9",
          "form": "ㅎㅎㅎㅎ 교구요?ㅎ",
          "original_form": "ㅎㅎㅎㅎ 교구요?ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:31"
        },
        {
          "id": "MDRW2100023400.1.10",
          "form": "애기준비물인가요?",
          "original_form": "애기준비물인가요?",
          "speaker_id": "2",
          "time": "20210830 01:31"
        },
        {
          "id": "MDRW2100023400.1.11",
          "form": "네네 애기 교육 제가  왠만함 해서",
          "original_form": "네네 애기 교육 제가  왠만함 해서",
          "speaker_id": "1",
          "time": "20210830 01:31"
        },
        {
          "id": "MDRW2100023400.1.12",
          "form": "우왐ㅎ",
          "original_form": "우왐ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100023400.1.13",
          "form": "아뇨 놀이 하루에 한가지씩은 하거든요",
          "original_form": "아뇨 놀이 하루에 한가지씩은 하거든요",
          "speaker_id": "1",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100023400.1.14",
          "form": "대단하시다정말ㅎ",
          "original_form": "대단하시다정말ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100023400.1.15",
          "form": "시간이 없다는게 맞네요ㅎ",
          "original_form": "시간이 없다는게 맞네요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100023400.1.16",
          "form": "저희 아내는...",
          "original_form": "저희 아내는...",
          "speaker_id": "2",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100023400.1.17",
          "form": "구레서 이시간이 좀 소즁해요 ㅎㅎ",
          "original_form": "구레서 이시간이 좀 소즁해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100023400.1.18",
          "form": "아내욕을 많이하네요ㅎㅎ",
          "original_form": "아내욕을 많이하네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100023400.1.19",
          "form": "그럼 교구만드셔요ㅎ",
          "original_form": "그럼 교구만드셔요ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100023400.1.20",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100023400.1.21",
          "form": "저때문에 시간뺏기시네요ㅎㅎ",
          "original_form": "저때문에 시간뺏기시네요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100023400.1.22",
          "form": "아녀 만들면서 햇요 ㅎㅎ",
          "original_form": "아녀 만들면서 햇요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:32"
        },
        {
          "id": "MDRW2100023400.1.23",
          "form": "밑그림.정도 그리는거라",
          "original_form": "밑그림.정도 그리는거라",
          "speaker_id": "1",
          "time": "20210830 01:33"
        },
        {
          "id": "MDRW2100023400.1.24",
          "form": "책일고 미술활동 많이 하거든요 ㅎㅎ",
          "original_form": "책일고 미술활동 많이 하거든요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210830 01:33"
        },
        {
          "id": "MDRW2100023400.1.25",
          "form": "정말 존경합니다ㅎ",
          "original_form": "정말 존경합니다ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:33"
        },
        {
          "id": "MDRW2100023400.1.26",
          "form": "전 아직도 딸하고 어떻게 놀아야할지 몰라서ㅠ",
          "original_form": "전 아직도 딸하고 어떻게 놀아야할지 몰라서ㅠ",
          "speaker_id": "2",
          "time": "20210830 01:34"
        },
        {
          "id": "MDRW2100023400.1.27",
          "form": "ㅎㅎㅎ 그만 존경하세여 ㅋㅋ",
          "original_form": "ㅎㅎㅎ 그만 존경하세여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:34"
        },
        {
          "id": "MDRW2100023400.1.28",
          "form": "음 4살이죠?",
          "original_form": "음 4살이죠?",
          "speaker_id": "1",
          "time": "20210830 01:34"
        },
        {
          "id": "MDRW2100023400.1.29",
          "form": "에이 제가못하는거 있으니깐ㅎ",
          "original_form": "에이 제가못하는거 있으니깐ㅎ",
          "speaker_id": "2",
          "time": "20210830 01:34"
        },
        {
          "id": "MDRW2100023400.1.30",
          "form": "잘하시니깐 존경하는거죠 ㅡㅎㄹ",
          "original_form": "잘하시니깐 존경하는거죠 ㅡㅎㄹ",
          "speaker_id": "2",
          "time": "20210830 01:34"
        }
      ]
    }
