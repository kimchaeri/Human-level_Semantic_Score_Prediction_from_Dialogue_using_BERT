      "utterance": [
        {
          "id": "MDRW2100019512.1.1",
          "form": "저 로즈마리 먹으려고",
          "original_form": "저 로즈마리 먹으려고",
          "speaker_id": "1",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100019512.1.2",
          "form": "키우다가 죽였답니다",
          "original_form": "키우다가 죽였답니다",
          "speaker_id": "1",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100019512.1.3",
          "form": "맞아요 .. 쓸 줄 모를 땐 몹쓸 맛이네, 하고 버렸는데",
          "original_form": "맞아요 .. 쓸 줄 모를 땐 몹쓸 맛이네, 하고 버렸는데",
          "speaker_id": "2",
          "time": "20210827 19:58"
        },
        {
          "id": "MDRW2100019512.1.4",
          "form": "앗 로즈마리 ㅜ",
          "original_form": "앗 로즈마리 ㅜ",
          "speaker_id": "2",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019512.1.5",
          "form": "쓸 줄 알게 되셨군요",
          "original_form": "쓸 줄 알게 되셨군요",
          "speaker_id": "1",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019512.1.6",
          "form": "허브가 키우기 쉬울 것 같은데",
          "original_form": "허브가 키우기 쉬울 것 같은데",
          "speaker_id": "2",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019512.1.7",
          "form": "재료를 활용할 줄 알게 되면 진짜 좋은거같아요",
          "original_form": "재료를 활용할 줄 알게 되면 진짜 좋은거같아요",
          "speaker_id": "1",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019512.1.8",
          "form": "은근 귀찮더라고요",
          "original_form": "은근 귀찮더라고요",
          "speaker_id": "2",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019512.1.9",
          "form": "레시피 안보고 그때그때 활용하기",
          "original_form": "레시피 안보고 그때그때 활용하기",
          "speaker_id": "1",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019512.1.10",
          "form": "맞아요 로테이션으로 돌려막기하기 좋아요 ㅋㅋ",
          "original_form": "맞아요 로테이션으로 돌려막기하기 좋아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019512.1.11",
          "form": "맞아요 ㅋㅋㅋㅋ 허브 다 죽였어요",
          "original_form": "맞아요 ㅋㅋㅋㅋ 허브 다 죽였어요",
          "speaker_id": "1",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019512.1.12",
          "form": "로테이션ㅋㅋㅋㅋ",
          "original_form": "로테이션ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 19:59"
        },
        {
          "id": "MDRW2100019512.1.13",
          "form": "양식 로테이션이군요",
          "original_form": "양식 로테이션이군요",
          "speaker_id": "1",
          "time": "20210827 20:00"
        },
        {
          "id": "MDRW2100019512.1.14",
          "form": "그",
          "original_form": "그",
          "speaker_id": "1",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019512.1.15",
          "form": "아메리칸셰프에 나오는 파슬리파스타 아시나요",
          "original_form": "아메리칸셰프에 나오는 파슬리파스타 아시나요",
          "speaker_id": "1",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019512.1.16",
          "form": "그거 보려고 리스트에 있는데",
          "original_form": "그거 보려고 리스트에 있는데",
          "speaker_id": "2",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019512.1.17",
          "form": "아직 몰라요",
          "original_form": "아직 몰라요",
          "speaker_id": "2",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019512.1.18",
          "form": "ㅋㅋㅋㅋ저도 영화는 안보고 레시피만 아는데",
          "original_form": "ㅋㅋㅋㅋ저도 영화는 안보고 레시피만 아는데",
          "speaker_id": "1",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019512.1.19",
          "form": "남셰프들 화 내는 거 보면 꼴사나워서",
          "original_form": "남셰프들 화 내는 거 보면 꼴사나워서",
          "speaker_id": "2",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019512.1.20",
          "form": "맛 진짜 특이했어요",
          "original_form": "맛 진짜 특이했어요",
          "speaker_id": "1",
          "time": "20210827 20:01"
        },
        {
          "id": "MDRW2100019512.1.21",
          "form": "앗 ㅎㅎㅎ 저랑 동일한 마음이신",
          "original_form": "앗 ㅎㅎㅎ 저랑 동일한 마음이신",
          "speaker_id": "1",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019512.1.22",
          "form": "오오",
          "original_form": "오오",
          "speaker_id": "2",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019512.1.23",
          "form": "엑기스만 봐야겠어요",
          "original_form": "엑기스만 봐야겠어요",
          "speaker_id": "2",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019512.1.24",
          "form": "레몬이랑 파슬리가 주된 맛이어서",
          "original_form": "레몬이랑 파슬리가 주된 맛이어서",
          "speaker_id": "1",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019512.1.25",
          "form": "되게 상큼하고.. 신기했어요",
          "original_form": "되게 상큼하고.. 신기했어요",
          "speaker_id": "1",
          "time": "20210827 20:02"
        },
        {
          "id": "MDRW2100019512.1.26",
          "form": "밀라논나 채널 아시나요 그거 원래 패션 전문인데 호박꽃 부침 요리하는 회차만 자꾸 보고 있네요",
          "original_form": "밀라논나 채널 아시나요 그거 원래 패션 전문인데 호박꽃 부침 요리하는 회차만 자꾸 보고 있네요",
          "speaker_id": "2",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019512.1.27",
          "form": "저는 파슬리 정량만큼 안넣어서 원래 맛을 모르긴 하지만..",
          "original_form": "저는 파슬리 정량만큼 안넣어서 원래 맛을 모르긴 하지만..",
          "speaker_id": "1",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019512.1.28",
          "form": "헐ㅋㅋㅋㅋㅋ",
          "original_form": "헐ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019512.1.29",
          "form": "패션 채널인줄알고 안봤는데 호박꽃 부침을ㅋㅋㅋ",
          "original_form": "패션 채널인줄알고 안봤는데 호박꽃 부침을ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019512.1.30",
          "form": "와 맛이 신선하고 향긋할 것 같아요 꼭 해보겠어요",
          "original_form": "와 맛이 신선하고 향긋할 것 같아요 꼭 해보겠어요",
          "speaker_id": "2",
          "time": "20210827 20:03"
        }
