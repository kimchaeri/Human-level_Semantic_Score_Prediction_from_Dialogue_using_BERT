        },
        {
          "id": "MDRW2100031084.1.2",
          "form": "영화 관람 좋아하시나요",
          "original_form": "영화 관람 좋아하시나요",
          "speaker_id": "2",
          "time": "20210904 18:20"
        },
        {
          "id": "MDRW2100031084.1.3",
          "form": "매우좋아합니다",
          "original_form": "매우좋아합니다",
          "speaker_id": "1",
          "time": "20210904 21:29"
        },
        {
          "id": "MDRW2100031084.1.4",
          "form": "근데 코로나..ㅜ",
          "original_form": "근데 코로나..ㅜ",
          "speaker_id": "1",
          "time": "20210904 21:29"
        },
        {
          "id": "MDRW2100031084.1.5",
          "form": "저도 코로나...ㅠㅠ",
          "original_form": "저도 코로나...ㅠㅠ",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100031084.1.6",
          "form": "마지막으로 영화관에서 뭘봣는지도 기억 안나려고해요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "마지막으로 영화관에서 뭘봣는지도 기억 안나려고해요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:52"
        },
        {
          "id": "MDRW2100031084.1.7",
          "form": "음 전 극장에서 공포영화 보는걸 정말 좋아합니다",
          "original_form": "음 전 극장에서 공포영화 보는걸 정말 좋아합니다",
          "speaker_id": "1",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031084.1.8",
          "form": "그 스릴..",
          "original_form": "그 스릴..",
          "speaker_id": "1",
          "time": "20210904 23:12"
        },
        {
          "id": "MDRW2100031084.1.9",
          "form": "제가 절대 보지않는 장르인데 ㅋㅋㅋㅋㅋ",
          "original_form": "제가 절대 보지않는 장르인데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 23:20"
        },
        {
          "id": "MDRW2100031084.1.10",
          "form": "헐 그럼 애나벨 이런거 안봤어요?",
          "original_form": "헐 그럼 애나벨 이런거 안봤어요?",
          "speaker_id": "1",
          "time": "20210905 01:21"
        },
        {
          "id": "MDRW2100031084.1.11",
          "form": "대박.. 말도안돼",
          "original_form": "대박.. 말도안돼",
          "speaker_id": "1",
          "time": "20210905 01:21"
        },
        {
          "id": "MDRW2100031084.1.12",
          "form": "네 절대안봅니다 ㅋㅋㅋㅋㅋ",
          "original_form": "네 절대안봅니다 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 19:53"
        },
        {
          "id": "MDRW2100031084.1.13",
          "form": "주로 어떤 장르를 즐겨봐요",
          "original_form": "주로 어떤 장르를 즐겨봐요",
          "speaker_id": "1",
          "time": "20210905 20:14"
        },
        {
          "id": "MDRW2100031084.1.14",
          "form": "공포나 고어물 빼고는 대부분 봐요 ㅋㅋㅋㅋㅋ",
          "original_form": "공포나 고어물 빼고는 대부분 봐요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 20:28"
        },
        {
          "id": "MDRW2100031084.1.15",
          "form": "뭐니뭐니해도 마블이 최고인듯요",
          "original_form": "뭐니뭐니해도 마블이 최고인듯요",
          "speaker_id": "1",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100031084.1.16",
          "form": "어벤져스의 그 감동ㅜㅜ",
          "original_form": "어벤져스의 그 감동ㅜㅜ",
          "speaker_id": "1",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100031084.1.17",
          "form": "아 마블 좋죠",
          "original_form": "아 마블 좋죠",
          "speaker_id": "2",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100031084.1.18",
          "form": "무슨 캐릭터를 제일 좋아하세요?",
          "original_form": "무슨 캐릭터를 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100031084.1.19",
          "form": "뭐니뭐니해도 아이언맨 아닐까요",
          "original_form": "뭐니뭐니해도 아이언맨 아닐까요",
          "speaker_id": "1",
          "time": "20210905 21:30"
