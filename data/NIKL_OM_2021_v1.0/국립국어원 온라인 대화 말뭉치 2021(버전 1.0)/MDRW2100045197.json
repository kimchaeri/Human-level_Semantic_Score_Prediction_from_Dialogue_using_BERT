          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045197.1.14",
          "form": "글킨 해도",
          "original_form": "글킨 해도",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045197.1.15",
          "form": "전투하다보면 메인딜러 죽는 경우 꽤 잇으니까",
          "original_form": "전투하다보면 메인딜러 죽는 경우 꽤 잇으니까",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045197.1.16",
          "form": "그건 그래",
          "original_form": "그건 그래",
          "speaker_id": "2",
          "time": "20210908 01:16"
        },
        {
          "id": "MDRW2100045197.1.17",
          "form": "주인공이랑 베넷 둘다 80렙에서 등업하면",
