      "utterance": [
        {
          "id": "MDRW2100002815.1.1",
          "form": "언니는 강아지가 좋아?고양이가좋아?",
          "original_form": "언니는 강아지가 좋아?고양이가좋아?",
          "speaker_id": "2",
          "time": "20210820 15:45"
        },
        {
          "id": "MDRW2100002815.1.2",
          "form": "난 강아지가 좋아ㅎ",
          "original_form": "난 강아지가 좋아ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002815.1.3",
          "form": "나도 강아지가 조아 근데 키우기는 고양이랑 강아지 다 길러보고싶당",
          "original_form": "나도 강아지가 조아 근데 키우기는 고양이랑 강아지 다 길러보고싶당",
          "speaker_id": "1",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002815.1.4",
          "form": "너는 고양이 길러봤자나",
          "original_form": "너는 고양이 길러봤자나",
          "speaker_id": "1",
          "time": "20210820 15:46"
        },
        {
          "id": "MDRW2100002815.1.5",
          "form": "둘다 키워봣지ㅎ",
          "original_form": "둘다 키워봣지ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002815.1.6",
          "form": "그니깐 그때 니가 되게 부러웠음ㅜㅜ",
          "original_form": "그니깐 그때 니가 되게 부러웠음ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 15:47"
        },
        {
          "id": "MDRW2100002815.1.7",
          "form": "이제는 고양이 안키우지?",
          "original_form": "이제는 고양이 안키우지?",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002815.1.8",
          "form": "고양이는깔끔해서 좋은데 옷들은 따로 분리시켜놔야해 안그럼 옷에 털 장난아니게 묻음 강아지도그렇긴하지만",
          "original_form": "고양이는깔끔해서 좋은데 옷들은 따로 분리시켜놔야해 안그럼 옷에 털 장난아니게 묻음 강아지도그렇긴하지만",
          "speaker_id": "2",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002815.1.9",
          "form": "응응 지금은 아무것도 안키워",
          "original_form": "응응 지금은 아무것도 안키워",
          "speaker_id": "2",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002815.1.10",
          "form": "그니깐 위생이 중요하니깐",
          "original_form": "그니깐 위생이 중요하니깐",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002815.1.11",
          "form": "너 고양이 되게 좋아했자나",
          "original_form": "너 고양이 되게 좋아했자나",
          "speaker_id": "1",
          "time": "20210820 15:48"
        },
        {
          "id": "MDRW2100002815.1.12",
          "form": "응ㅎㅎㅎ",
          "original_form": "응ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002815.1.13",
          "form": "맞아ㅎ",
          "original_form": "맞아ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:51"
        },
        {
          "id": "MDRW2100002815.1.14",
          "form": "너네집 놀러갈때 고양이 되게 귀여웠는데",
          "original_form": "너네집 놀러갈때 고양이 되게 귀여웠는데",
          "speaker_id": "1",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002815.1.15",
          "form": "밀크랑초코",
          "original_form": "밀크랑초코",
          "speaker_id": "1",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002815.1.16",
          "form": "그치? 고양이 이름",
          "original_form": "그치? 고양이 이름",
          "speaker_id": "1",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002815.1.17",
          "form": "응응 맞아ㅎㅎ",
          "original_form": "응응 맞아ㅎㅎ",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002815.1.18",
          "form": "되게오래됏는데기억하네ㅎ",
          "original_form": "되게오래됏는데기억하네ㅎ",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002815.1.19",
          "form": "그때 니 자취했을때니깐 나도 그 고양이 보면 고양이이름불러줬었자나",
          "original_form": "그때 니 자취했을때니깐 나도 그 고양이 보면 고양이이름불러줬었자나",
          "speaker_id": "1",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002815.1.20",
          "form": "나중에 기회되면 고양이랑 강아지 두마리 키워보고싶다",
          "original_form": "나중에 기회되면 고양이랑 강아지 두마리 키워보고싶다",
          "speaker_id": "1",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002815.1.21",
          "form": "나도 강아지 키우고싶다ㅜ",
          "original_form": "나도 강아지 키우고싶다ㅜ",
          "speaker_id": "2",
          "time": "20210820 16:52"
        },
        {
          "id": "MDRW2100002815.1.22",
          "form": "나는 허스키도 키우고싶다ㅜㅜ",
          "original_form": "나는 허스키도 키우고싶다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210820 17:12"
        },
        {
          "id": "MDRW2100002815.1.23",
          "form": "난 포메ㅜ",
          "original_form": "난 포메ㅜ",
          "speaker_id": "2",
          "time": "20210820 17:28"
        },
        {
          "id": "MDRW2100002815.1.24",
          "form": "애완동물 골고루 다 키워보그 싶다",
          "original_form": "애완동물 골고루 다 키워보그 싶다",
