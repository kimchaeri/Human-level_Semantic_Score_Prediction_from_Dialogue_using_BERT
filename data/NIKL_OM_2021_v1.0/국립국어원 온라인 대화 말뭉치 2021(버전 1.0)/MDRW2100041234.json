      "utterance": [
        {
          "id": "MDRW2100041234.1.1",
          "form": "쇼핑좋아하세요?",
          "original_form": "쇼핑좋아하세요?",
          "speaker_id": "2",
          "time": "20210905 21:11"
        },
        {
          "id": "MDRW2100041234.1.2",
          "form": "아이쇼핑은 좋아해요",
          "original_form": "아이쇼핑은 좋아해요",
          "speaker_id": "1",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100041234.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210905 21:12"
        },
        {
          "id": "MDRW2100041234.1.4",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100041234.1.5",
          "form": "요즘은 좀 못가요",
          "original_form": "요즘은 좀 못가요",
          "speaker_id": "2",
          "time": "20210905 21:13"
        },
        {
          "id": "MDRW2100041234.1.6",
          "form": "name2님은 오프라인쇼핑 좋아하시나요??",
          "original_form": "&name2&님은 오프라인쇼핑 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100041234.1.7",
          "form": "둘다좋아해요!!",
          "original_form": "둘다좋아해요!!",
          "speaker_id": "2",
          "time": "20210905 21:14"
        },
        {
          "id": "MDRW2100041234.1.8",
          "form": "그렇군요",
          "original_form": "그렇군요",
          "speaker_id": "1",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100041234.1.9",
          "form": "최근에 사신 물건 있으신가요?",
          "original_form": "최근에 사신 물건 있으신가요?",
          "speaker_id": "1",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100041234.1.10",
          "form": "어떤거많이사세요?",
          "original_form": "어떤거많이사세요?",
          "speaker_id": "2",
          "time": "20210905 21:15"
        },
        {
          "id": "MDRW2100041234.1.11",
          "form": "코코가서 옷사고 장보고",
          "original_form": "코코가서 옷사고 장보고",
          "speaker_id": "2",
          "time": "20210905 21:16"
        },
        {
          "id": "MDRW2100041234.1.12",
          "form": "저는 주로 전공책 많이사요",
          "original_form": "저는 주로 전공책 많이사요",
          "speaker_id": "1",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100041234.1.13",
          "form": "아님 냉동식품류요",
          "original_form": "아님 냉동식품류요",
          "speaker_id": "1",
          "time": "20210905 21:17"
        },
        {
          "id": "MDRW2100041234.1.14",
          "form": "책도사시는군요.멋져요",
          "original_form": "책도사시는군요.멋져요",
          "speaker_id": "2",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100041234.1.15",
          "form": "아니 그냥 교재라서 사는거예요ㅋㅋㅋ",
          "original_form": "아니 그냥 교재라서 사는거예요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 21:18"
        },
        {
          "id": "MDRW2100041234.1.16",
          "form": "학생?",
          "original_form": "학생?",
          "speaker_id": "2",
          "time": "20210905 21:19"
        },
        {
          "id": "MDRW2100041234.1.17",
          "form": "네ㅋㅋㅋ 최근에 구매하고 만족하신 제품이 있으신가요??",
          "original_form": "네ㅋㅋㅋ 최근에 구매하고 만족하신 제품이 있으신가요??",
          "speaker_id": "1",
          "time": "20210905 21:19"
        },
        {
          "id": "MDRW2100041234.1.18",
          "form": "학생이시면 책많이사겠네요",
          "original_form": "학생이시면 책많이사겠네요",
          "speaker_id": "2",
          "time": "20210905 21:19"
        },
        {
          "id": "MDRW2100041234.1.19",
          "form": "코코에서사면 거의다 만족",
          "original_form": "코코에서사면 거의다 만족",
          "speaker_id": "2",
          "time": "20210905 21:19"
        },
        {
          "id": "MDRW2100041234.1.20",
          "form": "네 요즘 교재값이 왜이리 비싼지ㅠㅠ",
          "original_form": "네 요즘 교재값이 왜이리 비싼지ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 21:20"
        },
        {
          "id": "MDRW2100041234.1.21",
          "form": "그런것같아요",
          "original_form": "그런것같아요",
          "speaker_id": "2",
          "time": "20210905 21:20"
        }
