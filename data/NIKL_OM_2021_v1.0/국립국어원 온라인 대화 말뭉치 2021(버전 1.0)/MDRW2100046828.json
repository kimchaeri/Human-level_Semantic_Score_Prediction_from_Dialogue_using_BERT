          "time": "20211104 20:56"
        },
        {
          "id": "MDRW2100046828.1.18",
          "form": "ㅋㅋㅋㅌㅋㅌㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅌㅋㅌㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211104 20:56"
        },
        {
