      "utterance": [
        {
          "id": "MDRW2100037722.1.1",
          "form": "안녕하세요 배민자주이용하세요?",
          "original_form": "안녕하세요 배민자주이용하세요?",
          "speaker_id": "1",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100037722.1.2",
          "form": "넵 ㅎㅎ name1님은요??",
          "original_form": "넵 ㅎㅎ &name1&님은요??",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037722.1.3",
          "form": "이용하다가 얼마전에 어플지웠어요",
          "original_form": "이용하다가 얼마전에 어플지웠어요",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037722.1.4",
          "form": "저도 최근에는 자제하고 있어요 ㅎㅎ",
          "original_form": "저도 최근에는 자제하고 있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037722.1.5",
          "form": "햄버거는 좋아하세요?",
          "original_form": "햄버거는 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037722.1.6",
          "form": "맥도날드같은거요",
          "original_form": "맥도날드같은거요",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037722.1.7",
          "form": "좋아해요 햄버거를 조아해서 ㅋㅋㅋ",
          "original_form": "좋아해요 햄버거를 조아해서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037722.1.8",
          "form": "베토디 조아합니다",
          "original_form": "베토디 조아합니다",
          "speaker_id": "2",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037722.1.9",
          "form": "아하 베토디 맛있죠",
          "original_form": "아하 베토디 맛있죠",
          "speaker_id": "1",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100037722.1.10",
          "form": "슈비버거 드시나요?",
          "original_form": "슈비버거 드시나요?",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100037722.1.11",
          "form": "네네 먹어봤어요 ㅎㅎ맛있더라구요",
          "original_form": "네네 먹어봤어요 ㅎㅎ맛있더라구요",
          "speaker_id": "2",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100037722.1.12",
          "form": "있는줄 몰랐는데 은근 맛있었어여",
          "original_form": "있는줄 몰랐는데 은근 맛있었어여",
          "speaker_id": "1",
          "time": "20210901 17:12"
        },
        {
          "id": "MDRW2100037722.1.13",
          "form": "맞아요 괜찮더라구요 ㅋㅋ 피자는 조아하시나요?",
          "original_form": "맞아요 괜찮더라구요 ㅋㅋ 피자는 조아하시나요?",
          "speaker_id": "2",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037722.1.14",
          "form": "네 파파존스 피자 좋아해요",
          "original_form": "네 파파존스 피자 좋아해요",
          "speaker_id": "1",
          "time": "20210901 17:14"
        },
        {
          "id": "MDRW2100037722.1.15",
          "form": "ㅋㅋㅋ 파파존스가 인기가 많은거같아요 ㅎㅎ 저는 포장만 되가지고 ㅠㅠ",
          "original_form": "ㅋㅋㅋ 파파존스가 인기가 많은거같아요 ㅎㅎ 저는 포장만 되가지고 ㅠㅠ",
