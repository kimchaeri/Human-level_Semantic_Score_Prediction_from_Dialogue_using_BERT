      "utterance": [
        {
          "id": "MDRW2100032573.1.1",
          "form": "저는막팩붙였어요",
          "original_form": "저는막팩붙였어요",
          "speaker_id": "1",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100032573.1.2",
          "form": "어떤팩이요??",
          "original_form": "어떤팩이요??",
          "speaker_id": "2",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100032573.1.3",
          "form": "마스크팩이요!",
          "original_form": "마스크팩이요!",
          "speaker_id": "1",
          "time": "20210905 20:47"
        },
        {
          "id": "MDRW2100032573.1.4",
          "form": "요새가을이라피부가안좋더라고요ㅜ",
          "original_form": "요새가을이라피부가안좋더라고요ㅜ",
          "speaker_id": "1",
          "time": "20210905 20:48"
        },
        {
          "id": "MDRW2100032573.1.5",
          "form": "그럼 수분채우는용으로 구매하신건가요?",
          "original_form": "그럼 수분채우는용으로 구매하신건가요?",
          "speaker_id": "2",
          "time": "20210905 20:50"
        },
        {
          "id": "MDRW2100032573.1.6",
          "form": "넵넵 제가지성이라 수분쪽이 잘 맞아요",
          "original_form": "넵넵 제가지성이라 수분쪽이 잘 맞아요",
          "speaker_id": "1",
          "time": "20210905 20:51"
        },
        {
          "id": "MDRW2100032573.1.7",
          "form": "요새운동하시나요?",
          "original_form": "요새운동하시나요?",
          "speaker_id": "1",
          "time": "20210905 20:51"
        },
        {
          "id": "MDRW2100032573.1.8",
          "form": "저는 운동 안해요ㅋㅋㅋ\r\n운동 하시나요?",
          "original_form": "저는 운동 안해요ㅋㅋㅋ\r\n운동 하시나요?",
          "speaker_id": "2",
          "time": "20210905 20:54"
        },
        {
          "id": "MDRW2100032573.1.9",
          "form": "음...너무하기싫지만플랭크정도해요",
          "original_form": "음...너무하기싫지만플랭크정도해요",
          "speaker_id": "1",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032573.1.10",
          "form": "살이잘붙는체질이라서요ㅠ",
          "original_form": "살이잘붙는체질이라서요ㅠ",
          "speaker_id": "1",
          "time": "20210905 20:56"
        },
        {
          "id": "MDRW2100032573.1.11",
          "form": "헐 저 플랭크 30초도 못 버티는데",
          "original_form": "헐 저 플랭크 30초도 못 버티는데",
          "speaker_id": "2",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100032573.1.12",
          "form": "플랭크 잘하시나요?",
          "original_form": "플랭크 잘하시나요?",
          "speaker_id": "2",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100032573.1.13",
          "form": "ㅎㅎ하다보니쪼꼼씩늘더라고요",
          "original_form": "ㅎㅎ하다보니쪼꼼씩늘더라고요",
          "speaker_id": "1",
          "time": "20210905 20:58"
        },
        {
          "id": "MDRW2100032573.1.14",
          "form": "산책도가끔하는데좋아하세요?",
          "original_form": "산책도가끔하는데좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 20:59"
        },
        {
          "id": "MDRW2100032573.1.15",
          "form": "아무생각없이 걷는거 좋아해요",
          "original_form": "아무생각없이 걷는거 좋아해요",
          "speaker_id": "2",
          "time": "20210905 21:00"
        },
        {
          "id": "MDRW2100032573.1.16",
          "form": "산책 좋아하시나요?",
          "original_form": "산책 좋아하시나요?",
