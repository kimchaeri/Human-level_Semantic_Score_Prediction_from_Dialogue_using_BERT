      "utterance": [
        {
          "id": "MDRW2100023700.1.1",
          "form": "거기 건물안에 돌아다니다가ㅋㅋㅋ",
          "original_form": "거기 건물안에 돌아다니다가ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023700.1.2",
          "form": "너였어?ㅋㅋㅋㅋ\r\n왜 name3 같지ㅋㅋ",
          "original_form": "너였어?ㅋㅋㅋㅋ\r\n왜 &name3& 같지ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023700.1.3",
          "form": "name3인 확실히 카스테라였어ㅋㅋㅋ",
          "original_form": "&name3&인 확실히 카스테라였어ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023700.1.4",
          "form": "마자 민폐였지",
          "original_form": "마자 민폐였지",
          "speaker_id": "1",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023700.1.5",
          "form": "아그래?ㅋㅋ",
          "original_form": "아그래?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:59"
        },
        {
          "id": "MDRW2100023700.1.6",
          "form": "응ㅋㅋ 나도 기억안났는데 이제 기억났다",
          "original_form": "응ㅋㅋ 나도 기억안났는데 이제 기억났다",
          "speaker_id": "2",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100023700.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ추억팔이 재밌지?ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ추억팔이 재밌지?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100023700.1.8",
          "form": "우리가 버블티를 사오다가 그렇게 된거야?ㅋㅋ",
          "original_form": "우리가 버블티를 사오다가 그렇게 된거야?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100023700.1.9",
          "form": "그르게ㅋㅋㅋ 영상이랑 사진좀 많이 찍을걸ㅠㅠ",
          "original_form": "그르게ㅋㅋㅋ 영상이랑 사진좀 많이 찍을걸ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100023700.1.10",
          "form": "그거까진 기억이 안나네ㅋㅋㅋ",
          "original_form": "그거까진 기억이 안나네ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100023700.1.11",
          "form": "마자ㅠㅠ 진짜 사진이랑 영상 남기는게 최고야 여행으뉴ㅠ",
          "original_form": "마자ㅠㅠ 진짜 사진이랑 영상 남기는게 최고야 여행으뉴ㅠ",
          "speaker_id": "1",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100023700.1.12",
          "form": "지금폰에 있어 사진?",
          "original_form": "지금폰에 있어 사진?",
          "speaker_id": "2",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100023700.1.13",
          "form": "있지! 당연히!",
          "original_form": "있지! 당연히!",
          "speaker_id": "1",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100023700.1.14",
          "form": "패션후르츠 아이스크림도 진짜 맛있었는데 못먹어봐서 아쉽다..ㅠㅠㅠ",
          "original_form": "패션후르츠 아이스크림도 진짜 맛있었는데 못먹어봐서 아쉽다..ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 15:00"
        },
        {
          "id": "MDRW2100023700.1.15",
          "form": "난 패션후르츠 안좋아해서 괜찮아ㅋㅋ",
          "original_form": "난 패션후르츠 안좋아해서 괜찮아ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 15:01"
        },
        {
          "id": "MDRW2100023700.1.16",
          "form": "아...ㅋㅋㅋ 그래 너 취향은 아닐거같긴햌ㅋㅋ",
          "original_form": "아...ㅋㅋㅋ 그래 너 취향은 아닐거같긴햌ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 15:01"
        },
        {
          "id": "MDRW2100023700.1.17",
          "form": "난 닭날개벆음밥이랑 찌파이ㅋㅋㅋ",
          "original_form": "난 닭날개벆음밥이랑 찌파이ㅋㅋㅋ",
