        },
        {
          "id": "MDRW2100043304.1.2",
          "form": "전문용어네요",
          "original_form": "전문용어네요",
          "speaker_id": "2",
          "time": "20210907 17:14"
        },
        {
          "id": "MDRW2100043304.1.3",
          "form": "아니아니 모델명ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니아니 모델명ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:14"
        },
        {
          "id": "MDRW2100043304.1.4",
          "form": "엄마 나시카라도 쥐여줄깤ㅋㅋㅋㅋㅋㅋ 근데 관심없으려나..",
          "original_form": "엄마 나시카라도 쥐여줄깤ㅋㅋㅋㅋㅋㅋ 근데 관심없으려나..",
          "speaker_id": "1",
          "time": "20210907 17:14"
        },
        {
          "id": "MDRW2100043304.1.5",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100043304.1.6",
          "form": "저는 바보입니다",
          "original_form": "저는 바보입니다",
          "speaker_id": "2",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100043304.1.7",
          "form": "대충 눈으로만 보거든요",
          "original_form": "대충 눈으로만 보거든요",
          "speaker_id": "2",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100043304.1.8",
          "form": "없는 것보단 나을 것 같아요",
          "original_form": "없는 것보단 나을 것 같아요",
          "speaker_id": "2",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100043304.1.9",
          "form": "엄마한테 빠순이들 문화 다 체험시켜주기",
          "original_form": "엄마한테 빠순이들 문화 다 체험시켜주기",
          "speaker_id": "1",
          "time": "20210907 17:18"
        },
        {
          "id": "MDRW2100043304.1.10",
          "form": "근데 이 호박넝쿨놈들 대체 스케줄표언제줌",
          "original_form": "근데 이 호박넝쿨놈들 대체 스케줄표언제줌",
          "speaker_id": "1",
          "time": "20210907 17:18"
        },
        {
          "id": "MDRW2100043304.1.11",
          "form": "호박넝쿵ㅋㅋㅋㅋㅋㅋㅋ스케줄표는 당일에 줘야 제맛이죠",
          "original_form": "호박넝쿵ㅋㅋㅋㅋㅋㅋㅋ스케줄표는 당일에 줘야 제맛이죠",
          "speaker_id": "2",
          "time": "20210907 17:35"
        },
        {
          "id": "MDRW2100043304.1.12",
          "form": "하.. 넘 화난다",
          "original_form": "하.. 넘 화난다",
          "speaker_id": "1",
          "time": "20210907 17:36"
        },
        {
          "id": "MDRW2100043304.1.13",
          "form": "ㅠㅠㅠㅠㅠ 일정봐야한다고요",
          "original_form": "ㅠㅠㅠㅠㅠ 일정봐야한다고요{emoji:•}{emoji:•}{emoji:•}{emoji:•}",
          "speaker_id": "1",
          "time": "20210907 17:37"
        },
        {
          "id": "MDRW2100043304.1.14",
          "form": "공연 말고 전시회는 관심 없으신가여",
          "original_form": "공연 말고 전시회는 관심 없으신가여",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100043304.1.15",
          "form": "전 작년 11월에 유미의 세포들 전시회 다녀왔는데",
          "original_form": "전 작년 11월에 유미의 세포들 전시회 다녀왔는데",
          "speaker_id": "2",
          "time": "20210907 17:39"
        },
        {
          "id": "MDRW2100043304.1.16",
          "form": "사람들 앛에서 포즈 취하고 사진찍어야 해서 아주 수치스럽고 재밌었어요",
          "original_form": "사람들 앛에서 포즈 취하고 사진찍어야 해서 아주 수치스럽고 재밌었어요",
          "speaker_id": "2",
          "time": "20210907 17:40"
        },
        {
          "id": "MDRW2100043304.1.17",
          "form": "관심있긴할텐뎈ㅋㅋㅋ 지방엔 전시회가 잘 없으니까ㅠㅠㅠ",
          "original_form": "관심있긴할텐뎈ㅋㅋㅋ 지방엔 전시회가 잘 없으니까ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100043304.1.18",
          "form": "아 그렇네 맞네",
          "original_form": "아 그렇네 맞네",
          "speaker_id": "2",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100043304.1.19",
          "form": "ㅅ서울 멀어요",
          "original_form": "ㅅ서울 멀어요",
          "speaker_id": "1",
          "time": "20210907 18:19"
        },
        {
          "id": "MDRW2100043304.1.20",
          "form": "청주 놀러오면 같이 현대미술관 가보자고",
          "original_form": "청주 놀러오면 같이 현대미술관 가보자고",
          "speaker_id": "2",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100043304.1.21",
          "form": "헉 좋지",
          "original_form": "헉 좋지",
          "speaker_id": "1",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100043304.1.22",
          "form": "한 번도 안 가봤어 ㅋㅋㅋㅋㅋ 지나가기만 함",
          "original_form": "한 번도 안 가봤어 ㅋㅋㅋㅋㅋ 지나가기만 함",
          "speaker_id": "2",
          "time": "20210907 18:20"
        },
        {
          "id": "MDRW2100043304.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋ나 한번씩보는거 좋아해 그런거",
          "original_form": "ㅋㅋㅋㅋㅋㅋ나 한번씩보는거 좋아해 그런거",
