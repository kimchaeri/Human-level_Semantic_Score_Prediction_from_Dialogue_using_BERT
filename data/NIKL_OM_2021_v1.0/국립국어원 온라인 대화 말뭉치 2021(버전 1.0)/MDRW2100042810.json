      "utterance": [
        {
          "id": "MDRW2100042810.1.1",
          "form": "저도 제주살이가 버킷리스트 중 하나였거든요",
          "original_form": "저도 제주살이가 버킷리스트 중 하나였거든요",
          "speaker_id": "2",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042810.1.2",
          "form": "멋있네요",
          "original_form": "멋있네요",
          "speaker_id": "2",
          "time": "20210907 09:26"
        },
        {
          "id": "MDRW2100042810.1.3",
          "form": "저는 과연 할 수 있을지...ㅋㅋㅋㅋ",
          "original_form": "저는 과연 할 수 있을지...ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042810.1.4",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042810.1.5",
          "form": "오면 다른 점도 많어요",
          "original_form": "오면 다른 점도 많어요",
          "speaker_id": "1",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042810.1.6",
          "form": "습하구",
          "original_form": "습하구",
          "speaker_id": "1",
          "time": "20210907 09:27"
        },
        {
          "id": "MDRW2100042810.1.7",
          "form": "그쵸 글고 날씨도 참 변덕이죠 ㅋㅋㅋ",
          "original_form": "그쵸 글고 날씨도 참 변덕이죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042810.1.8",
          "form": "혹시 제주 어느지역으로 정하셨는지 여쭤봐도 되나요?",
          "original_form": "혹시 제주 어느지역으로 정하셨는지 여쭤봐도 되나요?",
          "speaker_id": "2",
          "time": "20210907 09:34"
        },
        {
          "id": "MDRW2100042810.1.9",
          "form": "저는 제주시요",
          "original_form": "저는 제주시요",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042810.1.10",
          "form": "도심이예요",
          "original_form": "도심이예요",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042810.1.11",
          "form": "사라봉근처",
          "original_form": "사라봉근처",
          "speaker_id": "1",
          "time": "20210907 09:35"
        },
        {
          "id": "MDRW2100042810.1.12",
          "form": "아 도심! 그럼 뚜벅이로 가신건가요?",
          "original_form": "아 도심! 그럼 뚜벅이로 가신건가요?",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042810.1.13",
          "form": "제주시면 뚜벅이로 여행가면 필수죠 ㅋㅋㅌㅋ",
          "original_form": "제주시면 뚜벅이로 여행가면 필수죠 ㅋㅋㅌㅋ",
          "speaker_id": "2",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042810.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210907 09:36"
        },
        {
          "id": "MDRW2100042810.1.15",
          "form": "집앞에 버스정류장이구",
          "original_form": "집앞에 버스정류장이구",
          "speaker_id": "1",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042810.1.16",
          "form": "교통때문에 제주시로 결정하신거예요?",
          "original_form": "교통때문에 제주시로 결정하신거예요?",
          "speaker_id": "2",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042810.1.17",
          "form": "아뇨 전 도심이 좋아요",
          "original_form": "아뇨 전 도심이 좋아요",
          "speaker_id": "1",
          "time": "20210907 09:37"
        },
        {
          "id": "MDRW2100042810.1.18",
          "form": "오 그러시구나",
          "original_form": "오 그러시구나",
          "speaker_id": "2",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100042810.1.19",
          "form": "그쵸 제주시 서울저리가라죠 ㅋㅋㅋㅋ",
          "original_form": "그쵸 제주시 서울저리가라죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100042810.1.20",
          "form": "버스타면 오름 해변 다 가니까요",
          "original_form": "버스타면 오름 해변 다 가니까요",
          "speaker_id": "1",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100042810.1.21",
          "form": "혹시 우진해장국 아세요?",
          "original_form": "혹시 우진해장국 아세요?",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042810.1.22",
          "form": "제주시에 잇는 유명한 해장국집",
          "original_form": "제주시에 잇는 유명한 해장국집",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042810.1.23",
          "form": "저 거기 자꾸 생각나요...몸국최고   ....",
          "original_form": "저 거기 자꾸 생각나요...몸국최고   ....",
          "speaker_id": "2",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042810.1.24",
          "form": "아뇨 맛집이군요",
          "original_form": "아뇨 맛집이군요",
          "speaker_id": "1",
          "time": "20210907 09:40"
        },
        {
          "id": "MDRW2100042810.1.25",
          "form": "네네 외지인도 현지인도 많이 가는 곳인거 같더라구요",
          "original_form": "네네 외지인도 현지인도 많이 가는 곳인거 같더라구요",
          "speaker_id": "2",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042810.1.26",
          "form": "고사리해장국이 제일 유명한데 저는 몸국이 더 맛있더라구요",
          "original_form": "고사리해장국이 제일 유명한데 저는 몸국이 더 맛있더라구요",
          "speaker_id": "2",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042810.1.27",
          "form": "나중에 한번 드셔보세요!",
          "original_form": "나중에 한번 드셔보세요!",
          "speaker_id": "2",
          "time": "20210907 09:41"
        },
        {
          "id": "MDRW2100042810.1.28",
          "form": "네 감삼돠",
          "original_form": "네 감삼돠",
          "speaker_id": "1",
          "time": "20210907 09:42"
        },
        {
          "id": "MDRW2100042810.1.29",
          "form": "제주 흐린날이 많아서 해장국 땡겨요",
          "original_form": "제주 흐린날이 많아서 해장국 땡겨요",
          "speaker_id": "1",
          "time": "20210907 09:42"
        }
      ]
    }
