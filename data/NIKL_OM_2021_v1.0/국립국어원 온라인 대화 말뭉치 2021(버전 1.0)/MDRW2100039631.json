      "utterance": [
        {
          "id": "MDRW2100039631.1.1",
          "form": "요즘은.. 어떤 노래가 유행하는가",
          "original_form": "요즘은.. 어떤 노래가 유행하는가",
          "speaker_id": "1",
          "time": "20210903 20:20"
        },
        {
          "id": "MDRW2100039631.1.2",
          "form": "그런 것이야말로 트렌드가 아닐까요(1도모름",
          "original_form": "그런 것이야말로 트렌드가 아닐까요(1도모름",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039631.1.3",
          "form": "어 근데 저도 모릅니다...",
          "original_form": "어 근데 저도 모릅니다...",
          "speaker_id": "2",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039631.1.4",
          "form": "아시다시피 케이팝에서도 오타쿠라서...",
          "original_form": "아시다시피 케이팝에서도 오타쿠라서...",
          "speaker_id": "2",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039631.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 갓반인들은...무슨 노래 듣지?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 갓반인들은...무슨 노래 듣지?",
          "speaker_id": "2",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039631.1.6",
          "form": "악뮤.. 같은 거?",
          "original_form": "악뮤.. 같은 거?",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039631.1.7",
          "form": "아이유... 같은 거?",
          "original_form": "아이유... 같은 거?",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039631.1.8",
          "form": "아 맞다 악뮤 신곡 나왔죵",
          "original_form": "아 맞다 악뮤 신곡 나왔죵",
          "speaker_id": "2",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039631.1.9",
          "form": "마자 그거 피쳐링 아이유가 했죠",
          "original_form": "마자 그거 피쳐링 아이유가 했죠",
          "speaker_id": "2",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039631.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039631.1.11",
          "form": "물론 듣지 않았어",
          "original_form": "물론 듣지 않았어",
          "speaker_id": "1",
          "time": "20210903 20:21"
        },
        {
          "id": "MDRW2100039631.1.12",
          "form": "저는 아이유 박효신 악뮤 그리고 엔시티127에서",
          "original_form": "저는 아이유 박효신 악뮤 그리고 엔시티127에서",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039631.1.13",
          "form": "백날천날 어떻게 이별까지.......를 듣는 오타쿠라서",
          "original_form": "백날천날 어떻게 이별까지.......를 듣는 오타쿠라서",
          "speaker_id": "1",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039631.1.14",
          "form": "엔시티 127을 담당중...",
          "original_form": "엔시티 127을 담당중...",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039631.1.15",
          "form": "아 너무 좋죠 띵곡",
          "original_form": "아 너무 좋죠 띵곡",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039631.1.16",
          "form": "저도 그...찬혁이 제대하고 나서는 어쩐지",
          "original_form": "저도 그...찬혁이 제대하고 나서는 어쩐지",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039631.1.17",
          "form": "손이 안가서",
          "original_form": "손이 안가서",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039631.1.18",
          "form": "박효신보다 요즘 애들한테 익숙할 거야",
          "original_form": "박효신보다 요즘 애들한테 익숙할 거야",
          "speaker_id": "1",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039631.1.19",
          "form": "아마도..........",
          "original_form": "아마도..........",
          "speaker_id": "1",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039631.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ희망을...희망을...",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ희망을...희망을...",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039631.1.21",
          "form": "근ㄷ 요즘 애들은 다 bts자나욧",
          "original_form": "근ㄷ 요즘 애들은 다 bts자나욧",
          "speaker_id": "2",
          "time": "20210903 20:22"
        },
        {
          "id": "MDRW2100039631.1.22",
          "form": "거의 이견이 없던데...무섭드라구용",
          "original_form": "거의 이견이 없던데...무섭드라구용",
          "speaker_id": "2",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039631.1.23",
          "form": "근데 방탄 왜 영어노래내...",
          "original_form": "근데 방탄 왜 영어노래내...",
          "speaker_id": "2",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039631.1.24",
          "form": "그럴까요 다들 좋아한다는데",
          "original_form": "그럴까요 다들 좋아한다는데",
          "speaker_id": "1",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039631.1.25",
          "form": "그러고도 너네가 케이팝이야? 재섭서...",
          "original_form": "그러고도 너네가 케이팝이야? 재섭서...",
          "speaker_id": "2",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039631.1.26",
          "form": "내 주변에는 없는 그런 현상",
          "original_form": "내 주변에는 없는 그런 현상",
          "speaker_id": "1",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039631.1.27",
          "form": "노래를 하나도 몰라 어쩌면 들어보면 알지도 모르지만",
          "original_form": "노래를 하나도 몰라 어쩌면 들어보면 알지도 모르지만",
          "speaker_id": "1",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100039631.1.28",
          "form": "마자요... 제 주변두 별로 없다",
          "original_form": "마자요... 제 주변두 별로 없다",
          "speaker_id": "2",
          "time": "20210903 20:24"
        },
        {
          "id": "MDRW2100039631.1.29",
          "form": "아마 거리에서 많이 나오니 아실지도...",
          "original_form": "아마 거리에서 많이 나오니 아실지도...",
          "speaker_id": "2",
