      "utterance": [
        {
          "id": "MDRW2100023017.1.1",
          "form": "야식 드시나요 ㅎ",
          "original_form": "야식 드시나요 ㅎ",
          "speaker_id": "1",
          "time": "20210829 22:38"
        },
        {
          "id": "MDRW2100023017.1.2",
          "form": "아니요 ㅋㅋ  name1 야식드세요?",
          "original_form": "아니요 ㅋㅋ  &name1& 야식드세요?",
          "speaker_id": "2",
          "time": "20210829 22:39"
        },
        {
          "id": "MDRW2100023017.1.3",
          "form": "전 저녁 늦게먹고  애들 자는시간에 같이 잠든적이 많아서 ㅠㅠ",
          "original_form": "전 저녁 늦게먹고  애들 자는시간에 같이 잠든적이 많아서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:39"
        },
        {
          "id": "MDRW2100023017.1.4",
          "form": "넵 ㅋㅋㅋㅋ 지금 냉동 꿔바로우랑 닭강정에어 돌려서",
          "original_form": "넵 ㅋㅋㅋㅋ 지금 냉동 꿔바로우랑 닭강정에어 돌려서",
          "speaker_id": "1",
          "time": "20210829 22:39"
        },
        {
          "id": "MDRW2100023017.1.5",
          "form": "티비보며 맥주한잔해요",
          "original_form": "티비보며 맥주한잔해요",
          "speaker_id": "1",
          "time": "20210829 22:39"
        },
        {
          "id": "MDRW2100023017.1.6",
          "form": "우와~~",
          "original_form": "우와~~",
          "speaker_id": "2",
          "time": "20210829 22:39"
        },
        {
          "id": "MDRW2100023017.1.7",
          "form": "이러니ㅜㅜ살이찌죠ㅠㅠ",
          "original_form": "이러니ㅜㅜ살이찌죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:39"
        },
        {
          "id": "MDRW2100023017.1.8",
          "form": "엄청나게 부러운 시간이네요",
          "original_form": "엄청나게 부러운 시간이네요",
          "speaker_id": "2",
          "time": "20210829 22:39"
        },
        {
          "id": "MDRW2100023017.1.9",
          "form": "ㅎㅎㅎ 저는 엄청 말랐는데 체질이라서..",
          "original_form": "ㅎㅎㅎ 저는 엄청 말랐는데 체질이라서..",
          "speaker_id": "2",
          "time": "20210829 22:40"
        },
        {
          "id": "MDRW2100023017.1.10",
          "form": "그리하면 배만 계속 나오더라구요 흑흑",
          "original_form": "그리하면 배만 계속 나오더라구요 흑흑",
          "speaker_id": "2",
          "time": "20210829 22:40"
        },
        {
          "id": "MDRW2100023017.1.11",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:40"
        },
        {
          "id": "MDRW2100023017.1.12",
          "form": "어머 맨마지막위를 이제봣네요",
          "original_form": "어머 맨마지막위를 이제봣네요",
          "speaker_id": "1",
          "time": "20210829 22:40"
        },
        {
          "id": "MDRW2100023017.1.13",
          "form": "부럽네요ㅠㅠ 마름 ㅠㅠ",
          "original_form": "부럽네요ㅠㅠ 마름 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100023017.1.14",
          "form": "ㅋㅋ 부러운 예쁜 마름은 아니에요.....ㅠㅠ",
          "original_form": "ㅋㅋ 부러운 예쁜 마름은 아니에요.....ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100023017.1.15",
          "form": "경악할 마름이라 ㅋㅋ",
          "original_form": "경악할 마름이라 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100023017.1.16",
          "form": "ㅠㅠㅠㅠ 제 남친도 완전 마름체질인데",
          "original_form": "ㅠㅠㅠㅠ 제 남친도 완전 마름체질인데",
          "speaker_id": "1",
          "time": "20210829 22:43"
        },
        {
          "id": "MDRW2100023017.1.17",
          "form": "진짜 같이 먹는데 배가 1도안나와요",
          "original_form": "진짜 같이 먹는데 배가 1도안나와요",
          "speaker_id": "1",
          "time": "20210829 22:43"
        },
        {
          "id": "MDRW2100023017.1.18",
          "form": "ㅋㅋㅋ  배도안나오세요? 부럽네요",
          "original_form": "ㅋㅋㅋ  배도안나오세요? 부럽네요",
          "speaker_id": "2",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023017.1.19",
          "form": "보통 마르면 많이 먹는순간에는 볼록할텐데ㅜㅋㅋ",
          "original_form": "보통 마르면 많이 먹는순간에는 볼록할텐데ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023017.1.20",
          "form": "네ㅠㅠ 진짜 배부르다하는데",
          "original_form": "네ㅠㅠ 진짜 배부르다하는데",
          "speaker_id": "1",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023017.1.21",
          "form": "안그래요 그냥 단단하고 진짜 살짝 먹기전보다",
          "original_form": "안그래요 그냥 단단하고 진짜 살짝 먹기전보다",
          "speaker_id": "1",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023017.1.22",
          "form": "나왓다하는데",
          "original_form": "나왓다하는데",
          "speaker_id": "1",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023017.1.23",
          "form": "그래도 모양이 다보여요ㅠㅠ 샬없이 근육모양?인지",
          "original_form": "그래도 모양이 다보여요ㅠㅠ 샬없이 근육모양?인지",
          "speaker_id": "1",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023017.1.24",
          "form": "ㅋㅋㄱㅋ  체질은 어쩔수없가봐요",
          "original_form": "ㅋㅋㄱㅋ  체질은 어쩔수없가봐요",
          "speaker_id": "2",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023017.1.25",
          "form": "남친분은 아침드시나요",
          "original_form": "남친분은 아침드시나요",
          "speaker_id": "2",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023017.1.26",
          "form": "구니깐요ㅠㅠ 진짜 부러워요ㅠㅠ",
          "original_form": "구니깐요ㅠㅠ 진짜 부러워요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023017.1.27",
          "form": "출근전엔 안먹고",
          "original_form": "출근전엔 안먹고",
          "speaker_id": "1",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100023017.1.28",
          "form": "회사에서 점심먹고 집에와서 저녁먹고",
          "original_form": "회사에서 점심먹고 집에와서 저녁먹고",
          "speaker_id": "1",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100023017.1.29",
          "form": "맥주마시고 ㅋㅋㅋ",
          "original_form": "맥주마시고 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:51"
        },
        {
          "id": "MDRW2100023017.1.30",
          "form": "ㅎㅎ  보통 말랐다하면 저녁에 치킨먹으라는둥 라면먹으라는둥 그런말 많이듣거든요",
          "original_form": "ㅎㅎ  보통 말랐다하면 저녁에 치킨먹으라는둥 라면먹으라는둥 그런말 많이듣거든요",
          "speaker_id": "2",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023017.1.31",
          "form": "라면을 제일좋아해요ㅠ",
          "original_form": "라면을 제일좋아해요ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023017.1.32",
          "form": "체질적으로 마른사람들은  그래봤자 살찌지않고 그러면 더 안좋다하더라구요 ㅎㅎ",
          "original_form": "체질적으로 마른사람들은  그래봤자 살찌지않고 그러면 더 안좋다하더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210829 22:55"
        },
        {
          "id": "MDRW2100023017.1.33",
          "form": "전 오히려 더 빠지는 부작용잌ㅋㅋㅋ",
          "original_form": "전 오히려 더 빠지는 부작용잌ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100023017.1.34",
          "form": "육해공안가리고 다좋아하고 많이먹고...ㅡㅡ ㅋㅋㅋ",
          "original_form": "육해공안가리고 다좋아하고 많이먹고...ㅡㅡ ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100023017.1.35",
          "form": "근데 한식을 엄청 좋아해서",
          "original_form": "근데 한식을 엄청 좋아해서",
          "speaker_id": "2",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100023017.1.36",
          "form": "저두요 다 안가리지만 그중에 특히 한식을 너무사랑해서",
          "original_form": "저두요 다 안가리지만 그중에 특히 한식을 너무사랑해서",
          "speaker_id": "2",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100023017.1.37",
          "form": "한식맛잇죠ㅠ",
          "original_form": "한식맛잇죠ㅠ",
          "speaker_id": "1",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100023017.1.38",
          "form": "진짜 평생 단한번 한달간 한식폭식?하고 살쪄본경험이있네욯ㅋㅋ",
          "original_form": "진짜 평생 단한번 한달간 한식폭식?하고 살쪄본경험이있네욯ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100023017.1.39",
          "form": "저도 그냥 밥국 반찬 먹는게 제일이에요 !",
          "original_form": "저도 그냥 밥국 반찬 먹는게 제일이에요 !",
          "speaker_id": "1",
          "time": "20210829 22:56"
        },
        {
          "id": "MDRW2100023017.1.40",
          "form": "그쵸! 그게 건강식단인거같아요~",
          "original_form": "그쵸! 그게 건강식단인거같아요~",
          "speaker_id": "2",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023017.1.41",
          "form": "배달음식이랑 레토르트 식품은",
          "original_form": "배달음식이랑 레토르트 식품은",
          "speaker_id": "2",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023017.1.42",
          "form": "너무너무 짜요ㅠ",
          "original_form": "너무너무 짜요ㅠ",
          "speaker_id": "2",
          "time": "20210829 22:58"
        },
        {
          "id": "MDRW2100023017.1.43",
          "form": "입맛이 건강입맛가지고계시는군요!!",
          "original_form": "입맛이 건강입맛가지고계시는군요!!",
          "speaker_id": "1",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023017.1.44",
          "form": "음식도 잘하실꺼같아요",
          "original_form": "음식도 잘하실꺼같아요",
          "speaker_id": "1",
          "time": "20210829 22:59"
        },
        {
          "id": "MDRW2100023017.1.45",
          "form": "으허허 근데 전혀 아니에요ㅠㅠ",
          "original_form": "으허허 근데 전혀 아니에요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 23:15"
        },
        {
          "id": "MDRW2100023017.1.46",
          "form": "건강식 먹는것만 좋아합니다ㅜㅜ",
          "original_form": "건강식 먹는것만 좋아합니다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 23:15"
        },
        {
          "id": "MDRW2100023017.1.47",
          "form": "아이들한테도 좋울꺼같은데여 저염으로 드시니!!",
          "original_form": "아이들한테도 좋울꺼같은데여 저염으로 드시니!!",
