      "utterance": [
        {
          "id": "MDRW2100014380.1.1",
          "form": "넌 뭐먹을거야",
          "original_form": "넌 뭐먹을거야",
          "speaker_id": "1",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014380.1.2",
          "form": "나도 고민이야..",
          "original_form": "나도 고민이야..",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014380.1.3",
          "form": "꿔바로우에 마라탕 먹을까 하는데 배달비 많이들어서",
          "original_form": "꿔바로우에 마라탕 먹을까 하는데 배달비 많이들어서",
          "speaker_id": "2",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014380.1.4",
          "form": "아님 배떡ㅋㅋㅋ",
          "original_form": "아님 배떡ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014380.1.5",
          "form": "아니면 연어회 사서 먹을까  생각중이야",
          "original_form": "아니면 연어회 사서 먹을까  생각중이야",
          "speaker_id": "2",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014380.1.6",
          "form": "회얘기하니까 연어 먹고싶네ㅋㅋ",
          "original_form": "회얘기하니까 연어 먹고싶네ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014380.1.7",
          "form": "ㅋㅋㅋ 정작 내가 오늘 못먹게 생겼다ㅠㅠ",
          "original_form": "ㅋㅋㅋ 정작 내가 오늘 못먹게 생겼다ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 18:45"
        },
        {
          "id": "MDRW2100014380.1.8",
          "form": "저번에 꿔바로우 먹은게 맛있었구나?ㅋㅋ",
          "original_form": "저번에 꿔바로우 먹은게 맛있었구나?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 18:46"
        },
        {
          "id": "MDRW2100014380.1.9",
          "form": "응 짱 맛있었어",
          "original_form": "응 짱 맛있었어",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014380.1.10",
          "form": "거기 이름 기억해뒀다ㅋㅋㅋ",
          "original_form": "거기 이름 기억해뒀다ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014380.1.11",
          "form": "다른 횟집 찾아봐 \r\n또 다른 맛집을 찾을수도 있잖아",
          "original_form": "다른 횟집 찾아봐 \r\n또 다른 맛집을 찾을수도 있잖아",
          "speaker_id": "2",
          "time": "20210825 18:47"
        },
        {
          "id": "MDRW2100014380.1.12",
          "form": "오늘은 안전하고 행복하게 먹고싶었다구",
          "original_form": "오늘은 안전하고 행복하게 먹고싶었다구",
          "speaker_id": "1",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100014380.1.13",
          "form": "일단 물어봐야겠당",
          "original_form": "일단 물어봐야겠당",
          "speaker_id": "1",
          "time": "20210825 18:48"
        },
        {
          "id": "MDRW2100014380.1.14",
          "form": "아 그치ㅋㅋㅋ새로운곳은 위험하지ㅋㅋ",
          "original_form": "아 그치ㅋㅋㅋ새로운곳은 위험하지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:49"
        },
        {
          "id": "MDRW2100014380.1.15",
          "form": "기분좋게 먹었는데 맨날 마지막기억이 안나서 큰일이야..",
          "original_form": "기분좋게 먹었는데 맨날 마지막기억이 안나서 큰일이야..",
          "speaker_id": "1",
          "time": "20210826 08:26"
        },
        {
          "id": "MDRW2100014380.1.16",
          "form": "어제 회먹었어?",
          "original_form": "어제 회먹었어?",
          "speaker_id": "2",
          "time": "20210826 11:03"
        },
        {
          "id": "MDRW2100014380.1.17",
          "form": "응 다른곳ㅋㅋ",
          "original_form": "응 다른곳ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 11:40"
        },
        {
          "id": "MDRW2100014380.1.18",
          "form": "넌 뭐먹었어?",
          "original_form": "넌 뭐먹었어?",
          "speaker_id": "1",
          "time": "20210826 11:40"
        },
        {
          "id": "MDRW2100014380.1.19",
          "form": "나는 돈닭먹었어ㅋㅋ",
          "original_form": "나는 돈닭먹었어ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:05"
        },
        {
          "id": "MDRW2100014380.1.20",
          "form": "오늘은 감자전해먹어야지ㅋㅋ",
          "original_form": "오늘은 감자전해먹어야지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:06"
        },
        {
          "id": "MDRW2100014380.1.21",
          "form": "마지막기억이 안난다는게 무슨말이야?",
          "original_form": "마지막기억이 안난다는게 무슨말이야?",
          "speaker_id": "2",
          "time": "20210826 12:07"
        },
        {
          "id": "MDRW2100014380.1.22",
          "form": "ㅋㅋㅋ 감자조림은!",
          "original_form": "ㅋㅋㅋ 감자조림은!",
          "speaker_id": "1",
          "time": "20210826 13:23"
        },
        {
          "id": "MDRW2100014380.1.23",
          "form": "내가 어떻게 잠들었는지 기억이 안난다는 말이야ㅠㅠㅋㅋ",
          "original_form": "내가 어떻게 잠들었는지 기억이 안난다는 말이야ㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 13:24"
        },
        {
          "id": "MDRW2100014380.1.24",
          "form": "감자 있는거 다 갈아버려가지구 감자조림은 못해..ㅋㅋ",
          "original_form": "감자 있는거 다 갈아버려가지구 감자조림은 못해..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:25"
        },
        {
          "id": "MDRW2100014380.1.25",
          "form": "아~ㅋㅋㅋ또 먹다가 잠들었구나?ㅋㅋ",
          "original_form": "아~ㅋㅋㅋ또 먹다가 잠들었구나?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 13:25"
        },
        {
          "id": "MDRW2100014380.1.26",
          "form": "응ㅋㅋ 아 갈아놨어? 믹서기로 한거니?",
          "original_form": "응ㅋㅋ 아 갈아놨어? 믹서기로 한거니?",
          "speaker_id": "1",
          "time": "20210826 17:14"
        },
        {
          "id": "MDRW2100014380.1.27",
          "form": "응 어제 아예 갈아놨어ㅋㅋ\r\n응 믹서기로!",
          "original_form": "응 어제 아예 갈아놨어ㅋㅋ\r\n응 믹서기로!",
          "speaker_id": "2",
          "time": "20210826 18:02"
        }
      ]
    }
