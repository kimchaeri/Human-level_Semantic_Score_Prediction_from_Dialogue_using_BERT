      "utterance": [
        {
          "id": "MDRW2100022307.1.1",
          "form": "아 진짜요",
          "original_form": "아 진짜요",
          "speaker_id": "2",
          "time": "20210829 12:23"
        },
        {
          "id": "MDRW2100022307.1.2",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "1",
          "time": "20210829 12:23"
        },
        {
          "id": "MDRW2100022307.1.3",
          "form": "전 소개팅으로 연애가 처음이라",
          "original_form": "전 소개팅으로 연애가 처음이라",
          "speaker_id": "2",
          "time": "20210829 12:23"
        },
        {
          "id": "MDRW2100022307.1.4",
          "form": "낯설어요 ㅋㅋㅋ",
          "original_form": "낯설어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:23"
        },
        {
          "id": "MDRW2100022307.1.5",
          "form": "근데 좋지 않아요?",
          "original_form": "근데 좋지 않아요?",
          "speaker_id": "1",
          "time": "20210829 12:23"
        },
        {
          "id": "MDRW2100022307.1.6",
          "form": "이런느낌 언제 느껴봐요 ㅋㅋ",
          "original_form": "이런느낌 언제 느껴봐요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:23"
        },
        {
          "id": "MDRW2100022307.1.7",
          "form": "네 이것도 또다른 매력이 있네요",
          "original_form": "네 이것도 또다른 매력이 있네요",
          "speaker_id": "2",
          "time": "20210829 12:23"
        },
        {
          "id": "MDRW2100022307.1.8",
          "form": "여친이 손 처음 잡을때 긴장해서 손에 땀도 났었는데 ㅋㅋㅋ",
          "original_form": "여친이 손 처음 잡을때 긴장해서 손에 땀도 났었는데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100022307.1.9",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:24"
        },
        {
          "id": "MDRW2100022307.1.10",
          "form": "뽀뽀는 거절당하고 언제 시도했어요",
          "original_form": "뽀뽀는 거절당하고 언제 시도했어요",
          "speaker_id": "2",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022307.1.11",
          "form": "그담날?",
          "original_form": "그담날?",
          "speaker_id": "2",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022307.1.12",
          "form": "거절당하고 나서",
          "original_form": "거절당하고 나서",
          "speaker_id": "1",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022307.1.13",
          "form": "제가 좀 뾰루퉁했어가지고",
          "original_form": "제가 좀 뾰루퉁했어가지고",
          "speaker_id": "1",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022307.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022307.1.15",
          "form": "일주일뒤 주말에 만날때",
          "original_form": "일주일뒤 주말에 만날때",
          "speaker_id": "1",
          "time": "20210829 12:25"
        },
        {
          "id": "MDRW2100022307.1.16",
          "form": "이제 준비됐어? 하고 그냥",
          "original_form": "이제 준비됐어? 하고 그냥",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022307.1.17",
          "form": "와 박력있다!!!",
          "original_form": "와 박력있다!!!",
          "speaker_id": "2",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022307.1.18",
          "form": "해버렸어요",
          "original_form": "해버렸어요",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022307.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022307.1.20",
          "form": "뽀뽀하고 키스까지 3개월 ㅋㅋㅋㅋㅋ",
          "original_form": "뽀뽀하고 키스까지 3개월 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022307.1.21",
          "form": "이게 30대 연애",
          "original_form": "이게 30대 연애",
          "speaker_id": "1",
          "time": "20210829 12:26"
        },
        {
          "id": "MDRW2100022307.1.22",
          "form": "에엥 진짜요?",
          "original_form": "에엥 진짜요?",
          "speaker_id": "2",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022307.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022307.1.24",
          "form": "뽀뽀 그냥 볼뽀뽀였습니까?!",
          "original_form": "뽀뽀 그냥 볼뽀뽀였습니까?!",
          "speaker_id": "2",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022307.1.25",
          "form": "입술뽀뽀",
          "original_form": "입술뽀뽀",
          "speaker_id": "1",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022307.1.26",
          "form": "근데 여친이 키스는 시간이 필요하다고",
          "original_form": "근데 여친이 키스는 시간이 필요하다고",
          "speaker_id": "1",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022307.1.27",
          "form": "와 그런데 어떻게 또 그런 간격이!!",
          "original_form": "와 그런데 어떻게 또 그런 간격이!!",
          "speaker_id": "2",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022307.1.28",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022307.1.29",
          "form": "아 진짜요? ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 진짜요? ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:27"
        },
        {
          "id": "MDRW2100022307.1.30",
          "form": "첫 연애셨나요?",
          "original_form": "첫 연애셨나요?",
          "speaker_id": "2",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022307.1.31",
          "form": "자기는 키스보다 뽀뽀가 좋다고",
          "original_form": "자기는 키스보다 뽀뽀가 좋다고",
          "speaker_id": "1",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022307.1.32",
          "form": "여친은 제가 처음이래요",
          "original_form": "여친은 제가 처음이래요",
          "speaker_id": "1",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022307.1.33",
          "form": "아 그러면 그럴 수 있겠네요",
          "original_form": "아 그러면 그럴 수 있겠네요",
          "speaker_id": "2",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022307.1.34",
          "form": "(근데 사실아니에요 제가 주선자한테 들음)",
          "original_form": "(근데 사실아니에요 제가 주선자한테 들음)",
          "speaker_id": "1",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022307.1.35",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022307.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022307.1.37",
          "form": "아 뭐야",
          "original_form": "아 뭐야",
          "speaker_id": "2",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022307.1.38",
          "form": "진짜 짧은 연애 한번이 있었대요",
          "original_form": "진짜 짧은 연애 한번이 있었대요",
          "speaker_id": "1",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022307.1.39",
          "form": "그리고 다음 저",
          "original_form": "그리고 다음 저",
          "speaker_id": "1",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022307.1.40",
          "form": "아아 ㅋㅋㅋㅋㅋ그럼 뭐",
          "original_form": "아아 ㅋㅋㅋㅋㅋ그럼 뭐",
          "speaker_id": "2",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022307.1.41",
          "form": "제 친구도 일주일 사귀고 헤어지고 연애경험 있는 걸로 굴어요",
          "original_form": "제 친구도 일주일 사귀고 헤어지고 연애경험 있는 걸로 굴어요",
          "speaker_id": "2",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022307.1.42",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022307.1.43",
          "form": "일주일요?",
          "original_form": "일주일요?",
          "speaker_id": "1",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022307.1.44",
          "form": "썸을 길게 타고 사귀는건 일주일",
          "original_form": "썸을 길게 타고 사귀는건 일주일",
          "speaker_id": "2",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022307.1.45",
          "form": "그럼 뭘 안햇으니 연애아닌거같은데",
          "original_form": "그럼 뭘 안햇으니 연애아닌거같은데",
          "speaker_id": "2",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022307.1.46",
          "form": "암튼 얘가 남친 생긴지도 몰랐는데",
          "original_form": "암튼 얘가 남친 생긴지도 몰랐는데",
          "speaker_id": "2",
          "time": "20210829 12:30"
        },
        {
          "id": "MDRW2100022307.1.47",
          "form": "어느날 만나서 하는말이 남친이랑 헤어졌다 여서",
          "original_form": "어느날 만나서 하는말이 남친이랑 헤어졌다 여서",
          "speaker_id": "2",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022307.1.48",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022307.1.49",
          "form": "저한테는 서태지 이지아 이혼 다음가는 스캔들이에요",
          "original_form": "저한테는 서태지 이지아 이혼 다음가는 스캔들이에요",
          "speaker_id": "2",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022307.1.50",
          "form": "웃긴다 ㅋㅋㅋㅋ",
          "original_form": "웃긴다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022307.1.51",
          "form": "그친구 지금 남친 있나요?",
          "original_form": "그친구 지금 남친 있나요?",
          "speaker_id": "1",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022307.1.52",
          "form": "없어요",
          "original_form": "없어요",
          "speaker_id": "2",
          "time": "20210829 12:31"
        },
        {
          "id": "MDRW2100022307.1.53",
          "form": "앗.....",
          "original_form": "앗.....",
          "speaker_id": "1",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022307.1.54",
          "form": "근데 연하남들이 계속 대시는 해요",
          "original_form": "근데 연하남들이 계속 대시는 해요",
          "speaker_id": "2",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022307.1.55",
          "form": "항상 연하남",
          "original_form": "항상 연하남",
          "speaker_id": "2",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022307.1.56",
          "form": "근데 얘는 연하가 싫은거에요",
          "original_form": "근데 얘는 연하가 싫은거에요",
          "speaker_id": "2",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022307.1.57",
