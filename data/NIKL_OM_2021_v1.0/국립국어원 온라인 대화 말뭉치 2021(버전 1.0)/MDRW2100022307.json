          "speaker_id": "1",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022307.1.59",
          "form": "그니까요 능력자인데",
          "original_form": "그니까요 능력자인데",
          "speaker_id": "2",
          "time": "20210829 12:32"
        },
        {
          "id": "MDRW2100022307.1.60",
          "form": "그 일주일말곤 연애안햇어요",
