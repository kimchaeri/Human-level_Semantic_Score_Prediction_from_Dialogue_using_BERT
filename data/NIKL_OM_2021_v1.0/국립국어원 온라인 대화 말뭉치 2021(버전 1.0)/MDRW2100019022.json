        },
        {
          "id": "MDRW2100019022.1.2",
          "form": "별거없음",
          "original_form": "별거없음",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019022.1.3",
          "form": "ㄹㅇ",
          "original_form": "ㄹㅇ",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019022.1.4",
          "form": "학교에서",
          "original_form": "학교에서",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019022.1.5",
          "form": "앵간한곳보더 괜찮음",
          "original_form": "앵간한곳보더 괜찮음",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019022.1.6",
          "form": "교외근로 안받아줘서",
          "original_form": "교외근로 안받아줘서",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019022.1.7",
          "form": "이번에 갠적으로 받는건가",
          "original_form": "이번에 갠적으로 받는건가",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019022.1.8",
          "form": "아마 이번에 갠적으로 받아도",
          "original_form": "아마 이번에 갠적으로 받아도",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019022.1.9",
          "form": "ㄹㅇ 위치만 가까우면",
          "original_form": "ㄹㅇ 위치만 가까우면",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019022.1.10",
          "form": "바로했다",
          "original_form": "바로했다",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019022.1.11",
          "form": "학교에서 승인안하면",
          "original_form": "학교에서 승인안하면",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019022.1.12",
          "form": "짤일듯",
          "original_form": "짤일듯",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019022.1.13",
          "form": "그럼 지금 하는애들은",
          "original_form": "그럼 지금 하는애들은",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019022.1.14",
          "form": "교외근로생들 아님?",
          "original_form": "교외근로생들 아님?",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019022.1.15",
          "form": "다 대청교",
          "original_form": "다 대청교",
          "speaker_id": "2",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019022.1.16",
          "form": "다른대학은 근로생많은데",
          "original_form": "다른대학은 근로생많은데",
          "speaker_id": "2",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019022.1.17",
          "form": "대구대는 다 대청교임",
          "original_form": "대구대는 다 대청교임",
          "speaker_id": "2",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019022.1.18",
          "form": "대청교는 1년계약이라",
          "original_form": "대청교는 1년계약이라",
          "speaker_id": "2",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019022.1.19",
          "form": "짜르질못함",
          "original_form": "짜르질못함",
          "speaker_id": "2",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019022.1.20",
          "form": "난 신청하려면",
          "original_form": "난 신청하려면",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019022.1.21",
          "form": "저기서 교외근로 통과해서",
          "original_form": "저기서 교외근로 통과해서",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019022.1.22",
          "form": "ㅇㅇ학교에서 문의해서",
          "original_form": "ㅇㅇ학교에서 문의해서",
          "speaker_id": "2",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019022.1.23",
          "form": "그때되야 할 수 있는건가",
          "original_form": "그때되야 할 수 있는건가",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019022.1.24",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019022.1.25",
          "form": "그래도 9월중엔일시작할걸",
          "original_form": "그래도 9월중엔일시작할걸",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019022.1.26",
          "form": "ㅋㅋㅋㅋ 언제 통과되노",
          "original_form": "ㅋㅋㅋㅋ 언제 통과되노",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019022.1.27",
          "form": "이제 개강인데",
          "original_form": "이제 개강인데",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019022.1.28",
          "form": "그 사복2학년한테출근해서",
          "original_form": "그 사복2학년한테출근해서",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019022.1.29",
          "form": "물어보지뭐",
          "original_form": "물어보지뭐",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019022.1.30",
          "form": "난 할 수 있으면 화목금 밖에 안될듯",
          "original_form": "난 할 수 있으면 화목금 밖에 안될듯",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019022.1.31",
          "form": "그러면 화목",
          "original_form": "그러면 화목",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019022.1.32",
          "form": "월수 1시부터 수업있음",
          "original_form": "월수 1시부터 수업있음",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019022.1.33",
          "form": "할수있을걸",
          "original_form": "할수있을걸",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019022.1.34",
          "form": "이틀이 최대임?",
          "original_form": "이틀이 최대임?",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019022.1.35",
          "form": "ㄴㄴㄴ 난 3일하는데",
          "original_form": "ㄴㄴㄴ 난 3일하는데",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019022.1.36",
          "form": "하루6시간이라",
          "original_form": "하루6시간이라",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019022.1.37",
          "form": "4일은못하고",
          "original_form": "4일은못하고",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019022.1.38",
          "form": "3일이최대",
          "original_form": "3일이최대",
          "speaker_id": "2",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019022.1.39",
          "form": "할수있으면",
          "original_form": "할수있으면",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019022.1.40",
          "form": "3일하고싶네",
          "original_form": "3일하고싶네",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019022.1.41",
          "form": "화목 아침수업듣고",
          "original_form": "화목 아침수업듣고",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019022.1.42",
          "form": "월화목만",
          "original_form": "월화목만",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019022.1.43",
          "form": "사람구해서",
          "original_form": "사람구해서",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019022.1.44",
          "form": "바로 2시간 달려가야하네 ㅋㅋㅋ",
          "original_form": "바로 2시간 달려가야하네 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019022.1.45",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019022.1.46",
          "form": "25명 관리하는데",
          "original_form": "25명 관리하는데",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019022.1.47",
          "form": "4명이서 통제가되나",
          "original_form": "4명이서 통제가되나",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019022.1.48",
          "form": "수금은 10명 가까이되서 편할거같은데",
          "original_form": "수금은 10명 가까이되서 편할거같은데",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019022.1.49",
          "form": "수금은이미 컷임",
          "original_form": "수금은이미 컷임",
          "speaker_id": "2",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100019022.1.50",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100019022.1.51",
          "form": "그담에 오후에",
          "original_form": "그담에 오후에",
          "speaker_id": "2",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100019022.1.52",
          "form": "2~3시간정도는 외부강사님오셔서따로 수업해서",
          "original_form": "2~3시간정도는 외부강사님오셔서따로 수업해서",
          "speaker_id": "2",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100019022.1.53",
          "form": "크게 뭐없다 ㄹㅇ",
          "original_form": "크게 뭐없다 ㄹㅇ",
          "speaker_id": "2",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100019022.1.54",
          "form": "사람많으면",
          "original_form": "사람많으면",
          "speaker_id": "1",
          "time": "20210827 14:59"
        },
        {
          "id": "MDRW2100019022.1.55",
          "form": "눈치안보고",
          "original_form": "눈치안보고",
          "speaker_id": "1",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100019022.1.56",
          "form": "구석탱이에 숨어있을거같음",
          "original_form": "구석탱이에 숨어있을거같음",
          "speaker_id": "1",
          "time": "20210827 15:00"
        },
        {
          "id": "MDRW2100019022.1.57",
          "form": "이제봤는데",
          "original_form": "이제봤는데",
          "speaker_id": "1",
          "time": "20210827 15:25"
        },
        {
          "id": "MDRW2100019022.1.58",
          "form": "대면이던 수업",
          "original_form": "대면이던 수업",
          "speaker_id": "1",
          "time": "20210827 15:25"
        },
        {
          "id": "MDRW2100019022.1.59",
          "form": "비대면으로 교체됨",
          "original_form": "비대면으로 교체됨",
          "speaker_id": "1",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100019022.1.60",
          "form": "근데 오리엔테이션 대면한다고 오라네",
          "original_form": "근데 오리엔테이션 대면한다고 오라네",
          "speaker_id": "1",
          "time": "20210827 15:26"
        },
        {
          "id": "MDRW2100019022.1.61",
          "form": "이게 무슨 상황이노",
          "original_form": "이게 무슨 상황이노",
          "speaker_id": "1",
          "time": "20210827 15:26"
        }
      ]
    }
