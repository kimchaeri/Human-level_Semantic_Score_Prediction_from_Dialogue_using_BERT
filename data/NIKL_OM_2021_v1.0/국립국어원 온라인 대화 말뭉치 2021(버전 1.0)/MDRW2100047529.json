      "utterance": [
        {
          "id": "MDRW2100047529.1.1",
          "form": "저녁밥 드셨나요?",
          "original_form": "저녁밥 드셨나요?",
          "speaker_id": "1",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100047529.1.2",
          "form": "네네 저는 육회비빔밥 먹었습니자",
          "original_form": "네네 저는 육회비빔밥 먹었습니자",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100047529.1.3",
          "form": "어떤거 드셨나요?!",
          "original_form": "어떤거 드셨나요?!",
          "speaker_id": "2",
          "time": "20210826 19:28"
        },
        {
          "id": "MDRW2100047529.1.4",
          "form": "저는 피자요ㅎㅎ 육회비빔밥 맛있겠다 외식하신거예요?",
          "original_form": "저는 피자요ㅎㅎ 육회비빔밥 맛있겠다 외식하신거예요?",
          "speaker_id": "1",
          "time": "20210826 19:29"
        },
        {
          "id": "MDRW2100047529.1.5",
          "form": "네넼ㅋㅋㅋㅋㅋ요즘 육회에 빠져섴ㅋㅋ",
          "original_form": "네넼ㅋㅋㅋㅋㅋ요즘 육회에 빠져섴ㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100047529.1.6",
          "form": "헐 피자 어떤거요?",
          "original_form": "헐 피자 어떤거요?",
          "speaker_id": "2",
          "time": "20210826 19:30"
        },
        {
          "id": "MDRW2100047529.1.7",
          "form": "도미노요ㅋㅋ 피자 좋아하세요?",
          "original_form": "도미노요ㅋㅋ 피자 좋아하세요?",
          "speaker_id": "1",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100047529.1.8",
          "form": "네네 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:32"
        },
        {
          "id": "MDRW2100047529.1.9",
          "form": "쓰읍 저",
          "original_form": "쓰읍 저",
          "speaker_id": "2",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100047529.1.10",
          "form": "적폐입맛이라고 하던데",
          "original_form": "적폐입맛이라고 하던데",
          "speaker_id": "2",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100047529.1.11",
          "form": "전 하와이안피자 좋아해요",
          "original_form": "전 하와이안피자 좋아해요",
          "speaker_id": "2",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100047529.1.12",
          "form": "민초단이신가요?",
          "original_form": "민초단이신가요?",
          "speaker_id": "1",
          "time": "20210826 19:33"
        },
        {
          "id": "MDRW2100047529.1.13",
          "form": "헐 네",
          "original_form": "헐 네",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100047529.1.14",
          "form": "어케아셨어요?",
          "original_form": "어케아셨어요?",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100047529.1.15",
          "form": "저 슈팅스타도 좋아해요",
          "original_form": "저 슈팅스타도 좋아해요",
          "speaker_id": "2",
          "time": "20210826 19:39"
        },
        {
          "id": "MDRW2100047529.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100047529.1.17",
          "form": "ㅋㅋㅋㅋ 호불호 끝판왕 입맛이시네요",
          "original_form": "ㅋㅋㅋㅋ 호불호 끝판왕 입맛이시네요",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100047529.1.18",
          "form": "체리쥬빌레는 안드시나요?",
          "original_form": "체리쥬빌레는 안드시나요?",
          "speaker_id": "1",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100047529.1.19",
          "form": "어 먹어요",
          "original_form": "어 먹어요",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100047529.1.20",
          "form": "솔의 눈만 안먹어요",
          "original_form": "솔의 눈만 안먹어요",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100047529.1.21",
          "form": "아니 ... 다 .. 안드세요?",
          "original_form": "아니 ... 다 .. 안드세요?",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100047529.1.22",
          "form": "민초 체리쥬빌레",
          "original_form": "민초 체리쥬빌레",
          "speaker_id": "2",
          "time": "20210826 19:40"
        },
        {
          "id": "MDRW2100047529.1.23",
          "form": "베라 무슨맛 제이르좋아하세요?",
          "original_form": "베라 무슨맛 제이르좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 19:41"
        },
        {
          "id": "MDRW2100047529.1.24",
          "form": "전 31요거트요ㅋㅋㅋ 좋아하세요?",
          "original_form": "전 31요거트요ㅋㅋㅋ 좋아하세요?",
