      "utterance": [
        {
          "id": "MDRW2100002831.1.1",
          "form": "생각 나신 곳 있으신가요?",
          "original_form": "생각 나신 곳 있으신가요?",
          "speaker_id": "2",
          "time": "20210820 15:50"
        },
        {
          "id": "MDRW2100002831.1.2",
          "form": "여행지요",
          "original_form": "여행지요",
          "speaker_id": "2",
          "time": "20210820 15:52"
        },
        {
          "id": "MDRW2100002831.1.3",
          "form": "홍콩이요",
          "original_form": "홍콩이요",
          "speaker_id": "1",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002831.1.4",
          "form": "홍콩 가고 싶네요",
          "original_form": "홍콩 가고 싶네요",
          "speaker_id": "2",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002831.1.5",
          "form": "가보셨나요",
          "original_form": "가보셨나요",
          "speaker_id": "1",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002831.1.6",
          "form": "네 좀 오래되긴 했는데",
          "original_form": "네 좀 오래되긴 했는데",
          "speaker_id": "2",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002831.1.7",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002831.1.8",
          "form": "코로나 터지고는 해외는 못가서",
          "original_form": "코로나 터지고는 해외는 못가서",
          "speaker_id": "1",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002831.1.9",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210820 15:53"
        },
        {
          "id": "MDRW2100002831.1.10",
          "form": "홍콩 그 해변 정말 좋았어요",
          "original_form": "홍콩 그 해변 정말 좋았어요",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002831.1.11",
          "form": "아 이름이 길었던 것 같은데",
          "original_form": "아 이름이 길었던 것 같은데",
          "speaker_id": "1",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002831.1.12",
          "form": "맞아요 리 무슨 베이던가 ㅋㅋㅋ",
          "original_form": "맞아요 리 무슨 베이던가 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 15:54"
        },
        {
          "id": "MDRW2100002831.1.13",
          "form": "도심에서 좀 먼 곳 ㅋㅋ",
          "original_form": "도심에서 좀 먼 곳 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002831.1.14",
          "form": "하루종일 해변에 앉아있으면 힐링",
          "original_form": "하루종일 해변에 앉아있으면 힐링",
          "speaker_id": "2",
          "time": "20210820 15:55"
        },
        {
          "id": "MDRW2100002831.1.15",
          "form": "힐링 ㅎㅎ좋죠",
          "original_form": "힐링 ㅎㅎ좋죠",
          "speaker_id": "1",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002831.1.16",
          "form": "야경도 좋았어요",
          "original_form": "야경도 좋았어요",
          "speaker_id": "2",
          "time": "20210820 15:56"
        },
        {
          "id": "MDRW2100002831.1.17",
          "form": "홍콩 야경 유명하죠",
          "original_form": "홍콩 야경 유명하죠",
          "speaker_id": "1",
          "time": "20210820 15:57"
        },
        {
          "id": "MDRW2100002831.1.18",
          "form": "저도 좋았어요",
          "original_form": "저도 좋았어요",
          "speaker_id": "1",
          "time": "20210820 15:57"
        }
      ]
