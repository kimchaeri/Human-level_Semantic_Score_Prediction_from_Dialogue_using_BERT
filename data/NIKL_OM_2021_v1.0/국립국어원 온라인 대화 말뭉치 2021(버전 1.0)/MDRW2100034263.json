      "utterance": [
        {
          "id": "MDRW2100034263.1.1",
          "form": "피티도 하고싶은데",
          "original_form": "피티도 하고싶은데",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100034263.1.2",
          "form": "요가가 좀 더 싸더라구요 근데 근육은 안만들어진대요",
          "original_form": "요가가 좀 더 싸더라구요 근데 근육은 안만들어진대요",
          "speaker_id": "1",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100034263.1.3",
          "form": "피티도 엄청비싸죠!",
          "original_form": "피티도 엄청비싸죠!",
          "speaker_id": "1",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100034263.1.4",
          "form": "가격이 극악무도 해서....",
          "original_form": "가격이 극악무도 해서....",
          "speaker_id": "2",
          "time": "20210827 22:10"
        },
        {
          "id": "MDRW2100034263.1.5",
          "form": "네... 필테의 장점은 자세교정이 되니까 좋은 거 같아요",
          "original_form": "네... 필테의 장점은 자세교정이 되니까 좋은 거 같아요",
          "speaker_id": "2",
          "time": "20210827 22:11"
        },
        {
          "id": "MDRW2100034263.1.6",
          "form": "피티가 진짜 좋긴할텐데..",
          "original_form": "피티가 진짜 좋긴할텐데..",
          "speaker_id": "1",
          "time": "20210827 22:12"
        },
        {
          "id": "MDRW2100034263.1.7",
          "form": "저는팔자걸음이랑 거북목 교정받고 싶은데 요가는 도움안되는거같아요",
          "original_form": "저는팔자걸음이랑 거북목 교정받고 싶은데 요가는 도움안되는거같아요",
          "speaker_id": "1",
          "time": "20210827 22:13"
        },
        {
          "id": "MDRW2100034263.1.8",
          "form": "아 저도 자세교정이요",
          "original_form": "아 저도 자세교정이요",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100034263.1.9",
          "form": "아주 심각쓰 ㅎ ㅎ",
          "original_form": "아주 심각쓰 ㅎ ㅎ",
          "speaker_id": "2",
          "time": "20210827 22:14"
        },
        {
          "id": "MDRW2100034263.1.10",
          "form": "앜ㅋㅋㅋㅋ 필테 후기 공유해드릴게요 제 친구들은 자세에 도움 많이됐대요!",
          "original_form": "앜ㅋㅋㅋㅋ 필테 후기 공유해드릴게요 제 친구들은 자세에 도움 많이됐대요!",
          "speaker_id": "1",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100034263.1.11",
          "form": "피티도 좋을텐데.. 근데 나중에 병원비 생각하면",
          "original_form": "피티도 좋을텐데.. 근데 나중에 병원비 생각하면",
          "speaker_id": "1",
          "time": "20210827 22:16"
        },
        {
          "id": "MDRW2100034263.1.12",
          "form": "운동비가 더 쌀지도요",
          "original_form": "운동비가 더 쌀지도요",
          "speaker_id": "1",
          "time": "20210827 22:17"
        },
        {
          "id": "MDRW2100034263.1.13",
          "form": "아ㅋㅋ 감사해요.. 궁금하네요",
          "original_form": "아ㅋㅋ 감사해요.. 궁금하네요",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034263.1.14",
          "form": "그래도 병원비는... 안듀ㅐ유..",
          "original_form": "그래도 병원비는... 안듀ㅐ유..",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100034263.1.15",
          "form": "그쵸 운동비로 일단ㅋㅋㅋ",
          "original_form": "그쵸 운동비로 일단ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100034263.1.16",
          "form": "ㅎㅎ 운동해서 건강한 몸 갖고싶넹ㅎ",
          "original_form": "ㅎㅎ 운동해서 건강한 몸 갖고싶넹ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034263.1.17",
          "form": "복근도 갖고싶어요 !!!@",
          "original_form": "복근도 갖고싶어요 !!!@",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034263.1.18",
          "form": "헐 저두요 ㅎㅎㅎ 저는 어깨 넓어지고싶어요",
          "original_form": "헐 저두요 ㅎㅎㅎ 저는 어깨 넓어지고싶어요",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100034263.1.19",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100034263.1.20",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210827 23:21"
        },
        {
          "id": "MDRW2100034263.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ어깨 어떻게 넓히죠 ㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ어깨 어떻게 넓히죠 ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100034263.1.22",
          "form": "ㅋㅋㅋㅋ건장한 체구",
          "original_form": "ㅋㅋㅋㅋ건장한 체구",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100034263.1.23",
          "form": "자세 교정되면 어깨 좀 넓어진다는 소문이있어요",
          "original_form": "자세 교정되면 어깨 좀 넓어진다는 소문이있어요",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100034263.1.24",
          "form": "진심.. 어깨좀 넓어졌으면...",
          "original_form": "진심.. 어깨좀 넓어졌으면...",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100034263.1.25",
          "form": "가능할 거 같아요",
          "original_form": "가능할 거 같아요",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100034263.1.26",
