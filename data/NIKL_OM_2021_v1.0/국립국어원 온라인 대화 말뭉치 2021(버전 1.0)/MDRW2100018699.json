        },
        {
          "id": "MDRW2100018699.1.2",
          "form": "치러 간다 ㅋㅋㅋㅋㄱ",
          "original_form": "치러 간다 ㅋㅋㅋㅋㄱ",
          "speaker_id": "2",
          "time": "20210827 11:07"
        },
        {
          "id": "MDRW2100018699.1.3",
          "form": "텐트에 비 묻는 거 싫은데",
          "original_form": "텐트에 비 묻는 거 싫은데",
          "speaker_id": "2",
          "time": "20210827 11:07"
        },
        {
          "id": "MDRW2100018699.1.4",
          "form": "갔다 와서 바짝 말려야죠 언니",
          "original_form": "갔다 와서 바짝 말려야죠 언니",
          "speaker_id": "1",
          "time": "20210827 11:09"
        },
        {
          "id": "MDRW2100018699.1.5",
          "form": "내 몸보다 소중한 텐트",
          "original_form": "내 몸보다 소중한 텐트",
          "speaker_id": "1",
          "time": "20210827 11:09"
        },
        {
          "id": "MDRW2100018699.1.6",
          "form": "지켜줘야죠 쇠 탬프",
          "original_form": "지켜줘야죠 쇠 탬프",
          "speaker_id": "1",
          "time": "20210827 11:09"
        },
        {
          "id": "MDRW2100018699.1.7",
          "form": "새 텐트",
          "original_form": "새 텐트",
          "speaker_id": "1",
          "time": "20210827 11:09"
        },
        {
          "id": "MDRW2100018699.1.8",
          "form": "그렇지 중요하다",
          "original_form": "그렇지 중요하다",
          "speaker_id": "2",
          "time": "20210827 11:09"
        },
        {
          "id": "MDRW2100018699.1.9",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 11:09"
        },
        {
          "id": "MDRW2100018699.1.10",
          "form": "캠핑 장비는 다 샀어요",
          "original_form": "캠핑 장비는 다 샀어요",
          "speaker_id": "1",
          "time": "20210827 11:09"
        },
        {
          "id": "MDRW2100018699.1.11",
          "form": "왜 내 텐트를 쇠로 만드노",
          "original_form": "왜 내 텐트를 쇠로 만드노",
          "speaker_id": "2",
          "time": "20210827 11:09"
        },
        {
          "id": "MDRW2100018699.1.12",
          "form": "면텐트샀는데...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "면텐트샀는데...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 11:09"
        },
        {
          "id": "MDRW2100018699.1.13",
          "form": "엄청 튼튼 할 듯",
          "original_form": "엄청 튼튼 할 듯",
          "speaker_id": "1",
          "time": "20210827 11:09"
        },
        {
          "id": "MDRW2100018699.1.14",
          "form": "그럼 장비는 다 샀지",
          "original_form": "그럼 장비는 다 샀지",
          "speaker_id": "2",
          "time": "20210827 11:09"
        },
        {
          "id": "MDRW2100018699.1.15",
          "form": "언니도 참 추진력 굿 굿",
          "original_form": "언니도 참 추진력 굿 굿",
          "speaker_id": "1",
          "time": "20210827 11:10"
        },
        {
          "id": "MDRW2100018699.1.16",
          "form": "뭐든지 장비 빨",
          "original_form": "뭐든지 장비 빨",
          "speaker_id": "1",
          "time": "20210827 11:10"
        },
        {
          "id": "MDRW2100018699.1.17",
