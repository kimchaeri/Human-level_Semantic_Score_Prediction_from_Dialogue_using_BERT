      "utterance": [
        {
          "id": "MDRW2100026795.1.1",
          "form": "서울에 보면 강남은 버거킹",
          "original_form": "서울에 보면 강남은 버거킹",
          "speaker_id": "2",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026795.1.2",
          "form": "강북은 맥날",
          "original_form": "강북은 맥날",
          "speaker_id": "2",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026795.1.3",
          "form": "맞나요?",
          "original_form": "맞나요?",
          "speaker_id": "2",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026795.1.4",
          "form": "? ? 금시초문인데요",
          "original_form": "? ? 금시초문인데요",
          "speaker_id": "1",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026795.1.5",
          "form": "왜 나눠요?",
          "original_form": "왜 나눠요?",
          "speaker_id": "1",
          "time": "20210902 00:09"
        },
        {
          "id": "MDRW2100026795.1.6",
          "form": "아니 친구가 자꾸 그렇다길래",
          "original_form": "아니 친구가 자꾸 그렇다길래",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026795.1.7",
          "form": "그런갚다 했었는데 가짜네요",
          "original_form": "그런갚다 했었는데 가짜네요",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026795.1.8",
          "form": "친구 지기고 오겠습니다 잠시",
          "original_form": "친구 지기고 오겠습니다 잠시",
          "speaker_id": "2",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026795.1.9",
          "form": "아ㅋㅋ ㅋ",
          "original_form": "아ㅋㅋ ㅋ",
          "speaker_id": "1",
          "time": "20210902 00:10"
        },
        {
          "id": "MDRW2100026795.1.10",
          "form": "서울 안가본 사람이 더 무섭다고",
          "original_form": "서울 안가본 사람이 더 무섭다고",
          "speaker_id": "2",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026795.1.11",
          "form": "검색하니 십년전;; 강남에 ,비싼 버거킹이 많았단 말이 있네요",
          "original_form": "검색하니 십년전;; 강남에 ,비싼 버거킹이 많았단 말이 있네요",
          "speaker_id": "1",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026795.1.12",
          "form": "요정이시네요",
          "original_form": "요정이시네요",
          "speaker_id": "2",
          "time": "20210902 00:11"
        },
        {
          "id": "MDRW2100026795.1.13",
          "form": "검색요정!",
          "original_form": "검색요정!",
          "speaker_id": "2",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026795.1.14",
          "form": "근데 버거킹이 더 싼데요ㅎㅎ 사딸라 때문에ㅋㅋ",
          "original_form": "근데 버거킹이 더 싼데요ㅎㅎ 사딸라 때문에ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026795.1.15",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026795.1.16",
          "form": "강북은 롯데리아라는데ㅅㅅ",
          "original_form": "강북은 롯데리아라는데ㅅㅅ",
          "speaker_id": "1",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026795.1.17",
          "form": "롯데리아도 런치는 버거킹에 안되쥬?",
          "original_form": "롯데리아도 런치는 버거킹에 안되쥬?",
          "speaker_id": "1",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026795.1.18",
          "form": "롯데리아 조아조아",
          "original_form": "롯데리아 조아조아",
          "speaker_id": "2",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026795.1.19",
          "form": "안될거요",
          "original_form": "안될거요",
          "speaker_id": "2",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026795.1.20",
          "form": "버거킹이 젤 싼듯ㅎㅎ",
          "original_form": "버거킹이 젤 싼듯ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 00:12"
        },
        {
          "id": "MDRW2100026795.1.21",
          "form": "롯리는 해쉬드포테이토 좋아해요ㅎ",
          "original_form": "롯리는 해쉬드포테이토 좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100026795.1.22",
          "form": "해시포테이토 좋아하는데 파는데가 잘 없어서ㅜ",
          "original_form": "해시포테이토 좋아하는데 파는데가 잘 없어서ㅜ",
          "speaker_id": "1",
          "time": "20210902 00:13"
        },
        {
          "id": "MDRW2100026795.1.23",
          "form": "잠깐 근데 전여친은 어떻게 됐죠!!",
          "original_form": "잠깐 근데 전여친은 어떻게 됐죠!!",
          "speaker_id": "1",
          "time": "20210902 00:14"
        },
        {
          "id": "MDRW2100026795.1.24",
          "form": "흑",
          "original_form": "흑",
          "speaker_id": "2",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026795.1.25",
          "form": "돼지",
          "original_form": "돼지",
          "speaker_id": "2",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026795.1.26",
          "form": "흙 돼 지",
          "original_form": "흙 돼 지",
          "speaker_id": "2",
          "time": "20210902 00:16"
        },
        {
          "id": "MDRW2100026795.1.27",
          "form": "3인분?",
          "original_form": "3인분?",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026795.1.28",
          "form": "먹엇는데요",
          "original_form": "먹엇는데요",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026795.1.29",
          "form": "2일차에",
          "original_form": "2일차에",
          "speaker_id": "2",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026795.1.30",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210902 00:17"
        },
        {
          "id": "MDRW2100026795.1.31",
          "form": "성산잀출봉앞에서 싸웟서요",
          "original_form": "성산잀출봉앞에서 싸웟서요",
          "speaker_id": "2",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026795.1.32",
          "form": "자긴 바지락을 더캐고 싶다고",
          "original_form": "자긴 바지락을 더캐고 싶다고",
          "speaker_id": "2",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026795.1.33",
          "form": "아니ㅋㅋㅇ갑분바지락ㅋㅋㅋㅋ",
          "original_form": "아니ㅋㅋㅇ갑분바지락ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026795.1.34",
          "form": "그래서 물들어올때까지 3시간 넘게 바지락캤는데",
          "original_form": "그래서 물들어올때까지 3시간 넘게 바지락캤는데",
          "speaker_id": "2",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026795.1.35",
          "form": "짜증이나더라고요",
          "original_form": "짜증이나더라고요",
          "speaker_id": "2",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026795.1.36",
          "form": "바지락캐러왓냐?",
          "original_form": "바지락캐러왓냐?",
          "speaker_id": "2",
          "time": "20210902 00:18"
        },
        {
          "id": "MDRW2100026795.1.37",
          "form": "ㅋㅋㅋ 미쵸ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ 미쵸ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026795.1.38",
          "form": "체험삶의현장찍고 와서",
          "original_form": "체험삶의현장찍고 와서",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026795.1.39",
          "form": "시트콤 찍으시나요ㅋㅋㅋ",
          "original_form": "시트콤 찍으시나요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026795.1.40",
          "form": "소원해졌네요",
          "original_form": "소원해졌네요",
          "speaker_id": "2",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026795.1.41",
          "form": "아ㅋㅋㅋ컬투쇼에 사연 보내시죠ㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ컬투쇼에 사연 보내시죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:19"
        },
        {
          "id": "MDRW2100026795.1.42",
          "form": "근데 거기가 바지락이 진짜 미친듯이 나와요",
          "original_form": "근데 거기가 바지락이 진짜 미친듯이 나와요",
