      "utterance": [
        {
          "id": "MDRW2100017602.1.1",
          "form": "배달음식 좋아하세요?",
          "original_form": "배달음식 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 20:17"
        },
        {
          "id": "MDRW2100017602.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017602.1.3",
          "form": "치킨 자주 먹는거 같네요",
          "original_form": "치킨 자주 먹는거 같네요",
          "speaker_id": "1",
          "time": "20210826 20:18"
        },
        {
          "id": "MDRW2100017602.1.4",
          "form": "저도 치킨 좋아해요",
          "original_form": "저도 치킨 좋아해요",
          "speaker_id": "2",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017602.1.5",
          "form": "어떤 종류의 치킨 좋아하세요?",
          "original_form": "어떤 종류의 치킨 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 20:19"
        },
        {
          "id": "MDRW2100017602.1.6",
          "form": "전 후라이드요",
          "original_form": "전 후라이드요",
          "speaker_id": "1",
          "time": "20210826 20:20"
        },
        {
          "id": "MDRW2100017602.1.7",
          "form": "시즈닝 뿌려져있는 건 안좋아해요?",
          "original_form": "시즈닝 뿌려져있는 건 안좋아해요?",
          "speaker_id": "2",
          "time": "20210826 20:23"
        },
        {
          "id": "MDRW2100017602.1.8",
          "form": "뿌링클 좋아해요ㅋㅋ",
          "original_form": "뿌링클 좋아해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 20:24"
        },
        {
          "id": "MDRW2100017602.1.9",
          "form": "비에이치꺼 맞죠? 뿌링클 맛있더라구요~",
          "original_form": "비에이치꺼 맞죠? 뿌링클 맛있더라구요~",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017602.1.10",
          "form": "전 간장치킨도 좋은 것 같아요",
          "original_form": "전 간장치킨도 좋은 것 같아요",
          "speaker_id": "2",
          "time": "20210826 20:25"
        },
        {
          "id": "MDRW2100017602.1.11",
          "form": "맞아요. 간장치킨도 맛있어요",
          "original_form": "맞아요. 간장치킨도 맛있어요",
          "speaker_id": "1",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017602.1.12",
          "form": "치킨은 거의 맛있긴 한것같아요",
          "original_form": "치킨은 거의 맛있긴 한것같아요",
          "speaker_id": "1",
          "time": "20210826 20:26"
        },
        {
          "id": "MDRW2100017602.1.13",
          "form": "근데 요즘 치킨값 비싼 것 같지 않아요?ㅠㅠ",
          "original_form": "근데 요즘 치킨값 비싼 것 같지 않아요?ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 20:27"
        },
        {
          "id": "MDRW2100017602.1.14",
          "form": "맞아요ㅠㅠ 자꾸 비싸져서 시켜먹기 고민하게되요",
          "original_form": "맞아요ㅠㅠ 자꾸 비싸져서 시켜먹기 고민하게되요",
          "speaker_id": "1",
          "time": "20210826 20:28"
        },
        {
          "id": "MDRW2100017602.1.15",
          "form": "거기다가 배달비까지 내려니 더 비싼 느낌이에요.",
          "original_form": "거기다가 배달비까지 내려니 더 비싼 느낌이에요.",
          "speaker_id": "2",
          "time": "20210826 20:29"
        },
        {
          "id": "MDRW2100017602.1.16",
          "form": "그러니까요. 사러 가기도 번거롭구...",
          "original_form": "그러니까요. 사러 가기도 번거롭구...",
          "speaker_id": "1",
          "time": "20210826 20:30"
        },
        {
          "id": "MDRW2100017602.1.17",
          "form": "다행히도 제 집 근처에 치킨집 많아서 포장할인 하네용",
          "original_form": "다행히도 제 집 근처에 치킨집 많아서 포장할인 하네용",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017602.1.18",
          "form": "굽네치킨 교촌치킨 피자헛 후참잘 비에이치 비비큐 등등 다 10분거리 안에 있어서 ㅎㅎ",
          "original_form": "굽네치킨 교촌치킨 피자헛 후참잘 비에이치 비비큐 등등 다 10분거리 안에 있어서 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 20:31"
        },
        {
          "id": "MDRW2100017602.1.19",
          "form": "부럽네요ㅋㅋㅋ 저희 집 근처는 조금 걸려서ㅎ",
          "original_form": "부럽네요ㅋㅋㅋ 저희 집 근처는 조금 걸려서ㅎ",
          "speaker_id": "1",
          "time": "20210826 20:33"
        },
        {
          "id": "MDRW2100017602.1.20",
          "form": "ㅠㅠ 이젠 치세권이란 단어도 탄생할 듯해요",
          "original_form": "ㅠㅠ 이젠 치세권이란 단어도 탄생할 듯해요",
          "speaker_id": "2",
          "time": "20210826 20:35"
        }
      ]
