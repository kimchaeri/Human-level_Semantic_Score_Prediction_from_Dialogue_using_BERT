        {
          "id": "MDRW2100038150.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210901 23:44"
        },
        {
          "id": "MDRW2100038150.1.2",
          "form": "날씨 어떤가요?",
          "original_form": "날씨 어떤가요?",
          "speaker_id": "2",
          "time": "20210901 23:46"
        },
        {
          "id": "MDRW2100038150.1.3",
          "form": "아직도 많이 흐려요",
          "original_form": "아직도 많이 흐려요",
          "speaker_id": "1",
          "time": "20210901 23:48"
        },
        {
          "id": "MDRW2100038150.1.4",
          "form": "여기두여",
          "original_form": "여기두여",
          "speaker_id": "2",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038150.1.5",
          "form": "이번 장마가 오래 가네요",
          "original_form": "이번 장마가 오래 가네요",
          "speaker_id": "1",
          "time": "20210901 23:49"
        },
        {
          "id": "MDRW2100038150.1.6",
          "form": "가늘고 길게 ㅠ",
          "original_form": "가늘고 길게 ㅠ",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100038150.1.7",
          "form": "어제는 비가 많이 왔었고",
          "original_form": "어제는 비가 많이 왔었고",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100038150.1.8",
          "form": "오늘은 흐리기만 하니 이상하네요",
          "original_form": "오늘은 흐리기만 하니 이상하네요",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100038150.1.9",
          "form": "이도저도아닌",
          "original_form": "이도저도아닌",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100038150.1.10",
          "form": "그게 딱 맞는 표현인거 같아요",
          "original_form": "그게 딱 맞는 표현인거 같아요",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100038150.1.11",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100038150.1.12",
          "form": "다음주는 어떨까요?",
          "original_form": "다음주는 어떨까요?",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100038150.1.13",
          "form": "다음주는 날씨 좋을거 같아요",
          "original_form": "다음주는 날씨 좋을거 같아요",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100038150.1.14",
          "form": "장마가 끝날때가 돼서",
          "original_form": "장마가 끝날때가 돼서",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100038150.1.15",
          "form": "완전 가을이네요",
          "original_form": "완전 가을이네요",
          "speaker_id": "2",
          "time": "20210902 00:03"
        },
        {
          "id": "MDRW2100038150.1.16",
          "form": "여름이 훅 가버렸죠",
          "original_form": "여름이 훅 가버렸죠",
          "speaker_id": "1",
          "time": "20210902 00:03"
