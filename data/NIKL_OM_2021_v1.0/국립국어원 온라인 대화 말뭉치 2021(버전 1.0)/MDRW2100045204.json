      "utterance": [
        {
          "id": "MDRW2100045204.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210908 01:15"
        },
        {
          "id": "MDRW2100045204.1.2",
          "form": "안녕하세용 저녁뭐드셨나요?",
          "original_form": "안녕하세용 저녁뭐드셨나요?",
          "speaker_id": "2",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045204.1.3",
          "form": "비빔밥먹엇어요",
          "original_form": "비빔밥먹엇어요",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045204.1.4",
          "form": "뭐드셧어요?",
          "original_form": "뭐드셧어요?",
          "speaker_id": "1",
          "time": "20210908 01:17"
        },
        {
          "id": "MDRW2100045204.1.5",
          "form": "저는 육개장사발면이욬ㅋㅋㅋ",
          "original_form": "저는 육개장사발면이욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:18"
        },
        {
          "id": "MDRW2100045204.1.6",
          "form": "작은거요?",
          "original_form": "작은거요?",
          "speaker_id": "1",
          "time": "20210908 01:19"
        },
        {
          "id": "MDRW2100045204.1.7",
          "form": "네! 작은게 맛있더라구요ㅋㅋ",
          "original_form": "네! 작은게 맛있더라구요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045204.1.8",
          "form": "그렇죠 작은게 맛잇죠",
          "original_form": "그렇죠 작은게 맛잇죠",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045204.1.9",
          "form": "그런데 양이 너무 작지 않아요?",
          "original_form": "그런데 양이 너무 작지 않아요?",
          "speaker_id": "1",
          "time": "20210908 01:20"
        },
        {
          "id": "MDRW2100045204.1.10",
          "form": "앗 제가 그전에 피자도 한조각 먹었어용",
          "original_form": "앗 제가 그전에 피자도 한조각 먹었어용",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045204.1.11",
          "form": "요즘 피자 너무 맛있어서ㅜㅜ",
          "original_form": "요즘 피자 너무 맛있어서ㅜㅜ",
          "speaker_id": "2",
          "time": "20210908 01:21"
        },
        {
          "id": "MDRW2100045204.1.12",
          "form": "피자 좋아하세여?",
          "original_form": "피자 좋아하세여?",
          "speaker_id": "1",
          "time": "20210908 01:22"
        },
        {
          "id": "MDRW2100045204.1.13",
          "form": "요즘 빠졌어요ㅋㅋ 치즈토핑추가해서 먹으니까 완전 맛있어요",
          "original_form": "요즘 빠졌어요ㅋㅋ 치즈토핑추가해서 먹으니까 완전 맛있어요",
          "speaker_id": "2",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045204.1.14",
          "form": "피자좋아하세요?",
          "original_form": "피자좋아하세요?",
          "speaker_id": "2",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045204.1.15",
          "form": "네 피자 좋아해요",
          "original_form": "네 피자 좋아해요",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045204.1.16",
          "form": "무슨피자 좋아하세요?",
          "original_form": "무슨피자 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100045204.1.17",
          "form": "저는 종류상관없는데 화덕피자보다는 두꺼운 피자가 좋아요!",
          "original_form": "저는 종류상관없는데 화덕피자보다는 두꺼운 피자가 좋아요!",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100045204.1.18",
          "form": "아 저도 두꺼운피자 좋아해요",
          "original_form": "아 저도 두꺼운피자 좋아해요",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045204.1.19",
          "form": "좋아하는 피자 브랜드 잇으세요?",
          "original_form": "좋아하는 피자 브랜드 잇으세요?",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045204.1.20",
          "form": "저는 요즘 도미노피자요!",
          "original_form": "저는 요즘 도미노피자요!",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100045204.1.21",
          "form": "도미노에 맛잇는 메뉴 잇나요?",
          "original_form": "도미노에 맛잇는 메뉴 잇나요?",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100045204.1.22",
          "form": "저는 페퍼로니피자 맛잇더라구요?",
          "original_form": "저는 페퍼로니피자 맛잇더라구요?",
          "speaker_id": "2",
          "time": "20210908 01:29"
        }
