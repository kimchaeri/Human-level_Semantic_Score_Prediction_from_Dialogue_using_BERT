      "utterance": [
        {
          "id": "MDRW2100026471.1.1",
          "form": "전 와인을 한잔 마셨어요 술 좋아하시나요?",
          "original_form": "전 와인을 한잔 마셨어요 술 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210901 22:28"
        },
        {
          "id": "MDRW2100026471.1.2",
          "form": "알쓰긴 한데ㅋㅋㅋㅋ 가끔 와인이나 전통주, 칵테일 같은걸 홀짝이는 편이에요",
          "original_form": "알쓰긴 한데ㅋㅋㅋㅋ 가끔 와인이나 전통주, 칵테일 같은걸 홀짝이는 편이에요",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026471.1.3",
          "form": "안주는 필수구요ㅋㅋㅋ",
          "original_form": "안주는 필수구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:29"
        },
        {
          "id": "MDRW2100026471.1.4",
          "form": "전통주 무슨 주 좋아하세요??",
          "original_form": "전통주 무슨 주 좋아하세요??",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026471.1.5",
          "form": "저도 알쓰지만 술을 좋아해요 ㅎㅎ",
          "original_form": "저도 알쓰지만 술을 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 22:32"
        },
        {
          "id": "MDRW2100026471.1.6",
          "form": "가장 최근에 먹은건 혼디주요",
          "original_form": "가장 최근에 먹은건 혼디주요",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026471.1.7",
          "form": "제주 감귤 발효주에요",
          "original_form": "제주 감귤 발효주에요",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026471.1.8",
          "form": "사실 아직 최애 술 같은건 없어요",
          "original_form": "사실 아직 최애 술 같은건 없어요",
          "speaker_id": "1",
          "time": "20210901 22:34"
        },
        {
          "id": "MDRW2100026471.1.9",
          "form": "걍 희석식 소주만 아니면 됨ㅋㅋㅋㅋ",
          "original_form": "걍 희석식 소주만 아니면 됨ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:35"
        },
        {
          "id": "MDRW2100026471.1.10",
          "form": "혼디주 저도 제주도 가서 마셔본적 있는데 기억이나네요 ㅎㅎ",
          "original_form": "혼디주 저도 제주도 가서 마셔본적 있는데 기억이나네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100026471.1.11",
          "form": "향은 좋은데 끝맛이 좀 쓰더라구요",
          "original_form": "향은 좋은데 끝맛이 좀 쓰더라구요",
          "speaker_id": "1",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100026471.1.12",
          "form": "귤 속껍질맛 같기도 하구 ㅎㅎ",
          "original_form": "귤 속껍질맛 같기도 하구 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100026471.1.13",
          "form": "그랬군요 몇년전이라 기억이 가물가물하네요 ㅋㅋㅋ\n술담화라고 술판매하는 회사에서 정기적으로 술을 보내주기도 하더라구요 그런것도 한번 해보고싶어요!",
          "original_form": "그랬군요 몇년전이라 기억이 가물가물하네요 ㅋㅋㅋ\n술담화라고 술판매하는 회사에서 정기적으로 술을 보내주기도 하더라구요 그런것도 한번 해보고싶어요!",
          "speaker_id": "2",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100026471.1.14",
          "form": "저도 흥미는 가는데",
          "original_form": "저도 흥미는 가는데",
          "speaker_id": "1",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100026471.1.15",
          "form": "1달안에 두세병? 불가능하단 판단을 해부렸지요ㅠ",
          "original_form": "1달안에 두세병? 불가능하단 판단을 해부렸지요ㅠ",
          "speaker_id": "1",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100026471.1.16",
          "form": "무슨 술이든 두잔이상을 넘지 못하는 주량이에요ㅠ",
          "original_form": "무슨 술이든 두잔이상을 넘지 못하는 주량이에요ㅠ",
          "speaker_id": "1",
          "time": "20210902 09:44"
        },
        {
          "id": "MDRW2100026471.1.17",
          "form": "그렇군요! 그대신 술을 깊게 느끼실수 있겠어요!",
          "original_form": "그렇군요! 그대신 술을 깊게 느끼실수 있겠어요!",
          "speaker_id": "2",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100026471.1.18",
          "form": "술맛을요 ㅎㅎㅎ",
          "original_form": "술맛을요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100026471.1.19",
          "form": "안주는 혼디주에 뭘 드셨나요?",
          "original_form": "안주는 혼디주에 뭘 드셨나요?",
          "speaker_id": "2",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100026471.1.20",
          "form": "육포랑 블루베리요",
          "original_form": "육포랑 블루베리요",
          "speaker_id": "1",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100026471.1.21",
          "form": "걍 집에 있는것중에 그럴듯한거 ㅎ",
          "original_form": "걍 집에 있는것중에 그럴듯한거 ㅎ",
          "speaker_id": "1",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100026471.1.22",
          "form": "맛있었겠어요! 안주는 정말 입맛이라 그냥 맛있으면 되는것같아요 ㅎㅎ",
          "original_form": "맛있었겠어요! 안주는 정말 입맛이라 그냥 맛있으면 되는것같아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 11:38"
        },
        {
          "id": "MDRW2100026471.1.23",
          "form": "그쵸, 전 좀 짭조름한 안주를 선호해요",
          "original_form": "그쵸, 전 좀 짭조름한 안주를 선호해요",
          "speaker_id": "1",
          "time": "20210902 11:41"
        },
        {
          "id": "MDRW2100026471.1.24",
          "form": "사실 블루베리도 엄마가 저러다 썩겠다 해서  먹은거지 과일안주를 선호하진 않아요 ㅎㅎ",
          "original_form": "사실 블루베리도 엄마가 저러다 썩겠다 해서  먹은거지 과일안주를 선호하진 않아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 11:42"
        },
        {
          "id": "MDRW2100026471.1.25",
          "form": "그렇군요! 저도 짭조름한거 좋아해요 ㅎㅎ",
          "original_form": "그렇군요! 저도 짭조름한거 좋아해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 13:06"
        }
      ]
    }
