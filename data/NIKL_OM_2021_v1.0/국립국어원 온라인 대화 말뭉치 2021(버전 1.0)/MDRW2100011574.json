      "utterance": [
        {
          "id": "MDRW2100011574.1.1",
          "form": "역시 빨리 졸업해서 한밑천 땡긴 다음 가는게 나을 것 같다",
          "original_form": "역시 빨리 졸업해서 한밑천 땡긴 다음 가는게 나을 것 같다",
          "speaker_id": "2",
          "time": "20210824 14:08"
        },
        {
          "id": "MDRW2100011574.1.2",
          "form": "가서 배우면 재밌긴 하겠다",
          "original_form": "가서 배우면 재밌긴 하겠다",
          "speaker_id": "2",
          "time": "20210824 14:08"
        },
        {
          "id": "MDRW2100011574.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋ그치",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그치",
          "speaker_id": "1",
          "time": "20210824 14:08"
        },
        {
          "id": "MDRW2100011574.1.4",
          "form": "작가도",
          "original_form": "작가도",
          "speaker_id": "1",
          "time": "20210824 14:08"
        },
        {
          "id": "MDRW2100011574.1.5",
          "form": "해외쪽이훨씬대우도좋지않음?",
          "original_form": "해외쪽이훨씬대우도좋지않음?",
          "speaker_id": "1",
          "time": "20210824 14:08"
        },
        {
          "id": "MDRW2100011574.1.6",
          "form": "커리큘럼도그렇고",
          "original_form": "커리큘럼도그렇고",
          "speaker_id": "1",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100011574.1.7",
          "form": "오 작가는 거의 다 비슷할 걸 아마",
          "original_form": "오 작가는 거의 다 비슷할 걸 아마",
          "speaker_id": "2",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100011574.1.8",
          "form": "내가 해외 쪽을 다 찾아본게 아니라",
          "original_form": "내가 해외 쪽을 다 찾아본게 아니라",
          "speaker_id": "2",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100011574.1.9",
          "form": "미국은 거의 대부분이 한국보다 좋던데",
          "original_form": "미국은 거의 대부분이 한국보다 좋던데",
          "speaker_id": "1",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100011574.1.10",
          "form": "와 관련해서 찾아보니까 광고가 너무 많다",
          "original_form": "와 관련해서 찾아보니까 광고가 너무 많다",
          "speaker_id": "2",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100011574.1.11",
          "form": "편하게 좀 가고 싶다",
          "original_form": "편하게 좀 가고 싶다",
          "speaker_id": "2",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100011574.1.12",
          "form": "맞아. 어학원 광고 너무 많아",
          "original_form": "맞아. 어학원 광고 너무 많아",
          "speaker_id": "1",
          "time": "20210824 14:12"
        },
        {
          "id": "MDRW2100011574.1.13",
          "form": "실질적인정보도거의없고",
          "original_form": "실질적인정보도거의없고",
          "speaker_id": "1",
          "time": "20210824 14:12"
        },
        {
          "id": "MDRW2100011574.1.14",
          "form": "그니까 다 광고야",
          "original_form": "그니까 다 광고야",
          "speaker_id": "2",
          "time": "20210824 14:12"
        },
        {
          "id": "MDRW2100011574.1.15",
          "form": "대학 좀 가겠다는데 어휴",
          "original_form": "대학 좀 가겠다는데 어휴",
          "speaker_id": "2",
          "time": "20210824 14:12"
        },
        {
          "id": "MDRW2100011574.1.16",
          "form": "그래서교수나선배한테",
          "original_form": "그래서교수나선배한테",
          "speaker_id": "1",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100011574.1.17",
          "form": "물어봐야지",
          "original_form": "물어봐야지",
          "speaker_id": "1",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100011574.1.18",
          "form": "정확한정보가나오긴함",
          "original_form": "정확한정보가나오긴함",
          "speaker_id": "1",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100011574.1.19",
          "form": "아 그럼 상담할 때 여쭤봐야 겠네",
          "original_form": "아 그럼 상담할 때 여쭤봐야 겠네",
          "speaker_id": "2",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100011574.1.20",
          "form": "복학할때상담안했음?",
          "original_form": "복학할때상담안했음?",
          "speaker_id": "1",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100011574.1.21",
          "form": "상담 한참 전에 했음",
          "original_form": "상담 한참 전에 했음",
          "speaker_id": "2",
          "time": "20210824 14:13"
        },
        {
          "id": "MDRW2100011574.1.22",
          "form": "무슨상담했음?",
          "original_form": "무슨상담했음?",
          "speaker_id": "1",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100011574.1.23",
          "form": "왜복학하는지?",
          "original_form": "왜복학하는지?",
          "speaker_id": "1",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100011574.1.24",
          "form": "그냥 휴학하는 동안 잘 지냈는지",
          "original_form": "그냥 휴학하는 동안 잘 지냈는지",
          "speaker_id": "2",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100011574.1.25",
          "form": "학교 일정은 어떻게 될 것인지",
          "original_form": "학교 일정은 어떻게 될 것인지",
          "speaker_id": "2",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100011574.1.26",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ재밌었음?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ재밌었음?",
          "speaker_id": "1",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100011574.1.27",
          "form": "물오볼 거는 업ㄱ는지",
          "original_form": "물오볼 거는 업ㄱ는지",
          "speaker_id": "2",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100011574.1.28",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210824 14:14"
        },
        {
          "id": "MDRW2100011574.1.29",
          "form": "ㅋㅋㅋㅋ근데또상담해?",
          "original_form": "ㅋㅋㅋㅋ근데또상담해?",
          "speaker_id": "1",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011574.1.30",
          "form": "그냥 빨리 끝내고 싶었음",
          "original_form": "그냥 빨리 끝내고 싶었음",
          "speaker_id": "2",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011574.1.31",
          "form": "아마 2학년 되면 진로 상담 할 테니까",
          "original_form": "아마 2학년 되면 진로 상담 할 테니까",
          "speaker_id": "2",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011574.1.32",
          "form": "그거 말한 거임",
          "original_form": "그거 말한 거임",
          "speaker_id": "2",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011574.1.33",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011574.1.34",
          "form": "내후년쯤에",
          "original_form": "내후년쯤에",
          "speaker_id": "1",
          "time": "20210824 14:15"
        },
        {
          "id": "MDRW2100011574.1.35",
          "form": "그상담하겠네",
          "original_form": "그상담하겠네",
          "speaker_id": "1",
          "time": "20210824 14:15"
        }
      ]
