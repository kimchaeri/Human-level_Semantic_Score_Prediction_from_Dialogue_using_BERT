      "utterance": [
        {
          "id": "MDRW2100036380.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100036380.1.2",
          "form": "저녁은 드셨나요",
          "original_form": "저녁은 드셨나요",
          "speaker_id": "1",
          "time": "20210830 22:10"
        },
        {
          "id": "MDRW2100036380.1.3",
          "form": "안녕하세용ㅎㅎ 대답이 너무 늦었죠ㅋㅋㅋㅋ 저녁 열시엔 이미 저녁을 먹었었죹!!",
          "original_form": "안녕하세용ㅎㅎ 대답이 너무 늦었죠ㅋㅋㅋㅋ 저녁 열시엔 이미 저녁을 먹었었죹!!",
          "speaker_id": "2",
          "time": "20210831 09:15"
        },
        {
          "id": "MDRW2100036380.1.4",
          "form": "아침은 드셨나요",
          "original_form": "아침은 드셨나요",
          "speaker_id": "2",
          "time": "20210831 09:15"
        },
        {
          "id": "MDRW2100036380.1.5",
          "form": "아침은 원래 걸러서요",
          "original_form": "아침은 원래 걸러서요",
          "speaker_id": "1",
          "time": "20210831 09:16"
        },
        {
          "id": "MDRW2100036380.1.6",
          "form": "야식도 늦게 먹었구",
          "original_form": "야식도 늦게 먹었구",
          "speaker_id": "1",
          "time": "20210831 09:16"
        },
        {
          "id": "MDRW2100036380.1.7",
          "form": "님은요?ㅎㅎ",
          "original_form": "님은요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 09:16"
        },
        {
          "id": "MDRW2100036380.1.8",
          "form": "아하 아침 거르시는구나",
          "original_form": "아하 아침 거르시는구나",
          "speaker_id": "2",
          "time": "20210831 09:18"
        },
        {
          "id": "MDRW2100036380.1.9",
          "form": "저두 거르다가 챙겨먹어야할거같아서",
          "original_form": "저두 거르다가 챙겨먹어야할거같아서",
          "speaker_id": "2",
          "time": "20210831 09:18"
        },
        {
          "id": "MDRW2100036380.1.10",
          "form": "이제 몇개월째 아침으러",
          "original_form": "이제 몇개월째 아침으러",
          "speaker_id": "2",
          "time": "20210831 09:19"
        },
        {
          "id": "MDRW2100036380.1.11",
          "form": "계란 두알을 먺어요",
          "original_form": "계란 두알을 먺어요",
          "speaker_id": "2",
          "time": "20210831 09:19"
        },
        {
          "id": "MDRW2100036380.1.12",
          "form": "먹어요",
          "original_form": "먹어요",
          "speaker_id": "2",
          "time": "20210831 09:19"
        },
        {
          "id": "MDRW2100036380.1.13",
          "form": "아침 챙기니 달라진게 느껴지세요?",
          "original_form": "아침 챙기니 달라진게 느껴지세요?",
          "speaker_id": "1",
          "time": "20210831 09:19"
        },
        {
          "id": "MDRW2100036380.1.14",
          "form": "네 좀 든든하긴 하더라구요",
          "original_form": "네 좀 든든하긴 하더라구요",
          "speaker_id": "2",
          "time": "20210831 09:23"
        },
        {
          "id": "MDRW2100036380.1.15",
          "form": "계란은 반숙? 완숙?",
          "original_form": "계란은 반숙? 완숙?",
          "speaker_id": "1",
          "time": "20210831 09:23"
        },
        {
          "id": "MDRW2100036380.1.16",
          "form": "완숙 먹어요 ㅋㅋㅋ",
          "original_form": "완숙 먹어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:24"
        },
        {
          "id": "MDRW2100036380.1.17",
          "form": "앗 프라이도 완숙이신가요",
          "original_form": "앗 프라이도 완숙이신가요",
          "speaker_id": "1",
          "time": "20210831 09:25"
        },
        {
          "id": "MDRW2100036380.1.18",
          "form": "아니요 보통 반숙 좋아하는데",
          "original_form": "아니요 보통 반숙 좋아하는데",
          "speaker_id": "2",
          "time": "20210831 09:25"
        },
        {
          "id": "MDRW2100036380.1.19",
          "form": "아침에 책상에서 먹는데 흘릴까봐 ㅋㅋㅋ",
          "original_form": "아침에 책상에서 먹는데 흘릴까봐 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 09:25"
        },
        {
          "id": "MDRW2100036380.1.20",
          "form": "아ㅎㅎ반숙이 다 좋은데 그런 맹점이",
          "original_form": "아ㅎㅎ반숙이 다 좋은데 그런 맹점이",
          "speaker_id": "1",
          "time": "20210831 09:27"
        },
        {
          "id": "MDRW2100036380.1.21",
          "form": "감동란 좋아하시나요",
          "original_form": "감동란 좋아하시나요",
          "speaker_id": "1",
          "time": "20210831 09:27"
        },
        {
          "id": "MDRW2100036380.1.22",
          "form": "네네 맛있더라구요!!",
          "original_form": "네네 맛있더라구요!!",
          "speaker_id": "2",
          "time": "20210831 10:00"
        },
        {
          "id": "MDRW2100036380.1.23",
          "form": "감동란 처음 먹고 진짜 감동이었죠ㅎ",
          "original_form": "감동란 처음 먹고 진짜 감동이었죠ㅎ",
          "speaker_id": "1",
