      "utterance": [
        {
          "id": "MDRW2100036268.1.1",
          "form": "디저트도 좋아하시나요 ㅎㅎ",
          "original_form": "디저트도 좋아하시나요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:36"
        },
        {
          "id": "MDRW2100036268.1.2",
          "form": "넹넹! 최애는 케이크 입니닷",
          "original_form": "넹넹! 최애는 케이크 입니닷",
          "speaker_id": "1",
          "time": "20210830 20:42"
        },
        {
          "id": "MDRW2100036268.1.3",
          "form": "한때는 마카롱이였어용!",
          "original_form": "한때는 마카롱이였어용!",
          "speaker_id": "1",
          "time": "20210830 20:42"
        },
        {
          "id": "MDRW2100036268.1.4",
          "form": "마카롱 ㅜㅜ 먹고싶네요 ...",
          "original_form": "마카롱 ㅜㅜ 먹고싶네요 ...",
          "speaker_id": "2",
          "time": "20210830 20:43"
        },
        {
          "id": "MDRW2100036268.1.5",
          "form": "최애 케이크가 뭔가요!",
          "original_form": "최애 케이크가 뭔가요!",
          "speaker_id": "2",
          "time": "20210830 20:43"
        },
        {
          "id": "MDRW2100036268.1.6",
          "form": "저도 한 케이크 합니다 ..",
          "original_form": "저도 한 케이크 합니다 ..",
          "speaker_id": "2",
          "time": "20210830 20:44"
        },
        {
          "id": "MDRW2100036268.1.7",
          "form": "티라미스랑 치즈랑 블루베리용!",
          "original_form": "티라미스랑 치즈랑 블루베리용!",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100036268.1.8",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100036268.1.9",
          "form": "저 치즈요 치즈 !!!",
          "original_form": "저 치즈요 치즈 !!!",
          "speaker_id": "2",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100036268.1.10",
          "form": "아 과일 얹어진건 딸기요 ㅎㅎㅎ",
          "original_form": "아 과일 얹어진건 딸기요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 20:47"
        },
        {
          "id": "MDRW2100036268.1.11",
          "form": "치즈!!!!!! 진짜 맛있죵",
          "original_form": "치즈!!!!!! 진짜 맛있죵",
          "speaker_id": "1",
          "time": "20210830 20:53"
        },
        {
          "id": "MDRW2100036268.1.12",
          "form": "어디 브랜드 좋아하세요?",
          "original_form": "어디 브랜드 좋아하세요?",
          "speaker_id": "1",
          "time": "20210830 20:54"
        },
        {
          "id": "MDRW2100036268.1.13",
          "form": "저 스벅 바스크치즈케익도 좋아허구요!",
          "original_form": "저 스벅 바스크치즈케익도 좋아허구요!",
          "speaker_id": "2",
          "time": "20210830 20:57"
        },
        {
          "id": "MDRW2100036268.1.14",
          "form": "투썸 레드벨벳이 최앱니다 .. 치즈맛짱 ..",
          "original_form": "투썸 레드벨벳이 최앱니다 .. 치즈맛짱 ..",
          "speaker_id": "2",
          "time": "20210830 20:57"
        },
        {
          "id": "MDRW2100036268.1.15",
          "form": "투썸.. 제 최애",
          "original_form": "투썸.. 제 최애{emoji:♡}",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100036268.1.16",
          "form": "스벅에 바스크가 있었다니",
          "original_form": "스벅에 바스크가 있었다니",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100036268.1.17",
          "form": "저 왜 몰랐죠..",
          "original_form": "저 왜 몰랐죠..",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100036268.1.18",
          "form": "스벅에서 잘 안파는거 같아요 ㅜㅡㆍ 저도 가끔씩 봐요 ...",
          "original_form": "스벅에서 잘 안파는거 같아요 ㅜㅡㆍ 저도 가끔씩 봐요 ...",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100036268.1.19",
          "form": "투썸 ! 뭐가 젤 최애세요 ㅎㅎ ?",
          "original_form": "투썸 ! 뭐가 젤 최애세요 ㅎㅎ ?",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100036268.1.20",
          "form": "티라미수인가요 ㅎㅎ",
          "original_form": "티라미수인가요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 21:29"
        },
        {
          "id": "MDRW2100036268.1.21",
          "form": "넹ㅋㅋ 체리 올라간 초코도 맛있구..",
          "original_form": "넹ㅋㅋ 체리 올라간 초코도 맛있구..",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036268.1.22",
          "form": "거긴 다 맛있더라구요",
          "original_form": "거긴 다 맛있더라구요",
          "speaker_id": "1",
          "time": "20210830 21:38"
        },
        {
          "id": "MDRW2100036268.1.23",
          "form": "아 체리초코",
          "original_form": "아 체리초코",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100036268.1.24",
          "form": "전 슬퍼요.. 딸기 없어지면 체리 올라가서 ..",
          "original_form": "전 슬퍼요.. 딸기 없어지면 체리 올라가서 ..",
          "speaker_id": "2",
          "time": "20210830 21:41"
        },
        {
          "id": "MDRW2100036268.1.25",
          "form": "아.. 세상에...",
          "original_form": "아.. 세상에...",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100036268.1.26",
          "form": "그렇게 바뀌던 거였군요...",
          "original_form": "그렇게 바뀌던 거였군요...",
          "speaker_id": "1",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100036268.1.27",
          "form": "네... 체리도 좋지만.,",
          "original_form": "네... 체리도 좋지만.,",
          "speaker_id": "2",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100036268.1.28",
          "form": "슬퍼욬ㅋㅋ 심지어 홀케이큰데 체리면..",
          "original_form": "슬퍼욬ㅋㅋ 심지어 홀케이큰데 체리면..",
          "speaker_id": "2",
          "time": "20210830 21:57"
        }
