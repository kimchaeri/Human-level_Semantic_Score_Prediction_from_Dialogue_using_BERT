      "utterance": [
        {
          "id": "MDRW2100045756.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210908 06:40"
        },
        {
          "id": "MDRW2100045756.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210908 06:41"
        },
        {
          "id": "MDRW2100045756.1.3",
          "form": "아침메뉴 정하셨나요?",
          "original_form": "아침메뉴 정하셨나요?",
          "speaker_id": "2",
          "time": "20210908 06:41"
        },
        {
          "id": "MDRW2100045756.1.4",
          "form": "도넛먹으려구요",
          "original_form": "도넛먹으려구요",
          "speaker_id": "1",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045756.1.5",
          "form": "정하셨어요?",
          "original_form": "정하셨어요?",
          "speaker_id": "1",
          "time": "20210908 06:44"
        },
        {
          "id": "MDRW2100045756.1.6",
          "form": "도넛 맛있겠어요!",
          "original_form": "도넛 맛있겠어요!",
          "speaker_id": "2",
          "time": "20210908 06:45"
        },
        {
          "id": "MDRW2100045756.1.7",
          "form": "어제 던킨에서 미니도넛 사왔거든요",
          "original_form": "어제 던킨에서 미니도넛 사왔거든요",
          "speaker_id": "1",
          "time": "20210908 06:45"
        },
        {
          "id": "MDRW2100045756.1.8",
          "form": "저는 아마 샌드위치 먹을 것 같아요!",
          "original_form": "저는 아마 샌드위치 먹을 것 같아요!",
          "speaker_id": "2",
          "time": "20210908 06:45"
        },
        {
          "id": "MDRW2100045756.1.9",
          "form": "샌드위치도 맛나겠어요",
          "original_form": "샌드위치도 맛나겠어요",
          "speaker_id": "1",
          "time": "20210908 06:46"
        },
        {
          "id": "MDRW2100045756.1.10",
          "form": "던킨에 미니도넛팩이라길래",
          "original_form": "던킨에 미니도넛팩이라길래",
          "speaker_id": "1",
          "time": "20210908 06:46"
        },
        {
          "id": "MDRW2100045756.1.11",
          "form": "그래도 비싸곗지 했는데",
          "original_form": "그래도 비싸곗지 했는데",
          "speaker_id": "1",
          "time": "20210908 06:46"
        },
        {
          "id": "MDRW2100045756.1.12",
          "form": "아 가끔 던킨 가게 앞에서 놓고 파는 그건가요?",
          "original_form": "아 가끔 던킨 가게 앞에서 놓고 파는 그건가요?",
          "speaker_id": "2",
          "time": "20210908 06:47"
        },
        {
          "id": "MDRW2100045756.1.13",
          "form": "생각보다 싸더라구요",
          "original_form": "생각보다 싸더라구요",
          "speaker_id": "1",
          "time": "20210908 06:47"
        },
        {
          "id": "MDRW2100045756.1.14",
          "form": "미니도넛이라 도넛 6개에 3300원이요",
          "original_form": "미니도넛이라 도넛 6개에 3300원이요",
          "speaker_id": "1",
          "time": "20210908 06:47"
        },
        {
          "id": "MDRW2100045756.1.15",
          "form": "해피앱에서 천원 할인쿠폰 걸려서 그거 적용하고",
          "original_form": "해피앱에서 천원 할인쿠폰 걸려서 그거 적용하고",
          "speaker_id": "1",
          "time": "20210908 06:47"
        },
        {
          "id": "MDRW2100045756.1.16",
          "form": "2300원에 사왔어요",
          "original_form": "2300원에 사왔어요",
          "speaker_id": "1",
          "time": "20210908 06:47"
        },
        {
          "id": "MDRW2100045756.1.17",
          "form": "던킨앞에는 큰것들아닌가요?",
          "original_form": "던킨앞에는 큰것들아닌가요?",
          "speaker_id": "1",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045756.1.18",
          "form": "앗 그런가요 미니가 따로 있는거군요!",
          "original_form": "앗 그런가요 미니가 따로 있는거군요!",
          "speaker_id": "2",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045756.1.19",
          "form": "작아서 먹기애도 편하고",
          "original_form": "작아서 먹기애도 편하고",
          "speaker_id": "1",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045756.1.20",
          "form": "할인까지 알뜰하게...!",
          "original_form": "할인까지 알뜰하게...!",
          "speaker_id": "2",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045756.1.21",
          "form": "아침으로 딱 좋을 것 같아요!",
          "original_form": "아침으로 딱 좋을 것 같아요!",
          "speaker_id": "2",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045756.1.22",
          "form": "이번에 마블인가 그거땜에 나온건지",
          "original_form": "이번에 마블인가 그거땜에 나온건지",
          "speaker_id": "1",
          "time": "20210908 06:48"
        },
        {
          "id": "MDRW2100045756.1.23",
          "form": "맞아요 사이즈도 손바닥만한데",
          "original_form": "맞아요 사이즈도 손바닥만한데",
          "speaker_id": "1",
          "time": "20210908 06:49"
        },
        {
          "id": "MDRW2100045756.1.24",
          "form": "가성비 갑인거 같아요",
          "original_form": "가성비 갑인거 같아요",
          "speaker_id": "1",
          "time": "20210908 06:49"
        }
      ]
    }
