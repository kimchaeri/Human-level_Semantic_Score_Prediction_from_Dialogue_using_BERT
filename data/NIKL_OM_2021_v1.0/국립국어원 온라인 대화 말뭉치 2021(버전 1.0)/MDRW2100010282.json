      "utterance": [
        {
          "id": "MDRW2100010282.1.1",
          "form": "마카롱맛집 가보신적있나용?",
          "original_form": "마카롱맛집 가보신적있나용?",
          "speaker_id": "1",
          "time": "20210823 16:24"
        },
        {
          "id": "MDRW2100010282.1.2",
          "form": "마카롱을 사실 좋아하지 않아서요ㅎㅎ",
          "original_form": "마카롱을 사실 좋아하지 않아서요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 16:30"
        },
        {
          "id": "MDRW2100010282.1.3",
          "form": "마카롱 좋아하시나요?",
          "original_form": "마카롱 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210823 16:30"
        },
        {
          "id": "MDRW2100010282.1.4",
          "form": "네! 요즘 설탕줄이느라 못먹지만요ㅠ",
          "original_form": "네! 요즘 설탕줄이느라 못먹지만요ㅠ",
          "speaker_id": "1",
          "time": "20210823 16:32"
        },
        {
          "id": "MDRW2100010282.1.5",
          "form": "좋아하는 음식있으세요?",
          "original_form": "좋아하는 음식있으세요?",
          "speaker_id": "1",
          "time": "20210823 16:32"
        },
        {
          "id": "MDRW2100010282.1.6",
          "form": "마카롱도 맛있는데는 엄청 맛있나봐요",
          "original_form": "마카롱도 맛있는데는 엄청 맛있나봐요",
          "speaker_id": "2",
          "time": "20210823 16:34"
        },
        {
          "id": "MDRW2100010282.1.7",
          "form": "전 맛없는곳만 갔는지 ㅎㄹ",
          "original_form": "전 맛없는곳만 갔는지 ㅎㄹ",
          "speaker_id": "2",
          "time": "20210823 16:34"
        },
        {
          "id": "MDRW2100010282.1.8",
          "form": "이런!",
          "original_form": "이런!",
          "speaker_id": "1",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010282.1.9",
          "form": "마카롱은 필링이 안느끼한게 맛있어용",
          "original_form": "마카롱은 필링이 안느끼한게 맛있어용",
          "speaker_id": "1",
          "time": "20210823 16:35"
        },
        {
          "id": "MDRW2100010282.1.10",
          "form": "추천해주실 맛집 있나요?",
          "original_form": "추천해주실 맛집 있나요?",
          "speaker_id": "2",
          "time": "20210823 16:36"
        },
        {
          "id": "MDRW2100010282.1.11",
          "form": "마리웨일과 패션5가 입문용으로 좋아용",
          "original_form": "마리웨일과 패션5가 입문용으로 좋아용",
          "speaker_id": "1",
          "time": "20210823 16:37"
        },
        {
          "id": "MDRW2100010282.1.12",
          "form": "체인인가요?",
          "original_form": "체인인가요?",
          "speaker_id": "2",
          "time": "20210823 16:39"
        },
        {
          "id": "MDRW2100010282.1.13",
          "form": "네네! 체인이 접하기 쉬우니깐요",
          "original_form": "네네! 체인이 접하기 쉬우니깐요",
          "speaker_id": "1",
          "time": "20210823 16:39"
        },
        {
          "id": "MDRW2100010282.1.14",
          "form": "근처있나 함 찾아볼게요 ㅎ",
          "original_form": "근처있나 함 찾아볼게요 ㅎ",
          "speaker_id": "2",
          "time": "20210823 16:40"
        },
        {
          "id": "MDRW2100010282.1.15",
          "form": "좋아요ㅎ 마리웨일은 기본마카롱 추천해요!",
          "original_form": "좋아요ㅎ 마리웨일은 기본마카롱 추천해요!",
          "speaker_id": "1",
          "time": "20210823 16:40"
        },
        {
          "id": "MDRW2100010282.1.16",
          "form": "무슨맛이 맛나나요?",
          "original_form": "무슨맛이 맛나나요?",
          "speaker_id": "2",
          "time": "20210823 16:41"
        },
        {
          "id": "MDRW2100010282.1.17",
          "form": "달달하고 상큼해용",
          "original_form": "달달하고 상큼해용",
          "speaker_id": "1",
          "time": "20210823 16:42"
        }
      ]
    }
