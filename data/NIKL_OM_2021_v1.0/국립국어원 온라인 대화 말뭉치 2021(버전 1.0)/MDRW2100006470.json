      "utterance": [
        {
          "id": "MDRW2100006470.1.1",
          "form": "네 그거 사야돼요",
          "original_form": "네 그거 사야돼요",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100006470.1.2",
          "form": "맞아요 안그래도 사려했는데요",
          "original_form": "맞아요 안그래도 사려했는데요",
          "speaker_id": "2",
          "time": "20210908 00:41"
        },
        {
          "id": "MDRW2100006470.1.3",
          "form": "주말엔  농협이 문을 닫아서",
          "original_form": "주말엔  농협이 문을 닫아서",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100006470.1.4",
          "form": "그냥 인터넷으로 시켜줘",
          "original_form": "그냥 인터넷으로 시켜줘",
          "speaker_id": "1",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100006470.1.5",
          "form": "네 지금 싼 가격 순으로 보고 있어요",
          "original_form": "네 지금 싼 가격 순으로 보고 있어요",
          "speaker_id": "2",
          "time": "20210908 00:43"
        },
        {
          "id": "MDRW2100006470.1.6",
          "form": "인터넷에서  사는 게 밖에서 사는 거 보다 많이 싼가",
          "original_form": "인터넷에서  사는 게 밖에서 사는 거 보다 많이 싼가",
          "speaker_id": "1",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100006470.1.7",
          "form": "네 진짜 차이 많이나요",
          "original_form": "네 진짜 차이 많이나요",
          "speaker_id": "2",
          "time": "20210908 00:44"
        },
        {
          "id": "MDRW2100006470.1.8",
          "form": "그럼 인터넷에서 다른 물건도 같이 또 같이 주문할까",
          "original_form": "그럼 인터넷에서 다른 물건도 같이 또 같이 주문할까",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100006470.1.9",
          "form": "같이 주문해여!!!!",
          "original_form": "같이 주문해여!!!!",
          "speaker_id": "2",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100006470.1.10",
          "form": "그럼 더 필요한 걸 생각해 볼게",
          "original_form": "그럼 더 필요한 걸 생각해 볼게",
          "speaker_id": "1",
          "time": "20210908 00:46"
        },
        {
          "id": "MDRW2100006470.1.11",
          "form": "네 저도 사야될거 생각해볼게요",
          "original_form": "네 저도 사야될거 생각해볼게요",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100006470.1.12",
          "form": "농기구도 다시 한 번씩 바꿀 때가 됐어",
          "original_form": "농기구도 다시 한 번씩 바꿀 때가 됐어",
          "speaker_id": "1",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100006470.1.13",
          "form": "맞아요 다 낡았어요",
          "original_form": "맞아요 다 낡았어요",
          "speaker_id": "2",
          "time": "20210908 00:47"
        },
        {
          "id": "MDRW2100006470.1.14",
          "form": "벌써 몇 년 썼으니까",
