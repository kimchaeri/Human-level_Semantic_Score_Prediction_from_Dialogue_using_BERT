      "utterance": [
        {
          "id": "MDRW2100036012.1.1",
          "form": "커피 어떤거 좋아하세용?",
          "original_form": "커피 어떤거 좋아하세용?",
          "speaker_id": "2",
          "time": "20210830 14:07"
        },
        {
          "id": "MDRW2100036012.1.2",
          "form": "저 카페모카나 아메리카노 먹어요",
          "original_form": "저 카페모카나 아메리카노 먹어요",
          "speaker_id": "1",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100036012.1.3",
          "form": "단거면 모카\n뭐 먹기 부담스러우면 아메리카노요 ㅋㅋㅋ",
          "original_form": "단거면 모카\n뭐 먹기 부담스러우면 아메리카노요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100036012.1.4",
          "form": "전 커피를 잘 못마시지만",
          "original_form": "전 커피를 잘 못마시지만",
          "speaker_id": "2",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100036012.1.5",
          "form": "모카향은 정말 좋아해용",
          "original_form": "모카향은 정말 좋아해용",
          "speaker_id": "2",
          "time": "20210830 14:11"
        },
        {
          "id": "MDRW2100036012.1.6",
          "form": "맞아요 모카향 그윽해요",
          "original_form": "맞아요 모카향 그윽해요",
          "speaker_id": "1",
          "time": "20210830 17:02"
        },
        {
          "id": "MDRW2100036012.1.7",
          "form": "저도 잠 잘 못자서 커피 잘 안먹는데 요새 너무졸려서 마시고있어요 ㅋㅋㅋ",
          "original_form": "저도 잠 잘 못자서 커피 잘 안먹는데 요새 너무졸려서 마시고있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 17:02"
        },
        {
          "id": "MDRW2100036012.1.8",
          "form": "저도 엄청 피곤할때 마시면 안졸리더라구요ㅋㅋㅋ 커피의 힘",
          "original_form": "저도 엄청 피곤할때 마시면 안졸리더라구요ㅋㅋㅋ 커피의 힘",
          "speaker_id": "2",
          "time": "20210830 18:43"
        },
        {
          "id": "MDRW2100036012.1.9",
          "form": "포션빨듯이..ㅋㅋㅋㅋㅋ",
          "original_form": "포션빨듯이..ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100036012.1.10",
          "form": "현대인의 필수품같습니다",
          "original_form": "현대인의 필수품같습니다",
          "speaker_id": "1",
          "time": "20210830 21:26"
        },
        {
          "id": "MDRW2100036012.1.11",
          "form": "맞아요 약간 생명수같다랄까요?",
          "original_form": "맞아요 약간 생명수같다랄까요?",
          "speaker_id": "2",
          "time": "20210830 21:28"
        },
        {
          "id": "MDRW2100036012.1.12",
          "form": "밤엔 불면증약 먹고 낮엔 아메리카노 먹는 현대인ㅋㅋㅋㅋ",
          "original_form": "밤엔 불면증약 먹고 낮엔 아메리카노 먹는 현대인ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100036012.1.13",
          "form": "으허허ㅋㅋㅋ",
          "original_form": "으허허ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100036012.1.14",
          "form": "꼭 커피 말고도",
          "original_form": "꼭 커피 말고도",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100036012.1.15",
          "form": "중간에 달달한 사탕이나",
          "original_form": "중간에 달달한 사탕이나",
          "speaker_id": "2",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100036012.1.16",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210830 22:35"
        },
        {
          "id": "MDRW2100036012.1.17",
          "form": "마카롱같은 간식 먹는것도 좋아요!",
          "original_form": "마카롱같은 간식 먹는것도 좋아요!",
          "speaker_id": "2",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100036012.1.18",
          "form": "저 포도당캔디 박스채사놓고먹어요 ㅋㅋㅋㅋ",
          "original_form": "저 포도당캔디 박스채사놓고먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100036012.1.19",
          "form": "마카롱! 맛잇겟네요",
          "original_form": "마카롱! 맛잇겟네요",
          "speaker_id": "1",
          "time": "20210830 22:36"
        },
        {
          "id": "MDRW2100036012.1.20",
          "form": "엇 그거 약국에서 파는거죠?ㅋㅋㅋㅋ",
          "original_form": "엇 그거 약국에서 파는거죠?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 22:36"
        }
      ]
