      "utterance": [
        {
          "id": "MDRW2100023983.1.1",
          "form": "쇼핑과 상품으로 앨범얘기해요ㅎㅋㅋ",
          "original_form": "쇼핑과 상품으로 앨범얘기해요ㅎㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 20:52"
        },
        {
          "id": "MDRW2100023983.1.2",
          "form": "네ㅋㅋㄱ이게편할거같아요ㅋㅋㅋㅋ",
          "original_form": "네ㅋㅋㄱ이게편할거같아요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:53"
        },
        {
          "id": "MDRW2100023983.1.3",
          "form": "앨범구매하시나용?",
          "original_form": "앨범구매하시나용?",
          "speaker_id": "2",
          "time": "20210830 20:53"
        },
        {
          "id": "MDRW2100023983.1.4",
          "form": "옛날에 방탄좋아할땐ㅋㅋ 열심히 샀는데 그 이후로는 한번도 안샀어요",
          "original_form": "옛날에 방탄좋아할땐ㅋㅋ 열심히 샀는데 그 이후로는 한번도 안샀어요",
          "speaker_id": "1",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023983.1.5",
          "form": "name2님은 앨범사시나요?",
          "original_form": "&name2&님은 앨범사시나요?",
          "speaker_id": "1",
          "time": "20210830 20:55"
        },
        {
          "id": "MDRW2100023983.1.6",
          "form": "네네 사고잇어요ㅋㅋㅋ모아두었습니다ㅋㅋㅋ\r\n갈아타면 정리 좀 하구..허허",
          "original_form": "네네 사고잇어요ㅋㅋㅋ모아두었습니다ㅋㅋㅋ\r\n갈아타면 정리 좀 하구..허허",
          "speaker_id": "2",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023983.1.7",
          "form": "앨범사셨던거그냥두시나용?",
          "original_form": "앨범사셨던거그냥두시나용?",
          "speaker_id": "2",
          "time": "20210830 21:01"
        },
        {
          "id": "MDRW2100023983.1.8",
          "form": "그냥 뒀다가 어느 순간 안되겠다싶어서",
          "original_form": "그냥 뒀다가 어느 순간 안되겠다싶어서",
          "speaker_id": "1",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100023983.1.9",
          "form": "나눔도 하고 당근에 싸게 팔기도 했어요ㅋㅋㅋ",
          "original_form": "나눔도 하고 당근에 싸게 팔기도 했어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:02"
        },
        {
          "id": "MDRW2100023983.1.10",
          "form": "뭔가 후회는 없는데 보관도 힘들고..",
          "original_form": "뭔가 후회는 없는데 보관도 힘들고..",
          "speaker_id": "1",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100023983.1.11",
          "form": "방탄앨범 다 팔고난 후엔 앨범을 안사려고 노력중이에요ㅋㅋ",
          "original_form": "방탄앨범 다 팔고난 후엔 앨범을 안사려고 노력중이에요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:03"
        },
        {
          "id": "MDRW2100023983.1.12",
          "form": "맞아요ㅋㄱㅋ이게 한번보관하기시작하면 답이없어지고막ㅋㅋㄱ공간차지어마어마하지요...",
          "original_form": "맞아요ㅋㄱㅋ이게 한번보관하기시작하면 답이없어지고막ㅋㅋㄱ공간차지어마어마하지요...",
          "speaker_id": "2",
          "time": "20210830 21:06"
        },
        {
          "id": "MDRW2100023983.1.13",
          "form": "네ㅜㅋㅋㅋㅋ 부모님이 많이 싫어하시기도 하고요..ㅋㅋ",
          "original_form": "네ㅜㅋㅋㅋㅋ 부모님이 많이 싫어하시기도 하고요..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:06"
        },
        {
          "id": "MDRW2100023983.1.14",
          "form": "아ㅋㅋㅋ저만그런거아니네요ㅋㅋㅋㅋㅋ\r\n그만사라하심ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋ저만그런거아니네요ㅋㅋㅋㅋㅋ\r\n그만사라하심ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100023983.1.15",
          "form": "그치만 저는 포스터도 붙여놓는 꿋꿋한사람..",
          "original_form": "그치만 저는 포스터도 붙여놓는 꿋꿋한사람..",
          "speaker_id": "2",
          "time": "20210830 21:07"
        },
        {
          "id": "MDRW2100023983.1.16",
          "form": "ㅋㅋ아 포스터하니까 포스터 안판게 생각나네요..",
          "original_form": "ㅋㅋ아 포스터하니까 포스터 안판게 생각나네요..",
          "speaker_id": "1",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100023983.1.17",
          "form": "방탄 포스터받으려고 퓨마 패딩샀던 기억이..ㅋㅋ",
          "original_form": "방탄 포스터받으려고 퓨마 패딩샀던 기억이..ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:08"
        },
        {
          "id": "MDRW2100023983.1.18",
          "form": "악ㅋㅋㅋㅋㅋㄱ겨울옷장만하셨네요..!",
          "original_form": "악ㅋㅋㅋㅋㅋㄱ겨울옷장만하셨네요..!",
          "speaker_id": "2",
          "time": "20210830 21:09"
        },
        {
          "id": "MDRW2100023983.1.19",
          "form": "넵ㅋㅋ",
          "original_form": "넵ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100023983.1.20",
          "form": "저도워너원팔때 겁나삿습니다..안산게없습니다...",
          "original_form": "저도워너원팔때 겁나삿습니다..안산게없습니다...",
          "speaker_id": "2",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100023983.1.21",
          "form": "광고한건거의다샀습니다...",
          "original_form": "광고한건거의다샀습니다...",
          "speaker_id": "2",
          "time": "20210830 21:10"
        },
        {
          "id": "MDRW2100023983.1.22",
          "form": "워너원이 네이쳐광고했었나요?",
          "original_form": "워너원이 네이쳐광고했었나요?",
          "speaker_id": "1",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100023983.1.23",
          "form": "아니용ㅋㅋ이니스프리했었어요!",
          "original_form": "아니용ㅋㅋ이니스프리했었어요!",
          "speaker_id": "2",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100023983.1.24",
          "form": "브마받으려고줄섰죠..ㅋㅋㅋㅋ",
          "original_form": "브마받으려고줄섰죠..ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100023983.1.25",
          "form": "앗ㅋㅋ 이니스프리였군요",
          "original_form": "앗ㅋㅋ 이니스프리였군요",
          "speaker_id": "1",
          "time": "20210830 21:11"
        },
        {
          "id": "MDRW2100023983.1.26",
          "form": "주변에 그 얼굴이 아래에 있는 팩트?",
          "original_form": "주변에 그 얼굴이 아래에 있는 팩트?",
          "speaker_id": "1",
          "time": "20210830 21:12"
        },
        {
          "id": "MDRW2100023983.1.27",
          "form": "한명씩 다 갖고 있었는데...추억이네요",
          "original_form": "한명씩 다 갖고 있었는데...추억이네요",
          "speaker_id": "1",
          "time": "20210830 21:12"
        },
        {
          "id": "MDRW2100023983.1.28",
          "form": "와그런것도있었어요?!",
          "original_form": "와그런것도있었어요?!",
          "speaker_id": "2",
          "time": "20210830 21:13"
        },
        {
          "id": "MDRW2100023983.1.29",
          "form": "넵 사진이 아래있었나 그래서",
          "original_form": "넵 사진이 아래있었나 그래서",
          "speaker_id": "1",
          "time": "20210830 21:15"
        },
        {
          "id": "MDRW2100023983.1.30",
          "form": "엄청 많이들 샀던것같아요",
          "original_form": "엄청 많이들 샀던것같아요",
          "speaker_id": "1",
          "time": "20210830 21:15"
        }
      ]
    }
