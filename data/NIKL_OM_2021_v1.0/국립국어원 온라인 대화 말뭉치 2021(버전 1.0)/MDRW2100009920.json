      "utterance": [
        {
          "id": "MDRW2100009920.1.1",
          "form": "안녕하세요 오늘 비오죠ㅠ",
          "original_form": "안녕하세요 오늘 비오죠ㅠ",
          "speaker_id": "1",
          "time": "20210823 11:42"
        },
        {
          "id": "MDRW2100009920.1.2",
          "form": "네ㅠ",
          "original_form": "네ㅠ",
          "speaker_id": "2",
          "time": "20210823 11:44"
        },
        {
          "id": "MDRW2100009920.1.3",
          "form": "비가 오네요",
          "original_form": "비가 오네요",
          "speaker_id": "2",
          "time": "20210823 11:44"
        },
        {
          "id": "MDRW2100009920.1.4",
          "form": "비오는 날 어때요",
          "original_form": "비오는 날 어때요",
          "speaker_id": "2",
          "time": "20210823 11:45"
        },
        {
          "id": "MDRW2100009920.1.5",
          "form": "전 별로 안좋아하는 편이에요",
          "original_form": "전 별로 안좋아하는 편이에요",
          "speaker_id": "1",
          "time": "20210823 16:45"
        },
        {
          "id": "MDRW2100009920.1.6",
          "form": "축축한걸 안좋아해서요",
          "original_form": "축축한걸 안좋아해서요",
          "speaker_id": "1",
          "time": "20210823 16:45"
        },
        {
          "id": "MDRW2100009920.1.7",
          "form": "지원님은요?",
          "original_form": "지원님은요?",
          "speaker_id": "1",
          "time": "20210823 16:46"
        },
        {
          "id": "MDRW2100009920.1.8",
          "form": "저도 비오는 날은 선호하지 않아요",
          "original_form": "저도 비오는 날은 선호하지 않아요",
          "speaker_id": "2",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100009920.1.9",
          "form": "약간 축축하고 습하고..",
          "original_form": "약간 축축하고 습하고..",
          "speaker_id": "2",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100009920.1.10",
          "form": "우산써도 젖고 그래서..ㅎ",
          "original_form": "우산써도 젖고 그래서..ㅎ",
          "speaker_id": "2",
          "time": "20210823 21:15"
        },
        {
          "id": "MDRW2100009920.1.11",
          "form": "좋아하는 계절있나요",
          "original_form": "좋아하는 계절있나요",
          "speaker_id": "2",
          "time": "20210823 21:16"
        },
        {
          "id": "MDRW2100009920.1.12",
          "form": "저는 가을 겨울 좋아해요",
          "original_form": "저는 가을 겨울 좋아해요",
          "speaker_id": "1",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100009920.1.13",
          "form": "더운거보다 추운게 낫거든요",
          "original_form": "더운거보다 추운게 낫거든요",
          "speaker_id": "1",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100009920.1.14",
          "form": "가을 겨울 옷이 좋기도 하고요",
          "original_form": "가을 겨울 옷이 좋기도 하고요",
          "speaker_id": "1",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100009920.1.15",
          "form": "맞아요 더운건 진짜 힘들더라구요",
          "original_form": "맞아요 더운건 진짜 힘들더라구요",
          "speaker_id": "2",
          "time": "20210823 21:28"
        },
        {
          "id": "MDRW2100009920.1.16",
          "form": "겨울 좋아하시면 눈 좋아하시나요",
          "original_form": "겨울 좋아하시면 눈 좋아하시나요",
          "speaker_id": "2",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100009920.1.17",
          "form": "네네! 저 눈 내리는거 보면",
          "original_form": "네네! 저 눈 내리는거 보면",
          "speaker_id": "1",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100009920.1.18",
          "form": "바로 뛰쳐나가요",
          "original_form": "바로 뛰쳐나가요",
          "speaker_id": "1",
          "time": "20210823 21:29"
        },
        {
          "id": "MDRW2100009920.1.19",
          "form": "약간 눈 오면 설레는 그게 있는거 같아요 그쵸",
          "original_form": "약간 눈 오면 설레는 그게 있는거 같아요 그쵸",
          "speaker_id": "2",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100009920.1.20",
          "form": "눈 오시면 눈사람도 만드시나요",
          "original_form": "눈 오시면 눈사람도 만드시나요",
          "speaker_id": "2",
          "time": "20210823 21:30"
        },
        {
          "id": "MDRW2100009920.1.21",
          "form": "맞아요맞아요",
          "original_form": "맞아요맞아요",
          "speaker_id": "1",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100009920.1.22",
          "form": "눈사람은 항상 만들려고 노력하는데",
          "original_form": "눈사람은 항상 만들려고 노력하는데",
          "speaker_id": "1",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100009920.1.23",
          "form": "뭉치는 눈이 자주 내리진 않더라구요",
          "original_form": "뭉치는 눈이 자주 내리진 않더라구요",
          "speaker_id": "1",
          "time": "20210824 12:40"
        },
        {
          "id": "MDRW2100009920.1.24",
          "form": "맞아요 그런 눈은 잘 안내리는거같아요ㅠ",
          "original_form": "맞아요 그런 눈은 잘 안내리는거같아요ㅠ",
          "speaker_id": "2",
          "time": "20210824 12:41"
        },
        {
          "id": "MDRW2100009920.1.25",
          "form": "그냥 언젠가 한번 진짜 포슬포슬한 눈이 쌓일 정도로 많이 내렸으면 좋겠어요",
          "original_form": "그냥 언젠가 한번 진짜 포슬포슬한 눈이 쌓일 정도로 많이 내렸으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210824 12:44"
        },
        {
          "id": "MDRW2100009920.1.26",
          "form": "그런 눈이 내린다면 어떻게 생긴 눈사람을 만들고 싶으신가요",
          "original_form": "그런 눈이 내린다면 어떻게 생긴 눈사람을 만들고 싶으신가요",
          "speaker_id": "2",
          "time": "20210824 12:45"
        },
        {
          "id": "MDRW2100009920.1.27",
          "form": "전 그냥 큰 눈사람....",
          "original_form": "전 그냥 큰 눈사람....",
          "speaker_id": "1",
          "time": "20210824 12:45"
        },
        {
          "id": "MDRW2100009920.1.28",
          "form": "아무리 만들어도 제 종아리까지밖에 안오는 눈사람이 최대였어서ㅠㅠ",
          "original_form": "아무리 만들어도 제 종아리까지밖에 안오는 눈사람이 최대였어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210824 12:46"
        },
        {
          "id": "MDRW2100009920.1.29",
          "form": "아.. 속상하셨겠어요",
          "original_form": "아.. 속상하셨겠어요",
          "speaker_id": "2",
          "time": "20210824 12:46"
        },
        {
          "id": "MDRW2100009920.1.30",
          "form": "올 겨울엔 눈이 펑펑와서 겁나 큰 눈사람 만드셨음 좋겠어요ㅎ",
          "original_form": "올 겨울엔 눈이 펑펑와서 겁나 큰 눈사람 만드셨음 좋겠어요ㅎ",
          "speaker_id": "2",
          "time": "20210824 12:46"
        },
        {
          "id": "MDRW2100009920.1.31",
          "form": "ㅋㅋㅋ제발요",
          "original_form": "ㅋㅋㅋ제발요",
          "speaker_id": "1",
          "time": "20210824 12:48"
        }
      ]
    }
  ]
