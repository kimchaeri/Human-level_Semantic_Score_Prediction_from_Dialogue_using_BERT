      "utterance": [
        {
          "id": "MDRW2100044860.1.1",
          "form": "소수민족들의 독립 응원합니다~~~~~~#~#~~~!!!",
          "original_form": "소수민족들의 독립 응원합니다~~~~~~#~#~~~!!!",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044860.1.2",
          "form": "민주주의 만세~~~~~~~",
          "original_form": "민주주의 만세~~~~~~~",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044860.1.3",
          "form": "백개의 중국~~~~~~~~~~~~~~~",
          "original_form": "백개의 중국~~~~~~~~~~~~~~~",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044860.1.4",
          "form": "남중국해 노노~~~~~~~~~~~~~~~~~",
          "original_form": "남중국해 노노~~~~~~~~~~~~~~~~~",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044860.1.5",
          "form": "남중국해 ㄲ ㅓ 지라고 해~~~~~~~~~~",
          "original_form": "남중국해 ㄲ ㅓ 지라고 해~~~~~~~~~~",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044860.1.6",
          "form": "지지하는 돌들도 돌아가~~~",
          "original_form": "지지하는 돌들도 돌아가~~~",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044860.1.7",
          "form": "우리나라 티비프로 그만 베껴~~~~~~~~##",
          "original_form": "우리나라 티비프로 그만 베껴~~~~~~~~##",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044860.1.8",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044860.1.9",
          "form": "중국의 민주주의를 응원합니다",
          "original_form": "중국의 민주주의를 응원합니다",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044860.1.10",
          "form": "제가 말씀드렷던가여",
          "original_form": "제가 말씀드렷던가여",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044860.1.11",
          "form": "앗 어떤거져",
          "original_form": "앗 어떤거져",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044860.1.12",
          "form": "팬텀싱어 한국에서 좀 화제 되고나서",
          "original_form": "팬텀싱어 한국에서 좀 화제 되고나서",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044860.1.13",
          "form": "중국에서",
          "original_form": "중국에서",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044860.1.14",
          "form": "후난tv에서",
          "original_form": "후난tv에서",
          "speaker_id": "2",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044860.1.15",
          "form": "ㅎ....",
          "original_form": "ㅎ....",
