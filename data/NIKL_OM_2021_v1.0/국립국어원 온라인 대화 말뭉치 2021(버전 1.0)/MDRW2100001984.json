      "utterance": [
        {
          "id": "MDRW2100001984.1.1",
          "form": "요새 반려동물 많이 키우드라",
          "original_form": "요새 반려동물 많이 키우드라",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001984.1.2",
          "form": "동물 사진 너무 귀여워",
          "original_form": "동물 사진 너무 귀여워",
          "speaker_id": "2",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001984.1.3",
          "form": "힐링되는 경우가 많다ㅋ",
          "original_form": "힐링되는 경우가 많다ㅋ",
          "speaker_id": "1",
          "time": "20210819 22:38"
        },
        {
          "id": "MDRW2100001984.1.4",
          "form": "어리니까 더 귀엽더라",
          "original_form": "어리니까 더 귀엽더라",
          "speaker_id": "2",
          "time": "20210819 22:39"
        },
        {
          "id": "MDRW2100001984.1.5",
          "form": "나도 한마리 키우고 싶다",
          "original_form": "나도 한마리 키우고 싶다",
          "speaker_id": "1",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001984.1.6",
          "form": "근데 돈이 너무 든다",
          "original_form": "근데 돈이 너무 든다",
          "speaker_id": "2",
          "time": "20210819 22:40"
        },
        {
          "id": "MDRW2100001984.1.7",
          "form": "돈 없으면 못키움.   사료 병원비등",
          "original_form": "돈 없으면 못키움.   사료 병원비등",
          "speaker_id": "1",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001984.1.8",
          "form": "엉 부가가치세 붙어서 더 비싸다",
          "original_form": "엉 부가가치세 붙어서 더 비싸다",
          "speaker_id": "2",
          "time": "20210819 22:41"
        },
        {
          "id": "MDRW2100001984.1.9",
          "form": "병원비도 병원마다 틀리드라 아니가",
          "original_form": "병원비도 병원마다 틀리드라 아니가",
          "speaker_id": "1",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001984.1.10",
          "form": "엉 사람보다 더 비쌈",
          "original_form": "엉 사람보다 더 비쌈",
          "speaker_id": "2",
          "time": "20210819 22:42"
        },
        {
          "id": "MDRW2100001984.1.11",
          "form": "그래서 반려동물 보험 드는 사람 많다더라",
          "original_form": "그래서 반려동물 보험 드는 사람 많다더라",
          "speaker_id": "1",
          "time": "20210819 22:43"
        },
        {
          "id": "MDRW2100001984.1.12",
          "form": "우리도 들려다 말았다 그돈이 그돈이더라",
          "original_form": "우리도 들려다 말았다 그돈이 그돈이더라",
          "speaker_id": "2",
          "time": "20210819 22:43"
