      "utterance": [
        {
          "id": "MDRW2100016528.1.1",
          "form": "업무중이신가요",
          "original_form": "업무중이신가요",
          "speaker_id": "1",
          "time": "20210826 13:31"
        },
        {
          "id": "MDRW2100016528.1.2",
          "form": "넵 근무중입니다",
          "original_form": "넵 근무중입니다",
          "speaker_id": "2",
          "time": "20210826 14:06"
        },
        {
          "id": "MDRW2100016528.1.3",
          "form": "회사 생활 어떠세요..",
          "original_form": "회사 생활 어떠세요..",
          "speaker_id": "1",
          "time": "20210826 14:11"
        },
        {
          "id": "MDRW2100016528.1.4",
          "form": "하...ㅋㅋ 그래도 가끔 재택하는 소소함이 있어요",
          "original_form": "하...ㅋㅋ 그래도 가끔 재택하는 소소함이 있어요",
          "speaker_id": "2",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100016528.1.5",
          "form": "재택하시나요?",
          "original_form": "재택하시나요?",
          "speaker_id": "2",
          "time": "20210826 14:50"
        },
        {
          "id": "MDRW2100016528.1.6",
          "form": "오호 네 재택합니다 ㅋㅋㅋ 얼마나자주하세요",
          "original_form": "오호 네 재택합니다 ㅋㅋㅋ 얼마나자주하세요",
          "speaker_id": "1",
          "time": "20210826 14:59"
        },
        {
          "id": "MDRW2100016528.1.7",
          "form": "아 진짜요??? 전 주2회만..ㅜ업무특성상요",
          "original_form": "아 진짜요??? 전 주2회만..ㅜ업무특성상요",
          "speaker_id": "2",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016528.1.8",
          "form": "회사에 몇달째 하는 분도 있어요",
          "original_form": "회사에 몇달째 하는 분도 있어요",
          "speaker_id": "2",
          "time": "20210826 15:02"
        },
        {
          "id": "MDRW2100016528.1.9",
          "form": "헐 몇달째",
          "original_form": "헐 몇달째",
          "speaker_id": "1",
          "time": "20210826 15:10"
        },
        {
          "id": "MDRW2100016528.1.10",
          "form": "근데 재택하면 일을 잘 안하긴 해요 ㅋㅋㅋㅋㅋ",
          "original_form": "근데 재택하면 일을 잘 안하긴 해요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016528.1.11",
          "form": "의지력이약해서",
          "original_form": "의지력이약해서",
          "speaker_id": "1",
          "time": "20210826 15:11"
        },
        {
          "id": "MDRW2100016528.1.12",
          "form": "그러니까요 주2회하면 출근한 날 바짝 처리하고 담날 재택하고 그 루틴 너무 좋아요",
          "original_form": "그러니까요 주2회하면 출근한 날 바짝 처리하고 담날 재택하고 그 루틴 너무 좋아요",
          "speaker_id": "2",
          "time": "20210826 15:24"
        },
        {
          "id": "MDRW2100016528.1.13",
          "form": "월말엔 바빠서 풀로 출근하니 어색하고 적응이 안되네요",
          "original_form": "월말엔 바빠서 풀로 출근하니 어색하고 적응이 안되네요",
          "speaker_id": "2",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016528.1.14",
          "form": "ㅋㅋㅋㅋ 코로나끝나두 그랫음 조켓네요ㅠㅠㅠ",
          "original_form": "ㅋㅋㅋㅋ 코로나끝나두 그랫음 조켓네요ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 15:25"
        },
        {
          "id": "MDRW2100016528.1.15",
          "form": "그니까요 삶이 여유롭고 조금 불안하긴해도 장점이 더 많아요",
          "original_form": "그니까요 삶이 여유롭고 조금 불안하긴해도 장점이 더 많아요",
          "speaker_id": "2",
          "time": "20210826 15:35"
        },
        {
          "id": "MDRW2100016528.1.16",
          "form": "다니는데는 평생직장 이신가용 ㅋㅋ",
          "original_form": "다니는데는 평생직장 이신가용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016528.1.17",
          "form": "아뇨ㅜ 그런데 다른곳에 이직하기가... ㅋㅋ 지금 5년째 다니다보니 너무 익숙해졌어요",
          "original_form": "아뇨ㅜ 그런데 다른곳에 이직하기가... ㅋㅋ 지금 5년째 다니다보니 너무 익숙해졌어요",
          "speaker_id": "2",
          "time": "20210826 16:44"
        },
        {
          "id": "MDRW2100016528.1.18",
          "form": "ㅋㅋㅋㅋ 이직하기가 쉽지않죠 저도 신기해요",
          "original_form": "ㅋㅋㅋㅋ 이직하기가 쉽지않죠 저도 신기해요",
          "speaker_id": "1",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100016528.1.19",
          "form": "부지런하고 결단력있어야하는듯",
          "original_form": "부지런하고 결단력있어야하는듯",
          "speaker_id": "1",
          "time": "20210826 16:45"
        },
        {
          "id": "MDRW2100016528.1.20",
          "form": "현직장에서 몇년차세요?",
          "original_form": "현직장에서 몇년차세요?",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100016528.1.21",
          "form": "진짜 결단력 필요해요ㅎㅎ",
          "original_form": "진짜 결단력 필요해요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210826 17:05"
        },
        {
          "id": "MDRW2100016528.1.22",
          "form": "저는... 2 .. 3 년 ?",
          "original_form": "저는... 2 .. 3 년 ?",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100016528.1.23",
          "form": "아 로또되고싶네요 ㅠㅠ",
          "original_form": "아 로또되고싶네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 17:07"
        },
        {
          "id": "MDRW2100016528.1.24",
          "form": "아..로또만이... 직장 동료는 괜찮아요??",
          "original_form": "아..로또만이... 직장 동료는 괜찮아요??",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100016528.1.25",
          "form": "사람스트레스만 없어도 반은 성공같아요",
          "original_form": "사람스트레스만 없어도 반은 성공같아요",
          "speaker_id": "2",
          "time": "20210826 17:21"
        },
        {
          "id": "MDRW2100016528.1.26",
          "form": "걍 그렇죠 ㅠㅠ그래도 걍 무시할정돈 돼서 다행이에요",
          "original_form": "걍 그렇죠 ㅠㅠ그래도 걍 무시할정돈 돼서 다행이에요",
