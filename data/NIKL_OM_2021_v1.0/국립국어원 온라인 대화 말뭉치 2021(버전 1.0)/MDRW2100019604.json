      "utterance": [
        {
          "id": "MDRW2100019604.1.1",
          "form": "요즘 들어 갑자기 다시 비가 많이 오네요",
          "original_form": "요즘 들어 갑자기 다시 비가 많이 오네요",
          "speaker_id": "2",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019604.1.2",
          "form": "이제 맑은 가을 들어서는 줄 알았는데",
          "original_form": "이제 맑은 가을 들어서는 줄 알았는데",
          "speaker_id": "2",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019604.1.3",
          "form": "다시 습하고 푹푹 쪄요",
          "original_form": "다시 습하고 푹푹 쪄요",
          "speaker_id": "2",
          "time": "20210827 20:35"
        },
        {
          "id": "MDRW2100019604.1.4",
          "form": "전 밤에 잘때",
          "original_form": "전 밤에 잘때",
          "speaker_id": "1",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019604.1.5",
          "form": "name1님은 어떤 날씨를 제일 좋아하세요?",
          "original_form": "&name1&님은 어떤 날씨를 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019604.1.6",
          "form": "너무힘들어요",
          "original_form": "너무힘들어요",
          "speaker_id": "1",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019604.1.7",
          "form": "저도요ㅋㅋㅋㅋ",
          "original_form": "저도요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019604.1.8",
          "form": "더워도 이렇게 더울수는 없을것같네요",
          "original_form": "더워도 이렇게 더울수는 없을것같네요",
          "speaker_id": "1",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019604.1.9",
          "form": "뭔가 습도만 덜해도",
          "original_form": "뭔가 습도만 덜해도",
          "speaker_id": "2",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019604.1.10",
          "form": "지금보단 나을 것 같은데",
          "original_form": "지금보단 나을 것 같은데",
          "speaker_id": "2",
          "time": "20210827 20:36"
        },
        {
          "id": "MDRW2100019604.1.11",
          "form": "찜통 속 만두가 되는 기분이예요",
          "original_form": "찜통 속 만두가 되는 기분이예요",
          "speaker_id": "2",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019604.1.12",
          "form": "가을을 제일 좋아해서",
          "original_form": "가을을 제일 좋아해서",
          "speaker_id": "1",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019604.1.13",
          "form": "입추될때 진짜 좋아했는데",
          "original_form": "입추될때 진짜 좋아했는데",
          "speaker_id": "1",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019604.1.14",
          "form": "이렇게 장마에...습한 가을을",
          "original_form": "이렇게 장마에...습한 가을을",
          "speaker_id": "1",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019604.1.15",
          "form": "얻을줄은 몰랐네요",
          "original_form": "얻을줄은 몰랐네요",
          "speaker_id": "1",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019604.1.16",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210827 20:37"
        },
        {
          "id": "MDRW2100019604.1.17",
          "form": "입추때까지만 해도 갑자기 기온 내려가고 선선하길래",
          "original_form": "입추때까지만 해도 갑자기 기온 내려가고 선선하길래",
          "speaker_id": "2",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100019604.1.18",
          "form": "와 입추매직~이러고 좋아했는데",
          "original_form": "와 입추매직~이러고 좋아했는데",
          "speaker_id": "2",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100019604.1.19",
          "form": "도로 더워지던데요",
          "original_form": "도로 더워지던데요",
          "speaker_id": "2",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100019604.1.20",
          "form": "더위 잘 타세요?",
          "original_form": "더위 잘 타세요?",
          "speaker_id": "2",
          "time": "20210827 20:38"
        },
        {
          "id": "MDRW2100019604.1.21",
          "form": "더위를 잘타서",
          "original_form": "더위를 잘타서",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019604.1.22",
          "form": "옷을 어떻게 입을지도",
          "original_form": "옷을 어떻게 입을지도",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019604.1.23",
          "form": "계속 고민중이예요",
          "original_form": "계속 고민중이예요",
          "speaker_id": "1",
          "time": "20210827 20:39"
        },
        {
          "id": "MDRW2100019604.1.24",
          "form": "아 저도요",
          "original_form": "아 저도요",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019604.1.25",
          "form": "껴입으면 덥고",
          "original_form": "껴입으면 덥고",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019604.1.26",
          "form": "얇게 입으면 또 아침이랑 밤에 춥고",
          "original_form": "얇게 입으면 또 아침이랑 밤에 춥고",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019604.1.27",
          "form": "서늘한 가을 바람 너무 그리워요",
          "original_form": "서늘한 가을 바람 너무 그리워요",
          "speaker_id": "1",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019604.1.28",
          "form": "그러게요 가을바람 좋은데",
          "original_form": "그러게요 가을바람 좋은데",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019604.1.29",
          "form": "그 바람에 가을향 섞여서 나는 것도 좋더라구요",
          "original_form": "그 바람에 가을향 섞여서 나는 것도 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 20:40"
        },
        {
          "id": "MDRW2100019604.1.30",
          "form": "낙엽향이라고 해야 하나",
          "original_form": "낙엽향이라고 해야 하나",
          "speaker_id": "2",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019604.1.31",
          "form": "그래도 전 한국의 사계절이 뚜렷해서",
          "original_form": "그래도 전 한국의 사계절이 뚜렷해서",
          "speaker_id": "1",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019604.1.32",
          "form": "하늘은 높고",
          "original_form": "하늘은 높고",
          "speaker_id": "2",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019604.1.33",
          "form": "너무 좋아요",
          "original_form": "너무 좋아요",
          "speaker_id": "1",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019604.1.34",
          "form": "저는 살찌는 계절이라 좋아요",
          "original_form": "저는 살찌는 계절이라 좋아요",
          "speaker_id": "2",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019604.1.35",
          "form": "벌레크기가 작아지잖아요...",
          "original_form": "벌레크기가 작아지잖아요...",
          "speaker_id": "1",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019604.1.36",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019604.1.37",
          "form": "그건 그래요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "그건 그래요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:41"
        },
        {
          "id": "MDRW2100019604.1.38",
          "form": "천고마비의 계절이네요",
          "original_form": "천고마비의 계절이네요",
          "speaker_id": "1",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019604.1.39",
          "form": "호주벌레가 그렇게 큼직하다는데",
          "original_form": "호주벌레가 그렇게 큼직하다는데",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019604.1.40",
          "form": "아이고 채팅이 느리게 쳐지네요 렉이 걸리네",
          "original_form": "아이고 채팅이 느리게 쳐지네요 렉이 걸리네",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019604.1.41",
          "form": "맞아요 호주는 살기좋은 계절이라",
          "original_form": "맞아요 호주는 살기좋은 계절이라",
          "speaker_id": "1",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019604.1.42",
          "form": "벌레크기가 손바닥만하다는데",
          "original_form": "벌레크기가 손바닥만하다는데",
          "speaker_id": "1",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019604.1.43",
          "form": "그게 사람 사는곳입니까!?!",
          "original_form": "그게 사람 사는곳입니까!?!",
          "speaker_id": "1",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019604.1.44",
          "form": "ㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅌㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:42"
        },
        {
          "id": "MDRW2100019604.1.45",
          "form": "벌레 진짜 싫어하시나봐요",
          "original_form": "벌레 진짜 싫어하시나봐요",
          "speaker_id": "2",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019604.1.46",
          "form": "물론 저도 싫어함",
          "original_form": "물론 저도 싫어함",
          "speaker_id": "2",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019604.1.47",
          "form": "한국벌레는 작아서",
          "original_form": "한국벌레는 작아서",
          "speaker_id": "2",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019604.1.48",
          "form": "참 다행이예요",
          "original_form": "참 다행이예요",
          "speaker_id": "2",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019604.1.49",
          "form": "네 이 망할 습한 날씨도 너무 싫어요",
          "original_form": "네 이 망할 습한 날씨도 너무 싫어요",
          "speaker_id": "1",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019604.1.50",
          "form": "저 어제 산모기에 10방 쏘여서",
          "original_form": "저 어제 산모기에 10방 쏘여서",
          "speaker_id": "1",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019604.1.51",
          "form": "아직도 긁고있거든요",
          "original_form": "아직도 긁고있거든요",
          "speaker_id": "1",
          "time": "20210827 20:43"
        },
        {
          "id": "MDRW2100019604.1.52",
          "form": "으아 가을모기가 진짜 최강이라던데",
          "original_form": "으아 가을모기가 진짜 최강이라던데",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019604.1.53",
          "form": "어쩌다 모기밥이 되셨대",
          "original_form": "어쩌다 모기밥이 되셨대",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019604.1.54",
          "form": "산 타셨나요ㅋㅋㅋㅋ",
          "original_form": "산 타셨나요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019604.1.55",
          "form": "네 날씨가 좋아서 산탔는데",
          "original_form": "네 날씨가 좋아서 산탔는데",
          "speaker_id": "1",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019604.1.56",
          "form": "긴바지 입고 가길 잘했다고",
          "original_form": "긴바지 입고 가길 잘했다고",
          "speaker_id": "1",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019604.1.57",
          "form": "생각했어요",
          "original_form": "생각했어요",
          "speaker_id": "1",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019604.1.58",
          "form": "아 다리가 아니라",
          "original_form": "아 다리가 아니라",
          "speaker_id": "2",
          "time": "20210827 20:44"
        },
        {
          "id": "MDRW2100019604.1.59",
          "form": "상체에만 10방을 물리신거예요?",
          "original_form": "상체에만 10방을 물리신거예요?",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019604.1.60",
          "form": "ㅌㅌㅋㅋㅋㅋㅋ어떡해",
          "original_form": "ㅌㅌㅋㅋㅋㅋㅋ어떡해",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019604.1.61",
          "form": "아뇨 발등에도 물렸어요",
          "original_form": "아뇨 발등에도 물렸어요",
          "speaker_id": "1",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019604.1.62",
          "form": "하하 인생",
          "original_form": "하하 인생",
          "speaker_id": "1",
          "time": "20210827 20:45"
        },
        {
          "id": "MDRW2100019604.1.63",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ진짜 웃겨요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ진짜 웃겨요",
          "speaker_id": "2",
          "time": "20210827 20:45"
        },
