      "utterance": [
        {
          "id": "MDRW2100019577.1.1",
          "form": "혹시 홈베이킹 좋아하세요?",
          "original_form": "혹시 홈베이킹 좋아하세요?",
          "speaker_id": "1",
          "time": "20210827 21:53"
        },
        {
          "id": "MDRW2100019577.1.2",
          "form": "제가 손재주가 없어서 ㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "제가 손재주가 없어서 ㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100019577.1.3",
          "form": "베이킹은 너무 먼 영역같아요",
          "original_form": "베이킹은 너무 먼 영역같아요",
          "speaker_id": "2",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100019577.1.4",
          "form": "베이킹 자주 하세요?",
          "original_form": "베이킹 자주 하세요?",
          "speaker_id": "2",
          "time": "20210827 21:54"
        },
        {
          "id": "MDRW2100019577.1.5",
          "form": "자주는 안하는데 요즘은 생지구워요",
          "original_form": "자주는 안하는데 요즘은 생지구워요",
          "speaker_id": "1",
          "time": "20210827 21:55"
        },
        {
          "id": "MDRW2100019577.1.6",
          "form": "생지를 아시나요?",
          "original_form": "생지를 아시나요?",
          "speaker_id": "1",
          "time": "20210827 21:55"
        },
        {
          "id": "MDRW2100019577.1.7",
          "form": "앗 네네",
          "original_form": "앗 네네",
          "speaker_id": "2",
          "time": "20210827 21:56"
        },
        {
          "id": "MDRW2100019577.1.8",
          "form": "크로아상만 먹어봤어요",
          "original_form": "크로아상만 먹어봤어요",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100019577.1.9",
          "form": "에어프라이어는 정말 최고의 발명품이에요",
          "original_form": "에어프라이어는 정말 최고의 발명품이에요",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100019577.1.10",
          "form": "전 생지를 사서 구워먹는 것도 홈베이킹이라고 말해요 ㅋㅋ",
          "original_form": "전 생지를 사서 구워먹는 것도 홈베이킹이라고 말해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100019577.1.11",
          "form": "어떤 생지 사세요?",
          "original_form": "어떤 생지 사세요?",
          "speaker_id": "2",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100019577.1.12",
          "form": "전 울프백 생지요 버터가 들어서 맛있어요",
          "original_form": "전 울프백 생지요 버터가 들어서 맛있어요",
          "speaker_id": "1",
          "time": "20210827 21:57"
        },
        {
          "id": "MDRW2100019577.1.13",
          "form": "종류는 크로아상 사시나요?",
          "original_form": "종류는 크로아상 사시나요?",
          "speaker_id": "2",
          "time": "20210827 21:59"
        },
        {
          "id": "MDRW2100019577.1.14",
          "form": "네 크로아상이 무난해요 근데 울프백은 좀 커서 담엔 미니크로아상을 머거보게여",
          "original_form": "네 크로아상이 무난해요 근데 울프백은 좀 커서 담엔 미니크로아상을 머거보게여",
          "speaker_id": "1",
          "time": "20210827 22:00"
        },
        {
          "id": "MDRW2100019577.1.15",
          "form": "크로플도 해보셨나요?",
          "original_form": "크로플도 해보셨나요?",
          "speaker_id": "2",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019577.1.16",
          "form": "와플누르는기계가없어서 아뇨",
          "original_form": "와플누르는기계가없어서 아뇨",
          "speaker_id": "1",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019577.1.17",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019577.1.18",
          "form": "저도 와플팬이 없어서 못해봤어요",
          "original_form": "저도 와플팬이 없어서 못해봤어요",
          "speaker_id": "2",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019577.1.19",
          "form": "그냥 사먹는걸로 해요 ㅋㅋ 와플팬 너무 짐이에요",
          "original_form": "그냥 사먹는걸로 해요 ㅋㅋ 와플팬 너무 짐이에요",
          "speaker_id": "1",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100019577.1.20",
          "form": "와플팬 있는 친구 보니까 이것저것 잘 눌러먹더라구요",
          "original_form": "와플팬 있는 친구 보니까 이것저것 잘 눌러먹더라구요",
          "speaker_id": "2",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100019577.1.21",
          "form": "그렇긴 하죠 ㅋㅋㅋㅋ",
          "original_form": "그렇긴 하죠 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100019577.1.22",
          "form": "아 진짜요? 전 있으면 짐될줄 아랐는데",
          "original_form": "아 진짜요? 전 있으면 짐될줄 아랐는데",
          "speaker_id": "1",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100019577.1.23",
          "form": "물론 초반에 그랬었고",
          "original_form": "물론 초반에 그랬었고",
          "speaker_id": "2",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100019577.1.24",
          "form": "지금은 어떤지 모르겠어요 ㅋㅋㅋㅋ",
          "original_form": "지금은 어떤지 모르겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 22:04"
        },
        {
          "id": "MDRW2100019577.1.25",
          "form": "전 빵도 프라이팬에 굽고파요",
          "original_form": "전 빵도 프라이팬에 굽고파요",
          "speaker_id": "1",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100019577.1.26",
          "form": "생지요?",
          "original_form": "생지요?",
          "speaker_id": "2",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100019577.1.27",
          "form": "아뇨 그냥 빵이요",
          "original_form": "아뇨 그냥 빵이요",
          "speaker_id": "1",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100019577.1.28",
          "form": "아 직접 반죽해서 굽는 것도 하세요??",
          "original_form": "아 직접 반죽해서 굽는 것도 하세요??",
