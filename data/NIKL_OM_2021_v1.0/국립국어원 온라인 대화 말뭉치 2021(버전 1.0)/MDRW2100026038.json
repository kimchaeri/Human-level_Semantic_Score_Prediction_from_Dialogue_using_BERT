      "utterance": [
        {
          "id": "MDRW2100026038.1.1",
          "form": "와 묘연 ㄷㄷ",
          "original_form": "와 묘연 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210901 19:05"
        },
        {
          "id": "MDRW2100026038.1.2",
          "form": "네 생일 얼마전에 지나서 이제 사람나이로 32살됐데요 ㅋㅋ",
          "original_form": "네 생일 얼마전에 지나서 이제 사람나이로 32살됐데요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:05"
        },
        {
          "id": "MDRW2100026038.1.3",
          "form": "4살",
          "original_form": "4살",
          "speaker_id": "1",
          "time": "20210901 19:06"
        },
        {
          "id": "MDRW2100026038.1.4",
          "form": "웹툰으로 그려져야할 사연이네요",
          "original_form": "웹툰으로 그려져야할 사연이네요",
          "speaker_id": "2",
          "time": "20210901 19:06"
        },
        {
          "id": "MDRW2100026038.1.5",
          "form": "오 언제태어났는지 날짜도 아세요?",
          "original_form": "오 언제태어났는지 날짜도 아세요?",
          "speaker_id": "2",
          "time": "20210901 19:06"
        },
        {
          "id": "MDRW2100026038.1.6",
          "form": "형제인데 너무 다르게 생겨서 웃겨요 ㅋㅋ",
          "original_form": "형제인데 너무 다르게 생겨서 웃겨요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:06"
        },
        {
          "id": "MDRW2100026038.1.7",
          "form": "엄마 나온 전날이 아닐까 추측중...",
          "original_form": "엄마 나온 전날이 아닐까 추측중...",
          "speaker_id": "1",
          "time": "20210901 19:06"
        },
        {
          "id": "MDRW2100026038.1.8",
          "form": "옷장에서 ㅋㅋㅋㅋ",
          "original_form": "옷장에서 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:06"
        },
        {
          "id": "MDRW2100026038.1.9",
          "form": "앗 남자애 둘이군요",
          "original_form": "앗 남자애 둘이군요",
          "speaker_id": "2",
          "time": "20210901 19:07"
        },
        {
          "id": "MDRW2100026038.1.10",
          "form": "형제라시는거보니 ㅋㅋ",
          "original_form": "형제라시는거보니 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:07"
        },
        {
          "id": "MDRW2100026038.1.11",
          "form": "ㅋㅋ엄마나온전날 생일 추측 ㅋㅋ",
          "original_form": "ㅋㅋ엄마나온전날 생일 추측 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:07"
        },
        {
          "id": "MDRW2100026038.1.12",
          "form": "웃겨욥",
          "original_form": "웃겨욥",
          "speaker_id": "2",
          "time": "20210901 19:07"
        },
        {
          "id": "MDRW2100026038.1.13",
          "form": "그 엄마 밥은 얻어먹고갔나요?",
          "original_form": "그 엄마 밥은 얻어먹고갔나요?",
          "speaker_id": "2",
          "time": "20210901 19:07"
        },
        {
          "id": "MDRW2100026038.1.14",
          "form": "네네 저는 뭔가 수컷애들 ㅋㅋ 좋아하는데 딱 형제여써요 ㅋㅋㅋ",
          "original_form": "네네 저는 뭔가 수컷애들 ㅋㅋ 좋아하는데 딱 형제여써요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100026038.1.15",
          "form": "아 손안타면 걍 사라졌을라나",
          "original_form": "아 손안타면 걍 사라졌을라나",
          "speaker_id": "2",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100026038.1.16",
          "form": "창틀에 올려놨는데",
          "original_form": "창틀에 올려놨는데",
          "speaker_id": "1",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100026038.1.17",
          "form": "서열 밀리는지 잘안보이더라구요 ㅠ",
          "original_form": "서열 밀리는지 잘안보이더라구요 ㅠ",
          "speaker_id": "1",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100026038.1.18",
          "form": "엄마도 손탔으면 같이 있었을건데",
          "original_form": "엄마도 손탔으면 같이 있었을건데",
          "speaker_id": "1",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100026038.1.19",
          "form": "name1님 근데 전에 고양이나 강아지 키워보신적은 있으셨어요?ㅋㅋ",
          "original_form": "&name1&님 근데 전에 고양이나 강아지 키워보신적은 있으셨어요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:08"
        },
        {
          "id": "MDRW2100026038.1.20",
          "form": "강쥐 초등학생때 키워보고 없었어요 햄스터 기니피그 이런거 말고는 ㅋㅋ",
          "original_form": "강쥐 초등학생때 키워보고 없었어요 햄스터 기니피그 이런거 말고는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100026038.1.21",
          "form": "오 그래도 경력이 좀 있으시네요",
          "original_form": "오 그래도 경력이 좀 있으시네요",
          "speaker_id": "2",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100026038.1.22",
          "form": "전 고양이 앞발이 꺾인거도 무슨 병걸려서 그런줄 알았어요 ㄷㄷ",
          "original_form": "전 고양이 앞발이 꺾인거도 무슨 병걸려서 그런줄 알았어요 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100026038.1.23",
          "form": "원래 그런 발이더군요 ㅋㅋㅋㅋ",
          "original_form": "원래 그런 발이더군요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100026038.1.24",
          "form": "고양이 엄마 혜안이 엄청나네요",
          "original_form": "고양이 엄마 혜안이 엄청나네요",
          "speaker_id": "2",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100026038.1.25",
          "form": "헐 그래도 잘 걷나요?",
          "original_form": "헐 그래도 잘 걷나요?",
          "speaker_id": "2",
          "time": "20210901 19:09"
        },
        {
          "id": "MDRW2100026038.1.26",
          "form": "저는 꼬리는 거의 꺾인 애들 많다는것만들ㅇ니서",
          "original_form": "저는 꼬리는 거의 꺾인 애들 많다는것만들ㅇ니서",
          "speaker_id": "2",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100026038.1.27",
          "form": "들어서",
          "original_form": "들어서",
          "speaker_id": "2",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100026038.1.28",
          "form": "고양이 앞발 보면 그렇자나요?",
          "original_form": "고양이 앞발 보면 그렇자나요?",
          "speaker_id": "1",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100026038.1.29",
          "form": "우리애들도 엄마가 못먹어서 꼬리 기형이에요 ㅠㅠ",
          "original_form": "우리애들도 엄마가 못먹어서 꼬리 기형이에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100026038.1.30",
          "form": "아하아하 이해했어요 ㅋㅋ",
          "original_form": "아하아하 이해했어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100026038.1.31",
          "form": "고등어애는 돼지꼬리 모양이라",
          "original_form": "고등어애는 돼지꼬리 모양이라",
          "speaker_id": "1",
          "time": "20210901 19:10"
        },
        {
          "id": "MDRW2100026038.1.32",
          "form": "그래도 지금은 둘다 잘먹지 않나요",
          "original_form": "그래도 지금은 둘다 잘먹지 않나요",
          "speaker_id": "2",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100026038.1.33",
          "form": "전선 감기면 큰일나요 ㄷㄷ 못풀어서",
          "original_form": "전선 감기면 큰일나요 ㄷㄷ 못풀어서",
          "speaker_id": "1",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100026038.1.34",
          "form": "중성화는 하셨어요?",
          "original_form": "중성화는 하셨어요?",
          "speaker_id": "2",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100026038.1.35",
          "form": "맞아요 집냥이 돼서 다행인",
          "original_form": "맞아요 집냥이 돼서 다행인",
          "speaker_id": "1",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100026038.1.36",
          "form": "6개월때 바로했어요 ㅎㅎ",
          "original_form": "6개월때 바로했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100026038.1.37",
          "form": "오ㅋㅋ돼지꼬리 어떤 느낌일까",
          "original_form": "오ㅋㅋ돼지꼬리 어떤 느낌일까",
          "speaker_id": "2",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100026038.1.38",
          "form": "매탈남님 유투브 보시나요?",
          "original_form": "매탈남님 유투브 보시나요?",
          "speaker_id": "1",
          "time": "20210901 19:11"
        },
        {
          "id": "MDRW2100026038.1.39",
          "form": "거기 꼬리도 뭉퉁한 모양이엿던거같은데",
          "original_form": "거기 꼬리도 뭉퉁한 모양이엿던거같은데",
          "speaker_id": "1",
          "time": "20210901 19:12"
        },
        {
          "id": "MDRW2100026038.1.40",
          "form": "고런모양이에요 ㅋㅋ약간 말려진 몽둥이",
          "original_form": "고런모양이에요 ㅋㅋ약간 말려진 몽둥이",
          "speaker_id": "1",
          "time": "20210901 19:14"
        },
        {
          "id": "MDRW2100026038.1.41",
          "form": "앗 제가 한동안 유투브 중독되서 끊었는데",
          "original_form": "앗 제가 한동안 유투브 중독되서 끊었는데",
          "speaker_id": "2",
          "time": "20210901 19:14"
        },
        {
          "id": "MDRW2100026038.1.42",
          "form": "또 검색해봐야겠네요 ㅋㅋㅋ",
          "original_form": "또 검색해봐야겠네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 19:14"
        },
        {
          "id": "MDRW2100026038.1.43",
          "form": "그걸 끊을 생각을 하시다니",
          "original_form": "그걸 끊을 생각을 하시다니",
          "speaker_id": "1",
          "time": "20210901 19:14"
        }
