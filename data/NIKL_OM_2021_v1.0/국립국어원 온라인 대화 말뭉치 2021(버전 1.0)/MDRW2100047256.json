      "utterance": [
        {
          "id": "MDRW2100047256.1.1",
          "form": "음 저는 강아지요!",
          "original_form": "음 저는 강아지요!",
          "speaker_id": "2",
          "time": "20210826 01:42"
        },
        {
          "id": "MDRW2100047256.1.2",
          "form": "근데 아마... 안키울거같긴해요 ㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "근데 아마... 안키울거같긴해요 ㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:42"
        },
        {
          "id": "MDRW2100047256.1.3",
          "form": "오 강쥐 좋죠 군데 매일 산책나가야하는 ㅋㅋ",
          "original_form": "오 강쥐 좋죠 군데 매일 산책나가야하는 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:42"
        },
        {
          "id": "MDRW2100047256.1.4",
          "form": "요새처럼 비가 매일오면..강아지에게 너는 산책갈수없단다.. 하고 열심히 설득해야해요ㅋㅋ",
          "original_form": "요새처럼 비가 매일오면..강아지에게 너는 산책갈수없단다.. 하고 열심히 설득해야해요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 01:44"
        },
        {
          "id": "MDRW2100047256.1.5",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋ그렇다고 하더라구요 ㅠㅠㅋㅋㅋㅋㅋ",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋ그렇다고 하더라구요 ㅠㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:47"
        },
        {
          "id": "MDRW2100047256.1.6",
          "form": "근데 산책다니는 강쥐들 보면",
          "original_form": "근데 산책다니는 강쥐들 보면",
          "speaker_id": "2",
          "time": "20210826 01:47"
        },
        {
          "id": "MDRW2100047256.1.7",
          "form": "지나칠때마다 넘 귀여워요 ㅋㅋㅋㅋㅋ",
          "original_form": "지나칠때마다 넘 귀여워요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 01:48"
        },
        {
          "id": "MDRW2100047256.1.8",
          "form": "ㅜㅜㅜㅜ 강쥐들 행복해라",
          "original_form": "ㅜㅜㅜㅜ 강쥐들 행복해라",
          "speaker_id": "1",
          "time": "20210826 01:48"
        },
        {
          "id": "MDRW2100047256.1.9",
          "form": "강쥐가 잠시 스쳐지나가면 행복해요",
          "original_form": "강쥐가 잠시 스쳐지나가면 행복해요",
          "speaker_id": "1",
          "time": "20210826 01:48"
        },
        {
          "id": "MDRW2100047256.1.10",
          "form": "오늘아침엔 비옷입은 강쥐 봤는데 넘 예뻣어요",
          "original_form": "오늘아침엔 비옷입은 강쥐 봤는데 넘 예뻣어요",
          "speaker_id": "1",
          "time": "20210826 01:54"
        },
        {
          "id": "MDRW2100047256.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋ와 비옷 ㅠㅠㅠ 귀여웠겠어요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ와 비옷 ㅠㅠㅠ 귀여웠겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100047256.1.12",
          "form": "전 뚠뚠한 강쥐 봤는데 귀엽더라구요 ㅠㅠㅋㅋ",
          "original_form": "전 뚠뚠한 강쥐 봤는데 귀엽더라구요 ㅠㅠㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 08:27"
        },
        {
          "id": "MDRW2100047256.1.13",
          "form": "아이고 이뿌겠어요 뚠뚠고양이는 많이 봤는데 뚠뚠개는 잘 못본거같아요",
          "original_form": "아이고 이뿌겠어요 뚠뚠고양이는 많이 봤는데 뚠뚠개는 잘 못본거같아요",
          "speaker_id": "1",
          "time": "20210826 09:20"
        },
        {
          "id": "MDRW2100047256.1.14",
          "form": "맞아욬ㅋㅋㅋㅋ 뚠뚠개라서 그런가 산책하기 싫다고 꾀부리던데 귀엽더라구요 ㅠ",
          "original_form": "맞아욬ㅋㅋㅋㅋ 뚠뚠개라서 그런가 산책하기 싫다고 꾀부리던데 귀엽더라구요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 12:02"
        },
        {
          "id": "MDRW2100047256.1.15",
          "form": "아니 산책을 싫어하다니",
          "original_form": "아니 산책을 싫어하다니",
          "speaker_id": "1",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100047256.1.16",
          "form": "특이한개네요",
          "original_form": "특이한개네요",
          "speaker_id": "1",
          "time": "20210826 12:04"
        },
        {
          "id": "MDRW2100047256.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그러니까요! 지나가면서도 자꾸 시선가더라구요 ㅠㅠㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그러니까요! 지나가면서도 자꾸 시선가더라구요 ㅠㅠㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:27"
        },
        {
          "id": "MDRW2100047256.1.18",
          "form": "ㅋㅋㅋ특이한애네요 다른애들은 나가자고 끈가져와서 시위를하는데ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ특이한애네요 다른애들은 나가자고 끈가져와서 시위를하는데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100047256.1.19",
          "form": "그니까욬ㅋㅋㅋㅋㅋ",
          "original_form": "그니까욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 12:37"
        },
        {
          "id": "MDRW2100047256.1.20",
          "form": "나가서 응아 해야하는데 걔는 변비인가보네요",
          "original_form": "나가서 응아 해야하는데 걔는 변비인가보네요",
          "speaker_id": "1",
          "time": "20210826 12:46"
        },
        {
          "id": "MDRW2100047256.1.21",
          "form": "잌ㅋㅋㅋㅋㅋㅋ 집에서 응ㅇ아하는 멍멍이도 많지않나요??",
          "original_form": "잌ㅋㅋㅋㅋㅋㅋ 집에서 응ㅇ아하는 멍멍이도 많지않나요??",
          "speaker_id": "2",
          "time": "20210826 18:19"
        },
        {
          "id": "MDRW2100047256.1.22",
          "form": "아 저희 삼촌댁은 배변패드있던데",
          "original_form": "아 저희 삼촌댁은 배변패드있던데",
          "speaker_id": "1",
          "time": "20210826 18:24"
        }
      ]
