      "utterance": [
        {
          "id": "MDRW2100009638.1.1",
          "form": "지금 연애중이신가요?",
          "original_form": "지금 연애중이신가요?",
          "speaker_id": "1",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009638.1.2",
          "form": "아뇨 저는 솔로입니다",
          "original_form": "아뇨 저는 솔로입니다",
          "speaker_id": "2",
          "time": "20210823 02:03"
        },
        {
          "id": "MDRW2100009638.1.3",
          "form": "그렇군요 소개팅 해보신 적 있으신가요?",
          "original_form": "그렇군요 소개팅 해보신 적 있으신가요?",
          "speaker_id": "1",
          "time": "20210823 02:04"
        },
        {
          "id": "MDRW2100009638.1.4",
          "form": "소개팅... 없습니다 ㅠ",
          "original_form": "소개팅... 없습니다 ㅠ",
          "speaker_id": "2",
          "time": "20210823 02:05"
        },
        {
          "id": "MDRW2100009638.1.5",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210823 02:05"
        },
        {
          "id": "MDRW2100009638.1.6",
          "form": "저는 2년 째 연애중이에요!!",
          "original_form": "저는 2년 째 연애중이에요!!",
          "speaker_id": "1",
          "time": "20210823 02:06"
        },
        {
          "id": "MDRW2100009638.1.7",
          "form": "대학교에서 만났어요 ㅎㅋㅋㅋㅋㅋ",
          "original_form": "대학교에서 만났어요 ㅎㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 02:06"
        },
        {
          "id": "MDRW2100009638.1.8",
          "form": "오...!!!!",
          "original_form": "오...!!!!",
          "speaker_id": "2",
          "time": "20210823 02:06"
        },
        {
          "id": "MDRW2100009638.1.9",
          "form": "CC인가요!!!!",
          "original_form": "CC인가요!!!!",
          "speaker_id": "2",
          "time": "20210823 02:06"
        },
        {
          "id": "MDRW2100009638.1.10",
          "form": "앟ㅎㅎ 맞아요!",
          "original_form": "앟ㅎㅎ 맞아요!",
          "speaker_id": "1",
          "time": "20210823 02:10"
        },
        {
          "id": "MDRW2100009638.1.11",
          "form": "제가 19학번이고 오빠가 17학번인데",
          "original_form": "제가 19학번이고 오빠가 17학번인데",
          "speaker_id": "1",
          "time": "20210823 02:10"
        },
        {
          "id": "MDRW2100009638.1.12",
          "form": "제가 엄청 들이대서 만났어요 ㅎㅎㅎㅎ",
          "original_form": "제가 엄청 들이대서 만났어요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 02:10"
        },
        {
          "id": "MDRW2100009638.1.13",
          "form": "17이면",
          "original_form": "17이면",
          "speaker_id": "2",
          "time": "20210823 02:11"
        },
        {
          "id": "MDRW2100009638.1.14",
          "form": "아 군휴학때문에...?!",
          "original_form": "아 군휴학때문에...?!",
          "speaker_id": "2",
          "time": "20210823 02:11"
        },
        {
          "id": "MDRW2100009638.1.15",
          "form": "아니 우리는 대학 특성상 졸업하고 군대를 가!",
          "original_form": "아니 우리는 대학 특성상 졸업하고 군대를 가!",
          "speaker_id": "1",
          "time": "20210823 02:11"
        },
        {
          "id": "MDRW2100009638.1.16",
          "form": "아..! 우리 대학이랑 비늣하네",
          "original_form": "아..! 우리 대학이랑 비늣하네",
          "speaker_id": "2",
          "time": "20210823 02:11"
        },
        {
          "id": "MDRW2100009638.1.17",
          "form": "새터에서 만났어 ㅋㅋㅋㅋ",
          "original_form": "새터에서 만났어 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 02:12"
        },
        {
          "id": "MDRW2100009638.1.18",
          "form": "엇 그러면 남자친구분은",
          "original_form": "엇 그러면 남자친구분은",
          "speaker_id": "2",
          "time": "20210823 02:12"
        },
        {
          "id": "MDRW2100009638.1.19",
          "form": "졸업하신 거 맞ㅈㄱ?",
          "original_form": "졸업하신 거 맞ㅈㄱ?",
          "speaker_id": "2",
          "time": "20210823 02:12"
        },
        {
          "id": "MDRW2100009638.1.20",
          "form": "응응 맞아~ 지금 직장인이야!",
          "original_form": "응응 맞아~ 지금 직장인이야!",
          "speaker_id": "1",
          "time": "20210823 02:15"
        },
        {
          "id": "MDRW2100009638.1.21",
          "form": "직장인이랑 연애하면 어때? ㅋㅋㅋㅋㅋ",
          "original_form": "직장인이랑 연애하면 어때? ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 02:15"
        },
        {
          "id": "MDRW2100009638.1.22",
          "form": "앜ㅋㅋㅋㅋㅋ 근데 이제 직장인이라 모아둔 돈은 똑같아서",
          "original_form": "앜ㅋㅋㅋㅋㅋ 근데 이제 직장인이라 모아둔 돈은 똑같아서",
          "speaker_id": "1",
          "time": "20210823 02:15"
        },
        {
          "id": "MDRW2100009638.1.23",
          "form": "1~2년 되보면 알 수 있지 않을까?",
          "original_form": "1~2년 되보면 알 수 있지 않을까?",
          "speaker_id": "1",
          "time": "20210823 02:16"
        },
        {
          "id": "MDRW2100009638.1.24",
          "form": "그치... 그렇겠지?",
          "original_form": "그치... 그렇겠지?",
          "speaker_id": "2",
          "time": "20210823 02:16"
        },
        {
          "id": "MDRW2100009638.1.25",
          "form": "결혼은 할 예정이야?",
          "original_form": "결혼은 할 예정이야?",
          "speaker_id": "2",
          "time": "20210823 02:16"
        },
        {
          "id": "MDRW2100009638.1.26",
          "form": "음 근데 나 아직 대학생이라서 모르겠어 ㅎㅎ",
          "original_form": "음 근데 나 아직 대학생이라서 모르겠어 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 02:21"
        },
        {
          "id": "MDRW2100009638.1.27",
          "form": "몇살에 결혼하고 싶어?",
          "original_form": "몇살에 결혼하고 싶어?",
          "speaker_id": "1",
          "time": "20210823 02:21"
        },
        {
          "id": "MDRW2100009638.1.28",
          "form": "난... 결혼 안 하고 싶어",
          "original_form": "난... 결혼 안 하고 싶어",
          "speaker_id": "2",
          "time": "20210823 02:22"
        },
        {
          "id": "MDRW2100009638.1.29",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210823 02:22"
        },
        {
          "id": "MDRW2100009638.1.30",
          "form": "그렇구나 나는 결혼은 꼭 하고 싶은데",
          "original_form": "그렇구나 나는 결혼은 꼭 하고 싶은데",
          "speaker_id": "1",
          "time": "20210823 02:23"
        },
        {
          "id": "MDRW2100009638.1.31",
          "form": "아이는 낳고 싶지 않아 잘 키울 자신이 없어서 ㅎㅎㅎ",
          "original_form": "아이는 낳고 싶지 않아 잘 키울 자신이 없어서 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 02:24"
        },
        {
          "id": "MDRW2100009638.1.32",
          "form": "헐 난 오히려 반대로",
          "original_form": "헐 난 오히려 반대로",
          "speaker_id": "2",
          "time": "20210823 02:25"
        },
        {
          "id": "MDRW2100009638.1.33",
          "form": "결혼은 자신 없는데",
          "original_form": "결혼은 자신 없는데",
          "speaker_id": "2",
          "time": "20210823 02:25"
        },
        {
          "id": "MDRW2100009638.1.34",
          "form": "아이는 키우고 싶어",
          "original_form": "아이는 키우고 싶어",
          "speaker_id": "2",
          "time": "20210823 02:25"
        },
        {
          "id": "MDRW2100009638.1.35",
          "form": "헐 진짜?",
          "original_form": "헐 진짜?",
          "speaker_id": "1",
          "time": "20210823 02:26"
        },
        {
          "id": "MDRW2100009638.1.36",
          "form": "엉 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "엉 ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210823 02:27"
        },
        {
          "id": "MDRW2100009638.1.37",
          "form": "애기는 근데 또",
          "original_form": "애기는 근데 또",
          "speaker_id": "2",
          "time": "20210823 02:27"
        },
        {
          "id": "MDRW2100009638.1.38",
          "form": "낳으려니 너무 힘들 거 뻔하고",
          "original_form": "낳으려니 너무 힘들 거 뻔하고",
          "speaker_id": "2",
          "time": "20210823 02:27"
        },
        {
          "id": "MDRW2100009638.1.39",
          "form": "안 낳으려니 나 닮은 애기 얼마나 예쁠까 싶고",
          "original_form": "안 낳으려니 나 닮은 애기 얼마나 예쁠까 싶고",
          "speaker_id": "2",
          "time": "20210823 02:27"
        },
        {
          "id": "MDRW2100009638.1.40",
          "form": "아 그치ㅠㅠㅠ 나 닮은 아기 넘 궁금해",
          "original_form": "아 그치ㅠㅠㅠ 나 닮은 아기 넘 궁금해",
          "speaker_id": "1",
