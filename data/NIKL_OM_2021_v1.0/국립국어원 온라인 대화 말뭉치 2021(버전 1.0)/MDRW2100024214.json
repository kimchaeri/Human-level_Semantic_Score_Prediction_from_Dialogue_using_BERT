      "utterance": [
        {
          "id": "MDRW2100024214.1.1",
          "form": "술은잘못해요",
          "original_form": "술은잘못해요",
          "speaker_id": "2",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024214.1.2",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "2",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024214.1.3",
          "form": "진로드꺼비 넘 귀엽지않나요",
          "original_form": "진로드꺼비 넘 귀엽지않나요",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024214.1.4",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024214.1.5",
          "form": "전 진짜좋아해요",
          "original_form": "전 진짜좋아해요",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024214.1.6",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024214.1.7",
          "form": "파란색이좋나요?",
          "original_form": "파란색이좋나요?",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100024214.1.8",
          "form": "아니용ㅋㅋ",
          "original_form": "아니용ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024214.1.9",
          "form": "분홍색은 징그럽더라구요ㅜㅜ",
          "original_form": "분홍색은 징그럽더라구요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024214.1.10",
          "form": "전 분홍이 좋아해요",
          "original_form": "전 분홍이 좋아해요",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024214.1.11",
          "form": "헐 완전 신기하네요",
          "original_form": "헐 완전 신기하네요",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024214.1.12",
          "form": "파랑이 좋아하나요",
          "original_form": "파랑이 좋아하나요",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024214.1.13",
          "form": "네! 굿즈도 샀어요",
          "original_form": "네! 굿즈도 샀어요",
          "speaker_id": "1",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024214.1.14",
          "form": "열혈팬 이네요",
          "original_form": "열혈팬 이네요",
          "speaker_id": "2",
          "time": "20210830 23:18"
        },
        {
          "id": "MDRW2100024214.1.15",
          "form": "길거리가다 파랑이있음 사진도찍어요",
          "original_form": "길거리가다 파랑이있음 사진도찍어요",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024214.1.16",
          "form": "하하하하",
          "original_form": "하하하하",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024214.1.17",
          "form": "곰표드셔보셨어요?",
          "original_form": "곰표드셔보셨어요?",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024214.1.18",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024214.1.19",
          "form": "핫하대서 먹어봤는데 별로...",
          "original_form": "핫하대서 먹어봤는데 별로...",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024214.1.20",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "2",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024214.1.21",
          "form": "곰표팝콘은 맛있어요 드셔보세요",
          "original_form": "곰표팝콘은 맛있어요 드셔보세요",
          "speaker_id": "1",
          "time": "20210830 23:19"
        },
        {
          "id": "MDRW2100024214.1.22",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024214.1.23",
          "form": "귀여운 굿즈들 많이나왔음 좋겠어요",
          "original_form": "귀여운 굿즈들 많이나왔음 좋겠어요",
          "speaker_id": "1",
          "time": "20210830 23:20"
        },
        {
          "id": "MDRW2100024214.1.24",
          "form": "저도요 ㅎㅎ",
          "original_form": "저도요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 23:20"
        }
