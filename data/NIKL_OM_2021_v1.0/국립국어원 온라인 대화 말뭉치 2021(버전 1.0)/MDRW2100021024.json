      "utterance": [
        {
          "id": "MDRW2100021024.1.1",
          "form": "모임하시는 거 있으세요?",
          "original_form": "모임하시는 거 있으세요?",
          "speaker_id": "1",
          "time": "20210828 15:06"
        },
        {
          "id": "MDRW2100021024.1.2",
          "form": "아뇨 집순이라 아무것도... ㅋㅋㅋㅋㅋ",
          "original_form": "아뇨 집순이라 아무것도... ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100021024.1.3",
          "form": "name1님은 하시는거 계신가요",
          "original_form": "&name1&님은 하시는거 계신가요",
          "speaker_id": "2",
          "time": "20210828 15:07"
        },
        {
          "id": "MDRW2100021024.1.4",
          "form": "저도 친구들이랑 모이는 거 빼곤 딱히 없어요 ㅋㅋㅋ",
          "original_form": "저도 친구들이랑 모이는 거 빼곤 딱히 없어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100021024.1.5",
          "form": "친구들과 모이는게 최고아니겠습니까",
          "original_form": "친구들과 모이는게 최고아니겠습니까",
          "speaker_id": "2",
          "time": "20210828 15:10"
        },
        {
          "id": "MDRW2100021024.1.6",
          "form": "새로운 사람사귀는거 넘 귀찮아여",
          "original_form": "새로운 사람사귀는거 넘 귀찮아여",
          "speaker_id": "2",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100021024.1.7",
          "form": "그래도 뭔가 해본다면 운동쪽으로 해보고싶어여!!",
          "original_form": "그래도 뭔가 해본다면 운동쪽으로 해보고싶어여!!",
          "speaker_id": "2",
          "time": "20210828 15:11"
        },
        {
          "id": "MDRW2100021024.1.8",
          "form": "운동 좋죠!",
          "original_form": "운동 좋죠!",
          "speaker_id": "1",
          "time": "20210828 15:14"
        },
        {
          "id": "MDRW2100021024.1.9",
          "form": "어떤 운동 좋아하세요?",
          "original_form": "어떤 운동 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 15:15"
        },
        {
          "id": "MDRW2100021024.1.10",
          "form": "배드민턴 이런거 같이하면 재밌을거같아요!!",
          "original_form": "배드민턴 이런거 같이하면 재밌을거같아요!!",
          "speaker_id": "2",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100021024.1.11",
          "form": "테니스도 좀 배워보고싶고욬ㅋㅋ",
          "original_form": "테니스도 좀 배워보고싶고욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100021024.1.12",
          "form": "이런건 아무래도 혼자못하는거니까!!",
          "original_form": "이런건 아무래도 혼자못하는거니까!!",
          "speaker_id": "2",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100021024.1.13",
          "form": "좋네요! 테니스 진짜 멋있던데!!",
          "original_form": "좋네요! 테니스 진짜 멋있던데!!",
          "speaker_id": "1",
          "time": "20210828 15:19"
        },
        {
          "id": "MDRW2100021024.1.14",
          "form": "전에 배구 잠깐 했을때 진짜 재미있었어요 ㅋㅋ",
          "original_form": "전에 배구 잠깐 했을때 진짜 재미있었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100021024.1.15",
          "form": "실력이 안늘어서 나오긴 했지만요 ㅜㅜㅋㅋ",
          "original_form": "실력이 안늘어서 나오긴 했지만요 ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100021024.1.16",
          "form": "헐 배구요?!?! 저 올림픽보고 배구에 과몰입상태인데",
          "original_form": "헐 배구요?!?! 저 올림픽보고 배구에 과몰입상태인데",
          "speaker_id": "2",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100021024.1.17",
          "form": "진짜 재밌었을거같아요 ㅋㅋㅋㅋㅋ",
          "original_form": "진짜 재밌었을거같아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100021024.1.18",
          "form": "배구 진짜 재밌어요!!!",
          "original_form": "배구 진짜 재밌어요!!!",
          "speaker_id": "1",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100021024.1.19",
          "form": "수비 받을때도 짜릿하고",
          "original_form": "수비 받을때도 짜릿하고",
          "speaker_id": "1",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100021024.1.20",
          "form": "어쩌다 때릴때면 진짜",
          "original_form": "어쩌다 때릴때면 진짜",
          "speaker_id": "1",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100021024.1.21",
          "form": "날아갈것 같아요 ㅋㅋㅋㅋ",
          "original_form": "날아갈것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100021024.1.22",
          "form": "우와웅왕 근데 배구로 경기까지하려면 상대팀까지 하면 사람많아야겠네요 확실히 ㅋㅋㅋㅋ",
          "original_form": "우와웅왕 근데 배구로 경기까지하려면 상대팀까지 하면 사람많아야겠네요 확실히 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100021024.1.23",
          "form": "사회인 배구팀 같은거 있나보던데",
          "original_form": "사회인 배구팀 같은거 있나보던데",
          "speaker_id": "2",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100021024.1.24",
          "form": "궁금하네용",
          "original_form": "궁금하네용",
          "speaker_id": "2",
          "time": "20210828 15:44"
        },
        {
          "id": "MDRW2100021024.1.25",
          "form": "저는 직장에 그런 모임이 있어서 다녔었어요 ㅋㅋㅋ",
          "original_form": "저는 직장에 그런 모임이 있어서 다녔었어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 08:06"
        },
        {
          "id": "MDRW2100021024.1.26",
          "form": "우왕 직장에서 배구팀이 있었어요?!?!?! 신기하다 ㅋㅋㅋㅋ",
          "original_form": "우왕 직장에서 배구팀이 있었어요?!?!?! 신기하다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100021024.1.27",
          "form": "제 직장동료는 사회인 여자 야구팀 다니더라구요 ㅋㅋㅋㅋㅋ",
          "original_form": "제 직장동료는 사회인 여자 야구팀 다니더라구요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:59"
        },
        {
          "id": "MDRW2100021024.1.28",
          "form": "우와 ㅋㅋㅋㅋㅋ 포지션은요?",
          "original_form": "우와 ㅋㅋㅋㅋㅋ 포지션은요?",
          "speaker_id": "1",
          "time": "20210829 21:39"
        },
        {
          "id": "MDRW2100021024.1.29",
          "form": "타자인거 같아요 근데 아무래도 인원이 적다보니까 ㅋㅋㅋ",
          "original_form": "타자인거 같아요 근데 아무래도 인원이 적다보니까 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:31"
        },
        {
          "id": "MDRW2100021024.1.30",
          "form": "돌아가면서 다 하는거같더라고요?!?!",
          "original_form": "돌아가면서 다 하는거같더라고요?!?!",
          "speaker_id": "2",
          "time": "20210829 23:31"
        },
        {
          "id": "MDRW2100021024.1.31",
          "form": "사실 저는 야구 잘몰라서 ㅋㅋㅋ",
          "original_form": "사실 저는 야구 잘몰라서 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:31"
        },
        {
          "id": "MDRW2100021024.1.32",
          "form": "저번에 글러브랑 공 가져와서 회사에서 캐치볼 했는데",
          "original_form": "저번에 글러브랑 공 가져와서 회사에서 캐치볼 했는데",
          "speaker_id": "2",
          "time": "20210829 23:31"
        },
        {
          "id": "MDRW2100021024.1.33",
          "form": "재밌더라구요!!",
          "original_form": "재밌더라구요!!",
          "speaker_id": "2",
          "time": "20210829 23:31"
        },
        {
          "id": "MDRW2100021024.1.34",
          "form": "우와!! 너무 재미있을 것 같아요 ㅋㅋㅋ",
