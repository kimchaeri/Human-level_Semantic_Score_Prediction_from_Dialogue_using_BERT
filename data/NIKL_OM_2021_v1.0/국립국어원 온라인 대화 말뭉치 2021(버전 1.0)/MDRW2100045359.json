      "utterance": [
        {
          "id": "MDRW2100045359.1.1",
          "form": "드라마 봤다",
          "original_form": "드라마 봤다",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045359.1.2",
          "form": "이것도 특이한 드라마",
          "original_form": "이것도 특이한 드라마",
          "speaker_id": "1",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045359.1.3",
          "form": "엉 티비엔꺼",
          "original_form": "엉 티비엔꺼",
          "speaker_id": "2",
          "time": "20210908 02:03"
        },
        {
          "id": "MDRW2100045359.1.4",
          "form": "제목이 너무 길다",
          "original_form": "제목이 너무 길다",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045359.1.5",
          "form": "멸망과 멸망을 빈 계약자",
          "original_form": "멸망과 멸망을 빈 계약자",
          "speaker_id": "2",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045359.1.6",
          "form": "맞아 그래서 어느날 우리집 현관으로 멸망이 들어왔다지",
          "original_form": "맞아 그래서 어느날 우리집 현관으로 멸망이 들어왔다지",
          "speaker_id": "1",
          "time": "20210908 02:04"
        },
        {
          "id": "MDRW2100045359.1.7",
          "form": "서인국이 오랜만에",
          "original_form": "서인국이 오랜만에",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045359.1.8",
          "form": "박보영 헤어스타일 이쁘던데",
          "original_form": "박보영 헤어스타일 이쁘던데",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045359.1.9",
          "form": "엉 완전 성숙미가",
          "original_form": "엉 완전 성숙미가",
          "speaker_id": "2",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045359.1.10",
          "form": "이수혁도 괜찮고",
          "original_form": "이수혁도 괜찮고",
          "speaker_id": "1",
          "time": "20210908 02:05"
        },
        {
          "id": "MDRW2100045359.1.11",
          "form": "남자배우들이 너무 마름",
          "original_form": "남자배우들이 너무 마름",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045359.1.12",
          "form": "서인국이라 이수혁이 그렇지",
          "original_form": "서인국이라 이수혁이 그렇지",
          "speaker_id": "1",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045359.1.13",
          "form": "엉 강태오도 괜찮았고",
          "original_form": "엉 강태오도 괜찮았고",
          "speaker_id": "2",
          "time": "20210908 02:06"
        },
        {
          "id": "MDRW2100045359.1.14",
