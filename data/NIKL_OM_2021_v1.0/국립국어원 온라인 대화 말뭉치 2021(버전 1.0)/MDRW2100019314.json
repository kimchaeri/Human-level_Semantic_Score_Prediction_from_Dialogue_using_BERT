      "utterance": [
        {
          "id": "MDRW2100019314.1.1",
          "form": "나 지금 샐러디에 와서 저녁식사중",
          "original_form": "나 지금 샐러디에 와서 저녁식사중",
          "speaker_id": "2",
          "time": "20210827 17:20"
        },
        {
          "id": "MDRW2100019314.1.2",
          "form": "오늘은 웜랩을 먹어보고 있엉",
          "original_form": "오늘은 웜랩을 먹어보고 있엉",
          "speaker_id": "2",
          "time": "20210827 17:20"
        },
        {
          "id": "MDRW2100019314.1.3",
          "form": "오 웜랩 메뉴는?",
          "original_form": "오 웜랩 메뉴는?",
          "speaker_id": "1",
          "time": "20210827 17:21"
        },
        {
          "id": "MDRW2100019314.1.4",
          "form": "칠리베이컨",
          "original_form": "칠리베이컨",
          "speaker_id": "2",
          "time": "20210827 17:22"
        },
        {
          "id": "MDRW2100019314.1.5",
          "form": "굳",
          "original_form": "굳",
          "speaker_id": "1",
          "time": "20210827 17:22"
        },
        {
          "id": "MDRW2100019314.1.6",
          "form": "나는 익숙한 걸 선택하지ㅋㅋ",
          "original_form": "나는 익숙한 걸 선택하지ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:22"
        },
        {
          "id": "MDRW2100019314.1.7",
          "form": "같은 드레싱이겠지?",
          "original_form": "같은 드레싱이겠지?",
          "speaker_id": "1",
          "time": "20210827 17:22"
        },
        {
          "id": "MDRW2100019314.1.8",
          "form": "응 같은 드레싱인듯",
          "original_form": "응 같은 드레싱인듯",
          "speaker_id": "2",
          "time": "20210827 17:22"
        },
        {
          "id": "MDRW2100019314.1.9",
          "form": "크리미 칠리",
          "original_form": "크리미 칠리",
          "speaker_id": "1",
          "time": "20210827 17:22"
        },
        {
          "id": "MDRW2100019314.1.10",
          "form": "집에서 내가 만든거랑 비겨해봐",
          "original_form": "집에서 내가 만든거랑 비겨해봐",
          "speaker_id": "1",
          "time": "20210827 17:22"
        },
        {
          "id": "MDRW2100019314.1.11",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 17:22"
        },
        {
          "id": "MDRW2100019314.1.12",
          "form": "이게 좀 더 매콤한 듯한데",
          "original_form": "이게 좀 더 매콤한 듯한데",
          "speaker_id": "2",
          "time": "20210827 17:22"
        },
        {
          "id": "MDRW2100019314.1.13",
          "form": "ㅋㅋㅋ글쿤 달콤한 맛은 어때",
          "original_form": "ㅋㅋㅋ글쿤 달콤한 맛은 어때",
          "speaker_id": "1",
          "time": "20210827 17:23"
        },
        {
          "id": "MDRW2100019314.1.14",
          "form": "반만 먹으려고 했는데 먹다보니 맛있어서 그냥 다 먹어야겠어",
          "original_form": "반만 먹으려고 했는데 먹다보니 맛있어서 그냥 다 먹어야겠어",
          "speaker_id": "2",
          "time": "20210827 17:23"
        },
        {
          "id": "MDRW2100019314.1.15",
          "form": "달콤한 건 우리거랑 큰 차이 없는듯",
          "original_form": "달콤한 건 우리거랑 큰 차이 없는듯",
          "speaker_id": "2",
          "time": "20210827 17:23"
        },
        {
          "id": "MDRW2100019314.1.16",
          "form": "가족들 저녁메뉴는 뭐야?",
          "original_form": "가족들 저녁메뉴는 뭐야?",
          "speaker_id": "2",
          "time": "20210827 17:23"
        },
        {
          "id": "MDRW2100019314.1.17",
          "form": "잘했어유",
          "original_form": "잘했어유",
          "speaker_id": "1",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019314.1.18",
          "form": "돈까스 먹었지",
          "original_form": "돈까스 먹었지",
          "speaker_id": "1",
          "time": "20210827 17:44"
        },
        {
          "id": "MDRW2100019314.1.19",
          "form": "아 시장표 돈까스",
          "original_form": "아 시장표 돈까스",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019314.1.20",
          "form": "든든히 잘먹었네",
          "original_form": "든든히 잘먹었네",
          "speaker_id": "2",
          "time": "20210827 17:45"
        },
        {
          "id": "MDRW2100019314.1.21",
          "form": "엉 배부르다 집에 김치찌개도 끓였어 아빠가",
          "original_form": "엉 배부르다 집에 김치찌개도 끓였어 아빠가",
          "speaker_id": "1",
          "time": "20210827 17:46"
        },
        {
          "id": "MDRW2100019314.1.22",
          "form": "오 요즘 메뉴가 화려하네",
          "original_form": "오 요즘 메뉴가 화려하네",
          "speaker_id": "2",
          "time": "20210827 19:37"
        }
