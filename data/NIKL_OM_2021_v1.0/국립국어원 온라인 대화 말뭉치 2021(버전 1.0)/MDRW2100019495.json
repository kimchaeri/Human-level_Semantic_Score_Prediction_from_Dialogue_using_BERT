      "utterance": [
        {
          "id": "MDRW2100019495.1.1",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100019495.1.2",
          "form": "특별시 하고 있는 운동은 없어요",
          "original_form": "특별시 하고 있는 운동은 없어요",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019495.1.3",
          "form": "운동하시나요",
          "original_form": "운동하시나요",
          "speaker_id": "2",
          "time": "20210827 22:37"
        },
        {
          "id": "MDRW2100019495.1.4",
          "form": "저도요.ㅡ",
          "original_form": "저도요.ㅡ",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100019495.1.5",
          "form": "홈트 하려곤 하는데",
          "original_form": "홈트 하려곤 하는데",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100019495.1.6",
          "form": "맨날 안해요 ㅠ",
          "original_form": "맨날 안해요 ㅠ",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100019495.1.7",
          "form": "꾸준히ㅈ하는거 어렵죠ㅠ",
          "original_form": "꾸준히ㅈ하는거 어렵죠ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100019495.1.8",
          "form": "약간 운동은 내일부터 ㅎ",
          "original_form": "약간 운동은 내일부터 ㅎ",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019495.1.9",
          "form": "국룰아닌가요",
          "original_form": "국룰아닌가요",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019495.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅎ 그렇죠",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅎ 그렇죠",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019495.1.11",
          "form": "꾸준함이 적은 저 반성해요ㅋㅋㅋㅋㅋ",
          "original_form": "꾸준함이 적은 저 반성해요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100019495.1.12",
          "form": "동작은 주로 어떤거",
          "original_form": "동작은 주로 어떤거",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100019495.1.13",
          "form": "하시나요",
          "original_form": "하시나요",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100019495.1.14",
          "form": "땅끄부부 아시나요?",
          "original_form": "땅끄부부 아시나요?",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100019495.1.15",
          "form": "어 들어본거같아요",
          "original_form": "어 들어본거같아요",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100019495.1.16",
          "form": "그거 참고하시나요",
          "original_form": "그거 참고하시나요",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100019495.1.17",
          "form": "네ㅋㅋㅋㅋㅋ 그거하면",
          "original_form": "네ㅋㅋㅋㅋㅋ 그거하면",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019495.1.18",
          "form": "다리가 후들거려요..",
          "original_form": "다리가 후들거려요..",
          "speaker_id": "1",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019495.1.19",
          "form": "어떤걸",
          "original_form": "어떤걸",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019495.1.20",
          "form": "하셨나요",
          "original_form": "하셨나요",
          "speaker_id": "2",
          "time": "20210827 23:22"
        },
        {
          "id": "MDRW2100019495.1.21",
          "form": "칼폭소인가",
          "original_form": "칼폭소인가",
          "speaker_id": "1",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100019495.1.22",
          "form": "그거요ㅋㅋㅋㅋㅋ",
          "original_form": "그거요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:23"
        },
        {
          "id": "MDRW2100019495.1.23",
          "form": "그게 뭐죠?",
          "original_form": "그게 뭐죠?",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100019495.1.24",
          "form": "어떤동작이지",
          "original_form": "어떤동작이지",
          "speaker_id": "2",
          "time": "20210827 23:24"
        },
        {
          "id": "MDRW2100019495.1.25",
          "form": "ㅋㅋㅋㅋㅋ쳐보시명 알아요",
          "original_form": "ㅋㅋㅋㅋㅋ쳐보시명 알아요",
          "speaker_id": "1",
          "time": "20210828 12:27"
