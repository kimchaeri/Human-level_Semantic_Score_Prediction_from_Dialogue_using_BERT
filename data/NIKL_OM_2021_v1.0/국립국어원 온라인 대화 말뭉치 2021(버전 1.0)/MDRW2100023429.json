      "utterance": [
        {
          "id": "MDRW2100023429.1.1",
          "form": "전 삼양라면이랑 짜파게티 좋아해요",
          "original_form": "전 삼양라면이랑 짜파게티 좋아해요",
          "speaker_id": "2",
          "time": "20210830 02:15"
        },
        {
          "id": "MDRW2100023429.1.2",
          "form": "오홍ㅋㅋㅋ",
          "original_form": "오홍ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 02:16"
        },
        {
          "id": "MDRW2100023429.1.3",
          "form": "저는 참깨라면파에요",
          "original_form": "저는 참깨라면파에요",
          "speaker_id": "1",
          "time": "20210830 02:16"
        },
        {
          "id": "MDRW2100023429.1.4",
          "form": "요즘 sns에서 열라면에 순두부 넣어먹는거 유행하던데",
          "original_form": "요즘 sns에서 열라면에 순두부 넣어먹는거 유행하던데",
          "speaker_id": "1",
          "time": "20210830 02:16"
        },
        {
          "id": "MDRW2100023429.1.5",
          "form": "드셔보셨어여?",
          "original_form": "드셔보셨어여?",
          "speaker_id": "1",
          "time": "20210830 02:16"
        },
        {
          "id": "MDRW2100023429.1.6",
          "form": "아뇨ㅠ 아직이여",
          "original_form": "아뇨ㅠ 아직이여",
          "speaker_id": "2",
          "time": "20210830 02:17"
        },
        {
          "id": "MDRW2100023429.1.7",
          "form": "name1 드셔보셨어요?",
          "original_form": "&name1& 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210830 02:18"
        },
        {
          "id": "MDRW2100023429.1.8",
          "form": "어제 누가 맛있다고해서",
          "original_form": "어제 누가 맛있다고해서",
          "speaker_id": "1",
          "time": "20210830 02:18"
        },
        {
          "id": "MDRW2100023429.1.9",
          "form": "진짜 먹어봤는데",
          "original_form": "진짜 먹어봤는데",
          "speaker_id": "1",
          "time": "20210830 02:18"
        },
        {
          "id": "MDRW2100023429.1.10",
          "form": "맛있어요ㅋㅋㅋㅋ",
          "original_form": "맛있어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 02:18"
        },
        {
          "id": "MDRW2100023429.1.11",
          "form": "순두부때문에 속이 편해요",
          "original_form": "순두부때문에 속이 편해요",
          "speaker_id": "1",
          "time": "20210830 02:18"
        },
        {
          "id": "MDRW2100023429.1.12",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210830 02:18"
        },
        {
          "id": "MDRW2100023429.1.13",
          "form": "나중에 한번 먹어봐야겠어요",
          "original_form": "나중에 한번 먹어봐야겠어요",
          "speaker_id": "2",
          "time": "20210830 02:19"
        },
        {
          "id": "MDRW2100023429.1.14",
          "form": "배달음식 자주 시켜드세요?",
          "original_form": "배달음식 자주 시켜드세요?",
          "speaker_id": "1",
          "time": "20210830 02:19"
        },
        {
          "id": "MDRW2100023429.1.15",
          "form": "아녀ㅎㅎ 돈아낄겸 왠만해서는 해먹는편이예요",
          "original_form": "아녀ㅎㅎ 돈아낄겸 왠만해서는 해먹는편이예요",
          "speaker_id": "2",
          "time": "20210830 02:22"
        },
        {
          "id": "MDRW2100023429.1.16",
          "form": "저는 돈아낀다고 방문포장해먹는데",
          "original_form": "저는 돈아낀다고 방문포장해먹는데",
          "speaker_id": "1",
          "time": "20210830 02:22"
        },
        {
          "id": "MDRW2100023429.1.17",
          "form": "해먹는 방법도 있었군요....ㅠㅠㅠㅋㅋㅋㅋ",
          "original_form": "해먹는 방법도 있었군요....ㅠㅠㅠㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 02:22"
        },
        {
          "id": "MDRW2100023429.1.18",
          "form": "ㅋㅋㅋㅋ큐ㅠㅠㅠ 근데 사먹는게 더 쌀때도 있어서",
          "original_form": "ㅋㅋㅋㅋ큐ㅠㅠㅠ 근데 사먹는게 더 쌀때도 있어서",
          "speaker_id": "2",
          "time": "20210830 02:23"
        },
        {
          "id": "MDRW2100023429.1.19",
          "form": "아아 맞아여",
          "original_form": "아아 맞아여",
          "speaker_id": "1",
          "time": "20210830 02:23"
        },
        {
          "id": "MDRW2100023429.1.20",
          "form": "지난주에 요기요 파리바게트 방문포장하면",
          "original_form": "지난주에 요기요 파리바게트 방문포장하면",
          "speaker_id": "1",
          "time": "20210830 02:23"
        },
        {
          "id": "MDRW2100023429.1.21",
          "form": "엄청 싸서 샐러드 많이 사다먹었어욬ㅋㅋㅋ",
          "original_form": "엄청 싸서 샐러드 많이 사다먹었어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 02:23"
        },
        {
          "id": "MDRW2100023429.1.22",
          "form": "오 진짜요??",
          "original_form": "오 진짜요??",
          "speaker_id": "2",
          "time": "20210830 02:24"
        },
        {
          "id": "MDRW2100023429.1.23",
          "form": "꿀팁이네요",
          "original_form": "꿀팁이네요",
          "speaker_id": "2",
          "time": "20210830 02:24"
        },
        {
          "id": "MDRW2100023429.1.24",
