      "utterance": [
        {
          "id": "MDRW2100021665.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021665.1.2",
          "form": "저녁 드셨나요?",
          "original_form": "저녁 드셨나요?",
          "speaker_id": "1",
          "time": "20210828 21:24"
        },
        {
          "id": "MDRW2100021665.1.3",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100021665.1.4",
          "form": "네 닭갈비 먹었어요~",
          "original_form": "네 닭갈비 먹었어요~",
          "speaker_id": "2",
          "time": "20210828 21:25"
        },
        {
          "id": "MDRW2100021665.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021665.1.6",
          "form": "와 완전 맛있는 거 드셨네요",
          "original_form": "와 완전 맛있는 거 드셨네요",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021665.1.7",
          "form": "전 된장찌개에 집밥 먹었어요",
          "original_form": "전 된장찌개에 집밥 먹었어요",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021665.1.8",
          "form": "닭갈비는 외식하셨나요 아님 배달앱?",
          "original_form": "닭갈비는 외식하셨나요 아님 배달앱?",
          "speaker_id": "1",
          "time": "20210828 21:26"
        },
        {
          "id": "MDRW2100021665.1.9",
          "form": "포장해왔어요!",
          "original_form": "포장해왔어요!",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021665.1.10",
          "form": "된장찌개도 맛있었겠어요",
          "original_form": "된장찌개도 맛있었겠어요",
          "speaker_id": "2",
          "time": "20210828 21:27"
        },
        {
          "id": "MDRW2100021665.1.11",
          "form": "차돌박이 듬뿍 넣은 된장찌개라 좋았어요",
          "original_form": "차돌박이 듬뿍 넣은 된장찌개라 좋았어요",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021665.1.12",
          "form": "김치찌개 된장찌개 뭐가 더 좋으세요?",
          "original_form": "김치찌개 된장찌개 뭐가 더 좋으세요?",
          "speaker_id": "1",
          "time": "20210828 21:28"
        },
        {
          "id": "MDRW2100021665.1.13",
          "form": "하....진짜 어려운 선택인데요ㅋㅋ",
          "original_form": "하....진짜 어려운 선택인데요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021665.1.14",
          "form": "요즘은 된장이 좀더 끌리네요",
          "original_form": "요즘은 된장이 좀더 끌리네요",
          "speaker_id": "2",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021665.1.15",
          "form": "ㅎㅎ저도 된장찌개파예요",
          "original_form": "ㅎㅎ저도 된장찌개파예요",
          "speaker_id": "1",
          "time": "20210828 21:30"
        },
        {
          "id": "MDRW2100021665.1.16",
          "form": "그래도 비오면 칼칼한 김치찌개가 생각나죠",
          "original_form": "그래도 비오면 칼칼한 김치찌개가 생각나죠",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100021665.1.17",
          "form": "김치찌개에는 고기? 참치?",
          "original_form": "김치찌개에는 고기? 참치?",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100021665.1.18",
          "form": "어려운질문만 하시네요ㅠㅋㅋㅋㅋㅋ",
          "original_form": "어려운질문만 하시네요ㅠㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:33"
        },
        {
          "id": "MDRW2100021665.1.19",
          "form": "고기는 얼마전에 넣어먹었으니 참치가 끌려요!",
          "original_form": "고기는 얼마전에 넣어먹었으니 참치가 끌려요!",
          "speaker_id": "2",
          "time": "20210828 21:33"
        },
        {
          "id": "MDRW2100021665.1.20",
          "form": "장마도 지겨운데 김치전이나 해먹을까봐요",
          "original_form": "장마도 지겨운데 김치전이나 해먹을까봐요",
          "speaker_id": "2",
          "time": "20210828 21:33"
        },
        {
          "id": "MDRW2100021665.1.21",
          "form": "ㅎㅎ 전 고기파요ㅎ  근데 캠핑가면 참치가 넘 맛나요",
          "original_form": "ㅎㅎ 전 고기파요ㅎ  근데 캠핑가면 참치가 넘 맛나요",
          "speaker_id": "1",
          "time": "20210828 21:33"
        },
        {
          "id": "MDRW2100021665.1.22",
          "form": "헉 김치전 최고죠ㅜ직접 요리하시나요",
          "original_form": "헉 김치전 최고죠ㅜ직접 요리하시나요",
          "speaker_id": "1",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100021665.1.23",
          "form": "캠핑가면 다 맛있잖아요ㅎㅎㅎ",
          "original_form": "캠핑가면 다 맛있잖아요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100021665.1.24",
          "form": "제가직접은 못하고..어머니 보조정도ㅎㅎ",
          "original_form": "제가직접은 못하고..어머니 보조정도ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100021665.1.25",
          "form": "ㅎㅎ 어머니 솜씨가 좋으시군요",
          "original_form": "ㅎㅎ 어머니 솜씨가 좋으시군요",
          "speaker_id": "1",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100021665.1.26",
          "form": "오늘 야식으로 김치전 드실 건가요?",
          "original_form": "오늘 야식으로 김치전 드실 건가요?",
          "speaker_id": "1",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100021665.1.27",
          "form": "오늘은 배불러서 내일로 미뤄야겠어요!",
          "original_form": "오늘은 배불러서 내일로 미뤄야겠어요!",
          "speaker_id": "2",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100021665.1.28",
          "form": "뭐먹을지 정하면 낼하루가 또 힘이나잖아요ㅋㅋ",
          "original_form": "뭐먹을지 정하면 낼하루가 또 힘이나잖아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100021665.1.29",
          "form": "ㅎㅎ그렇죠! 배우신 분이네요",
          "original_form": "ㅎㅎ그렇죠! 배우신 분이네요",
          "speaker_id": "1",
          "time": "20210828 21:44"
        }
      ]
    }
