          "speaker_id": "1",
          "time": "20210901 16:09"
        },
        {
          "id": "MDRW2100025691.1.4",
          "form": "어휴ㅋ",
          "original_form": "어휴ㅋ",
          "speaker_id": "1",
          "time": "20210901 16:09"
        },
        {
          "id": "MDRW2100025691.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 16:41"
        },
        {
          "id": "MDRW2100025691.1.6",
          "form": "초대하고 싶은데 시간이 없군",
          "original_form": "초대하고 싶은데 시간이 없군",
          "speaker_id": "2",
          "time": "20210901 16:42"
        },
        {
          "id": "MDRW2100025691.1.7",
          "form": "ㅋㅋㅋ 라떼사왔다",
          "original_form": "ㅋㅋㅋ 라떼사왔다",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025691.1.8",
          "form": "스벅에서",
          "original_form": "스벅에서",
          "speaker_id": "1",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025691.1.9",
          "form": "오 이시간에 스벅 라떼라니",
          "original_form": "오 이시간에 스벅 라떼라니",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025691.1.10",
          "form": "멋진데",
          "original_form": "멋진데",
          "speaker_id": "2",
          "time": "20210901 17:34"
        },
        {
          "id": "MDRW2100025691.1.11",
          "form": "이시간에 가면 멋있나?ㅋㅋ",
          "original_form": "이시간에 가면 멋있나?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100025691.1.12",
          "form": "저녁은 빵먹는단다",
          "original_form": "저녁은 빵먹는단다",
          "speaker_id": "1",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100025691.1.13",
          "form": "우리식구들이",
          "original_form": "우리식구들이",
          "speaker_id": "1",
          "time": "20210901 17:36"
        },
        {
          "id": "MDRW2100025691.1.14",
          "form": "오 저녁을 빵으로 때우다니",
          "original_form": "오 저녁을 빵으로 때우다니",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025691.1.15",
          "form": "대단한데",
          "original_form": "대단한데",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025691.1.16",
          "form": "그것도 멋진걸",
          "original_form": "그것도 멋진걸",
          "speaker_id": "2",
          "time": "20210901 18:17"
        },
        {
          "id": "MDRW2100025691.1.17",
          "form": "다먹었는데 배가 안부르다ㅡㅡ어쩌지",
          "original_form": "다먹었는데 배가 안부르다ㅡㅡ어쩌지",
          "speaker_id": "1",
          "time": "20210901 20:05"
        },
        {
          "id": "MDRW2100025691.1.18",
          "form": "ㅋㅋㅋㅋㅋㄱ  그래 빵이 저녁은 안되지",
          "original_form": "ㅋㅋㅋㅋㅋㄱ  그래 빵이 저녁은 안되지",
          "speaker_id": "2",
          "time": "20210901 20:12"
        },
        {
