      "utterance": [
        {
          "id": "MDRW2100046926.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20211108 10:47"
        },
        {
          "id": "MDRW2100046926.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211108 11:00"
        },
        {
          "id": "MDRW2100046926.1.3",
          "form": "요즘 즐기고 계시는 취미가 있으신가요?",
          "original_form": "요즘 즐기고 계시는 취미가 있으신가요?",
          "speaker_id": "2",
          "time": "20211108 11:00"
        },
        {
          "id": "MDRW2100046926.1.4",
          "form": "집에서 넷플릭스 보는게 취미에요",
          "original_form": "집에서 넷플릭스 보는게 취미에요",
          "speaker_id": "1",
          "time": "20211108 11:40"
        },
        {
          "id": "MDRW2100046926.1.5",
          "form": "ㅎㅎ 저두요",
          "original_form": "ㅎㅎ 저두요",
          "speaker_id": "2",
          "time": "20211109 00:14"
        },
        {
          "id": "MDRW2100046926.1.6",
          "form": "코로나 이후에 전국민 취미 대통합 된 느낌",
          "original_form": "코로나 이후에 전국민 취미 대통합 된 느낌",
          "speaker_id": "2",
          "time": "20211109 00:15"
        },
        {
          "id": "MDRW2100046926.1.7",
          "form": "키키",
          "original_form": "키키",
          "speaker_id": "2",
          "time": "20211109 00:15"
        },
        {
          "id": "MDRW2100046926.1.8",
          "form": "넷플릭스 말곤 볼께 없어요 ㅋㅋ",
          "original_form": "넷플릭스 말곤 볼께 없어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20211109 00:20"
        },
        {
          "id": "MDRW2100046926.1.9",
          "form": "모범택시 보셨나요,",
          "original_form": "모범택시 보셨나요,",
          "speaker_id": "1",
          "time": "20211109 00:23"
        },
        {
          "id": "MDRW2100046926.1.10",
          "form": "??",
          "original_form": "??",
          "speaker_id": "1",
          "time": "20211109 00:23"
        },
        {
          "id": "MDRW2100046926.1.11",
          "form": "사실 넷플릭스에 있는 것들만 봐도",
          "original_form": "사실 넷플릭스에 있는 것들만 봐도",
          "speaker_id": "2",
          "time": "20211109 00:26"
        },
        {
          "id": "MDRW2100046926.1.12",
          "form": "넘쳐나죠 ㅠㅠㅋㅋㅋㅋ 못본거 너무 많아요...",
          "original_form": "넘쳐나죠 ㅠㅠㅋㅋㅋㅋ 못본거 너무 많아요...",
          "speaker_id": "2",
          "time": "20211109 00:26"
        },
        {
          "id": "MDRW2100046926.1.13",
          "form": "아니요 ㅠ 근데 무슨 내용인지는 대충 알아요 ㅎㅎ",
          "original_form": "아니요 ㅠ 근데 무슨 내용인지는 대충 알아요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211109 00:26"
        },
        {
          "id": "MDRW2100046926.1.14",
          "form": "이제훈 나오고",
          "original_form": "이제훈 나오고",
          "speaker_id": "2",
          "time": "20211109 00:26"
        },
        {
          "id": "MDRW2100046926.1.15",
          "form": "차지연 배우님이 악역으로 나오는 드라마 맞죠 ㅎㅎ",
          "original_form": "차지연 배우님이 악역으로 나오는 드라마 맞죠 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211109 00:26"
        },
        {
          "id": "MDRW2100046926.1.16",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20211109 00:29"
        },
        {
          "id": "MDRW2100046926.1.17",
          "form": "재밌나요??",
          "original_form": "재밌나요??",
          "speaker_id": "2",
          "time": "20211109 00:29"
        },
        {
          "id": "MDRW2100046926.1.18",
          "form": "네 재미있었어요",
          "original_form": "네 재미있었어요",
          "speaker_id": "1",
          "time": "20211109 00:29"
        },
        {
          "id": "MDRW2100046926.1.19",
          "form": "몇부작 정도 되나요?",
          "original_form": "몇부작 정도 되나요?",
          "speaker_id": "2",
          "time": "20211109 00:30"
        },
        {
          "id": "MDRW2100046926.1.20",
          "form": "억울한 피해자를 위해 대신 복수하는 복수극 이랄까뇨",
          "original_form": "억울한 피해자를 위해 대신 복수하는 복수극 이랄까뇨",
          "speaker_id": "1",
          "time": "20211109 00:30"
        },
        {
          "id": "MDRW2100046926.1.21",
          "form": "요",
          "original_form": "요",
          "speaker_id": "1",
          "time": "20211109 00:30"
        },
        {
          "id": "MDRW2100046926.1.22",
          "form": "16부작까지 나왔어요",
          "original_form": "16부작까지 나왔어요",
          "speaker_id": "1",
          "time": "20211109 00:31"
        },
        {
          "id": "MDRW2100046926.1.23",
          "form": "요즘 현대인들에게 딱 맞는 내용이군요",
          "original_form": "요즘 현대인들에게 딱 맞는 내용이군요",
          "speaker_id": "2",
          "time": "20211109 00:31"
        },
        {
          "id": "MDRW2100046926.1.24",
          "form": "사이다 전개인가요 ㅎㅎㅎ",
          "original_form": "사이다 전개인가요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20211109 00:31"
        },
        {
          "id": "MDRW2100046926.1.25",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20211109 00:31"
        },
        {
          "id": "MDRW2100046926.1.26",
          "form": "나중에 시간 여유 있을 때 몰아서 봐야겠따",
          "original_form": "나중에 시간 여유 있을 때 몰아서 봐야겠따",
          "speaker_id": "2",
          "time": "20211109 00:31"
        },
        {
          "id": "MDRW2100046926.1.27",
          "form": "웹툰이 원작이에요",
          "original_form": "웹툰이 원작이에요",
          "speaker_id": "1",
          "time": "20211109 00:31"
        },
        {
          "id": "MDRW2100046926.1.28",
          "form": "아 정말요!?",
          "original_form": "아 정말요!?",
          "speaker_id": "2",
          "time": "20211109 00:32"
        },
        {
          "id": "MDRW2100046926.1.29",
          "form": "그건 몰랐네요...!!!",
          "original_form": "그건 몰랐네요...!!!",
          "speaker_id": "2",
          "time": "20211109 00:32"
        },
        {
          "id": "MDRW2100046926.1.30",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20211109 00:32"
        },
        {
          "id": "MDRW2100046926.1.31",
          "form": "웹툰도",
          "original_form": "웹툰도",
          "speaker_id": "2",
          "time": "20211109 00:32"
        },
        {
          "id": "MDRW2100046926.1.32",
          "form": "보셨어요?",
          "original_form": "보셨어요?",
          "speaker_id": "2",
          "time": "20211109 00:32"
        },
        {
          "id": "MDRW2100046926.1.33",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20211109 00:32"
        },
        {
          "id": "MDRW2100046926.1.34",
          "form": "웹툰은 아직안봤어요",
          "original_form": "웹툰은 아직안봤어요",
          "speaker_id": "1",
          "time": "20211109 00:32"
        },
        {
          "id": "MDRW2100046926.1.35",
          "form": "아하 혹시",
          "original_form": "아하 혹시",
          "speaker_id": "2",
          "time": "20211109 00:36"
        },
        {
          "id": "MDRW2100046926.1.36",
          "form": "웹툰이 더 재밌으면",
          "original_form": "웹툰이 더 재밌으면",
          "speaker_id": "2",
          "time": "20211109 00:36"
        },
        {
          "id": "MDRW2100046926.1.37",
          "form": "드라마말고",
          "original_form": "드라마말고",
          "speaker_id": "2",
          "time": "20211109 00:36"
        },
        {
          "id": "MDRW2100046926.1.38",
          "form": "웹툰을 볼까..했는데",
          "original_form": "웹툰을 볼까..했는데",
          "speaker_id": "2",
          "time": "20211109 00:36"
        },
        {
          "id": "MDRW2100046926.1.39",
          "form": "그냥 드라마를 봐야겠네요 ㅎㅎ",
          "original_form": "그냥 드라마를 봐야겠네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20211109 00:36"
        },
        {
          "id": "MDRW2100046926.1.40",
          "form": "추천 감사해요",
          "original_form": "추천 감사해요",
          "speaker_id": "2",
          "time": "20211109 00:36"
        }
      ]
    }
