      "utterance": [
        {
          "id": "MDRW2100036689.1.1",
          "form": "저도 그래서 집에서 요리하는 편이에요",
          "original_form": "저도 그래서 집에서 요리하는 편이에요",
          "speaker_id": "1",
          "time": "20210831 08:54"
        },
        {
          "id": "MDRW2100036689.1.2",
          "form": "여자분이신가요?",
          "original_form": "여자분이신가요?",
          "speaker_id": "2",
          "time": "20210831 09:17"
        },
        {
          "id": "MDRW2100036689.1.3",
          "form": "저는 집에서 요리하는게 힘들어서ㅜ저도 주부예요~",
          "original_form": "저는 집에서 요리하는게 힘들어서ㅜ저도 주부예요~",
          "speaker_id": "2",
          "time": "20210831 09:17"
        },
        {
          "id": "MDRW2100036689.1.4",
          "form": "여자에요 ㅎㅎ 근데 비혼이라 주부라고 하긴 그렇고",
          "original_form": "여자에요 ㅎㅎ 근데 비혼이라 주부라고 하긴 그렇고",
          "speaker_id": "1",
          "time": "20210831 09:20"
        },
        {
          "id": "MDRW2100036689.1.5",
          "form": "집안일을 많이 하죠",
          "original_form": "집안일을 많이 하죠",
          "speaker_id": "1",
          "time": "20210831 09:20"
        },
        {
          "id": "MDRW2100036689.1.6",
          "form": "집안일 어떤거에 중점을 두시나요?",
          "original_form": "집안일 어떤거에 중점을 두시나요?",
          "speaker_id": "2",
          "time": "20210831 09:22"
        },
        {
          "id": "MDRW2100036689.1.7",
          "form": "요즘은 살균소독과 짐정리요",
          "original_form": "요즘은 살균소독과 짐정리요",
          "speaker_id": "1",
          "time": "20210831 09:22"
        },
        {
          "id": "MDRW2100036689.1.8",
          "form": "와~살균소독~저도 얼마전 짐정리했었는데ㅜ넘 힘들더라구요",
          "original_form": "와~살균소독~저도 얼마전 짐정리했었는데ㅜ넘 힘들더라구요",
          "speaker_id": "2",
          "time": "20210831 09:29"
        },
        {
          "id": "MDRW2100036689.1.9",
          "form": "깔끔하신가봐요?",
          "original_form": "깔끔하신가봐요?",
          "speaker_id": "2",
          "time": "20210831 09:29"
        },
        {
          "id": "MDRW2100036689.1.10",
          "form": "코로나 이후로 집착적으로 살균소독하고 있어요.. 원래는 안그랬죠",
          "original_form": "코로나 이후로 집착적으로 살균소독하고 있어요.. 원래는 안그랬죠",
          "speaker_id": "1",
          "time": "20210831 09:33"
        },
        {
          "id": "MDRW2100036689.1.11",
          "form": "저는 살짝 코로나에 대해서 둔감해졌어요ㅜ부지런하신것같아요~",
          "original_form": "저는 살짝 코로나에 대해서 둔감해졌어요ㅜ부지런하신것같아요~",
          "speaker_id": "2",
          "time": "20210831 10:45"
        },
        {
          "id": "MDRW2100036689.1.12",
          "form": "근데 그렇게 하다보니 좀 피폐해지는 듯 해요.. 너무 강박적이 되야한다고 하나",
          "original_form": "근데 그렇게 하다보니 좀 피폐해지는 듯 해요.. 너무 강박적이 되야한다고 하나",
          "speaker_id": "1",
          "time": "20210831 11:16"
        },
        {
          "id": "MDRW2100036689.1.13",
          "form": "저는 게을러서ㅜㅜ문제예요ㅜ",
          "original_form": "저는 게을러서ㅜㅜ문제예요ㅜ",
          "speaker_id": "2",
          "time": "20210831 12:12"
        },
        {
          "id": "MDRW2100036689.1.14",
          "form": "저는 너무 예민해서 오히려 요새는 좀 놔두려고 하고 있어요",
          "original_form": "저는 너무 예민해서 오히려 요새는 좀 놔두려고 하고 있어요",
          "speaker_id": "1",
          "time": "20210831 12:17"
        },
        {
          "id": "MDRW2100036689.1.15",
          "form": "이게 다 코로나때문이죠ㅜ",
          "original_form": "이게 다 코로나때문이죠ㅜ",
          "speaker_id": "2",
          "time": "20210831 12:48"
        },
        {
          "id": "MDRW2100036689.1.16",
          "form": "맞아요.. 옷도 더 자주 빨게되고..",
          "original_form": "맞아요.. 옷도 더 자주 빨게되고..",
          "speaker_id": "1",
          "time": "20210831 12:50"
        },
        {
          "id": "MDRW2100036689.1.17",
          "form": "신경쓸게 한두개가 아니에요",
          "original_form": "신경쓸게 한두개가 아니에요",
          "speaker_id": "1",
          "time": "20210831 12:50"
        },
        {
          "id": "MDRW2100036689.1.18",
          "form": "그니깐요ㅜ그래도 부지런하셔서~",
          "original_form": "그니깐요ㅜ그래도 부지런하셔서~",
          "speaker_id": "2",
          "time": "20210831 12:50"
        }
      ]
