      "utterance": [
        {
          "id": "MDRW2100022579.1.1",
          "form": "던킨도 지금 할인하는거같아요",
          "original_form": "던킨도 지금 할인하는거같아요",
          "speaker_id": "2",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100022579.1.2",
          "form": "어 저도 던킨 얘기하려고 했어요 ㅎㅎ",
          "original_form": "어 저도 던킨 얘기하려고 했어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100022579.1.3",
          "form": "크리스피?",
          "original_form": "크리스피?",
          "speaker_id": "2",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100022579.1.4",
          "form": "아 ㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100022579.1.5",
          "form": "던킨 행사요@",
          "original_form": "던킨 행사요@",
          "speaker_id": "1",
          "time": "20210829 15:54"
        },
        {
          "id": "MDRW2100022579.1.6",
          "form": "페이북에 사도 할인해주고",
          "original_form": "페이북에 사도 할인해주고",
          "speaker_id": "1",
          "time": "20210829 15:55"
        },
        {
          "id": "MDRW2100022579.1.7",
          "form": "무슨 카드행사도 하고 그러더라구요",
          "original_form": "무슨 카드행사도 하고 그러더라구요",
          "speaker_id": "1",
          "time": "20210829 15:55"
        },
        {
          "id": "MDRW2100022579.1.8",
          "form": "아 저도 그거이야기해주려고햣어요 ㅋㅋㅋㅋ",
          "original_form": "아 저도 그거이야기해주려고햣어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 15:55"
        },
        {
          "id": "MDRW2100022579.1.9",
          "form": "페이북 할인 저렴한거같아요",
          "original_form": "페이북 할인 저렴한거같아요",
          "speaker_id": "2",
          "time": "20210829 15:55"
        },
        {
          "id": "MDRW2100022579.1.10",
          "form": "할인 되게 잘 아시네요",
          "original_form": "할인 되게 잘 아시네요",
          "speaker_id": "1",
          "time": "20210829 15:55"
        },
        {
          "id": "MDRW2100022579.1.11",
          "form": "근데 저 한번 던킨 갔다가 낭패 본 적 있어요 ㅋㅋ",
          "original_form": "근데 저 한번 던킨 갔다가 낭패 본 적 있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:55"
        },
        {
          "id": "MDRW2100022579.1.12",
          "form": "앗 왜요 ㅋㅋㅋㅋㅋ잘못 구매하셧나요",
          "original_form": "앗 왜요 ㅋㅋㅋㅋㅋ잘못 구매하셧나요",
          "speaker_id": "2",
          "time": "20210829 15:55"
        },
        {
          "id": "MDRW2100022579.1.13",
          "form": "지하철역에 입점한 던킨도너츠는 할인행사 안한대요!!",
          "original_form": "지하철역에 입점한 던킨도너츠는 할인행사 안한대요!!",
          "speaker_id": "1",
          "time": "20210829 15:55"
        },
        {
          "id": "MDRW2100022579.1.14",
          "form": "그래서 슬펐어요..",
          "original_form": "그래서 슬펐어요..",
          "speaker_id": "1",
          "time": "20210829 15:55"
        },
        {
          "id": "MDRW2100022579.1.15",
          "form": "아이고 그럼 정가로 구매한건가요",
          "original_form": "아이고 그럼 정가로 구매한건가요",
          "speaker_id": "2",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022579.1.16",
          "form": "아뇨 그냥 구매 안하고 나와써요..",
          "original_form": "아뇨 그냥 구매 안하고 나와써요..",
          "speaker_id": "1",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022579.1.17",
          "form": "어차피 집 가려면 거기 내려야되거든요 ㅋㅋㅋ",
          "original_form": "어차피 집 가려면 거기 내려야되거든요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022579.1.18",
          "form": "그렇군요 ㅠㅠ아쉽네요",
          "original_form": "그렇군요 ㅠㅠ아쉽네요",
          "speaker_id": "2",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022579.1.19",
          "form": "저도 가까이에는 역안에잇는덴데",
          "original_form": "저도 가까이에는 역안에잇는덴데",
          "speaker_id": "2",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022579.1.20",
          "form": "그래도 다음에 다른 지점 가서 성공했어요~",
          "original_form": "그래도 다음에 다른 지점 가서 성공했어요~",
          "speaker_id": "1",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022579.1.21",
          "form": "자세히찾아봐랴겟어요",
          "original_form": "자세히찾아봐랴겟어요",
          "speaker_id": "2",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022579.1.22",
          "form": "그 던킨 생각보다 당이 낮더라구요?",
          "original_form": "그 던킨 생각보다 당이 낮더라구요?",
          "speaker_id": "1",
          "time": "20210829 15:56"
        },
        {
          "id": "MDRW2100022579.1.23",
          "form": "오 그래요? 의외인데요?",
          "original_form": "오 그래요? 의외인데요?",
          "speaker_id": "2",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022579.1.24",
          "form": "살찔까봐 잘 안사거든요 ㅠㅠ",
          "original_form": "살찔까봐 잘 안사거든요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022579.1.25",
          "form": "그래도 다른 음식에 비하면 많은 편이긴 해요 ...",
          "original_form": "그래도 다른 음식에 비하면 많은 편이긴 해요 ...",
          "speaker_id": "1",
          "time": "20210829 15:57"
        },
        {
          "id": "MDRW2100022579.1.26",
          "form": "그래도 음료수보단 나았어요",
          "original_form": "그래도 음료수보단 나았어요",
          "speaker_id": "1",
