      "utterance": [
        {
          "id": "MDRW2100037663.1.1",
          "form": "짬뽕은 저번에 먹어봤고 오늘은 짜장이닷!!!",
          "original_form": "짬뽕은 저번에 먹어봤고 오늘은 짜장이닷!!!",
          "speaker_id": "1",
          "time": "20210901 14:45"
        },
        {
          "id": "MDRW2100037663.1.2",
          "form": "결국 중식이균",
          "original_form": "결국 중식이균",
          "speaker_id": "2",
          "time": "20210901 14:48"
        },
        {
          "id": "MDRW2100037663.1.3",
          "form": "탕수육없너유,",
          "original_form": "탕수육없너유,",
          "speaker_id": "2",
          "time": "20210901 14:48"
        },
        {
          "id": "MDRW2100037663.1.4",
          "form": "네...짜장면만 먹었슈 ㅋㅋㅋ배불러",
          "original_form": "네...짜장면만 먹었슈 ㅋㅋㅋ배불러",
          "speaker_id": "1",
          "time": "20210901 15:19"
        },
        {
          "id": "MDRW2100037663.1.5",
          "form": "ㅋㅋㅋㅋ 오늘같은날에딱인음식이군요",
          "original_form": "ㅋㅋㅋㅋ 오늘같은날에딱인음식이군요",
          "speaker_id": "2",
          "time": "20210901 15:20"
        },
        {
          "id": "MDRW2100037663.1.6",
          "form": "ㅋㅋ짬뽕 국물 맛있당..오늘 짬뽕을 시켰어야해...",
          "original_form": "ㅋㅋ짬뽕 국물 맛있당..오늘 짬뽕을 시켰어야해...",
          "speaker_id": "1",
          "time": "20210901 15:21"
        },
        {
          "id": "MDRW2100037663.1.7",
          "form": "ㅋㅋㅋㅋㅋ 어머님은짬뽕 ?",
          "original_form": "ㅋㅋㅋㅋㅋ 어머님은짬뽕 ?",
          "speaker_id": "2",
          "time": "20210901 15:22"
        },
        {
          "id": "MDRW2100037663.1.8",
          "form": "응ㅋㅋㅋ엄마짬뽕 뺏어먹었슈 ㅋㅋㅋ",
          "original_form": "응ㅋㅋㅋ엄마짬뽕 뺏어먹었슈 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:24"
        },
        {
          "id": "MDRW2100037663.1.9",
          "form": "언니는 밥먹고 갔남?!",
          "original_form": "언니는 밥먹고 갔남?!",
          "speaker_id": "1",
          "time": "20210901 15:24"
        },
        {
          "id": "MDRW2100037663.1.10",
          "form": "ㅋㅋㅋㅋ 웅 참치찌개끓여먹고나옴 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 웅 참치찌개끓여먹고나옴 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 15:28"
        },
        {
          "id": "MDRW2100037663.1.11",
          "form": "후식은뭔가요?",
          "original_form": "후식은뭔가요?",
          "speaker_id": "2",
          "time": "20210901 15:28"
        },
        {
          "id": "MDRW2100037663.1.12",
          "form": "ㅋㅋㅋ일단 배가 넘 불러서...",
          "original_form": "ㅋㅋㅋ일단 배가 넘 불러서...",
          "speaker_id": "1",
          "time": "20210901 15:34"
        },
        {
          "id": "MDRW2100037663.1.13",
          "form": "안사먹고 그냥 있다가 집에 갈지도 ㅋㅋㅋㅋㅋ",
          "original_form": "안사먹고 그냥 있다가 집에 갈지도 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:34"
        },
        {
          "id": "MDRW2100037663.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ계속 배속에서 불어",
          "original_form": "ㅋㅋㅋㅋㅋㅋ계속 배속에서 불어",
          "speaker_id": "2",
          "time": "20210901 15:35"
        },
        {
          "id": "MDRW2100037663.1.15",
          "form": "ㅋㅋㅋ 그니깐ㅋㅋㅋ 아주 배부르다 꽉 찼다",
          "original_form": "ㅋㅋㅋ 그니깐ㅋㅋㅋ 아주 배부르다 꽉 찼다",
          "speaker_id": "1",
          "time": "20210901 15:35"
        },
        {
          "id": "MDRW2100037663.1.16",
          "form": "울엄만 집에서 안먹으면 다 맛있대 ㅋㅋㅋㅋㅋ",
          "original_form": "울엄만 집에서 안먹으면 다 맛있대 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:36"
        },
        {
          "id": "MDRW2100037663.1.17",
          "form": "남이 해주는 밥 최고",
          "original_form": "남이 해주는 밥 최고",
          "speaker_id": "1",
          "time": "20210901 15:36"
