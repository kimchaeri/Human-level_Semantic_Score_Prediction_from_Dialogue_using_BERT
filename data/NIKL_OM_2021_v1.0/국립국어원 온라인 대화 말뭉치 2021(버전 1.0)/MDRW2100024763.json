      "utterance": [
        {
          "id": "MDRW2100024763.1.1",
          "form": "스포츠 좋아하세요?",
          "original_form": "스포츠 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100024763.1.2",
          "form": "전 잘몰라서ㅋㅋ",
          "original_form": "전 잘몰라서ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:12"
        },
        {
          "id": "MDRW2100024763.1.3",
          "form": "저도 룰을 다 꿰고 있고 그런 정도는 아닌데",
          "original_form": "저도 룰을 다 꿰고 있고 그런 정도는 아닌데",
          "speaker_id": "2",
          "time": "20210901 23:23"
        },
        {
          "id": "MDRW2100024763.1.4",
          "form": "이번에 올림픽 이후로 여자 배구 챙겨 보고있어요 ㅎㅎ",
          "original_form": "이번에 올림픽 이후로 여자 배구 챙겨 보고있어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100024763.1.5",
          "form": "올림픽 중에 챙겨보신 종목 있나요?",
          "original_form": "올림픽 중에 챙겨보신 종목 있나요?",
          "speaker_id": "2",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100024763.1.6",
          "form": "아니요ㅜㅠ 전 티비가 없답니다",
          "original_form": "아니요ㅜㅠ 전 티비가 없답니다",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100024763.1.7",
          "form": "올림픽할동안 바쁘기도했어요",
          "original_form": "올림픽할동안 바쁘기도했어요",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100024763.1.8",
          "form": "여배랑 양궁 기사가 많이나서",
          "original_form": "여배랑 양궁 기사가 많이나서",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100024763.1.9",
          "form": "인터뷰들은 몇개 봤어요ㅋㅋ",
          "original_form": "인터뷰들은 몇개 봤어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100024763.1.10",
          "form": "아 시기가 안 맞았군요ㅠㅠ",
          "original_form": "아 시기가 안 맞았군요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 23:27"
        },
        {
          "id": "MDRW2100024763.1.11",
          "form": "배구가 이번에 정말 극적인 순간이 많았어요 ㅎㅎ",
          "original_form": "배구가 이번에 정말 극적인 순간이 많았어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100024763.1.12",
          "form": "그래서 더 주목 받는 것 같아요!",
          "original_form": "그래서 더 주목 받는 것 같아요!",
          "speaker_id": "2",
          "time": "20210901 23:28"
        },
        {
          "id": "MDRW2100024763.1.13",
          "form": "단합도 너무 잘됐더라고요",
          "original_form": "단합도 너무 잘됐더라고요",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100024763.1.14",
          "form": "정말 최고의 팀이에요",
          "original_form": "정말 최고의 팀이에요",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100024763.1.15",
          "form": "이제 국대중심 멤버들이 다 은퇴를 해서 좀 아쉽지만 그래도 마지막 경기 결과가 좋아서 다행이었어요!",
          "original_form": "이제 국대중심 멤버들이 다 은퇴를 해서 좀 아쉽지만 그래도 마지막 경기 결과가 좋아서 다행이었어요!",
          "speaker_id": "2",
          "time": "20210901 23:38"
        },
        {
          "id": "MDRW2100024763.1.16",
          "form": "은퇴하시는구나ㅠㅜ",
          "original_form": "은퇴하시는구나ㅠㅜ",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100024763.1.17",
          "form": "진짜 아쉬워요",
          "original_form": "진짜 아쉬워요",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100024763.1.18",
          "form": "전 단합하는 모습이 너무 감동적이었어요",
          "original_form": "전 단합하는 모습이 너무 감동적이었어요",
          "speaker_id": "1",
          "time": "20210901 23:40"
        },
        {
          "id": "MDRW2100024763.1.19",
          "form": "맞아요ㅠ 공격 성공하거나 실패해도 서로 모여서 응원하는 거 너무 보기 좋았어요!",
          "original_form": "맞아요ㅠ 공격 성공하거나 실패해도 서로 모여서 응원하는 거 너무 보기 좋았어요!",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100024763.1.20",
          "form": "정말 대단하고 배우고싶더라고요",
          "original_form": "정말 대단하고 배우고싶더라고요",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100024763.1.21",
          "form": "경기 보셨다면 좋았을텐데 아쉽네요 ㅎㅎ",
          "original_form": "경기 보셨다면 좋았을텐데 아쉽네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100024763.1.22",
          "form": "그러게요ㅜㅜ",
          "original_form": "그러게요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100024763.1.23",
          "form": "양궁도 넘 멋있더라고요",
          "original_form": "양궁도 넘 멋있더라고요",
          "speaker_id": "1",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100024763.1.24",
          "form": "경기중 심박수ㅋㅋ",
          "original_form": "경기중 심박수ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:04"
        },
        {
          "id": "MDRW2100024763.1.25",
          "form": "수면쿵야 너무 웃겼어요 ㅋㅋㅋㅋㅋ",
          "original_form": "수면쿵야 너무 웃겼어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100024763.1.26",
          "form": "그정도는 돼야 월클되나보다 했는데 다른 외국인 선수들은 정상인의 심박수가 나오고 ㅋㅋㅋㅋ",
          "original_form": "그정도는 돼야 월클되나보다 했는데 다른 외국인 선수들은 정상인의 심박수가 나오고 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 00:07"
        },
        {
          "id": "MDRW2100024763.1.27",
          "form": "진짜 대단해요 다들ㅋㅋ",
          "original_form": "진짜 대단해요 다들ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 00:11"
        }
      ]
    }
