        {
          "id": "MDRW2100031860.1.1",
          "form": "냅다 유학을가버렸죠",
          "original_form": "냅다 유학을가버렸죠",
          "speaker_id": "2",
          "time": "20210905 03:17"
        },
        {
          "id": "MDRW2100031860.1.2",
          "form": "유학이요?!!?",
          "original_form": "유학이요?!!?",
          "speaker_id": "1",
          "time": "20210905 03:17"
        },
        {
          "id": "MDRW2100031860.1.3",
          "form": "대박 어디로 가셨어요?!?!",
          "original_form": "대박 어디로 가셨어요?!?!",
          "speaker_id": "1",
          "time": "20210905 03:17"
        },
        {
          "id": "MDRW2100031860.1.4",
          "form": "넹 그래서 유학생입니다지금",
          "original_form": "넹 그래서 유학생입니다지금",
          "speaker_id": "2",
          "time": "20210905 03:17"
        },
        {
          "id": "MDRW2100031860.1.5",
          "form": "헐!?!?!?",
          "original_form": "헐!?!?!?",
          "speaker_id": "1",
          "time": "20210905 03:17"
        },
        {
          "id": "MDRW2100031860.1.6",
          "form": "뉴욕으로갔는데",
          "original_form": "뉴욕으로갔는데",
          "speaker_id": "2",
          "time": "20210905 03:17"
        },
        {
          "id": "MDRW2100031860.1.7",
          "form": "지금 뉴욕이세요???",
          "original_form": "지금 뉴욕이세요???",
          "speaker_id": "1",
          "time": "20210905 03:17"
        },
        {
          "id": "MDRW2100031860.1.8",
          "form": "3학년까지하다가 코로롱때문에",
          "original_form": "3학년까지하다가 코로롱때문에",
          "speaker_id": "2",
          "time": "20210905 03:17"
        },
        {
          "id": "MDRW2100031860.1.9",
          "form": "대박?!?!?",
          "original_form": "대박?!?!?",
          "speaker_id": "1",
          "time": "20210905 03:17"
        },
        {
          "id": "MDRW2100031860.1.10",
          "form": "한국이에요지금ㅋ큐ㅠ",
          "original_form": "한국이에요지금ㅋ큐ㅠ",
          "speaker_id": "2",
          "time": "20210905 03:17"
        },
        {
          "id": "MDRW2100031860.1.11",
          "form": "와,,,, 와,,,,",
          "original_form": "와,,,, 와,,,,",
          "speaker_id": "1",
          "time": "20210905 03:17"
        },
        {
          "id": "MDRW2100031860.1.12",
          "form": "경기도...",
          "original_form": "경기도...",
          "speaker_id": "2",
          "time": "20210905 03:17"
        },
        {
          "id": "MDRW2100031860.1.13",
          "form": "와,,",
          "original_form": "와,,",
          "speaker_id": "1",
          "time": "20210905 03:17"
        },
        {
          "id": "MDRW2100031860.1.14",
          "form": "저도 경기도민인데?!?!",
          "original_form": "저도 경기도민인데?!?!",
          "speaker_id": "1",
          "time": "20210905 03:17"
        },
        {
          "id": "MDRW2100031860.1.15",
          "form": "작년4월부터쭉있네요...",
          "original_form": "작년4월부터쭉있네요...",
          "speaker_id": "2",
          "time": "20210905 03:18"
