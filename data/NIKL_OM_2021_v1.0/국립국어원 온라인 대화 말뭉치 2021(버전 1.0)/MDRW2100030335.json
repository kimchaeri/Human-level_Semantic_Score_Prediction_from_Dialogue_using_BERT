      "utterance": [
        {
          "id": "MDRW2100030335.1.1",
          "form": "종교 있니?",
          "original_form": "종교 있니?",
          "speaker_id": "2",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030335.1.2",
          "form": "종교면 나 초등학생때",
          "original_form": "종교면 나 초등학생때",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030335.1.3",
          "form": "기독교",
          "original_form": "기독교",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030335.1.4",
          "form": "하지만",
          "original_form": "하지만",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030335.1.5",
          "form": "지금은 무교",
          "original_form": "지금은 무교",
          "speaker_id": "1",
          "time": "20210903 23:53"
        },
        {
          "id": "MDRW2100030335.1.6",
          "form": "헐 너 종교 있었어??",
          "original_form": "헐 너 종교 있었어??",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030335.1.7",
          "form": "지금은 나도 무교 ㅎㅎ",
          "original_form": "지금은 나도 무교 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030335.1.8",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030335.1.9",
          "form": "친구따리",
          "original_form": "친구따리",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030335.1.10",
          "form": "근데 시내만 가면 믿을 생각도 없는데 그렇게 많이 붙잡드라",
          "original_form": "근데 시내만 가면 믿을 생각도 없는데 그렇게 많이 붙잡드라",
          "speaker_id": "2",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030335.1.11",
          "form": "라",
          "original_form": "라",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030335.1.12",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210903 23:54"
        },
        {
          "id": "MDRW2100030335.1.13",
          "form": "날치 너도 나도 만만한 인상이라 완전 많이 붙잡힐듯",
          "original_form": "날치 너도 나도 만만한 인상이라 완전 많이 붙잡힐듯",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030335.1.14",
          "form": "\"그 종교..?\"",
          "original_form": "\"그 종교..?\"",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030335.1.15",
          "form": "난 그런 낌새 보이면",
          "original_form": "난 그런 낌새 보이면",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030335.1.16",
          "form": "ㅋㅋㅋㅋ신천지 말고도",
          "original_form": "ㅋㅋㅋㅋ신천지 말고도",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030335.1.17",
          "form": "바로 튐",
          "original_form": "바로 튐",
          "speaker_id": "1",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030335.1.18",
          "form": "이상한 도 닦는 분들도잡던데",
          "original_form": "이상한 도 닦는 분들도잡던데",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030335.1.19",
          "form": "나보고 전생의 공덕이 많대",
          "original_form": "나보고 전생의 공덕이 많대",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030335.1.20",
          "form": "기도 아름답대",
          "original_form": "기도 아름답대",
          "speaker_id": "2",
          "time": "20210903 23:55"
        },
        {
          "id": "MDRW2100030335.1.21",
          "form": "기도 아름다움 빡하",
          "original_form": "기도 아름다움 빡하",
          "speaker_id": "1",
          "time": "20210903 23:56"
        },
        {
          "id": "MDRW2100030335.1.22",
          "form": "난 터미널에서 어떤 남자 두명이",
          "original_form": "난 터미널에서 어떤 남자 두명이",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030335.1.23",
          "form": "붙잡아서",
          "original_form": "붙잡아서",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030335.1.24",
          "form": "나보고 무슨띠냐고",
          "original_form": "나보고 무슨띠냐고",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030335.1.25",
          "form": "물으니까 용띠라고 대답하니",
          "original_form": "물으니까 용띠라고 대답하니",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030335.1.26",
          "form": "ㅠㅠㅠ 오 ㅐ묻는대?",
          "original_form": "ㅠㅠㅠ 오 ㅐ묻는대?",
          "speaker_id": "2",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030335.1.27",
          "form": "용띠는 대개 뭐라고 했지 무슨 좋다라는 식으로 말해줌",
          "original_form": "용띠는 대개 뭐라고 했지 무슨 좋다라는 식으로 말해줌",
          "speaker_id": "1",
          "time": "20210903 23:57"
        },
        {
          "id": "MDRW2100030335.1.28",
          "form": "그건 모르겠네...",
          "original_form": "그건 모르겠네...",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030335.1.29",
          "form": "밤늦게 라서",
          "original_form": "밤늦게 라서",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030335.1.30",
          "form": "기독교아니라도",
          "original_form": "기독교아니라도",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030335.1.31",
          "form": "ㅠㅠ 점보는 분들인가?",
          "original_form": "ㅠㅠ 점보는 분들인가?",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030335.1.32",
          "form": "다른 신자가 아닐까...",
          "original_form": "다른 신자가 아닐까...",
          "speaker_id": "1",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030335.1.33",
          "form": "어우...붙잡지 마세요",
          "original_form": "어우...붙잡지 마세요",
          "speaker_id": "2",
          "time": "20210903 23:58"
        },
        {
          "id": "MDRW2100030335.1.34",
          "form": "그치그치 너도 조심혀",
          "original_form": "그치그치 너도 조심혀",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030335.1.35",
          "form": "난 사실 사이비 말고 그냥 교회두 싫어",
          "original_form": "난 사실 사이비 말고 그냥 교회두 싫어",
          "speaker_id": "2",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030335.1.36",
          "form": "좀..나랑 안 맞는 것 같애",
          "original_form": "좀..나랑 안 맞는 것 같애",
          "speaker_id": "2",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030335.1.37",
          "form": "교회 성경책에서 기억나는 구절이",
          "original_form": "교회 성경책에서 기억나는 구절이",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030335.1.38",
          "form": "사람은 흑으로",
          "original_form": "사람은 흑으로",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030335.1.39",
          "form": "시작했다는거",
          "original_form": "시작했다는거",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030335.1.40",
          "form": "ㅋㅋㅋㅋ아 맞아 그랬던 것 같애",
          "original_form": "ㅋㅋㅋㅋ아 맞아 그랬던 것 같애",
          "speaker_id": "2",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030335.1.41",
          "form": "흙",
          "original_form": "흙",
          "speaker_id": "1",
          "time": "20210903 23:59"
        },
        {
          "id": "MDRW2100030335.1.42",
          "form": "맞어맞어",
          "original_form": "맞어맞어",
          "speaker_id": "1",
          "time": "20210903 23:59"
        }
      ]
    }
