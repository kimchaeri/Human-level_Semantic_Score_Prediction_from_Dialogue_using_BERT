      "utterance": [
        {
          "id": "MDRW2100034249.1.1",
          "form": "혹시 야식 드시나요?",
          "original_form": "혹시 야식 드시나요?",
          "speaker_id": "2",
          "time": "20210827 22:22"
        },
        {
          "id": "MDRW2100034249.1.2",
          "form": "잘 안먹어요 ㅋㅋ",
          "original_form": "잘 안먹어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100034249.1.3",
          "form": "그냥 두유정도?",
          "original_form": "그냥 두유정도?",
          "speaker_id": "1",
          "time": "20210827 22:24"
        },
        {
          "id": "MDRW2100034249.1.4",
          "form": "그기 좋은 습관이죠~ 전 어제 메밀국수 야식으로 먹었어요",
          "original_form": "그기 좋은 습관이죠~ 전 어제 메밀국수 야식으로 먹었어요",
          "speaker_id": "2",
          "time": "20210828 10:57"
        },
        {
          "id": "MDRW2100034249.1.5",
          "form": "우와 맛있겠다 해드셨나요? 아님배달?",
          "original_form": "우와 맛있겠다 해드셨나요? 아님배달?",
          "speaker_id": "1",
          "time": "20210828 12:23"
        },
        {
          "id": "MDRW2100034249.1.6",
          "form": "코스트코에 파는거 사다 끓여먹었어요",
          "original_form": "코스트코에 파는거 사다 끓여먹었어요",
          "speaker_id": "2",
          "time": "20210828 12:46"
        },
        {
          "id": "MDRW2100034249.1.7",
          "form": "인스턴트 라면 같은 건가요? 아님 일반 국수처럼 된건가요?",
          "original_form": "인스턴트 라면 같은 건가요? 아님 일반 국수처럼 된건가요?",
          "speaker_id": "1",
          "time": "20210828 12:46"
        },
        {
          "id": "MDRW2100034249.1.8",
          "form": "잠깐밥먹구올께요 잠시만요 ㅎㅎ죄송",
          "original_form": "잠깐밥먹구올께요 잠시만요 ㅎㅎ죄송",
          "speaker_id": "1",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100034249.1.9",
          "form": "면이랑 육수랑 따로 있는거요 ㅎ",
          "original_form": "면이랑 육수랑 따로 있는거요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 12:47"
        },
        {
          "id": "MDRW2100034249.1.10",
          "form": "아항 맛있겠어요 전 코슼소 회원권이 없어서 ㅠ ㅋㅋㅋ",
          "original_form": "아항 맛있겠어요 전 코슼소 회원권이 없어서 ㅠ ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100034249.1.11",
          "form": "아~ 점심은 뭐 드셨나요?",
          "original_form": "아~ 점심은 뭐 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 18:06"
        },
        {
          "id": "MDRW2100034249.1.12",
          "form": "전 그냥 밥이요 ㅋㅋ 님은요?",
          "original_form": "전 그냥 밥이요 ㅋㅋ 님은요?",
          "speaker_id": "1",
          "time": "20210828 18:09"
        },
        {
          "id": "MDRW2100034249.1.13",
          "form": "전 그 시간에 잤어요 낮잠 이제 뭐 좀 먹어야 하는데 귀찮네요",
          "original_form": "전 그 시간에 잤어요 낮잠 이제 뭐 좀 먹어야 하는데 귀찮네요",
          "speaker_id": "2",
          "time": "20210828 18:23"
        },
        {
          "id": "MDRW2100034249.1.14",
          "form": "저녁은 맛있는걸 챙겨드세욬ㅋ",
          "original_form": "저녁은 맛있는걸 챙겨드세욬ㅋ",
          "speaker_id": "1",
          "time": "20210828 18:46"
        },
        {
          "id": "MDRW2100034249.1.15",
          "form": "그래도 주말에 따끈한걸 먹어줘야",
          "original_form": "그래도 주말에 따끈한걸 먹어줘야",
          "speaker_id": "1",
          "time": "20210828 18:49"
        },
        {
          "id": "MDRW2100034249.1.16",
          "form": "방금 김치랑 김이랑 밥 먹었네요.",
          "original_form": "방금 김치랑 김이랑 밥 먹었네요.",
          "speaker_id": "2",
          "time": "20210828 19:11"
        },
        {
          "id": "MDRW2100034249.1.17",
          "form": "잘 하셨어요 전 지금 커피먹고 있어요 ㅋㅋ",
          "original_form": "잘 하셨어요 전 지금 커피먹고 있어요 ㅋㅋ",
