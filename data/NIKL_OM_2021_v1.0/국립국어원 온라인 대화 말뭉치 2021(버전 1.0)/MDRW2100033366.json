      "utterance": [
        {
          "id": "MDRW2100033366.1.1",
          "form": "name2 여행하는거 좋아해??",
          "original_form": "&name2& 여행하는거 좋아해??",
          "speaker_id": "1",
          "time": "20210906 23:07"
        },
        {
          "id": "MDRW2100033366.1.2",
          "form": "자주나가는건 싫어하는데",
          "original_form": "자주나가는건 싫어하는데",
          "speaker_id": "2",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100033366.1.3",
          "form": "나 놀러가는거 조아해~!",
          "original_form": "나 놀러가는거 조아해~!",
          "speaker_id": "2",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100033366.1.4",
          "form": "이왕 나갈거면 화끈하게^~^!",
          "original_form": "이왕 나갈거면 화끈하게^~^!",
          "speaker_id": "2",
          "time": "20210906 23:08"
        },
        {
          "id": "MDRW2100033366.1.5",
          "form": "아하 하긴 나도 자주 나가는 건 싫은데",
          "original_form": "아하 하긴 나도 자주 나가는 건 싫은데",
          "speaker_id": "1",
          "time": "20210906 23:09"
        },
        {
          "id": "MDRW2100033366.1.6",
          "form": "여행하는거면 각잡고 제대로 놀러가는게 좋아",
          "original_form": "여행하는거면 각잡고 제대로 놀러가는게 좋아",
          "speaker_id": "1",
          "time": "20210906 23:09"
        },
        {
          "id": "MDRW2100033366.1.7",
          "form": "마자마자~!~~~!",
          "original_form": "마자마자~!~~~!",
          "speaker_id": "2",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100033366.1.8",
          "form": "ㅎ ㅏ 출근했는데 여행얘기하니까",
          "original_form": "ㅎ ㅏ 출근했는데 여행얘기하니까",
          "speaker_id": "2",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100033366.1.9",
          "form": "슬프네^.ㅠ",
          "original_form": "슬프네^.ㅠ",
          "speaker_id": "2",
          "time": "20210907 09:50"
        },
        {
          "id": "MDRW2100033366.1.10",
          "form": "아...ㅜㅜ 안그래도 요즘 못놀러가는데...",
          "original_form": "아...ㅜㅜ 안그래도 요즘 못놀러가는데...",
          "speaker_id": "1",
          "time": "20210907 10:08"
        },
        {
          "id": "MDRW2100033366.1.11",
          "form": "말하니까 여행가구싶네..",
          "original_form": "말하니까 여행가구싶네..",
          "speaker_id": "1",
          "time": "20210907 10:08"
        },
        {
          "id": "MDRW2100033366.1.12",
          "form": "바다 보기만하다왔는데",
          "original_form": "바다 보기만하다왔는데",
          "speaker_id": "2",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100033366.1.13",
          "form": "바다 들어가고싶다^,^",
          "original_form": "바다 들어가고싶다^,^",
          "speaker_id": "2",
          "time": "20210907 10:10"
        },
        {
          "id": "MDRW2100033366.1.14",
          "form": "하지만 이미 여름이.. 끝났다는거",
          "original_form": "하지만 이미 여름이.. 끝났다는거",
          "speaker_id": "2",
          "time": "20210907 10:11"
        },
        {
          "id": "MDRW2100033366.1.15",
          "form": "그치... 암것도 암햇는데 벌써 끝낫네...",
          "original_form": "그치... 암것도 암햇는데 벌써 끝낫네...",
          "speaker_id": "1",
          "time": "20210907 10:13"
        },
        {
          "id": "MDRW2100033366.1.16",
          "form": "난 바다보다는 계곡이 더 조아",
          "original_form": "난 바다보다는 계곡이 더 조아",
          "speaker_id": "1",
          "time": "20210907 10:13"
        },
        {
          "id": "MDRW2100033366.1.17",
          "form": "아헐 나도나도~!~!~~!~!!!",
          "original_form": "아헐 나도나도~!~!~~!~!!!",
          "speaker_id": "2",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100033366.1.18",
          "form": "계곡이 훨씬 깨끗해",
          "original_form": "계곡이 훨씬 깨끗해",
          "speaker_id": "2",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100033366.1.19",
          "form": "바다는... 놀고나오면",
          "original_form": "바다는... 놀고나오면",
          "speaker_id": "2",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100033366.1.20",
          "form": "빤쓰 속까지 모래들어와서 싫어",
          "original_form": "빤쓰 속까지 모래들어와서 싫어",
          "speaker_id": "2",
          "time": "20210907 10:14"
        },
        {
          "id": "MDRW2100033366.1.21",
          "form": "잉 맞아.. 글고 나는 모래사장이 싫어",
          "original_form": "잉 맞아.. 글고 나는 모래사장이 싫어",
          "speaker_id": "1",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100033366.1.22",
          "form": "계곡가서 물놀이하는거 짱재밋는데...",
          "original_form": "계곡가서 물놀이하는거 짱재밋는데...",
          "speaker_id": "1",
          "time": "20210907 10:16"
        },
        {
          "id": "MDRW2100033366.1.23",
          "form": "진짜 놀러가고싶다",
          "original_form": "진짜 놀러가고싶다",
          "speaker_id": "1",
          "time": "20210907 10:17"
        },
        {
          "id": "MDRW2100033366.1.24",
          "form": "물놀이하고 수박...",
          "original_form": "물놀이하고 수박...",
          "speaker_id": "2",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100033366.1.25",
          "form": "그리고아이스크림",
          "original_form": "그리고아이스크림",
          "speaker_id": "2",
          "time": "20210907 10:18"
        },
        {
          "id": "MDRW2100033366.1.26",
          "form": "헐 진짜 짱이지...",
          "original_form": "헐 진짜 짱이지...",
          "speaker_id": "1",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100033366.1.27",
          "form": "물놀이 다하고 씻은 담에",
          "original_form": "물놀이 다하고 씻은 담에",
          "speaker_id": "1",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100033366.1.28",
          "form": "고기 먹으면 진짜 맛잇는데",
          "original_form": "고기 먹으면 진짜 맛잇는데",
          "speaker_id": "1",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100033366.1.29",
          "form": "아헐",
          "original_form": "아헐",
          "speaker_id": "2",
          "time": "20210907 10:19"
        },
        {
          "id": "MDRW2100033366.1.30",
          "form": "바베큐 무조건 필수코스~~~~",
          "original_form": "바베큐 무조건 필수코스~~~~",
          "speaker_id": "2",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100033366.1.31",
          "form": "물놀이하고 먹는 컵라면도 짱인데",
          "original_form": "물놀이하고 먹는 컵라면도 짱인데",
          "speaker_id": "2",
          "time": "20210907 10:20"
        },
        {
          "id": "MDRW2100033366.1.32",
          "form": "그치~~ 그냥 잔뜩 놀고나서 먹는건 다 맛있는것 같애ㅋㅋ",
          "original_form": "그치~~ 그냥 잔뜩 놀고나서 먹는건 다 맛있는것 같애ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 10:21"
        },
        {
          "id": "MDRW2100033366.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 10:23"
        },
        {
          "id": "MDRW2100033366.1.34",
          "form": "마져",
          "original_form": "마져",
          "speaker_id": "2",
          "time": "20210907 10:23"
        },
        {
          "id": "MDRW2100033366.1.35",
          "form": "배고프고 피곤할때 먹어서 더 맛있게 느껴질수도 있지만",
          "original_form": "배고프고 피곤할때 먹어서 더 맛있게 느껴질수도 있지만",
          "speaker_id": "2",
          "time": "20210907 10:23"
        },
        {
          "id": "MDRW2100033366.1.36",
          "form": "일단 그 상황? 자체가 좋아서",
          "original_form": "일단 그 상황? 자체가 좋아서",
