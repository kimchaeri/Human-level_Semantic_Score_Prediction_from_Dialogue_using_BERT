      "utterance": [
        {
          "id": "MDRW2100005821.1.1",
          "form": "요즘은 집놀이족이라고 하나보네",
          "original_form": "요즘은 집놀이족이라고 하나보네",
          "speaker_id": "1",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100005821.1.2",
          "form": "집순이의 보다 공식적인 이름인걸까",
          "original_form": "집순이의 보다 공식적인 이름인걸까",
          "speaker_id": "2",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100005821.1.3",
          "form": "그런듯ㅋㅋㅋ피포페인팅은 잘되어가?",
          "original_form": "그런듯ㅋㅋㅋ피포페인팅은 잘되어가?",
          "speaker_id": "1",
          "time": "20210907 13:24"
        },
        {
          "id": "MDRW2100005821.1.4",
          "form": "나는 누구씨랑 다르게 대충해서",
          "original_form": "나는 누구씨랑 다르게 대충해서",
          "speaker_id": "2",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100005821.1.5",
          "form": "일주일이면 다하지",
          "original_form": "일주일이면 다하지",
          "speaker_id": "2",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100005821.1.6",
          "form": "이미 다했어",
          "original_form": "이미 다했어",
          "speaker_id": "2",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100005821.1.7",
          "form": "오...빠르다",
          "original_form": "오...빠르다",
          "speaker_id": "1",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100005821.1.8",
          "form": "나는 그거 하나하나 색칠하느라 한달은 걸리던데",
          "original_form": "나는 그거 하나하나 색칠하느라 한달은 걸리던데",
          "speaker_id": "1",
          "time": "20210907 13:25"
        },
        {
          "id": "MDRW2100005821.1.9",
          "form": "니는  색 이쁘게 낼려고 덪칠해가면서 해서 그런듯",
          "original_form": "니는  색 이쁘게 낼려고 덪칠해가면서 해서 그런듯",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100005821.1.10",
          "form": "미술 좋아해서 그런가 대충이 안돼",
          "original_form": "미술 좋아해서 그런가 대충이 안돼",
          "speaker_id": "1",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100005821.1.11",
          "form": "완성된거보면 니꺼가 진짜 이쁘더라",
          "original_form": "완성된거보면 니꺼가 진짜 이쁘더라",
          "speaker_id": "2",
          "time": "20210907 13:26"
        },
        {
          "id": "MDRW2100005821.1.12",
          "form": "감사감사 또 니 뭐하는거 있지않았나?",
          "original_form": "감사감사 또 니 뭐하는거 있지않았나?",
          "speaker_id": "1",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100005821.1.13",
          "form": "나 조립좋아해서 프라모델도 몇번하고",
          "original_form": "나 조립좋아해서 프라모델도 몇번하고",
          "speaker_id": "2",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100005821.1.14",
          "form": "퍼즐도하고",
          "original_form": "퍼즐도하고",
          "speaker_id": "2",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100005821.1.15",
          "form": "집만드는거 ? 그런것도 하지",
          "original_form": "집만드는거 ? 그런것도 하지",
          "speaker_id": "2",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100005821.1.16",
          "form": "손재주가 좋아서 그런가 되게 잘하더라",
          "original_form": "손재주가 좋아서 그런가 되게 잘하더라",
          "speaker_id": "1",
          "time": "20210907 13:27"
        },
        {
          "id": "MDRW2100005821.1.17",
          "form": "요즘은 이런게 잘나와서 안어려워",
          "original_form": "요즘은 이런게 잘나와서 안어려워",
          "speaker_id": "2",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100005821.1.18",
          "form": "어딜가도 많이보이긴하더라 비싸서그렇지",
          "original_form": "어딜가도 많이보이긴하더라 비싸서그렇지",
          "speaker_id": "1",
          "time": "20210907 13:28"
        },
        {
          "id": "MDRW2100005821.1.19",
          "form": "그렇지...비싸긴해",
          "original_form": "그렇지...비싸긴해",
          "speaker_id": "2",
          "time": "20210907 13:28"
        }
