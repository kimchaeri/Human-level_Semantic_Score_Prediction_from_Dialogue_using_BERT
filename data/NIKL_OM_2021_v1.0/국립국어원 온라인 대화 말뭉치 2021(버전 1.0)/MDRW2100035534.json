      "utterance": [
        {
          "id": "MDRW2100035534.1.1",
          "form": "케이크랑 같이 드시는 음료는 어떤거인가요?",
          "original_form": "케이크랑 같이 드시는 음료는 어떤거인가요?",
          "speaker_id": "2",
          "time": "20210829 20:01"
        },
        {
          "id": "MDRW2100035534.1.2",
          "form": "저는 디카페인 커피나 우유요",
          "original_form": "저는 디카페인 커피나 우유요",
          "speaker_id": "1",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100035534.1.3",
          "form": "뭐랑 드세요?",
          "original_form": "뭐랑 드세요?",
          "speaker_id": "1",
          "time": "20210829 20:02"
        },
        {
          "id": "MDRW2100035534.1.4",
          "form": "저는 우유요!",
          "original_form": "저는 우유요!",
          "speaker_id": "2",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100035534.1.5",
          "form": "좋아하는 간식 있으세요?",
          "original_form": "좋아하는 간식 있으세요?",
          "speaker_id": "2",
          "time": "20210829 20:11"
        },
        {
          "id": "MDRW2100035534.1.6",
          "form": "저는 육포 좋아해요",
          "original_form": "저는 육포 좋아해요",
          "speaker_id": "1",
          "time": "20210829 22:47"
        },
        {
          "id": "MDRW2100035534.1.7",
          "form": "단거보다 짭짤한게 좋아서요",
          "original_form": "단거보다 짭짤한게 좋아서요",
          "speaker_id": "1",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100035534.1.8",
          "form": "어떤 육포 좋아하세요?",
          "original_form": "어떤 육포 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 23:20"
        },
        {
          "id": "MDRW2100035534.1.9",
          "form": "육포가 비싼편이라 가성비있는 그때 그때 육포사먹어요",
          "original_form": "육포가 비싼편이라 가성비있는 그때 그때 육포사먹어요",
          "speaker_id": "1",
          "time": "20210829 23:22"
        },
        {
          "id": "MDRW2100035534.1.10",
          "form": "딱히 안가려요",
          "original_form": "딱히 안가려요",
          "speaker_id": "1",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100035534.1.11",
          "form": "좋아하시는. 간식 있으신가요?",
          "original_form": "좋아하시는. 간식 있으신가요?",
          "speaker_id": "1",
          "time": "20210829 23:23"
        },
        {
          "id": "MDRW2100035534.1.12",
          "form": "아항ㅋㅋ전 과자 좋아해요ㅋㅋ",
          "original_form": "아항ㅋㅋ전 과자 좋아해요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 23:26"
        },
        {
          "id": "MDRW2100035534.1.13",
          "form": "어떤과자가 최애세요?",
          "original_form": "어떤과자가 최애세요?",
          "speaker_id": "1",
          "time": "20210829 23:28"
        },
        {
          "id": "MDRW2100035534.1.14",
          "form": "사또밥이요!!",
          "original_form": "사또밥이요!!",
          "speaker_id": "2",
          "time": "20210830 00:17"
        },
        {
          "id": "MDRW2100035534.1.15",
          "form": "예전께 더 좋은데 바뀌어서 아쉬워요ㅠ",
          "original_form": "예전께 더 좋은데 바뀌어서 아쉬워요ㅠ",
          "speaker_id": "2",
          "time": "20210830 00:17"
        },
        {
          "id": "MDRW2100035534.1.16",
          "form": "사또밥 맛있죠! 고소하고 달고",
          "original_form": "사또밥 맛있죠! 고소하고 달고",
          "speaker_id": "1",
          "time": "20210830 00:38"
        },
        {
          "id": "MDRW2100035534.1.17",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210830 00:40"
        },
        {
          "id": "MDRW2100035534.1.18",
          "form": "의외로 이 맛을 모르는 친그ㅡ가 있어서 놀랐어요",
          "original_form": "의외로 이 맛을 모르는 친그ㅡ가 있어서 놀랐어요",
          "speaker_id": "2",
          "time": "20210830 00:41"
        },
        {
          "id": "MDRW2100035534.1.19",
          "form": "많이 달지않으면서 계속 땡기는 맛인데",
          "original_form": "많이 달지않으면서 계속 땡기는 맛인데",
          "speaker_id": "1",
          "time": "20210830 00:47"
        },
        {
          "id": "MDRW2100035534.1.20",
          "form": "그맛을 모르다니 아쉽네요",
          "original_form": "그맛을 모르다니 아쉽네요",
          "speaker_id": "1",
          "time": "20210830 00:47"
        },
        {
          "id": "MDRW2100035534.1.21",
          "form": "사르르 녹는데...",
          "original_form": "사르르 녹는데...",
          "speaker_id": "1",
          "time": "20210830 00:47"
        },
        {
          "id": "MDRW2100035534.1.22",
          "form": "그니까요.. 이 맛을 모르고 어린시절을 보냈다니..하면서 사줬었어요ㅋㅋ",
          "original_form": "그니까요.. 이 맛을 모르고 어린시절을 보냈다니..하면서 사줬었어요ㅋㅋ",
