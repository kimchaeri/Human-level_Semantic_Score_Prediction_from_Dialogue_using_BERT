        },
        {
          "id": "MDRW2100000874.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210819 20:15"
        },
        {
          "id": "MDRW2100000874.1.3",
          "form": "이상형이.어찌되시나요",
          "original_form": "이상형이.어찌되시나요",
          "speaker_id": "1",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000874.1.4",
          "form": "저는 요리잘하고 상대생각해주는 남자요!",
          "original_form": "저는 요리잘하고 상대생각해주는 남자요!",
          "speaker_id": "2",
          "time": "20210819 20:22"
        },
        {
          "id": "MDRW2100000874.1.5",
          "form": "자상함도 잇음 정말 좋응거같아요",
          "original_form": "자상함도 잇음 정말 좋응거같아요",
          "speaker_id": "1",
          "time": "20210819 20:24"
        },
        {
          "id": "MDRW2100000874.1.6",
          "form": "전 아기예뻐하는 남자가 좋더라구요",
          "original_form": "전 아기예뻐하는 남자가 좋더라구요",
          "speaker_id": "1",
          "time": "20210819 20:24"
        },
        {
          "id": "MDRW2100000874.1.7",
          "form": "최근 미팅은 하셨나요",
          "original_form": "최근 미팅은 하셨나요",
          "speaker_id": "1",
          "time": "20210819 20:24"
        },
        {
          "id": "MDRW2100000874.1.8",
          "form": "아뇨 한번도 안해봤어요",
          "original_form": "아뇨 한번도 안해봤어요",
          "speaker_id": "2",
          "time": "20210819 20:24"
        },
        {
          "id": "MDRW2100000874.1.9",
          "form": "결혼은 몇년안에하실계획이신가요",
          "original_form": "결혼은 몇년안에하실계획이신가요",
          "speaker_id": "1",
          "time": "20210819 20:25"
        },
        {
          "id": "MDRW2100000874.1.10",
          "form": "결혼은 한 30세안에 하고싶어요",
          "original_form": "결혼은 한 30세안에 하고싶어요",
          "speaker_id": "2",
          "time": "20210819 20:26"
        },
        {
          "id": "MDRW2100000874.1.11",
          "form": "마자요 그 나이안에 하는게 좋은고같아요",
          "original_form": "마자요 그 나이안에 하는게 좋은고같아요",
          "speaker_id": "1",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000874.1.12",
          "form": "소개팅은 해보신적있으세요?",
          "original_form": "소개팅은 해보신적있으세요?",
          "speaker_id": "1",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000874.1.13",
          "form": "아뇨.. 이제 20살이라",
          "original_form": "아뇨.. 이제 20살이라",
          "speaker_id": "2",
          "time": "20210819 20:27"
        },
        {
          "id": "MDRW2100000874.1.14",
          "form": "아직 어리시군요",
          "original_form": "아직 어리시군요",
          "speaker_id": "1",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000874.1.15",
          "form": "많이 해보세요",
          "original_form": "많이 해보세요",
          "speaker_id": "1",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000874.1.16",
          "form": "코로나 때문에 ㅜ 과팅도 못해보고 ㅜ",
          "original_form": "코로나 때문에 ㅜ 과팅도 못해보고 ㅜ",
          "speaker_id": "2",
          "time": "20210819 20:28"
        },
        {
          "id": "MDRW2100000874.1.17",
          "form": "혹시 엠티도 못가셨겠네요?",
          "original_form": "혹시 엠티도 못가셨겠네요?",
          "speaker_id": "1",
          "time": "20210819 20:29"
        },
        {
          "id": "MDRW2100000874.1.18",
          "form": "네 ㅠㅠㅠ",
          "original_form": "네 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000874.1.19",
          "form": "아이고 정말 속상하시겠어요",
          "original_form": "아이고 정말 속상하시겠어요",
          "speaker_id": "1",
          "time": "20210819 20:30"
        },
        {
          "id": "MDRW2100000874.1.20",
          "form": "제일 그게 재미있을때인대",
          "original_form": "제일 그게 재미있을때인대",
          "speaker_id": "1",
          "time": "20210819 20:31"
        },
        {
          "id": "MDRW2100000874.1.21",
          "form": "ㅠㅠㅠㅠ 데이트도 해보고 싶고 다해보고 싶네요 ㅜ",
          "original_form": "ㅠㅠㅠㅠ 데이트도 해보고 싶고 다해보고 싶네요 ㅜ",
          "speaker_id": "2",
          "time": "20210819 20:31"
        }
