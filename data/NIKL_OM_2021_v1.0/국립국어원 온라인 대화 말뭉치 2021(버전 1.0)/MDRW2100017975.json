      "utterance": [
        {
          "id": "MDRW2100017975.1.1",
          "form": "나 칼국수 먹고 싶어...",
          "original_form": "나 칼국수 먹고 싶어...",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017975.1.2",
          "form": "면종류 냉모밀..",
          "original_form": "면종류 냉모밀..",
          "speaker_id": "2",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017975.1.3",
          "form": "난 따뜻한거 먹고싶어",
          "original_form": "난 따뜻한거 먹고싶어",
          "speaker_id": "1",
          "time": "20210826 21:56"
        },
        {
          "id": "MDRW2100017975.1.4",
          "form": "갈비탕같은거...",
          "original_form": "갈비탕같은거...",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017975.1.5",
          "form": "점심 뭐먹엇어?",
          "original_form": "점심 뭐먹엇어?",
          "speaker_id": "1",
          "time": "20210826 21:57"
        },
        {
          "id": "MDRW2100017975.1.6",
          "form": "점심 구내식당에서... 미역국 쫄면 제육?",
          "original_form": "점심 구내식당에서... 미역국 쫄면 제육?",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017975.1.7",
          "form": "갈비탕도 맛있겠다 언니는 뭐먹었는데?",
          "original_form": "갈비탕도 맛있겠다 언니는 뭐먹었는데?",
          "speaker_id": "2",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017975.1.8",
          "form": "쫄면 맛있겠다",
          "original_form": "쫄면 맛있겠다",
          "speaker_id": "1",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017975.1.9",
          "form": "난 본죽 김치해물해장죽? 먹었어",
          "original_form": "난 본죽 김치해물해장죽? 먹었어",
          "speaker_id": "1",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017975.1.10",
          "form": "하지만 비추함",
          "original_form": "하지만 비추함",
          "speaker_id": "1",
          "time": "20210826 21:58"
        },
        {
          "id": "MDRW2100017975.1.11",
          "form": "음.. 조아 나중에 혹시 기회가 있다면 안먹어야지(?",
          "original_form": "음.. 조아 나중에 혹시 기회가 있다면 안먹어야지(?",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017975.1.12",
          "form": "해장죽.. 뭔가 해장이 될까 싶은걸",
          "original_form": "해장죽.. 뭔가 해장이 될까 싶은걸",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017975.1.13",
          "form": "쫄면 맛있었는데 넘 쪼끔 먹었어 아쉬워",
          "original_form": "쫄면 맛있었는데 넘 쪼끔 먹었어 아쉬워",
          "speaker_id": "2",
          "time": "20210826 21:59"
        },
        {
          "id": "MDRW2100017975.1.14",
          "form": "내가 김치낙지죽을 좋아해서",
          "original_form": "내가 김치낙지죽을 좋아해서",
          "speaker_id": "1",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100017975.1.15",
          "form": "비슷한 맛인줄 알고 시켰는데 아니었어...",
          "original_form": "비슷한 맛인줄 알고 시켰는데 아니었어...",
          "speaker_id": "1",
          "time": "20210826 22:00"
        },
        {
          "id": "MDRW2100017975.1.16",
          "form": "ㅠㅠ 왜 속이냐 본죽..나쁜 놈들...",
          "original_form": "ㅠㅠ 왜 속이냐 본죽..나쁜 놈들...",
          "speaker_id": "2",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100017975.1.17",
          "form": "너무 맛이 강해",
          "original_form": "너무 맛이 강해",
          "speaker_id": "1",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100017975.1.18",
          "form": "김치 신맛이 너무 강해서",
          "original_form": "김치 신맛이 너무 강해서",
          "speaker_id": "1",
          "time": "20210826 22:01"
        },
        {
          "id": "MDRW2100017975.1.19",
          "form": "같이 온 반찬중에 장조림만 좀 집어먹고 나머지는 그대로 버림",
          "original_form": "같이 온 반찬중에 장조림만 좀 집어먹고 나머지는 그대로 버림",
          "speaker_id": "1",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100017975.1.20",
          "form": "아하.. 김치랑 해물 잘못 섞이면 너무 그렇겠다",
          "original_form": "아하.. 김치랑 해물 잘못 섞이면 너무 그렇겠다",
          "speaker_id": "2",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100017975.1.21",
          "form": "ㅠㅜ 아까워",
          "original_form": "ㅠㅜ 아까워",
          "speaker_id": "2",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100017975.1.22",
          "form": "ㅇㅇ 그리고 가리비는 좀 비리더라구...",
          "original_form": "ㅇㅇ 그리고 가리비는 좀 비리더라구...",
          "speaker_id": "1",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100017975.1.23",
          "form": "바지락도 기본 맛이 있어서 조개비린거 싫어하면 못먹을듯",
          "original_form": "바지락도 기본 맛이 있어서 조개비린거 싫어하면 못먹을듯",
          "speaker_id": "1",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100017975.1.24",
          "form": "조개 진짜 안비리게 먹는거 어렵지..ㅇㅇ",
          "original_form": "조개 진짜 안비리게 먹는거 어렵지..ㅇㅇ",
          "speaker_id": "2",
          "time": "20210826 22:02"
        },
        {
          "id": "MDRW2100017975.1.25",
          "form": "? 모래가 이에 걸려서",
          "original_form": "? 모래가 이에 걸려서",
          "speaker_id": "2",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017975.1.26",
          "form": "ㅇㅇ 혹시 짬뽕죽 먹어봤어?",
          "original_form": "ㅇㅇ 혹시 짬뽕죽 먹어봤어?",
          "speaker_id": "1",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017975.1.27",
          "form": "아그작아그작 거려 됐니 심심아?",
          "original_form": "아그작아그작 거려 됐니 심심아?",
          "speaker_id": "2",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017975.1.28",
          "form": "해감은 잘 돼있긴 해 ㅋㅋㅋㅋ",
          "original_form": "해감은 잘 돼있긴 해 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017975.1.29",
          "form": "아니 안먹어봤엉 맛있어?",
          "original_form": "아니 안먹어봤엉 맛있어?",
          "speaker_id": "2",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017975.1.30",
          "form": "ㅋㅋㅋㅋ 다행이다",
          "original_form": "ㅋㅋㅋㅋ 다행이다",
          "speaker_id": "2",
          "time": "20210826 22:03"
        },
        {
          "id": "MDRW2100017975.1.31",
          "form": "나도 안먹어봄 ㅋㅋㅋㅋ",
          "original_form": "나도 안먹어봄 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017975.1.32",
          "form": "그치만 짬뽕이면 맛 괜찮을거같아",
          "original_form": "그치만 짬뽕이면 맛 괜찮을거같아",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017975.1.33",
          "form": "다음엔 이거 시켜먹어봐야지...",
          "original_form": "다음엔 이거 시켜먹어봐야지...",
          "speaker_id": "1",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017975.1.34",
          "form": "ㅋㅋㅋㅋ 뭔가 짬뽕인데 꾸덕..한건가 하지만 짬뽕밥도 있으니까..",
          "original_form": "ㅋㅋㅋㅋ 뭔가 짬뽕인데 꾸덕..한건가 하지만 짬뽕밥도 있으니까..",
          "speaker_id": "2",
          "time": "20210826 22:04"
        },
        {
          "id": "MDRW2100017975.1.35",
          "form": "오 먹고 후기 알려줘",
          "original_form": "오 먹고 후기 알려줘",
