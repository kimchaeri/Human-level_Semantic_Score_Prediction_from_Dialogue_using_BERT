        },
        {
          "id": "MDRW2100036580.1.2",
          "form": "피자가 좋아 치킨이 좋아",
          "original_form": "피자가 좋아 치킨이 좋아",
          "speaker_id": "2",
          "time": "20210831 01:03"
        },
        {
          "id": "MDRW2100036580.1.3",
          "form": "나는 치킨 되게 안 좋아해",
          "original_form": "나는 치킨 되게 안 좋아해",
          "speaker_id": "1",
          "time": "20210831 01:03"
        },
        {
          "id": "MDRW2100036580.1.4",
          "form": "그럼 어떤 피자가 좋아",
          "original_form": "그럼 어떤 피자가 좋아",
          "speaker_id": "2",
          "time": "20210831 01:03"
        },
        {
          "id": "MDRW2100036580.1.5",
          "form": "버섯 피자 거나 베이컨 피자 좋아해",
          "original_form": "버섯 피자 거나 베이컨 피자 좋아해",
          "speaker_id": "1",
          "time": "20210831 01:03"
        },
        {
          "id": "MDRW2100036580.1.6",
          "form": "고구마무스 싫어하는구나",
          "original_form": "고구마무스 싫어하는구나",
          "speaker_id": "2",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100036580.1.7",
          "form": "감자 피자는 맛있는데",
          "original_form": "감자 피자는 맛있는데",
          "speaker_id": "1",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100036580.1.8",
          "form": "이 감자 짱 싫어",
          "original_form": "이 감자 짱 싫어",
          "speaker_id": "2",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100036580.1.9",
          "form": "고구마 보다는 감자지",
          "original_form": "고구마 보다는 감자지",
          "speaker_id": "1",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100036580.1.10",
          "form": "밤고구마가 좋아 호박고구마가 좋아",
          "original_form": "밤고구마가 좋아 호박고구마가 좋아",
          "speaker_id": "2",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100036580.1.11",
          "form": "고구마는 그냥 가게 같아",
          "original_form": "고구마는 그냥 가게 같아",
          "speaker_id": "1",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100036580.1.12",
          "form": "다 같아",
          "original_form": "다 같아",
          "speaker_id": "1",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100036580.1.13",
          "form": "맛 알 못",
          "original_form": "맛 알 못",
          "speaker_id": "2",
          "time": "20210831 01:04"
        },
        {
          "id": "MDRW2100036580.1.14",
          "form": "너는 뭐가 더 좋아",
          "original_form": "너는 뭐가 더 좋아",
          "speaker_id": "1",
          "time": "20210831 01:05"
        },
        {
          "id": "MDRW2100036580.1.15",
          "form": "나도 그렇게 좋아하진 않아",
          "original_form": "나도 그렇게 좋아하진 않아",
          "speaker_id": "2",
