      "utterance": [
        {
          "id": "MDRW2100015135.1.1",
          "form": "뭐 치킨이나 족발요 ㅋㅋ",
          "original_form": "뭐 치킨이나 족발요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 21:58"
        },
        {
          "id": "MDRW2100015135.1.2",
          "form": "오 치킨 어디꺼 좋아하세유",
          "original_form": "오 치킨 어디꺼 좋아하세유",
          "speaker_id": "1",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015135.1.3",
          "form": "전 맘스터치 bhc",
          "original_form": "전 맘스터치 bhc",
          "speaker_id": "2",
          "time": "20210825 21:59"
        },
        {
          "id": "MDRW2100015135.1.4",
          "form": "깻잎 두마리 시켜요",
          "original_form": "깻잎 두마리 시켜요",
          "speaker_id": "2",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015135.1.5",
          "form": "어디꺼 드세요",
          "original_form": "어디꺼 드세요",
          "speaker_id": "2",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015135.1.6",
          "form": "간장은?교촌이요",
          "original_form": "간장은?교촌이요",
          "speaker_id": "2",
          "time": "20210825 22:00"
        },
        {
          "id": "MDRW2100015135.1.7",
          "form": "저도 bhc랑 노랑통닭이요!",
          "original_form": "저도 bhc랑 노랑통닭이요!",
          "speaker_id": "1",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015135.1.8",
          "form": "노랑통닭 인기네요",
          "original_form": "노랑통닭 인기네요",
          "speaker_id": "2",
          "time": "20210825 22:02"
        },
        {
          "id": "MDRW2100015135.1.9",
          "form": "저 아직  못 먹었는데",
          "original_form": "저 아직  못 먹었는데",
          "speaker_id": "2",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015135.1.10",
          "form": "주말에 달립니다 ㅎㅎ",
          "original_form": "주말에 달립니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015135.1.11",
          "form": "콜라 맥주 소주 ?뭐랑드세요",
          "original_form": "콜라 맥주 소주 ?뭐랑드세요",
          "speaker_id": "2",
          "time": "20210825 22:03"
        },
        {
          "id": "MDRW2100015135.1.12",
          "form": "전 맥주랑만요!!! 소주는 잘 못 마셔서요 ㅎㅎ",
          "original_form": "전 맥주랑만요!!! 소주는 잘 못 마셔서요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015135.1.13",
          "form": "뭐랑 드시나요",
          "original_form": "뭐랑 드시나요",
          "speaker_id": "1",
          "time": "20210825 22:06"
        },
        {
          "id": "MDRW2100015135.1.14",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015135.1.15",
          "form": "소주 노요 ㅜㅜ",
          "original_form": "소주 노요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015135.1.16",
          "form": "치맥이 최고예요 ㅎㅎ",
          "original_form": "치맥이 최고예요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015135.1.17",
          "form": "맥주는 어디꺼 젤 좋아하세요?",
          "original_form": "맥주는 어디꺼 젤 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:15"
        },
        {
          "id": "MDRW2100015135.1.18",
          "form": "클라우드요 ㅎㅎ",
          "original_form": "클라우드요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015135.1.19",
          "form": "뭐좋아하세요",
          "original_form": "뭐좋아하세요",
          "speaker_id": "2",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015135.1.20",
          "form": "클라우드 맛있죠 저는 스텔라 젤 좋아해여",
          "original_form": "클라우드 맛있죠 저는 스텔라 젤 좋아해여",
          "speaker_id": "1",
          "time": "20210825 22:18"
        },
        {
          "id": "MDRW2100015135.1.21",
          "form": "아 스텔라도 좋죠",
          "original_form": "아 스텔라도 좋죠",
          "speaker_id": "2",
          "time": "20210825 22:20"
        },
        {
          "id": "MDRW2100015135.1.22",
          "form": "먹어봤어요 호",
          "original_form": "먹어봤어요 호",
          "speaker_id": "2",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100015135.1.23",
          "form": "파울라너도 맛있어요 쓰다보니 군침이 ㅠ",
          "original_form": "파울라너도 맛있어요 쓰다보니 군침이 ㅠ",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100015135.1.24",
          "form": "앙 먹어 볼께요",
          "original_form": "앙 먹어 볼께요",
          "speaker_id": "2",
          "time": "20210825 22:22"
        }
      ]
