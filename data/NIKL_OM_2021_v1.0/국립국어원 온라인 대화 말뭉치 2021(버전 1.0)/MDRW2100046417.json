        {
          "id": "MDRW2100046417.1.2",
          "form": "안녕하세요~~",
          "original_form": "안녕하세요~~",
          "speaker_id": "2",
          "time": "20211021 15:09"
        },
        {
          "id": "MDRW2100046417.1.3",
          "form": "백신 다 맞으셨나요?",
          "original_form": "백신 다 맞으셨나요?",
          "speaker_id": "2",
          "time": "20211021 15:09"
        },
        {
          "id": "MDRW2100046417.1.4",
          "form": "네 2번 맞았어요",
          "original_form": "네 2번 맞았어요",
          "speaker_id": "1",
          "time": "20211021 15:09"
        },
        {
          "id": "MDRW2100046417.1.5",
          "form": "와우 어떠셨어요 안 아팠나요?",
          "original_form": "와우 어떠셨어요 안 아팠나요?",
          "speaker_id": "2",
          "time": "20211021 15:10"
        },
        {
          "id": "MDRW2100046417.1.6",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20211021 15:10"
        },
        {
          "id": "MDRW2100046417.1.7",
          "form": "2번째는 아파요",
          "original_form": "2번째는 아파요",
          "speaker_id": "1",
          "time": "20211021 15:10"
        },
        {
          "id": "MDRW2100046417.1.8",
          "form": "전 아직 1차만요!",
          "original_form": "전 아직 1차만요!",
          "speaker_id": "2",
          "time": "20211021 15:10"
        },
        {
          "id": "MDRW2100046417.1.9",
          "form": "그쵸! 2차 그렇게들 아프다던데",
          "original_form": "그쵸! 2차 그렇게들 아프다던데",
          "speaker_id": "2",
          "time": "20211021 15:10"
        },
        {
          "id": "MDRW2100046417.1.10",
          "form": "음. 1차랑은 다르네요",
          "original_form": "음. 1차랑은 다르네요",
          "speaker_id": "1",
          "time": "20211021 15:10"
        },
        {
          "id": "MDRW2100046417.1.11",
          "form": "전 1차도 너무 아팠단 말이에요 ㅠㅠ ",
          "original_form": "전 1차도 너무 아팠단 말이에요 ㅠㅠ ",
          "speaker_id": "2",
          "time": "20211021 15:11"
        },
        {
          "id": "MDRW2100046417.1.12",
          "form": "2차 무서워서 어떻게 맞나요",
          "original_form": "2차 무서워서 어떻게 맞나요",
          "speaker_id": "2",
          "time": "20211021 15:11"
        },
        {
          "id": "MDRW2100046417.1.13",
          "form": "그래도 앞으로를 위해 참고 맞아야죠",
          "original_form": "그래도 앞으로를 위해 참고 맞아야죠",
          "speaker_id": "1",
          "time": "20211021 15:11"
        },
        {
          "id": "MDRW2100046417.1.14",
          "form": "친구들 만나려면요",
          "original_form": "친구들 만나려면요",
          "speaker_id": "1",
          "time": "20211021 15:12"
        },
        {
          "id": "MDRW2100046417.1.15",
          "form": "날짜는 잡혔어요? 2차?",
          "original_form": "날짜는 잡혔어요? 2차?",
          "speaker_id": "1",
          "time": "20211021 15:13"
        },
        {
          "id": "MDRW2100046417.1.16",
          "form": "네 전 다음 주요",
          "original_form": "네 전 다음 주요",
          "speaker_id": "2",
          "time": "20211021 15:13"
        },
        {
          "id": "MDRW2100046417.1.17",
          "form": "전 이제 2주가 지났어요",
          "original_form": "전 이제 2주가 지났어요",
          "speaker_id": "1",
          "time": "20211021 15:13"
        },
        {
          "id": "MDRW2100046417.1.18",
          "form": "말씀하신대로 얼른 다 마스크 벗고 친구들 만나러 다닐 수 있으면 좋겠네요 정말",
          "original_form": "말씀하신대로 얼른 다 마스크 벗고 친구들 만나러 다닐 수 있으면 좋겠네요 정말",
          "speaker_id": "2",
          "time": "20211021 15:13"
        }
