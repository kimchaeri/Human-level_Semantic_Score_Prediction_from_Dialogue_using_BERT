      "utterance": [
        {
          "id": "MDRW2100017298.1.1",
          "form": "오 20대 초반에 벌써",
          "original_form": "오 20대 초반에 벌써",
          "speaker_id": "2",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100017298.1.2",
          "form": "몸이 좀 허한거같아서",
          "original_form": "몸이 좀 허한거같아서",
          "speaker_id": "1",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100017298.1.3",
          "form": "건강에 관심을 가지다니",
          "original_form": "건강에 관심을 가지다니",
          "speaker_id": "2",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100017298.1.4",
          "form": "아주 좋은 자세입니다",
          "original_form": "아주 좋은 자세입니다",
          "speaker_id": "2",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100017298.1.5",
          "form": "종합비타민이랑 루테인..?",
          "original_form": "종합비타민이랑 루테인..?",
          "speaker_id": "1",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100017298.1.6",
          "form": "그거 샀어요",
          "original_form": "그거 샀어요",
          "speaker_id": "1",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100017298.1.7",
          "form": "그런가요ㅎ",
          "original_form": "그런가요ㅎ",
          "speaker_id": "1",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100017298.1.8",
          "form": "바람직한 세트네요",
          "original_form": "바람직한 세트네요",
          "speaker_id": "2",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100017298.1.9",
          "form": "일찍일찍 먹으면 좋죠",
          "original_form": "일찍일찍 먹으면 좋죠",
          "speaker_id": "2",
          "time": "20210826 18:35"
        },
        {
          "id": "MDRW2100017298.1.10",
          "form": "전 이제 챙기기 시작했는데!",
          "original_form": "전 이제 챙기기 시작했는데!",
          "speaker_id": "2",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017298.1.11",
          "form": "영양제 드시는거있으세요",
          "original_form": "영양제 드시는거있으세요",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017298.1.12",
          "form": "어떤거 드시나요",
          "original_form": "어떤거 드시나요",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017298.1.13",
          "form": "전 비타민C?",
          "original_form": "전 비타민C?",
          "speaker_id": "2",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017298.1.14",
          "form": "것도 먹었다 안먹었다해요",
          "original_form": "것도 먹었다 안먹었다해요",
          "speaker_id": "2",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017298.1.15",
          "form": "자꾸 까먹어요",
          "original_form": "자꾸 까먹어요",
          "speaker_id": "2",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017298.1.16",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017298.1.17",
          "form": "꾸준히 먹는거 은근 어렵",
          "original_form": "꾸준히 먹는거 은근 어렵",
          "speaker_id": "1",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017298.1.18",
          "form": "먹는 거 제때 챙기는 게 힘들어요",
          "original_form": "먹는 거 제때 챙기는 게 힘들어요",
          "speaker_id": "2",
          "time": "20210826 18:36"
        },
        {
          "id": "MDRW2100017298.1.19",
          "form": "공감합니다",
          "original_form": "공감합니다",
          "speaker_id": "1",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017298.1.20",
          "form": "아 대신 요즘 홈트 시작해서",
          "original_form": "아 대신 요즘 홈트 시작해서",
          "speaker_id": "2",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017298.1.21",
          "form": "운동기구도 사고",
          "original_form": "운동기구도 사고",
          "speaker_id": "2",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017298.1.22",
          "form": "운동기구라",
          "original_form": "운동기구라",
          "speaker_id": "1",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017298.1.23",
          "form": "홈트용품 이런거",
          "original_form": "홈트용품 이런거",
          "speaker_id": "2",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017298.1.24",
          "form": "어떤거사셨어요",
          "original_form": "어떤거사셨어요",
          "speaker_id": "1",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017298.1.25",
          "form": "매트사셨나",
          "original_form": "매트사셨나",
          "speaker_id": "1",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017298.1.26",
          "form": "아령이나",
          "original_form": "아령이나",
          "speaker_id": "1",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017298.1.27",
          "form": "아 기구는 너무 전문가스럽고",
          "original_form": "아 기구는 너무 전문가스럽고",
          "speaker_id": "2",
          "time": "20210826 18:37"
        },
        {
          "id": "MDRW2100017298.1.28",
          "form": "네네 그런 용품들",
          "original_form": "네네 그런 용품들",
          "speaker_id": "2",
          "time": "20210826 18:37"
        }
      ]
