      "utterance": [
        {
          "id": "MDRW2100014758.1.1",
          "form": "반려동물 키우세요?",
          "original_form": "반려동물 키우세요?",
          "speaker_id": "2",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014758.1.2",
          "form": "전 토야랑 살아요",
          "original_form": "전 토야랑 살아요",
          "speaker_id": "2",
          "time": "20210825 20:35"
        },
        {
          "id": "MDRW2100014758.1.3",
          "form": "아니 안키워요  키우고싶어요",
          "original_form": "아니 안키워요  키우고싶어요",
          "speaker_id": "1",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014758.1.4",
          "form": "토야라니 이쁜이름이군요",
          "original_form": "토야라니 이쁜이름이군요",
          "speaker_id": "1",
          "time": "20210825 20:36"
        },
        {
          "id": "MDRW2100014758.1.5",
          "form": "알러지때문에 못키워요",
          "original_form": "알러지때문에 못키워요",
          "speaker_id": "1",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014758.1.6",
          "form": "아니요 토끼를 토야라고 불러요 토끼집사들은",
          "original_form": "아니요 토끼를 토야라고 불러요 토끼집사들은",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014758.1.7",
          "form": "이름은 name3요",
          "original_form": "이름은 &name3&요",
          "speaker_id": "2",
          "time": "20210825 20:37"
        },
        {
          "id": "MDRW2100014758.1.8",
          "form": "토끼를 키우시군아",
          "original_form": "토끼를 키우시군아",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014758.1.9",
          "form": "사진을 못보는게  아쉽네요",
          "original_form": "사진을 못보는게  아쉽네요",
          "speaker_id": "1",
          "time": "20210825 20:38"
        },
        {
          "id": "MDRW2100014758.1.10",
          "form": "사진첨부가 없네요",
          "original_form": "사진첨부가 없네요",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014758.1.11",
          "form": "아숩 자랑하고픈데",
          "original_form": "아숩 자랑하고픈데",
          "speaker_id": "2",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014758.1.12",
          "form": "토끼는 간식 뭐먹어요??",
          "original_form": "토끼는 간식 뭐먹어요??",
          "speaker_id": "1",
          "time": "20210825 20:39"
        },
        {
          "id": "MDRW2100014758.1.13",
          "form": "간식은 말린 과일이요",
          "original_form": "간식은 말린 과일이요",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014758.1.14",
          "form": "토끼는 당근 싫어해요",
          "original_form": "토끼는 당근 싫어해요",
          "speaker_id": "2",
          "time": "20210825 20:40"
        },
        {
          "id": "MDRW2100014758.1.15",
          "form": "세상이야",
          "original_form": "세상이야",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014758.1.16",
          "form": "진짜요",
          "original_form": "진짜요",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014758.1.17",
          "form": "오이는 먹어요?",
          "original_form": "오이는 먹어요?",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014758.1.18",
          "form": "당근을 싫어한다니",
          "original_form": "당근을 싫어한다니",
          "speaker_id": "1",
          "time": "20210825 20:41"
        },
        {
          "id": "MDRW2100014758.1.19",
          "form": "아뇨 우리아가는 단거 좋아해요",
          "original_form": "아뇨 우리아가는 단거 좋아해요",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014758.1.20",
          "form": "저 닮아서",
          "original_form": "저 닮아서",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014758.1.21",
          "form": "뽕잎도 좋아하고",
          "original_form": "뽕잎도 좋아하고",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014758.1.22",
          "form": "클로버도 좋아해요",
          "original_form": "클로버도 좋아해요",
          "speaker_id": "2",
          "time": "20210825 20:42"
        },
        {
          "id": "MDRW2100014758.1.23",
          "form": "귤피 진짜 좋아해요",
          "original_form": "귤피 진짜 좋아해요",
          "speaker_id": "2",
          "time": "20210825 20:43"
        },
        {
          "id": "MDRW2100014758.1.24",
          "form": "신기하네요",
          "original_form": "신기하네요",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014758.1.25",
          "form": "강아지풀은요?",
          "original_form": "강아지풀은요?",
          "speaker_id": "1",
          "time": "20210825 20:44"
        },
        {
          "id": "MDRW2100014758.1.26",
          "form": "사본적이 없어서 모르겠네요",
          "original_form": "사본적이 없어서 모르겠네요",
          "speaker_id": "2",
          "time": "20210825 20:45"
        },
        {
          "id": "MDRW2100014758.1.27",
          "form": "주식으로 주는 건초에 강아지풀같은게 달려있긴해요",
          "original_form": "주식으로 주는 건초에 강아지풀같은게 달려있긴해요",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014758.1.28",
          "form": "그것만 골라먹어요",
          "original_form": "그것만 골라먹어요",
          "speaker_id": "2",
          "time": "20210825 20:46"
        },
        {
          "id": "MDRW2100014758.1.29",
          "form": "아하  옛날에  친구가  기니피크 키울때 본것같아요",
          "original_form": "아하  옛날에  친구가  기니피크 키울때 본것같아요",
          "speaker_id": "1",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014758.1.30",
          "form": "기니피크라니 귀여울것 같아요",
          "original_form": "기니피크라니 귀여울것 같아요",
          "speaker_id": "2",
          "time": "20210825 20:47"
        },
        {
          "id": "MDRW2100014758.1.31",
          "form": "같은류라서. 사료도 같을 거예요",
          "original_form": "같은류라서. 사료도 같을 거예요",
          "speaker_id": "2",
          "time": "20210825 20:48"
        },
        {
          "id": "MDRW2100014758.1.32",
          "form": "막 뛰고하더라거요",
          "original_form": "막 뛰고하더라거요",
          "speaker_id": "1",
          "time": "20210825 20:49"
        }
      ]
    }
  ]
