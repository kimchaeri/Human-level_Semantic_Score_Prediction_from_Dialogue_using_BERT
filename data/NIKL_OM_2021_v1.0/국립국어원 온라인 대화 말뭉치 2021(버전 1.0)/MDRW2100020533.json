      "utterance": [
        {
          "id": "MDRW2100020533.1.1",
          "form": "보고오셨나요",
          "original_form": "보고오셨나요",
          "speaker_id": "1",
          "time": "20210828 09:15"
        },
        {
          "id": "MDRW2100020533.1.2",
          "form": "아니 사진보고 넘 맛있어보여서 놀랐네요",
          "original_form": "아니 사진보고 넘 맛있어보여서 놀랐네요",
          "speaker_id": "2",
          "time": "20210828 09:15"
        },
        {
          "id": "MDRW2100020533.1.3",
          "form": "두툼하고",
          "original_form": "두툼하고",
          "speaker_id": "2",
          "time": "20210828 09:15"
        },
        {
          "id": "MDRW2100020533.1.4",
          "form": "사진은 괜찮죠",
          "original_form": "사진은 괜찮죠",
          "speaker_id": "1",
          "time": "20210828 09:15"
        },
        {
          "id": "MDRW2100020533.1.5",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210828 09:15"
        },
        {
          "id": "MDRW2100020533.1.6",
          "form": "빵가루맛이 이상하던가요",
          "original_form": "빵가루맛이 이상하던가요",
          "speaker_id": "2",
          "time": "20210828 09:15"
        },
        {
          "id": "MDRW2100020533.1.7",
          "form": "빵가루가 두툼한거에요",
          "original_form": "빵가루가 두툼한거에요",
          "speaker_id": "1",
          "time": "20210828 09:15"
        },
        {
          "id": "MDRW2100020533.1.8",
          "form": "ㅋㅋㅋ진짜에요 믿어주세요",
          "original_form": "ㅋㅋㅋ진짜에요 믿어주세요",
          "speaker_id": "1",
          "time": "20210828 09:15"
        },
        {
          "id": "MDRW2100020533.1.9",
          "form": "헉...",
          "original_form": "헉...",
          "speaker_id": "2",
          "time": "20210828 09:15"
        },
        {
          "id": "MDRW2100020533.1.10",
          "form": "고기는 얇는데 빵가루가 두퉁해서 두꺼워 보이는 매직",
          "original_form": "고기는 얇는데 빵가루가 두퉁해서 두꺼워 보이는 매직",
          "speaker_id": "1",
          "time": "20210828 09:16"
        },
        {
          "id": "MDRW2100020533.1.11",
          "form": "그건 돈까스에 대한 능멸이에요",
          "original_form": "그건 돈까스에 대한 능멸이에요",
          "speaker_id": "2",
          "time": "20210828 09:16"
        },
        {
          "id": "MDRW2100020533.1.12",
          "form": "돈까스도 찍먹파십니까",
          "original_form": "돈까스도 찍먹파십니까",
          "speaker_id": "1",
          "time": "20210828 09:16"
        },
        {
          "id": "MDRW2100020533.1.13",
          "form": "고기가 빵빵한건줄,,,",
          "original_form": "고기가 빵빵한건줄,,,",
          "speaker_id": "2",
          "time": "20210828 09:16"
        },
        {
          "id": "MDRW2100020533.1.14",
          "form": "네 찍먹파랍니다",
          "original_form": "네 찍먹파랍니다",
          "speaker_id": "2",
          "time": "20210828 09:16"
        },
        {
          "id": "MDRW2100020533.1.15",
          "form": "돈까스도 올라운더신가요?",
          "original_form": "돈까스도 올라운더신가요?",
          "speaker_id": "2",
          "time": "20210828 09:16"
        },
        {
          "id": "MDRW2100020533.1.16",
          "form": "저도 돈까스는 찍어먹습니다",
          "original_form": "저도 돈까스는 찍어먹습니다",
          "speaker_id": "1",
          "time": "20210828 09:16"
        },
        {
          "id": "MDRW2100020533.1.17",
          "form": "돈까스 눅눅한건 참을 수 없어",
          "original_form": "돈까스 눅눅한건 참을 수 없어",
          "speaker_id": "1",
          "time": "20210828 09:16"
        },
        {
          "id": "MDRW2100020533.1.18",
          "form": "돈까스 소스가... 시판형 소스는 제 취향이 아니더라구요",
          "original_form": "돈까스 소스가... 시판형 소스는 제 취향이 아니더라구요",
          "speaker_id": "2",
          "time": "20210828 09:16"
        },
        {
          "id": "MDRW2100020533.1.19",
          "form": "약간 다 시죠",
          "original_form": "약간 다 시죠",
          "speaker_id": "1",
          "time": "20210828 09:16"
        },
        {
          "id": "MDRW2100020533.1.20",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210828 09:17"
        },
        {
          "id": "MDRW2100020533.1.21",
          "form": "근데 가끔 매콤소스 있는 곳은",
          "original_form": "근데 가끔 매콤소스 있는 곳은",
          "speaker_id": "2",
          "time": "20210828 09:17"
        },
        {
          "id": "MDRW2100020533.1.22",
          "form": "비슷비슷한 맛인데 이름만 다른느낌",
          "original_form": "비슷비슷한 맛인데 이름만 다른느낌",
          "speaker_id": "1",
          "time": "20210828 09:17"
        },
        {
          "id": "MDRW2100020533.1.23",
          "form": "좀 더 많이 찍어먹어요",
          "original_form": "좀 더 많이 찍어먹어요",
          "speaker_id": "2",
          "time": "20210828 09:17"
        },
        {
          "id": "MDRW2100020533.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210828 09:17"
        },
        {
          "id": "MDRW2100020533.1.25",
          "form": "그런곳 좋아요",
          "original_form": "그런곳 좋아요",
          "speaker_id": "1",
          "time": "20210828 09:17"
        },
        {
          "id": "MDRW2100020533.1.26",
          "form": "매운거 드시나요",
          "original_form": "매운거 드시나요",
          "speaker_id": "2",
          "time": "20210828 09:17"
        },
        {
          "id": "MDRW2100020533.1.27",
          "form": "너무 맛있어요",
          "original_form": "너무 맛있어요",
          "speaker_id": "1",
          "time": "20210828 09:17"
        },
        {
          "id": "MDRW2100020533.1.28",
          "form": "네 잘은 못먹는데",
          "original_form": "네 잘은 못먹는데",
          "speaker_id": "1",
          "time": "20210828 09:17"
        },
        {
          "id": "MDRW2100020533.1.29",
          "form": "좋아합니다",
          "original_form": "좋아합니다",
          "speaker_id": "1",
          "time": "20210828 09:17"
        }
      ]
    }
  ]
