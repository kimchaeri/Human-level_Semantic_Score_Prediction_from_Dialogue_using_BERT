      "utterance": [
        {
          "id": "MDRW2100030390.1.1",
          "form": "안녕하세요?",
          "original_form": "안녕하세요?",
          "speaker_id": "1",
          "time": "20210904 00:26"
        },
        {
          "id": "MDRW2100030390.1.2",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030390.1.3",
          "form": "요새 날씨가 참 좋아요",
          "original_form": "요새 날씨가 참 좋아요",
          "speaker_id": "2",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030390.1.4",
          "form": "많이 선선해져서 너무 좋아요",
          "original_form": "많이 선선해져서 너무 좋아요{emoji:♡}",
          "speaker_id": "2",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030390.1.5",
          "form": "네 날씨너무좋아요.",
          "original_form": "네 날씨너무좋아요.",
          "speaker_id": "1",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030390.1.6",
          "form": "오늘 특히요ㅋ",
          "original_form": "오늘 특히요ㅋ",
          "speaker_id": "1",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030390.1.7",
          "form": "어제까지 비왔었는데ㅜㅜㅋㅋ",
          "original_form": "어제까지 비왔었는데ㅜㅜㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:27"
        },
        {
          "id": "MDRW2100030390.1.8",
          "form": "맞아요 ㅋㅋㅋ 주말동안은 비가 올 예정이없다고 해서",
          "original_form": "맞아요 ㅋㅋㅋ 주말동안은 비가 올 예정이없다고 해서",
          "speaker_id": "2",
          "time": "20210904 00:32"
        },
        {
          "id": "MDRW2100030390.1.9",
          "form": "맑은 하늘을 볼 수 있을 것 같아요",
          "original_form": "맑은 하늘을 볼 수 있을 것 같아요",
          "speaker_id": "2",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030390.1.10",
          "form": "name1님께서는 어떤 계절을 좋아하시나요?",
          "original_form": "&name1&님께서는 어떤 계절을 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030390.1.11",
          "form": "네 ㅋㅋ주말에 그래서 어디가고싶으니생각이드네요ㅋㅋ",
          "original_form": "네 ㅋㅋ주말에 그래서 어디가고싶으니생각이드네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100030390.1.12",
          "form": "코시국이라 힘들지만ㅜ",
          "original_form": "코시국이라 힘들지만ㅜ",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030390.1.13",
          "form": "저는 가을이랑 봄좋아해요ㅋ",
          "original_form": "저는 가을이랑 봄좋아해요ㅋ",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030390.1.14",
          "form": "여름은 너무덥고ㅜ습해서 싫어요",
          "original_form": "여름은 너무덥고ㅜ습해서 싫어요",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030390.1.15",
          "form": "어떤 계절 좋아하세요?",
          "original_form": "어떤 계절 좋아하세요?",
          "speaker_id": "1",
          "time": "20210904 00:34"
        },
        {
          "id": "MDRW2100030390.1.16",
          "form": "저도 가을을 좋아해요!",
          "original_form": "저도 가을을 좋아해요!",
          "speaker_id": "2",
          "time": "20210904 00:35"
        },
        {
          "id": "MDRW2100030390.1.17",
          "form": "예쁜 하늘이랑 맛있는 과일이 나와서 좋아한답니다",
          "original_form": "예쁜 하늘이랑 맛있는 과일이 나와서 좋아한답니다",
          "speaker_id": "2",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100030390.1.18",
          "form": "그래서 어디 가지 못해서 많이 아쉬워요 ㅠ",
          "original_form": "그래서 어디 가지 못해서 많이 아쉬워요 ㅠ",
          "speaker_id": "2",
          "time": "20210904 00:36"
        },
        {
          "id": "MDRW2100030390.1.19",
          "form": "가을 예쁘죠",
          "original_form": "가을 예쁘죠",
          "speaker_id": "1",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030390.1.20",
          "form": "단풍구경가야되는데ㅜ흑",
          "original_form": "단풍구경가야되는데ㅜ흑",
          "speaker_id": "1",
          "time": "20210904 00:37"
        },
        {
          "id": "MDRW2100030390.1.21",
          "form": "헉 벌써 단풍이 올라오기 시작했을까요?",
          "original_form": "헉 벌써 단풍이 올라오기 시작했을까요?",
          "speaker_id": "2",
          "time": "20210904 00:43"
        },
        {
          "id": "MDRW2100030390.1.22",
          "form": "아니요 아직은 아닌것같아요",
          "original_form": "아니요 아직은 아닌것같아요",
          "speaker_id": "1",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030390.1.23",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030390.1.24",
          "form": "가을옷은 꺼내셨어요?",
          "original_form": "가을옷은 꺼내셨어요?",
          "speaker_id": "1",
          "time": "20210904 00:44"
        },
        {
          "id": "MDRW2100030390.1.25",
          "form": "아니요 ㅠ 이번 주말에 옷을 정리하려고 해요",
          "original_form": "아니요 ㅠ 이번 주말에 옷을 정리하려고 해요",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030390.1.26",
          "form": "여즉 반팔만 입고 다녔더니 슬슬 날이 차가워져서 안되겠더라구요 ㅎㅎ",
          "original_form": "여즉 반팔만 입고 다녔더니 슬슬 날이 차가워져서 안되겠더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030390.1.27",
          "form": "저두요. 아직 여름옷 정리못했어요",
          "original_form": "저두요. 아직 여름옷 정리못했어요",
          "speaker_id": "1",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030390.1.28",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "1",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030390.1.29",
          "form": "이번주말에 가을옷꺼내고 옷장정리하려구요ㅋ",
          "original_form": "이번주말에 가을옷꺼내고 옷장정리하려구요ㅋ",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030390.1.30",
          "form": "가을 정리하면거 가을 옷도 겸사겸사 쇼핑도 해야겠어요️",
          "original_form": "가을 정리하면거 가을 옷도 겸사겸사 쇼핑도 해야겠어요{emoji:☺}️",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030390.1.31",
          "form": "네 맞아요 쇼핑해야죠ㅋㅋㅋ",
          "original_form": "네 맞아요 쇼핑해야죠ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:55"
        }
      ]
