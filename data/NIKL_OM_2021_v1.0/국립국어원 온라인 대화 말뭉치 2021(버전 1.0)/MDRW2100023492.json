      "utterance": [
        {
          "id": "MDRW2100023492.1.1",
          "form": "차용품은 많이 사셨어요?",
          "original_form": "차용품은 많이 사셨어요?",
          "speaker_id": "1",
          "time": "20210830 09:17"
        },
        {
          "id": "MDRW2100023492.1.2",
          "form": "이것 저것 사는걸 안좋아해서 핸드폰 거치대만 샀어요ㅋㅋ",
          "original_form": "이것 저것 사는걸 안좋아해서 핸드폰 거치대만 샀어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:18"
        },
        {
          "id": "MDRW2100023492.1.3",
          "form": "name1은 최근에 뭐사는거에 관심 있으세요",
          "original_form": "&name1&은 최근에 뭐사는거에 관심 있으세요",
          "speaker_id": "2",
          "time": "20210830 09:18"
        },
        {
          "id": "MDRW2100023492.1.4",
          "form": "저는 그렇게 쇼핑을 안좋아해서",
          "original_form": "저는 그렇게 쇼핑을 안좋아해서",
          "speaker_id": "1",
          "time": "20210830 09:18"
        },
        {
          "id": "MDRW2100023492.1.5",
          "form": "딱히 관심은 없어요",
          "original_form": "딱히 관심은 없어요",
          "speaker_id": "1",
          "time": "20210830 09:18"
        },
        {
          "id": "MDRW2100023492.1.6",
          "form": "거치대도 종류 다양하던데",
          "original_form": "거치대도 종류 다양하던데",
          "speaker_id": "1",
          "time": "20210830 09:19"
        },
        {
          "id": "MDRW2100023492.1.7",
          "form": "자동으로 잡아주는 제품 사셨나요?",
          "original_form": "자동으로 잡아주는 제품 사셨나요?",
          "speaker_id": "1",
          "time": "20210830 09:19"
        },
        {
          "id": "MDRW2100023492.1.8",
          "form": "네 자동으로 잡아주는거 샀어요",
          "original_form": "네 자동으로 잡아주는거 샀어요",
          "speaker_id": "2",
          "time": "20210830 09:20"
        },
        {
          "id": "MDRW2100023492.1.9",
          "form": "핸들 커버도 사고 싶더라고요",
          "original_form": "핸들 커버도 사고 싶더라고요",
          "speaker_id": "2",
          "time": "20210830 09:20"
        },
        {
          "id": "MDRW2100023492.1.10",
          "form": "손에. 땀이 많은 편이라서요",
          "original_form": "손에. 땀이 많은 편이라서요",
          "speaker_id": "2",
          "time": "20210830 09:20"
        },
        {
          "id": "MDRW2100023492.1.11",
          "form": "맞네요 핸들 커버도 필요하겠어요",
          "original_form": "맞네요 핸들 커버도 필요하겠어요",
          "speaker_id": "1",
          "time": "20210830 09:21"
        },
        {
          "id": "MDRW2100023492.1.12",
          "form": "운전을 하질 않아서 어떤게 필요한지 몰라서 신기해요",
          "original_form": "운전을 하질 않아서 어떤게 필요한지 몰라서 신기해요",
          "speaker_id": "1",
          "time": "20210830 09:22"
        },
        {
          "id": "MDRW2100023492.1.13",
          "form": "중고거래는 해보신적 있으세요?",
          "original_form": "중고거래는 해보신적 있으세요?",
          "speaker_id": "1",
          "time": "20210830 09:22"
        },
        {
          "id": "MDRW2100023492.1.14",
          "form": "스팸 팔아본적 있어요ㅋㅋ",
          "original_form": "스팸 팔아본적 있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:23"
        },
        {
          "id": "MDRW2100023492.1.15",
          "form": "저도 운전한지 얼마안되서 그때그때 사고있어요",
          "original_form": "저도 운전한지 얼마안되서 그때그때 사고있어요",
          "speaker_id": "2",
          "time": "20210830 09:23"
        },
        {
          "id": "MDRW2100023492.1.16",
          "form": "중고거래 해보셨어요?",
          "original_form": "중고거래 해보셨어요?",
          "speaker_id": "2",
          "time": "20210830 09:24"
        },
        {
          "id": "MDRW2100023492.1.17",
          "form": "네 이사하기전에 책상, 만화책, 음반, 다트판,공기청정기 이런거 팔아봤어요",
          "original_form": "네 이사하기전에 책상, 만화책, 음반, 다트판,공기청정기 이런거 팔아봤어요",
          "speaker_id": "1",
          "time": "20210830 09:27"
        },
        {
          "id": "MDRW2100023492.1.18",
          "form": "오 자주하시나보네요 파는게 쉽지 않더라고요",
          "original_form": "오 자주하시나보네요 파는게 쉽지 않더라고요",
          "speaker_id": "2",
          "time": "20210830 09:29"
        },
        {
          "id": "MDRW2100023492.1.19",
          "form": "그죠...싸게올리면 그래도 팔리긴해요",
          "original_form": "그죠...싸게올리면 그래도 팔리긴해요",
          "speaker_id": "1",
          "time": "20210830 09:31"
        },
        {
          "id": "MDRW2100023492.1.20",
          "form": "이사할때 버리자니 아깝고 쓰진않을것들 팔기 좋았어요",
          "original_form": "이사할때 버리자니 아깝고 쓰진않을것들 팔기 좋았어요",
          "speaker_id": "1",
          "time": "20210830 09:31"
        },
        {
          "id": "MDRW2100023492.1.21",
          "form": "저도 가방 팔아보려고 싸게 올렸는데 안사더라고요ㅋㅋ",
          "original_form": "저도 가방 팔아보려고 싸게 올렸는데 안사더라고요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 09:31"
        },
        {
          "id": "MDRW2100023492.1.22",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "2",
          "time": "20210830 09:31"
        },
        {
          "id": "MDRW2100023492.1.23",
          "form": "사보신적도 있으세요?",
          "original_form": "사보신적도 있으세요?",
          "speaker_id": "1",
          "time": "20210830 09:58"
        },
        {
          "id": "MDRW2100023492.1.24",
          "form": "저는 가방 사봤어요ㅋㅋ 혼자 살아서 딱히 뭘 사질 않네요ㅋㅋ",
          "original_form": "저는 가방 사봤어요ㅋㅋ 혼자 살아서 딱히 뭘 사질 않네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 10:10"
        },
        {
          "id": "MDRW2100023492.1.25",
          "form": "저는 게임을 좋아해서 동물의숲 사봤어요",
          "original_form": "저는 게임을 좋아해서 동물의숲 사봤어요",
          "speaker_id": "1",
          "time": "20210830 10:12"
        },
        {
          "id": "MDRW2100023492.1.26",
          "form": "아 게임씨디 그런건가요?게임 쪽은 몰리ㅡ서요",
          "original_form": "아 게임씨디 그런건가요?게임 쪽은 몰리ㅡ서요",
          "speaker_id": "2",
          "time": "20210830 10:16"
        },
        {
          "id": "MDRW2100023492.1.27",
          "form": "네 비슷해요!",
          "original_form": "네 비슷해요!",
          "speaker_id": "1",
          "time": "20210830 10:42"
        }
      ]
