      "utterance": [
        {
          "id": "MDRW2100020608.1.1",
          "form": "트위터도 온라인 커뮤 포함일까요?",
          "original_form": "트위터도 온라인 커뮤 포함일까요?",
          "speaker_id": "1",
          "time": "20210828 10:23"
        },
        {
          "id": "MDRW2100020608.1.2",
          "form": "네 괜찮지 않을까요?",
          "original_form": "네 괜찮지 않을까요?",
          "speaker_id": "2",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100020608.1.3",
          "form": "트위터 많이 하세요?",
          "original_form": "트위터 많이 하세요?",
          "speaker_id": "2",
          "time": "20210828 10:24"
        },
        {
          "id": "MDRW2100020608.1.4",
          "form": "오 덕질용으로 많이 해요 ㅎㅎ",
          "original_form": "오 덕질용으로 많이 해요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020608.1.5",
          "form": "콘서트 직캠도 보통 트위터 타고 많이 가는 것 같아요",
          "original_form": "콘서트 직캠도 보통 트위터 타고 많이 가는 것 같아요",
          "speaker_id": "1",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020608.1.6",
          "form": "저는 트위터는 안하긴하는데",
          "original_form": "저는 트위터는 안하긴하는데",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020608.1.7",
          "form": "아 근데 비추하실 줄은 몰랐어요 ㅠㅠ",
          "original_form": "아 근데 비추하실 줄은 몰랐어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020608.1.8",
          "form": "트위터에 진짜 볼게 많더라고",
          "original_form": "트위터에 진짜 볼게 많더라고",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020608.1.9",
          "form": "스탠딩이요?",
          "original_form": "스탠딩이요?",
          "speaker_id": "2",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020608.1.10",
          "form": "트위터에는 손 잡아주고",
          "original_form": "트위터에는 손 잡아주고",
          "speaker_id": "1",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020608.1.11",
          "form": "옆에서 인사하고",
          "original_form": "옆에서 인사하고",
          "speaker_id": "1",
          "time": "20210828 10:25"
        },
        {
          "id": "MDRW2100020608.1.12",
          "form": "좋은 장면이 많아서 ㅋㅋㅋ",
          "original_form": "좋은 장면이 많아서 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100020608.1.13",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100020608.1.14",
          "form": "ㅋㅋㄱ아하!",
          "original_form": "ㅋㅋㄱ아하!",
          "speaker_id": "2",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100020608.1.15",
          "form": "현실을 알려주셔서 감사합니다",
          "original_form": "현실을 알려주셔서 감사합니다",
          "speaker_id": "1",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100020608.1.16",
          "form": "근데 주변사람따라서 달라지나봐요",
          "original_form": "근데 주변사람따라서 달라지나봐요",
          "speaker_id": "2",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100020608.1.17",
          "form": "팬덤 성향 따라서도 다를거에요",
          "original_form": "팬덤 성향 따라서도 다를거에요",
          "speaker_id": "2",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100020608.1.18",
          "form": "아아 그런가",
          "original_form": "아아 그런가",
          "speaker_id": "1",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100020608.1.19",
          "form": "그래도 경험삼아 가보세요",
          "original_form": "그래도 경험삼아 가보세요",
          "speaker_id": "2",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100020608.1.20",
          "form": "좋을수도 있으니까요",
          "original_form": "좋을수도 있으니까요",
          "speaker_id": "2",
          "time": "20210828 10:26"
        },
        {
          "id": "MDRW2100020608.1.21",
          "form": "그래도 가까이서 봐서 좋으셨겠어요",
          "original_form": "그래도 가까이서 봐서 좋으셨겠어요",
          "speaker_id": "1",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020608.1.22",
          "form": "진짜 바로 앞에서 보긴했어요",
          "original_form": "진짜 바로 앞에서 보긴했어요",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020608.1.23",
          "form": "그치만 찌부되는줄알았죠ㅋㅋ",
          "original_form": "그치만 찌부되는줄알았죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:27"
        },
        {
          "id": "MDRW2100020608.1.24",
          "form": "앜ㅋㅋ 트위터에 그런 후기가 없어서",
          "original_form": "앜ㅋㅋ 트위터에 그런 후기가 없어서",
          "speaker_id": "1",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020608.1.25",
          "form": "이 분들이 좋은 것만 써주셨구나 ㅋㅋㅋㅋ",
          "original_form": "이 분들이 좋은 것만 써주셨구나 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020608.1.26",
          "form": "다들 좋으셨나보네요ㅠ",
          "original_form": "다들 좋으셨나보네요ㅠ",
          "speaker_id": "2",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020608.1.27",
          "form": "하긴 팬이 그리 많은데 ㅋㅋㅋ",
          "original_form": "하긴 팬이 그리 많은데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020608.1.28",
          "form": "아니에요 제가 팬스쪽이여서 그럴수도있어요",
          "original_form": "아니에요 제가 팬스쪽이여서 그럴수도있어요",
          "speaker_id": "2",
          "time": "20210828 10:28"
        },
        {
          "id": "MDRW2100020608.1.29",
          "form": "아마 좋은 것만 쓰자하는 마음이었을 거예요",
          "original_form": "아마 좋은 것만 쓰자하는 마음이었을 거예요",
          "speaker_id": "1",
          "time": "20210828 10:28"
        }
      ]
