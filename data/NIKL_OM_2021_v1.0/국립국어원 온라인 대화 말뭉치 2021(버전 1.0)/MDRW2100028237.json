      "utterance": [
        {
          "id": "MDRW2100028237.1.1",
          "form": "파티요? 나눠내는거요?",
          "original_form": "파티요? 나눠내는거요?",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028237.1.2",
          "form": "제가 결제하고 가족들한테 아이디 뿌렸어요ㅋㅋ",
          "original_form": "제가 결제하고 가족들한테 아이디 뿌렸어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:30"
        },
        {
          "id": "MDRW2100028237.1.3",
          "form": "와 저는 외동이라 나눠볼 사람이 없다는 ㅋㅋㅋ",
          "original_form": "와 저는 외동이라 나눠볼 사람이 없다는 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028237.1.4",
          "form": "파티 짜주는 사이트가 있어요 ㅎㅎㅎ",
          "original_form": "파티 짜주는 사이트가 있어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028237.1.5",
          "form": "그럼 제법 돈 쓰시겠어요",
          "original_form": "그럼 제법 돈 쓰시겠어요",
          "speaker_id": "1",
          "time": "20210902 18:31"
        },
        {
          "id": "MDRW2100028237.1.6",
          "form": "아하 그런 사이트도 있군요",
          "original_form": "아하 그런 사이트도 있군요",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028237.1.7",
          "form": "부모님이 주말부부셔서",
          "original_form": "부모님이 주말부부셔서",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028237.1.8",
          "form": "따로따로 등록해드리고",
          "original_form": "따로따로 등록해드리고",
          "speaker_id": "2",
          "time": "20210902 18:32"
        },
        {
          "id": "MDRW2100028237.1.9",
          "form": "시댁에도 등록해드렸어요ㅋㅋ",
          "original_form": "시댁에도 등록해드렸어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028237.1.10",
          "form": "용돈가끔 주시니 할만합니다",
          "original_form": "용돈가끔 주시니 할만합니다",
          "speaker_id": "2",
          "time": "20210902 18:33"
        },
        {
          "id": "MDRW2100028237.1.11",
          "form": "오 여러사람 보니 좋네요",
          "original_form": "오 여러사람 보니 좋네요",
          "speaker_id": "1",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100028237.1.12",
          "form": "넷플",
          "original_form": "넷플",
          "speaker_id": "1",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100028237.1.13",
          "form": "브리저튼 재밌게 봤어요",
          "original_form": "브리저튼 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210902 18:35"
        },
        {
          "id": "MDRW2100028237.1.14",
          "form": "저도 브리저튼 봤어요!",
          "original_form": "저도 브리저튼 봤어요!",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100028237.1.15",
          "form": "누군가랑 같이 볼 수 있는건 아니었지만ㅋㅋㅋ",
          "original_form": "누군가랑 같이 볼 수 있는건 아니었지만ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100028237.1.16",
          "form": "진짜 재미있었어요 ㅋㅋㅋ",
          "original_form": "진짜 재미있었어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:36"
        },
        {
          "id": "MDRW2100028237.1.17",
          "form": "뒤로갈수록 오우 ㅋㅋㅋㅋ",
          "original_form": "뒤로갈수록 오우 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:37"
        },
        {
          "id": "MDRW2100028237.1.18",
          "form": "남여배우 다음시즌에도 보고싶은데 안나온데요",
          "original_form": "남여배우 다음시즌에도 보고싶은데 안나온데요",
          "speaker_id": "1",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100028237.1.19",
          "form": "아이고 민망하지만 눈을 못떼겠네",
          "original_form": "아이고 민망하지만 눈을 못떼겠네",
          "speaker_id": "2",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100028237.1.20",
          "form": "아 들었어요 그집 형제자매들 얘기로 이어진다면서요",
          "original_form": "아 들었어요 그집 형제자매들 얘기로 이어진다면서요",
          "speaker_id": "2",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100028237.1.21",
          "form": "남자주인공 진짜 잘생겼는데 아쉬워요",
          "original_form": "남자주인공 진짜 잘생겼는데 아쉬워요",
          "speaker_id": "2",
          "time": "20210902 18:38"
        },
        {
          "id": "MDRW2100028237.1.22",
          "form": "ㄱㅋㅋㅋㅋ  핫한 배우가 됐더라구요",
          "original_form": "ㄱㅋㅋㅋㅋ  핫한 배우가 됐더라구요",
          "speaker_id": "1",
          "time": "20210902 18:39"
        },
        {
          "id": "MDRW2100028237.1.23",
          "form": "그럴만도 합니다ㅋㅋ",
          "original_form": "그럴만도 합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100028237.1.24",
          "form": "저는 요새 넷플릭스에선 그레이스 앤 프랭키를 보고있어요",
          "original_form": "저는 요새 넷플릭스에선 그레이스 앤 프랭키를 보고있어요",
          "speaker_id": "2",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100028237.1.25",
          "form": "혹시 들어보셨나요?",
          "original_form": "혹시 들어보셨나요?",
          "speaker_id": "2",
          "time": "20210902 18:40"
        },
        {
          "id": "MDRW2100028237.1.26",
          "form": "오 비슷한 계열일까요?!",
          "original_form": "오 비슷한 계열일까요?!",
          "speaker_id": "1",
          "time": "20210902 18:44"
        },
        {
          "id": "MDRW2100028237.1.27",
          "form": "완전 달라요ㅋㅋ",
          "original_form": "완전 달라요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 18:45"
        },
        {
          "id": "MDRW2100028237.1.28",
          "form": "헉 6시즌이래요",
          "original_form": "헉 6시즌이래요",
          "speaker_id": "1",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100028237.1.29",
          "form": "게이결혼이 합법화되면서 갑자기 남편들이 커밍아웃하고 결혼하겠다고 해서 70대 그레이스와 프랭키가",
          "original_form": "게이결혼이 합법화되면서 갑자기 남편들이 커밍아웃하고 결혼하겠다고 해서 70대 그레이스와 프랭키가",
          "speaker_id": "2",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100028237.1.30",
          "form": "이걸 첨 들어보다니",
          "original_form": "이걸 첨 들어보다니",
          "speaker_id": "1",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100028237.1.31",
          "form": "ㅋㅋㅋㅋ 급 같이 살게되서 생기는 에피소드예요",
          "original_form": "ㅋㅋㅋㅋ 급 같이 살게되서 생기는 에피소드예요",
          "speaker_id": "2",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100028237.1.32",
          "form": "부인끼리 친규가됏나요 ㅋㅋㅋㅋㅋ",
          "original_form": "부인끼리 친규가됏나요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 18:46"
        },
        {
          "id": "MDRW2100028237.1.33",
          "form": "이상한데 훈훈하고 교훈적인듯 웃기고",
          "original_form": "이상한데 훈훈하고 교훈적인듯 웃기고",
          "speaker_id": "2",
          "time": "20210902 18:46"
        }
      ]
