      "utterance": [
        {
          "id": "MDRW2100026665.1.1",
          "form": "연민의 굴레 아니",
          "original_form": "연민의 굴레 아니",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026665.1.2",
          "form": "그 책 나 세 번 읽었어",
          "original_form": "그 책 나 세 번 읽었어",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026665.1.3",
          "form": "인기 작품이지",
          "original_form": "인기 작품이지",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026665.1.4",
          "form": "그 책은 꾸준하게 인기가 많더라",
          "original_form": "그 책은 꾸준하게 인기가 많더라",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026665.1.5",
          "form": "책으로 나오기 전에 연재 당시에도 인기가 많았어",
          "original_form": "책으로 나오기 전에 연재 당시에도 인기가 많았어",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026665.1.6",
          "form": "뭔가 매력적인 내용이야",
          "original_form": "뭔가 매력적인 내용이야",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026665.1.7",
          "form": "여주인공 차 련이 귀여워",
          "original_form": "여주인공 차 련이 귀여워",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026665.1.8",
          "form": "나도 여주인공 캐릭터가 너무 좋아",
          "original_form": "나도 여주인공 캐릭터가 너무 좋아",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026665.1.9",
          "form": "자 주인공도 좋아",
          "original_form": "자 주인공도 좋아",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026665.1.10",
          "form": "나는 남 주는 별로",
          "original_form": "나는 남 주는 별로",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026665.1.11",
          "form": "나는 그 주왕 머리 남자에 귀여운데",
          "original_form": "나는 그 주왕 머리 남자에 귀여운데",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026665.1.12",
          "form": "그게 누구지 정보통인가",
          "original_form": "그게 누구지 정보통인가",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026665.1.13",
          "form": "이전 학생회장",
          "original_form": "이전 학생회장",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026665.1.14",
          "form": "아 그 병약한 학생회장 말하는구나",
          "original_form": "아 그 병약한 학생회장 말하는구나",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026665.1.15",
          "form": "나 병역 하네 안 좋아하는데",
          "original_form": "나 병역 하네 안 좋아하는데",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
