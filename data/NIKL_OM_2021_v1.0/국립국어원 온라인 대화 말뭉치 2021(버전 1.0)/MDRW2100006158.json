      "utterance": [
        {
          "id": "MDRW2100006158.1.1",
          "form": "하하....어떻게든 되겠죠ㅠㅜ",
          "original_form": "하하....어떻게든 되겠죠ㅠㅜ",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006158.1.2",
          "form": "좋아하는 게임이 뭐야",
          "original_form": "좋아하는 게임이 뭐야",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006158.1.3",
          "form": "당연히 롤이지!",
          "original_form": "당연히 롤이지!",
          "speaker_id": "2",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006158.1.4",
          "form": "주캐뭐야?",
          "original_form": "주캐뭐야?",
          "speaker_id": "1",
          "time": "20210907 20:59"
        },
        {
          "id": "MDRW2100006158.1.5",
          "form": "근데 요즘엔 스타크래프트도 조금 관심가더라",
          "original_form": "근데 요즘엔 스타크래프트도 조금 관심가더라",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006158.1.6",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006158.1.7",
          "form": "나 하는게 많아서",
          "original_form": "나 하는게 많아서",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006158.1.8",
          "form": "동문서답 __",
          "original_form": "동문서답 __",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006158.1.9",
          "form": "주로 사미라 베인?",
          "original_form": "주로 사미라 베인?",
          "speaker_id": "2",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006158.1.10",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006158.1.11",
          "form": "좋아하는 서폿은?",
          "original_form": "좋아하는 서폿은?",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100006158.1.12",
          "form": "룰루...?",
          "original_form": "룰루...?",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006158.1.13",
          "form": "룰루 좋잖아요!",
          "original_form": "룰루 좋잖아요!",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006158.1.14",
          "form": "럭스는여 ㅡㅡ",
          "original_form": "럭스는여 ㅡㅡ",
          "speaker_id": "1",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100006158.1.15",
          "form": "룰루를 이길순...1티어잖아요!",
          "original_form": "룰루를 이길순...1티어잖아요!",
          "speaker_id": "2",
          "time": "20210907 21:04"
        },
        {
          "id": "MDRW2100006158.1.16",
          "form": "담판 룰루?",
          "original_form": "담판 룰루?",
          "speaker_id": "1",
          "time": "20210907 21:18"
        },
        {
          "id": "MDRW2100006158.1.17",
          "form": "좋지!",
          "original_form": "좋지!",
          "speaker_id": "2",
          "time": "20210907 21:26"
        },
        {
          "id": "MDRW2100006158.1.18",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100006158.1.19",
          "form": "유미입니다",
          "original_form": "유미입니다",
          "speaker_id": "1",
          "time": "20210907 21:27"
        },
        {
          "id": "MDRW2100006158.1.20",
          "form": "에??",
          "original_form": "에??",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006158.1.21",
          "form": "룰루네요",
          "original_form": "룰루네요",
          "speaker_id": "1",
          "time": "20210907 21:59"
        },
        {
