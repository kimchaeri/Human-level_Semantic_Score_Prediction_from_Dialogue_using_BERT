      "utterance": [
        {
          "id": "MDRW2100023345.1.1",
          "form": "같이 노려보아요 ㅋㅋㅋ",
          "original_form": "같이 노려보아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 00:33"
        },
        {
          "id": "MDRW2100023345.1.2",
          "form": "넹ㅋㅋㅋ",
          "original_form": "넹ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100023345.1.3",
          "form": "저 네이버스토어파인데 11번가로 갈아탈 수도 있을것 같아요ㅋㅋㅋ",
          "original_form": "저 네이버스토어파인데 11번가로 갈아탈 수도 있을것 같아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 00:37"
        },
        {
          "id": "MDRW2100023345.1.4",
          "form": "ㅋㅋㅋㅋ 전 간헐적으로 갈아타야겠어요 ㅋㅋㅋㅋㅋ 딜을 노리는 하이애나로 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ 전 간헐적으로 갈아타야겠어요 ㅋㅋㅋㅋㅋ 딜을 노리는 하이애나로 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 18:44"
        },
        {
          "id": "MDRW2100023345.1.5",
          "form": "오.. 고수님!!ㅋㅋㅋ",
          "original_form": "오.. 고수님!!ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100023345.1.6",
          "form": "모르면 고수님의 길을 따라도 반은 된댔는데 따라가야겠슴닷",
          "original_form": "모르면 고수님의 길을 따라도 반은 된댔는데 따라가야겠슴닷",
          "speaker_id": "1",
          "time": "20210830 18:48"
        },
        {
          "id": "MDRW2100023345.1.7",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 좋은게 있다면 알려드릴께여 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ 좋은게 있다면 알려드릴께여 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 20:10"
        },
        {
          "id": "MDRW2100023345.1.8",
          "form": "우왕 진짜 천사님 아니신가욧?!",
          "original_form": "우왕 진짜 천사님 아니신가욧?!",
          "speaker_id": "1",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100023345.1.9",
          "form": "저도 열심히 기웃거려보겠슴닷",
          "original_form": "저도 열심히 기웃거려보겠슴닷",
          "speaker_id": "1",
          "time": "20210830 20:19"
        },
        {
          "id": "MDRW2100023345.1.10",
          "form": "ㅋㅋㅋㅋㅋ 빨리  블랙프라이데이가 오길 기다리는중이에용ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 빨리  블랙프라이데이가 오길 기다리는중이에용ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:19"
        },
        {
          "id": "MDRW2100023345.1.11",
          "form": "그때 저 참여해보려했다가 하는게 맞나 이러는 사이 끝났어욬ㅋㄲㄱㄲ",
          "original_form": "그때 저 참여해보려했다가 하는게 맞나 이러는 사이 끝났어욬ㅋㄲㄱㄲ",
          "speaker_id": "1",
          "time": "20210830 21:35"
        },
        {
          "id": "MDRW2100023345.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ 아니 너무 늦게 아신거 아니냐구요 ㅋㅋㅋㅋ친구들 뭐하는거야 알랴줘야지!!ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 아니 너무 늦게 아신거 아니냐구요 ㅋㅋㅋㅋ친구들 뭐하는거야 알랴줘야지!!ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:39"
        },
        {
          "id": "MDRW2100023345.1.13",
          "form": "그니까요!ㅋㅋㅋ 제 주변에 왜 안하는거죠ㅠㅠ",
          "original_form": "그니까요!ㅋㅋㅋ 제 주변에 왜 안하는거죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100023345.1.14",
          "form": "그렇게 2번 놓쳤더니 안하게 되더라구요 흑흑",
          "original_form": "그렇게 2번 놓쳤더니 안하게 되더라구요 흑흑",
          "speaker_id": "1",
          "time": "20210830 21:46"
        },
        {
          "id": "MDRW2100023345.1.15",
          "form": "제주변은 딜 뜨면 서로 알려주는데 고마운 사람 ㅋㅋㅋ",
          "original_form": "제주변은 딜 뜨면 서로 알려주는데 고마운 사람 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 21:50"
        },
        {
          "id": "MDRW2100023345.1.16",
          "form": "진짜 좋은분 옆 좋은분이네요ㅋㅋ",
          "original_form": "진짜 좋은분 옆 좋은분이네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100023345.1.17",
          "form": "부럽슴당",
          "original_form": "부럽슴당",
          "speaker_id": "1",
          "time": "20210830 21:57"
        },
        {
          "id": "MDRW2100023345.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 도우며 살자 이런거같아요 ㅋㅋㅋㅋㅋㅋ 당근으로도 잘사더라구요 ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 도우며 살자 이런거같아요 ㅋㅋㅋㅋㅋㅋ 당근으로도 잘사더라구요 ㅋㅋ",
