      "utterance": [
        {
          "id": "MDRW2100003373.1.1",
          "form": "가위눌리는건 꿈이랑 다른가요? 건강 문제 인가?",
          "original_form": "가위눌리는건 꿈이랑 다른가요? 건강 문제 인가?",
          "speaker_id": "1",
          "time": "20210820 22:17"
        },
        {
          "id": "MDRW2100003373.1.2",
          "form": "혈액순환이 안되면 눌리기도하는거같고",
          "original_form": "혈액순환이 안되면 눌리기도하는거같고",
          "speaker_id": "2",
          "time": "20210820 22:18"
        },
        {
          "id": "MDRW2100003373.1.3",
          "form": "피곤해도 그런거같아요",
          "original_form": "피곤해도 그런거같아요",
          "speaker_id": "2",
          "time": "20210820 22:18"
        },
        {
          "id": "MDRW2100003373.1.4",
          "form": "건강을 위해 요즘 약 많이 먹더라고요",
          "original_form": "건강을 위해 요즘 약 많이 먹더라고요",
          "speaker_id": "1",
          "time": "20210820 22:30"
        },
        {
          "id": "MDRW2100003373.1.5",
          "form": "영양제 다들 엄청먹더라구요 전귀찮아서 사면 꼭 날짜지나서 버리게되네요",
          "original_form": "영양제 다들 엄청먹더라구요 전귀찮아서 사면 꼭 날짜지나서 버리게되네요",
          "speaker_id": "2",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003373.1.6",
          "form": "먹었는지 안먹었는지 까먹기도ㅋㅋ",
          "original_form": "먹었는지 안먹었는지 까먹기도ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003373.1.7",
          "form": "아직 건강하신가봐요",
          "original_form": "아직 건강하신가봐요",
          "speaker_id": "1",
          "time": "20210820 22:32"
        },
        {
          "id": "MDRW2100003373.1.8",
          "form": "건강한편인거 같아요 몇년째 감기도 안걸렸으니",
          "original_form": "건강한편인거 같아요 몇년째 감기도 안걸렸으니",
          "speaker_id": "2",
          "time": "20210820 22:33"
        },
        {
          "id": "MDRW2100003373.1.9",
          "form": "와 예방접종 맞으세요?",
          "original_form": "와 예방접종 맞으세요?",
          "speaker_id": "1",
          "time": "20210820 22:36"
        },
        {
          "id": "MDRW2100003373.1.10",
          "form": "백신 신청하긴했는데  무서워서 고민중이예요",
          "original_form": "백신 신청하긴했는데  무서워서 고민중이예요",
          "speaker_id": "2",
          "time": "20210820 22:37"
        },
        {
          "id": "MDRW2100003373.1.11",
          "form": "백신맞으셨어요?",
          "original_form": "백신맞으셨어요?",
          "speaker_id": "2",
          "time": "20210820 22:37"
        },
        {
          "id": "MDRW2100003373.1.12",
          "form": "네 1차는 맞았어요",
          "original_form": "네 1차는 맞았어요",
          "speaker_id": "1",
          "time": "20210820 22:46"
        },
        {
          "id": "MDRW2100003373.1.13",
          "form": "생각보다 별 일 없었어요",
          "original_form": "생각보다 별 일 없었어요",
          "speaker_id": "1",
          "time": "20210820 22:46"
        },
        {
          "id": "MDRW2100003373.1.14",
          "form": "화이자맞으셨나요?",
          "original_form": "화이자맞으셨나요?",
          "speaker_id": "2",
          "time": "20210820 22:47"
        },
        {
          "id": "MDRW2100003373.1.15",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210820 22:47"
        },
        {
          "id": "MDRW2100003373.1.16",
          "form": "측근들은 맞고 다 괜찮아서 맞을까싶다가도 건너건너 사고난경우도 봐서 무섭더라구요",
          "original_form": "측근들은 맞고 다 괜찮아서 맞을까싶다가도 건너건너 사고난경우도 봐서 무섭더라구요",
          "speaker_id": "2",
          "time": "20210820 22:47"
        },
        {
          "id": "MDRW2100003373.1.17",
          "form": "그래도 맞고나면 좀 든든해요",
          "original_form": "그래도 맞고나면 좀 든든해요",
          "speaker_id": "1",
          "time": "20210820 22:47"
        },
        {
          "id": "MDRW2100003373.1.18",
          "form": "증상이 어때요?",
          "original_form": "증상이 어때요?",
          "speaker_id": "2",
          "time": "20210820 22:48"
        },
        {
          "id": "MDRW2100003373.1.19",
          "form": "일단 팔 아프긴 아프고",
          "original_form": "일단 팔 아프긴 아프고",
          "speaker_id": "1",
          "time": "20210820 22:48"
        },
        {
          "id": "MDRW2100003373.1.20",
          "form": "열 나긴 나요ㅋㅋ",
          "original_form": "열 나긴 나요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 22:48"
        },
        {
          "id": "MDRW2100003373.1.21",
          "form": "화이자가 팔이 아프대요",
          "original_form": "화이자가 팔이 아프대요",
          "speaker_id": "2",
          "time": "20210820 22:49"
        },
        {
          "id": "MDRW2100003373.1.22",
          "form": "2차가 더 아프대서",
          "original_form": "2차가 더 아프대서",
          "speaker_id": "1",
          "time": "20210820 22:50"
        },
        {
          "id": "MDRW2100003373.1.23",
          "form": "좀 무섭긴 합니다ㅎ",
          "original_form": "좀 무섭긴 합니다ㅎ",
          "speaker_id": "1",
          "time": "20210820 22:50"
        }
      ]
    }
