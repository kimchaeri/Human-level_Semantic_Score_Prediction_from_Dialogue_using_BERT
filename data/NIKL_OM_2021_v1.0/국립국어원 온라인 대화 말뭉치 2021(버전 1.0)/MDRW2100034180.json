      "utterance": [
        {
          "id": "MDRW2100034180.1.1",
          "form": "야식 드시나용?",
          "original_form": "야식 드시나용?",
          "speaker_id": "2",
          "time": "20210827 20:24"
        },
        {
          "id": "MDRW2100034180.1.2",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100034180.1.3",
          "form": "오늘 입맛 버려서",
          "original_form": "오늘 입맛 버려서",
          "speaker_id": "1",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100034180.1.4",
          "form": "복숭아 먹어야 겠어요",
          "original_form": "복숭아 먹어야 겠어요",
          "speaker_id": "1",
          "time": "20210827 20:25"
        },
        {
          "id": "MDRW2100034180.1.5",
          "form": "크아 과일이 후식으로 최고죠",
          "original_form": "크아 과일이 후식으로 최고죠",
          "speaker_id": "2",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100034180.1.6",
          "form": "네 양심상 ㅋㅋ아이스크림 패쓰",
          "original_form": "네 양심상 ㅋㅋ아이스크림 패쓰",
          "speaker_id": "1",
          "time": "20210827 20:26"
        },
        {
          "id": "MDRW2100034180.1.7",
          "form": "전 딱딱한 복숭아요",
          "original_form": "전 딱딱한 복숭아요",
          "speaker_id": "1",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100034180.1.8",
          "form": "뭐세요?",
          "original_form": "뭐세요?",
          "speaker_id": "1",
          "time": "20210827 20:27"
        },
        {
          "id": "MDRW2100034180.1.9",
          "form": "오 저는 말랑한 복숭아요!",
          "original_form": "오 저는 말랑한 복숭아요!",
          "speaker_id": "2",
          "time": "20210827 21:16"
        },
        {
          "id": "MDRW2100034180.1.10",
          "form": "오 다들 식성이 이렇게 달라요 ㅎㅎ",
          "original_form": "오 다들 식성이 이렇게 달라요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100034180.1.11",
          "form": "복숭아 ㅋㅋ몇개 먹고 맛없는거 잊으려고요",
          "original_form": "복숭아 ㅋㅋ몇개 먹고 맛없는거 잊으려고요",
          "speaker_id": "1",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100034180.1.12",
          "form": "야식은 하세요?",
          "original_form": "야식은 하세요?",
          "speaker_id": "1",
          "time": "20210827 21:18"
        },
        {
          "id": "MDRW2100034180.1.13",
          "form": "복숭아 끝은 맛있게 끝내고 싶어져요",
          "original_form": "복숭아 끝은 맛있게 끝내고 싶어져요",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100034180.1.14",
          "form": "설빙 먹으려구요!",
          "original_form": "설빙 먹으려구요!",
          "speaker_id": "2",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100034180.1.15",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100034180.1.16",
          "form": "설빙 정말 사르르 맛나죠",
          "original_form": "설빙 정말 사르르 맛나죠",
          "speaker_id": "1",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100034180.1.17",
          "form": "저 빙수쿠폰 있는데",
          "original_form": "저 빙수쿠폰 있는데",
          "speaker_id": "1",
          "time": "20210827 21:20"
        },
        {
          "id": "MDRW2100034180.1.18",
          "form": "여름가기전에 낼 먹어야 겠어요",
          "original_form": "여름가기전에 낼 먹어야 겠어요",
          "speaker_id": "1",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100034180.1.19",
          "form": "오늘 풀코스 완성 이시네요 ㅎㅎ",
          "original_form": "오늘 풀코스 완성 이시네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100034180.1.20",
          "form": "크으 좋죵",
          "original_form": "크으 좋죵",
          "speaker_id": "2",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100034180.1.21",
          "form": "넹ㅋㅋ 이러다 내일 못일어날까 걱정이여요",
          "original_form": "넹ㅋㅋ 이러다 내일 못일어날까 걱정이여요",
          "speaker_id": "2",
          "time": "20210827 21:21"
        },
        {
          "id": "MDRW2100034180.1.22",
          "form": "ㅋㅋ에이 설마요",
          "original_form": "ㅋㅋ에이 설마요",
          "speaker_id": "1",
          "time": "20210827 21:22"
        },
        {
          "id": "MDRW2100034180.1.23",
          "form": "낼은 내일의 태양이 떠오른답니다 ㅋㅋ",
          "original_form": "낼은 내일의 태양이 떠오른답니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:23"
        },
        {
          "id": "MDRW2100034180.1.24",
          "form": "또낼은 뭐 내일대로 잘 먹음되죠 뭐",
          "original_form": "또낼은 뭐 내일대로 잘 먹음되죠 뭐",
          "speaker_id": "1",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034180.1.25",
          "form": "오늘은 불금 달리죠 ㅋㅋ",
          "original_form": "오늘은 불금 달리죠 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 21:24"
        },
        {
          "id": "MDRW2100034180.1.26",
          "form": "맞아욥",
          "original_form": "맞아욥",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100034180.1.27",
          "form": "낼 또 운동해야죠",
          "original_form": "낼 또 운동해야죠",
          "speaker_id": "2",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100034180.1.28",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100034180.1.29",
          "form": "사실 다욧은 낼부터",
          "original_form": "사실 다욧은 낼부터",
          "speaker_id": "1",
          "time": "20210827 21:33"
        },
        {
          "id": "MDRW2100034180.1.30",
          "form": "어차피 먹은거 신경쓰지 마시고",
          "original_form": "어차피 먹은거 신경쓰지 마시고",
          "speaker_id": "1",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100034180.1.31",
          "form": "낼 열심히 운동하세요",
          "original_form": "낼 열심히 운동하세요",
          "speaker_id": "1",
          "time": "20210827 21:34"
        },
        {
          "id": "MDRW2100034180.1.32",
          "form": "넵! 감사합니당~",
          "original_form": "넵! 감사합니당~",
          "speaker_id": "2",
          "time": "20210827 21:35"
        }
      ]
    }
  ]
