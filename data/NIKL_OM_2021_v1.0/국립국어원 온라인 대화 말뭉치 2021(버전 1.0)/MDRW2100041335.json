      "utterance": [
        {
          "id": "MDRW2100041335.1.1",
          "form": "사료가 뭐가 좋은지 모르겠어요",
          "original_form": "사료가 뭐가 좋은지 모르겠어요",
          "speaker_id": "2",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041335.1.2",
          "form": "사료 일단 안전 인증받은곳이 좋다고는하더라구요",
          "original_form": "사료 일단 안전 인증받은곳이 좋다고는하더라구요",
          "speaker_id": "1",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041335.1.3",
          "form": "안전 인증이요?",
          "original_form": "안전 인증이요?",
          "speaker_id": "2",
          "time": "20210905 22:13"
        },
        {
          "id": "MDRW2100041335.1.4",
          "form": "아ㅠㅠ 저는 힐스먹이는데 괜찮겠죠??ㅜㅜ",
          "original_form": "아ㅠㅠ 저는 힐스먹이는데 괜찮겠죠??ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100041335.1.5",
          "form": "HACCP 인증이런거요?",
          "original_form": "HACCP 인증이런거요?",
          "speaker_id": "1",
          "time": "20210905 22:14"
        },
        {
          "id": "MDRW2100041335.1.6",
          "form": "아 해썹 ㅜㅜ",
          "original_form": "아 해썹 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 22:16"
        },
        {
          "id": "MDRW2100041335.1.7",
          "form": "지금 검색해보니 힐스가 오래되고 더 신경많이 쓴 사료네요~",
          "original_form": "지금 검색해보니 힐스가 오래되고 더 신경많이 쓴 사료네요~",
          "speaker_id": "1",
          "time": "20210905 22:16"
        },
        {
          "id": "MDRW2100041335.1.8",
          "form": "오 그런가요 다행이다ㅜㅜ",
          "original_form": "오 그런가요 다행이다ㅜㅜ",
          "speaker_id": "2",
          "time": "20210905 22:16"
        },
        {
          "id": "MDRW2100041335.1.9",
          "form": "미국사료라서ㅠㅠ 전 한국에서 국내 사료 먹이고픈데",
          "original_form": "미국사료라서ㅠㅠ 전 한국에서 국내 사료 먹이고픈데",
          "speaker_id": "2",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100041335.1.10",
          "form": "믿을만한게 없어요ㅠㅠ",
          "original_form": "믿을만한게 없어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100041335.1.11",
          "form": "풀무원에서도 사료만들던데",
          "original_form": "풀무원에서도 사료만들던데",
          "speaker_id": "1",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100041335.1.12",
          "form": "아 봤어요",
          "original_form": "아 봤어요",
          "speaker_id": "2",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100041335.1.13",
          "form": "믿을만한건지 모르겠어요 잘 먹긴 하던데",
          "original_form": "믿을만한건지 모르겠어요 잘 먹긴 하던데",
          "speaker_id": "2",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100041335.1.14",
          "form": "하림도 만들잖아요",
          "original_form": "하림도 만들잖아요",
          "speaker_id": "2",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100041335.1.15",
          "form": "저는 괜찮아보이더라구요~ 그것도 같이 먹었어요.",
          "original_form": "저는 괜찮아보이더라구요~ 그것도 같이 먹었어요.",
          "speaker_id": "1",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100041335.1.16",
          "form": "하림은 잘안먹더라구요ㅋ",
          "original_form": "하림은 잘안먹더라구요ㅋ",
          "speaker_id": "1",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100041335.1.17",
          "form": "저희도 하림이랑 풀무원 먹여봤어요",
          "original_form": "저희도 하림이랑 풀무원 먹여봤어요",
          "speaker_id": "2",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100041335.1.18",
          "form": "하림은 타피오카들어있어서 좀 꺼려져요ㅠㅠ",
          "original_form": "하림은 타피오카들어있어서 좀 꺼려져요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100041335.1.19",
          "form": "아미오로 바꿔볼까요 ...",
          "original_form": "아미오로 바꿔볼까요 ...",
          "speaker_id": "2",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100041335.1.20",
          "form": "풀무원 이번에 생수도 안좋은 리스트에 있어서",
          "original_form": "풀무원 이번에 생수도 안좋은 리스트에 있어서",
          "speaker_id": "2",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100041335.1.21",
          "form": "신뢰도가 떨어졌어요ㅠㅠ",
          "original_form": "신뢰도가 떨어졌어요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:18"
        },
        {
          "id": "MDRW2100041335.1.22",
          "form": "근데 그 기사가 과거 5년간 몇번 지적받았냐 이런거던데",
          "original_form": "근데 그 기사가 과거 5년간 몇번 지적받았냐 이런거던데",
          "speaker_id": "1",
          "time": "20210905 22:19"
        },
        {
          "id": "MDRW2100041335.1.23",
          "form": "아 네네",
          "original_form": "아 네네",
          "speaker_id": "2",
          "time": "20210905 22:19"
        },
        {
          "id": "MDRW2100041335.1.24",
          "form": "업체에서는 지금은 다 시정했다고 하고..",
          "original_form": "업체에서는 지금은 다 시정했다고 하고..",
          "speaker_id": "1",
          "time": "20210905 22:20"
        },
        {
          "id": "MDRW2100041335.1.25",
          "form": "그렇구나ㅠㅠ 믿을수없어ㅠㅠ",
          "original_form": "그렇구나ㅠㅠ 믿을수없어ㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 22:20"
        }
      ]
