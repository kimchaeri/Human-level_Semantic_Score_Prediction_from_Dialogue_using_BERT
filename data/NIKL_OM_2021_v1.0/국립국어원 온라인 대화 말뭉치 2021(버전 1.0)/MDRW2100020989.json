      "utterance": [
        {
          "id": "MDRW2100020989.1.1",
          "form": "진짜요 ㅜㅜ 그래서 가끔 언니랑도 싸워요 ㅋㅋㅋ",
          "original_form": "진짜요 ㅜㅜ 그래서 가끔 언니랑도 싸워요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100020989.1.2",
          "form": "하 저두 동생이랑..",
          "original_form": "하 저두 동생이랑..",
          "speaker_id": "2",
          "time": "20210828 14:43"
        },
        {
          "id": "MDRW2100020989.1.3",
          "form": "저도 바로치우는편이거든요",
          "original_form": "저도 바로치우는편이거든요",
          "speaker_id": "2",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020989.1.4",
          "form": "동생은 그냥 먹고 그대로놔두고 몇번말해도 안들어요 에휴",
          "original_form": "동생은 그냥 먹고 그대로놔두고 몇번말해도 안들어요 에휴",
          "speaker_id": "2",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020989.1.5",
          "form": "아 젤 싫어요 먹고 그대로 두는겈ㅋㅋㅋ",
          "original_form": "아 젤 싫어요 먹고 그대로 두는겈ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020989.1.6",
          "form": "전 그걸 엄마가 치워주는게 싫어요ㅜㅜ",
          "original_form": "전 그걸 엄마가 치워주는게 싫어요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210828 14:44"
        },
        {
          "id": "MDRW2100020989.1.7",
          "form": "아 저두요",
          "original_form": "아 저두요",
          "speaker_id": "2",
          "time": "20210828 14:47"
        },
        {
          "id": "MDRW2100020989.1.8",
          "form": "휴 자꾸 엄마가치워주니까 더그러능거같구",
          "original_form": "휴 자꾸 엄마가치워주니까 더그러능거같구",
          "speaker_id": "2",
          "time": "20210828 14:47"
        },
        {
          "id": "MDRW2100020989.1.9",
          "form": "맞아요 .. 제 친구도 오빠 있는데",
          "original_form": "맞아요 .. 제 친구도 오빠 있는데",
          "speaker_id": "1",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100020989.1.10",
          "form": "마마보이가 괜히 있는게 아니라면서 ㅋㅋ",
          "original_form": "마마보이가 괜히 있는게 아니라면서 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 14:48"
        },
        {
          "id": "MDRW2100020989.1.11",
          "form": "그러니까요..정신좀차렷으면 ㅠㅠ",
          "original_form": "그러니까요..정신좀차렷으면 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100020989.1.12",
          "form": "그럼 집안일 많이하능편이세요?",
          "original_form": "그럼 집안일 많이하능편이세요?",
          "speaker_id": "2",
          "time": "20210828 14:50"
        },
        {
          "id": "MDRW2100020989.1.13",
          "form": "전 막 많이는 아니고",
          "original_form": "전 막 많이는 아니고",
          "speaker_id": "1",
          "time": "20210828 14:51"
        },
        {
          "id": "MDRW2100020989.1.14",
          "form": "빨래 개는거랑 설거지해요 !",
          "original_form": "빨래 개는거랑 설거지해요 !",
          "speaker_id": "1",
          "time": "20210828 14:51"
        },
        {
          "id": "MDRW2100020989.1.15",
          "form": "오 그렇군요! 저도가끔 설거지나 청소기돌려요 ㅋㅋㅋㅋ",
          "original_form": "오 그렇군요! 저도가끔 설거지나 청소기돌려요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:36"
        },
        {
          "id": "MDRW2100020989.1.16",
          "form": "ㅋㅋㅋ 맞아요 저도 매일 하는건 아니에요",
          "original_form": "ㅋㅋㅋ 맞아요 저도 매일 하는건 아니에요",
          "speaker_id": "1",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100020989.1.17",
          "form": "언니는 안해서 ,, 저라도 해요 ㅋㅋ",
          "original_form": "언니는 안해서 ,, 저라도 해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 19:37"
        },
        {
          "id": "MDRW2100020989.1.18",
          "form": "아이고 고생많으시네여 ㅠㅠ",
          "original_form": "아이고 고생많으시네여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100020989.1.19",
          "form": "언니분이 많이바쁘신가봐요",
          "original_form": "언니분이 많이바쁘신가봐요",
          "speaker_id": "2",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100020989.1.20",
          "form": "아무래도 직장인이니까 ㅠㅠ",
          "original_form": "아무래도 직장인이니까 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100020989.1.21",
          "form": "야근을 많이 하더라구요",
          "original_form": "야근을 많이 하더라구요",
          "speaker_id": "1",
          "time": "20210828 19:44"
        },
        {
          "id": "MDRW2100020989.1.22",
          "form": "아 직장인이시구나 ㅋㅋㅋㅋ그럼 어쩔수없네요..",
          "original_form": "아 직장인이시구나 ㅋㅋㅋㅋ그럼 어쩔수없네요..",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100020989.1.23",
          "form": "야근하고 집오면 진짜 피곤하니가요 ㅜㅠ",
          "original_form": "야근하고 집오면 진짜 피곤하니가요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210828 19:46"
        },
        {
          "id": "MDRW2100020989.1.24",
          "form": "맞아요 ㅜㅠㅜ",
          "original_form": "맞아요 ㅜㅠㅜ",
          "speaker_id": "1",
