      "utterance": [
        {
          "id": "MDRW2100024945.1.1",
          "form": "없음 죽을거 같진 않은데 가끔 생각나요",
          "original_form": "없음 죽을거 같진 않은데 가끔 생각나요",
          "speaker_id": "2",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024945.1.2",
          "form": "어느날 거리서 낙엽을 밟다가 문득,,",
          "original_form": "어느날 거리서 낙엽을 밟다가 문득,,",
          "speaker_id": "2",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024945.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024945.1.4",
          "form": "죽으면 안돼요ㅠㅠㅠㅠㅠ",
          "original_form": "죽으면 안돼요ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024945.1.5",
          "form": "그러고 보니 뻥이요를 두달전에 먹었지",
          "original_form": "그러고 보니 뻥이요를 두달전에 먹었지",
          "speaker_id": "2",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024945.1.6",
          "form": "앗",
          "original_form": "앗",
          "speaker_id": "1",
          "time": "20210831 21:15"
        },
        {
          "id": "MDRW2100024945.1.7",
          "form": "진심이신 거 치고 너무 오랜 텀인데요",
          "original_form": "진심이신 거 치고 너무 오랜 텀인데요",
          "speaker_id": "1",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024945.1.8",
          "form": "세상인 너무 많은 과자가 있는걸요",
          "original_form": "세상인 너무 많은 과자가 있는걸요",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024945.1.9",
          "form": "뻥이요가 이해해야해요",
          "original_form": "뻥이요가 이해해야해요",
          "speaker_id": "2",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024945.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210831 21:16"
        },
        {
          "id": "MDRW2100024945.1.11",
          "form": "저는 과자 자주 먹진 않는데",
          "original_form": "저는 과자 자주 먹진 않는데",
          "speaker_id": "1",
          "time": "20210831 21:17"
        },
        {
          "id": "MDRW2100024945.1.12",
          "form": "한 개에 꽂히면 죽어라 먹는 편이에요",
          "original_form": "한 개에 꽂히면 죽어라 먹는 편이에요",
          "speaker_id": "1",
          "time": "20210831 21:17"
        },
        {
          "id": "MDRW2100024945.1.13",
          "form": "아하 그렇게 드시다가 질리시나요?",
          "original_form": "아하 그렇게 드시다가 질리시나요?",
          "speaker_id": "2",
          "time": "20210831 21:17"
        },
        {
          "id": "MDRW2100024945.1.14",
          "form": "전 좀 오래 롱디 하는 느낌으로 먹어요 ㅋㅋ",
          "original_form": "전 좀 오래 롱디 하는 느낌으로 먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 21:17"
        },
        {
          "id": "MDRW2100024945.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:17"
        },
        {
          "id": "MDRW2100024945.1.16",
          "form": "네 전 잘 질려서.",
          "original_form": "네 전 잘 질려서.",
          "speaker_id": "1",
          "time": "20210831 21:17"
        },
        {
          "id": "MDRW2100024945.1.17",
          "form": "그럼 롱디로 여러다리를 걸치신 게 아닌가요ㅠㅋㅋ큐ㅠㅠㅠ",
          "original_form": "그럼 롱디로 여러다리를 걸치신 게 아닌가요ㅠㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024945.1.18",
          "form": "사귀는 식으로 치자면  한,, 서울  - 부산 거리서 한달에 한번 보며 10년 사귀는",
          "original_form": "사귀는 식으로 치자면  한,, 서울  - 부산 거리서 한달에 한번 보며 10년 사귀는",
          "speaker_id": "2",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024945.1.19",
          "form": "과자가 이해해야죠,, 제가 엔시티 루카스처럼 과자에게 돈을 뜯으낸 것두 아니고",
          "original_form": "과자가 이해해야죠,, 제가 엔시티 루카스처럼 과자에게 돈을 뜯으낸 것두 아니고",
          "speaker_id": "2",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024945.1.20",
          "form": "맞아요 억울하면",
          "original_form": "맞아요 억울하면",
          "speaker_id": "1",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024945.1.21",
          "form": "고소하든가요 과자들 다 맛있고 고소하니까",
          "original_form": "고소하든가요 과자들 다 맛있고 고소하니까",
          "speaker_id": "1",
          "time": "20210831 21:18"
        },
        {
          "id": "MDRW2100024945.1.22",
          "form": "쇼미더 머니에 나가셔야겠는데요!",
          "original_form": "쇼미더 머니에 나가셔야겠는데요!",
          "speaker_id": "2",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100024945.1.23",
          "form": "과자들아 나를 고소해 \r\n너네는 고소 하니까,,,!",
          "original_form": "과자들아 나를 고소해 \r\n너네는 고소 하니까,,,!",
          "speaker_id": "2",
          "time": "20210831 21:20"
        },
        {
          "id": "MDRW2100024945.1.24",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100024945.1.25",
          "form": "쇠고랑 찰랑",
          "original_form": "쇠고랑 찰랑",
          "speaker_id": "1",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100024945.1.26",
          "form": "다른 음식은 뭐 좋아하세요?",
          "original_form": "다른 음식은 뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210831 21:21"
        },
        {
          "id": "MDRW2100024945.1.27",
          "form": "과자는 인권이 없으니 괜찮습니다",
          "original_form": "과자는 인권이 없으니 괜찮습니다",
