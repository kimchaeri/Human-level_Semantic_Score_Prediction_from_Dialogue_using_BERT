      "utterance": [
        {
          "id": "MDRW2100033562.1.1",
          "form": "쇼핑 자주하시나요",
          "original_form": "쇼핑 자주하시나요",
          "speaker_id": "1",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100033562.1.2",
          "form": "요즘 귀차나서 옷은 안사고 주로 먹을거사요ㅎ",
          "original_form": "요즘 귀차나서 옷은 안사고 주로 먹을거사요ㅎ",
          "speaker_id": "2",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100033562.1.3",
          "form": "저도 식료품 위주로 사네요~",
          "original_form": "저도 식료품 위주로 사네요~",
          "speaker_id": "1",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100033562.1.4",
          "form": "최근에 밀키트를 샀는데 편했어요",
          "original_form": "최근에 밀키트를 샀는데 편했어요",
          "speaker_id": "1",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100033562.1.5",
          "form": "요즘 밀키트잘나오는듯해요",
          "original_form": "요즘 밀키트잘나오는듯해요",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100033562.1.6",
          "form": "맞아요 식재료 따로 살필요도 없고 손질도 안해도되니 좋아요",
          "original_form": "맞아요 식재료 따로 살필요도 없고 손질도 안해도되니 좋아요",
          "speaker_id": "1",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100033562.1.7",
          "form": "전 저번에 신림백순대사봤는데",
          "original_form": "전 저번에 신림백순대사봤는데",
          "speaker_id": "2",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100033562.1.8",
          "form": "맛있더라구요",
          "original_form": "맛있더라구요",
          "speaker_id": "2",
          "time": "20210824 14:22"
        },
        {
          "id": "MDRW2100033562.1.9",
          "form": "저도 사봐야겠네요",
          "original_form": "저도 사봐야겠네요",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100033562.1.10",
          "form": "저는 타코랑 해물누룽지탕 사봤는데",
          "original_form": "저는 타코랑 해물누룽지탕 사봤는데",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100033562.1.11",
          "form": "만족스러웠어요",
          "original_form": "만족스러웠어요",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100033562.1.12",
          "form": "진짜 신림서 먹는 맛이예요",
          "original_form": "진짜 신림서 먹는 맛이예요",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100033562.1.13",
          "form": "오 그것도 맛있겠어요",
          "original_form": "오 그것도 맛있겠어요",
          "speaker_id": "2",
          "time": "20210824 14:24"
        },
        {
          "id": "MDRW2100033562.1.14",
          "form": "쇼핑몰 어디이용하세요?",
          "original_form": "쇼핑몰 어디이용하세요?",
          "speaker_id": "1",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100033562.1.15",
          "form": "지에스프레시몰이나 정원샵이요",
          "original_form": "지에스프레시몰이나 정원샵이요",
          "speaker_id": "2",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100033562.1.16",
          "form": "신규이벤트있음 그것도 이용해요",
          "original_form": "신규이벤트있음 그것도 이용해요",
          "speaker_id": "2",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100033562.1.17",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100033562.1.18",
          "form": "저는 네이버쇼핑 자주이용해요",
          "original_form": "저는 네이버쇼핑 자주이용해요",
          "speaker_id": "1",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100033562.1.19",
          "form": "라방보고 구매하세요?",
          "original_form": "라방보고 구매하세요?",
          "speaker_id": "2",
          "time": "20210824 14:26"
        },
        {
          "id": "MDRW2100033562.1.20",
          "form": "아니요 검색해서 구매해요",
          "original_form": "아니요 검색해서 구매해요",
          "speaker_id": "1",
          "time": "20210824 14:27"
        }
      ]
