      "utterance": [
        {
          "id": "MDRW2100023189.1.1",
          "form": "응 이번편은 재밌었어",
          "original_form": "응 이번편은 재밌었어",
          "speaker_id": "2",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100023189.1.2",
          "form": "게스트 누구 나왔어?",
          "original_form": "게스트 누구 나왔어?",
          "speaker_id": "1",
          "time": "20210829 22:41"
        },
        {
          "id": "MDRW2100023189.1.3",
          "form": "게스트 안나왔어",
          "original_form": "게스트 안나왔어",
          "speaker_id": "2",
          "time": "20210829 22:42"
        },
        {
          "id": "MDRW2100023189.1.4",
          "form": "아 무슨 설정이었는데?",
          "original_form": "아 무슨 설정이었는데?",
          "speaker_id": "1",
          "time": "20210829 22:42"
        },
        {
          "id": "MDRW2100023189.1.5",
          "form": "멤버들 중 귀신이 한명 있어",
          "original_form": "멤버들 중 귀신이 한명 있어",
          "speaker_id": "2",
          "time": "20210829 22:43"
        },
        {
          "id": "MDRW2100023189.1.6",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ재밌네 귀신 찾는거야?",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ재밌네 귀신 찾는거야?",
          "speaker_id": "1",
          "time": "20210829 22:43"
        },
        {
          "id": "MDRW2100023189.1.7",
          "form": "응",
          "original_form": "응",
          "speaker_id": "2",
          "time": "20210829 22:43"
        },
        {
          "id": "MDRW2100023189.1.8",
          "form": "요즘 1박2일 재밌어?",
          "original_form": "요즘 1박2일 재밌어?",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023189.1.9",
          "form": "귀신 누구였는데?",
          "original_form": "귀신 누구였는데?",
          "speaker_id": "1",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023189.1.10",
          "form": "송지효",
          "original_form": "송지효",
          "speaker_id": "2",
          "time": "20210829 22:46"
        },
        {
          "id": "MDRW2100023189.1.11",
          "form": "1박2일 오늘 거는 재미있던데?",
          "original_form": "1박2일 오늘 거는 재미있던데?",
          "speaker_id": "1",
          "time": "20210829 22:48"
        },
        {
          "id": "MDRW2100023189.1.12",
          "form": "재밌긴 하더라",
          "original_form": "재밌긴 하더라",
          "speaker_id": "2",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023189.1.13",
          "form": "ㅋㅋㅋㅋㅋ나도 바다가고 싶었어",
          "original_form": "ㅋㅋㅋㅋㅋ나도 바다가고 싶었어",
          "speaker_id": "1",
          "time": "20210829 22:49"
        },
        {
          "id": "MDRW2100023189.1.14",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "2",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023189.1.15",
          "form": "바다 구경한지 너무 오래됐어",
          "original_form": "바다 구경한지 너무 오래됐어",
          "speaker_id": "1",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023189.1.16",
          "form": "한 번 가야 되는데",
          "original_form": "한 번 가야 되는데",
          "speaker_id": "2",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023189.1.17",
          "form": "동해 가고싶다",
          "original_form": "동해 가고싶다",
          "speaker_id": "1",
          "time": "20210829 22:50"
        },
        {
          "id": "MDRW2100023189.1.18",
          "form": "지금 티비에 뭐해?",
          "original_form": "지금 티비에 뭐해?",
          "speaker_id": "1",
          "time": "20210829 22:57"
        }
      ]
