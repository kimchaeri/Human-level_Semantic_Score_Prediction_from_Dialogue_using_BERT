      "utterance": [
        {
          "id": "MDRW2100006455.1.1",
          "form": "여행 많이 다니시나요",
          "original_form": "여행 많이 다니시나요",
          "speaker_id": "1",
          "time": "20210908 00:27"
        },
        {
          "id": "MDRW2100006455.1.2",
          "form": "시간이 안되서 자주 못가요",
          "original_form": "시간이 안되서 자주 못가요",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100006455.1.3",
          "form": "여행 자주 가시나요",
          "original_form": "여행 자주 가시나요",
          "speaker_id": "2",
          "time": "20210908 00:28"
        },
        {
          "id": "MDRW2100006455.1.4",
          "form": "아이들이 있어서 가까운곳으로 잠깐씩 다녀요",
          "original_form": "아이들이 있어서 가까운곳으로 잠깐씩 다녀요",
          "speaker_id": "1",
          "time": "20210908 00:29"
        },
        {
          "id": "MDRW2100006455.1.5",
          "form": "아이들과에 추억도 좋죠",
          "original_form": "아이들과에 추억도 좋죠",
          "speaker_id": "2",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100006455.1.6",
          "form": "네.캠핑다니는데 아이들이.좋아해요",
          "original_form": "네.캠핑다니는데 아이들이.좋아해요",
          "speaker_id": "1",
          "time": "20210908 00:35"
        },
        {
          "id": "MDRW2100006455.1.7",
          "form": "아이들과 다양한 여행 , 경험 해보는것 좋아요",
          "original_form": "아이들과 다양한 여행 , 경험 해보는것 좋아요",
          "speaker_id": "2",
          "time": "20210908 00:38"
        },
        {
          "id": "MDRW2100006455.1.8",
          "form": "name2님도 많이 다니셨었나요",
          "original_form": "&name2&님도 많이 다니셨었나요",
          "speaker_id": "1",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100006455.1.9",
          "form": "지금 힘들어도 아이들과 여행 많이 하세요",
          "original_form": "지금 힘들어도 아이들과 여행 많이 하세요",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100006455.1.10",
          "form": "못하면 나중에 후회되요",
          "original_form": "못하면 나중에 후회되요",
          "speaker_id": "2",
          "time": "20210908 00:39"
        },
        {
          "id": "MDRW2100006455.1.11",
          "form": "네 새겨듣겠습니다",
          "original_form": "네 새겨듣겠습니다",
          "speaker_id": "1",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100006455.1.12",
          "form": "애들 어릴때 가까운 곳이라도 많이 가려고 노력했죠",
          "original_form": "애들 어릴때 가까운 곳이라도 많이 가려고 노력했죠",
          "speaker_id": "2",
          "time": "20210908 00:40"
        },
        {
          "id": "MDRW2100006455.1.13",
          "form": "아이들과 유명한 찜질방 가는것도 추억이되요",
          "original_form": "아이들과 유명한 찜질방 가는것도 추억이되요",
          "speaker_id": "2",
          "time": "20210908 00:42"
        },
        {
          "id": "MDRW2100006455.1.14",
          "form": "그렇네요 찜찔방도 여행지로.ㅡㅎㅎ",
          "original_form": "그렇네요 찜찔방도 여행지로.ㅡㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 00:45"
        },
        {
          "id": "MDRW2100006455.1.15",
          "form": "하늘물빛정원 이란 찜질방 참좋아요",
          "original_form": "하늘물빛정원 이란 찜질방 참좋아요",
          "speaker_id": "2",
          "time": "20210908 00:49"
        },
        {
          "id": "MDRW2100006455.1.16",
          "form": "글램핑도 있고 삼겹살도 있고 라이브노래도 있고 \r\n산책로도 있고 다양해요",
          "original_form": "글램핑도 있고 삼겹살도 있고 라이브노래도 있고 \r\n산책로도 있고 다양해요",
          "speaker_id": "2",
          "time": "20210908 00:50"
        },
        {
          "id": "MDRW2100006455.1.17",
          "form": "아 지역이 어딘가요?",
          "original_form": "아 지역이 어딘가요?",
          "speaker_id": "1",
          "time": "20210908 00:52"
        },
        {
          "id": "MDRW2100006455.1.18",
          "form": "대전 근교 금산 가는곳 이예요",
          "original_form": "대전 근교 금산 가는곳 이예요",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100006455.1.19",
          "form": "전국에서 다오죠",
          "original_form": "전국에서 다오죠",
          "speaker_id": "2",
          "time": "20210908 00:53"
        },
        {
          "id": "MDRW2100006455.1.20",
          "form": "검색하면 나와요",
          "original_form": "검색하면 나와요",
