      "utterance": [
        {
          "id": "MDRW2100039653.1.1",
          "form": "화장실청소요~^^",
          "original_form": "화장실청소요~^^",
          "speaker_id": "2",
          "time": "20210903 20:13"
        },
        {
          "id": "MDRW2100039653.1.2",
          "form": "쉽지 않은 일이죠",
          "original_form": "쉽지 않은 일이죠",
          "speaker_id": "1",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039653.1.3",
          "form": "곰팡이 제거가 어렵죠?",
          "original_form": "곰팡이 제거가 어렵죠?",
          "speaker_id": "1",
          "time": "20210903 20:23"
        },
        {
          "id": "MDRW2100039653.1.4",
          "form": "너~~무 어려워요",
          "original_form": "너~~무 어려워요",
          "speaker_id": "2",
          "time": "20210903 21:00"
        },
        {
          "id": "MDRW2100039653.1.5",
          "form": "날씨때문에 자주 습하니 더 심하네요",
          "original_form": "날씨때문에 자주 습하니 더 심하네요",
          "speaker_id": "2",
          "time": "20210903 21:02"
        },
        {
          "id": "MDRW2100039653.1.6",
          "form": "곰팡이 제거 제품을 여러개 사봤는데..",
          "original_form": "곰팡이 제거 제품을 여러개 사봤는데..",
          "speaker_id": "1",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039653.1.7",
          "form": "맘에 쏙 드는 걸 아직 못찾았어요",
          "original_form": "맘에 쏙 드는 걸 아직 못찾았어요",
          "speaker_id": "1",
          "time": "20210903 21:11"
        },
        {
          "id": "MDRW2100039653.1.8",
          "form": "저두요~",
          "original_form": "저두요~",
          "speaker_id": "2",
          "time": "20210903 21:13"
        },
        {
          "id": "MDRW2100039653.1.9",
          "form": "바르는거,뿌리는거..종류별로..",
          "original_form": "바르는거,뿌리는거..종류별로..",
          "speaker_id": "2",
          "time": "20210903 21:14"
        },
        {
          "id": "MDRW2100039653.1.10",
          "form": "저도 갖가지 브랜드로 해봤는데.. 아직 정착한게 없네요",
          "original_form": "저도 갖가지 브랜드로 해봤는데.. 아직 정착한게 없네요",
          "speaker_id": "1",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039653.1.11",
          "form": "곰팡이 말고도 항상 습하니까 자주 환기해주고 물기 닦아주고",
          "original_form": "곰팡이 말고도 항상 습하니까 자주 환기해주고 물기 닦아주고",
          "speaker_id": "1",
          "time": "20210903 21:29"
        },
        {
          "id": "MDRW2100039653.1.12",
          "form": "거울 얼룩 지우고.. 쉽지 않죠",
          "original_form": "거울 얼룩 지우고.. 쉽지 않죠",
          "speaker_id": "1",
          "time": "20210903 21:30"
        },
        {
          "id": "MDRW2100039653.1.13",
          "form": "힘들게 해놔도 하루밖에 안 가니 더 허무하네요..",
          "original_form": "힘들게 해놔도 하루밖에 안 가니 더 허무하네요..",
          "speaker_id": "2",
          "time": "20210903 21:32"
        },
        {
          "id": "MDRW2100039653.1.14",
          "form": "그다음으로는, 비우는 삶이 잘 안 되요",
          "original_form": "그다음으로는, 비우는 삶이 잘 안 되요",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039653.1.15",
          "form": "뭘 잘 못 버려요",
          "original_form": "뭘 잘 못 버려요",
          "speaker_id": "2",
          "time": "20210903 21:33"
        },
        {
          "id": "MDRW2100039653.1.16",
          "form": "물건도.. 미련도..",
          "original_form": "물건도.. 미련도..",
          "speaker_id": "2",
          "time": "20210903 21:34"
        },
        {
          "id": "MDRW2100039653.1.17",
          "form": "저도 물건 잘 못버리는 사람이라서",
          "original_form": "저도 물건 잘 못버리는 사람이라서",
          "speaker_id": "1",
          "time": "20210904 05:13"
        },
        {
          "id": "MDRW2100039653.1.18",
          "form": "왠만한건 가지고 있는데",
          "original_form": "왠만한건 가지고 있는데",
          "speaker_id": "1",
          "time": "20210904 05:13"
        },
        {
          "id": "MDRW2100039653.1.19",
          "form": "그러다보니 매번 정돈할 것들이 많아서",
          "original_form": "그러다보니 매번 정돈할 것들이 많아서",
          "speaker_id": "1",
          "time": "20210904 05:13"
        },
        {
          "id": "MDRW2100039653.1.20",
          "form": "요새는 좀 정리해서 버리려고 하고 있어요",
          "original_form": "요새는 좀 정리해서 버리려고 하고 있어요",
          "speaker_id": "1",
          "time": "20210904 05:14"
        },
        {
          "id": "MDRW2100039653.1.21",
          "form": "진짜 일찍 일어나시네요~^^",
          "original_form": "진짜 일찍 일어나시네요~^^",
          "speaker_id": "2",
          "time": "20210904 10:21"
        },
        {
          "id": "MDRW2100039653.1.22",
          "form": "저도 요즘 하루에 하나씩 버리기하고 있어요",
          "original_form": "저도 요즘 하루에 하나씩 버리기하고 있어요",
          "speaker_id": "2",
          "time": "20210904 10:22"
        },
        {
          "id": "MDRW2100039653.1.23",
          "form": "팔고 나누고 버리고..",
          "original_form": "팔고 나누고 버리고..",
          "speaker_id": "2",
          "time": "20210904 10:22"
        },
        {
          "id": "MDRW2100039653.1.24",
          "form": "오늘 평소보다 1시간 일찍 일어났어요..ㅎㅎ",
          "original_form": "오늘 평소보다 1시간 일찍 일어났어요..ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 11:15"
        },
        {
          "id": "MDRW2100039653.1.25",
          "form": "집에 짐이 많으면 청소도 힘들고 잘 치워도 지저분해 보이더라구요",
          "original_form": "집에 짐이 많으면 청소도 힘들고 잘 치워도 지저분해 보이더라구요",
          "speaker_id": "1",
          "time": "20210904 11:16"
        },
        {
          "id": "MDRW2100039653.1.26",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "2",
          "time": "20210904 11:20"
        },
        {
          "id": "MDRW2100039653.1.27",
          "form": "배치를 바꾸는것도 한계가 있어서 짐이 많으니 소용없더라구요.",
          "original_form": "배치를 바꾸는것도 한계가 있어서 짐이 많으니 소용없더라구요.",
          "speaker_id": "2",
          "time": "20210904 11:21"
        },
        {
          "id": "MDRW2100039653.1.28",
          "form": "그니까요.. 물건들 유지관리도 힘들고.. 버리는게 답이에요",
          "original_form": "그니까요.. 물건들 유지관리도 힘들고.. 버리는게 답이에요",
          "speaker_id": "1",
          "time": "20210904 14:05"
        },
        {
          "id": "MDRW2100039653.1.29",
          "form": "전문가들이 한결같이 첫번째로 말하길 '버리고 사지마라~'",
          "original_form": "전문가들이 한결같이 첫번째로 말하길 '버리고 사지마라~'",
          "speaker_id": "2",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100039653.1.30",
          "form": "'하나를 사고 싶으면 하나를 버려라'",
          "original_form": "'하나를 사고 싶으면 하나를 버려라'",
          "speaker_id": "2",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100039653.1.31",
          "form": "근데,잘 안되요~^^",
          "original_form": "근데,잘 안되요~^^",
          "speaker_id": "2",
          "time": "20210904 14:08"
        },
        {
          "id": "MDRW2100039653.1.32",
          "form": "묵은 짐 정리를 위해 켜켜이 쌓아놓은 것들 또 막 헤집어야 하잖아요",
          "original_form": "묵은 짐 정리를 위해 켜켜이 쌓아놓은 것들 또 막 헤집어야 하잖아요",
          "speaker_id": "1",
          "time": "20210904 20:57"
        }
      ]
