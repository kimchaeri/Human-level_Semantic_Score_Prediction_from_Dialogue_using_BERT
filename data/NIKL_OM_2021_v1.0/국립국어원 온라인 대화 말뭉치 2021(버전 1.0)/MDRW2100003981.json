      "utterance": [
        {
          "id": "MDRW2100003981.1.1",
          "form": "혹시 다이어트해보신 적 있으세요?",
          "original_form": "혹시 다이어트해보신 적 있으세요?",
          "speaker_id": "1",
          "time": "20210821 15:09"
        },
        {
          "id": "MDRW2100003981.1.2",
          "form": "한 번해봤어요",
          "original_form": "한 번해봤어요",
          "speaker_id": "2",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100003981.1.3",
          "form": "어렵더라구요",
          "original_form": "어렵더라구요",
          "speaker_id": "2",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100003981.1.4",
          "form": "그쵸 식이요법이랑 운동 같이 하는 게 너무 힘들어요",
          "original_form": "그쵸 식이요법이랑 운동 같이 하는 게 너무 힘들어요",
          "speaker_id": "1",
          "time": "20210821 15:10"
        },
        {
          "id": "MDRW2100003981.1.5",
          "form": "운동도 힘든데 운동만 하면 다이어트가 안 된다니 불합리해요",
          "original_form": "운동도 힘든데 운동만 하면 다이어트가 안 된다니 불합리해요",
          "speaker_id": "1",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100003981.1.6",
          "form": "그리고 세상에 맛있는게 너무 많잖아요",
          "original_form": "그리고 세상에 맛있는게 너무 많잖아요",
          "speaker_id": "2",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100003981.1.7",
          "form": "살이 찐다는 것 너무 슬퍼요",
          "original_form": "살이 찐다는 것 너무 슬퍼요",
          "speaker_id": "2",
          "time": "20210821 15:11"
        },
        {
          "id": "MDRW2100003981.1.8",
          "form": "맞아요 왜 맛있을수록 몸에 나쁠까요?",
          "original_form": "맞아요 왜 맛있을수록 몸에 나쁠까요?",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100003981.1.9",
          "form": "다이어트 하려면 밀가루는 꼭 끊어야한다는게...ㅠ",
          "original_form": "다이어트 하려면 밀가루는 꼭 끊어야한다는게...ㅠ",
          "speaker_id": "1",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100003981.1.10",
          "form": "아 진짜 그걸 어떻게 끊어요 그쵸",
          "original_form": "아 진짜 그걸 어떻게 끊어요 그쵸",
          "speaker_id": "2",
          "time": "20210821 15:13"
        },
        {
          "id": "MDRW2100003981.1.11",
          "form": "면 빵 이런거",
          "original_form": "면 빵 이런거",
          "speaker_id": "2",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100003981.1.12",
          "form": "맞아요... 다이어트하다가 성질 버려요",
          "original_form": "맞아요... 다이어트하다가 성질 버려요",
          "speaker_id": "1",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100003981.1.13",
          "form": "너무 맛있는데 먹으면 살이 찐다는게 너무 억울해요",
          "original_form": "너무 맛있는데 먹으면 살이 찐다는게 너무 억울해요",
          "speaker_id": "2",
          "time": "20210821 15:14"
        },
        {
          "id": "MDRW2100003981.1.14",
          "form": "근데 그런 다이어트가 과하지만 않으면 몸이 좋아지는 게 보여서 가끔 짜증나요 ㅋㅋ ㅠㅠ",
          "original_form": "근데 그런 다이어트가 과하지만 않으면 몸이 좋아지는 게 보여서 가끔 짜증나요 ㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 15:15"
        },
        {
          "id": "MDRW2100003981.1.15",
          "form": "밀가루만 끊어도 피부가 좋아지고...",
          "original_form": "밀가루만 끊어도 피부가 좋아지고...",
          "speaker_id": "1",
          "time": "20210821 15:15"
        },
        {
          "id": "MDRW2100003981.1.16",
          "form": "아 맞아요...",
          "original_form": "아 맞아요...",
          "speaker_id": "2",
          "time": "20210821 15:15"
        }
      ]
