      "utterance": [
        {
          "id": "MDRW2100019794.1.1",
          "form": "아 그럼 자연산이랑은 맛이 많이 다를까요?",
          "original_form": "아 그럼 자연산이랑은 맛이 많이 다를까요?",
          "speaker_id": "2",
          "time": "20210827 22:01"
        },
        {
          "id": "MDRW2100019794.1.2",
          "form": "자연산 먹어봤는데 기분탓인지 맛있긴 했어요",
          "original_form": "자연산 먹어봤는데 기분탓인지 맛있긴 했어요",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019794.1.3",
          "form": "근데 전에 허영만 식객 보니",
          "original_form": "근데 전에 허영만 식객 보니",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019794.1.4",
          "form": "횟집 사장님들 여럿 모아 양식과 자연산 테스트를 했는데",
          "original_form": "횟집 사장님들 여럿 모아 양식과 자연산 테스트를 했는데",
          "speaker_id": "1",
          "time": "20210827 22:02"
        },
        {
          "id": "MDRW2100019794.1.5",
          "form": "반이 못 맞췄대요ㅎ",
          "original_form": "반이 못 맞췄대요ㅎ",
          "speaker_id": "1",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019794.1.6",
          "form": "사실상 반반 확률인데",
          "original_form": "사실상 반반 확률인데",
          "speaker_id": "1",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019794.1.7",
          "form": "생각하면 양식이 더 기름져서 취향따라 나뉠 수도요ㅎ",
          "original_form": "생각하면 양식이 더 기름져서 취향따라 나뉠 수도요ㅎ",
          "speaker_id": "1",
          "time": "20210827 22:03"
        },
        {
          "id": "MDRW2100019794.1.8",
          "form": "아 진짜 취향에 따라 다르겠네요ㅋㅋ 반이 못 맞췄다니 그럼 그냥 둘 다 맛있는걸로..",
          "original_form": "아 진짜 취향에 따라 다르겠네요ㅋㅋ 반이 못 맞췄다니 그럼 그냥 둘 다 맛있는걸로..",
          "speaker_id": "2",
          "time": "20210827 22:05"
        },
        {
          "id": "MDRW2100019794.1.9",
          "form": "산낙지는 좋아하세요?",
          "original_form": "산낙지는 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 22:06"
        },
        {
          "id": "MDRW2100019794.1.10",
          "form": "없어서 못 먹죠ㅎㅎ",
          "original_form": "없어서 못 먹죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100019794.1.11",
          "form": "개불도 좋아해요",
          "original_form": "개불도 좋아해요",
          "speaker_id": "1",
          "time": "20210827 22:19"
        },
        {
          "id": "MDRW2100019794.1.12",
          "form": "name2님도 산낙지 드시나요?",
          "original_form": "&name2&님도 산낙지 드시나요?",
          "speaker_id": "1",
          "time": "20210827 22:20"
        },
        {
          "id": "MDRW2100019794.1.13",
          "form": "저 산낙지 진짜 좋아해요 근데 개불은 먹어본적이 없네요",
          "original_form": "저 산낙지 진짜 좋아해요 근데 개불은 먹어본적이 없네요",
          "speaker_id": "2",
          "time": "20210828 09:33"
        },
        {
          "id": "MDRW2100019794.1.14",
          "form": "개불은 식감이랑 맛이 어때요?",
          "original_form": "개불은 식감이랑 맛이 어때요?",
          "speaker_id": "2",
          "time": "20210828 09:33"
        },
        {
          "id": "MDRW2100019794.1.15",
          "form": "잘게 채썰어 먹는데 꼬들꼬들한 식감이에요",
          "original_form": "잘게 채썰어 먹는데 꼬들꼬들한 식감이에요",
          "speaker_id": "1",
          "time": "20210828 09:35"
        },
        {
          "id": "MDRW2100019794.1.16",
          "form": "오독오독하고 해산물 특유의 은은한 단맛이 있고요",
          "original_form": "오독오독하고 해산물 특유의 은은한 단맛이 있고요",
          "speaker_id": "1",
          "time": "20210828 09:37"
        },
        {
          "id": "MDRW2100019794.1.17",
          "form": "향은 별로 없어요. 비린 맛도 전혀",
          "original_form": "향은 별로 없어요. 비린 맛도 전혀",
          "speaker_id": "1",
          "time": "20210828 09:37"
        },
        {
          "id": "MDRW2100019794.1.18",
          "form": "전 개불인거 모르고 먹어서 정체 알고 놀랐죠ㅎ",
          "original_form": "전 개불인거 모르고 먹어서 정체 알고 놀랐죠ㅎ",
          "speaker_id": "1",
          "time": "20210828 09:38"
        },
        {
          "id": "MDRW2100019794.1.19",
          "form": "좀 비싼게 단점인데ㅜ 해산물 좋아하시면 강추해요!",
          "original_form": "좀 비싼게 단점인데ㅜ 해산물 좋아하시면 강추해요!",
          "speaker_id": "1",
          "time": "20210828 09:38"
        },
        {
          "id": "MDRW2100019794.1.20",
          "form": "비린맛이 없고 꼬들꼬들하다니 나중에 꼭 먹어봐야겠네요 전 물컹거릴줄 알았어요",
          "original_form": "비린맛이 없고 꼬들꼬들하다니 나중에 꼭 먹어봐야겠네요 전 물컹거릴줄 알았어요",
          "speaker_id": "2",
          "time": "20210828 12:43"
        },
        {
          "id": "MDRW2100019794.1.21",
          "form": "혹시 초밥도 좋아하세요?",
          "original_form": "혹시 초밥도 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 12:44"
        },
        {
          "id": "MDRW2100019794.1.22",
          "form": "없어서 못 먹죠ㅎ",
          "original_form": "없어서 못 먹죠ㅎ",
          "speaker_id": "1",
          "time": "20210828 12:53"
        },
        {
          "id": "MDRW2100019794.1.23",
          "form": "해산물은 다 좋아해요",
          "original_form": "해산물은 다 좋아해요",
          "speaker_id": "1",
          "time": "20210828 12:53"
        },
        {
          "id": "MDRW2100019794.1.24",
          "form": "name2님은 무슨 초밥 좋아하세요",
          "original_form": "&name2&님은 무슨 초밥 좋아하세요",
          "speaker_id": "1",
          "time": "20210828 12:53"
        },
        {
          "id": "MDRW2100019794.1.25",
          "form": "저도 초밥은 진짜 다 좋아해요",
          "original_form": "저도 초밥은 진짜 다 좋아해요",
          "speaker_id": "2",
          "time": "20210828 14:22"
        },
        {
          "id": "MDRW2100019794.1.26",
          "form": "근데 그중에서도 소라초밥... 쫄깃쫄깃한게 너무 좋아요",
          "original_form": "근데 그중에서도 소라초밥... 쫄깃쫄깃한게 너무 좋아요",
          "speaker_id": "2",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100019794.1.27",
          "form": "ㅎㅎ 소라 숙회로도 너무 맛있어요",
          "original_form": "ㅎㅎ 소라 숙회로도 너무 맛있어요",
          "speaker_id": "1",
          "time": "20210828 14:23"
        },
        {
          "id": "MDRW2100019794.1.28",
          "form": "때론 전복보다 더 생각나네요",
          "original_form": "때론 전복보다 더 생각나네요",
          "speaker_id": "1",
          "time": "20210828 14:24"
        },
        {
          "id": "MDRW2100019794.1.29",
          "form": "아 맞아요 숙회도 너무 좋죠 다슬기도 좋고요..",
          "original_form": "아 맞아요 숙회도 너무 좋죠 다슬기도 좋고요..",
          "speaker_id": "2",
          "time": "20210828 14:26"
        },
        {
          "id": "MDRW2100019794.1.30",
          "form": "다슬기는 어떤 식으로 주로 드시나요?",
          "original_form": "다슬기는 어떤 식으로 주로 드시나요?",
          "speaker_id": "1",
          "time": "20210828 14:27"
        },
        {
          "id": "MDRW2100019794.1.31",
          "form": "근데 너무 어렸을적이라 맛은 잘 기억이 안나요 맛있었다는것만 기억나고ㅋ기",
          "original_form": "근데 너무 어렸을적이라 맛은 잘 기억이 안나요 맛있었다는것만 기억나고ㅋ기",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100019794.1.32",
          "form": "ㅎㅎ 다슬기국이었나...? 분명 맛있었는데",
          "original_form": "ㅎㅎ 다슬기국이었나...? 분명 맛있었는데",
          "speaker_id": "1",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100019794.1.33",
          "form": "가물가물하네요",
          "original_form": "가물가물하네요",
          "speaker_id": "1",
          "time": "20210828 14:30"
        }
      ]
    }
  ]
