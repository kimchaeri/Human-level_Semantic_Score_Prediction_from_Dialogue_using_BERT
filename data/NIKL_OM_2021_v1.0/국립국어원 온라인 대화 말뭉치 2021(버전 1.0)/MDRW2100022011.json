        {
          "id": "MDRW2100022011.1.1",
          "form": "오늘 안 나갔다왔지?",
          "original_form": "오늘 안 나갔다왔지?",
          "speaker_id": "2",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100022011.1.2",
          "form": "응 내일 나가려고",
          "original_form": "응 내일 나가려고",
          "speaker_id": "1",
          "time": "20210829 21:16"
        },
        {
          "id": "MDRW2100022011.1.3",
          "form": "나갈 때 반팔 반바지 입어?",
          "original_form": "나갈 때 반팔 반바지 입어?",
          "speaker_id": "2",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100022011.1.4",
          "form": "아니 슬랙스 입어",
          "original_form": "아니 슬랙스 입어",
          "speaker_id": "1",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100022011.1.5",
          "form": "안 더워?",
          "original_form": "안 더워?",
          "speaker_id": "2",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100022011.1.6",
          "form": "응 괜찮아",
          "original_form": "응 괜찮아",
          "speaker_id": "1",
          "time": "20210829 21:17"
        },
        {
          "id": "MDRW2100022011.1.7",
          "form": "춥지도 않고?",
          "original_form": "춥지도 않고?",
          "speaker_id": "2",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100022011.1.8",
          "form": "응 별로 안추워",
          "original_form": "응 별로 안추워",
          "speaker_id": "1",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100022011.1.9",
          "form": "이제 태풍도 더이상 안오겠지?",
          "original_form": "이제 태풍도 더이상 안오겠지?",
          "speaker_id": "2",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100022011.1.10",
          "form": "응 아직 그런 얘기 안나오던데?",
          "original_form": "응 아직 그런 얘기 안나오던데?",
          "speaker_id": "1",
          "time": "20210829 21:18"
        },
        {
          "id": "MDRW2100022011.1.11",
          "form": "요즘 일기예보 못봤어",
          "original_form": "요즘 일기예보 못봤어",
          "speaker_id": "2",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100022011.1.12",
          "form": "나도 ㅎㅎ",
          "original_form": "나도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 21:19"
        },
        {
          "id": "MDRW2100022011.1.13",
          "form": "ㅋㅋㅋ뉴스 자주 보지않아?",
          "original_form": "ㅋㅋㅋ뉴스 자주 보지않아?",
