      "utterance": [
        {
          "id": "MDRW2100003697.1.1",
          "form": "지금 치과세요?",
          "original_form": "지금 치과세요?",
          "speaker_id": "1",
          "time": "20210821 11:07"
        },
        {
          "id": "MDRW2100003697.1.2",
          "form": "아뇨 아직도 가고 있어요",
          "original_form": "아뇨 아직도 가고 있어요",
          "speaker_id": "2",
          "time": "20210821 11:10"
        },
        {
          "id": "MDRW2100003697.1.3",
          "form": "저 지방사는데",
          "original_form": "저 지방사는데",
          "speaker_id": "2",
          "time": "20210821 11:10"
        },
        {
          "id": "MDRW2100003697.1.4",
          "form": "치과가 서울이예요",
          "original_form": "치과가 서울이예요",
          "speaker_id": "2",
          "time": "20210821 11:10"
        },
        {
          "id": "MDRW2100003697.1.5",
          "form": "시간안에 가야하는데",
          "original_form": "시간안에 가야하는데",
          "speaker_id": "2",
          "time": "20210821 11:10"
        },
        {
          "id": "MDRW2100003697.1.6",
          "form": "헙!!왜이리 먼곳으로ㅠ",
          "original_form": "헙!!왜이리 먼곳으로ㅠ",
          "speaker_id": "1",
          "time": "20210821 11:10"
        },
        {
          "id": "MDRW2100003697.1.7",
          "form": "거기가 잘해요",
          "original_form": "거기가 잘해요",
          "speaker_id": "2",
          "time": "20210821 11:11"
        },
        {
          "id": "MDRW2100003697.1.8",
          "form": "제가 치아가 좀 특이해요",
          "original_form": "제가 치아가 좀 특이해요",
          "speaker_id": "2",
          "time": "20210821 11:11"
        },
        {
          "id": "MDRW2100003697.1.9",
          "form": "딸도 일반 병원에서 볼수가 없어요",
          "original_form": "딸도 일반 병원에서 볼수가 없어요",
          "speaker_id": "2",
          "time": "20210821 11:11"
        },
        {
          "id": "MDRW2100003697.1.10",
          "form": "치아가 석회질이 없어서 녹아요",
          "original_form": "치아가 석회질이 없어서 녹아요",
          "speaker_id": "2",
          "time": "20210821 11:11"
        },
        {
          "id": "MDRW2100003697.1.11",
          "form": "헙 치아 건강이 정말 안 좋으신가봐요",
          "original_form": "헙 치아 건강이 정말 안 좋으신가봐요",
          "speaker_id": "1",
          "time": "20210821 11:12"
        },
        {
          "id": "MDRW2100003697.1.12",
          "form": "저보다도",
          "original_form": "저보다도",
          "speaker_id": "1",
          "time": "20210821 11:12"
        },
        {
          "id": "MDRW2100003697.1.13",
          "form": "네 휴 잇몸은 항시 엉망이예요",
          "original_form": "네 휴 잇몸은 항시 엉망이예요",
          "speaker_id": "2",
          "time": "20210821 11:12"
        },
        {
          "id": "MDRW2100003697.1.14",
          "form": "어렷을때 부타요?",
          "original_form": "어렷을때 부타요?",
          "speaker_id": "1",
          "time": "20210821 11:13"
        },
        {
          "id": "MDRW2100003697.1.15",
          "form": "저희딸은 태어나서부터 유전적으로문제고",
          "original_form": "저희딸은 태어나서부터 유전적으로문제고",
          "speaker_id": "2",
          "time": "20210821 11:13"
        },
        {
          "id": "MDRW2100003697.1.16",
          "form": "전 나이들어서요",
          "original_form": "전 나이들어서요",
          "speaker_id": "2",
          "time": "20210821 11:14"
        },
        {
          "id": "MDRW2100003697.1.17",
          "form": "아~~에구ㅠㅠ 치과비용도 만만치 않죠",
          "original_form": "아~~에구ㅠㅠ 치과비용도 만만치 않죠",
          "speaker_id": "1",
          "time": "20210821 11:15"
        },
        {
          "id": "MDRW2100003697.1.18",
          "form": "네 저희딸 치아전부 심어야한대요",
          "original_form": "네 저희딸 치아전부 심어야한대요",
          "speaker_id": "2",
          "time": "20210821 11:16"
        },
        {
          "id": "MDRW2100003697.1.19",
          "form": "일단 금니부터 하고요",
          "original_form": "일단 금니부터 하고요",
          "speaker_id": "2",
          "time": "20210821 11:16"
        },
        {
          "id": "MDRW2100003697.1.20",
          "form": "저도 문제지만 어린게",
          "original_form": "저도 문제지만 어린게",
          "speaker_id": "2",
          "time": "20210821 11:16"
        },
        {
          "id": "MDRW2100003697.1.21",
          "form": "헛 전부요 ㅠㅠ",
          "original_form": "헛 전부요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 11:16"
        },
        {
          "id": "MDRW2100003697.1.22",
          "form": "제가 태아 였을때 뭘 잘못했나봐요",
          "original_form": "제가 태아 였을때 뭘 잘못했나봐요",
          "speaker_id": "2",
          "time": "20210821 11:16"
        },
        {
          "id": "MDRW2100003697.1.23",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 11:16"
        },
        {
          "id": "MDRW2100003697.1.24",
          "form": "전부 녹아요",
          "original_form": "전부 녹아요",
          "speaker_id": "2",
          "time": "20210821 11:16"
        },
        {
          "id": "MDRW2100003697.1.25",
          "form": "아이구ㅠㅠ 따님은 몇살이에요?",
          "original_form": "아이구ㅠㅠ 따님은 몇살이에요?",
          "speaker_id": "1",
          "time": "20210821 11:17"
        },
        {
          "id": "MDRW2100003697.1.26",
          "form": "견적이 왠만한 원룸 보증금",
          "original_form": "견적이 왠만한 원룸 보증금",
          "speaker_id": "2",
          "time": "20210821 11:17"
        },
        {
          "id": "MDRW2100003697.1.27",
          "form": "13 이요",
          "original_form": "13 이요",
