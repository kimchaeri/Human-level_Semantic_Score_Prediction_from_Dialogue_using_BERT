      "utterance": [
        {
          "id": "MDRW2100014297.1.1",
          "form": "싸이월드 사용하신 세대이신가요?",
          "original_form": "싸이월드 사용하신 세대이신가요?",
          "speaker_id": "1",
          "time": "20210825 18:15"
        },
        {
          "id": "MDRW2100014297.1.2",
          "form": "음... 걸쳐지긴 했어요",
          "original_form": "음... 걸쳐지긴 했어요",
          "speaker_id": "2",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014297.1.3",
          "form": "끝무렵?",
          "original_form": "끝무렵?",
          "speaker_id": "2",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014297.1.4",
          "form": "나이대가 예상되네요",
          "original_form": "나이대가 예상되네요",
          "speaker_id": "1",
          "time": "20210825 18:16"
        },
        {
          "id": "MDRW2100014297.1.5",
          "form": "배경음악은 혹시",
          "original_form": "배경음악은 혹시",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014297.1.6",
          "form": "어떤걸 하셨나요",
          "original_form": "어떤걸 하셨나요",
          "speaker_id": "1",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014297.1.7",
          "form": "타임리스...",
          "original_form": "타임리스...",
          "speaker_id": "2",
          "time": "20210825 18:17"
        },
        {
          "id": "MDRW2100014297.1.8",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014297.1.9",
          "form": "명곡이죠",
          "original_form": "명곡이죠",
          "speaker_id": "1",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014297.1.10",
          "form": "저는 멋낸다고 팝송을 했던 것 같아요",
          "original_form": "저는 멋낸다고 팝송을 했던 것 같아요",
          "speaker_id": "1",
          "time": "20210825 18:18"
        },
        {
          "id": "MDRW2100014297.1.11",
          "form": "팝송도 많이들 했죠",
          "original_form": "팝송도 많이들 했죠",
          "speaker_id": "2",
          "time": "20210825 18:21"
        },
        {
          "id": "MDRW2100014297.1.12",
          "form": "웨스트라이프 노래도 봤던것같고",
          "original_form": "웨스트라이프 노래도 봤던것같고",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014297.1.13",
          "form": "댄싱인더 문라이트였나",
          "original_form": "댄싱인더 문라이트였나",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014297.1.14",
          "form": "추억이 새록새록",
          "original_form": "추억이 새록새록",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014297.1.15",
          "form": "혹시 흑역사 저장되어 있나요?",
          "original_form": "혹시 흑역사 저장되어 있나요?",
          "speaker_id": "1",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014297.1.16",
          "form": "아 저는 다행히",
          "original_form": "아 저는 다행히",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014297.1.17",
          "form": "눈팅족이었어서",
          "original_form": "눈팅족이었어서",
          "speaker_id": "2",
          "time": "20210825 18:22"
        },
        {
          "id": "MDRW2100014297.1.18",
          "form": "오",
          "original_form": "오",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014297.1.19",
          "form": "그건 다행이네요",
          "original_form": "그건 다행이네요",
          "speaker_id": "1",
          "time": "20210825 18:24"
        },
        {
          "id": "MDRW2100014297.1.20",
          "form": "흑역사 있으신가요 혹시",
          "original_form": "흑역사 있으신가요 혹시",
          "speaker_id": "2",
          "time": "20210825 18:38"
        },
        {
          "id": "MDRW2100014297.1.21",
          "form": "슬픈척 찍었던 사진이 불현듯 떠오르네요",
          "original_form": "슬픈척 찍었던 사진이 불현듯 떠오르네요",
          "speaker_id": "1",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014297.1.22",
          "form": "ㅋㅋㅋㄲ",
          "original_form": "ㅋㅋㅋㄲ",
          "speaker_id": "2",
          "time": "20210825 18:39"
        },
        {
          "id": "MDRW2100014297.1.23",
          "form": "또르르...",
          "original_form": "또르르...{emoji:☆}",
          "speaker_id": "2",
          "time": "20210825 18:40"
        },
        {
          "id": "MDRW2100014297.1.24",
          "form": "추억의 노래인데",
          "original_form": "추억의 노래인데",
          "speaker_id": "1",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014297.1.25",
          "form": "지연의 또르르 기억하시나요",
          "original_form": "지연의 또르르 기억하시나요",
          "speaker_id": "1",
          "time": "20210825 18:41"
        },
        {
          "id": "MDRW2100014297.1.26",
          "form": "그건 첨들어보네요ㅋㅋㅋ",
          "original_form": "그건 첨들어보네요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:42"
        },
        {
          "id": "MDRW2100014297.1.27",
          "form": "나이가 들어나네요",
          "original_form": "나이가 들어나네요",
          "speaker_id": "1",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014297.1.28",
          "form": "제가 아무래도 위인 것 같아요",
          "original_form": "제가 아무래도 위인 것 같아요",
          "speaker_id": "1",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014297.1.29",
          "form": "그럴것같아요ㅋㅋ",
          "original_form": "그럴것같아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014297.1.30",
          "form": "싸이월드 사실 전 추억이 별로 없어서ㅋㅋㅋ",
          "original_form": "싸이월드 사실 전 추억이 별로 없어서ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 18:43"
        },
        {
          "id": "MDRW2100014297.1.31",
          "form": "주제 선정부터 나이가 나왔네요",
          "original_form": "주제 선정부터 나이가 나왔네요",
          "speaker_id": "1",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014297.1.32",
          "form": "누님 형님들이 많이들 하시더라구요",
          "original_form": "누님 형님들이 많이들 하시더라구요",
          "speaker_id": "2",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014297.1.33",
          "form": "하하하하",
          "original_form": "하하하하",
          "speaker_id": "1",
          "time": "20210825 18:44"
        },
        {
          "id": "MDRW2100014297.1.34",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "1",
          "time": "20210825 18:44"
        }
      ]
    }
