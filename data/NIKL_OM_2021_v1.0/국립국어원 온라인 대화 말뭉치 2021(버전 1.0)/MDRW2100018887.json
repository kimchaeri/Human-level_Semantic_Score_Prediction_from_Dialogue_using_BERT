      "utterance": [
        {
          "id": "MDRW2100018887.1.1",
          "form": "그럼 요새는 고기위주로 드세용?",
          "original_form": "그럼 요새는 고기위주로 드세용?",
          "speaker_id": "1",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100018887.1.2",
          "form": "아무래도요",
          "original_form": "아무래도요",
          "speaker_id": "2",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100018887.1.3",
          "form": "고기 어떤거 좋아하세요",
          "original_form": "고기 어떤거 좋아하세요",
          "speaker_id": "2",
          "time": "20210827 13:17"
        },
        {
          "id": "MDRW2100018887.1.4",
          "form": "저는 소 닭 돼지 다좋아해요 ㅠㅠ",
          "original_form": "저는 소 닭 돼지 다좋아해요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 13:18"
        },
        {
          "id": "MDRW2100018887.1.5",
          "form": "그럼 치킨 좋아하시겠다",
          "original_form": "그럼 치킨 좋아하시겠다",
          "speaker_id": "2",
          "time": "20210827 13:18"
        },
        {
          "id": "MDRW2100018887.1.6",
          "form": "후라이드 좋아하세요 양념류 발라진거 좋아하세요",
          "original_form": "후라이드 좋아하세요 양념류 발라진거 좋아하세요",
          "speaker_id": "2",
          "time": "20210827 13:18"
        },
        {
          "id": "MDRW2100018887.1.7",
          "form": "저는 1순위 후라이드",
          "original_form": "저는 1순위 후라이드",
          "speaker_id": "1",
          "time": "20210827 13:19"
        },
        {
          "id": "MDRW2100018887.1.8",
          "form": "2순위 양념이용",
          "original_form": "2순위 양념이용",
          "speaker_id": "1",
          "time": "20210827 13:19"
        },
        {
          "id": "MDRW2100018887.1.9",
          "form": "님은 ?",
          "original_form": "님은 ?",
          "speaker_id": "1",
          "time": "20210827 13:19"
        },
        {
          "id": "MDRW2100018887.1.10",
          "form": "저두요",
          "original_form": "저두요",
          "speaker_id": "2",
          "time": "20210827 13:19"
        },
        {
          "id": "MDRW2100018887.1.11",
          "form": "기본적인 맛이 좋더라구요",
          "original_form": "기본적인 맛이 좋더라구요",
          "speaker_id": "2",
          "time": "20210827 13:19"
        },
        {
          "id": "MDRW2100018887.1.12",
          "form": "그쵸 좋아하는 브랜드있나요",
          "original_form": "그쵸 좋아하는 브랜드있나요",
          "speaker_id": "1",
          "time": "20210827 13:19"
        },
        {
          "id": "MDRW2100018887.1.13",
          "form": "그래서 치킨 새로운거 시키면 후라이드 꼭 먹어봐요",
          "original_form": "그래서 치킨 새로운거 시키면 후라이드 꼭 먹어봐요",
          "speaker_id": "2",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018887.1.14",
          "form": "브랜드 다 좋아하긴하는데",
          "original_form": "브랜드 다 좋아하긴하는데",
          "speaker_id": "2",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018887.1.15",
          "form": "요즘 자담치킨 프라이드가 맛있더라구요",
          "original_form": "요즘 자담치킨 프라이드가 맛있더라구요",
          "speaker_id": "2",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018887.1.16",
          "form": "어느치킨 좋아하시나요",
          "original_form": "어느치킨 좋아하시나요",
          "speaker_id": "2",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018887.1.17",
          "form": "오.. 저랑 통하시는데요 ..",
          "original_form": "오.. 저랑 통하시는데요 ..",
          "speaker_id": "1",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018887.1.18",
          "form": "저 자담좋아해용 ㅋㅋ",
          "original_form": "저 자담좋아해용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018887.1.19",
          "form": "사실 딱히 가리는건아닌데 자담 맛나더라구요",
          "original_form": "사실 딱히 가리는건아닌데 자담 맛나더라구요",
          "speaker_id": "1",
          "time": "20210827 13:20"
        },
        {
          "id": "MDRW2100018887.1.20",
          "form": "그쵸 자담이 맛있어요ㅋㅋㅋ",
          "original_form": "그쵸 자담이 맛있어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018887.1.21",
          "form": "후라이드 말고 다른거 시킬 땐 뭐 시키세요?",
          "original_form": "후라이드 말고 다른거 시킬 땐 뭐 시키세요?",
          "speaker_id": "2",
          "time": "20210827 13:21"
        },
        {
          "id": "MDRW2100018887.1.22",
          "form": "처가집 슈프림치킨이용!",
          "original_form": "처가집 슈프림치킨이용!",
          "speaker_id": "1",
          "time": "20210827 13:22"
        },
        {
          "id": "MDRW2100018887.1.23",
          "form": "그것  맛있더라구요",
          "original_form": "그것  맛있더라구요",
          "speaker_id": "2",
          "time": "20210827 13:23"
        },
        {
          "id": "MDRW2100018887.1.24",
          "form": "그 흰색 소스 그게 진짜 찐",
          "original_form": "그 흰색 소스 그게 진짜 찐",
          "speaker_id": "2",
          "time": "20210827 13:23"
        },
        {
          "id": "MDRW2100018887.1.25",
          "form": "그쵸 ㅠㅠ 다른양념치킨보다 맛난거같아용",
          "original_form": "그쵸 ㅠㅠ 다른양념치킨보다 맛난거같아용",
          "speaker_id": "1",
          "time": "20210827 13:23"
        }
      ]
    }
