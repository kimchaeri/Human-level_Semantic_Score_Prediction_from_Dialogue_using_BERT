        },
        {
          "id": "MDRW2100025533.1.2",
          "form": "치아바타 좋아하시나요?",
          "original_form": "치아바타 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 12:54"
        },
        {
          "id": "MDRW2100025533.1.3",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210901 12:54"
        },
        {
          "id": "MDRW2100025533.1.4",
          "form": "네완전좋아해요",
          "original_form": "네완전좋아해요",
          "speaker_id": "2",
          "time": "20210901 12:54"
        },
        {
          "id": "MDRW2100025533.1.5",
          "form": "쫄깃해서 좋아용",
          "original_form": "쫄깃해서 좋아용",
          "speaker_id": "2",
          "time": "20210901 12:54"
        },
        {
          "id": "MDRW2100025533.1.6",
          "form": "치아바타 맛있죠!!",
          "original_form": "치아바타 맛있죠!!",
          "speaker_id": "1",
          "time": "20210901 12:58"
        },
        {
          "id": "MDRW2100025533.1.7",
          "form": "맛있는 곳 알고계시나요?",
          "original_form": "맛있는 곳 알고계시나요?",
          "speaker_id": "1",
          "time": "20210901 12:58"
        },
        {
          "id": "MDRW2100025533.1.8",
          "form": "앗 저는 가리지않고 잘먹어서 ㅋㄱㅋㅋㅋ",
          "original_form": "앗 저는 가리지않고 잘먹어서 ㅋㄱㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 12:59"
        },
        {
          "id": "MDRW2100025533.1.9",
          "form": "동네빵집에서 잘시켜먹어요!",
          "original_form": "동네빵집에서 잘시켜먹어요!",
          "speaker_id": "2",
          "time": "20210901 12:59"
        },
        {
          "id": "MDRW2100025533.1.10",
          "form": "아 그렇군요 ㅎㅎ 쫄깃한 식감이 맛있죠 ㅎㅎ",
          "original_form": "아 그렇군요 ㅎㅎ 쫄깃한 식감이 맛있죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 13:33"
        },
        {
          "id": "MDRW2100025533.1.11",
          "form": "네넹 저는 단걸 별로 안좋아해서",
          "original_form": "네넹 저는 단걸 별로 안좋아해서",
          "speaker_id": "2",
          "time": "20210901 13:33"
        },
        {
          "id": "MDRW2100025533.1.12",
          "form": "제가 예전에 가던 빵집에 올리브치아바타가 맛있어서 닉네임 보니까 생각났어요 ㅎㅎ",
          "original_form": "제가 예전에 가던 빵집에 올리브치아바타가 맛있어서 닉네임 보니까 생각났어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 13:34"
        },
        {
          "id": "MDRW2100025533.1.13",
          "form": "짭짤한빵이 좋은거 같아요",
          "original_form": "짭짤한빵이 좋은거 같아요",
          "speaker_id": "2",
          "time": "20210901 13:34"
        },
        {
          "id": "MDRW2100025533.1.14",
          "form": "오 정말요",
          "original_form": "오 정말요",
          "speaker_id": "2",
          "time": "20210901 13:34"
        },
        {
          "id": "MDRW2100025533.1.15",
          "form": "치아바타가 맛있으면...그집빵들은 다맛있더라구요",
          "original_form": "치아바타가 맛있으면...그집빵들은 다맛있더라구요",
          "speaker_id": "2",
          "time": "20210901 13:34"
        },
        {
          "id": "MDRW2100025533.1.16",
          "form": "그쵸 맞아요 다 맛있었는데 가게가 없어졌더라고요 ㅠㅠ",
          "original_form": "그쵸 맞아요 다 맛있었는데 가게가 없어졌더라고요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 13:35"
        },
        {
          "id": "MDRW2100025533.1.17",
          "form": "앗 아쉽네요 ㅠㅠ",
          "original_form": "앗 아쉽네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 13:37"
        },
        {
          "id": "MDRW2100025533.1.18",
          "form": "맛있는데 흔치않은데",
          "original_form": "맛있는데 흔치않은데",
          "speaker_id": "2",
          "time": "20210901 13:37"
        },
        {
          "id": "MDRW2100025533.1.19",
          "form": "그쵸.. 소신있는 맛있는 빵집이 잘 없는거 같아요..",
          "original_form": "그쵸.. 소신있는 맛있는 빵집이 잘 없는거 같아요..",
          "speaker_id": "1",
          "time": "20210901 13:37"
        },
        {
          "id": "MDRW2100025533.1.20",
          "form": "혹시 바게트도 좋아하시나요?",
          "original_form": "혹시 바게트도 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210901 13:38"
        },
        {
          "id": "MDRW2100025533.1.21",
          "form": "오 네....진짜좋아해요",
          "original_form": "오 네....진짜좋아해요",
          "speaker_id": "2",
          "time": "20210901 13:42"
        },
        {
          "id": "MDRW2100025533.1.22",
          "form": "저는 뭐 안들어있는 쫄깃한빵을 제일 좋아하거든요 ㅋㅋㅋ",
          "original_form": "저는 뭐 안들어있는 쫄깃한빵을 제일 좋아하거든요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 13:42"
        },
        {
          "id": "MDRW2100025533.1.23",
          "form": "바게트도 플레인 잘하는데가 많지않아서 아쉬워요",
          "original_form": "바게트도 플레인 잘하는데가 많지않아서 아쉬워요",
          "speaker_id": "2",
          "time": "20210901 13:43"
        },
        {
          "id": "MDRW2100025533.1.24",
          "form": "그쵸 왠지 좋아하실거 같았어요 ㅋㅋㅋ",
          "original_form": "그쵸 왠지 좋아하실거 같았어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100025533.1.25",
          "form": "바게트에 버터랑 딸기잼 얇게 발라먹어도 맛있어요 ㅎㅎ",
          "original_form": "바게트에 버터랑 딸기잼 얇게 발라먹어도 맛있어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 13:44"
        },
        {
          "id": "MDRW2100025533.1.26",
          "form": "오 저버터스프레드 발라먹는거 정말 좋아해요",
          "original_form": "오 저버터스프레드 발라먹는거 정말 좋아해요",
          "speaker_id": "2",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100025533.1.27",
          "form": "홍종흔베이커리가 혹시 주변에있으신가요?",
          "original_form": "홍종흔베이커리가 혹시 주변에있으신가요?",
          "speaker_id": "2",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100025533.1.28",
          "form": "거기 나중에보시면 꼭 바게트 드셔보세요...",
          "original_form": "거기 나중에보시면 꼭 바게트 드셔보세요...",
          "speaker_id": "2",
          "time": "20210901 13:46"
        },
        {
          "id": "MDRW2100025533.1.29",
          "form": "정말맛있어요",
          "original_form": "정말맛있어요",
          "speaker_id": "2",
          "time": "20210901 13:46"
        }
      ]
    }
