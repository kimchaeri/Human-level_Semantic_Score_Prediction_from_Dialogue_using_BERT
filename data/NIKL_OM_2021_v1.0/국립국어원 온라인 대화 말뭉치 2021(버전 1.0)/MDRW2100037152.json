      "utterance": [
        {
          "id": "MDRW2100037152.1.1",
          "form": "들깨 메밀국수 유명한 고기리 막국수 들어 보셨나요?",
          "original_form": "들깨 메밀국수 유명한 고기리 막국수 들어 보셨나요?",
          "speaker_id": "2",
          "time": "20210831 21:09"
        },
        {
          "id": "MDRW2100037152.1.2",
          "form": "아니욥.,!",
          "original_form": "아니욥.,!",
          "speaker_id": "1",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100037152.1.3",
          "form": "맛집도 알고계시는군요!",
          "original_form": "맛집도 알고계시는군요!",
          "speaker_id": "1",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100037152.1.4",
          "form": "가보셨나용?",
          "original_form": "가보셨나용?",
          "speaker_id": "1",
          "time": "20210831 21:28"
        },
        {
          "id": "MDRW2100037152.1.5",
          "form": "거기 엄청 유명한 맛집이라고 인터넷에서 유명하더라고요",
          "original_form": "거기 엄청 유명한 맛집이라고 인터넷에서 유명하더라고요",
          "speaker_id": "2",
          "time": "20210831 21:29"
        },
        {
          "id": "MDRW2100037152.1.6",
          "form": "오옹..",
          "original_form": "오옹..",
          "speaker_id": "1",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100037152.1.7",
          "form": "궁금하네용",
          "original_form": "궁금하네용",
          "speaker_id": "1",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100037152.1.8",
          "form": "용인이군욥!",
          "original_form": "용인이군욥!",
          "speaker_id": "1",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100037152.1.9",
          "form": "네 엄청 기다린다 하더라구요 오뚜기에서 제품도나오구",
          "original_form": "네 엄청 기다린다 하더라구요 오뚜기에서 제품도나오구",
          "speaker_id": "2",
          "time": "20210831 21:41"
        },
        {
          "id": "MDRW2100037152.1.10",
          "form": "우와.. 제품도 출시할정도면 진짜 맛집인가보네요",
          "original_form": "우와.. 제품도 출시할정도면 진짜 맛집인가보네요",
          "speaker_id": "1",
          "time": "20210831 21:45"
        },
        {
          "id": "MDRW2100037152.1.11",
          "form": "그런가봐요 거기사장님 책도냇어요 브랜딩을 잘하나봐요",
          "original_form": "그런가봐요 거기사장님 책도냇어요 브랜딩을 잘하나봐요",
          "speaker_id": "2",
          "time": "20210831 21:47"
        },
        {
          "id": "MDRW2100037152.1.12",
          "form": "오우ㅋㅋ 대단하시네요",
          "original_form": "오우ㅋㅋ 대단하시네요",
          "speaker_id": "1",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037152.1.13",
          "form": "마케팅의 끝판왕이신가봐요",
          "original_form": "마케팅의 끝판왕이신가봐요",
          "speaker_id": "1",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037152.1.14",
          "form": "맛집가시면 기다리시는타입이세요?",
          "original_form": "맛집가시면 기다리시는타입이세요?",
          "speaker_id": "1",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037152.1.15",
          "form": "아뇨 저는 기다리는거싫어해서요",
          "original_form": "아뇨 저는 기다리는거싫어해서요",
          "speaker_id": "2",
          "time": "20210831 21:52"
        },
        {
          "id": "MDRW2100037152.1.16",
          "form": "아!  저랑 같으시네욤",
          "original_form": "아!  저랑 같으시네욤",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100037152.1.17",
          "form": "굳이 기다려서까지 먹어야하나싶어요 배고픈데요...",
          "original_form": "굳이 기다려서까지 먹어야하나싶어요 배고픈데요...",
          "speaker_id": "2",
          "time": "20210831 22:19"
        },
        {
          "id": "MDRW2100037152.1.18",
          "form": "맞아요..ㅋㅋ 예약하는거 아닌이상은 그렇게 되더라구요",
          "original_form": "맞아요..ㅋㅋ 예약하는거 아닌이상은 그렇게 되더라구요",
          "speaker_id": "1",
          "time": "20210831 22:22"
        },
        {
          "id": "MDRW2100037152.1.19",
          "form": "네 줄서기싫어요 ㅎㅎㅎ",
          "original_form": "네 줄서기싫어요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 22:23"
        }
      ]
