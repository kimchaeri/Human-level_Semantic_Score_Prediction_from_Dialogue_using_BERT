      "utterance": [
        {
          "id": "MDRW2100005558.1.1",
          "form": "무슨 동물 제일 좋아해?",
          "original_form": "무슨 동물 제일 좋아해?",
          "speaker_id": "1",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005558.1.2",
          "form": "음 딱히 가리지는 않는거 같아",
          "original_form": "음 딱히 가리지는 않는거 같아",
          "speaker_id": "2",
          "time": "20210821 23:01"
        },
        {
          "id": "MDRW2100005558.1.3",
          "form": "강아지가 근처에 많아서 강아지?",
          "original_form": "강아지가 근처에 많아서 강아지?",
          "speaker_id": "2",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005558.1.4",
          "form": "강아지가 근처에 많다고?",
          "original_form": "강아지가 근처에 많다고?",
          "speaker_id": "1",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005558.1.5",
          "form": "아니면 작은 고양이..? 크면 무서워..",
          "original_form": "아니면 작은 고양이..? 크면 무서워..",
          "speaker_id": "2",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005558.1.6",
          "form": "주변에 공원이 있어?",
          "original_form": "주변에 공원이 있어?",
          "speaker_id": "1",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005558.1.7",
          "form": "아 친구들이나ㅋㅋㅋㅋㅋ",
          "original_form": "아 친구들이나ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005558.1.8",
          "form": "같이 산책시키고 이럴 때도 많거든ㅋㅋㅋㅋ",
          "original_form": "같이 산책시키고 이럴 때도 많거든ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005558.1.9",
          "form": "앜ㅋㅋㅋㅋ 그렇구나 너는 키워?",
          "original_form": "앜ㅋㅋㅋㅋ 그렇구나 너는 키워?",
          "speaker_id": "1",
          "time": "20210821 23:03"
        },
        {
          "id": "MDRW2100005558.1.10",
          "form": "아니... 난 키울 자신은 없어ㅜㅜ",
          "original_form": "아니... 난 키울 자신은 없어ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005558.1.11",
          "form": "막 동물을 너무 사랑해서 집에 혼자 둘 수 없어서 못키우는 애들도 있던데",
          "original_form": "막 동물을 너무 사랑해서 집에 혼자 둘 수 없어서 못키우는 애들도 있던데",
          "speaker_id": "2",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005558.1.12",
          "form": "난 그냥 스스로 안될거라 짐작ㅋㅋㅋ 넌 어때?",
          "original_form": "난 그냥 스스로 안될거라 짐작ㅋㅋㅋ 넌 어때?",
          "speaker_id": "2",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005558.1.13",
          "form": "앜ㅋㅋㅋ 나는 동생때문에 키우는데",
          "original_form": "앜ㅋㅋㅋ 나는 동생때문에 키우는데",
          "speaker_id": "1",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005558.1.14",
          "form": "너무 귀엽더라고",
          "original_form": "너무 귀엽더라고",
          "speaker_id": "1",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005558.1.15",
          "form": "페키니즈 키워!",
          "original_form": "페키니즈 키워!",
          "speaker_id": "1",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005558.1.16",
          "form": "오오? 강아지 종류야?",
          "original_form": "오오? 강아지 종류야?",
          "speaker_id": "2",
          "time": "20210821 23:06"
        },
        {
          "id": "MDRW2100005558.1.17",
          "form": "맞아 강아지야! 흰색 털이라서 귀여워 ㅎㅎㅎ",
          "original_form": "맞아 강아지야! 흰색 털이라서 귀여워 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005558.1.18",
          "form": "우왕ㅋㅋㅋ동물 좋아하는구나ㅋㅋㅋㅋ언제부터 키웠어",
          "original_form": "우왕ㅋㅋㅋ동물 좋아하는구나ㅋㅋㅋㅋ언제부터 키웠어",
          "speaker_id": "2",
          "time": "20210821 23:11"
        },
        {
          "id": "MDRW2100005558.1.19",
          "form": "나 고2때부터 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "나 고2때부터 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005558.1.20",
          "form": "근데 자취시작하고 잘 못가서 너무 아쉬워",
          "original_form": "근데 자취시작하고 잘 못가서 너무 아쉬워",
          "speaker_id": "1",
          "time": "20210821 23:13"
        }
      ]
