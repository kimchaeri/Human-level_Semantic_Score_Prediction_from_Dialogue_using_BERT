      "utterance": [
        {
          "id": "MDRW2100022199.1.1",
          "form": "최근에 구매한 물건 있어요?",
          "original_form": "최근에 구매한 물건 있어요?",
          "speaker_id": "1",
          "time": "20210829 10:55"
        },
        {
          "id": "MDRW2100022199.1.2",
          "form": "친구생일선물샀어요 향수",
          "original_form": "친구생일선물샀어요 향수",
          "speaker_id": "2",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100022199.1.3",
          "form": "name1님은 뭐사셨나요",
          "original_form": "&name1&님은 뭐사셨나요",
          "speaker_id": "2",
          "time": "20210829 11:13"
        },
        {
          "id": "MDRW2100022199.1.4",
          "form": "아까 버팔로윙이랑 버팔로봉 샀어요 ㅋㅋㅋ",
          "original_form": "아까 버팔로윙이랑 버팔로봉 샀어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100022199.1.5",
          "form": "이거 은근 맛있거든요",
          "original_form": "이거 은근 맛있거든요",
          "speaker_id": "1",
          "time": "20210829 11:15"
        },
        {
          "id": "MDRW2100022199.1.6",
          "form": "그래요?",
          "original_form": "그래요?",
          "speaker_id": "2",
          "time": "20210829 11:18"
        },
        {
          "id": "MDRW2100022199.1.7",
          "form": "한번도안먹어봤는데",
          "original_form": "한번도안먹어봤는데",
          "speaker_id": "2",
          "time": "20210829 11:18"
        },
        {
          "id": "MDRW2100022199.1.8",
          "form": "이거 맥주안주로 딱이에요 ㅋㅋㅋ",
          "original_form": "이거 맥주안주로 딱이에요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100022199.1.9",
          "form": "가게에서도 다 이런거 갖다 팔더라구요",
          "original_form": "가게에서도 다 이런거 갖다 팔더라구요",
          "speaker_id": "1",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100022199.1.10",
          "form": "그럼 저도 모르게 먹었을수있겠군요ㅋㅋ",
          "original_form": "그럼 저도 모르게 먹었을수있겠군요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100022199.1.11",
          "form": "에어프라이어 필요한가요?",
          "original_form": "에어프라이어 필요한가요?",
          "speaker_id": "2",
          "time": "20210829 11:22"
        },
        {
          "id": "MDRW2100022199.1.12",
          "form": "에어프라이기에 먹으면 진짜 맛있어요 ㅜㅜ",
          "original_form": "에어프라이기에 먹으면 진짜 맛있어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 11:26"
        },
        {
          "id": "MDRW2100022199.1.13",
          "form": "근데 저 에어프라이기 없을 적에 프라이팬에 기름 둘러서 했는데 제 기준 그게 훨씬 더 맛있었어요 ㅋㅋ",
          "original_form": "근데 저 에어프라이기 없을 적에 프라이팬에 기름 둘러서 했는데 제 기준 그게 훨씬 더 맛있었어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100022199.1.14",
          "form": "기계가없어서 그렇게 먹어야게네요",
          "original_form": "기계가없어서 그렇게 먹어야게네요",
          "speaker_id": "2",
          "time": "20210829 11:32"
        },
        {
          "id": "MDRW2100022199.1.15",
          "form": "마트에서 보이면 한번사봐야겠어요",
          "original_form": "마트에서 보이면 한번사봐야겠어요",
          "speaker_id": "2",
          "time": "20210829 11:32"
        },
        {
          "id": "MDRW2100022199.1.16",
          "form": "마트에서 사면 엄청비싸요 ㅜㅜ",
          "original_form": "마트에서 사면 엄청비싸요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100022199.1.17",
          "form": "지금 핫딜 떠서 저 거의 반값 주고 샀어요 호호",
          "original_form": "지금 핫딜 떠서 저 거의 반값 주고 샀어요 호호",
          "speaker_id": "1",
          "time": "20210829 11:46"
        },
        {
          "id": "MDRW2100022199.1.18",
          "form": "일단한번 먹어보는거니까요 뭐ㅋㅋ",
          "original_form": "일단한번 먹어보는거니까요 뭐ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:19"
        },
        {
          "id": "MDRW2100022199.1.19",
          "form": "친구들 놀러오면 안주로 그거 해주는데 싫어하는 사람 없었어요",
          "original_form": "친구들 놀러오면 안주로 그거 해주는데 싫어하는 사람 없었어요",
          "speaker_id": "1",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100022199.1.20",
          "form": "근데 아주 후각이 예민하신 분은 닭냄새 난다 하더라구요",
          "original_form": "근데 아주 후각이 예민하신 분은 닭냄새 난다 하더라구요",
          "speaker_id": "1",
          "time": "20210829 13:49"
        },
        {
          "id": "MDRW2100022199.1.21",
          "form": "잘익혀서 먹어야겠네요",
          "original_form": "잘익혀서 먹어야겠네요",
          "speaker_id": "2",
          "time": "20210829 14:01"
        },
        {
          "id": "MDRW2100022199.1.22",
          "form": "땡기네요ㅋㅋ",
          "original_form": "땡기네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 14:01"
        },
        {
          "id": "MDRW2100022199.1.23",
          "form": "짭조름해서 맛있어요",
          "original_form": "짭조름해서 맛있어요",
          "speaker_id": "1",
          "time": "20210829 14:04"
        },
        {
          "id": "MDRW2100022199.1.24",
          "form": "교촌에서 봉이나 윙 맛있게 먹었으면 맛있을거에요",
          "original_form": "교촌에서 봉이나 윙 맛있게 먹었으면 맛있을거에요",
          "speaker_id": "1",
          "time": "20210829 14:04"
        }
      ]
    }
