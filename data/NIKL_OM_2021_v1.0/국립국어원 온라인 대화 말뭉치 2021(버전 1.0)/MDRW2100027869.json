      "utterance": [
        {
          "id": "MDRW2100027869.1.1",
          "form": "우와 스타듀벨리도 스위치로 할 수 있군요!",
          "original_form": "우와 스타듀벨리도 스위치로 할 수 있군요!",
          "speaker_id": "2",
          "time": "20210902 15:57"
        },
        {
          "id": "MDRW2100027869.1.2",
          "form": "농사짓기 난이도 너무 높을것같은데요...",
          "original_form": "농사짓기 난이도 너무 높을것같은데요...",
          "speaker_id": "2",
          "time": "20210902 15:57"
        },
        {
          "id": "MDRW2100027869.1.3",
          "form": "낚시도",
          "original_form": "낚시도",
          "speaker_id": "1",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100027869.1.4",
          "form": "모드가 없으니 헬난이도",
          "original_form": "모드가 없으니 헬난이도",
          "speaker_id": "1",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100027869.1.5",
          "form": "악 모드없이 낚시라니",
          "original_form": "악 모드없이 낚시라니",
          "speaker_id": "2",
          "time": "20210902 16:00"
        },
        {
          "id": "MDRW2100027869.1.6",
          "form": "그거 마을회관 채우는거 불가능하겠는데요ㅜㅜ",
          "original_form": "그거 마을회관 채우는거 불가능하겠는데요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100027869.1.7",
          "form": "그래서 조금하다 방치중이에요 ㅠㅠ",
          "original_form": "그래서 조금하다 방치중이에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027869.1.8",
          "form": "캐릭터도 순정이라 못생긴 ㅋㅋㅋ",
          "original_form": "캐릭터도 순정이라 못생긴 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:06"
        },
        {
          "id": "MDRW2100027869.1.9",
          "form": "아 저도 캐릭터는 모드 안씌우고 순정으로 플레이했는데",
          "original_form": "아 저도 캐릭터는 모드 안씌우고 순정으로 플레이했는데",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027869.1.10",
          "form": "생김새에 비해 너무 잘생긴척들을 해대서",
          "original_form": "생김새에 비해 너무 잘생긴척들을 해대서",
          "speaker_id": "2",
          "time": "20210902 16:08"
        },
        {
          "id": "MDRW2100027869.1.11",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027869.1.12",
          "form": "제 여캐를 헤일리랑 결혼시켰어요",
          "original_form": "제 여캐를 헤일리랑 결혼시켰어요",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027869.1.13",
          "form": "아 동성도 됐었지요 ㅎㅎ",
          "original_form": "아 동성도 됐었지요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027869.1.14",
          "form": "예쁘고 참하고 가끔 아침에 밥도 해주고...ㅋㅋ",
          "original_form": "예쁘고 참하고 가끔 아침에 밥도 해주고...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027869.1.15",
          "form": "애기는 안생기는거겠죠?",
          "original_form": "애기는 안생기는거겠죠?",
          "speaker_id": "1",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027869.1.16",
          "form": "어떻게 애기도 데려오더라고요",
          "original_form": "어떻게 애기도 데려오더라고요",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027869.1.17",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027869.1.18",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027869.1.19",
          "form": "신게임이네요 ㅋㅋㅋㅋㅋ",
          "original_form": "신게임이네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027869.1.20",
          "form": "둘이나 있어요",
          "original_form": "둘이나 있어요",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100027869.1.21",
          "form": "애기가 자라는거도 구현하면 좋겠어요",
          "original_form": "애기가 자라는거도 구현하면 좋겠어요",
