      "utterance": [
        {
          "id": "MDRW2100012475.1.1",
          "form": "영화 자주봐?",
          "original_form": "영화 자주봐?",
          "speaker_id": "1",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012475.1.2",
          "form": "음 자주는 안보는듯?",
          "original_form": "음 자주는 안보는듯?",
          "speaker_id": "2",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012475.1.3",
          "form": "코로나 전에는 친구가 같이 보자하면 갔었어",
          "original_form": "코로나 전에는 친구가 같이 보자하면 갔었어",
          "speaker_id": "2",
          "time": "20210824 22:20"
        },
        {
          "id": "MDRW2100012475.1.4",
          "form": "제일 기억에 남는 영화는 뭐야?",
          "original_form": "제일 기억에 남는 영화는 뭐야?",
          "speaker_id": "1",
          "time": "20210824 22:21"
        },
        {
          "id": "MDRW2100012475.1.5",
          "form": "나 해리포터 좀 좋아했고 겨울왕국도 재밌게 봤던 것 같아",
          "original_form": "나 해리포터 좀 좋아했고 겨울왕국도 재밌게 봤던 것 같아",
          "speaker_id": "2",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012475.1.6",
          "form": "오래돼서 내용은 기억이 안나지만..",
          "original_form": "오래돼서 내용은 기억이 안나지만..",
          "speaker_id": "2",
          "time": "20210824 22:50"
        },
        {
          "id": "MDRW2100012475.1.7",
          "form": "진짜 오래 전이구나",
          "original_form": "진짜 오래 전이구나",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012475.1.8",
          "form": "나는 이번에 모가디슈 봤는데",
          "original_form": "나는 이번에 모가디슈 봤는데",
          "speaker_id": "1",
          "time": "20210824 22:58"
        },
        {
          "id": "MDRW2100012475.1.9",
          "form": "모가디슈? 나 최근 영화는 잘 모르는데",
          "original_form": "모가디슈? 나 최근 영화는 잘 모르는데",
          "speaker_id": "2",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100012475.1.10",
          "form": "국내영화야?",
          "original_form": "국내영화야?",
          "speaker_id": "2",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100012475.1.11",
          "form": "응 국내영화ㅋㅋㅋㅋ 조인성 나와",
          "original_form": "응 국내영화ㅋㅋㅋㅋ 조인성 나와",
          "speaker_id": "1",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100012475.1.12",
          "form": "조인성 나와서 보러간 건 아니고",
          "original_form": "조인성 나와서 보러간 건 아니고",
          "speaker_id": "1",
          "time": "20210824 23:37"
        },
        {
          "id": "MDRW2100012475.1.13",
          "form": "지금 흥행하는1위영화일걸?",
          "original_form": "지금 흥행하는1위영화일걸?",
          "speaker_id": "1",
          "time": "20210824 23:38"
        },
        {
          "id": "MDRW2100012475.1.14",
          "form": "오 그런거같네",
          "original_form": "오 그런거같네",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100012475.1.15",
          "form": "평점도 높은거 같은데",
          "original_form": "평점도 높은거 같은데",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100012475.1.16",
          "form": "재밌었어?",
          "original_form": "재밌었어?",
          "speaker_id": "2",
          "time": "20210824 23:39"
        },
        {
          "id": "MDRW2100012475.1.17",
          "form": "사실 난 그저 그랬어",
          "original_form": "사실 난 그저 그랬어",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100012475.1.18",
          "form": "그냥 남북영화?",
          "original_form": "그냥 남북영화?",
          "speaker_id": "1",
          "time": "20210824 23:40"
        },
        {
          "id": "MDRW2100012475.1.19",
          "form": "줄거리 대충 보니까 소말리아 얘기도 있고..",
          "original_form": "줄거리 대충 보니까 소말리아 얘기도 있고..",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012475.1.20",
          "form": "그냥 스토리보단 액션씬만 돋보이는 영화인가?",
          "original_form": "그냥 스토리보단 액션씬만 돋보이는 영화인가?",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012475.1.21",
          "form": "맞아 그거 때문에 총싸움 좀 나오고 그래",
          "original_form": "맞아 그거 때문에 총싸움 좀 나오고 그래",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012475.1.22",
          "form": "어 그렇게 봐도 될 듯?",
          "original_form": "어 그렇게 봐도 될 듯?",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012475.1.23",
          "form": "근데 액션이 그렇게 훌륭한지는 잘 모르겠어",
          "original_form": "근데 액션이 그렇게 훌륭한지는 잘 모르겠어",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012475.1.24",
          "form": "아아.. 그런 액션 영화들 많지",
          "original_form": "아아.. 그런 액션 영화들 많지",
          "speaker_id": "2",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012475.1.25",
          "form": "차라리 존윅을 보겠어",
          "original_form": "차라리 존윅을 보겠어",
          "speaker_id": "1",
          "time": "20210824 23:42"
        },
        {
          "id": "MDRW2100012475.1.26",
          "form": "그럼 영화 중에 가장 많이 본 장르는 뭐야?",
          "original_form": "그럼 영화 중에 가장 많이 본 장르는 뭐야?",
          "speaker_id": "1",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100012475.1.27",
          "form": "난 아마 판타지쪽..? 애니메이션 많이 본 것 같아",
          "original_form": "난 아마 판타지쪽..? 애니메이션 많이 본 것 같아",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100012475.1.28",
          "form": "로맨스랑 가족코미디 같은 것도 좀 보고..",
          "original_form": "로맨스랑 가족코미디 같은 것도 좀 보고..",
          "speaker_id": "2",
          "time": "20210824 23:43"
        },
        {
          "id": "MDRW2100012475.1.29",
          "form": "무난무난하네",
          "original_form": "무난무난하네",
          "speaker_id": "1",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100012475.1.30",
          "form": "소울은 봤어?? 픽사 껀데 이번 년도에 나왔어",
          "original_form": "소울은 봤어?? 픽사 껀데 이번 년도에 나왔어",
          "speaker_id": "1",
          "time": "20210824 23:44"
        },
        {
          "id": "MDRW2100012475.1.31",
          "form": "아니아니 내가 최근에 영화를 안봐서 몰랐어",
          "original_form": "아니아니 내가 최근에 영화를 안봐서 몰랐어",
          "speaker_id": "2",
          "time": "20210824 23:45"
        },
        {
          "id": "MDRW2100012475.1.32",
          "form": "근데 뭔가 재밌어보인다",
          "original_form": "근데 뭔가 재밌어보인다",
          "speaker_id": "2",
          "time": "20210824 23:45"
        }
      ]
