      "utterance": [
        {
          "id": "MDRW2100010981.1.1",
          "form": "익숙한 곡이 제일 좋잖아요",
          "original_form": "익숙한 곡이 제일 좋잖아요",
          "speaker_id": "1",
          "time": "20210823 23:27"
        },
        {
          "id": "MDRW2100010981.1.2",
          "form": "네 저도 옛날노래 많이들어요",
          "original_form": "네 저도 옛날노래 많이들어요",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100010981.1.3",
          "form": "아이유노래도 좋아해요",
          "original_form": "아이유노래도 좋아해요",
          "speaker_id": "2",
          "time": "20210901 22:52"
        },
        {
          "id": "MDRW2100010981.1.4",
          "form": "아이유 노래 다 좋죠",
          "original_form": "아이유 노래 다 좋죠",
          "speaker_id": "1",
          "time": "20210901 23:00"
        },
        {
          "id": "MDRW2100010981.1.5",
          "form": "무슨 곡 제일 좋아하세요",
          "original_form": "무슨 곡 제일 좋아하세요",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100010981.1.6",
          "form": "진짜 다 좋아서 ㅜㅜㅜㅜㅜ",
          "original_form": "진짜 다 좋아서 ㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100010981.1.7",
          "form": "요즘엔 넥스트레벨? 그 신곡 좋아요 중독성있네요",
          "original_form": "요즘엔 넥스트레벨? 그 신곡 좋아요 중독성있네요",
          "speaker_id": "2",
          "time": "20210901 23:03"
        },
        {
          "id": "MDRW2100010981.1.8",
          "form": "티비에도 많이 나오잖아요ㅋㅋ",
          "original_form": "티비에도 많이 나오잖아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:06"
        },
        {
          "id": "MDRW2100010981.1.9",
          "form": "예능에도 브금으로 계속 쓰더라구요",
          "original_form": "예능에도 브금으로 계속 쓰더라구요",
          "speaker_id": "1",
          "time": "20210901 23:07"
        },
        {
          "id": "MDRW2100010981.1.10",
          "form": "네 그래서 알았어욬ㅋㅋㅋ",
          "original_form": "네 그래서 알았어욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:08"
        },
        {
          "id": "MDRW2100010981.1.11",
          "form": "tvn 놀토 보세요? 거기 자주 나오더라구요ㅋㅋ",
          "original_form": "tvn 놀토 보세요? 거기 자주 나오더라구요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:09"
        },
        {
          "id": "MDRW2100010981.1.12",
          "form": "그거 가끔봐요 풀로는 안보고욬ㅋ",
          "original_form": "그거 가끔봐요 풀로는 안보고욬ㅋ",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100010981.1.13",
          "form": "전 구해줘홈즈에 나오는거보고 알았어요",
          "original_form": "전 구해줘홈즈에 나오는거보고 알았어요",
          "speaker_id": "2",
          "time": "20210901 23:17"
        },
        {
          "id": "MDRW2100010981.1.14",
          "form": "놀토에서선 신동엽님이 춤출때마다 나와여ㅋㅋ",
          "original_form": "놀토에서선 신동엽님이 춤출때마다 나와여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:21"
        },
        {
          "id": "MDRW2100010981.1.15",
          "form": "춤이 비슷한 느낌이라는데 억지 같긴 하더라구요ㅋㅋㅋ",
          "original_form": "춤이 비슷한 느낌이라는데 억지 같긴 하더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100010981.1.16",
          "form": "앜ㅋㅋㅋ웃길거같아요",
          "original_form": "앜ㅋㅋㅋ웃길거같아요",
          "speaker_id": "2",
          "time": "20210901 23:22"
        },
        {
          "id": "MDRW2100010981.1.17",
          "form": "넘 욱겨요ㅋㅋ 어떤 노래를 틀어도 같은 춤만 추시는...ㅋㅋㅋ",
          "original_form": "넘 욱겨요ㅋㅋ 어떤 노래를 틀어도 같은 춤만 추시는...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100010981.1.18",
          "form": "ㅋㅋㅋㅋㅋ신동엽은 진짜 타고났어요",
          "original_form": "ㅋㅋㅋㅋㅋ신동엽은 진짜 타고났어요",
          "speaker_id": "2",
          "time": "20210902 15:08"
        },
        {
          "id": "MDRW2100010981.1.19",
          "form": "적절한 멘트를 타이밍 맞춰서 잘 하져",
          "original_form": "적절한 멘트를 타이밍 맞춰서 잘 하져",
          "speaker_id": "1",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100010981.1.20",
          "form": "좋아하는 연예인있으세요?",
          "original_form": "좋아하는 연예인있으세요?",
          "speaker_id": "2",
