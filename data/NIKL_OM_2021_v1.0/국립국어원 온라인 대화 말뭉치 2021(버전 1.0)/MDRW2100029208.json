      "utterance": [
        {
          "id": "MDRW2100029208.1.1",
          "form": "군대에서 종교 행사 많이 하잖아요",
          "original_form": "군대에서 종교 행사 많이 하잖아요",
          "speaker_id": "1",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029208.1.2",
          "form": "어릴때 할머니따라",
          "original_form": "어릴때 할머니따라",
          "speaker_id": "2",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029208.1.3",
          "form": "절을 많이감",
          "original_form": "절을 많이감",
          "speaker_id": "2",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029208.1.4",
          "form": "설에 많이 다니셨어요",
          "original_form": "설에 많이 다니셨어요",
          "speaker_id": "1",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029208.1.5",
          "form": "초코파이교",
          "original_form": "초코파이교",
          "speaker_id": "2",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029208.1.6",
          "form": "서른 분위기가 참 좋아요",
          "original_form": "서른 분위기가 참 좋아요",
          "speaker_id": "1",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029208.1.7",
          "form": "음식 많이 주는곳따라",
          "original_form": "음식 많이 주는곳따라",
          "speaker_id": "2",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029208.1.8",
          "form": "절이 왜 계속 설이라고 나올까요",
          "original_form": "절이 왜 계속 설이라고 나올까요",
          "speaker_id": "1",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029208.1.9",
          "form": "바뀐다던데",
          "original_form": "바뀐다던데",
          "speaker_id": "2",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029208.1.10",
          "form": "발음문제.ㅣ",
          "original_form": "발음문제.ㅣ",
          "speaker_id": "2",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029208.1.11",
          "form": "그쵸 근데 교회가 많이 준대요",
          "original_form": "그쵸 근데 교회가 많이 준대요",
          "speaker_id": "1",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029208.1.12",
          "form": "내원정사에 자주감",
          "original_form": "내원정사에 자주감",
          "speaker_id": "2",
          "time": "20210903 00:19"
        },
        {
          "id": "MDRW2100029208.1.13",
          "form": "내 발음이 문제군요",
          "original_form": "내 발음이 문제군요",
          "speaker_id": "1",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100029208.1.14",
          "form": "ㅋㅋㅋㅋ어릴때 일주일에 한번씩",
          "original_form": "ㅋㅋㅋㅋ어릴때 일주일에 한번씩",
          "speaker_id": "2",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100029208.1.15",
          "form": "내원 정사는 운동하러 많이 갔는데",
          "original_form": "내원 정사는 운동하러 많이 갔는데",
          "speaker_id": "1",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100029208.1.16",
          "form": "절에갔었음",
          "original_form": "절에갔었음",
          "speaker_id": "2",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100029208.1.17",
          "form": "저 르는 분위기가 참 좋은 것 같아요",
          "original_form": "저 르는 분위기가 참 좋은 것 같아요",
          "speaker_id": "1",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100029208.1.18",
          "form": "근처 경치도 좋고",
          "original_form": "근처 경치도 좋고",
          "speaker_id": "1",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100029208.1.19",
          "form": "비오는날",
          "original_form": "비오는날",
          "speaker_id": "2",
          "time": "20210903 00:20"
        },
        {
          "id": "MDRW2100029208.1.20",
