      "utterance": [
        {
          "id": "MDRW2100011285.1.1",
          "form": "비가 많이 와요 밤새님 지역도 비가 오나요",
          "original_form": "비가 많이 와요 밤새님 지역도 비가 오나요",
          "speaker_id": "1",
          "time": "20210824 10:43"
        },
        {
          "id": "MDRW2100011285.1.2",
          "form": "네 아침부터 계속 내리네요",
          "original_form": "네 아침부터 계속 내리네요",
          "speaker_id": "2",
          "time": "20210824 10:44"
        },
        {
          "id": "MDRW2100011285.1.3",
          "form": "태풍이 와서 그런가 봐요",
          "original_form": "태풍이 와서 그런가 봐요",
          "speaker_id": "1",
          "time": "20210824 10:44"
        },
        {
          "id": "MDRW2100011285.1.4",
          "form": "그래서 그런가 습하지 않나요",
          "original_form": "그래서 그런가 습하지 않나요",
          "speaker_id": "1",
          "time": "20210824 10:45"
        },
        {
          "id": "MDRW2100011285.1.5",
          "form": "맞아요. 습해서 에어컨 틀어뒀더니 춥네요",
          "original_form": "맞아요. 습해서 에어컨 틀어뒀더니 춥네요",
          "speaker_id": "2",
          "time": "20210824 10:45"
        },
        {
          "id": "MDRW2100011285.1.6",
          "form": "비 오는 날 어떠세요",
          "original_form": "비 오는 날 어떠세요",
          "speaker_id": "1",
          "time": "20210824 10:46"
        },
        {
          "id": "MDRW2100011285.1.7",
          "form": "비 좋아하시나요 싫어하시나요",
          "original_form": "비 좋아하시나요 싫어하시나요",
          "speaker_id": "1",
          "time": "20210824 10:46"
        },
        {
          "id": "MDRW2100011285.1.8",
          "form": "적당히 내리는건 좋아해요",
          "original_form": "적당히 내리는건 좋아해요",
          "speaker_id": "2",
          "time": "20210824 10:47"
        },
        {
          "id": "MDRW2100011285.1.9",
          "form": "운치있기도 하구요",
          "original_form": "운치있기도 하구요",
          "speaker_id": "2",
          "time": "20210824 10:47"
        },
        {
          "id": "MDRW2100011285.1.10",
          "form": "어떤 느낌인지 알 것 같아요",
          "original_form": "어떤 느낌인지 알 것 같아요",
          "speaker_id": "1",
          "time": "20210824 10:47"
        },
        {
          "id": "MDRW2100011285.1.11",
          "form": "지원님은 어떠세요?",
          "original_form": "지원님은 어떠세요?",
          "speaker_id": "2",
          "time": "20210824 10:47"
        },
        {
          "id": "MDRW2100011285.1.12",
          "form": "저는그렇게 좋아하지는 않아요",
          "original_form": "저는그렇게 좋아하지는 않아요",
          "speaker_id": "1",
          "time": "20210824 10:48"
        },
        {
          "id": "MDRW2100011285.1.13",
          "form": "습한 것도 있고 젖는 것도 싫고 그래서 별로 선호하지 않는 편이에요",
          "original_form": "습한 것도 있고 젖는 것도 싫고 그래서 별로 선호하지 않는 편이에요",
          "speaker_id": "1",
          "time": "20210824 10:48"
        },
        {
          "id": "MDRW2100011285.1.14",
          "form": "그렇긴하죠. 눈도 별로 좋아하진 않겠네요?",
          "original_form": "그렇긴하죠. 눈도 별로 좋아하진 않겠네요?",
          "speaker_id": "2",
          "time": "20210824 10:48"
        },
        {
          "id": "MDRW2100011285.1.15",
          "form": "눈 오면 설레기는 하는데  녹으면 미끄러워서",
          "original_form": "눈 오면 설레기는 하는데  녹으면 미끄러워서",
          "speaker_id": "1",
          "time": "20210824 10:49"
        },
        {
          "id": "MDRW2100011285.1.16",
          "form": "밤새님은 눈 좋아하시나요",
          "original_form": "밤새님은 눈 좋아하시나요",
          "speaker_id": "1",
          "time": "20210824 10:49"
        },
        {
          "id": "MDRW2100011285.1.17",
          "form": "좋아하는 편이에요",
          "original_form": "좋아하는 편이에요",
          "speaker_id": "2",
          "time": "20210824 10:50"
        },
        {
          "id": "MDRW2100011285.1.18",
          "form": "그러시구나 눈도 비도 좋아하는 편이시네요",
          "original_form": "그러시구나 눈도 비도 좋아하는 편이시네요",
          "speaker_id": "1",
          "time": "20210824 10:50"
        }
