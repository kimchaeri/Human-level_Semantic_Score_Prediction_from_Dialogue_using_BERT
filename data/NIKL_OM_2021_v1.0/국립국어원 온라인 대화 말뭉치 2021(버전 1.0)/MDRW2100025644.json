      "utterance": [
        {
          "id": "MDRW2100025644.1.1",
          "form": "집안일은 해도해도 끝이없어요",
          "original_form": "집안일은 해도해도 끝이없어요",
          "speaker_id": "1",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100025644.1.2",
          "form": "맞아요 저는잘안하늗데엄마 하는거보니  힘들어보이더라구요",
          "original_form": "맞아요 저는잘안하늗데엄마 하는거보니  힘들어보이더라구요",
          "speaker_id": "2",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100025644.1.3",
          "form": "틈틈히 도와줄려고하는데",
          "original_form": "틈틈히 도와줄려고하는데",
          "speaker_id": "2",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100025644.1.4",
          "form": "일만 만든다고 머라허셔서..ㅎ",
          "original_form": "일만 만든다고 머라허셔서..ㅎ",
          "speaker_id": "2",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100025644.1.5",
          "form": "설거지가 제일 싫어요",
          "original_form": "설거지가 제일 싫어요",
          "speaker_id": "1",
          "time": "20210901 14:57"
        },
        {
          "id": "MDRW2100025644.1.6",
          "form": "식기세척기 사고싶어요",
          "original_form": "식기세척기 사고싶어요",
          "speaker_id": "1",
          "time": "20210901 14:57"
        },
        {
          "id": "MDRW2100025644.1.7",
          "form": "편하긴하겠네요",
          "original_form": "편하긴하겠네요",
          "speaker_id": "2",
          "time": "20210901 15:17"
        },
        {
          "id": "MDRW2100025644.1.8",
          "form": "머든지 자동이최고예용",
          "original_form": "머든지 자동이최고예용",
          "speaker_id": "2",
          "time": "20210901 15:17"
        },
        {
          "id": "MDRW2100025644.1.9",
          "form": "요즘 기술이 좋네요",
          "original_form": "요즘 기술이 좋네요",
          "speaker_id": "1",
          "time": "20210901 15:17"
        },
        {
          "id": "MDRW2100025644.1.10",
          "form": "청소기 필터 자동 비움 기계도 짱 좋아보여요",
          "original_form": "청소기 필터 자동 비움 기계도 짱 좋아보여요",
          "speaker_id": "1",
          "time": "20210901 15:17"
        },
        {
          "id": "MDRW2100025644.1.11",
          "form": "헐 저도 쓰레기통 자동으로 비워주는거 가지고 싶더라구요 ㅠㅠ",
          "original_form": "헐 저도 쓰레기통 자동으로 비워주는거 가지고 싶더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 15:22"
        },
        {
          "id": "MDRW2100025644.1.12",
          "form": "헐 그런거도 있나봐요",
          "original_form": "헐 그런거도 있나봐요",
          "speaker_id": "1",
          "time": "20210901 15:23"
        },
        {
          "id": "MDRW2100025644.1.13",
          "form": "쓰레기 자동으로 비워주면 짱이네요",
          "original_form": "쓰레기 자동으로 비워주면 짱이네요",
          "speaker_id": "1",
          "time": "20210901 15:24"
        },
        {
          "id": "MDRW2100025644.1.14",
          "form": "맞아요 묶어서 나와서 바로 버릴수있게 되어 있더라구여 ㅠㅠ",
          "original_form": "맞아요 묶어서 나와서 바로 버릴수있게 되어 있더라구여 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210901 15:35"
        },
        {
          "id": "MDRW2100025644.1.15",
          "form": "와 짱이네요",
          "original_form": "와 짱이네요",
          "speaker_id": "1",
          "time": "20210901 15:35"
        },
        {
          "id": "MDRW2100025644.1.16",
          "form": "전 집안일중에 화장실 청소가 젤 좋아요",
          "original_form": "전 집안일중에 화장실 청소가 젤 좋아요",
          "speaker_id": "1",
          "time": "20210901 15:36"
        },
        {
          "id": "MDRW2100025644.1.17",
          "form": "좋아하는 청소도있군요...",
          "original_form": "좋아하는 청소도있군요...",
          "speaker_id": "2",
          "time": "20210901 15:38"
        },
        {
          "id": "MDRW2100025644.1.18",
          "form": "락스냄새 수영장냄새라 좋더라구요 ㅋㅋ",
          "original_form": "락스냄새 수영장냄새라 좋더라구요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 15:39"
        },
        {
          "id": "MDRW2100025644.1.19",
          "form": "저도 물놀이좋아해요",
          "original_form": "저도 물놀이좋아해요",
          "speaker_id": "2",
          "time": "20210901 15:45"
        },
        {
          "id": "MDRW2100025644.1.20",
          "form": "역시 이런날엔 물놀이가 짱",
          "original_form": "역시 이런날엔 물놀이가 짱",
          "speaker_id": "1",
          "time": "20210901 15:45"
        }
      ]
