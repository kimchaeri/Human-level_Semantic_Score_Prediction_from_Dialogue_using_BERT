      "utterance": [
        {
          "id": "MDRW2100032146.1.1",
          "form": "name2은 어떤 과목을 좋아하세요?",
          "original_form": "&name2&은 어떤 과목을 좋아하세요?",
          "speaker_id": "1",
          "time": "20210905 13:15"
        },
        {
          "id": "MDRW2100032146.1.2",
          "form": "저는 역사교육전공하고있어요!",
          "original_form": "저는 역사교육전공하고있어요!",
          "speaker_id": "2",
          "time": "20210905 13:16"
        },
        {
          "id": "MDRW2100032146.1.3",
          "form": "우와 멋져요! 선생님이 될 분이시군요!",
          "original_form": "우와 멋져요! 선생님이 될 분이시군요!",
          "speaker_id": "1",
          "time": "20210905 13:19"
        },
        {
          "id": "MDRW2100032146.1.4",
          "form": "원래부터 역사를 좋아하셨나요?",
          "original_form": "원래부터 역사를 좋아하셨나요?",
          "speaker_id": "1",
          "time": "20210905 13:19"
        },
        {
          "id": "MDRW2100032146.1.5",
          "form": "네넨 그렇습니당",
          "original_form": "네넨 그렇습니당",
          "speaker_id": "2",
          "time": "20210905 13:27"
        },
        {
          "id": "MDRW2100032146.1.6",
          "form": "한국사를 좋아하신건가요? 아니면 세계사?",
          "original_form": "한국사를 좋아하신건가요? 아니면 세계사?",
          "speaker_id": "1",
          "time": "20210905 13:27"
        },
        {
          "id": "MDRW2100032146.1.7",
          "form": "저는 한국사요!",
          "original_form": "저는 한국사요!",
          "speaker_id": "2",
          "time": "20210905 13:28"
        },
        {
          "id": "MDRW2100032146.1.8",
          "form": "대학생이세요??!",
          "original_form": "대학생이세요??!",
          "speaker_id": "2",
          "time": "20210905 13:28"
        },
        {
          "id": "MDRW2100032146.1.9",
          "form": "안타깝게도 졸업한지 조금 됐어요 ㅠ",
          "original_form": "안타깝게도 졸업한지 조금 됐어요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 13:29"
        },
        {
          "id": "MDRW2100032146.1.10",
          "form": "다시 대학생이 되고 싶어요 ㅠㅠ",
          "original_form": "다시 대학생이 되고 싶어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 13:29"
        },
        {
          "id": "MDRW2100032146.1.11",
          "form": "휴학할걸 ㅠㅠ",
          "original_form": "휴학할걸 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210905 13:29"
        },
        {
          "id": "MDRW2100032146.1.12",
          "form": "아아아앙아",
          "original_form": "아아아앙아",
          "speaker_id": "2",
          "time": "20210905 13:29"
        },
        {
          "id": "MDRW2100032146.1.13",
          "form": "그렇군요 ㅠㅠㅠㅠ소울메이트님이랑 대학다니면서",
          "original_form": "그렇군요 ㅠㅠㅠㅠ소울메이트님이랑 대학다니면서",
          "speaker_id": "2",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100032146.1.14",
          "form": "카페투어하면",
          "original_form": "카페투어하면",
          "speaker_id": "2",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100032146.1.15",
          "form": "크,,,,",
          "original_form": "크,,,,",
          "speaker_id": "2",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100032146.1.16",
          "form": "한국사 필수 수능 보신 분이시죠?",
          "original_form": "한국사 필수 수능 보신 분이시죠?",
          "speaker_id": "1",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100032146.1.17",
          "form": "그러게요 ㅠ 제가 휴학을 했어야했네요 같이 대학다니면서 카페 투어도 하게 그랬어야 했는데",
          "original_form": "그러게요 ㅠ 제가 휴학을 했어야했네요 같이 대학다니면서 카페 투어도 하게 그랬어야 했는데",
          "speaker_id": "1",
          "time": "20210905 13:30"
        },
        {
          "id": "MDRW2100032146.1.18",
          "form": "네네네 맞아요!!",
          "original_form": "네네네 맞아요!!",
          "speaker_id": "2",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032146.1.19",
          "form": "소율메이트...같이카페투어하구싶어요 ㅠㅠㅠ",
          "original_form": "소율메이트...같이카페투어하구싶어요 ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 13:57"
        },
        {
          "id": "MDRW2100032146.1.20",
          "form": "요새 개강하셨는데 싸강 힘들지 않아요?",
          "original_form": "요새 개강하셨는데 싸강 힘들지 않아요?",
          "speaker_id": "1",
          "time": "20210905 15:40"
        },
        {
          "id": "MDRW2100032146.1.21",
          "form": "학교안가서 편하기두 해요!",
          "original_form": "학교안가서 편하기두 해요!",
          "speaker_id": "2",
          "time": "20210905 15:42"
        },
        {
          "id": "MDRW2100032146.1.22",
          "form": "ㅋㅋㅋ 그건 그럴것 같아요",
          "original_form": "ㅋㅋㅋ 그건 그럴것 같아요",
          "speaker_id": "1",
          "time": "20210905 15:43"
        },
        {
          "id": "MDRW2100032146.1.23",
          "form": "통학하는데 너무 힘들죠ㅠ",
          "original_form": "통학하는데 너무 힘들죠ㅠ",
          "speaker_id": "1",
          "time": "20210905 15:43"
        },
        {
          "id": "MDRW2100032146.1.24",
          "form": "맞아요 ㅠㅠㅠㅠ",
          "original_form": "맞아요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210905 15:45"
        },
        {
          "id": "MDRW2100032146.1.25",
          "form": "대부분 학교다니는 동안 줌 수업으로 해서 아쉽지 않나요?",
          "original_form": "대부분 학교다니는 동안 줌 수업으로 해서 아쉽지 않나요?",
          "speaker_id": "1",
          "time": "20210905 15:50"
        },
        {
          "id": "MDRW2100032146.1.26",
          "form": "동기들이랑 친해질 짬도 없었겠어요 ㅠ",
          "original_form": "동기들이랑 친해질 짬도 없었겠어요 ㅠ",
          "speaker_id": "1",
          "time": "20210905 15:50"
        },
        {
          "id": "MDRW2100032146.1.27",
          "form": "저는 그래도 대면수업도있고해서",
          "original_form": "저는 그래도 대면수업도있고해서",
          "speaker_id": "2",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100032146.1.28",
          "form": "고냥고냥인느낌이에여!",
          "original_form": "고냥고냥인느낌이에여!",
          "speaker_id": "2",
          "time": "20210905 15:52"
        },
        {
          "id": "MDRW2100032146.1.29",
          "form": "개강하고는 아직 대면수업을 못했죠?",
          "original_form": "개강하고는 아직 대면수업을 못했죠?",
          "speaker_id": "1",
          "time": "20210905 18:10"
        },
        {
          "id": "MDRW2100032146.1.30",
          "form": "옆에서 다시 빨리 방학했으면 좋겠다며 성화에요 ㅋㅋ",
          "original_form": "옆에서 다시 빨리 방학했으면 좋겠다며 성화에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 18:10"
        },
        {
          "id": "MDRW2100032146.1.31",
          "form": "아니.. 진짜 공부 잘했던게 맞잖아요 ㅋㅋㅋㅋ",
          "original_form": "아니.. 진짜 공부 잘했던게 맞잖아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:02"
        }
      ]
