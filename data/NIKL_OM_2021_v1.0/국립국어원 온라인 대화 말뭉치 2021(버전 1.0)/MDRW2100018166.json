      "utterance": [
        {
          "id": "MDRW2100018166.1.1",
          "form": "거긴 오늘 날씨 어떠셨어요?",
          "original_form": "거긴 오늘 날씨 어떠셨어요?",
          "speaker_id": "1",
          "time": "20210826 22:58"
        },
        {
          "id": "MDRW2100018166.1.2",
          "form": "여긴 오늘은 비안왔더라구용",
          "original_form": "여긴 오늘은 비안왔더라구용",
          "speaker_id": "1",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018166.1.3",
          "form": "여기는 아까 잠깐 왔어요",
          "original_form": "여기는 아까 잠깐 왔어요",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018166.1.4",
          "form": "요즘 날씨가",
          "original_form": "요즘 날씨가",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018166.1.5",
          "form": "진짜 무슨 동남아마냥 ㅠㅠ",
          "original_form": "진짜 무슨 동남아마냥 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 23:00"
        },
        {
          "id": "MDRW2100018166.1.6",
          "form": "맞아요 진짜ㅠㅠ 먹구름도 저쪽은 많은데 반대편은 맑고,,,!",
          "original_form": "맞아요 진짜ㅠㅠ 먹구름도 저쪽은 많은데 반대편은 맑고,,,!",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018166.1.7",
          "form": "알수 없는 날씨에요..",
          "original_form": "알수 없는 날씨에요..",
          "speaker_id": "1",
          "time": "20210826 23:01"
        },
        {
          "id": "MDRW2100018166.1.8",
          "form": "마자여 그리고 올해 여름너ㅓ무 더웠어여",
          "original_form": "마자여 그리고 올해 여름너ㅓ무 더웠어여",
          "speaker_id": "2",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018166.1.9",
          "form": "지금도 여름이긴 하지만",
          "original_form": "지금도 여름이긴 하지만",
          "speaker_id": "2",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018166.1.10",
          "form": "진짜 더워서 미치는 줄 알앗어여",
          "original_form": "진짜 더워서 미치는 줄 알앗어여",
          "speaker_id": "2",
          "time": "20210826 23:02"
        },
        {
          "id": "MDRW2100018166.1.11",
          "form": "그쵸그초ㅠㅠ",
          "original_form": "그쵸그초ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018166.1.12",
          "form": "진짜 이제 에어컨 없으면 못살겠어요ㅜㅠ",
          "original_form": "진짜 이제 에어컨 없으면 못살겠어요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210826 23:03"
        },
        {
          "id": "MDRW2100018166.1.13",
          "form": "그니까여 ㅠㅠㅠㅠ 이젠 에어컨 없는 여름은 상상할 수가 없져...",
          "original_form": "그니까여 ㅠㅠㅠㅠ 이젠 에어컨 없는 여름은 상상할 수가 없져...",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018166.1.14",
          "form": "이제 건조기도 제습기도 다들 많이 사시더라구요ㅠ",
          "original_form": "이제 건조기도 제습기도 다들 많이 사시더라구요ㅠ",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018166.1.15",
          "form": "마자여 있으면 좋더라구여",
          "original_form": "마자여 있으면 좋더라구여",
          "speaker_id": "2",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018166.1.16",
          "form": "날씨가 습하니까",
          "original_form": "날씨가 습하니까",
          "speaker_id": "2",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018166.1.17",
          "form": "필요한거 같아여",
          "original_form": "필요한거 같아여",
          "speaker_id": "2",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018166.1.18",
          "form": "오 두개중에 사용하시는거 있으세요?",
          "original_form": "오 두개중에 사용하시는거 있으세요?",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018166.1.19",
          "form": "둘다 잌ㅅ어여",
          "original_form": "둘다 잌ㅅ어여",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018166.1.20",
          "form": "와 부러워요ㅜㅠ",
          "original_form": "와 부러워요ㅜㅠ",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018166.1.21",
          "form": "제습기는 없어도 어떻게 잘 있을것같은데 건조기는 진짜 사고싶네요",
          "original_form": "제습기는 없어도 어떻게 잘 있을것같은데 건조기는 진짜 사고싶네요",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018166.1.22",
          "form": "근데 은근 제습기도 필요해여",
          "original_form": "근데 은근 제습기도 필요해여",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018166.1.23",
          "form": "있으면 좋더라구여",
          "original_form": "있으면 좋더라구여",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018166.1.24",
          "form": "오 정말요?",
          "original_form": "오 정말요?",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018166.1.25",
          "form": "진짜 날씨가 날씨라서",
          "original_form": "진짜 날씨가 날씨라서",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018166.1.26",
          "form": "아무래도 점점 날이 습해지니까 사두면 유용하게 쓰는거 같아여",
          "original_form": "아무래도 점점 날이 습해지니까 사두면 유용하게 쓰는거 같아여",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018166.1.27",
          "form": "필요하겠네용",
          "original_form": "필요하겠네용",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018166.1.28",
          "form": "ㅋㅋㅋㅋ얼른 가을이나 됐음 좋겠어요",
          "original_form": "ㅋㅋㅋㅋ얼른 가을이나 됐음 좋겠어요",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018166.1.29",
          "form": "마자요 ㅋㅋㅋㅋㅋ",
          "original_form": "마자요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018166.1.30",
          "form": "얼른 가을옷 입고싶네요ㅎㅎ",
          "original_form": "얼른 가을옷 입고싶네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 23:09"
        },
        {
          "id": "MDRW2100018166.1.31",
          "form": "저두여 ㅋㅋㅋ",
          "original_form": "저두여 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:10"
        }
      ]
    }
