        },
        {
          "id": "MDRW2100028916.1.2",
          "form": "신천지에요",
          "original_form": "신천지에요",
          "speaker_id": "1",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100028916.1.3",
          "form": "정말요?",
          "original_form": "정말요?",
          "speaker_id": "2",
          "time": "20210902 22:47"
        },
        {
          "id": "MDRW2100028916.1.4",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100028916.1.5",
          "form": "농담이었어요.",
          "original_form": "농담이었어요.",
          "speaker_id": "2",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100028916.1.6",
          "form": "네 저는 기독교에요",
          "original_form": "네 저는 기독교에요",
          "speaker_id": "1",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100028916.1.7",
          "form": "언제부터 믿었어요?",
          "original_form": "언제부터 믿었어요?",
          "speaker_id": "2",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100028916.1.8",
          "form": "모태신앙이에요",
          "original_form": "모태신앙이에요",
          "speaker_id": "1",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100028916.1.9",
          "form": "저는 전에 다녔는데 지금은 싫어해요",
          "original_form": "저는 전에 다녔는데 지금은 싫어해요",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100028916.1.10",
          "form": "왜 싫어하시나요~??",
          "original_form": "왜 싫어하시나요~??",
          "speaker_id": "1",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100028916.1.11",
          "form": "여러가지 이유가 있는데\r\n그중에 가장 큰 건\r\n사람한테 실망해서에요.",
          "original_form": "여러가지 이유가 있는데\r\n그중에 가장 큰 건\r\n사람한테 실망해서에요.",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100028916.1.12",
          "form": "어머 그랗군요",
          "original_form": "어머 그랗군요",
          "speaker_id": "1",
          "time": "20210902 22:50"
        },
        {
          "id": "MDRW2100028916.1.13",
          "form": "나쁜 사람중에 기독교인 사람을 만났을때 그종교가 싫어지더라구요.",
