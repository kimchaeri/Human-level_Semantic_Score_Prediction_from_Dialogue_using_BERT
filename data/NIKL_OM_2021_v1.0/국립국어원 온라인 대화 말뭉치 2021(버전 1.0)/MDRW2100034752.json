      "utterance": [
        {
          "id": "MDRW2100034752.1.1",
          "form": "편의점 디카페는 진짜 맛없더라구요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "편의점 디카페는 진짜 맛없더라구요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:43"
        },
        {
          "id": "MDRW2100034752.1.2",
          "form": "아ㅋㅋㅋㅋ 디카페인 맛을 또 구분하시는군요",
          "original_form": "아ㅋㅋㅋㅋ 디카페인 맛을 또 구분하시는군요",
          "speaker_id": "2",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100034752.1.3",
          "form": "역시 맛잘알이셔서 그런가",
          "original_form": "역시 맛잘알이셔서 그런가",
          "speaker_id": "2",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100034752.1.4",
          "form": "앜ㅋㅋㅋㅋ 사실 커피맛은 잘 모르는데",
          "original_form": "앜ㅋㅋㅋㅋ 사실 커피맛은 잘 모르는데",
          "speaker_id": "1",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100034752.1.5",
          "form": "아메리카노는 편의점건..보리차같더라고요  ..",
          "original_form": "아메리카노는 편의점건..보리차같더라고요  ..",
          "speaker_id": "1",
          "time": "20210828 18:44"
        },
        {
          "id": "MDRW2100034752.1.6",
          "form": "ㅋㅋㅋㅋㅋ아 그런게 없지않아있죠",
          "original_form": "ㅋㅋㅋㅋㅋ아 그런게 없지않아있죠",
          "speaker_id": "2",
          "time": "20210828 18:45"
        },
        {
          "id": "MDRW2100034752.1.7",
          "form": "저는 근데 차만큼 연한 아메리카노도 좋아해서 그럭저럭 잘 마셔요 ㅋㅋㅋㅋ",
          "original_form": "저는 근데 차만큼 연한 아메리카노도 좋아해서 그럭저럭 잘 마셔요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:46"
        },
        {
          "id": "MDRW2100034752.1.8",
          "form": "아그래요?ㅋㅋㅋㅋ",
          "original_form": "아그래요?ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:46"
        },
        {
          "id": "MDRW2100034752.1.9",
          "form": "전 뭔가 아메리카노는 사약맛 나야된다 생각해서요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "전 뭔가 아메리카노는 사약맛 나야된다 생각해서요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:46"
        },
        {
          "id": "MDRW2100034752.1.10",
          "form": "사약맛ㅋㅋㅋㅋㅋ",
          "original_form": "사약맛ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:47"
        },
        {
          "id": "MDRW2100034752.1.11",
          "form": "진한 것도 물론 좋아하긴해여",
          "original_form": "진한 것도 물론 좋아하긴해여",
          "speaker_id": "2",
          "time": "20210828 18:47"
        },
        {
          "id": "MDRW2100034752.1.12",
          "form": "제가 가리는게 없어서...",
          "original_form": "제가 가리는게 없어서...",
          "speaker_id": "2",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100034752.1.13",
          "form": "앜ㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100034752.1.14",
          "form": "전 원래 모카나 바닐라라떼 좋아해요 단걸 좋아해서요 ㅋㅋㅋ",
          "original_form": "전 원래 모카나 바닐라라떼 좋아해요 단걸 좋아해서요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:48"
        },
        {
          "id": "MDRW2100034752.1.15",
          "form": "저두요 젤 많이 시키는건 아바라ㅋㅋㅋㅋ",
          "original_form": "저두요 젤 많이 시키는건 아바라ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:49"
        },
        {
          "id": "MDRW2100034752.1.16",
          "form": "오오 저도요 ㅋㅋㅋㅋ",
          "original_form": "오오 저도요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 18:49"
        },
        {
          "id": "MDRW2100034752.1.17",
          "form": "말하니까 먹고싶네요 ㅠ",
          "original_form": "말하니까 먹고싶네요 ㅠ",
          "speaker_id": "1",
          "time": "20210828 18:50"
        },
        {
          "id": "MDRW2100034752.1.18",
          "form": "그러게여 내일은 낮에 카페가서 테이크아웃 해와야겠네요 ㅋㅋㅋ",
          "original_form": "그러게여 내일은 낮에 카페가서 테이크아웃 해와야겠네요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:50"
        },
        {
          "id": "MDRW2100034752.1.19",
          "form": "저희집은 바로 앞에 카페가 있답니다 호호",
          "original_form": "저희집은 바로 앞에 카페가 있답니다 호호",
          "speaker_id": "1",
          "time": "20210828 18:51"
        },
        {
          "id": "MDRW2100034752.1.20",
          "form": "오 저두 저희집 바로 뒤에ㅋㅋㅋㅋ",
