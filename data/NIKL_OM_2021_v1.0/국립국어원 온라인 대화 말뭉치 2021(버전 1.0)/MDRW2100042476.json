      "utterance": [
        {
          "id": "MDRW2100042476.1.1",
          "form": "하아 낙곱새 먹고싶어요 나중에 만들어주세요",
          "original_form": "하아 낙곱새 먹고싶어요 나중에 만들어주세요",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042476.1.2",
          "form": "그게 뭐냐",
          "original_form": "그게 뭐냐",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042476.1.3",
          "form": "엄마 맨날 이거 하면서 그게 뭐냐고 하는거같아요",
          "original_form": "엄마 맨날 이거 하면서 그게 뭐냐고 하는거같아요",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042476.1.4",
          "form": "낙지 곱창 새우!",
          "original_form": "낙지 곱창 새우!",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042476.1.5",
          "form": "그걸 왜 다 줄여불러",
          "original_form": "그걸 왜 다 줄여불러",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042476.1.6",
          "form": "별거 다 줄여부른다 줄여서 별다줄",
          "original_form": "별거 다 줄여부른다 줄여서 별다줄",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042476.1.7",
          "form": "이상한 소리만 하는군",
          "original_form": "이상한 소리만 하는군",
          "speaker_id": "2",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042476.1.8",
          "form": "암튼 낙지 곱창 새우 들어간 전골인데",
          "original_form": "암튼 낙지 곱창 새우 들어간 전골인데",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042476.1.9",
          "form": "맛있어요 ㅋㅋㅋ",
          "original_form": "맛있어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:23"
        },
        {
          "id": "MDRW2100042476.1.10",
          "form": "낙지랑 곱창이랑 새우가 들어가는데 맛이 없을수가 있나",
          "original_form": "낙지랑 곱창이랑 새우가 들어가는데 맛이 없을수가 있나",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042476.1.11",
          "form": "맞는 말이죠 아무래도",
          "original_form": "맞는 말이죠 아무래도",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042476.1.12",
          "form": "근데 엄마는 전골 잘 못끓여",
          "original_form": "근데 엄마는 전골 잘 못끓여",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042476.1.13",
          "form": "엄마가 하는 전골은 아무래도 다 부대찌개가 돼버리니까",
          "original_form": "엄마가 하는 전골은 아무래도 다 부대찌개가 돼버리니까{emoji:…}",
          "speaker_id": "1",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042476.1.14",
          "form": "그정도는 아니야 아무리 그래도",
          "original_form": "그정도는 아니야 아무리 그래도",
          "speaker_id": "2",
          "time": "20210906 22:24"
        },
        {
          "id": "MDRW2100042476.1.15",
          "form": "근데 집에서 전골 비슷한거 먹어본게 그런거밖에 없는걸요",
          "original_form": "근데 집에서 전골 비슷한거 먹어본게 그런거밖에 없는걸요",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042476.1.16",
          "form": "하려면 할수있어",
          "original_form": "하려면 할수있어",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042476.1.17",
          "form": "아까랑 말이 다르신데 ㅋㅋㅋ",
          "original_form": "아까랑 말이 다르신데 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042476.1.18",
          "form": "레시피 검색해서 보내라.",
          "original_form": "레시피 검색해서 보내라.",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042476.1.19",
          "form": "근데 우리집은 고추장이 문제라니까요",
          "original_form": "근데 우리집은 고추장이 문제라니까요",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042476.1.20",
          "form": "이모네 고추장이 왜",
          "original_form": "이모네 고추장이 왜",
          "speaker_id": "2",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042476.1.21",
          "form": "고추장이 너무 달아서 맛이 이상해지잖아요",
          "original_form": "고추장이 너무 달아서 맛이 이상해지잖아요",
          "speaker_id": "1",
          "time": "20210906 22:25"
        },
        {
          "id": "MDRW2100042476.1.22",
          "form": "매실맛도 많이나고",
          "original_form": "매실맛도 많이나고",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042476.1.23",
          "form": "고추장 사먹는게 더 맛있긴 하지 아무래도",
          "original_form": "고추장 사먹는게 더 맛있긴 하지 아무래도",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042476.1.24",
          "form": "있는거 먹지말고 새로 하나 사면 좋을텐데 ㅠ",
          "original_form": "있는거 먹지말고 새로 하나 사면 좋을텐데 ㅠ",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042476.1.25",
          "form": "아깝잖아 글구 이모가 준건데 어떡해",
          "original_form": "아깝잖아 글구 이모가 준건데 어떡해",
          "speaker_id": "2",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042476.1.26",
          "form": "그건 그래요",
          "original_form": "그건 그래요",
          "speaker_id": "1",
          "time": "20210906 22:26"
        },
        {
          "id": "MDRW2100042476.1.27",
          "form": "이모 요리 잘하시는데 엄마랑 그 고추장은 안맞는듯",
          "original_form": "이모 요리 잘하시는데 엄마랑 그 고추장은 안맞는듯",
          "speaker_id": "1",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042476.1.28",
          "form": "엄마는 레시피 보고 만드는 사람이니까",
          "original_form": "엄마는 레시피 보고 만드는 사람이니까",
          "speaker_id": "2",
          "time": "20210906 22:27"
        },
        {
          "id": "MDRW2100042476.1.29",
          "form": "name3 고추장 아니면 맛이 안나나보지",
