      "utterance": [
        {
          "id": "MDRW2100030677.1.1",
          "form": "요즘은 배달이 잘되어서 괜찮아요",
          "original_form": "요즘은 배달이 잘되어서 괜찮아요",
          "speaker_id": "2",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100030677.1.2",
          "form": "어제는 치킨을 먹었습니다",
          "original_form": "어제는 치킨을 먹었습니다",
          "speaker_id": "2",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100030677.1.3",
          "form": "계란 후라이는 해 드시나요",
          "original_form": "계란 후라이는 해 드시나요",
          "speaker_id": "1",
          "time": "20210904 11:17"
        },
        {
          "id": "MDRW2100030677.1.4",
          "form": "전어회가 먹고 싶은데 제가 가는 횟집이 문을 닫았어요",
          "original_form": "전어회가 먹고 싶은데 제가 가는 횟집이 문을 닫았어요",
          "speaker_id": "2",
          "time": "20210904 11:18"
        },
        {
          "id": "MDRW2100030677.1.5",
          "form": "치킨 많이 겠어요",
          "original_form": "치킨 많이 겠어요",
          "speaker_id": "1",
          "time": "20210904 11:18"
        },
        {
          "id": "MDRW2100030677.1.6",
          "form": "다음 주까지 공사 해요",
          "original_form": "다음 주까지 공사 해요",
          "speaker_id": "2",
          "time": "20210904 11:18"
        },
        {
          "id": "MDRW2100030677.1.7",
          "form": "아니요 자주 못 해 먹어요",
          "original_form": "아니요 자주 못 해 먹어요",
          "speaker_id": "2",
          "time": "20210904 11:18"
        },
        {
          "id": "MDRW2100030677.1.8",
          "form": "그 가게 이름이 무엇이죠",
          "original_form": "그 가게 이름이 무엇이죠",
          "speaker_id": "1",
          "time": "20210904 11:18"
        },
        {
          "id": "MDRW2100030677.1.9",
          "form": "금어가인가요",
          "original_form": "금어가인가요",
          "speaker_id": "1",
          "time": "20210904 11:18"
        },
        {
          "id": "MDRW2100030677.1.10",
          "form": "네 맞아요 그 뭐가 횟집이에요",
          "original_form": "네 맞아요 그 뭐가 횟집이에요",
          "speaker_id": "2",
          "time": "20210904 11:18"
        },
        {
          "id": "MDRW2100030677.1.11",
          "form": "전에 아버지가 방문해서 바가지 썼어요",
          "original_form": "전에 아버지가 방문해서 바가지 썼어요",
          "speaker_id": "1",
          "time": "20210904 11:18"
        },
        {
          "id": "MDRW2100030677.1.12",
          "form": "헐 어쩌다가요",
          "original_form": "헐 어쩌다가요",
          "speaker_id": "2",
          "time": "20210904 11:19"
        },
        {
          "id": "MDRW2100030677.1.13",
          "form": "예약해서 갔더니 생선까지 예약하지 않는 않았냐고 하더라고요",
          "original_form": "예약해서 갔더니 생선까지 예약하지 않는 않았냐고 하더라고요",
          "speaker_id": "1",
          "time": "20210904 11:19"
        },
        {
          "id": "MDRW2100030677.1.14",
          "form": "그냥네 명만 예약했다고 했죠",
          "original_form": "그냥네 명만 예약했다고 했죠",
          "speaker_id": "1",
          "time": "20210904 11:19"
        },
        {
          "id": "MDRW2100030677.1.15",
          "form": "그런 일이 있었군요",
          "original_form": "그런 일이 있었군요",
          "speaker_id": "2",
          "time": "20210904 11:19"
        },
        {
          "id": "MDRW2100030677.1.16",
          "form": "근데 이미 물고기를 잡아 났대요",
          "original_form": "근데 이미 물고기를 잡아 났대요",
          "speaker_id": "1",
          "time": "20210904 11:19"
        },
        {
          "id": "MDRW2100030677.1.17",
          "form": "좋지 않은 기억이네요",
          "original_form": "좋지 않은 기억이네요",
          "speaker_id": "2",
          "time": "20210904 11:19"
        },
        {
          "id": "MDRW2100030677.1.18",
          "form": "그래서 그냥 달라고 했죠",
          "original_form": "그래서 그냥 달라고 했죠",
          "speaker_id": "1",
          "time": "20210904 11:19"
        },
        {
          "id": "MDRW2100030677.1.19",
          "form": "13만 원 나왔어요",
          "original_form": "13만 원 나왔어요",
          "speaker_id": "1",
          "time": "20210904 11:19"
        },
        {
          "id": "MDRW2100030677.1.20",
          "form": "어쩔 수 없었네요원하지 않은 음식까지 돈 주고 먹어야 하다니",
          "original_form": "어쩔 수 없었네요원하지 않은 음식까지 돈 주고 먹어야 하다니",
          "speaker_id": "2",
          "time": "20210904 11:19"
        }
