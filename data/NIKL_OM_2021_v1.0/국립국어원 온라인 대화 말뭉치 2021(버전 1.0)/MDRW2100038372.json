      "utterance": [
        {
          "id": "MDRW2100038372.1.1",
          "form": "아이고ㅠㅠ 많이 아파하나요ㅠㅠ",
          "original_form": "아이고ㅠㅠ 많이 아파하나요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 11:52"
        },
        {
          "id": "MDRW2100038372.1.2",
          "form": "그정도는 아닌데 모양이 길지않고 알알이 끊어져 나와요 ㅋㅋㅋㅋ",
          "original_form": "그정도는 아닌데 모양이 길지않고 알알이 끊어져 나와요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:52"
        },
        {
          "id": "MDRW2100038372.1.3",
          "form": "매일 한번씩 싸긴하는데 모양이 맘에 안드는 ...",
          "original_form": "매일 한번씩 싸긴하는데 모양이 맘에 안드는 ...",
          "speaker_id": "1",
          "time": "20210902 11:53"
        },
        {
          "id": "MDRW2100038372.1.4",
          "form": "시원시원하질 못하나봐요 ㅋㅋㅋㅋ",
          "original_form": "시원시원하질 못하나봐요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100038372.1.5",
          "form": "애기들 변비나 방광염일 때 참 어떻게 해줄수도 없고 걱정이에요ㅠ",
          "original_form": "애기들 변비나 방광염일 때 참 어떻게 해줄수도 없고 걱정이에요ㅠ",
          "speaker_id": "2",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100038372.1.6",
          "form": "화장실 힘줄때",
          "original_form": "화장실 힘줄때",
          "speaker_id": "1",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100038372.1.7",
          "form": "수염이 앞으로 모이는데 너무 웃김 ㅋㅋㅋㅋ",
          "original_form": "수염이 앞으로 모이는데 너무 웃김 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:56"
        },
        {
          "id": "MDRW2100038372.1.8",
          "form": "방광염 온적 있나요?",
          "original_form": "방광염 온적 있나요?",
          "speaker_id": "1",
          "time": "20210902 12:00"
        },
        {
          "id": "MDRW2100038372.1.9",
          "form": "네 방광염 때 울고 그랬어요 ㅎㅎ",
          "original_form": "네 방광염 때 울고 그랬어요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 12:01"
        },
        {
          "id": "MDRW2100038372.1.10",
          "form": "그때 우는 소리 처음 들었어요ㅜ",
          "original_form": "그때 우는 소리 처음 들었어요ㅜ",
          "speaker_id": "2",
          "time": "20210902 12:02"
        },
        {
          "id": "MDRW2100038372.1.11",
          "form": "ㅠㅠㅠ 막 들락날락 하는데 못누더라구요 유투브보니",
          "original_form": "ㅠㅠㅠ 막 들락날락 하는데 못누더라구요 유투브보니",
          "speaker_id": "1",
          "time": "20210902 12:12"
        },
        {
          "id": "MDRW2100038372.1.12",
          "form": "어떻게 치료하셨나요",
          "original_form": "어떻게 치료하셨나요",
          "speaker_id": "1",
          "time": "20210902 12:46"
        },
        {
          "id": "MDRW2100038372.1.13",
          "form": "병원도 열심히 다녔고, 물 마시게 물그릇도 수십번을 바꾸고ㅠㅠ",
          "original_form": "병원도 열심히 다녔고, 물 마시게 물그릇도 수십번을 바꾸고ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 15:49"
        },
        {
          "id": "MDRW2100038372.1.14",
          "form": "무료나눔한 물그릇만 스무개는 넘을거에요 ㅎㅎ",
          "original_form": "무료나눔한 물그릇만 스무개는 넘을거에요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 15:50"
        },
        {
          "id": "MDRW2100038372.1.15",
          "form": "ㅠㅠㅠㅠㅠㅠ 결국 그래서 어떤 물그릇이 젤 좋나요",
          "original_form": "ㅠㅠㅠㅠㅠㅠ 결국 그래서 어떤 물그릇이 젤 좋나요",
          "speaker_id": "1",
          "time": "20210902 16:01"
        },
        {
          "id": "MDRW2100038372.1.16",
          "form": "강아지 높이 물그릇으로 바꿨어요ㅠ\r\n덩치에 맞는 걸로 바꾸는게 답이었어요 우리 애는 ㅋㅋㅋㅋ",
          "original_form": "강아지 높이 물그릇으로 바꿨어요ㅠ\r\n덩치에 맞는 걸로 바꾸는게 답이었어요 우리 애는 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:04"
        },
        {
          "id": "MDRW2100038372.1.17",
          "form": "아 너무 낮은게 문제였을까요?",
          "original_form": "아 너무 낮은게 문제였을까요?",
          "speaker_id": "1",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100038372.1.18",
          "form": "그 정수기는 안쓰시나요?",
          "original_form": "그 정수기는 안쓰시나요?",
          "speaker_id": "1",
          "time": "20210902 16:05"
        },
        {
          "id": "MDRW2100038372.1.19",
          "form": "네ㅠ 연꽃정수기랑 몇 개 써봤는데 기계소리를 싫어하는지 처음 몇 번 보고 무시하더라고요 ㅋㅋㅋㅋ",
          "original_form": "네ㅠ 연꽃정수기랑 몇 개 써봤는데 기계소리를 싫어하는지 처음 몇 번 보고 무시하더라고요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:09"
        },
        {
          "id": "MDRW2100038372.1.20",
          "form": "귀엽다 ㅋㅋㅋㅋㅋ 슬프지만 ㅜㅜ",
          "original_form": "귀엽다 ㅋㅋㅋㅋㅋ 슬프지만 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100038372.1.21",
          "form": "저는 인히어런트인가",
          "original_form": "저는 인히어런트인가",
          "speaker_id": "1",
          "time": "20210902 16:10"
        },
        {
          "id": "MDRW2100038372.1.22",
          "form": "이름이 그런거였는데",
          "original_form": "이름이 그런거였는데",
          "speaker_id": "1",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100038372.1.23",
          "form": "씻기가 넘 힘들어소 ㅎㅎ",
          "original_form": "씻기가 넘 힘들어소 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 16:11"
        },
        {
          "id": "MDRW2100038372.1.24",
          "form": "아 그거 씻기 힘들군요",
          "original_form": "아 그거 씻기 힘들군요",
          "speaker_id": "2",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100038372.1.25",
          "form": "오레오 식기였나 보면서 관심 갔었는데 ㅎㅎ",
          "original_form": "오레오 식기였나 보면서 관심 갔었는데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 16:12"
        },
        {
          "id": "MDRW2100038372.1.26",
          "form": "저희 애는 또 사기그릇에만 먹어요 ㅋㅋㅋㅋ",
          "original_form": "저희 애는 또 사기그릇에만 먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:13"
        },
        {
          "id": "MDRW2100038372.1.27",
          "form": "까탈스럽기가... ㅋㅋㅋㅋㅋ",
          "original_form": "까탈스럽기가... ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 16:13"
        }
      ]
