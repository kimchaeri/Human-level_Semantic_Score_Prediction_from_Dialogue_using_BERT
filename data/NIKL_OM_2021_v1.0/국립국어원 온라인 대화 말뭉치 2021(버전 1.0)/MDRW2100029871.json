      "utterance": [
        {
          "id": "MDRW2100029871.1.1",
          "form": "아, 주말부부 오래 했나요?",
          "original_form": "아, 주말부부 오래 했나요?",
          "speaker_id": "1",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100029871.1.2",
          "form": "1년반?",
          "original_form": "1년반?",
          "speaker_id": "2",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100029871.1.3",
          "form": "그때가  좋았습니다",
          "original_form": "그때가  좋았습니다",
          "speaker_id": "2",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100029871.1.4",
          "form": "술도 마음대로 먹고",
          "original_form": "술도 마음대로 먹고",
          "speaker_id": "2",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100029871.1.5",
          "form": "고기도 마음대로 먹고",
          "original_form": "고기도 마음대로 먹고",
          "speaker_id": "2",
          "time": "20210903 15:50"
        },
        {
          "id": "MDRW2100029871.1.6",
          "form": "ㅋㅋㅋ천사도 그 생각하지 않을까요ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ천사도 그 생각하지 않을까요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:51"
        },
        {
          "id": "MDRW2100029871.1.7",
          "form": "그래도 일종의 허니문 기간이라",
          "original_form": "그래도 일종의 허니문 기간이라",
          "speaker_id": "1",
          "time": "20210903 15:51"
        },
        {
          "id": "MDRW2100029871.1.8",
          "form": "그렇게ㅈ식단이 화려했나요!",
          "original_form": "그렇게ㅈ식단이 화려했나요!",
          "speaker_id": "1",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029871.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029871.1.10",
          "form": "이제 나중에는 라면 콘푸라이트 밥에김...",
          "original_form": "이제 나중에는 라면 콘푸라이트 밥에김...",
          "speaker_id": "1",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029871.1.11",
          "form": "그럴겁니다",
          "original_form": "그럴겁니다",
          "speaker_id": "2",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029871.1.12",
          "form": "좋아요",
          "original_form": "좋아요",
          "speaker_id": "2",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029871.1.13",
          "form": "사실 이거 이벤트성이에요",
          "original_form": "사실 이거 이벤트성이에요",
          "speaker_id": "2",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029871.1.14",
          "form": "솥 사서",
          "original_form": "솥 사서",
          "speaker_id": "2",
          "time": "20210903 15:52"
        },
        {
          "id": "MDRW2100029871.1.15",
          "form": "그거 본전뽑을려고",
          "original_form": "그거 본전뽑을려고",
          "speaker_id": "2",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100029871.1.16",
          "form": "해주는거",
          "original_form": "해주는거",
          "speaker_id": "2",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100029871.1.17",
          "form": "아니ㅋㅋ 본전뽑으려면 아직 더해주겠네요ㅋㅋ",
          "original_form": "아니ㅋㅋ 본전뽑으려면 아직 더해주겠네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100029871.1.18",
          "form": "평소에는 매일 ㅅ시켜먹어유",
          "original_form": "평소에는 매일 ㅅ시켜먹어유",
          "speaker_id": "2",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100029871.1.19",
          "form": "한 열번은 더 할듯 ㅋㅋ",
          "original_form": "한 열번은 더 할듯 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100029871.1.20",
          "form": "오오 주로 뭘 시키나요!",
          "original_form": "오오 주로 뭘 시키나요!",
          "speaker_id": "1",
          "time": "20210903 15:53"
        },
        {
          "id": "MDRW2100029871.1.21",
          "form": "주로 한식종류?",
          "original_form": "주로 한식종류?",
          "speaker_id": "2",
          "time": "20210903 15:56"
        },
        {
          "id": "MDRW2100029871.1.22",
          "form": "비빔밥",
          "original_form": "비빔밥",
          "speaker_id": "2",
          "time": "20210903 15:57"
        },
        {
          "id": "MDRW2100029871.1.23",
          "form": "찌개",
          "original_form": "찌개",
          "speaker_id": "2",
          "time": "20210903 15:57"
        },
        {
          "id": "MDRW2100029871.1.24",
          "form": "짜장면",
          "original_form": "짜장면",
          "speaker_id": "2",
          "time": "20210903 15:59"
        },
        {
          "id": "MDRW2100029871.1.25",
          "form": "돌아가면서 배민 박살내고 있습니다",
          "original_form": "돌아가면서 배민 박살내고 있습니다",
          "speaker_id": "2",
          "time": "20210903 15:59"
        },
        {
          "id": "MDRW2100029871.1.26",
          "form": "ㅋㅋㅋ 배민 vip신가요",
          "original_form": "ㅋㅋㅋ 배민 vip신가요",
          "speaker_id": "1",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100029871.1.27",
          "form": "비빔밥도 시켜 먹는군요!잘 오나요",
          "original_form": "비빔밥도 시켜 먹는군요!잘 오나요",
          "speaker_id": "1",
          "time": "20210903 16:16"
        },
        {
          "id": "MDRW2100029871.1.28",
          "form": "생각보다 괜찮습니다 고향의맛!",
          "original_form": "생각보다 괜찮습니다 고향의맛!",
          "speaker_id": "2",
          "time": "20210903 16:24"
        },
        {
          "id": "MDRW2100029871.1.29",
          "form": "다시다!",
          "original_form": "다시다!",
          "speaker_id": "2",
          "time": "20210903 16:25"
        },
        {
          "id": "MDRW2100029871.1.30",
          "form": "ㅋㅋ 다시다는 정말 혁명입니다",
          "original_form": "ㅋㅋ 다시다는 정말 혁명입니다",
          "speaker_id": "1",
          "time": "20210903 16:30"
        },
        {
          "id": "MDRW2100029871.1.31",
          "form": "노벨상 안준 스웨덴아카데미는 반성해야쥬",
          "original_form": "노벨상 안준 스웨덴아카데미는 반성해야쥬",
          "speaker_id": "1",
          "time": "20210903 16:31"
        }
      ]
