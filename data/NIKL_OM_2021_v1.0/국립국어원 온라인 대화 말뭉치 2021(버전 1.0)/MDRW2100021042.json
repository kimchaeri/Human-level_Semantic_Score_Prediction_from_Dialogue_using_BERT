      "utterance": [
        {
          "id": "MDRW2100021042.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100021042.1.2",
          "form": "안녕하새요",
          "original_form": "안녕하새요",
          "speaker_id": "1",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100021042.1.3",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "2",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100021042.1.4",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100021042.1.5",
          "form": "name2님은 뭐드셧어요",
          "original_form": "&name2&님은 뭐드셧어요",
          "speaker_id": "1",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100021042.1.6",
          "form": "?",
          "original_form": "?",
          "speaker_id": "1",
          "time": "20210828 15:20"
        },
        {
          "id": "MDRW2100021042.1.7",
          "form": "전 냉장고에 있던 반찬들로 먹었어요",
          "original_form": "전 냉장고에 있던 반찬들로 먹었어요",
          "speaker_id": "2",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100021042.1.8",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100021042.1.9",
          "form": "오.. 저는 인스턴트 쌀국수 먹엇어요",
          "original_form": "오.. 저는 인스턴트 쌀국수 먹엇어요",
          "speaker_id": "1",
          "time": "20210828 15:21"
        },
        {
          "id": "MDRW2100021042.1.10",
          "form": "쌀국수 평소에 자주 드시나요?",
          "original_form": "쌀국수 평소에 자주 드시나요?",
          "speaker_id": "2",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100021042.1.11",
          "form": "집에 인스턴트 쌀국수가 많이서\r\n한달에 두번정도는 먹어요.",
          "original_form": "집에 인스턴트 쌀국수가 많이서\r\n한달에 두번정도는 먹어요.",
          "speaker_id": "1",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100021042.1.12",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210828 15:22"
        },
        {
          "id": "MDRW2100021042.1.13",
          "form": "저는 인스턴트 쌀국수 한 번도 안 먹어봤어요ㅎㅎ",
          "original_form": "저는 인스턴트 쌀국수 한 번도 안 먹어봤어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021042.1.14",
          "form": "쌀국수 대량으로 사두셨나봐요",
          "original_form": "쌀국수 대량으로 사두셨나봐요",
          "speaker_id": "2",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021042.1.15",
          "form": "오. 국내브랜드 멸치 쌀국수 정도는 초심자가 시도하기좋아요",
          "original_form": "오. 국내브랜드 멸치 쌀국수 정도는 초심자가 시도하기좋아요",
          "speaker_id": "1",
          "time": "20210828 15:23"
        },
        {
          "id": "MDRW2100021042.1.16",
          "form": "마트가면 구매 후 도전해봐야겠네요",
          "original_form": "마트가면 구매 후 도전해봐야겠네요",
          "speaker_id": "2",
          "time": "20210828 15:24"
        },
        {
          "id": "MDRW2100021042.1.17",
          "form": "베트남쌀국수도 먹어봤는데\r\n부모님들은 이국적인 향신료가 들어가서 별로 안좋아하시더라고요.",
          "original_form": "베트남쌀국수도 먹어봤는데\r\n부모님들은 이국적인 향신료가 들어가서 별로 안좋아하시더라고요.",
          "speaker_id": "1",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100021042.1.18",
          "form": "아 저도 향신료 안 좋아하는데 잘 맞지는 않겠네요ㅠㅠ",
          "original_form": "아 저도 향신료 안 좋아하는데 잘 맞지는 않겠네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 15:28"
        },
        {
