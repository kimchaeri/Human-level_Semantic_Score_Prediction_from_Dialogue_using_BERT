      "utterance": [
        {
          "id": "MDRW2100023414.1.1",
          "form": "와 그렇겠네요",
          "original_form": "와 그렇겠네요",
          "speaker_id": "2",
          "time": "20210830 01:47"
        },
        {
          "id": "MDRW2100023414.1.2",
          "form": "부모님이랑 가신거예요?",
          "original_form": "부모님이랑 가신거예요?",
          "speaker_id": "2",
          "time": "20210830 01:47"
        },
        {
          "id": "MDRW2100023414.1.3",
          "form": "부모님 모시고 대중교통 지도보면서 찾을수는 없어서ㅠㅠ",
          "original_form": "부모님 모시고 대중교통 지도보면서 찾을수는 없어서ㅠㅠ",
          "speaker_id": "1",
          "time": "20210830 01:47"
        },
        {
          "id": "MDRW2100023414.1.4",
          "form": "네네! 패키지는 부모님이랑 갔는데",
          "original_form": "네네! 패키지는 부모님이랑 갔는데",
          "speaker_id": "1",
          "time": "20210830 01:48"
        },
        {
          "id": "MDRW2100023414.1.5",
          "form": "맞아요 길거리에서 길잃으면 식은땀나고..",
          "original_form": "맞아요 길거리에서 길잃으면 식은땀나고..",
          "speaker_id": "2",
          "time": "20210830 01:48"
        },
        {
          "id": "MDRW2100023414.1.6",
          "form": "나중에는 일행들끼리 친해져서",
          "original_form": "나중에는 일행들끼리 친해져서",
          "speaker_id": "1",
          "time": "20210830 01:48"
        },
        {
          "id": "MDRW2100023414.1.7",
          "form": "부모님은 사람들이랑 어울리고 재밌게 노셨어요ㅋㅋㅋ",
          "original_form": "부모님은 사람들이랑 어울리고 재밌게 노셨어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:48"
        },
        {
          "id": "MDRW2100023414.1.8",
          "form": "좋네요~~ 부모님이 인싸셨군요 ㅎㅎ",
          "original_form": "좋네요~~ 부모님이 인싸셨군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 01:48"
        },
        {
          "id": "MDRW2100023414.1.9",
          "form": "네넼ㅋㅋ 맞아요 저는 구석에서 쭈구리...",
          "original_form": "네넼ㅋㅋ 맞아요 저는 구석에서 쭈구리...",
          "speaker_id": "1",
          "time": "20210830 01:48"
        },
        {
          "id": "MDRW2100023414.1.10",
          "form": "ㅎㅎ 그럼 대만여행 가게되면",
          "original_form": "ㅎㅎ 그럼 대만여행 가게되면",
          "speaker_id": "2",
          "time": "20210830 01:48"
        },
        {
          "id": "MDRW2100023414.1.11",
          "form": "자유여행으로 가실거예요?",
          "original_form": "자유여행으로 가실거예요?",
          "speaker_id": "2",
          "time": "20210830 01:49"
        },
        {
          "id": "MDRW2100023414.1.12",
          "form": "그래도 뭔가 유동적이고 자유를 누리고싶으면 자유여행을 추천합니당",
          "original_form": "그래도 뭔가 유동적이고 자유를 누리고싶으면 자유여행을 추천합니당",
          "speaker_id": "1",
          "time": "20210830 01:49"
        },
        {
          "id": "MDRW2100023414.1.13",
          "form": "네네!",
          "original_form": "네네!",
          "speaker_id": "1",
          "time": "20210830 01:49"
        },
        {
          "id": "MDRW2100023414.1.14",
          "form": "가고싶은 곳이나 드시고 싶으신게",
          "original_form": "가고싶은 곳이나 드시고 싶으신게",
          "speaker_id": "2",
          "time": "20210830 01:49"
        },
        {
          "id": "MDRW2100023414.1.15",
          "form": "있으세요?",
          "original_form": "있으세요?",
          "speaker_id": "2",
          "time": "20210830 01:49"
        },
        {
          "id": "MDRW2100023414.1.16",
          "form": "패키지는 한번 해봐서...ㅋㅋㅋ",
          "original_form": "패키지는 한번 해봐서...ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:49"
        },
        {
          "id": "MDRW2100023414.1.17",
          "form": "저 펑리수도 먹고싶고",
          "original_form": "저 펑리수도 먹고싶고",
          "speaker_id": "1",
          "time": "20210830 01:49"
        },
        {
          "id": "MDRW2100023414.1.18",
          "form": "망고빙수랑",
          "original_form": "망고빙수랑",
          "speaker_id": "1",
          "time": "20210830 01:49"
        },
        {
          "id": "MDRW2100023414.1.19",
          "form": "곱창국수랑",
          "original_form": "곱창국수랑",
          "speaker_id": "1",
          "time": "20210830 01:49"
        },
        {
          "id": "MDRW2100023414.1.20",
          "form": "펑리수가 뭐에요? 망고빙수는 먹어봤는데",
          "original_form": "펑리수가 뭐에요? 망고빙수는 먹어봤는데",
          "speaker_id": "2",
          "time": "20210830 01:49"
        },
        {
          "id": "MDRW2100023414.1.21",
          "form": "아 너무 많네욬ㅋㅋㅋ",
          "original_form": "아 너무 많네욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 01:49"
        },
        {
          "id": "MDRW2100023414.1.22",
          "form": "가보시면 샤오롱바오도 한번 드셔보세요",
          "original_form": "가보시면 샤오롱바오도 한번 드셔보세요",
          "speaker_id": "2",
          "time": "20210830 01:49"
        },
        {
          "id": "MDRW2100023414.1.23",
          "form": "안에 파인애플 쨈? 같은거 들어간 대만 과자에용!",
          "original_form": "안에 파인애플 쨈? 같은거 들어간 대만 과자에용!",
          "speaker_id": "1",
          "time": "20210830 01:49"
        },
        {
          "id": "MDRW2100023414.1.24",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100023414.1.25",
          "form": "약간 모나카같은....?",
          "original_form": "약간 모나카같은....?",
          "speaker_id": "1",
          "time": "20210830 01:50"
        },
        {
          "id": "MDRW2100023414.1.26",
          "form": "먹은거 같아요",
          "original_form": "먹은거 같아요",
          "speaker_id": "2",
          "time": "20210830 01:50"
        }
      ]
    }
