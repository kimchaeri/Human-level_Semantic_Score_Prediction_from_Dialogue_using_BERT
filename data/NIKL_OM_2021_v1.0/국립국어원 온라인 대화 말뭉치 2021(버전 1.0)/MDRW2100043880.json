      "utterance": [
        {
          "id": "MDRW2100043880.1.1",
          "form": "좋아하는 음료는 있으신가용?",
          "original_form": "좋아하는 음료는 있으신가용?",
          "speaker_id": "1",
          "time": "20210907 20:43"
        },
        {
          "id": "MDRW2100043880.1.2",
          "form": "거의 다 좋아하는데",
          "original_form": "거의 다 좋아하는데",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043880.1.3",
          "form": "커피랑 차를 많이마셔요!",
          "original_form": "커피랑 차를 많이마셔요!",
          "speaker_id": "2",
          "time": "20210907 20:44"
        },
        {
          "id": "MDRW2100043880.1.4",
          "form": "그럼 어떤 브랜드 가장좋아하시나요?",
          "original_form": "그럼 어떤 브랜드 가장좋아하시나요?",
          "speaker_id": "1",
          "time": "20210907 20:45"
        },
        {
          "id": "MDRW2100043880.1.5",
          "form": "프랜차이즈요?",
          "original_form": "프랜차이즈요?",
          "speaker_id": "2",
          "time": "20210907 20:46"
        },
        {
          "id": "MDRW2100043880.1.6",
          "form": "넴넴",
          "original_form": "넴넴",
          "speaker_id": "1",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043880.1.7",
          "form": "생활권에 있어서 스타벅스 많이갔었는데",
          "original_form": "생활권에 있어서 스타벅스 많이갔었는데",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043880.1.8",
          "form": "이래저래 이슈ㅎㅎ가 많아서",
          "original_form": "이래저래 이슈ㅎㅎ가 많아서",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043880.1.9",
          "form": "최근엔 그냥 직접 드립해서먹거나 하고있어요!",
          "original_form": "최근엔 그냥 직접 드립해서먹거나 하고있어요!",
          "speaker_id": "2",
          "time": "20210907 20:47"
        },
        {
          "id": "MDRW2100043880.1.10",
          "form": "아하 원두 사다가 갈아서 드세요?!",
          "original_form": "아하 원두 사다가 갈아서 드세요?!",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043880.1.11",
          "form": "할리스 좋아하긴하는데 주변에 없네요ㅜㅜ",
          "original_form": "할리스 좋아하긴하는데 주변에 없네요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043880.1.12",
          "form": "네맞아요!",
          "original_form": "네맞아요!",
          "speaker_id": "2",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043880.1.13",
          "form": "할리스는 정말 잘없죠 ㅠ",
          "original_form": "할리스는 정말 잘없죠 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043880.1.14",
          "form": "주로 어디서 원두 사세요?",
          "original_form": "주로 어디서 원두 사세요?",
          "speaker_id": "1",
          "time": "20210907 20:48"
        },
        {
          "id": "MDRW2100043880.1.15",
          "form": "정착못하고 여기저기서 사보는중이에요ㅜㅜ",
          "original_form": "정착못하고 여기저기서 사보는중이에요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043880.1.16",
          "form": "노브랜드에서도 사보고 개인판매자것도 사보고",
          "original_form": "노브랜드에서도 사보고 개인판매자것도 사보고",
          "speaker_id": "2",
          "time": "20210907 20:49"
        },
        {
          "id": "MDRW2100043880.1.17",
          "form": "커피 좋아하세요?",
          "original_form": "커피 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043880.1.18",
          "form": "네 ㅎㅎ 좋아하죠!",
          "original_form": "네 ㅎㅎ 좋아하죠!",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043880.1.19",
          "form": "저는 모카포트 사서 하는중인데",
          "original_form": "저는 모카포트 사서 하는중인데",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043880.1.20",
          "form": "항상 원가 모카포트에하면 맛이달라서 ㅠ",
          "original_form": "항상 원가 모카포트에하면 맛이달라서 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043880.1.21",
          "form": "슬퍼요",
          "original_form": "슬퍼요",
          "speaker_id": "1",
          "time": "20210907 20:50"
        },
        {
          "id": "MDRW2100043880.1.22",
          "form": "하ㅏㅏ진짜",
          "original_form": "하ㅏㅏ진짜",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043880.1.23",
          "form": "모카포트너무힘들어요",
          "original_form": "모카포트너무힘들어요",
          "speaker_id": "2",
          "time": "20210907 20:51"
        },
        {
          "id": "MDRW2100043880.1.24",
          "form": "세척도힘들고",
          "original_form": "세척도힘들고",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043880.1.25",
          "form": "저도몇번써봤는데 진짜 맛맞추기도 어렵고ㅜㅜ",
          "original_form": "저도몇번써봤는데 진짜 맛맞추기도 어렵고ㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 20:52"
        },
        {
          "id": "MDRW2100043880.1.26",
          "form": "아... 그건그래요 ㅠ",
          "original_form": "아... 그건그래요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043880.1.27",
          "form": "세척은 그렇다해요",
          "original_form": "세척은 그렇다해요",
          "speaker_id": "1",
          "time": "20210907 20:53"
        },
        {
          "id": "MDRW2100043880.1.28",
          "form": "맛맞추기가 넘 어려워요 ㅠ",
          "original_form": "맛맞추기가 넘 어려워요 ㅠ",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043880.1.29",
          "form": "해요->해도",
          "original_form": "해요->해도",
          "speaker_id": "1",
          "time": "20210907 20:54"
        },
        {
          "id": "MDRW2100043880.1.30",
          "form": "저는 홀빈써서그런지 그라인더부터 잘못된거같긴한데",
          "original_form": "저는 홀빈써서그런지 그라인더부터 잘못된거같긴한데",
          "speaker_id": "2",
          "time": "20210907 20:55"
        }
      ]
    }
