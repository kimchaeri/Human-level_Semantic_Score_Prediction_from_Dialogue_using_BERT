      "utterance": [
        {
          "id": "MDRW2100008197.1.1",
          "form": "공연은 코시국이라 무리고 영화는 봤어요! ㅋㅋ",
          "original_form": "공연은 코시국이라 무리고 영화는 봤어요! ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 13:57"
        },
        {
          "id": "MDRW2100008197.1.2",
          "form": "무슨영화보셨어요?",
          "original_form": "무슨영화보셨어요?",
          "speaker_id": "2",
          "time": "20210822 14:06"
        },
        {
          "id": "MDRW2100008197.1.3",
          "form": "저는 최근 블랙 위도우 봤어요!",
          "original_form": "저는 최근 블랙 위도우 봤어요!",
          "speaker_id": "1",
          "time": "20210822 17:18"
        },
        {
          "id": "MDRW2100008197.1.4",
          "form": "위도우 잼나나요?",
          "original_form": "위도우 잼나나요?",
          "speaker_id": "2",
          "time": "20210822 23:32"
        },
        {
          "id": "MDRW2100008197.1.5",
          "form": "마블은 항상 평타이상인거 같아요 ㅎㅅㄴ",
          "original_form": "마블은 항상 평타이상인거 같아요 ㅎㅅㄴ",
          "speaker_id": "1",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100008197.1.6",
          "form": "ㅎㅎ",
          "original_form": "ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100008197.1.7",
          "form": "그렇긴하죠",
          "original_form": "그렇긴하죠",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100008197.1.8",
          "form": "네이버시리즈온에 1위긴하네요",
          "original_form": "네이버시리즈온에 1위긴하네요",
          "speaker_id": "2",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100008197.1.9",
          "form": "이걸봐야하나 보스베이비를볼까 고민중이요 ㅎ",
          "original_form": "이걸봐야하나 보스베이비를볼까 고민중이요 ㅎ",
          "speaker_id": "2",
          "time": "20210822 23:40"
        },
        {
          "id": "MDRW2100008197.1.10",
          "form": "전 영화관에서 큰화면으로 봐서 만족했어요",
          "original_form": "전 영화관에서 큰화면으로 봐서 만족했어요",
          "speaker_id": "1",
          "time": "20210822 23:46"
        },
        {
          "id": "MDRW2100008197.1.11",
          "form": "블록버스터는 그렇게 봐야한다며 ㅋㅋ",
          "original_form": "블록버스터는 그렇게 봐야한다며 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:46"
        },
        {
          "id": "MDRW2100008197.1.12",
          "form": "하긴 마블은 그렇게봐야 제맛이쥬",
          "original_form": "하긴 마블은 그렇게봐야 제맛이쥬",
          "speaker_id": "2",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100008197.1.13",
          "form": "이해가 어렵지 않아서 좋았어요",
          "original_form": "이해가 어렵지 않아서 좋았어요",
          "speaker_id": "1",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100008197.1.14",
          "form": "마블시리즈 다 안봐도",
          "original_form": "마블시리즈 다 안봐도",
          "speaker_id": "1",
          "time": "20210822 23:51"
        },
        {
          "id": "MDRW2100008197.1.15",
          "form": "생각없이 킬링타임용으로 딱인거네요",
          "original_form": "생각없이 킬링타임용으로 딱인거네요",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100008197.1.16",
          "form": "그건 참 좋은거같아요",
          "original_form": "그건 참 좋은거같아요",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100008197.1.17",
          "form": "앞에 꼭봐야하는것들도 있는데 안봐도되니요",
          "original_form": "앞에 꼭봐야하는것들도 있는데 안봐도되니요",
          "speaker_id": "2",
          "time": "20210822 23:52"
        },
        {
          "id": "MDRW2100008197.1.18",
          "form": "근데 미묘하게 스포당해서 안타까웠어요 ㅋㅋ",
          "original_form": "근데 미묘하게 스포당해서 안타까웠어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:58"
        },
        {
          "id": "MDRW2100008197.1.19",
          "form": "블랙위도우 캐릭터에대해 ㅜㅜㅜ",
          "original_form": "블랙위도우 캐릭터에대해 ㅜㅜㅜ",
          "speaker_id": "1",
          "time": "20210822 23:59"
        },
        {
          "id": "MDRW2100008197.1.20",
          "form": "스포를 당해요? 스포없이봤음 좋았을껄요",
          "original_form": "스포를 당해요? 스포없이봤음 좋았을껄요",
          "speaker_id": "2",
          "time": "20210823 10:56"
        },
        {
          "id": "MDRW2100008197.1.21",
          "form": "그냠 동생이 지나가는 말로",
          "original_form": "그냠 동생이 지나가는 말로",
          "speaker_id": "1",
          "time": "20210823 10:59"
        },
        {
          "id": "MDRW2100008197.1.22",
          "form": "근데 블래위도우 어떻게 어떻게 된다 이야기해버려서 괜히 으헝ㅜ 말해주지말지 이랬달까 ㅋㅋ",
          "original_form": "근데 블래위도우 어떻게 어떻게 된다 이야기해버려서 괜히 으헝ㅜ 말해주지말지 이랬달까 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 10:59"
        },
        {
          "id": "MDRW2100008197.1.23",
          "form": "안타깝네요 ㅎ",
          "original_form": "안타깝네요 ㅎ",
          "speaker_id": "2",
          "time": "20210823 11:26"
        },
        {
          "id": "MDRW2100008197.1.24",
          "form": "저는 듣기전에 봐야겠어요 ㅎ",
          "original_form": "저는 듣기전에 봐야겠어요 ㅎ",
          "speaker_id": "2",
          "time": "20210823 11:26"
        }
      ]
    }
