      "utterance": [
        {
          "id": "MDRW2100021193.1.1",
          "form": "운동하고계신가요?",
          "original_form": "운동하고계신가요?",
          "speaker_id": "1",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100021193.1.2",
          "form": "오 처음해보는 주제에요",
          "original_form": "오 처음해보는 주제에요",
          "speaker_id": "2",
          "time": "20210828 16:42"
        },
        {
          "id": "MDRW2100021193.1.3",
          "form": "네 최근에 시작했어요!",
          "original_form": "네 최근에 시작했어요!",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021193.1.4",
          "form": "운동하세요?",
          "original_form": "운동하세요?",
          "speaker_id": "2",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021193.1.5",
          "form": "어떤운동 하시나요?",
          "original_form": "어떤운동 하시나요?",
          "speaker_id": "1",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021193.1.6",
          "form": "네 적게나마 하고있어요",
          "original_form": "네 적게나마 하고있어요",
          "speaker_id": "1",
          "time": "20210828 16:43"
        },
        {
          "id": "MDRW2100021193.1.7",
          "form": "저 필라테스요!",
          "original_form": "저 필라테스요!",
          "speaker_id": "2",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100021193.1.8",
          "form": "비싼돈 주고 호흡연습만 하다오ㅏ요 ㅎ",
          "original_form": "비싼돈 주고 호흡연습만 하다오ㅏ요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 16:44"
        },
        {
          "id": "MDRW2100021193.1.9",
          "form": "알수없는 말을 트레이너가 하신다는 그 필라테스군요ㅋㅋㅋㅋ",
          "original_form": "알수없는 말을 트레이너가 하신다는 그 필라테스군요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:47"
        },
        {
          "id": "MDRW2100021193.1.10",
          "form": "제 주변 필라테스하는 친구들 전부 체력이 엄청 좋아졌어요",
          "original_form": "제 주변 필라테스하는 친구들 전부 체력이 엄청 좋아졌어요",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021193.1.11",
          "form": "효과보시겠다",
          "original_form": "효과보시겠다",
          "speaker_id": "1",
          "time": "20210828 16:48"
        },
        {
          "id": "MDRW2100021193.1.12",
          "form": "진짜 뭔 단어들을 쓰시는데 잘 못알아들어요 ㅋㅋㅋㅋ",
          "original_form": "진짜 뭔 단어들을 쓰시는데 잘 못알아들어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021193.1.13",
          "form": "에?... 이런 느낌 ㅋ ㅋㅋㅋㅋ",
          "original_form": "에?... 이런 느낌 ㅋ ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021193.1.14",
          "form": "몸치는 운동도 어렵더라구요,,",
          "original_form": "몸치는 운동도 어렵더라구요,,",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021193.1.15",
          "form": "갈비뼈를 어떻게하시고...",
          "original_form": "갈비뼈를 어떻게하시고...",
          "speaker_id": "1",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021193.1.16",
          "form": "분절!",
          "original_form": "분절!",
          "speaker_id": "2",
          "time": "20210828 16:49"
        },
        {
          "id": "MDRW2100021193.1.17",
          "form": "맞아요!ㅋㅋㅋㅋ",
          "original_form": "맞아요!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021193.1.18",
          "form": "동글게 마는 분절은 잘 되는데",
          "original_form": "동글게 마는 분절은 잘 되는데",
          "speaker_id": "2",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021193.1.19",
          "form": "다시 직선으로 돌아올때는... 잘 모르겠어요 ㅎ",
          "original_form": "다시 직선으로 돌아올때는... 잘 모르겠어요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 16:50"
        },
        {
          "id": "MDRW2100021193.1.20",
          "form": "무슨 운동 하고 계세요?",
          "original_form": "무슨 운동 하고 계세요?",
          "speaker_id": "2",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021193.1.21",
          "form": "저는 링피트 하고있어요",
          "original_form": "저는 링피트 하고있어요",
          "speaker_id": "1",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021193.1.22",
          "form": "우와 그게 뭐에요?",
          "original_form": "우와 그게 뭐에요?",
          "speaker_id": "2",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021193.1.23",
          "form": "닌텐도 스위치를 이용한 운동하는 게임이예요",
          "original_form": "닌텐도 스위치를 이용한 운동하는 게임이예요",
          "speaker_id": "1",
          "time": "20210828 16:51"
        },
        {
          "id": "MDRW2100021193.1.24",
          "form": "아 뭔지 알 것 같아요!",
          "original_form": "아 뭔지 알 것 같아요!",
          "speaker_id": "2",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021193.1.25",
          "form": "한번 찾아보고와볼게요 ㅎ",
          "original_form": "한번 찾아보고와볼게요 ㅎ",
          "speaker_id": "2",
          "time": "20210828 16:52"
        },
        {
          "id": "MDRW2100021193.1.26",
          "form": "제가 게임을 좋아해서 도움이 되더라구요",
          "original_form": "제가 게임을 좋아해서 도움이 되더라구요",
          "speaker_id": "1",
          "time": "20210828 16:53"
        },
        {
          "id": "MDRW2100021193.1.27",
          "form": "재밌을것같아요",
          "original_form": "재밌을것같아요",
          "speaker_id": "2",
          "time": "20210828 16:53"
        }
      ]
    }
  ]
