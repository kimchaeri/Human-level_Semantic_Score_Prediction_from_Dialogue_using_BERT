      "utterance": [
        {
          "id": "MDRW2100029385.1.1",
          "form": "좋아하는 계절이 뭐예요~?",
          "original_form": "좋아하는 계절이 뭐예요~?",
          "speaker_id": "1",
          "time": "20210903 04:05"
        },
        {
          "id": "MDRW2100029385.1.2",
          "form": "겨울 좋아해요!",
          "original_form": "겨울 좋아해요!",
          "speaker_id": "2",
          "time": "20210903 04:10"
        },
        {
          "id": "MDRW2100029385.1.3",
          "form": "헐 저도 겨울 좋아해요~~",
          "original_form": "헐 저도 겨울 좋아해요~~",
          "speaker_id": "1",
          "time": "20210903 04:11"
        },
        {
          "id": "MDRW2100029385.1.4",
          "form": "벌써 9월이라 넘 두근두근하죠",
          "original_form": "벌써 9월이라 넘 두근두근하죠",
          "speaker_id": "1",
          "time": "20210903 04:11"
        },
        {
          "id": "MDRW2100029385.1.5",
          "form": "맞아요! 그렇게생각하니 시간 너무 빠른거같기두하구..",
          "original_form": "맞아요! 그렇게생각하니 시간 너무 빠른거같기두하구..",
          "speaker_id": "2",
          "time": "20210903 04:11"
        },
        {
          "id": "MDRW2100029385.1.6",
          "form": "ㅠㅠ 그건 그래요ㅠㅠ",
          "original_form": "ㅠㅠ 그건 그래요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 04:13"
        },
        {
          "id": "MDRW2100029385.1.7",
          "form": "그치만 저는 여름 진짜 싫어해서... 선선해진 것만으로도 만족해요",
          "original_form": "그치만 저는 여름 진짜 싫어해서... 선선해진 것만으로도 만족해요",
          "speaker_id": "1",
          "time": "20210903 04:13"
        },
        {
          "id": "MDRW2100029385.1.8",
          "form": "맞아요 너무 습하구..",
          "original_form": "맞아요 너무 습하구..",
          "speaker_id": "2",
          "time": "20210903 04:14"
        },
        {
          "id": "MDRW2100029385.1.9",
          "form": "맞아요ㅠㅠ 장마 기간에는 계속 우산 들고 다녀야 하구",
          "original_form": "맞아요ㅠㅠ 장마 기간에는 계속 우산 들고 다녀야 하구",
          "speaker_id": "1",
          "time": "20210903 04:14"
        },
        {
          "id": "MDRW2100029385.1.10",
          "form": "맞아요 맞아요..",
          "original_form": "맞아요 맞아요..",
          "speaker_id": "2",
          "time": "20210903 04:19"
        },
        {
          "id": "MDRW2100029385.1.11",
          "form": "여름도 끝나가는데 요즘엔 이상하게 비가 쏟아지더라구요ㅠ!!",
          "original_form": "여름도 끝나가는데 요즘엔 이상하게 비가 쏟아지더라구요ㅠ!!",
          "speaker_id": "1",
          "time": "20210903 04:20"
        },
        {
          "id": "MDRW2100029385.1.12",
          "form": "저희 지역은 오늘도 비 왔는데 그쪽은 어때요?",
          "original_form": "저희 지역은 오늘도 비 왔는데 그쪽은 어때요?",
          "speaker_id": "1",
          "time": "20210903 04:20"
        },
        {
          "id": "MDRW2100029385.1.13",
          "form": "여기도 비 엄청 왔어요!!",
          "original_form": "여기도 비 엄청 왔어요!!",
          "speaker_id": "2",
          "time": "20210903 04:22"
        },
        {
          "id": "MDRW2100029385.1.14",
          "form": "오다가 안오다가 계속 반복하구..",
          "original_form": "오다가 안오다가 계속 반복하구..",
          "speaker_id": "2",
          "time": "20210903 04:22"
        },
        {
          "id": "MDRW2100029385.1.15",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 04:22"
        },
        {
          "id": "MDRW2100029385.1.16",
          "form": "근데 꼭 우산 챙기면 비가 안 와서ㅠㅋㅋ",
          "original_form": "근데 꼭 우산 챙기면 비가 안 와서ㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 04:22"
        },
        {
          "id": "MDRW2100029385.1.17",
          "form": "그러니까요 ㅠㅠㅠㅠ",
          "original_form": "그러니까요 ㅠㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210903 04:23"
        },
        {
          "id": "MDRW2100029385.1.18",
          "form": "저두 아침에 학교갈때 비 안오길래 그냥 갔는데",
          "original_form": "저두 아침에 학교갈때 비 안오길래 그냥 갔는데",
          "speaker_id": "2",
          "time": "20210903 04:23"
        },
        {
          "id": "MDRW2100029385.1.19",
          "form": "집 갈땐 비 엄청와서 ㅠㅠ",
          "original_form": "집 갈땐 비 엄청와서 ㅠㅠ",
