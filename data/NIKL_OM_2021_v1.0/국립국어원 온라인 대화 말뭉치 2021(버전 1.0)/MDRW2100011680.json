      "utterance": [
        {
          "id": "MDRW2100011680.1.1",
          "form": "비혼을 결심하신 계기가 있나유?",
          "original_form": "비혼을 결심하신 계기가 있나유?",
          "speaker_id": "1",
          "time": "20210824 15:30"
        },
        {
          "id": "MDRW2100011680.1.2",
          "form": "음 솔직히말하자면",
          "original_form": "음 솔직히말하자면",
          "speaker_id": "2",
          "time": "20210824 15:30"
        },
        {
          "id": "MDRW2100011680.1.3",
          "form": "오 네네",
          "original_form": "오 네네",
          "speaker_id": "1",
          "time": "20210824 15:31"
        },
        {
          "id": "MDRW2100011680.1.4",
          "form": "너무 폐쇄적이라는 생각이 들어서요",
          "original_form": "너무 폐쇄적이라는 생각이 들어서요",
          "speaker_id": "2",
          "time": "20210824 15:31"
        },
        {
          "id": "MDRW2100011680.1.5",
          "form": "아닌 사람들도 있겠지만 묶여서 갖혀있는 느낌?",
          "original_form": "아닌 사람들도 있겠지만 묶여서 갖혀있는 느낌?",
          "speaker_id": "2",
          "time": "20210824 15:31"
        },
        {
          "id": "MDRW2100011680.1.6",
          "form": "비혼에 묶인거요?",
          "original_form": "비혼에 묶인거요?",
          "speaker_id": "1",
          "time": "20210824 15:31"
        },
        {
          "id": "MDRW2100011680.1.7",
          "form": "아니요 결혼 자체가",
          "original_form": "아니요 결혼 자체가",
          "speaker_id": "2",
          "time": "20210824 15:31"
        },
        {
          "id": "MDRW2100011680.1.8",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210824 15:31"
        },
        {
          "id": "MDRW2100011680.1.9",
          "form": "사람과 사람을 억지로 묶는 느낌이에요",
          "original_form": "사람과 사람을 억지로 묶는 느낌이에요",
          "speaker_id": "2",
          "time": "20210824 15:31"
        },
        {
          "id": "MDRW2100011680.1.10",
          "form": "좀 기이하긴 해요",
          "original_form": "좀 기이하긴 해요",
          "speaker_id": "1",
          "time": "20210824 15:32"
        },
        {
          "id": "MDRW2100011680.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 15:32"
        },
        {
          "id": "MDRW2100011680.1.12",
          "form": "헤어질 사람도",
          "original_form": "헤어질 사람도",
          "speaker_id": "1",
          "time": "20210824 15:32"
        },
        {
          "id": "MDRW2100011680.1.13",
          "form": "법때문에 못헤어지고..",
          "original_form": "법때문에 못헤어지고..",
          "speaker_id": "1",
          "time": "20210824 15:32"
        },
        {
          "id": "MDRW2100011680.1.14",
          "form": "상대 가족하고 남인데 가족으 되는거고",
          "original_form": "상대 가족하고 남인데 가족으 되는거고",
          "speaker_id": "2",
          "time": "20210824 15:32"
        },
        {
          "id": "MDRW2100011680.1.15",
          "form": "그거 진짜 기괴해요",
          "original_form": "그거 진짜 기괴해요",
          "speaker_id": "1",
          "time": "20210824 15:32"
        },
        {
          "id": "MDRW2100011680.1.16",
          "form": "이혼도 솔직히 말하자면 쉽진않으니까요",
          "original_form": "이혼도 솔직히 말하자면 쉽진않으니까요",
          "speaker_id": "2",
          "time": "20210824 15:32"
        },
        {
          "id": "MDRW2100011680.1.17",
          "form": "맞아요 애라도 있으면 책임을 나눠야하고",
          "original_form": "맞아요 애라도 있으면 책임을 나눠야하고",
          "speaker_id": "1",
          "time": "20210824 15:32"
        },
        {
          "id": "MDRW2100011680.1.18",
          "form": "네 진짜 기괴하고 책임과 의무도 여성에게 더 무겁구요",
          "original_form": "네 진짜 기괴하고 책임과 의무도 여성에게 더 무겁구요",
          "speaker_id": "2",
          "time": "20210824 15:32"
        },
        {
          "id": "MDRW2100011680.1.19",
          "form": "맞아요 요즘은 반반결혼이 대세던데",
          "original_form": "맞아요 요즘은 반반결혼이 대세던데",
          "speaker_id": "1",
          "time": "20210824 15:33"
        },
        {
          "id": "MDRW2100011680.1.20",
          "form": "점점 가성비 챙기는게 보여서",
          "original_form": "점점 가성비 챙기는게 보여서",
          "speaker_id": "2",
          "time": "20210824 15:33"
        },
        {
          "id": "MDRW2100011680.1.21",
          "form": "그걸 굳이 하는거 너무 손해같아보이더라구요",
          "original_form": "그걸 굳이 하는거 너무 손해같아보이더라구요",
          "speaker_id": "1",
          "time": "20210824 15:33"
        },
        {
          "id": "MDRW2100011680.1.22",
          "form": "재수없어요ㅋㅋㅋ",
          "original_form": "재수없어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:33"
        },
        {
          "id": "MDRW2100011680.1.23",
          "form": "맞아요 가성비가 내려요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 가성비가 내려요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:33"
        },
        {
          "id": "MDRW2100011680.1.24",
          "form": "최저가부장제",
          "original_form": "최저가부장제",
          "speaker_id": "1",
