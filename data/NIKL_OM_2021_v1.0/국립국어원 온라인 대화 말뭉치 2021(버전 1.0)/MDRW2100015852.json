      "utterance": [
        {
          "id": "MDRW2100015852.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210826 01:50"
        },
        {
          "id": "MDRW2100015852.1.2",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210826 01:51"
        },
        {
          "id": "MDRW2100015852.1.3",
          "form": "요즘 비 많이와서 너무 습하죠 ㅠ",
          "original_form": "요즘 비 많이와서 너무 습하죠 ㅠ",
          "speaker_id": "2",
          "time": "20210826 01:51"
        },
        {
          "id": "MDRW2100015852.1.4",
          "form": "네 ㅠㅠ 전여기 부산인데 물난리 장난아니네요..",
          "original_form": "네 ㅠㅠ 전여기 부산인데 물난리 장난아니네요..",
          "speaker_id": "1",
          "time": "20210826 01:51"
        },
        {
          "id": "MDRW2100015852.1.5",
          "form": "아 부산 ㅜㅜ 진짜 심각하더라구요 ..",
          "original_form": "아 부산 ㅜㅜ 진짜 심각하더라구요 ..",
          "speaker_id": "2",
          "time": "20210826 01:52"
        },
        {
          "id": "MDRW2100015852.1.6",
          "form": "아직도 그러나요??",
          "original_form": "아직도 그러나요??",
          "speaker_id": "2",
          "time": "20210826 01:52"
        },
        {
          "id": "MDRW2100015852.1.7",
          "form": "네.. 잠긴곳도 많고 그런거같더라구요..!",
          "original_form": "네.. 잠긴곳도 많고 그런거같더라구요..!",
          "speaker_id": "1",
          "time": "20210826 01:54"
        },
        {
          "id": "MDRW2100015852.1.8",
          "form": "지하도로는 길막아둔거봤어요 ㅠㅠ",
          "original_form": "지하도로는 길막아둔거봤어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 01:54"
        },
        {
          "id": "MDRW2100015852.1.9",
          "form": "아이고 ㅠㅠ 진짜 장마때 부산은 매번 피해소식이 들리네요..ㅠㅠ",
          "original_form": "아이고 ㅠㅠ 진짜 장마때 부산은 매번 피해소식이 들리네요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 01:55"
        },
        {
          "id": "MDRW2100015852.1.10",
          "form": "그러니말이에요 ㅜㅜ",
          "original_form": "그러니말이에요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 01:55"
        },
        {
          "id": "MDRW2100015852.1.11",
          "form": "그래도 날자체는 많이 덥진않아서 괜찮은거같아요",
          "original_form": "그래도 날자체는 많이 덥진않아서 괜찮은거같아요",
          "speaker_id": "1",
          "time": "20210826 01:56"
        },
        {
          "id": "MDRW2100015852.1.12",
          "form": "맞아요 그나마 다행이에요",
          "original_form": "맞아요 그나마 다행이에요",
          "speaker_id": "2",
          "time": "20210826 01:56"
        },
        {
          "id": "MDRW2100015852.1.13",
          "form": "갑자기 날씨가 선선해졌더라구요",
          "original_form": "갑자기 날씨가 선선해졌더라구요",
          "speaker_id": "2",
          "time": "20210826 01:56"
        },
        {
          "id": "MDRW2100015852.1.14",
          "form": "어느계절 좋아하세요?",
          "original_form": "어느계절 좋아하세요?",
          "speaker_id": "2",
          "time": "20210826 01:56"
        },
        {
          "id": "MDRW2100015852.1.15",
          "form": "그쵸! 가을이요~ 저는 봄보다도 가을이에요 ㅎㅎ",
          "original_form": "그쵸! 가을이요~ 저는 봄보다도 가을이에요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 01:58"
        },
        {
          "id": "MDRW2100015852.1.16",
          "form": "저두 가을좋아해요! 봄가을둘다 좋아하지만",
          "original_form": "저두 가을좋아해요! 봄가을둘다 좋아하지만",
          "speaker_id": "2",
          "time": "20210826 01:59"
        },
        {
          "id": "MDRW2100015852.1.17",
          "form": "더위 추위 너무 잘타서요 ㅠ",
          "original_form": "더위 추위 너무 잘타서요 ㅠ",
          "speaker_id": "2",
          "time": "20210826 01:59"
        },
        {
          "id": "MDRW2100015852.1.18",
          "form": "아 저두요..!ㅠㅠ",
          "original_form": "아 저두요..!ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 02:00"
        },
        {
          "id": "MDRW2100015852.1.19",
          "form": "이제 곧 가을옷 장만할생각에 두근거려요",
          "original_form": "이제 곧 가을옷 장만할생각에 두근거려요",
          "speaker_id": "1",
          "time": "20210826 02:00"
        },
        {
          "id": "MDRW2100015852.1.20",
          "form": "아 저두요 ㅋㅋㅋ 옷도 가을옷이 뭔가 예쁘고 좋아요",
          "original_form": "아 저두요 ㅋㅋㅋ 옷도 가을옷이 뭔가 예쁘고 좋아요",
          "speaker_id": "2",
          "time": "20210826 02:00"
        },
        {
          "id": "MDRW2100015852.1.21",
          "form": "그리고 산책하능거 좋아해서 바람불면서 선선한 날씨 좋아해요",
          "original_form": "그리고 산책하능거 좋아해서 바람불면서 선선한 날씨 좋아해요",
          "speaker_id": "2",
          "time": "20210826 02:01"
        },
        {
          "id": "MDRW2100015852.1.22",
          "form": "맞아요~ 가을바람냄새 너무좋아해요!!!",
          "original_form": "맞아요~ 가을바람냄새 너무좋아해요!!!",
          "speaker_id": "1",
          "time": "20210826 02:03"
        }
      ]
