      "utterance": [
        {
          "id": "MDRW2100018181.1.1",
          "form": "네 그렇다더라구요 놀랐어요",
          "original_form": "네 그렇다더라구요 놀랐어요",
          "speaker_id": "2",
          "time": "20210826 23:04"
        },
        {
          "id": "MDRW2100018181.1.2",
          "form": "아이돌 안좋아하세여?",
          "original_form": "아이돌 안좋아하세여?",
          "speaker_id": "1",
          "time": "20210826 23:05"
        },
        {
          "id": "MDRW2100018181.1.3",
          "form": "네 저는 딱히 없어요 ㅋㅋㅋ있으세요?",
          "original_form": "네 저는 딱히 없어요 ㅋㅋㅋ있으세요?",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018181.1.4",
          "form": "보통 남돌은 국내에서 다 그런패턴이에요..",
          "original_form": "보통 남돌은 국내에서 다 그런패턴이에요..",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018181.1.5",
          "form": "그래요? 놀랍네요..",
          "original_form": "그래요? 놀랍네요..",
          "speaker_id": "2",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018181.1.6",
          "form": "앨범 발매하면 팬클럽에서 대량 구매하고 \n스트리밍 돌리고..",
          "original_form": "앨범 발매하면 팬클럽에서 대량 구매하고 \n스트리밍 돌리고..",
          "speaker_id": "1",
          "time": "20210826 23:06"
        },
        {
          "id": "MDRW2100018181.1.7",
          "form": "그래서 첫주 음방1위 시키고..",
          "original_form": "그래서 첫주 음방1위 시키고..",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018181.1.8",
          "form": "아 그건 들어봤어요",
          "original_form": "아 그건 들어봤어요",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018181.1.9",
          "form": "거긴 회사에서라던데.. 자세히는 저도 몰라요 ㅋㅋㅋ",
          "original_form": "거긴 회사에서라던데.. 자세히는 저도 몰라요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018181.1.10",
          "form": "이게 중국큰손 싹쓸이가 커서.. 중국멤버들 못빼는거에요..",
          "original_form": "이게 중국큰손 싹쓸이가 커서.. 중국멤버들 못빼는거에요..",
          "speaker_id": "1",
          "time": "20210826 23:07"
        },
        {
          "id": "MDRW2100018181.1.11",
          "form": "방탄은 해외에서 먼저떠서.. \n그런거 안통하죠.. 아마도 잘못된 정보일듯..",
          "original_form": "방탄은 해외에서 먼저떠서.. \n그런거 안통하죠.. 아마도 잘못된 정보일듯..",
          "speaker_id": "1",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018181.1.12",
          "form": "중국사람들 스케일 어마어마하더라구요",
          "original_form": "중국사람들 스케일 어마어마하더라구요",
          "speaker_id": "2",
          "time": "20210826 23:08"
        },
        {
          "id": "MDRW2100018181.1.13",
          "form": "역시 인구가 깡패죠..",
          "original_form": "역시 인구가 깡패죠..",
          "speaker_id": "1",
          "time": "20210826 23:11"
        },
        {
          "id": "MDRW2100018181.1.14",
          "form": "음악은 어서 들으세요?",
          "original_form": "음악은 어서 들으세요?",
          "speaker_id": "1",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018181.1.15",
          "form": "저는 잘 안 듣긴 하는데 멜론이요",
          "original_form": "저는 잘 안 듣긴 하는데 멜론이요",
          "speaker_id": "2",
          "time": "20210826 23:12"
        },
        {
          "id": "MDRW2100018181.1.16",
          "form": "멜론이 제일 유명하죠?",
          "original_form": "멜론이 제일 유명하죠?",
          "speaker_id": "1",
          "time": "20210826 23:13"
        },
        {
          "id": "MDRW2100018181.1.17",
          "form": "맞아요 ㅋㅋㅋ 차트보고 골라듣는 편이에요",
          "original_form": "맞아요 ㅋㅋㅋ 차트보고 골라듣는 편이에요",
          "speaker_id": "2",
          "time": "20210826 23:13"
        }
