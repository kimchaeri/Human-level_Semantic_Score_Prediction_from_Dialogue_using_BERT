        },
        {
          "id": "MDRW2100042465.1.2",
          "form": "고딩때 수학여행 가본게끝이야",
          "original_form": "고딩때 수학여행 가본게끝이야",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042465.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042465.1.4",
          "form": "아직도 기억남",
          "original_form": "아직도 기억남",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042465.1.5",
          "form": "성산어쩌구 헥헥대면서 걸은거..",
          "original_form": "성산어쩌구 헥헥대면서 걸은거..",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042465.1.6",
          "form": "으",
          "original_form": "으",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042465.1.7",
          "form": "성산일출봉",
          "original_form": "성산일출봉",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042465.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042465.1.9",
          "form": "친구랑 가면",
          "original_form": "친구랑 가면",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042465.1.10",
          "form": "맞다맞다ㅋㅋㅋㅋㅋ",
          "original_form": "맞다맞다ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042465.1.11",
          "form": "그런곳 절대 안가지",
          "original_form": "그런곳 절대 안가지",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042465.1.12",
          "form": "나 올해 초에",
          "original_form": "나 올해 초에",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042465.1.13",
          "form": "name3이랑 갔다가",
          "original_form": "&name3&이랑 갔다가",
          "speaker_id": "2",
          "time": "20210906 22:19"
        },
        {
          "id": "MDRW2100042465.1.14",
          "form": "속터질뻔 했잖슴ㅋㅋㅋㅋㅋㅋ",
          "original_form": "속터질뻔 했잖슴ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042465.1.15",
          "form": "헐 둘이?",
          "original_form": "헐 둘이?",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042465.1.16",
          "form": "응응 그 디져트 사렅ㅋㅋㅋㅋㅋㅋ",
          "original_form": "응응 그 디져트 사렅ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042465.1.17",
          "form": "와 남동생이랑 여행간거",
          "original_form": "와 남동생이랑 여행간거",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042465.1.18",
          "form": "왘ㅋㅋㅋㅋㅋ진짜 대단하다",
          "original_form": "왘ㅋㅋㅋㅋㅋ진짜 대단하다",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042465.1.19",
          "form": "우린 가서 뭐하지",
          "original_form": "우린 가서 뭐하지",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042465.1.20",
          "form": "좀 길게 가서",
          "original_form": "좀 길게 가서",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042465.1.21",
          "form": "여유롭게 놀자",
          "original_form": "여유롭게 놀자",
          "speaker_id": "2",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042465.1.22",
          "form": "ㅇㅋㅇㅋ",
          "original_form": "ㅇㅋㅇㅋ",
          "speaker_id": "1",
          "time": "20210906 22:20"
        },
        {
          "id": "MDRW2100042465.1.23",
          "form": "좋다",
          "original_form": "좋다",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042465.1.24",
          "form": "근데 제주도는 차가 없으면 진짜 너무 블편해 ㅠㅓ",
          "original_form": "근데 제주도는 차가 없으면 진짜 너무 블편해 ㅠㅓ",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042465.1.25",
          "form": "차로 가면 15분거린데",
          "original_form": "차로 가면 15분거린데",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042465.1.26",
          "form": "버스타면 1시간 20분 막 이런식도 많아서 ㅠㅠ",
          "original_form": "버스타면 1시간 20분 막 이런식도 많아서 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042465.1.27",
          "form": "아..그럴거같긴 하다 ㅠㅠ",
          "original_form": "아..그럴거같긴 하다 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 22:21"
        },
        {
          "id": "MDRW2100042465.1.28",
          "form": "아니면 우리 면허딴뒤에",
          "original_form": "아니면 우리 면허딴뒤에",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042465.1.29",
          "form": "렌트카 빌려서 갈까",
          "original_form": "렌트카 빌려서 갈까",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042465.1.30",
          "form": "아 근데",
          "original_form": "아 근데",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042465.1.31",
          "form": "근데 그러려면 올해는안될듯...ㅎ",
          "original_form": "근데 그러려면 올해는안될듯...ㅎ",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042465.1.32",
          "form": "제주도에서",
          "original_form": "제주도에서",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042465.1.33",
          "form": "초보들이 사고 엄청 내잖아...ㅎ",
          "original_form": "초보들이 사고 엄청 내잖아...ㅎ",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042465.1.34",
          "form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042465.1.35",
          "form": "글쿠나",
          "original_form": "글쿠나",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042465.1.36",
          "form": "나같은 생각하는사람들이 많나봐",
          "original_form": "나같은 생각하는사람들이 많나봐",
          "speaker_id": "1",
          "time": "20210906 22:22"
        },
        {
          "id": "MDRW2100042465.1.37",
          "form": "막 주차 못해서",
          "original_form": "막 주차 못해서",
          "speaker_id": "2",
          "time": "20210906 22:22"
        },
