      "utterance": [
        {
          "id": "MDRW2100004591.1.1",
          "form": "요즘 날씨가 너무 습해요ㅡ",
          "original_form": "요즘 날씨가 너무 습해요ㅡ",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004591.1.2",
          "form": "비가 많이와요",
          "original_form": "비가 많이와요",
          "speaker_id": "1",
          "time": "20210821 17:20"
        },
        {
          "id": "MDRW2100004591.1.3",
          "form": "마저요 비와서 추운데 축축하기까지햐요",
          "original_form": "마저요 비와서 추운데 축축하기까지햐요",
          "speaker_id": "2",
          "time": "20210821 17:21"
        },
        {
          "id": "MDRW2100004591.1.4",
          "form": "요즘 아침 밤 너무춥지않나요",
          "original_form": "요즘 아침 밤 너무춥지않나요",
          "speaker_id": "2",
          "time": "20210821 17:22"
        },
        {
          "id": "MDRW2100004591.1.5",
          "form": "맞아요 항상 선풍기 틀어놓고 자는편인데",
          "original_form": "맞아요 항상 선풍기 틀어놓고 자는편인데",
          "speaker_id": "1",
          "time": "20210821 17:43"
        },
        {
          "id": "MDRW2100004591.1.6",
          "form": "오늘 아침 너무 추워서 깼어요",
          "original_form": "오늘 아침 너무 추워서 깼어요",
          "speaker_id": "1",
          "time": "20210821 17:43"
        },
        {
          "id": "MDRW2100004591.1.7",
          "form": "덕분에 목이 텁텁한게 기침이 나오네요",
          "original_form": "덕분에 목이 텁텁한게 기침이 나오네요",
          "speaker_id": "1",
          "time": "20210821 17:43"
        },
        {
          "id": "MDRW2100004591.1.8",
          "form": "거기는 비가 많이오나요? 제가 사는쪽은 오늘 비가 좍좍 내리네요ㅠㅠ",
          "original_form": "거기는 비가 많이오나요? 제가 사는쪽은 오늘 비가 좍좍 내리네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210821 17:44"
        },
        {
          "id": "MDRW2100004591.1.9",
          "form": "여기도 비가 좍좍좍내려요....비때문에 본가못갔어요. ..",
          "original_form": "여기도 비가 좍좍좍내려요....비때문에 본가못갔어요. ..",
          "speaker_id": "2",
          "time": "20210821 17:45"
        },
        {
          "id": "MDRW2100004591.1.10",
          "form": "ㅜㅠ 추적추적해서 이런날씨에 이동하면",
          "original_form": "ㅜㅠ 추적추적해서 이런날씨에 이동하면",
          "speaker_id": "1",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004591.1.11",
          "form": "너무 짜증나고 스트레스받죠",
          "original_form": "너무 짜증나고 스트레스받죠",
          "speaker_id": "1",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004591.1.12",
          "form": "곧 장마라고 하는데 걱정이겠어요",
          "original_form": "곧 장마라고 하는데 걱정이겠어요",
          "speaker_id": "1",
          "time": "20210821 17:48"
        },
        {
          "id": "MDRW2100004591.1.13",
          "form": "장마...? 아 너무싫어요 빨래도못하고...",
          "original_form": "장마...? 아 너무싫어요 빨래도못하고...",
          "speaker_id": "2",
          "time": "20210821 17:53"
        },
        {
          "id": "MDRW2100004591.1.14",
          "form": "쿰쿰한냄새도나고...",
          "original_form": "쿰쿰한냄새도나고...",
          "speaker_id": "2",
          "time": "20210821 17:54"
        },
        {
          "id": "MDRW2100004591.1.15",
          "form": "맞아요 맞아요ㅜㅜ 집이 습한편이면 곰팡이피는것도  너무 싫어요",
          "original_form": "맞아요 맞아요ㅜㅜ 집이 습한편이면 곰팡이피는것도  너무 싫어요",
          "speaker_id": "1",
          "time": "20210821 17:55"
        },
        {
          "id": "MDRW2100004591.1.16",
          "form": "곧 태풍도 올 예정이래요",
          "original_form": "곧 태풍도 올 예정이래요",
          "speaker_id": "1",
          "time": "20210821 17:55"
        },
        {
          "id": "MDRW2100004591.1.17",
          "form": "태...풍......? 우째서 그런...",
          "original_form": "태...풍......? 우째서 그런...",
          "speaker_id": "2",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004591.1.18",
          "form": "아이고 집 다날라가것네~~",
          "original_form": "아이고 집 다날라가것네~~",
          "speaker_id": "2",
          "time": "20210821 17:56"
        },
        {
          "id": "MDRW2100004591.1.19",
          "form": "항상 그렇듯 비껴갔으면 하네요ㅋㅋㅋㅋ",
          "original_form": "항상 그렇듯 비껴갔으면 하네요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004591.1.20",
          "form": "계절은 어떤 계절이 좋으신가요?",
          "original_form": "계절은 어떤 계절이 좋으신가요?",
          "speaker_id": "1",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004591.1.21",
          "form": "저는 겨울 사랑해요",
          "original_form": "저는 겨울 사랑해요",
          "speaker_id": "2",
          "time": "20210821 18:13"
        },
        {
          "id": "MDRW2100004591.1.22",
          "form": "얼어죽을거같지만 사랑햐요",
          "original_form": "얼어죽을거같지만 사랑햐요",
          "speaker_id": "2",
          "time": "20210821 18:14"
        },
        {
          "id": "MDRW2100004591.1.23",
          "form": "겨울 포근하고 너무 좋아요",
          "original_form": "겨울 포근하고 너무 좋아요",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004591.1.24",
          "form": "연말과 크리스마스 분위기 생각만해도 즐거워요",
          "original_form": "연말과 크리스마스 분위기 생각만해도 즐거워요",
          "speaker_id": "1",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004591.1.25",
          "form": "크리스마스~~~",
          "original_form": "크리스마스~~~",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004591.1.26",
          "form": "머라이어캐리와함께하는~~~",
          "original_form": "머라이어캐리와함께하는~~~",
          "speaker_id": "2",
          "time": "20210821 18:15"
        },
        {
          "id": "MDRW2100004591.1.27",
          "form": "저 롱패딩 새로산거",
          "original_form": "저 롱패딩 새로산거",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004591.1.28",
          "form": "몇번못입어봐서 너무슬퍼요",
          "original_form": "몇번못입어봐서 너무슬퍼요",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004591.1.29",
          "form": "빨리겨울와서 입고싶어요",
          "original_form": "빨리겨울와서 입고싶어요",
          "speaker_id": "2",
          "time": "20210821 18:16"
        },
        {
          "id": "MDRW2100004591.1.30",
          "form": "올아이원뽀 크리스마스~~~~~",
          "original_form": "올아이원뽀 크리스마스~~~~~",
          "speaker_id": "1",
          "time": "20210821 18:18"
        },
        {
          "id": "MDRW2100004591.1.31",
