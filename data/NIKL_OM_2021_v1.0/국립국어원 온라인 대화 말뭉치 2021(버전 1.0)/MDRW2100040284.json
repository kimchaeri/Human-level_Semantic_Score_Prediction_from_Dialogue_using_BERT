      "utterance": [
        {
          "id": "MDRW2100040284.1.1",
          "form": "이제 배꼽시계가 살짝 올리네요",
          "original_form": "이제 배꼽시계가 살짝 올리네요",
          "speaker_id": "2",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040284.1.2",
          "form": "하지만 아직 밥을 하지 않았어요",
          "original_form": "하지만 아직 밥을 하지 않았어요",
          "speaker_id": "2",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040284.1.3",
          "form": "얼른 밥부터 하세요",
          "original_form": "얼른 밥부터 하세요",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040284.1.4",
          "form": "쿠쿠 버튼을 눌러야 할 텐데",
          "original_form": "쿠쿠 버튼을 눌러야 할 텐데",
          "speaker_id": "2",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040284.1.5",
          "form": "고 고 레고 레고 빨리 갔다 와요",
          "original_form": "고 고 레고 레고 빨리 갔다 와요",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040284.1.6",
          "form": "시선은은 낫거든요",
          "original_form": "시선은은 낫거든요",
          "speaker_id": "2",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040284.1.7",
          "form": "씻어",
          "original_form": "씻어",
          "speaker_id": "2",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040284.1.8",
          "form": "얼른 버튼 누르고 와요",
          "original_form": "얼른 버튼 누르고 와요",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040284.1.9",
          "form": "일어나기가 힘들어요 맛있는 음식을 먹으려면 부지런히 움직여야 해요",
          "original_form": "일어나기가 힘들어요 맛있는 음식을 먹으려면 부지런히 움직여야 해요",
          "speaker_id": "2",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040284.1.10",
          "form": "일 안 할 때 밥 먹어둬야 돼요",
          "original_form": "일 안 할 때 밥 먹어둬야 돼요",
          "speaker_id": "1",
          "time": "20210904 15:26"
        },
        {
          "id": "MDRW2100040284.1.11",
          "form": "궁디야 힘을 내",
          "original_form": "궁디야 힘을 내",
          "speaker_id": "1",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100040284.1.12",
          "form": "저는 일 할 때도 먹고 일 안 할 때도 먹어요",
          "original_form": "저는 일 할 때도 먹고 일 안 할 때도 먹어요",
          "speaker_id": "2",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100040284.1.13",
          "form": "안 먹으면 신경질나요",
          "original_form": "안 먹으면 신경질나요",
          "speaker_id": "2",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100040284.1.14",
          "form": "혹시 그냥 신경질 나는 건 아닐까요",
          "original_form": "혹시 그냥 신경질 나는 건 아닐까요",
          "speaker_id": "1",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100040284.1.15",
          "form": "저녁엔 뭘 먹을 건가요",
          "original_form": "저녁엔 뭘 먹을 건가요",
          "speaker_id": "2",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100040284.1.16",
          "form": "아직 배가 고픈데 옥수수 먹을까 싶어요",
          "original_form": "아직 배가 고픈데 옥수수 먹을까 싶어요",
          "speaker_id": "1",
          "time": "20210904 15:27"
        },
        {
          "id": "MDRW2100040284.1.17",
          "form": "그런 거일 수도 있고요",
          "original_form": "그런 거일 수도 있고요",
          "speaker_id": "2",
          "time": "20210904 15:27"
        }
      ]
