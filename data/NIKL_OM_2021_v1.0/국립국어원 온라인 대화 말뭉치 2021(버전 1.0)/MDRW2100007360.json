      "utterance": [
        {
          "id": "MDRW2100007360.1.1",
          "form": "배스킨라빈스에 가시면 뭘 드시나요",
          "original_form": "배스킨라빈스에 가시면 뭘 드시나요",
          "speaker_id": "1",
          "time": "20210822 01:34"
        },
        {
          "id": "MDRW2100007360.1.2",
          "form": "저 아몬드봉봉이요",
          "original_form": "저 아몬드봉봉이요",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007360.1.3",
          "form": "최애 맛이에요!",
          "original_form": "최애 맛이에요!",
          "speaker_id": "2",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007360.1.4",
          "form": "그거 맛있더라구요",
          "original_form": "그거 맛있더라구요",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007360.1.5",
          "form": "저도 최근에 먹어봤는데 씹는맛이 좋아서 좋아햐게됐어요",
          "original_form": "저도 최근에 먹어봤는데 씹는맛이 좋아서 좋아햐게됐어요",
          "speaker_id": "1",
          "time": "20210822 01:36"
        },
        {
          "id": "MDRW2100007360.1.6",
          "form": "그쵸그쵸",
          "original_form": "그쵸그쵸",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007360.1.7",
          "form": "자모카 아몬드 훠지 맛 아세요?",
          "original_form": "자모카 아몬드 훠지 맛 아세요?",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007360.1.8",
          "form": "그것도 진짜 맛있는데",
          "original_form": "그것도 진짜 맛있는데",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007360.1.9",
          "form": "헉",
          "original_form": "헉",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007360.1.10",
          "form": "저 그것도 좋아해요",
          "original_form": "저 그것도 좋아해요",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007360.1.11",
          "form": "단종됐다는 말이 있더라고요",
          "original_form": "단종됐다는 말이 있더라고요",
          "speaker_id": "2",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007360.1.12",
          "form": "약간 아봉이란 비슷",
          "original_form": "약간 아봉이란 비슷",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007360.1.13",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210822 01:38"
        },
        {
          "id": "MDRW2100007360.1.14",
          "form": "단종이라니",
          "original_form": "단종이라니",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007360.1.15",
          "form": "믿을 수 없어",
          "original_form": "믿을 수 없어",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007360.1.16",
          "form": "제발 아니었으면 좋겠어요ㅋㅋㅠㅜ",
          "original_form": "제발 아니었으면 좋겠어요ㅋㅋㅠㅜ",
          "speaker_id": "2",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007360.1.17",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007360.1.18",
          "form": "그 맛있는걸 왜",
          "original_form": "그 맛있는걸 왜",
          "speaker_id": "1",
          "time": "20210822 01:39"
        },
        {
          "id": "MDRW2100007360.1.19",
          "form": "그럼 혹시",
          "original_form": "그럼 혹시",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007360.1.20",
          "form": "민초파신가요",
          "original_form": "민초파신가요",
          "speaker_id": "1",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007360.1.21",
          "form": "헉..전..반민초입니다...",
          "original_form": "헉..전..반민초입니다...",
          "speaker_id": "2",
          "time": "20210822 01:40"
        },
        {
          "id": "MDRW2100007360.1.22",
          "form": "저도 사실 반민초입니다",
          "original_form": "저도 사실 반민초입니다",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007360.1.23",
          "form": "ㅎㅎㅎ 사실 민초 왜 먹는지 모르겠습니다",
          "original_form": "ㅎㅎㅎ 사실 민초 왜 먹는지 모르겠습니다",
          "speaker_id": "1",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007360.1.24",
          "form": "ㅋㅋㄱㅋㅋㅋㅋㅋㄱㅋㅋㅋㄱ아 그죠그죠",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋㅋㄱㅋㅋㅋㄱ아 그죠그죠",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
          "id": "MDRW2100007360.1.25",
          "form": "치약맛이라구요~~",
          "original_form": "치약맛이라구요~~",
          "speaker_id": "2",
          "time": "20210822 01:41"
        },
        {
