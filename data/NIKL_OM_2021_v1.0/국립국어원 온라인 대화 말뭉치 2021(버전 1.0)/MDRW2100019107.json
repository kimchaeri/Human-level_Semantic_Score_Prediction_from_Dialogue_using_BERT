      "utterance": [
        {
          "id": "MDRW2100019107.1.1",
          "form": "저는 간단하게 집밥 먹었어요",
          "original_form": "저는 간단하게 집밥 먹었어요",
          "speaker_id": "2",
          "time": "20210827 15:18"
        },
        {
          "id": "MDRW2100019107.1.2",
          "form": "부지런하시네용",
          "original_form": "부지런하시네용",
          "speaker_id": "1",
          "time": "20210827 15:18"
        },
        {
          "id": "MDRW2100019107.1.3",
          "form": "뭐드셨어요??",
          "original_form": "뭐드셨어요??",
          "speaker_id": "2",
          "time": "20210827 15:18"
        },
        {
          "id": "MDRW2100019107.1.4",
          "form": "저는 구내식당에서 주는대로ㅋㅋ",
          "original_form": "저는 구내식당에서 주는대로ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:19"
        },
        {
          "id": "MDRW2100019107.1.5",
          "form": "구내식당 메뉴 생각 안 해도되고 좋죠 ㅋㅋㅋ",
          "original_form": "구내식당 메뉴 생각 안 해도되고 좋죠 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 15:20"
        },
        {
          "id": "MDRW2100019107.1.6",
          "form": "맞아요 매끼 고민하는것도 수고스러운 일이라서",
          "original_form": "맞아요 매끼 고민하는것도 수고스러운 일이라서",
          "speaker_id": "1",
          "time": "20210827 15:23"
        },
        {
          "id": "MDRW2100019107.1.7",
          "form": "감사히 먹고 있어요",
          "original_form": "감사히 먹고 있어요",
          "speaker_id": "1",
          "time": "20210827 15:23"
        },
        {
          "id": "MDRW2100019107.1.8",
          "form": "보통 무슨 메뉴가 많이 나오나요??",
          "original_form": "보통 무슨 메뉴가 많이 나오나요??",
          "speaker_id": "2",
          "time": "20210827 15:24"
        },
        {
          "id": "MDRW2100019107.1.9",
          "form": "그냥 밥 국 반찬",
          "original_form": "그냥 밥 국 반찬",
          "speaker_id": "1",
          "time": "20210827 15:24"
        },
        {
          "id": "MDRW2100019107.1.10",
          "form": "고루고루 나올거같아요",
          "original_form": "고루고루 나올거같아요",
          "speaker_id": "2",
          "time": "20210827 15:24"
        },
        {
          "id": "MDRW2100019107.1.11",
          "form": "복날에 삼계탕정도 나와요",
          "original_form": "복날에 삼계탕정도 나와요",
          "speaker_id": "1",
          "time": "20210827 15:24"
        },
        {
          "id": "MDRW2100019107.1.12",
          "form": "오 나름 센스는 있네요",
          "original_form": "오 나름 센스는 있네요",
          "speaker_id": "2",
          "time": "20210827 15:24"
        },
        {
          "id": "MDRW2100019107.1.13",
          "form": "마자용 저번에는 동지 팥죽도 나왔었어요",
          "original_form": "마자용 저번에는 동지 팥죽도 나왔었어요",
          "speaker_id": "1",
          "time": "20210827 15:25"
        },
        {
          "id": "MDRW2100019107.1.14",
