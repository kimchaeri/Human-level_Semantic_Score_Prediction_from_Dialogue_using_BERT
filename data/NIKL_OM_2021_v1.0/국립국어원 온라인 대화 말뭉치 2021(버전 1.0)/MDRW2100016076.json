      "utterance": [
        {
          "id": "MDRW2100016076.1.1",
          "form": "운동 하시는거 있나요",
          "original_form": "운동 하시는거 있나요",
          "speaker_id": "1",
          "time": "20210826 09:14"
        },
        {
          "id": "MDRW2100016076.1.2",
          "form": "예전에 수영 했었어요~ name1님은요?",
          "original_form": "예전에 수영 했었어요~ &name1&님은요?",
          "speaker_id": "2",
          "time": "20210826 11:00"
        },
        {
          "id": "MDRW2100016076.1.3",
          "form": "저는 요가랑 걷기 자주해요",
          "original_form": "저는 요가랑 걷기 자주해요",
          "speaker_id": "1",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100016076.1.4",
          "form": "수영 어디까지 배우셨나요?",
          "original_form": "수영 어디까지 배우셨나요?",
          "speaker_id": "1",
          "time": "20210826 12:11"
        },
        {
          "id": "MDRW2100016076.1.5",
          "form": "접영까지요 근데 접영은 잘 못해요",
          "original_form": "접영까지요 근데 접영은 잘 못해요",
          "speaker_id": "2",
          "time": "20210826 12:29"
        },
        {
          "id": "MDRW2100016076.1.6",
          "form": "와 오래 배우셨네요",
          "original_form": "와 오래 배우셨네요",
          "speaker_id": "1",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100016076.1.7",
          "form": "자유형 배웠는데 시간지나니 안되더라구요",
          "original_form": "자유형 배웠는데 시간지나니 안되더라구요",
          "speaker_id": "1",
          "time": "20210826 12:35"
        },
        {
          "id": "MDRW2100016076.1.8",
          "form": "그러셨군요 ㅠㅠ",
          "original_form": "그러셨군요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016076.1.9",
          "form": "호흡이 잘안되세요?",
          "original_form": "호흡이 잘안되세요?",
          "speaker_id": "2",
          "time": "20210826 12:43"
        },
        {
          "id": "MDRW2100016076.1.10",
          "form": "음 동작부터가 틀려먹은거 같아요",
          "original_form": "음 동작부터가 틀려먹은거 같아요",
          "speaker_id": "1",
          "time": "20210826 13:00"
        },
        {
          "id": "MDRW2100016076.1.11",
          "form": "배울때도 잘 못했는데 학교실기시험이라 억지로 통과했어요",
          "original_form": "배울때도 잘 못했는데 학교실기시험이라 억지로 통과했어요",
          "speaker_id": "1",
          "time": "20210826 13:01"
        },
        {
          "id": "MDRW2100016076.1.12",
          "form": "몸에 힘이 들어가셨나봐요",
          "original_form": "몸에 힘이 들어가셨나봐요",
          "speaker_id": "2",
          "time": "20210826 13:06"
        },
        {
          "id": "MDRW2100016076.1.13",
          "form": "음ㅎ원래 몸치라서 동작을 잘 못따라해서 그런거같아요",
          "original_form": "음ㅎ원래 몸치라서 동작을 잘 못따라해서 그런거같아요",
          "speaker_id": "1",
          "time": "20210826 13:20"
        },
        {
          "id": "MDRW2100016076.1.14",
          "form": "저도 몸치라 남일같지않네요",
          "original_form": "저도 몸치라 남일같지않네요",
          "speaker_id": "2",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100016076.1.15",
          "form": "몸으로 하는걸 잘 못따라해요",
          "original_form": "몸으로 하는걸 잘 못따라해요",
          "speaker_id": "2",
          "time": "20210826 13:36"
        },
        {
          "id": "MDRW2100016076.1.16",
          "form": "그래서 스포츠 잘 못해요 스쿼시도 배울랬는데 잘 안되더라구요",
          "original_form": "그래서 스포츠 잘 못해요 스쿼시도 배울랬는데 잘 안되더라구요",
          "speaker_id": "1",
          "time": "20210826 13:43"
        },
        {
          "id": "MDRW2100016076.1.17",
          "form": "그러셨군요. 그나마 수영이 오래한걸로 메꿀 수 있을거에요",
          "original_form": "그러셨군요. 그나마 수영이 오래한걸로 메꿀 수 있을거에요",
          "speaker_id": "2",
          "time": "20210826 13:53"
        },
        {
          "id": "MDRW2100016076.1.18",
          "form": "배우는 초반에요 아주머니가 샤워실까지 쫓아와서 제 동작 지적할만큼 못했는데요 자꾸 연습하니 이제는 잘해보인단 말 가끔 들어요",
          "original_form": "배우는 초반에요 아주머니가 샤워실까지 쫓아와서 제 동작 지적할만큼 못했는데요 자꾸 연습하니 이제는 잘해보인단 말 가끔 들어요",
          "speaker_id": "2",
          "time": "20210826 13:54"
        }
      ]
