      "utterance": [
        {
          "id": "MDRW2100001867.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210819 21:49"
        },
        {
          "id": "MDRW2100001867.1.2",
          "form": "알바하시나요??",
          "original_form": "알바하시나요??",
          "speaker_id": "2",
          "time": "20210819 21:50"
        },
        {
          "id": "MDRW2100001867.1.3",
          "form": "저는 재택근무를 하고있습니다~",
          "original_form": "저는 재택근무를 하고있습니다~",
          "speaker_id": "1",
          "time": "20210819 21:50"
        },
        {
          "id": "MDRW2100001867.1.4",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210819 21:50"
        },
        {
          "id": "MDRW2100001867.1.5",
          "form": "저는 키즈카페 알바하고 있어요!",
          "original_form": "저는 키즈카페 알바하고 있어요!",
          "speaker_id": "2",
          "time": "20210819 21:50"
        },
        {
          "id": "MDRW2100001867.1.6",
          "form": "우와~~ 일은 많이 힘든가요~?",
          "original_form": "우와~~ 일은 많이 힘든가요~?",
          "speaker_id": "1",
          "time": "20210819 21:51"
        },
        {
          "id": "MDRW2100001867.1.7",
          "form": "살짝 힘들어요 ㅋㅋㅋ",
          "original_form": "살짝 힘들어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 21:52"
        },
        {
          "id": "MDRW2100001867.1.8",
          "form": "애들이 많이 힘들게안하나여ㅠㅠ?",
          "original_form": "애들이 많이 힘들게안하나여ㅠㅠ?",
          "speaker_id": "1",
          "time": "20210819 21:52"
        },
        {
          "id": "MDRW2100001867.1.9",
          "form": "애들은 괜찮은데 부모님들이 하하",
          "original_form": "애들은 괜찮은데 부모님들이 하하",
          "speaker_id": "2",
          "time": "20210819 21:52"
        },
        {
          "id": "MDRW2100001867.1.10",
          "form": "허걱ㅠㅠ 부모님들이 유독 심하게 스트레스를 주나여ㅠ??",
          "original_form": "허걱ㅠㅠ 부모님들이 유독 심하게 스트레스를 주나여ㅠ??",
          "speaker_id": "1",
          "time": "20210819 21:54"
        },
        {
          "id": "MDRW2100001867.1.11",
          "form": "네 ㅜ 키즈카페라 부모님 진상이.,",
          "original_form": "네 ㅜ 키즈카페라 부모님 진상이.,",
          "speaker_id": "2",
          "time": "20210819 21:54"
        },
        {
          "id": "MDRW2100001867.1.12",
          "form": "흐ㅠㅠ 말만들어도 싫네요ㅠㅠ",
          "original_form": "흐ㅠㅠ 말만들어도 싫네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001867.1.13",
          "form": "컴플에인을 많이ㅜ거나요ㅠ?",
          "original_form": "컴플에인을 많이ㅜ거나요ㅠ?",
          "speaker_id": "1",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001867.1.14",
          "form": "네 ㅜ 트램펄린쪽 키즈카페라서 양말필수인데 신으라해도 안신고",
          "original_form": "네 ㅜ 트램펄린쪽 키즈카페라서 양말필수인데 신으라해도 안신고",
          "speaker_id": "2",
          "time": "20210819 21:55"
        },
        {
          "id": "MDRW2100001867.1.15",
          "form": "에구ㅠㅠ 한번말할때 들으면 좋겠지만ㅠㅠ",
          "original_form": "에구ㅠㅠ 한번말할때 들으면 좋겠지만ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100001867.1.16",
          "form": "말해도 안듣는분들때매 답답하겠어요!",
          "original_form": "말해도 안듣는분들때매 답답하겠어요!",
          "speaker_id": "1",
          "time": "20210819 22:00"
        },
        {
          "id": "MDRW2100001867.1.17",
          "form": "맞아요 ㅠㅠㅠ 그냥 양말 하나가지고 ㅠㅠ",
          "original_form": "맞아요 ㅠㅠㅠ 그냥 양말 하나가지고 ㅠㅠ",
