      "utterance": [
        {
          "id": "MDRW2100036718.1.1",
          "form": "name2님은 운동하시는 거 있으신가요?",
          "original_form": "&name2&님은 운동하시는 거 있으신가요?",
          "speaker_id": "1",
          "time": "20210831 10:20"
        },
        {
          "id": "MDRW2100036718.1.2",
          "form": "네 링피트 해요",
          "original_form": "네 링피트 해요",
          "speaker_id": "2",
          "time": "20210831 10:28"
        },
        {
          "id": "MDRW2100036718.1.3",
          "form": "오늘 백신맞느라...운동 못하겠지만요",
          "original_form": "오늘 백신맞느라...운동 못하겠지만요",
          "speaker_id": "2",
          "time": "20210831 10:28"
        },
        {
          "id": "MDRW2100036718.1.4",
          "form": "우와 링피트 !!!",
          "original_form": "우와 링피트 !!!",
          "speaker_id": "1",
          "time": "20210831 11:04"
        },
        {
          "id": "MDRW2100036718.1.5",
          "form": "닌텐도 있으셔서 링피트도 하시는군요",
          "original_form": "닌텐도 있으셔서 링피트도 하시는군요",
          "speaker_id": "1",
          "time": "20210831 11:04"
        },
        {
          "id": "MDRW2100036718.1.6",
          "form": "부럽습니다,,",
          "original_form": "부럽습니다,,",
          "speaker_id": "1",
          "time": "20210831 11:04"
        },
        {
          "id": "MDRW2100036718.1.7",
          "form": "오늘 백신 몇 시에 맞으세요? 긴장되시겠어요",
          "original_form": "오늘 백신 몇 시에 맞으세요? 긴장되시겠어요",
          "speaker_id": "1",
          "time": "20210831 11:04"
        },
        {
          "id": "MDRW2100036718.1.8",
          "form": "세시예요!",
          "original_form": "세시예요!",
          "speaker_id": "2",
          "time": "20210831 11:05"
        },
        {
          "id": "MDRW2100036718.1.9",
          "form": "오늘 하루 푹쉬려구요",
          "original_form": "오늘 하루 푹쉬려구요",
          "speaker_id": "2",
          "time": "20210831 11:05"
        },
        {
          "id": "MDRW2100036718.1.10",
          "form": "오오 저도 지난 금요일 세시에 맞았었는데 ㅎㅎ",
          "original_form": "오오 저도 지난 금요일 세시에 맞았었는데 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 11:09"
        },
        {
          "id": "MDRW2100036718.1.11",
          "form": "친구들이 백신 맞고나서",
          "original_form": "친구들이 백신 맞고나서",
          "speaker_id": "1",
          "time": "20210831 11:09"
        },
        {
          "id": "MDRW2100036718.1.12",
          "form": "반응이 정말 천차만별이었어서",
          "original_form": "반응이 정말 천차만별이었어서",
          "speaker_id": "1",
          "time": "20210831 11:09"
        },
        {
          "id": "MDRW2100036718.1.13",
          "form": "긴장 잔뜩했는데 저는 정말 괜찮았어요 ㅎㅎ",
          "original_form": "긴장 잔뜩했는데 저는 정말 괜찮았어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 11:09"
        },
        {
          "id": "MDRW2100036718.1.14",
          "form": "저도 괜찮았으면 좋겠어요",
          "original_form": "저도 괜찮았으면 좋겠어요",
          "speaker_id": "2",
          "time": "20210831 11:18"
        },
        {
          "id": "MDRW2100036718.1.15",
          "form": "얼른 일주일지나서 링피트 마저하고 싶어요",
          "original_form": "얼른 일주일지나서 링피트 마저하고 싶어요",
          "speaker_id": "2",
          "time": "20210831 11:18"
        },
        {
          "id": "MDRW2100036718.1.16",
          "form": "링피트 많이 재미있나보네요!!",
          "original_form": "링피트 많이 재미있나보네요!!",
          "speaker_id": "1",
          "time": "20210831 12:15"
        },
        {
          "id": "MDRW2100036718.1.17",
          "form": "저는 운동하려고",
          "original_form": "저는 운동하려고",
          "speaker_id": "1",
          "time": "20210831 12:15"
        },
        {
          "id": "MDRW2100036718.1.18",
          "form": "저스트댄스만 핸드폰에 설치했답니다 ㅎㅎ",
          "original_form": "저스트댄스만 핸드폰에 설치했답니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 12:15"
        },
        {
          "id": "MDRW2100036718.1.19",
          "form": "은근 운동되더라구요",
          "original_form": "은근 운동되더라구요",
          "speaker_id": "1",
          "time": "20210831 12:15"
        },
        {
          "id": "MDRW2100036718.1.20",
          "form": "운동을 안하면 아침몸이 무겁고",
          "original_form": "운동을 안하면 아침몸이 무겁고",
          "speaker_id": "2",
          "time": "20210831 12:17"
        },
        {
          "id": "MDRW2100036718.1.21",
          "form": "밥먹으면 너무 졸려서요ㅠㅠ",
          "original_form": "밥먹으면 너무 졸려서요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210831 12:17"
        },
        {
          "id": "MDRW2100036718.1.22",
          "form": "기분도 쳐지구요",
          "original_form": "기분도 쳐지구요",
          "speaker_id": "2",
          "time": "20210831 12:17"
        },
        {
          "id": "MDRW2100036718.1.23",
          "form": "아 맞아요",
          "original_form": "아 맞아요",
          "speaker_id": "1",
          "time": "20210831 12:52"
        },
        {
          "id": "MDRW2100036718.1.24",
          "form": "원래 우울할 때 일부러",
          "original_form": "원래 우울할 때 일부러",
          "speaker_id": "1",
          "time": "20210831 12:53"
        },
        {
          "id": "MDRW2100036718.1.25",
          "form": "운동하라는 처방 내리기도 한다니까...!",
          "original_form": "운동하라는 처방 내리기도 한다니까...!",
          "speaker_id": "1",
          "time": "20210831 12:53"
        },
        {
          "id": "MDRW2100036718.1.26",
          "form": "링피트 안해봐서 잘 모르는데",
          "original_form": "링피트 안해봐서 잘 모르는데",
          "speaker_id": "1",
          "time": "20210831 12:53"
        },
        {
          "id": "MDRW2100036718.1.27",
          "form": "스토리라인이 있나요?",
          "original_form": "스토리라인이 있나요?",
          "speaker_id": "1",
          "time": "20210831 12:53"
        },
        {
          "id": "MDRW2100036718.1.28",
          "form": "단순하긴해도 스토리 라인이 있어요",
          "original_form": "단순하긴해도 스토리 라인이 있어요",
          "speaker_id": "2",
          "time": "20210831 12:53"
        },
        {
          "id": "MDRW2100036718.1.29",
          "form": "약간 슈퍼마리오정도의 스토리라인 느낌이예요",
          "original_form": "약간 슈퍼마리오정도의 스토리라인 느낌이예요",
          "speaker_id": "2",
          "time": "20210831 12:54"
        },
        {
          "id": "MDRW2100036718.1.30",
          "form": "퀘스트를 깨는거군요!",
          "original_form": "퀘스트를 깨는거군요!",
          "speaker_id": "1",
          "time": "20210831 12:54"
        },
        {
          "id": "MDRW2100036718.1.31",
          "form": "요즘은 게임으로도 운동할 수 있어서",
          "original_form": "요즘은 게임으로도 운동할 수 있어서",
          "speaker_id": "1",
          "time": "20210831 12:54"
        },
        {
          "id": "MDRW2100036718.1.32",
          "form": "너무 괜찮네요",
          "original_form": "너무 괜찮네요",
          "speaker_id": "1",
          "time": "20210831 12:54"
        },
        {
          "id": "MDRW2100036718.1.33",
          "form": "그죠그죠",
          "original_form": "그죠그죠",
          "speaker_id": "2",
          "time": "20210831 12:54"
        },
        {
          "id": "MDRW2100036718.1.34",
          "form": "보스를 깨부수는 재미까지 줘서",
          "original_form": "보스를 깨부수는 재미까지 줘서",
          "speaker_id": "2",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100036718.1.35",
          "form": "더 재밌어요",
          "original_form": "더 재밌어요",
          "speaker_id": "2",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100036718.1.36",
          "form": "오 보스까지 ㅋㅋㅋㅋ",
          "original_form": "오 보스까지 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100036718.1.37",
          "form": "보스랑 싸우려면",
          "original_form": "보스랑 싸우려면",
          "speaker_id": "1",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100036718.1.38",
          "form": "근력 운동 많이 필요할 것 같아요 ㅋㅋㅋㅋ",
          "original_form": "근력 운동 많이 필요할 것 같아요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100036718.1.39",
          "form": "맞아요ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100036718.1.40",
          "form": "끝판보스 깨는데",
          "original_form": "끝판보스 깨는데",
          "speaker_id": "2",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100036718.1.41",
          "form": "한시간 반 운동했어요",
          "original_form": "한시간 반 운동했어요",
          "speaker_id": "2",
          "time": "20210831 12:55"
        },
        {
          "id": "MDRW2100036718.1.42",
          "form": "저세요?",
          "original_form": "저세요?",
          "speaker_id": "2",
          "time": "20210901 14:30"
        },
        {
          "id": "MDRW2100036718.1.43",
          "form": "저번주에 안과다녀왔어요 결막염으로요...",
          "original_form": "저번주에 안과다녀왔어요 결막염으로요...",
          "speaker_id": "2",
