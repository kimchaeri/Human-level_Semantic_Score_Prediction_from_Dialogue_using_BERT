      "utterance": [
        {
          "id": "MDRW2100039261.1.1",
          "form": "넷플릭스에 디피 켰다가 껐어요",
          "original_form": "넷플릭스에 디피 켰다가 껐어요",
          "speaker_id": "2",
          "time": "20210903 15:35"
        },
        {
          "id": "MDRW2100039261.1.2",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100039261.1.3",
          "form": "다른 거 봐요오",
          "original_form": "다른 거 봐요오",
          "speaker_id": "1",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100039261.1.4",
          "form": "장난스런키스켰어요",
          "original_form": "장난스런키스켰어요",
          "speaker_id": "2",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100039261.1.5",
          "form": "유치잼",
          "original_form": "유치잼",
          "speaker_id": "2",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100039261.1.6",
          "form": "ㅋㅋㅋㅋ영화? 드라마?",
          "original_form": "ㅋㅋㅋㅋ영화? 드라마?",
          "speaker_id": "1",
          "time": "20210903 18:36"
        },
        {
          "id": "MDRW2100039261.1.7",
          "form": "영화 봤어요 !!",
          "original_form": "영화 봤어요 !!",
          "speaker_id": "2",
          "time": "20210906 16:00"
        },
        {
          "id": "MDRW2100039261.1.8",
          "form": "우리 대화 있는줄 몰랐어",
          "original_form": "우리 대화 있는줄 몰랐어",
          "speaker_id": "2",
          "time": "20210906 16:00"
        },
        {
          "id": "MDRW2100039261.1.9",
          "form": "왕대륙 나오는거",
          "original_form": "왕대륙 나오는거",
          "speaker_id": "2",
          "time": "20210906 16:00"
        },
        {
          "id": "MDRW2100039261.1.10",
          "form": "ㅋㅋㅋㅋ아 왕대륙 잘생겨서 봤어요",
          "original_form": "ㅋㅋㅋㅋ아 왕대륙 잘생겨서 봤어요",
          "speaker_id": "1",
          "time": "20210906 16:15"
        },
        {
          "id": "MDRW2100039261.1.11",
          "form": "진짜 유치했지만",
          "original_form": "진짜 유치했지만",
          "speaker_id": "1",
          "time": "20210906 16:15"
        },
        {
          "id": "MDRW2100039261.1.12",
          "form": "저도요 ㅋㅋㅋㅋ 왕대륙 보려고 봐요 ...",
          "original_form": "저도요 ㅋㅋㅋㅋ 왕대륙 보려고 봐요 ...",
          "speaker_id": "2",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100039261.1.13",
          "form": "일할때도 툴어놓고 밥먹을때도 틀어놔요 ...",
          "original_form": "일할때도 툴어놓고 밥먹을때도 틀어놔요 ...",
          "speaker_id": "2",
          "time": "20210906 16:54"
        },
        {
          "id": "MDRW2100039261.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100039261.1.15",
          "form": "여신강림 넥스트이군요",
          "original_form": "여신강림 넥스트이군요",
          "speaker_id": "1",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100039261.1.16",
          "form": "아 구러네요 기억력 짱",
          "original_form": "아 구러네요 기억력 짱",
          "speaker_id": "2",
          "time": "20210906 18:40"
        },
        {
          "id": "MDRW2100039261.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 18:41"
        },
        {
          "id": "MDRW2100039261.1.18",
          "form": "이런 영상 있나요? 자주 보고 또 보는?",
          "original_form": "이런 영상 있나요? 자주 보고 또 보는?",
          "speaker_id": "2",
          "time": "20210906 18:41"
        },
        {
          "id": "MDRW2100039261.1.19",
          "form": "전 같은 건 잘 안 봐여 ㅋㅋㅋㅋ",
          "original_form": "전 같은 건 잘 안 봐여 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 19:02"
        },
        {
          "id": "MDRW2100039261.1.20",
          "form": "아 본 건 또 안보나보네요",
          "original_form": "아 본 건 또 안보나보네요",
          "speaker_id": "2",
          "time": "20210906 19:02"
        },
        {
          "id": "MDRW2100039261.1.21",
          "form": "우리엄마도 그래요 ㅋㅋ",
          "original_form": "우리엄마도 그래요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:02"
        },
        {
          "id": "MDRW2100039261.1.22",
          "form": "본거 또 보냐고 ㅋㅋㅋ",
          "original_form": "본거 또 보냐고 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:03"
        },
        {
          "id": "MDRW2100039261.1.23",
          "form": "ㅋㅋㅋㅋ네 세상에 재밌는게 많은데",
          "original_form": "ㅋㅋㅋㅋ네 세상에 재밌는게 많은데",
          "speaker_id": "1",
          "time": "20210906 19:03"
        },
        {
          "id": "MDRW2100039261.1.24",
          "form": "굳이 같은걸 ㅋㅋㅋㅋ",
          "original_form": "굳이 같은걸 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100039261.1.25",
          "form": "옷도 깔별로 안사시겠다!",
          "original_form": "옷도 깔별로 안사시겠다!",
          "speaker_id": "2",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100039261.1.26",
          "form": "세상에 예쁜게 많자나요 그쵸!",
          "original_form": "세상에 예쁜게 많자나요 그쵸!",
          "speaker_id": "2",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100039261.1.27",
          "form": "옷은 깔별로 사요",
          "original_form": "옷은 깔별로 사요",
          "speaker_id": "1",
          "time": "20210906 19:05"
        },
        {
          "id": "MDRW2100039261.1.28",
