      "utterance": [
        {
          "id": "MDRW2100012044.1.1",
          "form": "제가 양으로는 어디 가서 안 꿀리는데.",
          "original_form": "제가 양으로는 어디 가서 안 꿀리는데.",
          "speaker_id": "1",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012044.1.2",
          "form": "시킨 전어 양이 진짜 많아요ㅎㅎ",
          "original_form": "시킨 전어 양이 진짜 많아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 18:43"
        },
        {
          "id": "MDRW2100012044.1.3",
          "form": "배달료 3500원이 안 아깝다",
          "original_form": "배달료 3500원이 안 아깝다",
          "speaker_id": "1",
          "time": "20210824 18:44"
        },
        {
          "id": "MDRW2100012044.1.4",
          "form": "와ㅠㅠㅠ 한입 먹고싶네요..",
          "original_form": "와ㅠㅠㅠ 한입 먹고싶네요..",
          "speaker_id": "2",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100012044.1.5",
          "form": "사실 전어회 맛 모름...",
          "original_form": "사실 전어회 맛 모름...",
          "speaker_id": "2",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100012044.1.6",
          "form": "기억도 안남...",
          "original_form": "기억도 안남...",
          "speaker_id": "2",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100012044.1.7",
          "form": "ㅎㅎ 그러면 괜히 시도했다 돈 아까울 수도요",
          "original_form": "ㅎㅎ 그러면 괜히 시도했다 돈 아까울 수도요",
          "speaker_id": "1",
          "time": "20210824 18:46"
        },
        {
          "id": "MDRW2100012044.1.8",
          "form": "그냥 누가 회 산다 그러면 곁들이로 드세요",
          "original_form": "그냥 누가 회 산다 그러면 곁들이로 드세요",
          "speaker_id": "1",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100012044.1.9",
          "form": "ㅋㅋㅋ 완전 흐뭇한 저녁을 드시고 계신것 같아 괜히 뿌듯하네욬ㅋㅋ",
          "original_form": "ㅋㅋㅋ 완전 흐뭇한 저녁을 드시고 계신것 같아 괜히 뿌듯하네욬ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100012044.1.10",
          "form": "전어는 호불호 갈리는 메뉴라.",
          "original_form": "전어는 호불호 갈리는 메뉴라.",
          "speaker_id": "1",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100012044.1.11",
          "form": "네네 여한이 없는 저녁입니다",
          "original_form": "네네 여한이 없는 저녁입니다",
          "speaker_id": "1",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100012044.1.12",
          "form": "저도 지금 돼지불고기 맛있어요ㅠㅠㅠ 도톰하게 썰었더니 씹는맛이 예술",
          "original_form": "저도 지금 돼지불고기 맛있어요ㅠㅠㅠ 도톰하게 썰었더니 씹는맛이 예술{emoji:♡}",
          "speaker_id": "2",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100012044.1.13",
          "form": "비오는 저녁이지만 음식으로 행복한저녁ㅋㅋㅋ",
          "original_form": "비오는 저녁이지만 음식으로 행복한저녁ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100012044.1.14",
          "form": "헉 맛나겟어요",
          "original_form": "헉 맛나겟어요",
          "speaker_id": "1",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100012044.1.15",
          "form": "밖에서는 고기가 박해서",
          "original_form": "밖에서는 고기가 박해서",
          "speaker_id": "1",
          "time": "20210824 18:47"
        },
        {
          "id": "MDRW2100012044.1.16",
          "form": "역시 집에서 먹어야 제대로죠ㅎ",
          "original_form": "역시 집에서 먹어야 제대로죠ㅎ",
          "speaker_id": "1",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100012044.1.17",
          "form": "진짜 ㅋㅋㅋㅋ 도톰하게 썬게 신의한수였던것 같아요",
          "original_form": "진짜 ㅋㅋㅋㅋ 도톰하게 썬게 신의한수였던것 같아요",
          "speaker_id": "2",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100012044.1.18",
          "form": "씹는맛 최고",
          "original_form": "씹는맛 최고",
          "speaker_id": "2",
          "time": "20210824 18:48"
        },
        {
          "id": "MDRW2100012044.1.19",
          "form": "입안에 만족할만큼 들어와요 ㅋㅋㅋㅋ",
          "original_form": "입안에 만족할만큼 들어와요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100012044.1.20",
          "form": "아앗... 왜 부럽죠ㅜ",
          "original_form": "아앗... 왜 부럽죠ㅜ",
          "speaker_id": "1",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100012044.1.21",
          "form": "저도 배불리 먹는 중인데도",
          "original_form": "저도 배불리 먹는 중인데도",
          "speaker_id": "1",
          "time": "20210824 18:49"
        },
        {
          "id": "MDRW2100012044.1.22",
          "form": "육해공 고기가 다 다른 매력이 있어 좋네요ㅎ",
          "original_form": "육해공 고기가 다 다른 매력이 있어 좋네요ㅎ",
          "speaker_id": "1",
          "time": "20210824 18:50"
        },
        {
          "id": "MDRW2100012044.1.23",
          "form": "같은 고기인데 생선이랑 돼지는 차이가 있는거 같아요~",
          "original_form": "같은 고기인데 생선이랑 돼지는 차이가 있는거 같아요~",
          "speaker_id": "2",
          "time": "20210824 18:54"
        },
        {
          "id": "MDRW2100012044.1.24",
          "form": "저는 지금 이시간 회가 먹고싶네요 ㅋㅋ",
          "original_form": "저는 지금 이시간 회가 먹고싶네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 18:54"
        },
        {
          "id": "MDRW2100012044.1.25",
          "form": "연어초밥!!",
          "original_form": "연어초밥!!",
          "speaker_id": "2",
          "time": "20210824 18:54"
        },
        {
          "id": "MDRW2100012044.1.26",
          "form": "초밥 좋아하신다 하셨죠ㅎ",
          "original_form": "초밥 좋아하신다 하셨죠ㅎ",
          "speaker_id": "1",
          "time": "20210824 19:11"
        },
        {
          "id": "MDRW2100012044.1.27",
          "form": "초밥은 안 시켜봤는데 궁금하네요",
          "original_form": "초밥은 안 시켜봤는데 궁금하네요",
          "speaker_id": "1",
          "time": "20210824 19:11"
        },
        {
          "id": "MDRW2100012044.1.28",
          "form": "동네에 초밥맛집이 있나요",
          "original_form": "동네에 초밥맛집이 있나요",
          "speaker_id": "1",
          "time": "20210824 19:11"
        },
        {
          "id": "MDRW2100012044.1.29",
          "form": "초밥 지지난주에 포장해왔었는데 맛있더라구요!",
          "original_form": "초밥 지지난주에 포장해왔었는데 맛있더라구요!",
          "speaker_id": "2",
          "time": "20210824 19:11"
        },
        {
          "id": "MDRW2100012044.1.30",
          "form": "그... 생강이랑 마늘 그 이름이뭐져... 락교?",
          "original_form": "그... 생강이랑 마늘 그 이름이뭐져... 락교?",
          "speaker_id": "2",
          "time": "20210824 19:12"
        },
        {
          "id": "MDRW2100012044.1.31",
          "form": "그것도 넉넉하게 챙겨주셔서 완전 맛있게 먹었어요 ㅋㅋ",
          "original_form": "그것도 넉넉하게 챙겨주셔서 완전 맛있게 먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 19:12"
        },
        {
          "id": "MDRW2100012044.1.32",
          "form": "넹넹 백합과 걔 락교요",
          "original_form": "넹넹 백합과 걔 락교요",
          "speaker_id": "1",
          "time": "20210824 19:12"
        },
        {
          "id": "MDRW2100012044.1.33",
          "form": "전 생강초절임 너무 좋아해요ㅎ",
          "original_form": "전 생강초절임 너무 좋아해요ㅎ",
          "speaker_id": "1",
          "time": "20210824 19:12"
        },
        {
          "id": "MDRW2100012044.1.34",
          "form": "생강초절임 잘게 썰어서 밥이랑 섞어서 야매 초밥이나 덮밥 만들어 먹어요ㅎ",
          "original_form": "생강초절임 잘게 썰어서 밥이랑 섞어서 야매 초밥이나 덮밥 만들어 먹어요ㅎ",
          "speaker_id": "1",
