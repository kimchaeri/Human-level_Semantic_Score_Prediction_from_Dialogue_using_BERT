          "speaker_id": "1",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100011239.1.22",
          "form": "계속 개봉",
          "original_form": "계속 개봉",
          "speaker_id": "1",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100011239.1.23",
          "form": "와 설레이시겠어요ㅋㅋ",
          "original_form": "와 설레이시겠어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100011239.1.24",
          "form": "자동차 극장에서 보실거에요?",
          "original_form": "자동차 극장에서 보실거에요?",
          "speaker_id": "2",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100011239.1.25",
          "form": "근데 극장가기가 좀...",
          "original_form": "근데 극장가기가 좀...",
          "speaker_id": "1",
          "time": "20210824 21:57"
        },
        {
          "id": "MDRW2100011239.1.26",
          "form": "자동차극장이 집근처에 있는데",
          "original_form": "자동차극장이 집근처에 있는데",
