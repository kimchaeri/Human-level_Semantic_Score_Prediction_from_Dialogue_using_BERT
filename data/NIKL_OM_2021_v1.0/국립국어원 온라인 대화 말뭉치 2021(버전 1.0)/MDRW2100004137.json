      "utterance": [
        {
          "id": "MDRW2100004137.1.1",
          "form": "안녕하세요 ㅎㅎ",
          "original_form": "안녕하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:05"
        },
        {
          "id": "MDRW2100004137.1.2",
          "form": "네 안녕하세요~",
          "original_form": "네 안녕하세요~",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004137.1.3",
          "form": "당근마켓으로 중고거래 해보신경험 있나요?",
          "original_form": "당근마켓으로 중고거래 해보신경험 있나요?",
          "speaker_id": "2",
          "time": "20210821 15:31"
        },
        {
          "id": "MDRW2100004137.1.4",
          "form": "해봤어요 요번에 왕창요",
          "original_form": "해봤어요 요번에 왕창요",
          "speaker_id": "1",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004137.1.5",
          "form": "첨했어요",
          "original_form": "첨했어요",
          "speaker_id": "1",
          "time": "20210821 15:32"
        },
        {
          "id": "MDRW2100004137.1.6",
          "form": "왕창이라고 하신걸 보니 중고거래의 매력에 빠지셨군요ㅋㅋㅋ",
          "original_form": "왕창이라고 하신걸 보니 중고거래의 매력에 빠지셨군요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004137.1.7",
          "form": "쏠쏠하게 용돈 생기는거 같아 재미나요",
          "original_form": "쏠쏠하게 용돈 생기는거 같아 재미나요",
          "speaker_id": "2",
          "time": "20210821 15:33"
        },
        {
          "id": "MDRW2100004137.1.8",
          "form": "ㅋㅋ 팔게이제 없어요 용돈 벌었어요",
          "original_form": "ㅋㅋ 팔게이제 없어요 용돈 벌었어요",
          "speaker_id": "1",
          "time": "20210821 15:34"
        },
        {
          "id": "MDRW2100004137.1.9",
          "form": "저는 배달음식 시킬때 같이 오는 탄산음료 모았다가 팔고있어요ㅋㅋ",
          "original_form": "저는 배달음식 시킬때 같이 오는 탄산음료 모았다가 팔고있어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:53"
        },
        {
          "id": "MDRW2100004137.1.10",
          "form": "오천원정도에 팔려면 한참 모아야하지만 나름 쏠쏠....",
          "original_form": "오천원정도에 팔려면 한참 모아야하지만 나름 쏠쏠....",
          "speaker_id": "2",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004137.1.11",
          "form": "세상에 대단하세요 ㅎㅎ",
          "original_form": "세상에 대단하세요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004137.1.12",
          "form": "파신돈으로 뭔가 구입할계획 있으신가요?",
          "original_form": "파신돈으로 뭔가 구입할계획 있으신가요?",
          "speaker_id": "2",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004137.1.13",
          "form": "애들 고기 사주고 책사줬어요 ㅎㅎ",
          "original_form": "애들 고기 사주고 책사줬어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 15:54"
        },
        {
          "id": "MDRW2100004137.1.14",
          "form": "많이 버셨어요?",
          "original_form": "많이 버셨어요?",
          "speaker_id": "1",
          "time": "20210821 15:55"
        },
        {
          "id": "MDRW2100004137.1.15",
          "form": "오호 엄청 쏠쏠하네요!!",
          "original_form": "오호 엄청 쏠쏠하네요!!",
          "speaker_id": "2",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004137.1.16",
          "form": "저는 왕창 팔아본적이 없어서 눈에 띄게 번적은 없어요ㅋㅋ",
          "original_form": "저는 왕창 팔아본적이 없어서 눈에 띄게 번적은 없어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004137.1.17",
          "form": "전 귀찮아서 몰아 팔아요",
          "original_form": "전 귀찮아서 몰아 팔아요",
          "speaker_id": "1",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004137.1.18",
          "form": "장난감들인데",
          "original_form": "장난감들인데",
          "speaker_id": "1",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004137.1.19",
          "form": "헐값에 팔았어요 ㅜㅜ",
          "original_form": "헐값에 팔았어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004137.1.20",
          "form": "몰아파는게 나은거 같아요",
          "original_form": "몰아파는게 나은거 같아요",
          "speaker_id": "2",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004137.1.21",
          "form": "그래도 갯수가 많아서 목돈되네요",
          "original_form": "그래도 갯수가 많아서 목돈되네요",
          "speaker_id": "1",
          "time": "20210821 15:57"
        },
        {
          "id": "MDRW2100004137.1.22",
          "form": "뜨문뜨문 파니 안그래도 적은돈 더 티가 안나더라구용ㅎㅎ",
          "original_form": "뜨문뜨문 파니 안그래도 적은돈 더 티가 안나더라구용ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004137.1.23",
          "form": "맞아요 택배도 붙이기 편해요",
          "original_form": "맞아요 택배도 붙이기 편해요",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004137.1.24",
          "form": "중고거래가 살땐 싸게 사서 좋은데 팔땐 또 그만큼 헐값에 팔아야하니 장단점이 있긴 한거 같아요",
          "original_form": "중고거래가 살땐 싸게 사서 좋은데 팔땐 또 그만큼 헐값에 팔아야하니 장단점이 있긴 한거 같아요",
          "speaker_id": "2",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004137.1.25",
          "form": "그리고 금방사라져요 돈이 ㅋㅋ",
          "original_form": "그리고 금방사라져요 돈이 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 15:58"
        },
        {
          "id": "MDRW2100004137.1.26",
          "form": "맞아요 필때 허무해요",
          "original_form": "맞아요 필때 허무해요",
          "speaker_id": "1",
          "time": "20210821 15:58"
        }
      ]
