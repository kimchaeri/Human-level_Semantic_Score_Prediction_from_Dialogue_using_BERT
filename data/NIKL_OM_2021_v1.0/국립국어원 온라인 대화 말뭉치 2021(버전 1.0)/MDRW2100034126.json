      "utterance": [
        {
          "id": "MDRW2100034126.1.1",
          "form": "최근에 쇼핑 하셧어요?",
          "original_form": "최근에 쇼핑 하셧어요?",
          "speaker_id": "2",
          "time": "20210827 16:46"
        },
        {
          "id": "MDRW2100034126.1.2",
          "form": "최근에 핸드폰 바꿨어요!",
          "original_form": "최근에 핸드폰 바꿨어요!",
          "speaker_id": "1",
          "time": "20210827 17:48"
        },
        {
          "id": "MDRW2100034126.1.3",
          "form": "설마 플립??",
          "original_form": "설마 플립??",
          "speaker_id": "2",
          "time": "20210827 17:51"
        },
        {
          "id": "MDRW2100034126.1.4",
          "form": "엇 맞아요 플립 샀습니다..!",
          "original_form": "엇 맞아요 플립 샀습니다..!",
          "speaker_id": "1",
          "time": "20210827 17:53"
        },
        {
          "id": "MDRW2100034126.1.5",
          "form": "저도 삿어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 삿어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:54"
        },
        {
          "id": "MDRW2100034126.1.6",
          "form": "무슨 색사셨나요ㅎㅎ",
          "original_form": "무슨 색사셨나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 18:15"
        },
        {
          "id": "MDRW2100034126.1.7",
          "form": "라벤더요! name1님은요?",
          "original_form": "라벤더요! &name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 18:36"
        },
        {
          "id": "MDRW2100034126.1.8",
          "form": "저는 크림이요!",
          "original_form": "저는 크림이요!",
          "speaker_id": "1",
          "time": "20210827 18:40"
        },
        {
          "id": "MDRW2100034126.1.9",
          "form": "라벤더 고민했는데 크림으로 결정했어요 ..ㅜ",
          "original_form": "라벤더 고민했는데 크림으로 결정했어요 ..ㅜ",
          "speaker_id": "1",
          "time": "20210827 18:40"
        },
        {
          "id": "MDRW2100034126.1.10",
          "form": "크림도 너무 예쁘죠",
          "original_form": "크림도 너무 예쁘죠",
          "speaker_id": "2",
          "time": "20210827 18:41"
        },
        {
          "id": "MDRW2100034126.1.11",
          "form": "꾸미기 너무 좋잖아요 ㅋㅋㅋ",
          "original_form": "꾸미기 너무 좋잖아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 18:41"
        },
        {
          "id": "MDRW2100034126.1.12",
          "form": "그쵸 폰꾸하려고 스티커도 샀어요",
          "original_form": "그쵸 폰꾸하려고 스티커도 샀어요",
          "speaker_id": "1",
          "time": "20210827 18:47"
        },
        {
          "id": "MDRW2100034126.1.13",
          "form": "근데 아직 폰을 못 받았네요... ㅠ 곧 오겠죠 뭐..!",
          "original_form": "근데 아직 폰을 못 받았네요... ㅠ 곧 오겠죠 뭐..!",
          "speaker_id": "1",
          "time": "20210827 18:47"
        },
        {
          "id": "MDRW2100034126.1.14",
          "form": "저도 아직 못받앗어요 ㅠㅠ",
          "original_form": "저도 아직 못받앗어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100034126.1.15",
          "form": "마지막날에 사서 ㅋㅋㅋㅋㅋ",
          "original_form": "마지막날에 사서 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 19:30"
        },
        {
          "id": "MDRW2100034126.1.16",
          "form": "플립 샀다고 자랑했는데.. 친구들한테 후기를 알려줄 수가 없네요..",
          "original_form": "플립 샀다고 자랑했는데.. 친구들한테 후기를 알려줄 수가 없네요..",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100034126.1.17",
          "form": "빨리 왔으면 좋겠어요 ㅎㅎ",
          "original_form": "빨리 왔으면 좋겠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 19:46"
        },
        {
          "id": "MDRW2100034126.1.18",
          "form": "저도 ㅋㅋㅋㅋㅋname1님 플립은 언제 배송된대요?",
          "original_form": "저도 ㅋㅋㅋㅋㅋ&name1&님 플립은 언제 배송된대요?",
          "speaker_id": "2",
          "time": "20210827 19:47"
        },
        {
          "id": "MDRW2100034126.1.19",
          "form": "내일 배송예정이라는데.. 제발 내일이라도 보내줬으면 좋겠어요 ..",
          "original_form": "내일 배송예정이라는데.. 제발 내일이라도 보내줬으면 좋겠어요 ..",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100034126.1.20",
          "form": "name2님은 언제 배송예정이신가요!",
          "original_form": "&name2&님은 언제 배송예정이신가요!",
          "speaker_id": "1",
          "time": "20210827 19:48"
        },
        {
          "id": "MDRW2100034126.1.21",
          "form": "저는 9월 초로 미뤄졋어요 ㅠㅠ",
          "original_form": "저는 9월 초로 미뤄졋어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100034126.1.22",
          "form": "좌절중...",
          "original_form": "좌절중...",
          "speaker_id": "2",
          "time": "20210827 19:49"
        },
        {
          "id": "MDRW2100034126.1.23",
          "form": "헉 정말요...? 근데 저도 그럴 것 같아요..28일이후로 배송이라..내일 보내줄지 잘 모르겠어요",
          "original_form": "헉 정말요...? 근데 저도 그럴 것 같아요..28일이후로 배송이라..내일 보내줄지 잘 모르겠어요",
          "speaker_id": "1",
          "time": "20210827 19:50"
        },
        {
          "id": "MDRW2100034126.1.24",
          "form": "원래 오늘 배송시작될 예정이었는데ㅠㅠ",
          "original_form": "원래 오늘 배송시작될 예정이었는데ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 19:56"
        },
        {
          "id": "MDRW2100034126.1.25",
          "form": "그쵸ㅠㅠ 사전예약이렇게까지 밀릴줄 몰랐네요..",
          "original_form": "그쵸ㅠㅠ 사전예약이렇게까지 밀릴줄 몰랐네요..",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100034126.1.26",
          "form": "저두요 ㅠㅠ 사전예약 사은품 신청 빨리 하고 싶은데 ㅠㅠ",
          "original_form": "저두요 ㅠㅠ 사전예약 사은품 신청 빨리 하고 싶은데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100034126.1.27",
          "form": "그러니까요 ㅠㅠㅠ 9월전에는 받을줄 알았는데 이렇게 배송지연이..ㅠㅠ",
