      "utterance": [
        {
          "id": "MDRW2100034667.1.1",
          "form": "면요리 좋아하세요?",
          "original_form": "면요리 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 17:16"
        },
        {
          "id": "MDRW2100034667.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100034667.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210828 17:17"
        },
        {
          "id": "MDRW2100034667.1.4",
          "form": "완전요..!",
          "original_form": "완전요..!",
          "speaker_id": "1",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100034667.1.5",
          "form": "면 어떤거 제일 좋아하세요?",
          "original_form": "면 어떤거 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210828 17:18"
        },
        {
          "id": "MDRW2100034667.1.6",
          "form": "으아 어렵다",
          "original_form": "으아 어렵다",
          "speaker_id": "2",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100034667.1.7",
          "form": "라면 냉면 짜장면 짬뽕 쌀국수 팟타이 파스타",
          "original_form": "라면 냉면 짜장면 짬뽕 쌀국수 팟타이 파스타",
          "speaker_id": "2",
          "time": "20210828 17:24"
        },
        {
          "id": "MDRW2100034667.1.8",
          "form": "못골라요",
          "original_form": "못골라요",
          "speaker_id": "2",
          "time": "20210828 17:25"
        },
        {
          "id": "MDRW2100034667.1.9",
          "form": "인정합니다... 면요리.. 밀가루..메밀.. 쌀면..",
          "original_form": "인정합니다... 면요리.. 밀가루..메밀.. 쌀면..",
          "speaker_id": "1",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100034667.1.10",
          "form": "끊을 수 없어여..ㅠ",
          "original_form": "끊을 수 없어여..ㅠ",
          "speaker_id": "1",
          "time": "20210828 17:26"
        },
        {
          "id": "MDRW2100034667.1.11",
          "form": "진짜 탄수화물중독이에요",
          "original_form": "진짜 탄수화물중독이에요",
          "speaker_id": "2",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100034667.1.12",
          "form": "라면 중에 뭘 좋아하냐 이런식이면 힘겹게 고를수는 잇어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "라면 중에 뭘 좋아하냐 이런식이면 힘겹게 고를수는 잇어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100034667.1.13",
          "form": "통틀어서 무슨면 좋아하는지는 고를수없어요...",
          "original_form": "통틀어서 무슨면 좋아하는지는 고를수없어요...",
          "speaker_id": "2",
          "time": "20210828 17:29"
        },
        {
          "id": "MDRW2100034667.1.14",
          "form": "다맛잇어 ..ㅠㅠㅠ",
          "original_form": "다맛잇어 ..ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 17:30"
        },
        {
          "id": "MDRW2100034667.1.15",
          "form": "그러니까요.. 면먹고싶네요..",
          "original_form": "그러니까요.. 면먹고싶네요..",
          "speaker_id": "1",
          "time": "20210828 17:37"
        },
        {
          "id": "MDRW2100034667.1.16",
          "form": "name1님은 무슨 면 좋아하세요?",
          "original_form": "&name1&님은 무슨 면 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 17:39"
        },
        {
          "id": "MDRW2100034667.1.17",
          "form": "저는 최애는 파스타면이요..",
          "original_form": "저는 최애는 파스타면이요..",
          "speaker_id": "1",
          "time": "20210828 17:44"
        },
        {
          "id": "MDRW2100034667.1.18",
          "form": "파스타 너무 좋아해서 집에서도 자주 해먹거든요.. 약간 집에서 먹으면 2인분은 먹는 것 같아요..",
          "original_form": "파스타 너무 좋아해서 집에서도 자주 해먹거든요.. 약간 집에서 먹으면 2인분은 먹는 것 같아요..",
          "speaker_id": "1",
          "time": "20210828 17:45"
        },
        {
          "id": "MDRW2100034667.1.19",
          "form": "무슨 파스타 제일 좋아하세요?",
          "original_form": "무슨 파스타 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210828 18:14"
        },
        {
          "id": "MDRW2100034667.1.20",
          "form": "집에서먹으면 원래 다들 2인분 이상 먹는거 아니에요? ㅋㅋㅋㅋㅋㅋ",
          "original_form": "집에서먹으면 원래 다들 2인분 이상 먹는거 아니에요? ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:15"
        },
        {
          "id": "MDRW2100034667.1.21",
          "form": "밖에서 먹는 파스타 양이 너무 작음...ㅋㅋㅋㅋㅋㅋ",
          "original_form": "밖에서 먹는 파스타 양이 너무 작음...ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 18:15"
        },
        {
          "id": "MDRW2100034667.1.22",
          "form": "요즘에는 오일파스타 좋아해요 !!",
          "original_form": "요즘에는 오일파스타 좋아해요 !!",
          "speaker_id": "1",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100034667.1.23",
          "form": "그쵸 .. 파스타 양이 밖에서는 넘 적죠..ㅠㅠ",
          "original_form": "그쵸 .. 파스타 양이 밖에서는 넘 적죠..ㅠㅠ",
          "speaker_id": "1",
          "time": "20210828 19:12"
        },
        {
          "id": "MDRW2100034667.1.24",
          "form": "오일파스타도 너무 맛잇죠",
          "original_form": "오일파스타도 너무 맛잇죠",
          "speaker_id": "2",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100034667.1.25",
          "form": "마늘 잔뜩 들어간!!",
          "original_form": "마늘 잔뜩 들어간!!",
          "speaker_id": "2",
          "time": "20210828 19:27"
        },
        {
          "id": "MDRW2100034667.1.26",
          "form": "오 맞아요 ㅠㅠ 밖에서는 마늘이 적어서 아쉬워요..",
          "original_form": "오 맞아요 ㅠㅠ 밖에서는 마늘이 적어서 아쉬워요..",
          "speaker_id": "1",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100034667.1.27",
          "form": "집에서 왕창 넣고 하면 맛있어요 !",
          "original_form": "집에서 왕창 넣고 하면 맛있어요 !",
          "speaker_id": "1",
          "time": "20210828 19:31"
        },
        {
          "id": "MDRW2100034667.1.28",
          "form": "맞아요 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100034667.1.29",
          "form": "전 로제나 크림파스타도 엄청 좋아해요",
          "original_form": "전 로제나 크림파스타도 엄청 좋아해요",
          "speaker_id": "2",
          "time": "20210828 19:32"
        },
        {
          "id": "MDRW2100034667.1.30",
          "form": "그쵸그쵸 파스타는 진짜 다 맛있어요 ㅠㅠ",
          "original_form": "그쵸그쵸 파스타는 진짜 다 맛있어요 ㅠㅠ",
          "speaker_id": "1",
