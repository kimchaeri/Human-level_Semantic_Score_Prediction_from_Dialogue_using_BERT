      "utterance": [
        {
          "id": "MDRW2100040436.1.1",
          "form": "저는 커피소년 노래는",
          "original_form": "저는 커피소년 노래는",
          "speaker_id": "1",
          "time": "20210904 19:56"
        },
        {
          "id": "MDRW2100040436.1.2",
          "form": "내가 니편이 되어줄게 듣고 힘받았었는데",
          "original_form": "내가 니편이 되어줄게 듣고 힘받았었는데",
          "speaker_id": "1",
          "time": "20210904 19:56"
        },
        {
          "id": "MDRW2100040436.1.3",
          "form": "혹시 들어보셨나요?",
          "original_form": "혹시 들어보셨나요?",
          "speaker_id": "1",
          "time": "20210904 19:56"
        },
        {
          "id": "MDRW2100040436.1.4",
          "form": "오 그노래도 좋죠",
          "original_form": "오 그노래도 좋죠",
          "speaker_id": "2",
          "time": "20210904 19:56"
        },
        {
          "id": "MDRW2100040436.1.5",
          "form": "커피소년 노래들도 노랫말이 따뜻하잖아요",
          "original_form": "커피소년 노래들도 노랫말이 따뜻하잖아요",
          "speaker_id": "2",
          "time": "20210904 19:56"
        },
        {
          "id": "MDRW2100040436.1.6",
          "form": "맞아요 완전 위로받는 느낌!!! 목소리도 뭔가 힘주는 느낌이에요",
          "original_form": "맞아요 완전 위로받는 느낌!!! 목소리도 뭔가 힘주는 느낌이에요",
          "speaker_id": "1",
          "time": "20210904 19:58"
        },
        {
          "id": "MDRW2100040436.1.7",
          "form": "네 맞아요!!",
          "original_form": "네 맞아요!!",
          "speaker_id": "2",
          "time": "20210904 20:11"
        },
        {
          "id": "MDRW2100040436.1.8",
          "form": "요즘은 그런 분위기의 노래가 좋더라고요",
          "original_form": "요즘은 그런 분위기의 노래가 좋더라고요",
          "speaker_id": "2",
          "time": "20210904 20:12"
        },
        {
          "id": "MDRW2100040436.1.9",
          "form": "스탠딩에그 little star 이 노래 들어보셨나요?",
          "original_form": "스탠딩에그 little star 이 노래 들어보셨나요?",
          "speaker_id": "1",
          "time": "20210904 20:13"
        },
        {
          "id": "MDRW2100040436.1.10",
          "form": "저는 자기전엔 이노래가 진짜 좋더라구요 ㅋㅋㅋ",
          "original_form": "저는 자기전엔 이노래가 진짜 좋더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 20:14"
        },
        {
          "id": "MDRW2100040436.1.11",
          "form": "아 그래요?",
          "original_form": "아 그래요?",
          "speaker_id": "2",
          "time": "20210904 20:14"
        },
        {
          "id": "MDRW2100040436.1.12",
          "form": "저도 오늘 자기 전에 찾아서 들어볼게요ㅎㅎ",
          "original_form": "저도 오늘 자기 전에 찾아서 들어볼게요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:14"
        },
        {
          "id": "MDRW2100040436.1.13",
          "form": "네네! 가사가 자장가 같고 노래도 조곤조곤? 따스해요",
          "original_form": "네네! 가사가 자장가 같고 노래도 조곤조곤? 따스해요",
          "speaker_id": "1",
          "time": "20210904 20:14"
        },
        {
          "id": "MDRW2100040436.1.14",
          "form": "오 잘 때 들음 딱이겠네요",
          "original_form": "오 잘 때 들음 딱이겠네요",
          "speaker_id": "2",
          "time": "20210904 20:15"
        },
        {
          "id": "MDRW2100040436.1.15",
          "form": "넷플릭스도 보시나용?",
          "original_form": "넷플릭스도 보시나용?",
          "speaker_id": "1",
          "time": "20210904 20:15"
        },
        {
          "id": "MDRW2100040436.1.16",
          "form": "네 넷플릭스는 사랑이죠ㅎㅎ",
          "original_form": "네 넷플릭스는 사랑이죠ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 20:16"
        },
        {
          "id": "MDRW2100040436.1.17",
          "form": "ㅋㅋㅋㅋㅋ요즘 어떤거 보셨어요?",
          "original_form": "ㅋㅋㅋㅋㅋ요즘 어떤거 보셨어요?",
          "speaker_id": "1",
          "time": "20210904 20:16"
        },
        {
          "id": "MDRW2100040436.1.18",
          "form": "최근에는 넷플릭스 오리지널은 잘 안보고",
          "original_form": "최근에는 넷플릭스 오리지널은 잘 안보고",
          "speaker_id": "2",
          "time": "20210904 20:17"
        },
        {
          "id": "MDRW2100040436.1.19",
          "form": "슬의생이랑 맛녀석 봤어요",
          "original_form": "슬의생이랑 맛녀석 봤어요",
          "speaker_id": "2",
          "time": "20210904 20:17"
        },
        {
          "id": "MDRW2100040436.1.20",
          "form": "오 ㅋㅋㅋㅋ맛녀석 김준현 하차해서 너무 아쉬워요ㅠ",
          "original_form": "오 ㅋㅋㅋㅋ맛녀석 김준현 하차해서 너무 아쉬워요ㅠ",
          "speaker_id": "1",
          "time": "20210904 20:18"
        },
        {
          "id": "MDRW2100040436.1.21",
          "form": "넷 캐미가 좋은데",
          "original_form": "넷 캐미가 좋은데",
          "speaker_id": "1",
          "time": "20210904 20:18"
        },
        {
          "id": "MDRW2100040436.1.22",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "1",
          "time": "20210904 20:18"
        },
        {
          "id": "MDRW2100040436.1.23",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "2",
          "time": "20210904 20:18"
        },
        {
          "id": "MDRW2100040436.1.24",
          "form": "원년멤버가 빠짐 아쉽죠ㅜ",
          "original_form": "원년멤버가 빠짐 아쉽죠ㅜ",
          "speaker_id": "2",
          "time": "20210904 20:18"
        }
      ]
    }
  ]
