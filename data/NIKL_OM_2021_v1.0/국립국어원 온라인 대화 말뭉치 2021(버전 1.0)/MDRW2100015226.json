      "utterance": [
        {
          "id": "MDRW2100015226.1.1",
          "form": "혹시 평소에 운동하세요?",
          "original_form": "혹시 평소에 운동하세요?",
          "speaker_id": "1",
          "time": "20210825 22:23"
        },
        {
          "id": "MDRW2100015226.1.2",
          "form": "집에서 홈트 조금 하고 있어요",
          "original_form": "집에서 홈트 조금 하고 있어요",
          "speaker_id": "2",
          "time": "20210825 22:24"
        },
        {
          "id": "MDRW2100015226.1.3",
          "form": "오 동영상 같은 거 보고 하시는거에요?",
          "original_form": "오 동영상 같은 거 보고 하시는거에요?",
          "speaker_id": "1",
          "time": "20210825 22:27"
        },
        {
          "id": "MDRW2100015226.1.4",
          "form": "네ㅎㅎ 유튜브 보고 하고 있어요",
          "original_form": "네ㅎㅎ 유튜브 보고 하고 있어요",
          "speaker_id": "2",
          "time": "20210825 22:30"
        },
        {
          "id": "MDRW2100015226.1.5",
          "form": "저도 똑같아요ㅋㅋㅋㅋ",
          "original_form": "저도 똑같아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015226.1.6",
          "form": "어느 채널 보고 하세요?",
          "original_form": "어느 채널 보고 하세요?",
          "speaker_id": "2",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015226.1.7",
          "form": "저는 마일리 사이러스 운동이랑",
          "original_form": "저는 마일리 사이러스 운동이랑",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015226.1.8",
          "form": "심으뜸님 채널이요!",
          "original_form": "심으뜸님 채널이요!",
          "speaker_id": "1",
          "time": "20210825 22:31"
        },
        {
          "id": "MDRW2100015226.1.9",
          "form": "아 저도 본적 있어요",
          "original_form": "아 저도 본적 있어요",
          "speaker_id": "2",
          "time": "20210825 22:32"
        },
        {
          "id": "MDRW2100015226.1.10",
          "form": "둘 다 유명하죠ㅎㅎㅎ 어떤 채널 보세요?",
          "original_form": "둘 다 유명하죠ㅎㅎㅎ 어떤 채널 보세요?",
          "speaker_id": "1",
          "time": "20210825 22:34"
        },
        {
          "id": "MDRW2100015226.1.11",
          "form": "저는 빅씨스 봐요",
          "original_form": "저는 빅씨스 봐요",
          "speaker_id": "2",
          "time": "20210825 22:35"
        },
        {
          "id": "MDRW2100015226.1.12",
          "form": "오 저 처음 들어봐요..",
          "original_form": "오 저 처음 들어봐요..",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015226.1.13",
          "form": "다른 채널에 비해서는 들 유명하긴 하죠",
          "original_form": "다른 채널에 비해서는 들 유명하긴 하죠",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015226.1.14",
          "form": "어떤 루틴 많이 하세요?",
          "original_form": "어떤 루틴 많이 하세요?",
          "speaker_id": "1",
          "time": "20210825 23:15"
        },
        {
          "id": "MDRW2100015226.1.15",
          "form": "혼자서 스트레칭 하고 10분짜리 체지방 불태우기 영상 보면서 따라해요",
          "original_form": "혼자서 스트레칭 하고 10분짜리 체지방 불태우기 영상 보면서 따라해요",
          "speaker_id": "2",
          "time": "20210825 23:17"
        },
        {
          "id": "MDRW2100015226.1.16",
          "form": "저는 그런 전신루틴 영상은 많이 안 봤는데",
          "original_form": "저는 그런 전신루틴 영상은 많이 안 봤는데",
          "speaker_id": "1",
          "time": "20210825 23:20"
        },
        {
          "id": "MDRW2100015226.1.17",
          "form": "효과가 어떤지 궁금해요",
          "original_form": "효과가 어떤지 궁금해요",
          "speaker_id": "1",
          "time": "20210825 23:21"
        },
        {
          "id": "MDRW2100015226.1.18",
          "form": "음... 효과라고 한다면 전신이 쑤셔요...ㅋㅋ",
          "original_form": "음... 효과라고 한다면 전신이 쑤셔요...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015226.1.19",
          "form": "대신 오래 하다보면 몸이 가벼워지긴 하더라구요",
          "original_form": "대신 오래 하다보면 몸이 가벼워지긴 하더라구요",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015226.1.20",
          "form": "오 확실히 자극이 가나봐요!",
          "original_form": "오 확실히 자극이 가나봐요!",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015226.1.21",
          "form": "저는 시작한지 얼마 안됐는데 오래 하셨어요?",
          "original_form": "저는 시작한지 얼마 안됐는데 오래 하셨어요?",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015226.1.22",
          "form": "거의 두 달 됐어요ㅎㅎ 종강하자마자 시작해서",
          "original_form": "거의 두 달 됐어요ㅎㅎ 종강하자마자 시작해서",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015226.1.23",
          "form": "꾸준히 하셨네요",
          "original_form": "꾸준히 하셨네요",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015226.1.24",
          "form": "운동은 진짜 꾸준히 해야하는 거 같아요",
          "original_form": "운동은 진짜 꾸준히 해야하는 거 같아요",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015226.1.25",
          "form": "맞아요... 저도 한때 운동 많이 하다가 수험생활 때문에 오래 끊어서 이번에 고생 좀 했어요",
          "original_form": "맞아요... 저도 한때 운동 많이 하다가 수험생활 때문에 오래 끊어서 이번에 고생 좀 했어요",
          "speaker_id": "2",
          "time": "20210825 23:37"
        },
        {
          "id": "MDRW2100015226.1.26",
          "form": "저도 진짜 오래 안하다가 시작하니까 힘들더라구요",
          "original_form": "저도 진짜 오래 안하다가 시작하니까 힘들더라구요",
          "speaker_id": "1",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015226.1.27",
          "form": "원래는 이렇지 않았는데 말이죠... 후",
          "original_form": "원래는 이렇지 않았는데 말이죠... 후",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015226.1.28",
          "form": "이번엔 중간에 안 그만두고 오래 해야겠어요ㅠㅠ",
          "original_form": "이번엔 중간에 안 그만두고 오래 해야겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:56"
        }
      ]
