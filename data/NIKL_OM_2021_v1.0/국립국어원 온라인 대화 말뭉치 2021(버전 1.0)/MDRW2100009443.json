      },
      "utterance": [
        {
          "id": "MDRW2100009443.1.1",
          "form": "돌싱글즈 보았니?",
          "original_form": "돌싱글즈 보았니?",
          "speaker_id": "1",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009443.1.2",
          "form": "아니 ㅜ",
          "original_form": "아니 ㅜ",
          "speaker_id": "2",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009443.1.3",
          "form": "얘기는 들었어",
          "original_form": "얘기는 들었어",
          "speaker_id": "2",
          "time": "20210822 23:29"
        },
        {
          "id": "MDRW2100009443.1.4",
          "form": "짱잼이더라고ㅎㅎ 돌싱에 대한 이야기도 신기한데 일반인들 나와서 하는거 신ㄱ",
          "original_form": "짱잼이더라고ㅎㅎ 돌싱에 대한 이야기도 신기한데 일반인들 나와서 하는거 신ㄱ",
          "speaker_id": "1",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009443.1.5",
          "form": "신기ㅎ",
          "original_form": "신기ㅎ",
          "speaker_id": "1",
          "time": "20210822 23:34"
        },
        {
          "id": "MDRW2100009443.1.6",
          "form": "앗 내가 착각했네",
          "original_form": "앗 내가 착각했네",
          "speaker_id": "2",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100009443.1.7",
          "form": "환승연애랑",
          "original_form": "환승연애랑",
          "speaker_id": "2",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100009443.1.8",
          "form": "돌싱글즈는 뭐야?",
          "original_form": "돌싱글즈는 뭐야?",
          "speaker_id": "2",
          "time": "20210822 23:35"
        },
        {
          "id": "MDRW2100009443.1.9",
          "form": "돌싱남녀들 나와서 3박 하며 커플 결정하고. 커플된 사람은 일주일 동거하는거!",
          "original_form": "돌싱남녀들 나와서 3박 하며 커플 결정하고. 커플된 사람은 일주일 동거하는거!",
          "speaker_id": "1",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009443.1.10",
          "form": "ㅋㅋ아 재밌겠다",
          "original_form": "ㅋㅋ아 재밌겠다",
          "speaker_id": "2",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009443.1.11",
          "form": "동거라니 쎄네",
          "original_form": "동거라니 쎄네",
          "speaker_id": "2",
          "time": "20210822 23:37"
        },
        {
          "id": "MDRW2100009443.1.12",
          "form": "응응 근데 수위가 높거나 하진않어ㅋ 아직까진ㅎ",
          "original_form": "응응 근데 수위가 높거나 하진않어ㅋ 아직까진ㅎ",
          "speaker_id": "1",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009443.1.13",
          "form": "높아지면 말해줘ㅋㅋ",
          "original_form": "높아지면 말해줘ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:38"
        },
        {
          "id": "MDRW2100009443.1.14",
          "form": "ㅋㅋ 응 넷플에 있더라! 처음에는 서로의 신상을 이야기 안하는데 신상에서 아이 유무가 크게 작용하더라구",
          "original_form": "ㅋㅋ 응 넷플에 있더라! 처음에는 서로의 신상을 이야기 안하는데 신상에서 아이 유무가 크게 작용하더라구",
          "speaker_id": "1",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009443.1.15",
          "form": "아..그치 천지차이지",
          "original_form": "아..그치 천지차이지",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009443.1.16",
          "form": "나는지금 동상이몽 보고있어 유튜브로",
          "original_form": "나는지금 동상이몽 보고있어 유튜브로",
          "speaker_id": "2",
          "time": "20210822 23:39"
        },
        {
          "id": "MDRW2100009443.1.17",
          "form": "동상이몽도 재미있지! 새로운 인물이 나왔어?",
          "original_form": "동상이몽도 재미있지! 새로운 인물이 나왔어?",
          "speaker_id": "1",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009443.1.18",
          "form": "아니 이지혜꺼보고있어 ㅋ 그집딸귀여워서",
          "original_form": "아니 이지혜꺼보고있어 ㅋ 그집딸귀여워서",
          "speaker_id": "2",
          "time": "20210822 23:43"
        },
        {
          "id": "MDRW2100009443.1.19",
          "form": "딸이랑 아빠랑 판박이던데!",
          "original_form": "딸이랑 아빠랑 판박이던데!",
          "speaker_id": "1",
          "time": "20210822 23:47"
        },
        {
          "id": "MDRW2100009443.1.20",
          "form": "응 둘째도 딸인거 같더라구",
          "original_form": "응 둘째도 딸인거 같더라구",
          "speaker_id": "2",
          "time": "20210822 23:48"
        },
        {
          "id": "MDRW2100009443.1.21",
          "form": "자매구나!",
          "original_form": "자매구나!",
          "speaker_id": "1",
          "time": "20210822 23:48"
        }
      ]
    }
  ]
