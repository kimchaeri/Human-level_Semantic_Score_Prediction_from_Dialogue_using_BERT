      "utterance": [
        {
          "id": "MDRW2100040907.1.1",
          "form": "아 그렇군요",
          "original_form": "아 그렇군요",
          "speaker_id": "2",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100040907.1.2",
          "form": "그래서 헬스도 하시는거예요?",
          "original_form": "그래서 헬스도 하시는거예요?",
          "speaker_id": "2",
          "time": "20210905 12:07"
        },
        {
          "id": "MDRW2100040907.1.3",
          "form": "했었어요",
          "original_form": "했었어요",
          "speaker_id": "1",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100040907.1.4",
          "form": "작년부터 못하죠",
          "original_form": "작년부터 못하죠",
          "speaker_id": "1",
          "time": "20210905 12:14"
        },
        {
          "id": "MDRW2100040907.1.5",
          "form": "아 운동가는 건 계속 쉬고 계시군요",
          "original_form": "아 운동가는 건 계속 쉬고 계시군요",
          "speaker_id": "2",
          "time": "20210905 12:17"
        },
        {
          "id": "MDRW2100040907.1.6",
          "form": "그럼 집에서 따로 운동하시는 것 있나요?",
          "original_form": "그럼 집에서 따로 운동하시는 것 있나요?",
          "speaker_id": "2",
          "time": "20210905 12:17"
        },
        {
          "id": "MDRW2100040907.1.7",
          "form": "네 작년부터 아무데도 안가고 있어요",
          "original_form": "네 작년부터 아무데도 안가고 있어요",
          "speaker_id": "1",
          "time": "20210905 12:17"
        },
        {
          "id": "MDRW2100040907.1.8",
          "form": "집에서는 시도해봤는데 쉽지 않더라구요",
          "original_form": "집에서는 시도해봤는데 쉽지 않더라구요",
          "speaker_id": "1",
          "time": "20210905 12:17"
        },
        {
          "id": "MDRW2100040907.1.9",
          "form": "네네 홈트는 웬만한 의지로는 하기 쉽지 않더라고요",
          "original_form": "네네 홈트는 웬만한 의지로는 하기 쉽지 않더라고요",
          "speaker_id": "2",
          "time": "20210905 12:18"
        },
        {
          "id": "MDRW2100040907.1.10",
          "form": "그래서 산책겸 걷기하고 있어요",
          "original_form": "그래서 산책겸 걷기하고 있어요",
          "speaker_id": "1",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100040907.1.11",
          "form": "아무것도 안하면 안될거 같아서 동네 공원을 몇바퀴씩 돌아요",
          "original_form": "아무것도 안하면 안될거 같아서 동네 공원을 몇바퀴씩 돌아요",
          "speaker_id": "1",
          "time": "20210905 13:22"
        },
        {
          "id": "MDRW2100040907.1.12",
          "form": "네 이제 날 선선해져서",
          "original_form": "네 이제 날 선선해져서",
          "speaker_id": "2",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100040907.1.13",
          "form": "산책하기 좋은 날씨가 됐어요",
          "original_form": "산책하기 좋은 날씨가 됐어요",
          "speaker_id": "2",
          "time": "20210905 14:28"
        },
        {
          "id": "MDRW2100040907.1.14",
          "form": "그렇죠 산책하니 정말 좋더라구요",
          "original_form": "그렇죠 산책하니 정말 좋더라구요",
          "speaker_id": "1",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100040907.1.15",
          "form": "최근 두주간 비가 자주 와서 산책못하니까",
          "original_form": "최근 두주간 비가 자주 와서 산책못하니까",
          "speaker_id": "1",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100040907.1.16",
          "form": "하다 안하니 몸이 쑤시더라구요",
          "original_form": "하다 안하니 몸이 쑤시더라구요",
          "speaker_id": "1",
          "time": "20210905 16:58"
        },
        {
          "id": "MDRW2100040907.1.17",
          "form": "운동 꾸준히 하셨으면",
          "original_form": "운동 꾸준히 하셨으면",
          "speaker_id": "2",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100040907.1.18",
          "form": "안할 때 확실히 느껴지시죠",
          "original_form": "안할 때 확실히 느껴지시죠",
          "speaker_id": "2",
          "time": "20210905 16:59"
        },
        {
          "id": "MDRW2100040907.1.19",
          "form": "네.. 전 움직이는 걸 좋아하는 사람이라 집에 가만이 있으면 오히려 병 생겨요",
          "original_form": "네.. 전 움직이는 걸 좋아하는 사람이라 집에 가만이 있으면 오히려 병 생겨요",
          "speaker_id": "1",
          "time": "20210905 17:18"
        },
        {
          "id": "MDRW2100040907.1.20",
          "form": "그러시군요",
          "original_form": "그러시군요",
          "speaker_id": "2",
          "time": "20210905 17:29"
        },
        {
          "id": "MDRW2100040907.1.21",
          "form": "많이 움직이면 건강도 챙기고 좋죠",
          "original_form": "많이 움직이면 건강도 챙기고 좋죠",
          "speaker_id": "2",
          "time": "20210905 17:29"
        },
        {
          "id": "MDRW2100040907.1.22",
          "form": "네.. 그동안은 좋았는데 코로나 걸리니까 저 같은 사람은 우울증  걸리기 딱이겠더라구요",
          "original_form": "네.. 그동안은 좋았는데 코로나 걸리니까 저 같은 사람은 우울증  걸리기 딱이겠더라구요",
          "speaker_id": "1",
          "time": "20210905 17:34"
        },
        {
          "id": "MDRW2100040907.1.23",
          "form": "전 자가격리 걸리면 너무 힘들거 같아요",
          "original_form": "전 자가격리 걸리면 너무 힘들거 같아요",
          "speaker_id": "1",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100040907.1.24",
          "form": "그래서 더 개인방역하고 있어요",
          "original_form": "그래서 더 개인방역하고 있어요",
          "speaker_id": "1",
          "time": "20210905 17:35"
        },
        {
          "id": "MDRW2100040907.1.25",
          "form": "네네 각자가 조심해야죠",
          "original_form": "네네 각자가 조심해야죠",
          "speaker_id": "2",
