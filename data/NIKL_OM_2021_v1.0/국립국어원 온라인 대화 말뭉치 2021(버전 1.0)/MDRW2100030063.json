      "utterance": [
        {
          "id": "MDRW2100030063.1.1",
          "form": "다른 온라인 커뮤니티 또 하시는데도 있나요!",
          "original_form": "다른 온라인 커뮤니티 또 하시는데도 있나요!",
          "speaker_id": "2",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100030063.1.2",
          "form": "예전엔 네이버나 다음카페에 이것저것 많았는데",
          "original_form": "예전엔 네이버나 다음카페에 이것저것 많았는데",
          "speaker_id": "2",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100030063.1.3",
          "form": "오...유튜브 정도요?",
          "original_form": "오...유튜브 정도요?",
          "speaker_id": "1",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100030063.1.4",
          "form": "마자요ㅋㅋㅋㅋㅋ카페 블로그 열심히했는데",
          "original_form": "마자요ㅋㅋㅋㅋㅋ카페 블로그 열심히했는데",
          "speaker_id": "1",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100030063.1.5",
          "form": "요즘은 잘 없죠",
          "original_form": "요즘은 잘 없죠",
          "speaker_id": "1",
          "time": "20210903 20:27"
        },
        {
          "id": "MDRW2100030063.1.6",
          "form": "아 유튜브도 커뮤니티네요ㅋㅋㅋㅋ",
          "original_form": "아 유튜브도 커뮤니티네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100030063.1.7",
          "form": "네이버 블로그 챌린지할때",
          "original_form": "네이버 블로그 챌린지할때",
          "speaker_id": "2",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100030063.1.8",
          "form": "열심히 썼었어요",
          "original_form": "열심히 썼었어요",
          "speaker_id": "2",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100030063.1.9",
          "form": "예전엔 블로그 이웃도 맺고 열심히 썼었는데",
          "original_form": "예전엔 블로그 이웃도 맺고 열심히 썼었는데",
          "speaker_id": "2",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100030063.1.10",
          "form": "헉 마자여 저도 그거 엄청 열심히 썼어요",
          "original_form": "헉 마자여 저도 그거 엄청 열심히 썼어요",
          "speaker_id": "1",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100030063.1.11",
          "form": "마자마자여ㅋㅋㅋㅋㅋ트위터 생겨서 트위터로 넘어왔죵",
          "original_form": "마자마자여ㅋㅋㅋㅋㅋ트위터 생겨서 트위터로 넘어왔죵",
          "speaker_id": "1",
          "time": "20210903 20:28"
        },
        {
          "id": "MDRW2100030063.1.12",
          "form": "블로그챌린지 보상 받으셨나용!",
          "original_form": "블로그챌린지 보상 받으셨나용!",
          "speaker_id": "1",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100030063.1.13",
          "form": "넵 받았어요!",
          "original_form": "넵 받았어요!",
          "speaker_id": "2",
          "time": "20210903 20:29"
        },
        {
          "id": "MDRW2100030063.1.14",
          "form": "확인이 안되서 조마조마했는데",
          "original_form": "확인이 안되서 조마조마했는데",
          "speaker_id": "2",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100030063.1.15",
          "form": "트위터나 커뮤니티에 자정넘기전에 블챌 글 쓰라고 꾸준히 글이 올라와서",
          "original_form": "트위터나 커뮤니티에 자정넘기전에 블챌 글 쓰라고 꾸준히 글이 올라와서",
          "speaker_id": "2",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100030063.1.16",
          "form": "하루도 안 잊어버릴 수 있었어요ㅋㅋㅋ",
          "original_form": "하루도 안 잊어버릴 수 있었어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:30"
        },
        {
          "id": "MDRW2100030063.1.17",
          "form": "커뮤니티 사람들 다들 온라인 폐지줍기에 진심이여서",
          "original_form": "커뮤니티 사람들 다들 온라인 폐지줍기에 진심이여서",
          "speaker_id": "2",
          "time": "20210903 20:31"
        },
        {
          "id": "MDRW2100030063.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ마자요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ마자요",
          "speaker_id": "1",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100030063.1.19",
          "form": "글고보니 이거 열심히 하는 분들 다",
          "original_form": "글고보니 이거 열심히 하는 분들 다",
          "speaker_id": "1",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100030063.1.20",
          "form": "블챌도 열심히 하셖을듯ㅋㅋ큐ㅠㅠㅠ",
          "original_form": "블챌도 열심히 하셖을듯ㅋㅋ큐ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100030063.1.21",
          "form": "저는 이번에 네이버페이 5만원까지 다 받는 게 목표예요....",
          "original_form": "저는 이번에 네이버페이 5만원까지 다 받는 게 목표예요....",
          "speaker_id": "1",
          "time": "20210903 20:35"
        },
        {
          "id": "MDRW2100030063.1.22",
          "form": "열심히 대화해야겠네요ㅋㅋㅋㅋ",
          "original_form": "열심히 대화해야겠네요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:36"
        },
        {
          "id": "MDRW2100030063.1.23",
          "form": "커뮤니티나 sns마다 소소하게 돈 버는 정보 물건 싸게사는 정보 올려주시는 분들 많아서",
          "original_form": "커뮤니티나 sns마다 소소하게 돈 버는 정보 물건 싸게사는 정보 올려주시는 분들 많아서",
          "speaker_id": "2",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100030063.1.24",
          "form": "잘 주시해야겠다 싶어요",
          "original_form": "잘 주시해야겠다 싶어요",
          "speaker_id": "2",
          "time": "20210903 20:37"
        },
        {
          "id": "MDRW2100030063.1.25",
          "form": "헐 대박... 그런가용",
          "original_form": "헐 대박... 그런가용",
          "speaker_id": "1",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100030063.1.26",
          "form": "마자여 잘 주시해야하죠",
          "original_form": "마자여 잘 주시해야하죠",
          "speaker_id": "1",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100030063.1.27",
          "form": "저는 약간 올리브영 세일 안할때 가면 억울한 타입이라",
          "original_form": "저는 약간 올리브영 세일 안할때 가면 억울한 타입이라",
          "speaker_id": "1",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100030063.1.28",
          "form": "꿀정보에 약해요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "꿀정보에 약해요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100030063.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ꿀정보때문에라도 인터넷을 못 끊겠어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ꿀정보때문에라도 인터넷을 못 끊겠어요",
          "speaker_id": "2",
          "time": "20210903 20:38"
        },
        {
          "id": "MDRW2100030063.1.30",
          "form": "계속 안보고 있으면 손해볼 것만 같고",
          "original_form": "계속 안보고 있으면 손해볼 것만 같고",
          "speaker_id": "2",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030063.1.31",
          "form": "맞아요ㅋㅋㅋㅋ 인터넷 아니면 누가 이런 거 알려줘용! 이 각박한 세상에서",
          "original_form": "맞아요ㅋㅋㅋㅋ 인터넷 아니면 누가 이런 거 알려줘용! 이 각박한 세상에서",
          "speaker_id": "1",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030063.1.32",
          "form": "얼마전에 여기서 대화하다가도 어떤 분이 꿀팁 주셔서",
          "original_form": "얼마전에 여기서 대화하다가도 어떤 분이 꿀팁 주셔서",
          "speaker_id": "1",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030063.1.33",
          "form": "ㅋㅋㅋㅋㅋ맞아욬ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ맞아욬ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030063.1.34",
          "form": "좋았어요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "좋았어요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 20:39"
        },
        {
          "id": "MDRW2100030063.1.35",
          "form": "ㅋㅋㅋ앗 꿀팁 뭐였는지 궁금해요",
          "original_form": "ㅋㅋㅋ앗 꿀팁 뭐였는지 궁금해요",
          "speaker_id": "2",
          "time": "20210903 20:40"
        },
        {
          "id": "MDRW2100030063.1.36",
          "form": "커뮤니티 사람이 손수 전수해준 꿀팁!",
          "original_form": "커뮤니티 사람이 손수 전수해준 꿀팁!",
          "speaker_id": "2",
