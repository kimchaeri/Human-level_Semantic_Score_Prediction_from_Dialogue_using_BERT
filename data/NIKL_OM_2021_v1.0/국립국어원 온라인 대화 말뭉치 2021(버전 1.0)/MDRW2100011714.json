      "utterance": [
        {
          "id": "MDRW2100011714.1.1",
          "form": "어머니가 끓여주신 국과 밥을 먹었음",
          "original_form": "어머니가 끓여주신 국과 밥을 먹었음",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011714.1.2",
          "form": "물론 밥은 내가 지음",
          "original_form": "물론 밥은 내가 지음",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011714.1.3",
          "form": "아 그러면 아픈 니가",
          "original_form": "아 그러면 아픈 니가",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011714.1.4",
          "form": "잘못인듯",
          "original_form": "잘못인듯",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011714.1.5",
          "form": "앜ㅌㅋㅋㅋㅌㅌㅌㅋㅋㅋㅋ",
          "original_form": "앜ㅌㅋㅋㅋㅌㅌㅌㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011714.1.6",
          "form": "와 맞지",
          "original_form": "와 맞지",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011714.1.7",
          "form": "내 몸이 이상한거지 맞지 어머니 탓은 아니다",
          "original_form": "내 몸이 이상한거지 맞지 어머니 탓은 아니다",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011714.1.8",
          "form": "ㅇㅇ맞음",
          "original_form": "ㅇㅇ맞음",
          "speaker_id": "1",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011714.1.9",
          "form": "뭐지 셀프 탈룰라인가",
          "original_form": "뭐지 셀프 탈룰라인가",
          "speaker_id": "2",
          "time": "20210824 15:19"
        },
        {
          "id": "MDRW2100011714.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011714.1.11",
          "form": "굴포천에서",
          "original_form": "굴포천에서",
          "speaker_id": "1",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011714.1.12",
          "form": "달림?",
          "original_form": "달림?",
          "speaker_id": "1",
          "time": "20210824 15:20"
        },
        {
          "id": "MDRW2100011714.1.13",
          "form": "집 근처에서 달리다 보니까 근처까지 가게 되더라",
          "original_form": "집 근처에서 달리다 보니까 근처까지 가게 되더라",
          "speaker_id": "2",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011714.1.14",
          "form": "아니 근데 원래 달리기 하고 나면 허벅지랑 정강이가 아픔?",
          "original_form": "아니 근데 원래 달리기 하고 나면 허벅지랑 정강이가 아픔?",
          "speaker_id": "2",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011714.1.15",
          "form": "정강이?",
          "original_form": "정강이?",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011714.1.16",
          "form": "뒤쪽?",
          "original_form": "뒤쪽?",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011714.1.17",
          "form": "앞쪽",
          "original_form": "앞쪽",
          "speaker_id": "2",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011714.1.18",
          "form": "허벅지는나도아팠음",
          "original_form": "허벅지는나도아팠음",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011714.1.19",
          "form": "뼈있는데?",
          "original_form": "뼈있는데?",
          "speaker_id": "1",
          "time": "20210824 15:21"
        },
        {
          "id": "MDRW2100011714.1.20",
          "form": "ㅇㅇ 왼쪽 앞쪽 정강이",
          "original_form": "ㅇㅇ 왼쪽 앞쪽 정강이",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011714.1.21",
          "form": "잘못달린거아님?",
          "original_form": "잘못달린거아님?",
          "speaker_id": "1",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011714.1.22",
          "form": "난종아리뒤쪽이아팠음",
          "original_form": "난종아리뒤쪽이아팠음",
          "speaker_id": "1",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011714.1.23",
          "form": "스트레칭안해서",
          "original_form": "스트레칭안해서",
          "speaker_id": "1",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011714.1.24",
          "form": "그럴수도있음",
          "original_form": "그럴수도있음",
          "speaker_id": "1",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011714.1.25",
          "form": "아",
          "original_form": "아",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011714.1.26",
          "form": "스트레칭 유무 차이가 커?",
          "original_form": "스트레칭 유무 차이가 커?",
          "speaker_id": "2",
          "time": "20210824 15:22"
        },
        {
          "id": "MDRW2100011714.1.27",
          "form": "나도몰랐는데스트레칭해야지",
          "original_form": "나도몰랐는데스트레칭해야지",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011714.1.28",
          "form": "관절에무리도안가고",
          "original_form": "관절에무리도안가고",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011714.1.29",
          "form": "차이가크대",
          "original_form": "차이가크대",
          "speaker_id": "1",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011714.1.30",
          "form": "아 그렇구나 다음부턴 해야 되겠네",
          "original_form": "아 그렇구나 다음부턴 해야 되겠네",
          "speaker_id": "2",
          "time": "20210824 15:23"
        },
        {
          "id": "MDRW2100011714.1.31",
          "form": "너런데이",
          "original_form": "너런데이",
          "speaker_id": "1",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100011714.1.32",
          "form": "이틀에한번하잖아",
          "original_form": "이틀에한번하잖아",
          "speaker_id": "1",
          "time": "20210827 12:57"
        },
        {
          "id": "MDRW2100011714.1.33",
          "form": "그러면 그 쉬는 날에 따로 운동해?",
          "original_form": "그러면 그 쉬는 날에 따로 운동해?",
          "speaker_id": "1",
          "time": "20210827 12:57"
        }
      ]
