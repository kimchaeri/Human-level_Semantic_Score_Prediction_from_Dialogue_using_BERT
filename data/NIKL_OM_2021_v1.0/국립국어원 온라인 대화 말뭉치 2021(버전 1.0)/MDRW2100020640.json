      "utterance": [
        {
          "id": "MDRW2100020640.1.1",
          "form": "name2님 최애ㅇ음식 있나요?!",
          "original_form": "&name2&님 최애ㅇ음식 있나요?!",
          "speaker_id": "1",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020640.1.2",
          "form": "술 좋아해요?",
          "original_form": "술 좋아해요?",
          "speaker_id": "2",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020640.1.3",
          "form": "아놔!!! 저 엄청 좋아해요!!!!!",
          "original_form": "아놔!!! 저 엄청 좋아해요!!!!!",
          "speaker_id": "1",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020640.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020640.1.5",
          "form": "술안주 얘기해볼까요 우리 ㅎㅎ",
          "original_form": "술안주 얘기해볼까요 우리 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020640.1.6",
          "form": "백신때문에 못 먹게 됐지만요! 하 씁 술안주 저는 국물있는 걸 좋아하는데 name2님은요?",
          "original_form": "백신때문에 못 먹게 됐지만요! 하 씁 술안주 저는 국물있는 걸 좋아하는데 &name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 10:41"
        },
        {
          "id": "MDRW2100020640.1.7",
          "form": "오.. 저는 튀김류나 고기류 선호해요",
          "original_form": "오.. 저는 튀김류나 고기류 선호해요",
          "speaker_id": "2",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020640.1.8",
          "form": "근데 술 많이 오랫동안 먹을거면 탕류 좋아해요",
          "original_form": "근데 술 많이 오랫동안 먹을거면 탕류 좋아해요",
          "speaker_id": "2",
          "time": "20210828 10:42"
        },
        {
          "id": "MDRW2100020640.1.9",
          "form": "고기류도 너무 좋죠ㅠ ㅋㅋㅋㅋㅋㅋ 맞아요! 전 감자튀김도 너무 좋아한답니다.. 어떤술이랑 먹어도 감자튀김은 꼭 있어야하더라고요",
          "original_form": "고기류도 너무 좋죠ㅠ ㅋㅋㅋㅋㅋㅋ 맞아요! 전 감자튀김도 너무 좋아한답니다.. 어떤술이랑 먹어도 감자튀김은 꼭 있어야하더라고요",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020640.1.10",
          "form": "ㅋㅋㅋㅋㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 10:43"
        },
        {
          "id": "MDRW2100020640.1.11",
          "form": "국물 있는 거 중에 어떤거 좋아해요?",
          "original_form": "국물 있는 거 중에 어떤거 좋아해요?",
          "speaker_id": "2",
          "time": "20210828 10:45"
        },
        {
          "id": "MDRW2100020640.1.12",
          "form": "전 매운국물 너무 좋아요,, 짬뽕탕 같은거요!",
          "original_form": "전 매운국물 너무 좋아요,, 짬뽕탕 같은거요!",
          "speaker_id": "1",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020640.1.13",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020640.1.14",
          "form": "저는 깔끔한 어묵탕이나 김치찌개, 부대찌개류요 ㅋㅋ",
          "original_form": "저는 깔끔한 어묵탕이나 김치찌개, 부대찌개류요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210828 10:47"
        },
        {
          "id": "MDRW2100020640.1.15",
          "form": "전 매운거 먹으면 배가 아프거든요..ㅠㅠ",
          "original_form": "전 매운거 먹으면 배가 아프거든요..ㅠㅠ",
          "speaker_id": "2",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020640.1.16",
          "form": "아 부찌 너무 맛있죠ㅠ 어묵탕도 좋아요 아하...!!!",
          "original_form": "아 부찌 너무 맛있죠ㅠ 어묵탕도 좋아요 아하...!!!",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020640.1.17",
          "form": "사실..전...안가립니다...",
          "original_form": "사실..전...안가립니다...",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020640.1.18",
          "form": "다 먹어요....",
          "original_form": "다 먹어요....",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020640.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:48"
        },
        {
          "id": "MDRW2100020640.1.20",
          "form": "저 예전에 갔던 가게가 김치찌개 맛집이여서",
          "original_form": "저 예전에 갔던 가게가 김치찌개 맛집이여서",
          "speaker_id": "2",
          "time": "20210828 10:50"
        },
        {
          "id": "MDRW2100020640.1.21",
          "form": "공기밥까지 시켜서 먹었던 기억이 나네요",
          "original_form": "공기밥까지 시켜서 먹었던 기억이 나네요",
          "speaker_id": "2",
          "time": "20210828 10:51"
        },
        {
          "id": "MDRW2100020640.1.22",
          "form": "식사하고 오셨네요!!ㅋㅋㅋㅋㅋㅋ 저는 그 술 먹으러 갔는데 거기 떡볶이가 너무 맛있는거예요 심지어 사이드메뉴였는데..",
          "original_form": "식사하고 오셨네요!!ㅋㅋㅋㅋㅋㅋ 저는 그 술 먹으러 갔는데 거기 떡볶이가 너무 맛있는거예요 심지어 사이드메뉴였는데..",
          "speaker_id": "1",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020640.1.23",
          "form": "나중에보니 메인메뉴 가격보다 더 시켜먹었더라고요 지인들이랑ㅋㅋㅋㅋ",
          "original_form": "나중에보니 메인메뉴 가격보다 더 시켜먹었더라고요 지인들이랑ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 10:53"
        },
        {
          "id": "MDRW2100020640.1.24",
          "form": "ㅋㅋㅋㅋ아 맞아요 그럴 때두 있어요",
          "original_form": "ㅋㅋㅋㅋ아 맞아요 그럴 때두 있어요",
          "speaker_id": "2",
          "time": "20210828 10:58"
        },
        {
          "id": "MDRW2100020640.1.25",
          "form": "그런거 포장으로도 팔았으몀 좋겠다 싶기도 하구",
          "original_form": "그런거 포장으로도 팔았으몀 좋겠다 싶기도 하구",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020640.1.26",
          "form": "배달도 되면 좋겠다 ㅎㅎ",
          "original_form": "배달도 되면 좋겠다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 10:59"
        },
        {
          "id": "MDRW2100020640.1.27",
          "form": "맞아요!!!!!ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요!!!!!ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020640.1.28",
          "form": "하 술얘기하니까 술먹고 싶네요 ...",
          "original_form": "하 술얘기하니까 술먹고 싶네요 ...",
          "speaker_id": "1",
          "time": "20210828 11:07"
        },
        {
          "id": "MDRW2100020640.1.29",
          "form": "소주VS맥주VS막걸리",
          "original_form": "소주VS맥주VS막걸리",
          "speaker_id": "1",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020640.1.30",
          "form": "어떤걸 선호하세요?",
          "original_form": "어떤걸 선호하세요?",
          "speaker_id": "1",
          "time": "20210828 11:08"
        },
        {
          "id": "MDRW2100020640.1.31",
          "form": "소주요!!!",
          "original_form": "소주요!!!",
          "speaker_id": "2",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020640.1.32",
          "form": "맥주랑 막걸리는 저랑 안맞아요 ㅜㅜㅜ",
          "original_form": "맥주랑 막걸리는 저랑 안맞아요 ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210828 11:11"
        },
        {
          "id": "MDRW2100020640.1.33",
          "form": "근데 과일막걸리나 꿀막걸리는 또 좋아해요!",
          "original_form": "근데 과일막걸리나 꿀막걸리는 또 좋아해요!",
          "speaker_id": "2",
          "time": "20210828 11:12"
        },
        {
          "id": "MDRW2100020640.1.34",
          "form": "아하!!! 헐 과일막걸리는 한 번도 안먹어봤어요 맛있나요?!",
          "original_form": "아하!!! 헐 과일막걸리는 한 번도 안먹어봤어요 맛있나요?!",
          "speaker_id": "1",
          "time": "20210828 11:20"
        },
        {
          "id": "MDRW2100020640.1.35",
          "form": "헐 과일막걸리 꼭 먹어보세요 두번 먹으세요 세번 드세요 ㅠ",
          "original_form": "헐 과일막걸리 꼭 먹어보세요 두번 먹으세요 세번 드세요 ㅠ",
          "speaker_id": "2",
          "time": "20210828 11:22"
        },
        {
          "id": "MDRW2100020640.1.36",
          "form": "바나나막걸리 = 진짜 바나나우유 맛이 남",
          "original_form": "바나나막걸리 = 진짜 바나나우유 맛이 남",
