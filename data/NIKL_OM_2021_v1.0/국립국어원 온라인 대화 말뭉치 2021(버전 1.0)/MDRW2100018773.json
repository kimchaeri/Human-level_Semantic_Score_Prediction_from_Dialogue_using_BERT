          "time": "20210827 12:04"
        },
        {
          "id": "MDRW2100018773.1.23",
          "form": "님 지금도 침대죠",
          "original_form": "님 지금도 침대죠",
          "speaker_id": "1",
          "time": "20210827 12:04"
        },
        {
          "id": "MDRW2100018773.1.24",
