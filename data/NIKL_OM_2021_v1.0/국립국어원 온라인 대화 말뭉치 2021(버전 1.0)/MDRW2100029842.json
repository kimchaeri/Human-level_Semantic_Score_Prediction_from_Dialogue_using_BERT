      "utterance": [
        {
          "id": "MDRW2100029842.1.1",
          "form": "ㅇㅇ배달 완료 문자왔다",
          "original_form": "ㅇㅇ배달 완료 문자왔다",
          "speaker_id": "2",
          "time": "20210903 14:52"
        },
        {
          "id": "MDRW2100029842.1.2",
          "form": "안 나가봐?",
          "original_form": "안 나가봐?",
          "speaker_id": "1",
          "time": "20210903 14:52"
        },
        {
          "id": "MDRW2100029842.1.3",
          "form": "고구마도 올 거 있어",
          "original_form": "고구마도 올 거 있어",
          "speaker_id": "2",
          "time": "20210903 14:52"
        },
        {
          "id": "MDRW2100029842.1.4",
          "form": "아 다 오면 같이 들고 들어오게?",
          "original_form": "아 다 오면 같이 들고 들어오게?",
          "speaker_id": "1",
          "time": "20210903 14:52"
        },
        {
          "id": "MDRW2100029842.1.5",
          "form": "ㅇㅇ",
          "original_form": "ㅇㅇ",
          "speaker_id": "2",
          "time": "20210903 14:52"
        },
        {
          "id": "MDRW2100029842.1.6",
          "form": "한번에 해야지",
          "original_form": "한번에 해야지",
          "speaker_id": "2",
          "time": "20210903 14:53"
        },
        {
          "id": "MDRW2100029842.1.7",
          "form": "조심해 엄청 무겁겠다",
          "original_form": "조심해 엄청 무겁겠다",
          "speaker_id": "1",
          "time": "20210903 14:53"
        },
        {
          "id": "MDRW2100029842.1.8",
          "form": "요즘 야채값 엄청 올랐대",
          "original_form": "요즘 야채값 엄청 올랐대",
          "speaker_id": "2",
          "time": "20210903 14:53"
        },
        {
          "id": "MDRW2100029842.1.9",
          "form": "추석 전이라 그런가?",
          "original_form": "추석 전이라 그런가?",
          "speaker_id": "1",
          "time": "20210903 14:53"
        },
        {
          "id": "MDRW2100029842.1.10",
          "form": "양파는 싸게 샀는데",
          "original_form": "양파는 싸게 샀는데",
          "speaker_id": "1",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100029842.1.11",
          "form": "양파 잘 샀지",
          "original_form": "양파 잘 샀지",
          "speaker_id": "2",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100029842.1.12",
          "form": "오키로샀나?",
          "original_form": "오키로샀나?",
          "speaker_id": "2",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100029842.1.13",
          "form": "ㅇㅇ오키로",
          "original_form": "ㅇㅇ오키로",
          "speaker_id": "1",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100029842.1.14",
          "form": "한참 먹겠지?",
          "original_form": "한참 먹겠지?",
          "speaker_id": "1",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100029842.1.15",
          "form": "그럴거 갔다",
          "original_form": "그럴거 갔다",
          "speaker_id": "2",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100029842.1.16",
          "form": "너무 많이 사도 금방 물러서 안돼",
          "original_form": "너무 많이 사도 금방 물러서 안돼",
          "speaker_id": "2",
          "time": "20210903 14:54"
        },
        {
          "id": "MDRW2100029842.1.17",
          "form": "양파 잘 상하나보네",
          "original_form": "양파 잘 상하나보네",
          "speaker_id": "1",
          "time": "20210903 14:55"
        },
        {
          "id": "MDRW2100029842.1.18",
          "form": "잘 왔는지 그럼 빨리 봐봐",
          "original_form": "잘 왔는지 그럼 빨리 봐봐",
          "speaker_id": "1",
          "time": "20210903 14:55"
        },
        {
          "id": "MDRW2100029842.1.19",
          "form": "가시네 귀찮게한다",
          "original_form": "가시네 귀찮게한다",
          "speaker_id": "2",
          "time": "20210903 14:55"
