      "utterance": [
        {
          "id": "MDRW2100042956.1.1",
          "form": "ㅋㅋㅋㅋ 그래서 버거킹에서 콰트로 치즈 먹고 있어",
          "original_form": "ㅋㅋㅋㅋ 그래서 버거킹에서 콰트로 치즈 먹고 있어",
          "speaker_id": "2",
          "time": "20210907 12:09"
        },
        {
          "id": "MDRW2100042956.1.2",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:09"
        },
        {
          "id": "MDRW2100042956.1.3",
          "form": "구내식당보다 1600원 비싸지만 만족도는 높다구",
          "original_form": "구내식당보다 1600원 비싸지만 만족도는 높다구",
          "speaker_id": "2",
          "time": "20210907 12:09"
        },
        {
          "id": "MDRW2100042956.1.4",
          "form": "난 결국 다른 김천엄",
          "original_form": "난 결국 다른 김천엄",
          "speaker_id": "1",
          "time": "20210907 12:09"
        },
        {
          "id": "MDRW2100042956.1.5",
          "form": "1600원쯤이야 ㄱㅊ",
          "original_form": "1600원쯤이야 ㄱㅊ",
          "speaker_id": "1",
          "time": "20210907 12:09"
        },
        {
          "id": "MDRW2100042956.1.6",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 12:10"
        },
        {
          "id": "MDRW2100042956.1.7",
          "form": "돈가스 쫄면 김밥 콜라보 먹고 있어?",
          "original_form": "돈가스 쫄면 김밥 콜라보 먹고 있어?",
          "speaker_id": "2",
          "time": "20210907 12:10"
        },
        {
          "id": "MDRW2100042956.1.8",
          "form": "아니 ㅜ",
          "original_form": "아니 ㅜ",
          "speaker_id": "1",
          "time": "20210907 12:10"
        },
        {
          "id": "MDRW2100042956.1.9",
          "form": "그거 먹으러 김밥천국 온건데...!",
          "original_form": "그거 먹으러 김밥천국 온건데...!",
          "speaker_id": "1",
          "time": "20210907 12:10"
        },
        {
          "id": "MDRW2100042956.1.10",
          "form": "ㅜㅜㅜㅜㅜㅜ",
          "original_form": "ㅜㅜㅜㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210907 12:10"
        },
        {
          "id": "MDRW2100042956.1.11",
          "form": "여기 김천엔 그 메뉴 안파나봐...",
          "original_form": "여기 김천엔 그 메뉴 안파나봐...",
          "speaker_id": "1",
          "time": "20210907 12:10"
        },
        {
          "id": "MDRW2100042956.1.12",
          "form": "슬프다",
          "original_form": "슬프다",
          "speaker_id": "2",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100042956.1.13",
          "form": "여기 장사 엄청 잘되네",
          "original_form": "여기 장사 엄청 잘되네",
          "speaker_id": "1",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100042956.1.14",
          "form": "내가 들어갈 때만 해도 나 말고 두팀 있었는데",
          "original_form": "내가 들어갈 때만 해도 나 말고 두팀 있었는데",
          "speaker_id": "1",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100042956.1.15",
          "form": "우옹.. 돈 많이 버시겠다",
          "original_form": "우옹.. 돈 많이 버시겠다",
          "speaker_id": "2",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100042956.1.16",
          "form": "지금 홀에 열팀정도?",
          "original_form": "지금 홀에 열팀정도?",
          "speaker_id": "1",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100042956.1.17",
          "form": "피곤하심만큼",
          "original_form": "피곤하심만큼",
          "speaker_id": "2",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100042956.1.18",
          "form": "자리가 업ㄱ어",
          "original_form": "자리가 업ㄱ어",
          "speaker_id": "1",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100042956.1.19",
          "form": "ㅋㅋㅋ오오",
          "original_form": "ㅋㅋㅋ오오",
          "speaker_id": "2",
          "time": "20210907 12:11"
        },
        {
          "id": "MDRW2100042956.1.20",
          "form": "거긴 근데 배달은 잘 없을거 같아",
          "original_form": "거긴 근데 배달은 잘 없을거 같아",
          "speaker_id": "2",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100042956.1.21",
          "form": "아닌가",
          "original_form": "아닌가",
          "speaker_id": "2",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100042956.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100042956.1.23",
          "form": "보통 포장하지 ㅇㅇ",
          "original_form": "보통 포장하지 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100042956.1.24",
          "form": "ㅇㅇ홀장사가 대부분일거 같은",
          "original_form": "ㅇㅇ홀장사가 대부분일거 같은",
          "speaker_id": "2",
          "time": "20210907 12:12"
        },
        {
          "id": "MDRW2100042956.1.25",
          "form": "글고 약간",
          "original_form": "글고 약간",
          "speaker_id": "1",
          "time": "20210907 12:13"
        },
        {
          "id": "MDRW2100042956.1.26",
          "form": "저렴하게 먹으려는 근처 사람들이라",
          "original_form": "저렴하게 먹으려는 근처 사람들이라",
          "speaker_id": "1",
          "time": "20210907 12:14"
        },
        {
          "id": "MDRW2100042956.1.27",
          "form": "배달비도 잘 안낼거같음",
          "original_form": "배달비도 잘 안낼거같음",
          "speaker_id": "1",
          "time": "20210907 12:14"
        }
      ]
    }
  ]
