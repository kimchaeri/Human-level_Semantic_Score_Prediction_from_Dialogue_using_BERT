      "utterance": [
        {
          "id": "MDRW2100038255.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "2",
          "time": "20210902 03:49"
        },
        {
          "id": "MDRW2100038255.1.2",
          "form": "요즘은 다들 폰을 오래쓴다더라고요",
          "original_form": "요즘은 다들 폰을 오래쓴다더라고요",
          "speaker_id": "1",
          "time": "20210902 03:50"
        },
        {
          "id": "MDRW2100038255.1.3",
          "form": "그쵸 저도 오래쓰고 있어요",
          "original_form": "그쵸 저도 오래쓰고 있어요",
          "speaker_id": "2",
          "time": "20210902 03:51"
        },
        {
          "id": "MDRW2100038255.1.4",
          "form": "얼마나 쓰셧나요?",
          "original_form": "얼마나 쓰셧나요?",
          "speaker_id": "1",
          "time": "20210902 03:51"
        },
        {
          "id": "MDRW2100038255.1.5",
          "form": "3년 채웠어요",
          "original_form": "3년 채웠어요",
          "speaker_id": "2",
          "time": "20210902 03:57"
        },
        {
          "id": "MDRW2100038255.1.6",
          "form": "오 그정도면 오래쓴거 아니죠ㅋㅋ",
          "original_form": "오 그정도면 오래쓴거 아니죠ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 04:00"
        },
        {
          "id": "MDRW2100038255.1.7",
          "form": "그런가요",
          "original_form": "그런가요",
          "speaker_id": "2",
          "time": "20210902 04:01"
        },
        {
          "id": "MDRW2100038255.1.8",
          "form": "네 전 그 두배 쓴적있어요ㅋㅋ",
          "original_form": "네 전 그 두배 쓴적있어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 04:01"
        },
        {
          "id": "MDRW2100038255.1.9",
          "form": "어머나",
          "original_form": "어머나",
          "speaker_id": "2",
          "time": "20210902 04:01"
        },
        {
          "id": "MDRW2100038255.1.10",
          "form": "폰이 작동 잘 했나요?",
          "original_form": "폰이 작동 잘 했나요?",
          "speaker_id": "2",
          "time": "20210902 04:01"
        },
        {
          "id": "MDRW2100038255.1.11",
          "form": "신기하죠?",
          "original_form": "신기하죠?",
          "speaker_id": "1",
          "time": "20210902 04:02"
        },
        {
          "id": "MDRW2100038255.1.12",
          "form": "아니요ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아니요ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 04:02"
        },
        {
          "id": "MDRW2100038255.1.13",
          "form": "처음봤어요 6년 쓴 사람은",
          "original_form": "처음봤어요 6년 쓴 사람은",
          "speaker_id": "2",
          "time": "20210902 04:02"
        },
        {
          "id": "MDRW2100038255.1.14",
          "form": "저애요ㅎㅎ",
          "original_form": "저애요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 04:02"
        },
        {
          "id": "MDRW2100038255.1.15",
          "form": "옛날엔 폰이 더 후졌죠",
          "original_form": "옛날엔 폰이 더 후졌죠",
          "speaker_id": "1",
          "time": "20210902 04:02"
        },
        {
          "id": "MDRW2100038255.1.16",
          "form": "정말 대단하신듯해요",
          "original_form": "정말 대단하신듯해요",
          "speaker_id": "2",
          "time": "20210902 04:02"
        }
