      "utterance": [
        {
          "id": "MDRW2100005265.1.1",
          "form": "음식 얘기 재밌는데 계속해요!!",
          "original_form": "음식 얘기 재밌는데 계속해요!!",
          "speaker_id": "1",
          "time": "20210821 21:31"
        },
        {
          "id": "MDRW2100005265.1.2",
          "form": "네 ㅋㅋㅋ좋아요.ㅋㅋ",
          "original_form": "네 ㅋㅋㅋ좋아요.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:42"
        },
        {
          "id": "MDRW2100005265.1.3",
          "form": "야식은 뭐드세요?",
          "original_form": "야식은 뭐드세요?",
          "speaker_id": "2",
          "time": "20210821 21:43"
        },
        {
          "id": "MDRW2100005265.1.4",
          "form": "야식은 아무래도 치킨? 많이 시켜요",
          "original_form": "야식은 아무래도 치킨? 많이 시켜요",
          "speaker_id": "1",
          "time": "20210821 21:45"
        },
        {
          "id": "MDRW2100005265.1.5",
          "form": "매운맛? 단짠?",
          "original_form": "매운맛? 단짠?",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005265.1.6",
          "form": "치킨에 맥주하시나요?ㅋㅋ",
          "original_form": "치킨에 맥주하시나요?ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:46"
        },
        {
          "id": "MDRW2100005265.1.7",
          "form": "치킨 시즈닝이나 양념이 하도 다양해서",
          "original_form": "치킨 시즈닝이나 양념이 하도 다양해서",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005265.1.8",
          "form": "매번 다르게 시키는 것 같아요",
          "original_form": "매번 다르게 시키는 것 같아요",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005265.1.9",
          "form": "치맥 너무 좋죠 ㅎ",
          "original_form": "치맥 너무 좋죠 ㅎ",
          "speaker_id": "1",
          "time": "20210821 21:47"
        },
        {
          "id": "MDRW2100005265.1.10",
          "form": "아아 다양하게 좋네요.ㅋㅋ",
          "original_form": "아아 다양하게 좋네요.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005265.1.11",
          "form": "네 치맥좋아해요.ㅋㅋ",
          "original_form": "네 치맥좋아해요.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 21:48"
        },
        {
          "id": "MDRW2100005265.1.12",
          "form": "어떤 치킨 제일 좋아하세요? 브랜드나",
          "original_form": "어떤 치킨 제일 좋아하세요? 브랜드나",
          "speaker_id": "1",
          "time": "20210821 21:49"
        },
        {
          "id": "MDRW2100005265.1.13",
          "form": "저는 교촌치킨좋아해요.",
          "original_form": "저는 교촌치킨좋아해요.",
          "speaker_id": "2",
          "time": "20210821 22:54"
        },
        {
          "id": "MDRW2100005265.1.14",
          "form": "교촌치킨 맛있죠ㅠ",
          "original_form": "교촌치킨 맛있죠ㅠ",
          "speaker_id": "1",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005265.1.15",
          "form": "기본간장 레드 허니 중에 어떤 맛 좋아흐세요?",
          "original_form": "기본간장 레드 허니 중에 어떤 맛 좋아흐세요?",
          "speaker_id": "1",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005265.1.16",
          "form": "레드요ㅋㅋㅋ레드가 제일 맛있어요.",
          "original_form": "레드요ㅋㅋㅋ레드가 제일 맛있어요.",
          "speaker_id": "2",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005265.1.17",
          "form": "좋아하는 브랜드있으세요?",
          "original_form": "좋아하는 브랜드있으세요?",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005265.1.18",
          "form": "레드 좋아하시는군요ㅋㅋㅋㅋ",
          "original_form": "레드 좋아하시는군요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005265.1.19",
          "form": "음 저는 교촌도 좋고",
          "original_form": "음 저는 교촌도 좋고",
          "speaker_id": "1",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005265.1.20",
          "form": "bhc bbq 많이 시키는 거 같아요",
          "original_form": "bhc bbq 많이 시키는 거 같아요",
          "speaker_id": "1",
          "time": "20210821 22:59"
        },
        {
          "id": "MDRW2100005265.1.21",
          "form": "아아 좋죠 좋죠 ㅋ어떤맛 좋아하세요?",
          "original_form": "아아 좋죠 좋죠 ㅋ어떤맛 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005265.1.22",
          "form": "지금 생각나는 건 뿌링클이네요ㅋㅋㅋ",
          "original_form": "지금 생각나는 건 뿌링클이네요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 23:05"
        },
        {
          "id": "MDRW2100005265.1.23",
          "form": "뿌링클 맛있죠",
          "original_form": "뿌링클 맛있죠",
          "speaker_id": "2",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005265.1.24",
          "form": "급으로 먹고싶네요. 히히",
          "original_form": "급으로 먹고싶네요. 히히",
          "speaker_id": "2",
          "time": "20210821 23:07"
        },
        {
          "id": "MDRW2100005265.1.25",
          "form": "bhc는 뿌링클 말고도",
          "original_form": "bhc는 뿌링클 말고도",
          "speaker_id": "1",
          "time": "20210821 23:09"
        },
        {
          "id": "MDRW2100005265.1.26",
          "form": "소떡소떡 이런 디저트가",
          "original_form": "소떡소떡 이런 디저트가",
          "speaker_id": "1",
          "time": "20210821 23:09"
        },
        {
          "id": "MDRW2100005265.1.27",
          "form": "너무 잘 나와서 먹고 싶어요",
          "original_form": "너무 잘 나와서 먹고 싶어요",
          "speaker_id": "1",
          "time": "20210821 23:09"
        },
        {
          "id": "MDRW2100005265.1.28",
          "form": "막상 시키려면 너무 비싸서 못 시키지ㅏㅁㄴ...",
          "original_form": "막상 시키려면 너무 비싸서 못 시키지ㅏㅁㄴ...",
          "speaker_id": "1",
          "time": "20210821 23:09"
        },
        {
          "id": "MDRW2100005265.1.29",
          "form": "네네 ㅋㅋㅋ 맞아요.ㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋ 맞아요.ㅋㅋㅋ",
