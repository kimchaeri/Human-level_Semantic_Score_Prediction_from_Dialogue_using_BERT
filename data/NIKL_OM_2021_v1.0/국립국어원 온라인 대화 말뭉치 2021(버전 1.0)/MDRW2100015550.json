      "utterance": [
        {
          "id": "MDRW2100015550.1.1",
          "form": "너 펜트하우스 봐?? 나 요즘 그거에 꽂혔어",
          "original_form": "너 펜트하우스 봐?? 나 요즘 그거에 꽂혔어",
          "speaker_id": "2",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015550.1.2",
          "form": "아니 나 안보는데",
          "original_form": "아니 나 안보는데",
          "speaker_id": "1",
          "time": "20210825 23:30"
        },
        {
          "id": "MDRW2100015550.1.3",
          "form": "헐 펜하를 안봐??",
          "original_form": "헐 펜하를 안봐??",
          "speaker_id": "2",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015550.1.4",
          "form": "펜트하우스 왜 재미써?",
          "original_form": "펜트하우스 왜 재미써?",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015550.1.5",
          "form": "난 그런 공포? 막장 드라마 잘 안봐서 ㅎㅎ..",
          "original_form": "난 그런 공포? 막장 드라마 잘 안봐서 ㅎㅎ..",
          "speaker_id": "1",
          "time": "20210825 23:31"
        },
        {
          "id": "MDRW2100015550.1.6",
          "form": "펜트하우스 막장이긴 한데 ...",
          "original_form": "펜트하우스 막장이긴 한데 ...",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015550.1.7",
          "form": "몰라 넘 재미써!!",
          "original_form": "몰라 넘 재미써!!",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015550.1.8",
          "form": "그래??",
          "original_form": "그래??",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015550.1.9",
          "form": "궁금하면 직접봐^^",
          "original_form": "궁금하면 직접봐^^",
          "speaker_id": "2",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015550.1.10",
          "form": "또 보는 드라마 있어?",
          "original_form": "또 보는 드라마 있어?",
          "speaker_id": "1",
          "time": "20210825 23:32"
        },
        {
          "id": "MDRW2100015550.1.11",
          "form": "내 최애 드라마 별에서 온 그대",
          "original_form": "내 최애 드라마 별에서 온 그대",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015550.1.12",
          "form": "너는 최애가 뭐야?",
          "original_form": "너는 최애가 뭐야?",
          "speaker_id": "2",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015550.1.13",
          "form": "나는 김비서가 왜이럴까",
          "original_form": "나는 김비서가 왜이럴까",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015550.1.14",
          "form": "박서준이랑 박민영나오는거",
          "original_form": "박서준이랑 박민영나오는거",
          "speaker_id": "1",
          "time": "20210825 23:33"
        },
        {
          "id": "MDRW2100015550.1.15",
          "form": "너무 재밋게 봐써",
          "original_form": "너무 재밋게 봐써",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015550.1.16",
          "form": "오오 그거 나도 본 적 있음! ^^",
          "original_form": "오오 그거 나도 본 적 있음! ^^",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015550.1.17",
          "form": "박서준 너무 멋있지 않아?",
          "original_form": "박서준 너무 멋있지 않아?",
          "speaker_id": "1",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015550.1.18",
          "form": "그니까 ㅠ",
          "original_form": "그니까 ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:34"
        },
        {
          "id": "MDRW2100015550.1.19",
          "form": "좋아하는 연예인 있어?",
          "original_form": "좋아하는 연예인 있어?",
          "speaker_id": "1",
          "time": "20210825 23:35"
        },
        {
          "id": "MDRW2100015550.1.20",
          "form": "펜트하우스 이지아 넘 조아",
          "original_form": "펜트하우스 이지아 넘 조아",
          "speaker_id": "2",
