      "utterance": [
        {
          "id": "MDRW2100041926.1.1",
          "form": "근데 그거 아세요",
          "original_form": "근데 그거 아세요",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041926.1.2",
          "form": "왤케 날카롭게 ㅋ",
          "original_form": "왤케 날카롭게 ㅋ",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041926.1.3",
          "form": "뭐냐 두부도 은근 칼로리 높더라구요",
          "original_form": "뭐냐 두부도 은근 칼로리 높더라구요",
          "speaker_id": "2",
          "time": "20210906 17:46"
        },
        {
          "id": "MDRW2100041926.1.4",
          "form": "두부 한모에 270칼로리나 한대요 ㅠ",
          "original_form": "두부 한모에 270칼로리나 한대요 ㅠ",
          "speaker_id": "2",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100041926.1.5",
          "form": "넘하시넹",
          "original_form": "넘하시넹",
          "speaker_id": "2",
          "time": "20210906 17:47"
        },
        {
          "id": "MDRW2100041926.1.6",
          "form": "하긴 그래도 그정도면 나쁘지 않긴 하네요 양념치면 삼밸칼로리 넘겠지만",
          "original_form": "하긴 그래도 그정도면 나쁘지 않긴 하네요 양념치면 삼밸칼로리 넘겠지만",
          "speaker_id": "2",
          "time": "20210906 17:48"
        },
        {
          "id": "MDRW2100041926.1.7",
          "form": "참나 스트레스 받으면 살빠지죠 무슨소리",
          "original_form": "참나 스트레스 받으면 살빠지죠 무슨소리",
          "speaker_id": "2",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041926.1.8",
          "form": "ㅠㅠ 맛있는거 안먹으면 화가 풀리지 않아",
          "original_form": "ㅠㅠ 맛있는거 안먹으면 화가 풀리지 않아",
          "speaker_id": "2",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041926.1.9",
          "form": "우리 뚱땡이",
          "original_form": "우리 뚱땡이",
          "speaker_id": "1",
          "time": "20210906 17:49"
        },
        {
          "id": "MDRW2100041926.1.10",
          "form": "뚱땡이라고 하지 마욧 흑흑",
          "original_form": "뚱땡이라고 하지 마욧 흑흑",
          "speaker_id": "2",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041926.1.11",
          "form": "우리 공주",
          "original_form": "우리 공주",
          "speaker_id": "1",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041926.1.12",
          "form": "그건 좋아",
          "original_form": "그건 좋아",
          "speaker_id": "2",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041926.1.13",
          "form": "너 좋아하는 유부도 칼로리 높으니까 조심해",
          "original_form": "너 좋아하는 유부도 칼로리 높으니까 조심해",
          "speaker_id": "1",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041926.1.14",
          "form": "튀긴거니까 어쩔수없긴 한데 엄마 잘아네요",
          "original_form": "튀긴거니까 어쩔수없긴 한데 엄마 잘아네요",
          "speaker_id": "2",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041926.1.15",
          "form": "많이 공부했지 엄마도 다이어트 했으니까",
          "original_form": "많이 공부했지 엄마도 다이어트 했으니까",
          "speaker_id": "1",
          "time": "20210906 17:50"
        },
        {
          "id": "MDRW2100041926.1.16",
          "form": "엄마도 뺐는데 나는 의지가 부족해서",
          "original_form": "엄마도 뺐는데 나는 의지가 부족해서",
          "speaker_id": "2",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041926.1.17",
          "form": "너두 할수있어 바보야",
          "original_form": "너두 할수있어 바보야",
          "speaker_id": "1",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041926.1.18",
          "form": "바보 아니니까 할수있어 ㅠ",
          "original_form": "바보 아니니까 할수있어 ㅠ",
          "speaker_id": "2",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041926.1.19",
          "form": "그래그래",
          "original_form": "그래그래",
          "speaker_id": "1",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041926.1.20",
          "form": "오늘은 밤에 생선꾸워먹으려구요",
          "original_form": "오늘은 밤에 생선꾸워먹으려구요",
          "speaker_id": "2",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041926.1.21",
          "form": "뭐먹어?",
          "original_form": "뭐먹어?",
          "speaker_id": "1",
          "time": "20210906 17:51"
        },
        {
          "id": "MDRW2100041926.1.22",
          "form": "저 간고등어요 ㅎㅎ 맛있겠죠",
          "original_form": "저 간고등어요 ㅎㅎ 맛있겠죠",
          "speaker_id": "2",
