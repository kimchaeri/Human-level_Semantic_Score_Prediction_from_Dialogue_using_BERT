      "utterance": [
        {
          "id": "MDRW2100006055.1.1",
          "form": "기억에 남는 선 물 있나요",
          "original_form": "기억에 남는 선 물 있나요",
          "speaker_id": "1",
          "time": "20210907 19:47"
        },
        {
          "id": "MDRW2100006055.1.2",
          "form": "작년에 헤어진 남친이 준 손난로요",
          "original_form": "작년에 헤어진 남친이 준 손난로요",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100006055.1.3",
          "form": "남친이 그리워선아니고",
          "original_form": "남친이 그리워선아니고",
          "speaker_id": "2",
          "time": "20210907 19:48"
        },
        {
          "id": "MDRW2100006055.1.4",
          "form": "당근에 팔려고내놨는데동네 사람들이",
          "original_form": "당근에 팔려고내놨는데동네 사람들이",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100006055.1.5",
          "form": "자꾸 깎아달라고 귀찮게해서",
          "original_form": "자꾸 깎아달라고 귀찮게해서",
          "speaker_id": "2",
          "time": "20210907 19:49"
        },
        {
          "id": "MDRW2100006055.1.6",
          "form": "당근에서요?ㅎㅎ",
          "original_form": "당근에서요?ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 19:52"
        },
        {
          "id": "MDRW2100006055.1.7",
          "form": "넵넵ㅋㅋ 터무늬없는 에누리라",
          "original_form": "넵넵ㅋㅋ 터무늬없는 에누리라",
          "speaker_id": "2",
          "time": "20210907 19:57"
        },
        {
          "id": "MDRW2100006055.1.8",
          "form": "그런거 기분나쁘죠 중고거래",
          "original_form": "그런거 기분나쁘죠 중고거래",
          "speaker_id": "1",
          "time": "20210907 19:58"
        },
        {
          "id": "MDRW2100006055.1.9",
          "form": "넵ㅋㅋ역시 당근은 스펙터클하게 깎지않으면 안 팔리나봐요",
          "original_form": "넵ㅋㅋ역시 당근은 스펙터클하게 깎지않으면 안 팔리나봐요",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100006055.1.10",
          "form": "미사용인데도 안팔리는거보면",
          "original_form": "미사용인데도 안팔리는거보면",
          "speaker_id": "2",
          "time": "20210907 20:02"
        },
        {
          "id": "MDRW2100006055.1.11",
          "form": "저는 그런 채팅은 그냥 답 안해요 피곤해요 ㅜ",
          "original_form": "저는 그런 채팅은 그냥 답 안해요 피곤해요 ㅜ",
          "speaker_id": "1",
          "time": "20210907 20:05"
        },
        {
          "id": "MDRW2100006055.1.12",
          "form": "당근하시나보네요ㅋㅋ",
          "original_form": "당근하시나보네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100006055.1.13",
          "form": "당근으로 산적있으세요?",
          "original_form": "당근으로 산적있으세요?",
          "speaker_id": "2",
          "time": "20210907 20:07"
        },
        {
          "id": "MDRW2100006055.1.14",
          "form": "네 ㅎㅎ 당근 애용합니다",
          "original_form": "네 ㅎㅎ 당근 애용합니다",
          "speaker_id": "1",
          "time": "20210907 20:09"
        },
        {
          "id": "MDRW2100006055.1.15",
          "form": "보통 어떤 물건 ㅅ주로 사시나요?",
          "original_form": "보통 어떤 물건 ㅅ주로 사시나요?",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100006055.1.16",
          "form": "저는 주로 캠핑용품..",
          "original_form": "저는 주로 캠핑용품..",
          "speaker_id": "1",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100006055.1.17",
          "form": "와 캠핑즐기시나보네요",
          "original_form": "와 캠핑즐기시나보네요",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100006055.1.18",
          "form": "직접 텐트치는 캠핑요?",
          "original_form": "직접 텐트치는 캠핑요?",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100006055.1.19",
          "form": "네 캠핑 좋아합니다 ㅎㅎ 중고거래 많이해요",
          "original_form": "네 캠핑 좋아합니다 ㅎㅎ 중고거래 많이해요",
