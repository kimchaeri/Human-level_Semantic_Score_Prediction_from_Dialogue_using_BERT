      "utterance": [
        {
          "id": "MDRW2100002221.1.1",
          "form": "터키즈라는 프로 알아?",
          "original_form": "터키즈라는 프로 알아?",
          "speaker_id": "1",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002221.1.2",
          "form": "아 그 이용진 나오는거?",
          "original_form": "아 그 이용진 나오는거?",
          "speaker_id": "2",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002221.1.3",
          "form": "나 그거 완전 재밌게봄 ㅋㅋㅋ",
          "original_form": "나 그거 완전 재밌게봄 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002221.1.4",
          "form": "웅ㅋㅋㅋ 요즘 대박이지",
          "original_form": "웅ㅋㅋㅋ 요즘 대박이지",
          "speaker_id": "1",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002221.1.5",
          "form": "진짜재밌어",
          "original_form": "진짜재밌어",
          "speaker_id": "1",
          "time": "20210820 00:45"
        },
        {
          "id": "MDRW2100002221.1.6",
          "form": "웅 또 뭐봐??",
          "original_form": "웅 또 뭐봐??",
          "speaker_id": "2",
          "time": "20210820 00:49"
        },
        {
          "id": "MDRW2100002221.1.7",
          "form": "나 백종원 유튜브도 좋아해 ㅋㅋㅋ",
          "original_form": "나 백종원 유튜브도 좋아해 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:49"
        },
        {
          "id": "MDRW2100002221.1.8",
          "form": "오 백종원 요리잘하지",
          "original_form": "오 백종원 요리잘하지",
          "speaker_id": "2",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002221.1.9",
          "form": "맞아근데 알려주는거도 쉽게알려줘 진짜 ㅋㅋㅋㅋ",
          "original_form": "맞아근데 알려주는거도 쉽게알려줘 진짜 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002221.1.10",
          "form": "그렇겠다 ㅋㅋㅋ",
          "original_form": "그렇겠다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002221.1.11",
          "form": "집밥백종원도했잖아",
          "original_form": "집밥백종원도했잖아",
          "speaker_id": "2",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002221.1.12",
          "form": "맞아 그거도 재밌게 봤어 ㅋㅋㅋㅋㅋ",
          "original_form": "맞아 그거도 재밌게 봤어 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 00:50"
        },
        {
          "id": "MDRW2100002221.1.13",
          "form": "삼대천왕도했구",
          "original_form": "삼대천왕도했구",
          "speaker_id": "2",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002221.1.14",
          "form": "웅 ㅋㅋ 그거 따라서 많이 다녔음 나는",
          "original_form": "웅 ㅋㅋ 그거 따라서 많이 다녔음 나는",
          "speaker_id": "1",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002221.1.15",
          "form": "오 어디가봤어?",
          "original_form": "오 어디가봤어?",
          "speaker_id": "2",
          "time": "20210820 00:51"
        },
        {
          "id": "MDRW2100002221.1.16",
          "form": "음 그 동네에서 좀 떨어진 곳의 중국집?",
          "original_form": "음 그 동네에서 좀 떨어진 곳의 중국집?",
          "speaker_id": "1",
          "time": "20210820 00:52"
        },
        {
          "id": "MDRW2100002221.1.17",
          "form": "ㅋㅋㅋㅋ맛있었겠다",
          "original_form": "ㅋㅋㅋㅋ맛있었겠다",
          "speaker_id": "2",
