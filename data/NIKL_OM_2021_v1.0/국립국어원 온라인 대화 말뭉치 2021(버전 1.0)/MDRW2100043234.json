      "utterance": [
        {
          "id": "MDRW2100043234.1.1",
          "form": "상품 올린건 팔리셨냐요?",
          "original_form": "상품 올린건 팔리셨냐요?",
          "speaker_id": "1",
          "time": "20210907 16:05"
        },
        {
          "id": "MDRW2100043234.1.2",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "2",
          "time": "20210907 16:08"
        },
        {
          "id": "MDRW2100043234.1.3",
          "form": "당근마켓 쉽지 않네요",
          "original_form": "당근마켓 쉽지 않네요",
          "speaker_id": "2",
          "time": "20210907 16:09"
        },
        {
          "id": "MDRW2100043234.1.4",
          "form": "제가 오늘 썼어요 그래서!!!",
          "original_form": "제가 오늘 썼어요 그래서!!!",
          "speaker_id": "2",
          "time": "20210907 16:09"
        },
        {
          "id": "MDRW2100043234.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ뭘쓰셨나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ뭘쓰셨나요",
          "speaker_id": "1",
          "time": "20210907 16:09"
        },
        {
          "id": "MDRW2100043234.1.6",
          "form": "향수를 그냥 제가 뿌렸어요",
          "original_form": "향수를 그냥 제가 뿌렸어요",
          "speaker_id": "2",
          "time": "20210907 16:10"
        },
        {
          "id": "MDRW2100043234.1.7",
          "form": "향수 팔기 쉽지 않네요",
          "original_form": "향수 팔기 쉽지 않네요",
          "speaker_id": "2",
          "time": "20210907 16:10"
        },
        {
          "id": "MDRW2100043234.1.8",
          "form": "그쵸....",
          "original_form": "그쵸....",
          "speaker_id": "1",
          "time": "20210907 16:10"
        },
        {
          "id": "MDRW2100043234.1.9",
          "form": "중고거래여도 그렇지 너무 후려치더라고요",
          "original_form": "중고거래여도 그렇지 너무 후려치더라고요",
          "speaker_id": "2",
          "time": "20210907 16:11"
        },
        {
          "id": "MDRW2100043234.1.10",
          "form": "최근에 구매하신 물건 자랑해주세요",
          "original_form": "최근에 구매하신 물건 자랑해주세요",
          "speaker_id": "2",
          "time": "20210907 16:11"
        },
        {
          "id": "MDRW2100043234.1.11",
          "form": "최근에 구매한거",
          "original_form": "최근에 구매한거",
          "speaker_id": "1",
          "time": "20210907 16:11"
        },
        {
          "id": "MDRW2100043234.1.12",
          "form": "만두샀어요 얇은피만두",
          "original_form": "만두샀어요 얇은피만두",
          "speaker_id": "1",
          "time": "20210907 16:11"
        },
        {
          "id": "MDRW2100043234.1.13",
          "form": "8봉지에 1.9더라고요",
          "original_form": "8봉지에 1.9더라고요",
          "speaker_id": "1",
          "time": "20210907 16:12"
        },
        {
          "id": "MDRW2100043234.1.14",
          "form": "엄마랑 돈나눳음요",
          "original_form": "엄마랑 돈나눳음요",
          "speaker_id": "1",
          "time": "20210907 16:12"
        },
        {
          "id": "MDRW2100043234.1.15",
          "form": "오 저렴하게 잘 구입했네요",
          "original_form": "오 저렴하게 잘 구입했네요",
          "speaker_id": "2",
          "time": "20210907 16:12"
        },
        {
          "id": "MDRW2100043234.1.16",
          "form": "저는 갑자기 여드름이 폭발하여 효과 좋은 워시오프팩을 구매했습니다",
          "original_form": "저는 갑자기 여드름이 폭발하여 효과 좋은 워시오프팩을 구매했습니다",
          "speaker_id": "2",
          "time": "20210907 16:13"
        },
        {
          "id": "MDRW2100043234.1.17",
          "form": "이번에 올리브영 할인하던데 ㄷ그걸로 사셨나요?",
          "original_form": "이번에 올리브영 할인하던데 ㄷ그걸로 사셨나요?",
          "speaker_id": "1",
          "time": "20210907 16:13"
        },
        {
          "id": "MDRW2100043234.1.18",
          "form": "아 올리브영에서도 필요한 거 좀 샀져",
          "original_form": "아 올리브영에서도 필요한 거 좀 샀져",
          "speaker_id": "2",
          "time": "20210907 16:15"
        },
        {
          "id": "MDRW2100043234.1.19",
          "form": "워시오프팩은 러쉬에서 구매했어요",
          "original_form": "워시오프팩은 러쉬에서 구매했어요",
          "speaker_id": "2",
          "time": "20210907 16:15"
        },
        {
          "id": "MDRW2100043234.1.20",
          "form": "많은 소비를 하셨네요",
          "original_form": "많은 소비를 하셨네요",
          "speaker_id": "1",
          "time": "20210907 16:16"
        },
        {
          "id": "MDRW2100043234.1.21",
          "form": "나 이번 올영 할인 결국 아무것도 못사고 넘어가는듯..",
          "original_form": "나 이번 올영 할인 결국 아무것도 못사고 넘어가는듯..",
          "speaker_id": "1",
          "time": "20210907 16:16"
        },
        {
          "id": "MDRW2100043234.1.22",
          "form": "안 사면 좋은 것입니다 ㅋㅋㅋㅋ 꼭 필요한 거 아님 자제하는게 좋죠",
          "original_form": "안 사면 좋은 것입니다 ㅋㅋㅋㅋ 꼭 필요한 거 아님 자제하는게 좋죠",
          "speaker_id": "2",
          "time": "20210907 16:46"
        },
        {
          "id": "MDRW2100043234.1.23",
          "form": "후 10월에 롭스라도 하겠지?",
          "original_form": "후 10월에 롭스라도 하겠지?",
          "speaker_id": "1",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100043234.1.24",
          "form": "퍼컬끝나고 할게요...",
          "original_form": "퍼컬끝나고 할게요...",
          "speaker_id": "1",
          "time": "20210907 16:49"
        },
        {
          "id": "MDRW2100043234.1.25",
          "form": "그럼 합리적인 소비가 가능합니다 ㅋㅋㅋㅋ",
          "original_form": "그럼 합리적인 소비가 가능합니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 16:50"
        },
        {
          "id": "MDRW2100043234.1.26",
          "form": "재난지원금으로 사려구요",
          "original_form": "재난지원금으로 사려구요",
          "speaker_id": "1",
          "time": "20210907 16:51"
        },
        {
          "id": "MDRW2100043234.1.27",
          "form": "굉장히 합리적이네요",
          "original_form": "굉장히 합리적이네요",
          "speaker_id": "2",
          "time": "20210907 16:52"
        },
        {
          "id": "MDRW2100043234.1.28",
          "form": "지원금 신청하셨나요???",
          "original_form": "지원금 신청하셨나요???",
          "speaker_id": "2",
          "time": "20210907 16:52"
        },
        {
          "id": "MDRW2100043234.1.29",
          "form": "물론이죠",
          "original_form": "물론이죠",
          "speaker_id": "1",
          "time": "20210907 16:52"
        },
        {
          "id": "MDRW2100043234.1.30",
          "form": "올영 되더라 지원음",
          "original_form": "올영 되더라 지원음",
