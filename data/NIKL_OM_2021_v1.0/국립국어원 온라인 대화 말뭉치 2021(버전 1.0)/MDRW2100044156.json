      "utterance": [
        {
          "id": "MDRW2100044156.1.1",
          "form": "요오즘 누가 아들이라고 챙기냐",
          "original_form": "요오즘 누가 아들이라고 챙기냐",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044156.1.2",
          "form": "우리집이 챙긴다",
          "original_form": "우리집이 챙긴다",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044156.1.3",
          "form": "ㅋㅋㅋㅋ 그니까",
          "original_form": "ㅋㅋㅋㅋ 그니까",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044156.1.4",
          "form": "하하",
          "original_form": "하하",
          "speaker_id": "2",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044156.1.5",
          "form": "누가 챙기냐? 우리집이 챙긴담",
          "original_form": "누가 챙기냐? 우리집이 챙긴담",
          "speaker_id": "1",
          "time": "20210907 21:44"
        },
        {
          "id": "MDRW2100044156.1.6",
          "form": "아놔 나쁜말",
          "original_form": "아놔 나쁜말",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044156.1.7",
          "form": "시대에 뒤쳐지는",
          "original_form": "시대에 뒤쳐지는",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044156.1.8",
          "form": "안타까운",
          "original_form": "안타까운",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044156.1.9",
          "form": "냄새가",
          "original_form": "냄새가",
          "speaker_id": "2",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100044156.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ ㅎ ㅏ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ ㅎ ㅏ",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044156.1.11",
          "form": "다들 이렇게 어화둥둥 키우니까",
          "original_form": "다들 이렇게 어화둥둥 키우니까",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044156.1.12",
          "form": "아들이 멀쩡한 집이 손에 꼽지...",
          "original_form": "아들이 멀쩡한 집이 손에 꼽지...",
          "speaker_id": "1",
          "time": "20210907 21:46"
        },
        {
          "id": "MDRW2100044156.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋ... 댁의 아드님이 결혼 못하시는 이유는",
          "original_form": "ㅋㅋㅋㅋㅋㅋ... 댁의 아드님이 결혼 못하시는 이유는",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044156.1.14",
          "form": "바로",
          "original_form": "바로",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044156.1.15",
          "form": "댁 때문니에요!",
          "original_form": "댁 때문니에요!",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044156.1.16",
          "form": "두둥",
          "original_form": "두둥",
          "speaker_id": "2",
          "time": "20210907 21:47"
        },
        {
          "id": "MDRW2100044156.1.17",
          "form": "ㅋㅋㅋㅋ 그러하다",
          "original_form": "ㅋㅋㅋㅋ 그러하다",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044156.1.18",
          "form": "일단 엄마의 자아부터 아들이랑 분리해야함",
          "original_form": "일단 엄마의 자아부터 아들이랑 분리해야함",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044156.1.19",
          "form": "ㅇㅇ..",
          "original_form": "ㅇㅇ..",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044156.1.20",
          "form": "아들은 효도를 할 생각을 버려.. 아님 지가해..",
          "original_form": "아들은 효도를 할 생각을 버려.. 아님 지가해..",
          "speaker_id": "2",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044156.1.21",
          "form": "맞아맞아",
          "original_form": "맞아맞아",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044156.1.22",
          "form": "요즘 효도는 셀프라구",
          "original_form": "요즘 효도는 셀프라구",
          "speaker_id": "1",
          "time": "20210907 21:48"
        },
        {
          "id": "MDRW2100044156.1.23",
          "form": "마자",
          "original_form": "마자",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044156.1.24",
          "form": "그리고 내 몫은 내가 챙겨야 한다규..(슬픔",
          "original_form": "그리고 내 몫은 내가 챙겨야 한다규..(슬픔",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044156.1.25",
          "form": "누우가 배우자한테 대리 효도를 시켜!",
          "original_form": "누우가 배우자한테 대리 효도를 시켜!",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044156.1.26",
          "form": "마자..",
          "original_form": "마자..",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044156.1.27",
          "form": "진짜 내 몫은 내가 챙겨야해...",
          "original_form": "진짜 내 몫은 내가 챙겨야해...",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044156.1.28",
          "form": "글고 가족이랑은 최대한 얼굴 덜봐야함..",
          "original_form": "글고 가족이랑은 최대한 얼굴 덜봐야함..",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044156.1.29",
          "form": "완전...",
          "original_form": "완전...",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044156.1.30",
          "form": "자취하던 친구들은 집에 사는 친구들 부러워했지만",
          "original_form": "자취하던 친구들은 집에 사는 친구들 부러워했지만",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044156.1.31",
          "form": "막상 같이 다시 살라고 하면 난색이었지",
          "original_form": "막상 같이 다시 살라고 하면 난색이었지",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044156.1.32",
          "form": "그냥 집밥 먹고 싶다는 거 말곤....ㅋ",
          "original_form": "그냥 집밥 먹고 싶다는 거 말곤....ㅋ",
          "speaker_id": "2",
          "time": "20210907 21:51"
        },
        {
          "id": "MDRW2100044156.1.33",
          "form": "ㅇㅇ 진짜 집밥 말고는 굳이...?",
          "original_form": "ㅇㅇ 진짜 집밥 말고는 굳이...?",
          "speaker_id": "1",
          "time": "20210907 21:51"
        }
      ]
    }
