      "utterance": [
        {
          "id": "MDRW2100013055.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "2",
          "time": "20210825 11:50"
        },
        {
          "id": "MDRW2100013055.1.2",
          "form": "안녕하세요 \r\n배달음식 어떤거 좋아하세요",
          "original_form": "안녕하세요 \r\n배달음식 어떤거 좋아하세요",
          "speaker_id": "1",
          "time": "20210825 11:53"
        },
        {
          "id": "MDRW2100013055.1.3",
          "form": "전",
          "original_form": "전",
          "speaker_id": "2",
          "time": "20210825 11:55"
        },
        {
          "id": "MDRW2100013055.1.4",
          "form": "초밥 원픽이요",
          "original_form": "초밥 원픽이요",
          "speaker_id": "2",
          "time": "20210825 11:56"
        },
        {
          "id": "MDRW2100013055.1.5",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "2",
          "time": "20210825 11:56"
        },
        {
          "id": "MDRW2100013055.1.6",
          "form": "전 보족 좋아해요\r\n쪽득쪽득ㅋㅋ",
          "original_form": "전 보족 좋아해요\r\n쪽득쪽득ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:57"
        },
        {
          "id": "MDRW2100013055.1.7",
          "form": "쫀ㅋㅋ",
          "original_form": "쫀ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 11:58"
        },
        {
          "id": "MDRW2100013055.1.8",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210825 12:01"
        },
        {
          "id": "MDRW2100013055.1.9",
          "form": "불족도 잘드시나요?",
          "original_form": "불족도 잘드시나요?",
          "speaker_id": "2",
          "time": "20210825 12:01"
        },
        {
          "id": "MDRW2100013055.1.10",
          "form": "불족좋아해요",
          "original_form": "불족좋아해요",
          "speaker_id": "1",
          "time": "20210825 12:03"
        },
        {
          "id": "MDRW2100013055.1.11",
          "form": "주먹밥이랑 같이 먹으면 꿀맛이죠",
          "original_form": "주먹밥이랑 같이 먹으면 꿀맛이죠",
          "speaker_id": "1",
          "time": "20210825 12:03"
        },
        {
          "id": "MDRW2100013055.1.12",
          "form": "맵지않나요?!",
          "original_form": "맵지않나요?!",
          "speaker_id": "2",
          "time": "20210825 12:06"
        },
        {
          "id": "MDRW2100013055.1.13",
          "form": "엄청 맵져",
          "original_form": "엄청 맵져",
          "speaker_id": "1",
          "time": "20210825 12:06"
        },
        {
          "id": "MDRW2100013055.1.14",
          "form": "매워서 손이 덜덜떨려요ㅋㅋ",
          "original_form": "매워서 손이 덜덜떨려요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:06"
        },
        {
          "id": "MDRW2100013055.1.15",
          "form": "근데도 자꾸 먹고싶은맛",
          "original_form": "근데도 자꾸 먹고싶은맛",
          "speaker_id": "1",
          "time": "20210825 12:06"
        },
        {
          "id": "MDRW2100013055.1.16",
          "form": "와우",
          "original_form": "와우",
          "speaker_id": "2",
          "time": "20210825 12:09"
        },
        {
          "id": "MDRW2100013055.1.17",
          "form": "매운걸 잘드시나봐요?",
          "original_form": "매운걸 잘드시나봐요?",
          "speaker_id": "2",
          "time": "20210825 12:09"
        },
        {
          "id": "MDRW2100013055.1.18",
          "form": "잘먹지는 못하는데 좋아해요",
          "original_form": "잘먹지는 못하는데 좋아해요",
          "speaker_id": "1",
          "time": "20210825 12:10"
        },
        {
          "id": "MDRW2100013055.1.19",
          "form": "그래서 닭발도좋아해요",
          "original_form": "그래서 닭발도좋아해요",
          "speaker_id": "1",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100013055.1.20",
          "form": "딸꾹질하면서 먹져ㅋㅋ",
          "original_form": "딸꾹질하면서 먹져ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:11"
        },
        {
          "id": "MDRW2100013055.1.21",
          "form": "ㅋㅋㅋ 엽턱",
          "original_form": "ㅋㅋㅋ 엽턱",
          "speaker_id": "2",
          "time": "20210825 12:14"
        },
        {
          "id": "MDRW2100013055.1.22",
          "form": "몇단계드세요",
          "original_form": "몇단계드세요",
          "speaker_id": "2",
          "time": "20210825 12:14"
        },
        {
          "id": "MDRW2100013055.1.23",
          "form": "와 엽떡은 진짜 맵자나요",
          "original_form": "와 엽떡은 진짜 맵자나요",
          "speaker_id": "1",
          "time": "20210825 12:15"
        },
        {
          "id": "MDRW2100013055.1.24",
          "form": "그건 딸꾹질수준이 아닌데",
          "original_form": "그건 딸꾹질수준이 아닌데",
          "speaker_id": "1",
          "time": "20210825 12:15"
        },
        {
          "id": "MDRW2100013055.1.25",
          "form": "울면서먹어여 그건",
          "original_form": "울면서먹어여 그건",
          "speaker_id": "1",
          "time": "20210825 12:16"
        },
        {
          "id": "MDRW2100013055.1.26",
          "form": "ㅋㅋㅋㅋ 허억",
          "original_form": "ㅋㅋㅋㅋ 허억",
          "speaker_id": "2",
          "time": "20210825 12:19"
        },
        {
          "id": "MDRW2100013055.1.27",
          "form": "잘드시는게아니시군요",
          "original_form": "잘드시는게아니시군요",
          "speaker_id": "2",
          "time": "20210825 12:19"
        },
        {
          "id": "MDRW2100013055.1.28",
          "form": "전 초보맛으로 먹는답니다",
          "original_form": "전 초보맛으로 먹는답니다",
          "speaker_id": "2",
          "time": "20210825 12:19"
        },
        {
          "id": "MDRW2100013055.1.29",
          "form": "저도 매운거 진짜못먹어여 \r\n맛있어서 먹는거져",
          "original_form": "저도 매운거 진짜못먹어여 \r\n맛있어서 먹는거져",
          "speaker_id": "1",
          "time": "20210825 12:20"
        },
        {
          "id": "MDRW2100013055.1.30",
          "form": "그래서매운건 항상 배달로만\r\n먹을때 너무 추해서 밖에선 못먹네여ㅋㅋ",
          "original_form": "그래서매운건 항상 배달로만\r\n먹을때 너무 추해서 밖에선 못먹네여ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:21"
        }
      ]
    }
  ]
