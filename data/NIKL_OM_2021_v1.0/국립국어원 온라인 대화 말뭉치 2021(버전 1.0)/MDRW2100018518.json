      "utterance": [
        {
          "id": "MDRW2100018518.1.1",
          "form": "아뇨 아침 안먹었어요",
          "original_form": "아뇨 아침 안먹었어요",
          "speaker_id": "2",
          "time": "20210827 07:47"
        },
        {
          "id": "MDRW2100018518.1.2",
          "form": "드셨나요?",
          "original_form": "드셨나요?",
          "speaker_id": "2",
          "time": "20210827 07:47"
        },
        {
          "id": "MDRW2100018518.1.3",
          "form": "아니요 오늘도 아침은 패스하려구요",
          "original_form": "아니요 오늘도 아침은 패스하려구요",
          "speaker_id": "1",
          "time": "20210827 07:47"
        },
        {
          "id": "MDRW2100018518.1.4",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "2",
          "time": "20210827 07:47"
        },
        {
          "id": "MDRW2100018518.1.5",
          "form": "원래 아침 드세요?",
          "original_form": "원래 아침 드세요?",
          "speaker_id": "2",
          "time": "20210827 07:47"
        },
        {
          "id": "MDRW2100018518.1.6",
          "form": "잘 안먹을 때가 더 많은 거 같아요",
          "original_form": "잘 안먹을 때가 더 많은 거 같아요",
          "speaker_id": "1",
          "time": "20210827 07:48"
        },
        {
          "id": "MDRW2100018518.1.7",
          "form": "name2님은 어떠신가요",
          "original_form": "&name2&님은 어떠신가요",
          "speaker_id": "1",
          "time": "20210827 07:48"
        },
        {
          "id": "MDRW2100018518.1.8",
          "form": "ㅋㅋ 어 저두요",
          "original_form": "ㅋㅋ 어 저두요",
          "speaker_id": "2",
          "time": "20210827 07:49"
        },
        {
          "id": "MDRW2100018518.1.9",
          "form": "밥보다는 잠이더 소중..",
          "original_form": "밥보다는 잠이더 소중..",
          "speaker_id": "2",
          "time": "20210827 07:49"
        },
        {
          "id": "MDRW2100018518.1.10",
          "form": "바쁜현대인에게.. 아침을 먹는건 너무 힘든일입니다ㅎ",
          "original_form": "바쁜현대인에게.. 아침을 먹는건 너무 힘든일입니다ㅎ",
          "speaker_id": "1",
          "time": "20210827 07:50"
        },
        {
          "id": "MDRW2100018518.1.11",
          "form": "생수 어떤거드세요",
          "original_form": "생수 어떤거드세요",
          "speaker_id": "1",
          "time": "20210827 07:51"
        },
        {
          "id": "MDRW2100018518.1.12",
          "form": "저 지리산수요ㅋㅋ",
          "original_form": "저 지리산수요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 08:17"
        },
        {
          "id": "MDRW2100018518.1.13",
          "form": "어디꺼 드세요?",
          "original_form": "어디꺼 드세요?",
          "speaker_id": "2",
          "time": "20210827 08:18"
        },
        {
          "id": "MDRW2100018518.1.14",
          "form": "전 동원생수마셨는데",
          "original_form": "전 동원생수마셨는데",
          "speaker_id": "1",
          "time": "20210827 08:18"
        },
        {
          "id": "MDRW2100018518.1.15",
          "form": "얼마전 부적합 판정나서 지금 너무 충격이에요",
          "original_form": "얼마전 부적합 판정나서 지금 너무 충격이에요",
          "speaker_id": "1",
          "time": "20210827 08:18"
        },
        {
          "id": "MDRW2100018518.1.16",
          "form": "아 정말요???",
          "original_form": "아 정말요???",
          "speaker_id": "2",
          "time": "20210827 08:18"
        },
        {
          "id": "MDRW2100018518.1.17",
          "form": "저도 예전에 마셨는데",
          "original_form": "저도 예전에 마셨는데",
          "speaker_id": "2",
          "time": "20210827 08:18"
        },
        {
          "id": "MDRW2100018518.1.18",
          "form": "전 묘하게 물맛이 별로라 바꿨어요",
          "original_form": "전 묘하게 물맛이 별로라 바꿨어요",
          "speaker_id": "2",
          "time": "20210827 08:19"
        },
        {
          "id": "MDRW2100018518.1.19",
          "form": "생수 삼다수 빼고 전부난리인거 같더라구요",
          "original_form": "생수 삼다수 빼고 전부난리인거 같더라구요",
          "speaker_id": "1",
          "time": "20210827 08:19"
        },
        {
          "id": "MDRW2100018518.1.20",
          "form": "스파클 아이시스 석수 풀무원 피코크 크리스탈",
          "original_form": "스파클 아이시스 석수 풀무원 피코크 크리스탈",
          "speaker_id": "1",
          "time": "20210827 08:19"
        },
        {
          "id": "MDRW2100018518.1.21",
          "form": "전부한번씩은 마셔본것들이라 더 충격ㅠ",
          "original_form": "전부한번씩은 마셔본것들이라 더 충격ㅠ",
          "speaker_id": "1",
          "time": "20210827 08:20"
        },
        {
          "id": "MDRW2100018518.1.22",
          "form": "충격이네요 ㅠ",
          "original_form": "충격이네요 ㅠ",
          "speaker_id": "2",
          "time": "20210827 08:21"
        },
        {
          "id": "MDRW2100018518.1.23",
          "form": "입으로 들어가는건ㄷ0",
          "original_form": "입으로 들어가는건ㄷ0",
          "speaker_id": "2",
          "time": "20210827 08:21"
        },
        {
          "id": "MDRW2100018518.1.24",
          "form": "너무해여",
          "original_form": "너무해여",
          "speaker_id": "2",
          "time": "20210827 08:21"
        },
        {
          "id": "MDRW2100018518.1.25",
          "form": "그러니까요..",
          "original_form": "그러니까요..",
          "speaker_id": "1",
          "time": "20210827 08:21"
        },
        {
          "id": "MDRW2100018518.1.26",
