      "utterance": [
        {
          "id": "MDRW2100027791.1.1",
          "form": "좋아하는 책 장르가 있으세요?",
          "original_form": "좋아하는 책 장르가 있으세요?",
          "speaker_id": "1",
          "time": "20210902 15:12"
        },
        {
          "id": "MDRW2100027791.1.2",
          "form": "저는 요즘 소설을 많이 읽어요.",
          "original_form": "저는 요즘 소설을 많이 읽어요.",
          "speaker_id": "2",
          "time": "20210902 15:21"
        },
        {
          "id": "MDRW2100027791.1.3",
          "form": "특히 sf신간이요.",
          "original_form": "특히 sf신간이요.",
          "speaker_id": "2",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027791.1.4",
          "form": "어떤 장르의 책을 좋아하세요?",
          "original_form": "어떤 장르의 책을 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 15:22"
        },
        {
          "id": "MDRW2100027791.1.5",
          "form": "저는 추리소설이용",
          "original_form": "저는 추리소설이용",
          "speaker_id": "1",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100027791.1.6",
          "form": "sf소설 추천해 주실거 있나요",
          "original_form": "sf소설 추천해 주실거 있나요",
          "speaker_id": "1",
          "time": "20210902 15:27"
        },
        {
          "id": "MDRW2100027791.1.7",
          "form": "저도 추리소설 좋아해요",
          "original_form": "저도 추리소설 좋아해요",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027791.1.8",
          "form": "보건교사 안은영 작가인 정세랑 작가님 책 중에 지구에서 한아뿐을 재밌게 봤어요",
          "original_form": "보건교사 안은영 작가인 정세랑 작가님 책 중에 지구에서 한아뿐을 재밌게 봤어요",
          "speaker_id": "2",
          "time": "20210902 15:28"
        },
        {
          "id": "MDRW2100027791.1.9",
          "form": "원래 로맨스는 싫어했는데 sf요소가 섞이니까 재밌더라구요",
          "original_form": "원래 로맨스는 싫어했는데 sf요소가 섞이니까 재밌더라구요",
          "speaker_id": "2",
          "time": "20210902 15:29"
        },
        {
          "id": "MDRW2100027791.1.10",
          "form": "아 보건교사 안은영 많이 들어봤는데 그런요소가 들어있었군요",
          "original_form": "아 보건교사 안은영 많이 들어봤는데 그런요소가 들어있었군요",
          "speaker_id": "1",
          "time": "20210902 17:23"
        },
        {
          "id": "MDRW2100027791.1.11",
          "form": "아 다른 책이네요 ㅋㅋ잘못 이해했어요",
          "original_form": "아 다른 책이네요 ㅋㅋ잘못 이해했어요",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100027791.1.12",
          "form": "한아뿐이 사람이름인가요?",
          "original_form": "한아뿐이 사람이름인가요?",
          "speaker_id": "1",
          "time": "20210902 17:24"
        },
        {
          "id": "MDRW2100027791.1.13",
          "form": "한아가 주인공 이름인데",
          "original_form": "한아가 주인공 이름인데",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100027791.1.14",
          "form": "하나뿐 이라는 말이랑 비슷하게 들리도록 만든 것 같아요",
          "original_form": "하나뿐 이라는 말이랑 비슷하게 들리도록 만든 것 같아요",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100027791.1.15",
          "form": "추리 소설은 어떤 거 좋아하세요?",
          "original_form": "추리 소설은 어떤 거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210902 17:38"
        },
        {
          "id": "MDRW2100027791.1.16",
          "form": "저는 히가시노 게이고 자주봐요",
          "original_form": "저는 히가시노 게이고 자주봐요",
          "speaker_id": "1",
          "time": "20210902 18:02"
        },
        {
          "id": "MDRW2100027791.1.17",
          "form": "술술 읽히고 쉬워서요",
          "original_form": "술술 읽히고 쉬워서요",
          "speaker_id": "1",
          "time": "20210902 18:02"
        },
        {
          "id": "MDRW2100027791.1.18",
          "form": "얼마 전에도 신작이 나왔더라고요",
          "original_form": "얼마 전에도 신작이 나왔더라고요",
          "speaker_id": "2",
          "time": "20210902 18:10"
        },
        {
          "id": "MDRW2100027791.1.19",
          "form": "저는 유성의 인연 좋아해요",
          "original_form": "저는 유성의 인연 좋아해요",
          "speaker_id": "2",
          "time": "20210902 18:11"
        },
        {
          "id": "MDRW2100027791.1.20",
          "form": "아 저도 그거좋아해요",
          "original_form": "아 저도 그거좋아해요",
          "speaker_id": "1",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100027791.1.21",
          "form": "신작은 따라가기가 벅차서 아직 못읽었네ㅇᆞ",
          "original_form": "신작은 따라가기가 벅차서 아직 못읽었네ㅇᆞ",
          "speaker_id": "1",
          "time": "20210902 19:55"
        },
        {
          "id": "MDRW2100027791.1.22",
          "form": "저도 신작은 아직...",
          "original_form": "저도 신작은 아직...",
          "speaker_id": "2",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100027791.1.23",
          "form": "유명한 것만 좀 읽어 본 것 같아요",
          "original_form": "유명한 것만 좀 읽어 본 것 같아요",
          "speaker_id": "2",
          "time": "20210902 19:56"
        },
        {
          "id": "MDRW2100027791.1.24",
          "form": "한달에 몇권정도 읽으세요?",
          "original_form": "한달에 몇권정도 읽으세요?",
          "speaker_id": "1",
          "time": "20210902 19:57"
        },
        {
          "id": "MDRW2100027791.1.25",
          "form": "학기중에는 4-5권 정도? 방학에는 그래도 더 많이 읽는 것 같아요",
          "original_form": "학기중에는 4-5권 정도? 방학에는 그래도 더 많이 읽는 것 같아요",
          "speaker_id": "2",
          "time": "20210902 20:00"
        },
        {
          "id": "MDRW2100027791.1.26",
          "form": "아 학생이신가봐요!",
          "original_form": "아 학생이신가봐요!",
