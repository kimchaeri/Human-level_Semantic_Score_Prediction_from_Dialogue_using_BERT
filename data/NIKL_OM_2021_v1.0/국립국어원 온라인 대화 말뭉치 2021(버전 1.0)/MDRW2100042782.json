      "utterance": [
        {
          "id": "MDRW2100042782.1.1",
          "form": "가을이라 선선하네요",
          "original_form": "가을이라 선선하네요",
          "speaker_id": "2",
          "time": "20210907 09:01"
        },
        {
          "id": "MDRW2100042782.1.2",
          "form": "남쪽도",
          "original_form": "남쪽도",
          "speaker_id": "2",
          "time": "20210907 09:01"
        },
        {
          "id": "MDRW2100042782.1.3",
          "form": "비행기는 몇시에 타시나요?",
          "original_form": "비행기는 몇시에 타시나요?",
          "speaker_id": "1",
          "time": "20210907 09:01"
        },
        {
          "id": "MDRW2100042782.1.4",
          "form": "1시반이여",
          "original_form": "1시반이여",
          "speaker_id": "2",
          "time": "20210907 09:01"
        },
        {
          "id": "MDRW2100042782.1.5",
          "form": "다행이 바람이세지않아서 괜찮겠네요",
          "original_form": "다행이 바람이세지않아서 괜찮겠네요",
          "speaker_id": "1",
          "time": "20210907 09:01"
        },
        {
          "id": "MDRW2100042782.1.6",
          "form": "추석전에 미리 부모님 뵙고 오는거예요",
          "original_form": "추석전에 미리 부모님 뵙고 오는거예요",
          "speaker_id": "2",
          "time": "20210907 09:01"
        },
        {
          "id": "MDRW2100042782.1.7",
          "form": "네에",
          "original_form": "네에",
          "speaker_id": "2",
          "time": "20210907 09:01"
        },
        {
          "id": "MDRW2100042782.1.8",
          "form": "아..그러시군요",
          "original_form": "아..그러시군요",
          "speaker_id": "1",
          "time": "20210907 09:01"
        },
        {
          "id": "MDRW2100042782.1.9",
          "form": "비가오면 비행기 못뜨시는줄 아시던데",
          "original_form": "비가오면 비행기 못뜨시는줄 아시던데",
          "speaker_id": "2",
          "time": "20210907 09:01"
        },
        {
          "id": "MDRW2100042782.1.10",
          "form": "제주도는 태풍오면 비행기  못뜨잖아요",
          "original_form": "제주도는 태풍오면 비행기  못뜨잖아요",
          "speaker_id": "1",
          "time": "20210907 09:02"
        },
        {
          "id": "MDRW2100042782.1.11",
          "form": "크게 많이 안오면 잘 다니더라구요",
          "original_form": "크게 많이 안오면 잘 다니더라구요",
          "speaker_id": "2",
          "time": "20210907 09:02"
        },
        {
          "id": "MDRW2100042782.1.12",
          "form": "태풍은 그..구렇죠",
          "original_form": "태풍은 그..구렇죠",
          "speaker_id": "2",
          "time": "20210907 09:02"
        },
        {
          "id": "MDRW2100042782.1.13",
          "form": "아..코로나로 제주도 못간지가 오래네요",
          "original_form": "아..코로나로 제주도 못간지가 오래네요",
          "speaker_id": "1",
          "time": "20210907 09:02"
        },
        {
          "id": "MDRW2100042782.1.14",
          "form": "담주 월욜쯤 태풍예상이래요",
          "original_form": "담주 월욜쯤 태풍예상이래요",
          "speaker_id": "2",
          "time": "20210907 09:02"
        },
        {
          "id": "MDRW2100042782.1.15",
          "form": "제주도 땅값비싸죠?",
          "original_form": "제주도 땅값비싸죠?",
          "speaker_id": "1",
          "time": "20210907 09:02"
        },
        {
          "id": "MDRW2100042782.1.16",
          "form": "끝나면 놀러오겠단 사람들 많네요",
          "original_form": "끝나면 놀러오겠단 사람들 많네요",
          "speaker_id": "2",
          "time": "20210907 09:02"
        },
        {
          "id": "MDRW2100042782.1.17",
          "form": "저도 내려간지 얼마안되서",
          "original_form": "저도 내려간지 얼마안되서",
          "speaker_id": "2",
          "time": "20210907 09:03"
        },
        {
          "id": "MDRW2100042782.1.18",
          "form": "제주도에 예쁘게집짓고 살고 싶네요",
          "original_form": "제주도에 예쁘게집짓고 살고 싶네요",
          "speaker_id": "1",
          "time": "20210907 09:03"
        },
        {
          "id": "MDRW2100042782.1.19",
          "form": "근데 제주가 생각보다 맑은날이 많지않아요",
          "original_form": "근데 제주가 생각보다 맑은날이 많지않아요",
          "speaker_id": "2",
          "time": "20210907 09:03"
        },
        {
          "id": "MDRW2100042782.1.20",
          "form": "아..그래요?",
          "original_form": "아..그래요?",
          "speaker_id": "1",
          "time": "20210907 09:03"
        },
        {
          "id": "MDRW2100042782.1.21",
          "form": "네 그리고 바다 근처라 습하구요",
          "original_form": "네 그리고 바다 근처라 습하구요",
          "speaker_id": "2",
          "time": "20210907 09:03"
        }
      ]
