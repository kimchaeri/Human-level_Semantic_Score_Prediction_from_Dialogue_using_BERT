      "utterance": [
        {
          "id": "MDRW2100043382.1.1",
          "form": "저녁은 어떤거 드실예정이신가요?",
          "original_form": "저녁은 어떤거 드실예정이신가요?",
          "speaker_id": "1",
          "time": "20210907 18:40"
        },
        {
          "id": "MDRW2100043382.1.2",
          "form": "저 비빔밥먹엇어요",
          "original_form": "저 비빔밥먹엇어요",
          "speaker_id": "2",
          "time": "20210907 18:40"
        },
        {
          "id": "MDRW2100043382.1.3",
          "form": "식사하셧어요?",
          "original_form": "식사하셧어요?",
          "speaker_id": "2",
          "time": "20210907 18:40"
        },
        {
          "id": "MDRW2100043382.1.4",
          "form": "네네 저 리조또 먹었어용",
          "original_form": "네네 저 리조또 먹었어용",
          "speaker_id": "1",
          "time": "20210907 18:42"
        },
        {
          "id": "MDRW2100043382.1.5",
          "form": "오오 리조또",
          "original_form": "오오 리조또",
          "speaker_id": "2",
          "time": "20210907 18:42"
        },
        {
          "id": "MDRW2100043382.1.6",
          "form": "무슨 소스엿나요??",
          "original_form": "무슨 소스엿나요??",
          "speaker_id": "2",
          "time": "20210907 18:43"
        },
        {
          "id": "MDRW2100043382.1.7",
          "form": "불닭크림리조또요 ! 갑자기 확끌려서 해먹었네요 ㅎㅎ",
          "original_form": "불닭크림리조또요 ! 갑자기 확끌려서 해먹었네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 18:44"
        },
        {
          "id": "MDRW2100043382.1.8",
          "form": "우와 ㅠㅠ",
          "original_form": "우와 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043382.1.9",
          "form": "맛잇엇겟네요 ㅠㅠ",
          "original_form": "맛잇엇겟네요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043382.1.10",
          "form": "매운거 좋아하세요?",
          "original_form": "매운거 좋아하세요?",
          "speaker_id": "2",
          "time": "20210907 18:46"
        },
        {
          "id": "MDRW2100043382.1.11",
          "form": "네 완전 좋아해요! 좋아하시나욭?",
          "original_form": "네 완전 좋아해요! 좋아하시나욭?",
          "speaker_id": "1",
          "time": "20210907 18:48"
        },
        {
          "id": "MDRW2100043382.1.12",
          "form": "네 저도 매운거 좋아해요",
          "original_form": "네 저도 매운거 좋아해요",
          "speaker_id": "2",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100043382.1.13",
          "form": "그런데 좋아하는것만큼 잘먹지는 못해요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그런데 좋아하는것만큼 잘먹지는 못해요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100043382.1.14",
          "form": "적당히 매운맛만 잘 먹는 정도 ㅋㅋㅋㅋ",
          "original_form": "적당히 매운맛만 잘 먹는 정도 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 18:49"
        },
        {
          "id": "MDRW2100043382.1.15",
          "form": "저도 그래요ㅋㅋ 엄청 잘먹진 않는데 좋아해요",
          "original_form": "저도 그래요ㅋㅋ 엄청 잘먹진 않는데 좋아해요",
          "speaker_id": "1",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100043382.1.16",
          "form": "매워하면서도 끌리더라구요",
          "original_form": "매워하면서도 끌리더라구요",
          "speaker_id": "1",
          "time": "20210907 18:51"
        },
        {
          "id": "MDRW2100043382.1.17",
          "form": "ㅋㅋㅋ어느정도까지 매운거 드세요?",
          "original_form": "ㅋㅋㅋ어느정도까지 매운거 드세요?",
          "speaker_id": "2",
          "time": "20210907 18:52"
        },
        {
          "id": "MDRW2100043382.1.18",
          "form": "음 .. 불닭은 그냥 딱좋게 먹고 엽떡 덜매운맛은 살짝 매워하는정도..! 입니다",
          "original_form": "음 .. 불닭은 그냥 딱좋게 먹고 엽떡 덜매운맛은 살짝 매워하는정도..! 입니다",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043382.1.19",
          "form": "근데 매운걸 자주 안먹으면 약간 더 못먹게되더라구요..",
          "original_form": "근데 매운걸 자주 안먹으면 약간 더 못먹게되더라구요..",
          "speaker_id": "1",
          "time": "20210907 18:54"
        },
        {
          "id": "MDRW2100043382.1.20",
          "form": "오 저랑비슷할것같아요",
          "original_form": "오 저랑비슷할것같아요",
          "speaker_id": "2",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043382.1.21",
          "form": "저도 불닭 딱 맛잇게 먹을 정도거든요",
          "original_form": "저도 불닭 딱 맛잇게 먹을 정도거든요",
          "speaker_id": "2",
          "time": "20210907 18:55"
        },
        {
          "id": "MDRW2100043382.1.22",
          "form": "오 그러네요ㅋㅋ 불닭 진짜 맛있어요..ㅠ",
          "original_form": "오 그러네요ㅋㅋ 불닭 진짜 맛있어요..ㅠ",
          "speaker_id": "1",
