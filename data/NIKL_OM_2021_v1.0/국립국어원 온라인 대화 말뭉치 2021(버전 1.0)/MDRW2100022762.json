      "utterance": [
        {
          "id": "MDRW2100022762.1.1",
          "form": "요새 사고싶은물건있으세요~?",
          "original_form": "요새 사고싶은물건있으세요~?",
          "speaker_id": "2",
          "time": "20210829 18:05"
        },
        {
          "id": "MDRW2100022762.1.2",
          "form": "네 아끼 말씀드린거같지만 제 폰 케이스요 ㅋㅋㅋ",
          "original_form": "네 아끼 말씀드린거같지만 제 폰 케이스요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022762.1.3",
          "form": "그리고 예쁜 신발 갖고싶네용!ㅎㅎㅎ",
          "original_form": "그리고 예쁜 신발 갖고싶네용!ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022762.1.4",
          "form": "왤케 쇼핑하는게 어려운짘ㅋㅋ",
          "original_form": "왤케 쇼핑하는게 어려운짘ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 18:40"
        },
        {
          "id": "MDRW2100022762.1.5",
          "form": "너무 종류가 다양해서 구런가봐요 ㅋㅋㅋ",
          "original_form": "너무 종류가 다양해서 구런가봐요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 19:31"
        },
        {
          "id": "MDRW2100022762.1.6",
          "form": "신발은 사실 신어봐야 아는거잖아용 ㅎㅎㅎ 그래서 더 신중하게 되나봐요ㅜ",
          "original_form": "신발은 사실 신어봐야 아는거잖아용 ㅎㅎㅎ 그래서 더 신중하게 되나봐요ㅜ",
          "speaker_id": "1",
          "time": "20210829 19:45"
        },
        {
          "id": "MDRW2100022762.1.7",
          "form": "맞아여 전 샌들이나 슬리퍼 여름신발만 온라인으로사요ㅠ",
          "original_form": "맞아여 전 샌들이나 슬리퍼 여름신발만 온라인으로사요ㅠ",
          "speaker_id": "2",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100022762.1.8",
          "form": "운동화랑 부츠는 꼭신어봐요ㅠㅠ",
          "original_form": "운동화랑 부츠는 꼭신어봐요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 19:53"
        },
        {
          "id": "MDRW2100022762.1.9",
          "form": "맞아요 실패해서 버린게 많아요ㅠㅠ",
          "original_form": "맞아요 실패해서 버린게 많아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100022762.1.10",
          "form": "신고 돌아다니다보니 불편하고 안맞고",
          "original_form": "신고 돌아다니다보니 불편하고 안맞고",
          "speaker_id": "1",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100022762.1.11",
          "form": "이미 신어서 반품도 안되구요ㅜ",
          "original_form": "이미 신어서 반품도 안되구요ㅜ",
          "speaker_id": "1",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100022762.1.12",
          "form": "그쵸그쵸ㅠㅠ",
          "original_form": "그쵸그쵸ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 20:15"
        },
        {
          "id": "MDRW2100022762.1.13",
          "form": "전 작년부터 살도쪄도 옷사기도 이제 살펴봐요ㅠㅍ 상세사이즈ㅠ 원래는 그냥 디자인보고삿능대ㅠㅠ",
          "original_form": "전 작년부터 살도쪄도 옷사기도 이제 살펴봐요ㅠㅍ 상세사이즈ㅠ 원래는 그냥 디자인보고삿능대ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100022762.1.14",
          "form": "ㅠ ㅠ 맞아요 사이즈같아도",
          "original_form": "ㅠ ㅠ 맞아요 사이즈같아도",
          "speaker_id": "1",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100022762.1.15",
          "form": "편차가 너무 심하죠?",
          "original_form": "편차가 너무 심하죠?",
          "speaker_id": "1",
          "time": "20210829 20:16"
        },
        {
          "id": "MDRW2100022762.1.16",
          "form": "인터넷 쇼핑하기 어려워요 ㅋㅋ",
          "original_form": "인터넷 쇼핑하기 어려워요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100022762.1.17",
          "form": "그래도 재밌고 좋지만용^^;",
          "original_form": "그래도 재밌고 좋지만용^^;",
          "speaker_id": "1",
          "time": "20210829 20:17"
        },
        {
          "id": "MDRW2100022762.1.18",
          "form": "맞아요ㅠㅠ ~보다보면 장바구니가득이에요",
          "original_form": "맞아요ㅠㅠ ~보다보면 장바구니가득이에요",
          "speaker_id": "2",
          "time": "20210829 20:19"
        },
        {
          "id": "MDRW2100022762.1.19",
          "form": "돈이 많아서 사고싶은거 원없이 한번 사보고파요 ㅋㅋㅋㄱ",
          "original_form": "돈이 많아서 사고싶은거 원없이 한번 사보고파요 ㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100022762.1.20",
          "form": "장바구니 담아놓은건 한번에 결제하기! ㅋㅋㅋ",
          "original_form": "장바구니 담아놓은건 한번에 결제하기! ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 20:20"
        },
        {
          "id": "MDRW2100022762.1.21",
          "form": "ㅋㅋㅋㅋㅋ돈걱정없이",
          "original_form": "ㅋㅋㅋㅋㅋ돈걱정없이",
          "speaker_id": "2",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100022762.1.22",
          "form": "쇼핑하면 ㅋㅋㅋ더 즐겁겟죠ㅠㅠ",
          "original_form": "쇼핑하면 ㅋㅋㅋ더 즐겁겟죠ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 20:21"
        },
        {
          "id": "MDRW2100022762.1.23",
          "form": "세상 살맛날거같아요 ㅋㄱㅋㅋ",
          "original_form": "세상 살맛날거같아요 ㅋㄱㅋㅋ",
