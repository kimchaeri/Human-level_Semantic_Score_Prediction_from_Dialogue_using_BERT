      "utterance": [
        {
          "id": "MDRW2100031168.1.1",
          "form": "학교 생활이라 좋지",
          "original_form": "학교 생활이라 좋지",
          "speaker_id": "2",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100031168.1.2",
          "form": "개강은 어때 재밌지?!",
          "original_form": "개강은 어때 재밌지?!",
          "speaker_id": "1",
          "time": "20210904 17:45"
        },
        {
          "id": "MDRW2100031168.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100031168.1.4",
          "form": "전 기숙사 왔습니다",
          "original_form": "전 기숙사 왔습니다",
          "speaker_id": "2",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100031168.1.5",
          "form": "개강 두근두근해",
          "original_form": "개강 두근두근해",
          "speaker_id": "2",
          "time": "20210904 17:46"
        },
        {
          "id": "MDRW2100031168.1.6",
          "form": "아직안했어?",
          "original_form": "아직안했어?",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031168.1.7",
          "form": "했자나",
          "original_form": "했자나",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031168.1.8",
          "form": "개강???",
          "original_form": "개강???",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031168.1.9",
          "form": "ㅊㅋㅊㅋ 기숙사!",
          "original_form": "ㅊㅋㅊㅋ 기숙사!",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031168.1.10",
          "form": "응? 해서 두근두근하다고 ㅋㅋㅋㅋㅋ",
          "original_form": "응? 해서 두근두근하다고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031168.1.11",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031168.1.12",
          "form": "대면하지?",
          "original_form": "대면하지?",
          "speaker_id": "1",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031168.1.13",
          "form": "음 거의 비대면으로 돌렸더라",
          "original_form": "음 거의 비대면으로 돌렸더라",
          "speaker_id": "2",
          "time": "20210904 17:47"
        },
        {
          "id": "MDRW2100031168.1.14",
          "form": "실험 같은 경우는 희망자만 대면인데 난 가서 하려고",
          "original_form": "실험 같은 경우는 희망자만 대면인데 난 가서 하려고",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031168.1.15",
          "form": "그럼 기숙사 괜히왔네",
          "original_form": "그럼 기숙사 괜히왔네",
          "speaker_id": "1",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031168.1.16",
          "form": "등록금 아깝잖아",
          "original_form": "등록금 아깝잖아",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031168.1.17",
          "form": "아 등록ㅇ금",
          "original_form": "아 등록ㅇ금",
          "speaker_id": "1",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031168.1.18",
          "form": "아깝긴하지",
          "original_form": "아깝긴하지",
          "speaker_id": "1",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031168.1.19",
          "form": "아니야 물리전자가 대면이라 뭐 어쩔수 없긴했어",
          "original_form": "아니야 물리전자가 대면이라 뭐 어쩔수 없긴했어",
          "speaker_id": "2",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031168.1.20",
          "form": "물전 !",
          "original_form": "물전 !",
          "speaker_id": "1",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031168.1.21",
          "form": "재밌는과목",
          "original_form": "재밌는과목",
          "speaker_id": "1",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031168.1.22",
          "form": "물전배우고 알려줘 ^_^",
          "original_form": "물전배우고 알려줘 ^_^",
          "speaker_id": "1",
          "time": "20210904 17:48"
        },
        {
          "id": "MDRW2100031168.1.23",
          "form": "응응 배우는건 난 다 앵간히 재미있어",
          "original_form": "응응 배우는건 난 다 앵간히 재미있어",
          "speaker_id": "2",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100031168.1.24",
          "form": "내가? 4학년을?",
          "original_form": "내가? 4학년을?",
          "speaker_id": "2",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100031168.1.25",
          "form": "영광입니다",
          "original_form": "영광입니다",
          "speaker_id": "2",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100031168.1.26",
          "form": "응응 아싸",
          "original_form": "응응 아싸",
          "speaker_id": "1",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100031168.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그러게 넌 벌써 막학기네",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ그러게 넌 벌써 막학기네",
          "speaker_id": "2",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100031168.1.28",
          "form": "시간표는 좀 여유롭지?",
          "original_form": "시간표는 좀 여유롭지?",
          "speaker_id": "2",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100031168.1.29",
          "form": "물전책 다시봐야대 면접보려면",
          "original_form": "물전책 다시봐야대 면접보려면",
          "speaker_id": "1",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100031168.1.30",
          "form": "근데 진짜 어렵던데",
          "original_form": "근데 진짜 어렵던데",
          "speaker_id": "1",
          "time": "20210904 17:49"
        },
        {
          "id": "MDRW2100031168.1.31",
          "form": "그래? 가능하면 도와줄게",
          "original_form": "그래? 가능하면 도와줄게",
          "speaker_id": "2",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100031168.1.32",
          "form": "내가 당시 어케했는지 정말 ",
          "original_form": "내가 당시 어케했는지 정말 ",
          "speaker_id": "1",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100031168.1.33",
          "form": "너 머리 좋잖아",
          "original_form": "너 머리 좋잖아",
          "speaker_id": "2",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100031168.1.34",
          "form": "잘해 너",
          "original_form": "잘해 너",
          "speaker_id": "2",
          "time": "20210904 17:50"
        },
        {
          "id": "MDRW2100031168.1.35",
          "form": "너가 더",
          "original_form": "너가 더",
          "speaker_id": "1",
          "time": "20210904 17:50"
        }
      ]
