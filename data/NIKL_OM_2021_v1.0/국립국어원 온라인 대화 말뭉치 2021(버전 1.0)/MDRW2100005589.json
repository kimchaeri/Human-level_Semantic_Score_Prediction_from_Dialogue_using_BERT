      "utterance": [
        {
          "id": "MDRW2100005589.1.1",
          "form": "안녕하세요! 혹시 어떤 반려동물 키우시나요?",
          "original_form": "안녕하세요! 혹시 어떤 반려동물 키우시나요?",
          "speaker_id": "1",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005589.1.2",
          "form": "저는 반려동물을 못 키워요ㅜㅜ",
          "original_form": "저는 반려동물을 못 키워요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 23:12"
        },
        {
          "id": "MDRW2100005589.1.3",
          "form": "털 알러지가 있어서요",
          "original_form": "털 알러지가 있어서요",
          "speaker_id": "2",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005589.1.4",
          "form": "name1 님은 반려동물 키우시나요?",
          "original_form": "&name1& 님은 반려동물 키우시나요?",
          "speaker_id": "2",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005589.1.5",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210821 23:13"
        },
        {
          "id": "MDRW2100005589.1.6",
          "form": "어떤 반려동물인가요?",
          "original_form": "어떤 반려동물인가요?",
          "speaker_id": "2",
          "time": "20210821 23:14"
        },
        {
          "id": "MDRW2100005589.1.7",
          "form": "지금 4살 말티즈 입니다",
          "original_form": "지금 4살 말티즈 입니다",
          "speaker_id": "1",
          "time": "20210821 23:15"
        },
        {
          "id": "MDRW2100005589.1.8",
          "form": "털 알레르기 있다해도 좋아하시는 반려동물 무엇인지요?",
          "original_form": "털 알레르기 있다해도 좋아하시는 반려동물 무엇인지요?",
          "speaker_id": "1",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005589.1.9",
          "form": "말티즈 정말 귀여울 것 같아요! 저도 강아지 좋아해요",
          "original_form": "말티즈 정말 귀여울 것 같아요! 저도 강아지 좋아해요",
          "speaker_id": "2",
          "time": "20210821 23:16"
        },
        {
          "id": "MDRW2100005589.1.10",
          "form": "오오 강아지 이쁘죠 ㅎㅎ",
          "original_form": "오오 강아지 이쁘죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100005589.1.11",
          "form": "강아지는 이름이 뭔가요?",
          "original_form": "강아지는 이름이 뭔가요?",
          "speaker_id": "2",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100005589.1.12",
          "form": "name3 입니다",
          "original_form": "&name3& 입니다",
          "speaker_id": "1",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100005589.1.13",
          "form": "고양이도 혹시 좋아하시나요?",
          "original_form": "고양이도 혹시 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210821 23:17"
        },
        {
          "id": "MDRW2100005589.1.14",
          "form": "고양이 좋아해요!",
          "original_form": "고양이 좋아해요!",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005589.1.15",
          "form": "알러지가 괜찮아지면 꼭 키워보고 싶어요",
          "original_form": "알러지가 괜찮아지면 꼭 키워보고 싶어요",
          "speaker_id": "2",
          "time": "20210821 23:18"
        },
        {
          "id": "MDRW2100005589.1.16",
          "form": "아쉽군요 ㅠㅠ 강아지 키운다하면 무슨 종 키우고 싶으시나요",
          "original_form": "아쉽군요 ㅠㅠ 강아지 키운다하면 무슨 종 키우고 싶으시나요",
          "speaker_id": "1",
          "time": "20210821 23:19"
        },
        {
          "id": "MDRW2100005589.1.17",
          "form": "저는 도베르만이 좋아요",
          "original_form": "저는 도베르만이 좋아요",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005589.1.18",
          "form": "친구가 키웠는데 멋있더라고요",
          "original_form": "친구가 키웠는데 멋있더라고요",
          "speaker_id": "2",
          "time": "20210821 23:21"
        },
        {
          "id": "MDRW2100005589.1.19",
          "form": "도베르만 듬직한 견이죠",
          "original_form": "도베르만 듬직한 견이죠",
          "speaker_id": "1",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005589.1.20",
          "form": "훈련 열심히 시키면 정말 듬직한 가족이 됄겁니다 ㅎㅎ",
          "original_form": "훈련 열심히 시키면 정말 듬직한 가족이 됄겁니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:26"
        },
        {
          "id": "MDRW2100005589.1.21",
          "form": "말티즈는 어때요? 훈련이 많이 필요한가요?",
          "original_form": "말티즈는 어때요? 훈련이 많이 필요한가요?",
          "speaker_id": "2",
          "time": "20210821 23:27"
        },
        {
          "id": "MDRW2100005589.1.22",
          "form": "말티즈는 다른견들 보면 훈련이 정말 많이 쉬운 얘구나 싶어요",
          "original_form": "말티즈는 다른견들 보면 훈련이 정말 많이 쉬운 얘구나 싶어요",
          "speaker_id": "1",
          "time": "20210821 23:28"
        },
        {
          "id": "MDRW2100005589.1.23",
          "form": "와!! 집에서 훈련시키신 거예요?",
          "original_form": "와!! 집에서 훈련시키신 거예요?",
          "speaker_id": "2",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005589.1.24",
          "form": "가족들과 다같이 해봤죠",
          "original_form": "가족들과 다같이 해봤죠",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005589.1.25",
          "form": "그래도 견마다 성격이 좀 다르겠지만",
          "original_form": "그래도 견마다 성격이 좀 다르겠지만",
          "speaker_id": "1",
          "time": "20210821 23:30"
        },
        {
          "id": "MDRW2100005589.1.26",
          "form": "name3이가 많이 절 좋아해서 좀 쉬웠어요 ㅎㅎ",
          "original_form": "&name3&이가 많이 절 좋아해서 좀 쉬웠어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005589.1.27",
          "form": "그렇구나~ 강아지가 잘 따르면 기분 좋으시겠어요",
          "original_form": "그렇구나~ 강아지가 잘 따르면 기분 좋으시겠어요",
          "speaker_id": "2",
          "time": "20210821 23:31"
        },
        {
          "id": "MDRW2100005589.1.28",
          "form": "친구겸 가족이 생겨서 기쁘죠",
          "original_form": "친구겸 가족이 생겨서 기쁘죠",
          "speaker_id": "1",
          "time": "20210821 23:33"
        },
        {
          "id": "MDRW2100005589.1.29",
          "form": "가족처럼 돈독한 사이가 될 수 있다는 게 정말 신기한 것 같아요",
          "original_form": "가족처럼 돈독한 사이가 될 수 있다는 게 정말 신기한 것 같아요",
          "speaker_id": "2",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005589.1.30",
          "form": "강아지가 그런매력이 아닐까싶어요",
          "original_form": "강아지가 그런매력이 아닐까싶어요",
          "speaker_id": "1",
          "time": "20210821 23:35"
        },
        {
          "id": "MDRW2100005589.1.31",
          "form": "언제나 힘들어도 저한테 항상 웃어주는느낌인지라",
          "original_form": "언제나 힘들어도 저한테 항상 웃어주는느낌인지라",
          "speaker_id": "1",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005589.1.32",
          "form": "언제나 제 편이 되어준다는 게 감동이네요",
          "original_form": "언제나 제 편이 되어준다는 게 감동이네요",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005589.1.33",
          "form": "힘들 때 보면 힘날 것 같아요",
          "original_form": "힘들 때 보면 힘날 것 같아요",
          "speaker_id": "2",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005589.1.34",
          "form": "그만큼 좋은 간식 사료 사주고싶죠 ㅎㅎ",
          "original_form": "그만큼 좋은 간식 사료 사주고싶죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 23:36"
        },
        {
          "id": "MDRW2100005589.1.35",
          "form": "경제적 여건 되는 만큼 좋은 거 사주고 싶겠어요",
          "original_form": "경제적 여건 되는 만큼 좋은 거 사주고 싶겠어요",
          "speaker_id": "2",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005589.1.36",
          "form": "혹시 고르는 기준이 있나요?",
          "original_form": "혹시 고르는 기준이 있나요?",
          "speaker_id": "2",
          "time": "20210821 23:37"
        },
        {
          "id": "MDRW2100005589.1.37",
          "form": "그냥 별거 없어요",
          "original_form": "그냥 별거 없어요",
          "speaker_id": "1",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005589.1.38",
          "form": "사료 성분 꼼꼼히보고",
          "original_form": "사료 성분 꼼꼼히보고",
          "speaker_id": "1",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005589.1.39",
          "form": "가격비교하면서 가성비가 좋은가 그런점으로 고릅니다",
          "original_form": "가격비교하면서 가성비가 좋은가 그런점으로 고릅니다",
          "speaker_id": "1",
          "time": "20210821 23:38"
        },
        {
          "id": "MDRW2100005589.1.40",
          "form": "사람 먹는 거랑 똑같네요",
          "original_form": "사람 먹는 거랑 똑같네요",
          "speaker_id": "2",
          "time": "20210821 23:39"
        },
        {
          "id": "MDRW2100005589.1.41",
          "form": "말 못할뿐이지 사람처럼 좋은거 먹어야 강아지도 몸이 튼튼해진다 생각합니다 ㅎㅎ",
          "original_form": "말 못할뿐이지 사람처럼 좋은거 먹어야 강아지도 몸이 튼튼해진다 생각합니다 ㅎㅎ",
