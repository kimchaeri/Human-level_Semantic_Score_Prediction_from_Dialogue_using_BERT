        },
        {
          "id": "MDRW2100034097.1.2",
          "form": "아 저는 주부에요:)",
          "original_form": "아 저는 주부에요:)",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100034097.1.3",
          "form": "name1님은 회사세요?",
          "original_form": "&name1&님은 회사세요?",
          "speaker_id": "2",
          "time": "20210827 15:46"
        },
        {
          "id": "MDRW2100034097.1.4",
          "form": "아항 아뇨 학생이요 ㅋㅋ 일하신적은 없나요*?",
          "original_form": "아항 아뇨 학생이요 ㅋㅋ 일하신적은 없나요*?",
          "speaker_id": "1",
          "time": "20210827 15:47"
        },
        {
          "id": "MDRW2100034097.1.5",
          "form": "아직 학생이시군용:)",
          "original_form": "아직 학생이시군용:)",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100034097.1.6",
          "form": "직장생활 10년 정도하고\r\n지금은 육아로 인해 퇴사했어요",
          "original_form": "직장생활 10년 정도하고\r\n지금은 육아로 인해 퇴사했어요",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100034097.1.7",
          "form": "알바 경험은 있으세요?",
          "original_form": "알바 경험은 있으세요?",
          "speaker_id": "2",
          "time": "20210827 15:48"
        },
        {
          "id": "MDRW2100034097.1.8",
          "form": "헉..10년동안 고생많으셨습니다",
          "original_form": "헉..10년동안 고생많으셨습니다",
          "speaker_id": "1",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100034097.1.9",
          "form": "다른고생 시작이시네요 ㅋㅋㅋ ㅠㅠ",
          "original_form": "다른고생 시작이시네요 ㅋㅋㅋ ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 15:49"
        },
        {
          "id": "MDRW2100034097.1.10",
          "form": "아뇨 알바는 과외만",
          "original_form": "아뇨 알바는 과외만",
          "speaker_id": "1",
          "time": "20210827 15:50"
        },
        {
          "id": "MDRW2100034097.1.11",
          "form": "일도잠깐 햇엇어용 ㅋㅋㅋ",
          "original_form": "일도잠깐 햇엇어용 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 15:50"
        },
        {
          "id": "MDRW2100034097.1.12",
          "form": "그럼 앞으로 다시 일은 안할 계획이신가요",
          "original_form": "그럼 앞으로 다시 일은 안할 계획이신가요",
          "speaker_id": "1",
          "time": "20210827 15:50"
        },
        {
          "id": "MDRW2100034097.1.13",
          "form": "오 과외쌤이셨군요:)",
          "original_form": "오 과외쌤이셨군요:)",
          "speaker_id": "2",
          "time": "20210827 15:51"
        },
        {
          "id": "MDRW2100034097.1.14",
          "form": "아이들 조금 크고나면\r\n다시 일 하려구요^^",
          "original_form": "아이들 조금 크고나면\r\n다시 일 하려구요^^",
          "speaker_id": "2",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100034097.1.15",
          "form": "다시 일하기 괜찮은 직종이신가봐요!",
          "original_form": "다시 일하기 괜찮은 직종이신가봐요!",
          "speaker_id": "1",
          "time": "20210827 15:52"
        },
        {
          "id": "MDRW2100034097.1.16",
          "form": "일이다보니 할때는\r\n스트레스도 받고\r\n힘들때도 있지만은\r\n나름 저한테는 맞는\r\n직업이었던거 같아요^^",
          "original_form": "일이다보니 할때는\r\n스트레스도 받고\r\n힘들때도 있지만은\r\n나름 저한테는 맞는\r\n직업이었던거 같아요^^",
          "speaker_id": "2",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100034097.1.17",
          "form": "name1님은 취준생이신거에요?",
          "original_form": "&name1&님은 취준생이신거에요?",
          "speaker_id": "2",
          "time": "20210827 15:54"
        },
        {
          "id": "MDRW2100034097.1.18",
          "form": "운이좋으셨네용 전 아직 대학원다녀서 졸업하면 취준해야해요ㅜ",
          "original_form": "운이좋으셨네용 전 아직 대학원다녀서 졸업하면 취준해야해요ㅜ",
          "speaker_id": "1",
          "time": "20210827 16:08"
        },
        {
          "id": "MDRW2100034097.1.19",
          "form": "아항 대학원생이군요!",
          "original_form": "아항 대학원생이군요!",
          "speaker_id": "2",
          "time": "20210827 16:12"
        },
        {
          "id": "MDRW2100034097.1.20",
          "form": "물론 업무도 중요하겠지만\r\n좋은 분들이 많이 계신 곳이길 바래요",
          "original_form": "물론 업무도 중요하겠지만\r\n좋은 분들이 많이 계신 곳이길 바래요{emoji:♡}",
