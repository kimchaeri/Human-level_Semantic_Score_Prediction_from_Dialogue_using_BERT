      "utterance": [
        {
          "id": "MDRW2100000531.1.1",
          "form": "재택근무 해본 적 있으세요?",
          "original_form": "재택근무 해본 적 있으세요?",
          "speaker_id": "1",
          "time": "20210819 16:39"
        },
        {
          "id": "MDRW2100000531.1.2",
          "form": "네 계속 재택근무했어요.",
          "original_form": "네 계속 재택근무했어요.",
          "speaker_id": "2",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000531.1.3",
          "form": "온라인수업!",
          "original_form": "온라인수업!",
          "speaker_id": "2",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000531.1.4",
          "form": "ㅋㅋㅋㅋ아 집에서 온라인 수업 들으시나보네요",
          "original_form": "ㅋㅋㅋㅋ아 집에서 온라인 수업 들으시나보네요",
          "speaker_id": "1",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000531.1.5",
          "form": "저는 재택이 불가한 직종이라 재택근무 한번도 해본적 없어요ㅠㅜ",
          "original_form": "저는 재택이 불가한 직종이라 재택근무 한번도 해본적 없어요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210819 16:40"
        },
        {
          "id": "MDRW2100000531.1.6",
          "form": "수업을 하는 입장이에요ㅋㅋㅋㅋ",
          "original_form": "수업을 하는 입장이에요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000531.1.7",
          "form": "앗 재택 불가라니 ㅠㅠ",
          "original_form": "앗 재택 불가라니 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 16:41"
        },
        {
          "id": "MDRW2100000531.1.8",
          "form": "헉 진짜요?ㅋㅋㅋㅋㅋ온라인으로 수업 준비하는 거 힘드시죠? 제 친구도 온라인으로 수업하는데 힘들어하더라고요",
          "original_form": "헉 진짜요?ㅋㅋㅋㅋㅋ온라인으로 수업 준비하는 거 힘드시죠? 제 친구도 온라인으로 수업하는데 힘들어하더라고요",
          "speaker_id": "1",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000531.1.9",
          "form": "진짜,,저도 재택 한번만 해봤음 좋겠어요ㅠㅜ",
          "original_form": "진짜,,저도 재택 한번만 해봤음 좋겠어요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210819 16:42"
        },
        {
          "id": "MDRW2100000531.1.10",
          "form": "흡 너무 힘든데 그래도 점점 적응해가고 있답니다",
          "original_form": "흡 너무 힘든데 그래도 점점 적응해가고 있답니다",
          "speaker_id": "2",
          "time": "20210819 16:43"
        },
        {
          "id": "MDRW2100000531.1.11",
          "form": "재택이 좋을 때도 있고 안 좋을 때도 있고 그래요",
          "original_form": "재택이 좋을 때도 있고 안 좋을 때도 있고 그래요",
          "speaker_id": "2",
          "time": "20210819 16:43"
        },
        {
          "id": "MDRW2100000531.1.12",
          "form": "name1이 님은 그럼 더 조심하셔야겠어요 ㅠㅠ",
          "original_form": "&name1&이 님은 그럼 더 조심하셔야겠어요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210819 16:43"
        },
        {
          "id": "MDRW2100000531.1.13",
          "form": "재택하면 마냥 좋을 줄 알았는데 안 좋은 점도 있나요??",
          "original_form": "재택하면 마냥 좋을 줄 알았는데 안 좋은 점도 있나요??",
          "speaker_id": "1",
          "time": "20210819 16:44"
        },
        {
          "id": "MDRW2100000531.1.14",
          "form": "학생들은 한 명 한 명 봐 주기가 어려운 부분도 있고!",
          "original_form": "학생들은 한 명 한 명 봐 주기가 어려운 부분도 있고!",
          "speaker_id": "2",
          "time": "20210819 16:44"
        },
        {
          "id": "MDRW2100000531.1.15",
          "form": "수업자료도 온라인용으로 따로 준비해야 하고요",
          "original_form": "수업자료도 온라인용으로 따로 준비해야 하고요",
          "speaker_id": "2",
          "time": "20210819 16:44"
        },
        {
          "id": "MDRW2100000531.1.16",
          "form": "일이 더 많아진 느낌이에요",
          "original_form": "일이 더 많아진 느낌이에요",
          "speaker_id": "2",
          "time": "20210819 16:45"
        },
        {
          "id": "MDRW2100000531.1.17",
          "form": "아이고,,,수업자료를 더 많이 만드셔야 하는군요ㅠㅜ",
          "original_form": "아이고,,,수업자료를 더 많이 만드셔야 하는군요ㅠㅜ",
          "speaker_id": "1",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000531.1.18",
          "form": "암튼 그래서 저는 줌이나 이런것도 한번도 써본 적 없어요ㅋㅋㅋ",
          "original_form": "암튼 그래서 저는 줌이나 이런것도 한번도 써본 적 없어요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:47"
        },
        {
          "id": "MDRW2100000531.1.19",
          "form": "친구들이랑도 줌으로 만나고 그러지 않아요?",
          "original_form": "친구들이랑도 줌으로 만나고 그러지 않아요?",
          "speaker_id": "2",
          "time": "20210819 16:55"
        },
        {
          "id": "MDRW2100000531.1.20",
          "form": "재밌어요! 한 번 해 보세요!",
          "original_form": "재밌어요! 한 번 해 보세요!",
          "speaker_id": "2",
          "time": "20210819 16:55"
        },
        {
          "id": "MDRW2100000531.1.21",
          "form": "ㅋㅋㅋ와 줌으로도 만나나요? 신기하네요ㅋㅋㅋㄱ줌은 재택할때만 쓰는 건 줄 알았어요ㅋㅋ",
          "original_form": "ㅋㅋㅋ와 줌으로도 만나나요? 신기하네요ㅋㅋㅋㄱ줌은 재택할때만 쓰는 건 줄 알았어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:56"
        },
        {
          "id": "MDRW2100000531.1.22",
          "form": "모임을 오프라인에서 못하니까",
          "original_form": "모임을 오프라인에서 못하니까",
          "speaker_id": "2",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000531.1.23",
          "form": "각자 집에서 줌 켜서 만나고 그러거든요",
          "original_form": "각자 집에서 줌 켜서 만나고 그러거든요",
          "speaker_id": "2",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000531.1.24",
          "form": "나름 또 재미가 있답니다!",
          "original_form": "나름 또 재미가 있답니다!",
          "speaker_id": "2",
          "time": "20210819 16:57"
        },
        {
          "id": "MDRW2100000531.1.25",
          "form": "와ㅋㅋ신기하네요ㅋㅋㅋㅋ",
          "original_form": "와ㅋㅋ신기하네요ㅋㅋㅋㅋ",
