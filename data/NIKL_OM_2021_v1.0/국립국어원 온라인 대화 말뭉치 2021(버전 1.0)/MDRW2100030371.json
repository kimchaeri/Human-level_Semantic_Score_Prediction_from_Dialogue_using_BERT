      "utterance": [
        {
          "id": "MDRW2100030371.1.1",
          "form": "샐러디 살기 위해 먹었습니다 ㅋㅋ",
          "original_form": "샐러디 살기 위해 먹었습니다 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:14"
        },
        {
          "id": "MDRW2100030371.1.2",
          "form": "무슨일하세요 ? 근무시간이 좀 들쭉날쭉 한거 같으세요 ㅋㅋ",
          "original_form": "무슨일하세요 ? 근무시간이 좀 들쭉날쭉 한거 같으세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:42"
        },
        {
          "id": "MDRW2100030371.1.3",
          "form": "저 지하철이요 ㅎㅎ",
          "original_form": "저 지하철이요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 00:46"
        },
        {
          "id": "MDRW2100030371.1.4",
          "form": "주야비휴에요 ㅋㅋ",
          "original_form": "주야비휴에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030371.1.5",
          "form": "아!! 이해가 됬어요!! ㅋㅋㅋ",
          "original_form": "아!! 이해가 됬어요!! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030371.1.6",
          "form": "전 지하철 없는 동네에 사니 좀 미지의 세계군요 !",
          "original_form": "전 지하철 없는 동네에 사니 좀 미지의 세계군요 !",
          "speaker_id": "2",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030371.1.7",
          "form": "울산은 지하철 없으니 ㅋㅋ",
          "original_form": "울산은 지하철 없으니 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 00:47"
        },
        {
          "id": "MDRW2100030371.1.8",
          "form": "맞아요 ㅋㅋㅋ고래타고 다니잖아요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맞아요 ㅋㅋㅋ고래타고 다니잖아요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 00:48"
        },
        {
          "id": "MDRW2100030371.1.9",
          "form": "저 가면 태워주세요?",
          "original_form": "저 가면 태워주세요?",
          "speaker_id": "1",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030371.1.10",
          "form": "집마다 애완고래 있죠?",
          "original_form": "집마다 애완고래 있죠?",
          "speaker_id": "1",
          "time": "20210904 00:49"
        },
        {
          "id": "MDRW2100030371.1.11",
          "form": "아 당연히 있죠 개인 고래가 있습니다 좀 있는집은 수염고래고 소소하게 밍크고래 정도는 다들 있죠",
          "original_form": "아 당연히 있죠 개인 고래가 있습니다 좀 있는집은 수염고래고 소소하게 밍크고래 정도는 다들 있죠",
          "speaker_id": "2",
          "time": "20210904 00:50"
        },
        {
          "id": "MDRW2100030371.1.12",
          "form": "와 재밌겠다 고래라니",
          "original_form": "와 재밌겠다 고래라니",
          "speaker_id": "1",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030371.1.13",
          "form": "그럼 울산고래공사도 있나요",
          "original_form": "그럼 울산고래공사도 있나요",
          "speaker_id": "1",
          "time": "20210904 01:04"
        },
        {
          "id": "MDRW2100030371.1.14",
          "form": "ㅋㅋㅋㅋㅋ 진짜 있으면 너무좋겠네요 ㅋㅋㅋ  저 다른이들의 직업에 관심이 많아요! ㅋㅋ 지하철에는 어떤일들을 하는 분들이 있나요? ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ 진짜 있으면 너무좋겠네요 ㅋㅋㅋ  저 다른이들의 직업에 관심이 많아요! ㅋㅋ 지하철에는 어떤일들을 하는 분들이 있나요? ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:09"
        },
        {
          "id": "MDRW2100030371.1.15",
          "form": "뭐 역무, 승무, 전자, 토목, 건축, 궤도, 전기",
          "original_form": "뭐 역무, 승무, 전자, 토목, 건축, 궤도, 전기",
          "speaker_id": "1",
          "time": "20210904 01:13"
        },
        {
          "id": "MDRW2100030371.1.16",
          "form": "이정도? 있을거고",
          "original_form": "이정도? 있을거고",
          "speaker_id": "1",
          "time": "20210904 01:13"
        },
        {
          "id": "MDRW2100030371.1.17",
          "form": "오오오 많군요!!",
          "original_form": "오오오 많군요!!",
          "speaker_id": "2",
          "time": "20210904 01:13"
        },
        {
          "id": "MDRW2100030371.1.18",
          "form": "승강장 안전문, 보안관, 후생지원도 있어요",
          "original_form": "승강장 안전문, 보안관, 후생지원도 있어요",
          "speaker_id": "1",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030371.1.19",
          "form": "생각보다도 다양한 분들이 계시군요! ㅋㅋ",
          "original_form": "생각보다도 다양한 분들이 계시군요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030371.1.20",
          "form": "저 고3 입시강사라서 ㅋㅋㅋㅋ관심많습니다 ㅋㅋ",
          "original_form": "저 고3 입시강사라서 ㅋㅋㅋㅋ관심많습니다 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030371.1.21",
          "form": "그럼요 ㅋㅋ 생각보다",
          "original_form": "그럼요 ㅋㅋ 생각보다",
          "speaker_id": "1",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030371.1.22",
          "form": "1,2학년도 수업하지만요 ㅋㅋㅋ",
          "original_form": "1,2학년도 수업하지만요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030371.1.23",
          "form": "다양한 백업이 있어야",
          "original_form": "다양한 백업이 있어야",
          "speaker_id": "1",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030371.1.24",
          "form": "지하철이 갑니다",
          "original_form": "지하철이 갑니다",
          "speaker_id": "1",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030371.1.25",
          "form": "칙칙폭폭",
          "original_form": "칙칙폭폭",
          "speaker_id": "1",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030371.1.26",
          "form": "선생님이 셨군요",
          "original_form": "선생님이 셨군요",
          "speaker_id": "1",
          "time": "20210904 01:14"
        },
        {
          "id": "MDRW2100030371.1.27",
          "form": "사실 지하철이 있는곳에서 살아본적이 없어서 거기까진 생각못했어요!",
          "original_form": "사실 지하철이 있는곳에서 살아본적이 없어서 거기까진 생각못했어요!",
          "speaker_id": "2",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100030371.1.28",
          "form": "선생님은 아니고 그냥 영어강사입니다 ㅋㅋㅋ",
          "original_form": "선생님은 아니고 그냥 영어강사입니다 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 01:15"
        },
        {
          "id": "MDRW2100030371.1.29",
          "form": "요즘애들은 학교에 별로 기대지 않아서 요즘같은 원서넣는 시기에 고민상담이 많이들 오네요 ㅜ",
          "original_form": "요즘애들은 학교에 별로 기대지 않아서 요즘같은 원서넣는 시기에 고민상담이 많이들 오네요 ㅜ",
          "speaker_id": "2",
          "time": "20210904 01:16"
        },
        {
          "id": "MDRW2100030371.1.30",
          "form": "ㅋㅋ음 근데 지하철은 이제 좀 채용이 줄어서...",
          "original_form": "ㅋㅋ음 근데 지하철은 이제 좀 채용이 줄어서...",
          "speaker_id": "1",
          "time": "20210904 01:16"
        },
        {
          "id": "MDRW2100030371.1.31",
          "form": "그쵸? ㅜㅜ 그래도 아이들에겐 희망적인 이야기도 필요하니까...",
          "original_form": "그쵸? ㅜㅜ 그래도 아이들에겐 희망적인 이야기도 필요하니까...",
          "speaker_id": "2",
          "time": "20210904 01:17"
        }
      ]
