      "utterance": [
        {
          "id": "MDRW2100009966.1.1",
          "form": "요즘 책 읽으셨어요?",
          "original_form": "요즘 책 읽으셨어요?",
          "speaker_id": "1",
          "time": "20210823 12:25"
        },
        {
          "id": "MDRW2100009966.1.2",
          "form": "도서관에서 두권 빌려서 한권 다 읽음",
          "original_form": "도서관에서 두권 빌려서 한권 다 읽음",
          "speaker_id": "2",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100009966.1.3",
          "form": "에세이",
          "original_form": "에세이",
          "speaker_id": "2",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100009966.1.4",
          "form": "도서관에서 빌린거면 신작은 아닌가요",
          "original_form": "도서관에서 빌린거면 신작은 아닌가요",
          "speaker_id": "1",
          "time": "20210823 12:26"
        },
        {
          "id": "MDRW2100009966.1.5",
          "form": "ㅠㅠ전 도서관 안간지 오래...",
          "original_form": "ㅠㅠ전 도서관 안간지 오래...",
          "speaker_id": "1",
          "time": "20210823 12:27"
        },
        {
          "id": "MDRW2100009966.1.6",
          "form": "나름 신작..올 3월",
          "original_form": "나름 신작..올 3월",
          "speaker_id": "2",
          "time": "20210823 12:29"
        },
        {
          "id": "MDRW2100009966.1.7",
          "form": "나이답게가 아니라 나!답게",
          "original_form": "나이답게가 아니라 나!답게",
          "speaker_id": "2",
          "time": "20210823 12:29"
        },
        {
          "id": "MDRW2100009966.1.8",
          "form": "부담없어서 하루만에 다 읽음",
          "original_form": "부담없어서 하루만에 다 읽음",
          "speaker_id": "2",
          "time": "20210823 12:33"
        },
        {
          "id": "MDRW2100009966.1.9",
          "form": "둘다 신작이긴 하지만 베스트셀러는 안될듯^^",
          "original_form": "둘다 신작이긴 하지만 베스트셀러는 안될듯^^",
          "speaker_id": "2",
          "time": "20210823 12:34"
        },
        {
          "id": "MDRW2100009966.1.10",
          "form": "베스트셀러는 도서관에 거의 없죠ㅜ",
          "original_form": "베스트셀러는 도서관에 거의 없죠ㅜ",
          "speaker_id": "1",
          "time": "20210823 12:41"
        },
        {
          "id": "MDRW2100009966.1.11",
          "form": "있긴 한데..대여하려면 몇주 기다려야 하지~",
          "original_form": "있긴 한데..대여하려면 몇주 기다려야 하지~",
          "speaker_id": "2",
          "time": "20210823 12:42"
        },
        {
          "id": "MDRW2100009966.1.12",
          "form": "ㅋㅋㅋㅋ저한텐 바로 못보면 없는걸로",
          "original_form": "ㅋㅋㅋㅋ저한텐 바로 못보면 없는걸로",
          "speaker_id": "1",
          "time": "20210823 12:43"
        },
        {
          "id": "MDRW2100009966.1.13",
          "form": "최근에 그래도 서점은 갔는데 악보 사왔어요ㅋㅋ",
          "original_form": "최근에 그래도 서점은 갔는데 악보 사왔어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 12:44"
        },
        {
          "id": "MDRW2100009966.1.14",
          "form": "요즘 전공도서외에는 읽은거 없어?",
          "original_form": "요즘 전공도서외에는 읽은거 없어?",
          "speaker_id": "2",
          "time": "20210823 12:44"
        },
        {
          "id": "MDRW2100009966.1.15",
          "form": "저 판타지소설 읽던거 조금 읽고",
          "original_form": "저 판타지소설 읽던거 조금 읽고",
          "speaker_id": "1",
          "time": "20210823 12:44"
        },
        {
          "id": "MDRW2100009966.1.16",
          "form": "책은 안사고 ㅋ",
          "original_form": "책은 안사고 ㅋ",
          "speaker_id": "2",
          "time": "20210823 12:44"
        },
        {
          "id": "MDRW2100009966.1.17",
          "form": "그외엔 안읽었네요",
          "original_form": "그외엔 안읽었네요",
          "speaker_id": "1",
          "time": "20210823 12:44"
        },
        {
          "id": "MDRW2100009966.1.18",
          "form": "ㅎㅎ 쇼팽책 구매...",
          "original_form": "ㅎㅎ 쇼팽책 구매...",
          "speaker_id": "1",
          "time": "20210823 12:45"
        },
        {
          "id": "MDRW2100009966.1.19",
          "form": "서점갔더니 읽고싶은건 많은데 다 사읽을수는 없으니ㅜ",
          "original_form": "서점갔더니 읽고싶은건 많은데 다 사읽을수는 없으니ㅜ",
          "speaker_id": "1",
          "time": "20210823 12:45"
        },
        {
          "id": "MDRW2100009966.1.20",
          "form": "생각해보니 도서구입은 안한지 꽤 되었네",
          "original_form": "생각해보니 도서구입은 안한지 꽤 되었네",
          "speaker_id": "2",
          "time": "20210823 12:45"
        },
        {
          "id": "MDRW2100009966.1.21",
          "form": "다 도서관대여로",
          "original_form": "다 도서관대여로",
          "speaker_id": "2",
          "time": "20210823 12:46"
        },
        {
          "id": "MDRW2100009966.1.22",
          "form": "책 넘 비싸여ㅜ",
          "original_form": "책 넘 비싸여ㅜ",
          "speaker_id": "1",
          "time": "20210823 12:46"
        },
        {
          "id": "MDRW2100009966.1.23",
          "form": "책가격도 만만치 않아서",
          "original_form": "책가격도 만만치 않아서",
          "speaker_id": "2",
          "time": "20210823 12:46"
        },
        {
          "id": "MDRW2100009966.1.24",
          "form": "맞오",
