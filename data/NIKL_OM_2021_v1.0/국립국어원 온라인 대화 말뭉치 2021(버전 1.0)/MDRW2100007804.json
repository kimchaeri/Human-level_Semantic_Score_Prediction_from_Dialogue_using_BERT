      "utterance": [
        {
          "id": "MDRW2100007804.1.1",
          "form": "고3이면 아무래도 신경쓰이시겠어요 ㅠㅠ",
          "original_form": "고3이면 아무래도 신경쓰이시겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100007804.1.2",
          "form": "그게 신경 쓰고파요 ㅜㅜ",
          "original_form": "그게 신경 쓰고파요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100007804.1.3",
          "form": "고3이 어디있나요",
          "original_form": "고3이 어디있나요",
          "speaker_id": "2",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100007804.1.4",
          "form": "백수세요 ㅋㅋ",
          "original_form": "백수세요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:57"
        },
        {
          "id": "MDRW2100007804.1.5",
          "form": "앗........ ㅠㅠㅠㅠㅠㅠ\r\n왜 어째서죠.. ㅠㅠㅠ",
          "original_form": "앗........ ㅠㅠㅠㅠㅠㅠ\r\n왜 어째서죠.. ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007804.1.6",
          "form": "송부를 안해요",
          "original_form": "송부를 안해요",
          "speaker_id": "2",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007804.1.7",
          "form": "공부",
          "original_form": "공부",
          "speaker_id": "2",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007804.1.8",
          "form": "코로나의 영향인가요?",
          "original_form": "코로나의 영향인가요?",
          "speaker_id": "1",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007804.1.9",
          "form": "아뇨 ㅋㅋ",
          "original_form": "아뇨 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007804.1.10",
          "form": "원래 안해요",
          "original_form": "원래 안해요",
          "speaker_id": "2",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007804.1.11",
          "form": "수시 할거라 할일 다했다며",
          "original_form": "수시 할거라 할일 다했다며",
          "speaker_id": "2",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007804.1.12",
          "form": "아... 제가.. 실례를 했군요 .....",
          "original_form": "아... 제가.. 실례를 했군요 .....",
          "speaker_id": "1",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007804.1.13",
          "form": "방학부터 놀아요",
          "original_form": "방학부터 놀아요",
          "speaker_id": "2",
          "time": "20210822 10:58"
        },
        {
          "id": "MDRW2100007804.1.14",
          "form": "저도 아직 까진 잘 모르지만... ㅎㅎㅎㅎㅎ\r\n첫째아이는 공부랑 거리가 ... 많이 먼거 같애요 ㅎㅎ",
          "original_form": "저도 아직 까진 잘 모르지만... ㅎㅎㅎㅎㅎ\r\n첫째아이는 공부랑 거리가 ... 많이 먼거 같애요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100007804.1.15",
          "form": "그러게요 딸이 오빠는 고3이 왜 히스테리가 없냐고",
          "original_form": "그러게요 딸이 오빠는 고3이 왜 히스테리가 없냐고",
          "speaker_id": "2",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100007804.1.16",
          "form": "보고싶었다고 아쉬워햐요 ㅋㅋ",
          "original_form": "보고싶었다고 아쉬워햐요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100007804.1.17",
          "form": "참나",
          "original_form": "참나",
          "speaker_id": "2",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100007804.1.18",
          "form": "꽃밭이예요 머리가",
          "original_form": "꽃밭이예요 머리가",
          "speaker_id": "2",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100007804.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ\r\n아.........너무 웃프네요 ㅎㅎㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ\r\n아.........너무 웃프네요 ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210822 10:59"
        },
        {
          "id": "MDRW2100007804.1.20",
          "form": "둘째는 조금 다른가요?",
          "original_form": "둘째는 조금 다른가요?",
          "speaker_id": "1",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100007804.1.21",
          "form": "어뇨",
          "original_form": "어뇨",
          "speaker_id": "2",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100007804.1.22",
          "form": "오빠가 저러니",
          "original_form": "오빠가 저러니",
          "speaker_id": "2",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100007804.1.23",
          "form": "근데 뭐든 열심히 해요",
          "original_form": "근데 뭐든 열심히 해요",
          "speaker_id": "2",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100007804.1.24",
          "form": "아.... 희망이 사라졌어요...",
          "original_form": "아.... 희망이 사라졌어요...",
          "speaker_id": "1",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100007804.1.25",
          "form": "예체능으로요",
          "original_form": "예체능으로요",
          "speaker_id": "2",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100007804.1.26",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100007804.1.27",
          "form": "저희애 영재반 이였답니다",
          "original_form": "저희애 영재반 이였답니다",
          "speaker_id": "2",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100007804.1.28",
          "form": "하긴 둘째도...열심히 보고 배우고 있지만.. 하겠지 싶었는데..",
          "original_form": "하긴 둘째도...열심히 보고 배우고 있지만.. 하겠지 싶었는데..",
          "speaker_id": "1",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100007804.1.29",
          "form": "옛날일이죠",
          "original_form": "옛날일이죠",
          "speaker_id": "2",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100007804.1.30",
          "form": "이였다는 과거형",
          "original_form": "이였다는 과거형",
          "speaker_id": "1",
          "time": "20210822 11:00"
        },
        {
          "id": "MDRW2100007804.1.31",
          "form": "네 ㅡㅡ",
          "original_form": "네 ㅡㅡ",
          "speaker_id": "2",
          "time": "20210822 11:01"
        },
        {
          "id": "MDRW2100007804.1.32",
          "form": "ㅠㅠㅠ",
          "original_form": "ㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210822 11:01"
        },
        {
          "id": "MDRW2100007804.1.33",
          "form": "천재인줄 알았네요",
          "original_form": "천재인줄 알았네요",
          "speaker_id": "2",
          "time": "20210822 11:01"
        },
