      "utterance": [
        {
          "id": "MDRW2100035152.1.1",
          "form": "식사 하셨어요?",
          "original_form": "식사 하셨어요?",
          "speaker_id": "1",
          "time": "20210829 10:41"
        },
        {
          "id": "MDRW2100035152.1.2",
          "form": "아직이요 식사하셨어요?",
          "original_form": "아직이요 식사하셨어요?",
          "speaker_id": "2",
          "time": "20210829 10:44"
        },
        {
          "id": "MDRW2100035152.1.3",
          "form": "저도 아직이요",
          "original_form": "저도 아직이요",
          "speaker_id": "1",
          "time": "20210829 10:50"
        },
        {
          "id": "MDRW2100035152.1.4",
          "form": "늦잠자서 아침은 땡됐어요ㅋㅋ",
          "original_form": "늦잠자서 아침은 땡됐어요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 10:50"
        },
        {
          "id": "MDRW2100035152.1.5",
          "form": "저도요ㅋㅋㅋ주말의 특권 즐기고있어요",
          "original_form": "저도요ㅋㅋㅋ주말의 특권 즐기고있어요",
          "speaker_id": "2",
          "time": "20210829 10:51"
        },
        {
          "id": "MDRW2100035152.1.6",
          "form": "역시ㅋㅋ 놓칠 수 없긴하죠",
          "original_form": "역시ㅋㅋ 놓칠 수 없긴하죠",
          "speaker_id": "1",
          "time": "20210829 11:01"
        },
        {
          "id": "MDRW2100035152.1.7",
          "form": "점심은 어떤거 드실예정이세요?",
          "original_form": "점심은 어떤거 드실예정이세요?",
          "speaker_id": "1",
          "time": "20210829 11:02"
        },
        {
          "id": "MDRW2100035152.1.8",
          "form": "아직 고민중인데요 아마 간단하게 샌드위치 해먹을거 같아요",
          "original_form": "아직 고민중인데요 아마 간단하게 샌드위치 해먹을거 같아요",
          "speaker_id": "2",
          "time": "20210829 11:11"
        },
        {
          "id": "MDRW2100035152.1.9",
          "form": "어떤거 드실거예요?",
          "original_form": "어떤거 드실거예요?",
          "speaker_id": "2",
          "time": "20210829 11:11"
        },
        {
          "id": "MDRW2100035152.1.10",
          "form": "샌드위치!",
          "original_form": "샌드위치!",
          "speaker_id": "1",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100035152.1.11",
          "form": "아직 못골랐어요ㅠ",
          "original_form": "아직 못골랐어요ㅠ",
          "speaker_id": "1",
          "time": "20210829 11:12"
        },
        {
          "id": "MDRW2100035152.1.12",
          "form": "스파게티 먹을까싶어욥",
          "original_form": "스파게티 먹을까싶어욥",
          "speaker_id": "1",
          "time": "20210829 11:14"
        },
        {
          "id": "MDRW2100035152.1.13",
          "form": "파스타가 간편하게 먹기좋죠",
          "original_form": "파스타가 간편하게 먹기좋죠",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100035152.1.14",
          "form": "어쩌다보니 둘다 양식이네요",
          "original_form": "어쩌다보니 둘다 양식이네요",
          "speaker_id": "2",
          "time": "20210829 11:21"
        },
        {
          "id": "MDRW2100035152.1.15",
          "form": "맞아요~",
          "original_form": "맞아요~",
          "speaker_id": "1",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100035152.1.16",
          "form": "어 그러네요ㅋㅋ 한식보다 양식이 간편한거같아요",
          "original_form": "어 그러네요ㅋㅋ 한식보다 양식이 간편한거같아요",
          "speaker_id": "1",
          "time": "20210829 11:24"
        },
        {
          "id": "MDRW2100035152.1.17",
          "form": "아무래도 그런거 같아요",
          "original_form": "아무래도 그런거 같아요",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100035152.1.18",
          "form": "한식은 간단히해도 뭘 끓여야해서....",
          "original_form": "한식은 간단히해도 뭘 끓여야해서....",
          "speaker_id": "2",
          "time": "20210829 11:27"
        },
        {
          "id": "MDRW2100035152.1.19",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100035152.1.20",
          "form": "양식은 전자렌지만으로 가능한게 많다보니 자주 찾아져오",
          "original_form": "양식은 전자렌지만으로 가능한게 많다보니 자주 찾아져오",
          "speaker_id": "1",
          "time": "20210829 11:49"
        },
        {
          "id": "MDRW2100035152.1.21",
          "form": "그죠ㅋㅋㅋ 양식중에 파스타 제일 좋아하세요?",
          "original_form": "그죠ㅋㅋㅋ 양식중에 파스타 제일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210829 11:50"
        },
        {
          "id": "MDRW2100035152.1.22",
          "form": "네!!",
          "original_form": "네!!",
          "speaker_id": "1",
          "time": "20210829 11:54"
        },
        {
          "id": "MDRW2100035152.1.23",
          "form": "일주일 내내 먹을 수 있을정도예요ㅋㅋㅋ",
          "original_form": "일주일 내내 먹을 수 있을정도예요ㅋㅋㅋ",
