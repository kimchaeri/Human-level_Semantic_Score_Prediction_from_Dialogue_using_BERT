      "utterance": [
        {
          "id": "MDRW2100025389.1.1",
          "form": "오잉",
          "original_form": "오잉",
          "speaker_id": "2",
          "time": "20210901 10:41"
        },
        {
          "id": "MDRW2100025389.1.2",
          "form": "난요새",
          "original_form": "난요새",
          "speaker_id": "1",
          "time": "20210901 10:42"
        },
        {
          "id": "MDRW2100025389.1.3",
          "form": "쿠키런킹덤하는디",
          "original_form": "쿠키런킹덤하는디",
          "speaker_id": "1",
          "time": "20210901 10:42"
        },
        {
          "id": "MDRW2100025389.1.4",
          "form": "데브도 가 엄청나",
          "original_form": "데브도 {censored}가 엄청나",
          "speaker_id": "1",
          "time": "20210901 10:43"
        },
        {
          "id": "MDRW2100025389.1.5",
          "form": "앗 그 심슨 스프링필드같은",
          "original_form": "앗 그 심슨 스프링필드같은",
          "speaker_id": "2",
          "time": "20210901 10:43"
        },
        {
          "id": "MDRW2100025389.1.6",
          "form": "그 게임",
          "original_form": "그 게임",
          "speaker_id": "2",
          "time": "20210901 10:43"
        },
        {
          "id": "MDRW2100025389.1.7",
          "form": "막 짓고 하던더",
          "original_form": "막 짓고 하던더",
          "speaker_id": "2",
          "time": "20210901 10:43"
        },
        {
          "id": "MDRW2100025389.1.8",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100025389.1.9",
          "form": "완전추억이야 ㅋㅋㅋㄱㄱㄱ",
          "original_form": "완전추억이야 ㅋㅋㅋㄱㄱㄱ",
          "speaker_id": "1",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100025389.1.10",
          "form": "스프링필드",
          "original_form": "스프링필드",
          "speaker_id": "1",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100025389.1.11",
          "form": "누나 2년 넘게했다는",
          "original_form": "누나 2년 넘게했다는",
          "speaker_id": "2",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100025389.1.12",
          "form": "그 전설겜",
          "original_form": "그 전설겜",
          "speaker_id": "2",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100025389.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㄱ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋㅋㅋㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100025389.1.14",
          "form": "그거돈내야해서",
          "original_form": "그거돈내야해서",
          "speaker_id": "1",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100025389.1.15",
          "form": "너무현질유도겜이었어",
          "original_form": "너무현질유도겜이었어",
          "speaker_id": "1",
          "time": "20210901 10:44"
        },
        {
          "id": "MDRW2100025389.1.16",
          "form": "근데킹덤하네",
          "original_form": "근데킹덤하네",
          "speaker_id": "1",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025389.1.17",
          "form": "역시 인간은 같은실수를반복해",
          "original_form": "역시 인간은 같은실수를반복해",
          "speaker_id": "1",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025389.1.18",
          "form": "대단합니다.",
          "original_form": "대단합니다.",
          "speaker_id": "2",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025389.1.19",
          "form": "나도 옵치 끊어내고파",
          "original_form": "나도 옵치 끊어내고파",
          "speaker_id": "2",
          "time": "20210901 10:45"
        },
        {
          "id": "MDRW2100025389.1.20",
          "form": "근데 여기 단체대화가 되나?",
          "original_form": "근데 여기 단체대화가 되나?",
          "speaker_id": "2",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100025389.1.21",
          "form": "아니그건안될거야",
          "original_form": "아니그건안될거야",
          "speaker_id": "1",
          "time": "20210901 10:46"
        },
        {
          "id": "MDRW2100025389.1.22",
          "form": "ㅋㄱㅋㅋㅋㄱㅋㅋ 우리나라 게임들",
          "original_form": "ㅋㄱㅋㅋㅋㄱㅋㅋ 우리나라 게임들",
          "speaker_id": "1",
          "time": "20210901 10:47"
        },
        {
          "id": "MDRW2100025389.1.23",
          "form": "호우 500세트 채우기가 생각보다 힘들겄다",
          "original_form": "호우 500세트 채우기가 생각보다 힘들겄다",
          "speaker_id": "2",
          "time": "20210901 10:47"
        },
        {
          "id": "MDRW2100025389.1.24",
          "form": "모바일겜 너무 돈뽑아내는거 힘듬",
          "original_form": "모바일겜 너무 돈뽑아내는거 힘듬",
          "speaker_id": "1",
          "time": "20210901 10:47"
        },
        {
          "id": "MDRW2100025389.1.25",
          "form": "가챠 시스템 발동!",
          "original_form": "가챠 시스템 발동!",
          "speaker_id": "2",
          "time": "20210901 10:47"
        },
        {
          "id": "MDRW2100025389.1.26",
          "form": "ㅋㅋㅋㅋㄱ쿠키런도 가챠라",
          "original_form": "ㅋㅋㅋㅋㄱ쿠키런도 가챠라",
          "speaker_id": "1",
          "time": "20210901 10:48"
        },
        {
          "id": "MDRW2100025389.1.27",
          "form": "저번에도 4만원씀",
          "original_form": "저번에도 4만원씀",
          "speaker_id": "1",
          "time": "20210901 10:48"
        },
        {
          "id": "MDRW2100025389.1.28",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "2",
          "time": "20210901 10:48"
        },
        {
          "id": "MDRW2100025389.1.29",
          "form": "직장인의 취미생활",
          "original_form": "직장인의 취미생활",
          "speaker_id": "2",
          "time": "20210901 10:48"
        },
        {
          "id": "MDRW2100025389.1.30",
          "form": "게임에돈쓰기...",
          "original_form": "게임에돈쓰기...",
          "speaker_id": "1",
          "time": "20210901 10:48"
        },
        {
          "id": "MDRW2100025389.1.31",
