      "utterance": [
        {
          "id": "MDRW2100006447.1.1",
          "form": "나는 빨간색 텀블러 들구다녕",
          "original_form": "나는 빨간색 텀블러 들구다녕",
          "speaker_id": "2",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100006447.1.2",
          "form": "텀블러 가져가면 무슨 혜택이 있지?",
          "original_form": "텀블러 가져가면 무슨 혜택이 있지?",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100006447.1.3",
          "form": "커피 할인해주더라??",
          "original_form": "커피 할인해주더라??",
          "speaker_id": "2",
          "time": "20210908 00:20"
        },
        {
          "id": "MDRW2100006447.1.4",
          "form": "얼마나?",
          "original_form": "얼마나?",
          "speaker_id": "1",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100006447.1.5",
          "form": "한 100원 해주던데",
          "original_form": "한 100원 해주던데",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100006447.1.6",
          "form": "아 스타벅스는 별 적립 해준당",
          "original_form": "아 스타벅스는 별 적립 해준당",
          "speaker_id": "2",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100006447.1.7",
          "form": "너무 짜다",
          "original_form": "너무 짜다",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100006447.1.8",
          "form": "100원해주다니!!",
          "original_form": "100원해주다니!!",
          "speaker_id": "1",
          "time": "20210908 00:32"
        },
        {
          "id": "MDRW2100006447.1.9",
          "form": "잉  그래? 100원도 모이면 1000원이라구",
          "original_form": "잉  그래? 100원도 모이면 1000원이라구",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100006447.1.10",
          "form": "ㅋㅋㅋㅋㅋ나는 좋앙",
          "original_form": "ㅋㅋㅋㅋㅋ나는 좋앙",
          "speaker_id": "2",
          "time": "20210908 00:33"
        },
        {
          "id": "MDRW2100006447.1.11",
          "form": "그런가?",
          "original_form": "그런가?",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100006447.1.12",
          "form": "그럼 텀블러말고 식당에 용기 가져가서 포장해달라고하는 것도 할인되려나?",
          "original_form": "그럼 텀블러말고 식당에 용기 가져가서 포장해달라고하는 것도 할인되려나?",
          "speaker_id": "1",
          "time": "20210908 01:24"
        },
        {
          "id": "MDRW2100006447.1.13",
          "form": "음? 그건 안해봤네",
          "original_form": "음? 그건 안해봤네",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100006447.1.14",
          "form": "아근데 그거 해봤닼ㅋㅋ 비슷한거",
          "original_form": "아근데 그거 해봤닼ㅋㅋ 비슷한거",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100006447.1.15",
          "form": "나 저번에 팝콘 이벤트 한거 있자낰ㅋㅋㅋㅋ",
          "original_form": "나 저번에 팝콘 이벤트 한거 있자낰ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:25"
        },
        {
          "id": "MDRW2100006447.1.16",
          "form": "아 그러네ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 그러네ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100006447.1.17",
          "form": "그때 팝콘 그렇게 많이 줄줄은 몰랐는데",
          "original_form": "그때 팝콘 그렇게 많이 줄줄은 몰랐는데",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100006447.1.18",
          "form": "그런 이벤트 다른데서도 많이했으면 좋겠다",
          "original_form": "그런 이벤트 다른데서도 많이했으면 좋겠다",
          "speaker_id": "1",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100006447.1.19",
          "form": "ㅋㅋㅋㅋ너 하면 갈거야?",
          "original_form": "ㅋㅋㅋㅋ너 하면 갈거야?",
          "speaker_id": "2",
          "time": "20210908 01:26"
        },
        {
          "id": "MDRW2100006447.1.20",
          "form": "가야지!!",
          "original_form": "가야지!!",
          "speaker_id": "1",
          "time": "20210908 01:28"
        },
        {
          "id": "MDRW2100006447.1.21",
          "form": "가서 맛있는거 많이 얻어와야지ㅋㅋㅋㅋ",
          "original_form": "가서 맛있는거 많이 얻어와야지ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 01:29"
        },
        {
          "id": "MDRW2100006447.1.22",
          "form": "오 좋은생각이야",
          "original_form": "오 좋은생각이야",
          "speaker_id": "2",
          "time": "20210908 01:30"
        },
        {
          "id": "MDRW2100006447.1.23",
          "form": "이벤트 뜨면 알려줄겡",
