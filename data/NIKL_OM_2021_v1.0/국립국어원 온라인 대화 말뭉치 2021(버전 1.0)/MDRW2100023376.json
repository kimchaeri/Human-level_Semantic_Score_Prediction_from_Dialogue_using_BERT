      "utterance": [
        {
          "id": "MDRW2100023376.1.1",
          "form": "내일 출근하기 싫다",
          "original_form": "내일 출근하기 싫다",
          "speaker_id": "2",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023376.1.2",
          "form": "지금나같은사람많겠지..",
          "original_form": "지금나같은사람많겠지..",
          "speaker_id": "2",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023376.1.3",
          "form": "나도 똑같음",
          "original_form": "나도 똑같음",
          "speaker_id": "1",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023376.1.4",
          "form": "낼회의땐또먼소리하지",
          "original_form": "낼회의땐또먼소리하지",
          "speaker_id": "2",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023376.1.5",
          "form": "ㅠㅠ",
          "original_form": "ㅠㅠ",
          "speaker_id": "2",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023376.1.6",
          "form": "토요일 밤에 자는거랑 일요일밤이랑",
          "original_form": "토요일 밤에 자는거랑 일요일밤이랑",
          "speaker_id": "1",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023376.1.7",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "2",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023376.1.8",
          "form": "느낌이 너무 달라",
          "original_form": "느낌이 너무 달라",
          "speaker_id": "1",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023376.1.9",
          "form": "내말이",
          "original_form": "내말이",
          "speaker_id": "2",
          "time": "20210830 01:08"
        },
        {
          "id": "MDRW2100023376.1.10",
          "form": "ㅠ 출근잘햇더?",
          "original_form": "ㅠ 출근잘햇더?",
          "speaker_id": "2",
          "time": "20210830 09:24"
        },
        {
          "id": "MDRW2100023376.1.11",
          "form": "찐빵먹구출근하라구할걸",
          "original_form": "찐빵먹구출근하라구할걸",
          "speaker_id": "2",
          "time": "20210830 09:24"
        },
        {
          "id": "MDRW2100023376.1.12",
          "form": "아침에 뭐먹었어",
          "original_form": "아침에 뭐먹었어",
          "speaker_id": "2",
          "time": "20210830 09:24"
        },
        {
          "id": "MDRW2100023376.1.13",
          "form": "오늘회의에서진짜머라하징",
          "original_form": "오늘회의에서진짜머라하징",
          "speaker_id": "2",
          "time": "20210830 09:25"
        },
        {
          "id": "MDRW2100023376.1.14",
          "form": "졸리다 일하기싫어",
          "original_form": "졸리다 일하기싫어",
          "speaker_id": "1",
          "time": "20210830 10:19"
        },
        {
          "id": "MDRW2100023376.1.15",
          "form": "그래두 잘갔나보구먼",
          "original_form": "그래두 잘갔나보구먼",
          "speaker_id": "1",
          "time": "20210830 10:19"
        },
        {
          "id": "MDRW2100023376.1.16",
          "form": "출근은 항상 힘들어",
          "original_form": "출근은 항상 힘들어",
          "speaker_id": "1",
          "time": "20210830 10:19"
        },
        {
          "id": "MDRW2100023376.1.17",
          "form": "점심시간 1시간반 좋아요",
          "original_form": "점심시간 1시간반 좋아요",
          "speaker_id": "1",
          "time": "20210830 11:57"
        },
        {
          "id": "MDRW2100023376.1.18",
          "form": "벌써 다먹고 회사앞이여",
          "original_form": "벌써 다먹고 회사앞이여",
          "speaker_id": "1",
          "time": "20210830 11:57"
        },
        {
          "id": "MDRW2100023376.1.19",
          "form": "엥벌써? 우린이제청소끝났는데",
          "original_form": "엥벌써? 우린이제청소끝났는데",
          "speaker_id": "2",
          "time": "20210830 12:19"
        },
        {
          "id": "MDRW2100023376.1.20",
          "form": "청소 매주하는거같어 보면",
          "original_form": "청소 매주하는거같어 보면",
          "speaker_id": "1",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100023376.1.21",
          "form": "예전 건물은 편했었는데 아쉽구먼",
          "original_form": "예전 건물은 편했었는데 아쉽구먼",
          "speaker_id": "1",
          "time": "20210830 12:24"
        },
        {
          "id": "MDRW2100023376.1.22",
          "form": "맞엉 ㅜㅜ 오늘 새로 아저씨하나 왓거든",
          "original_form": "맞엉 ㅜㅜ 오늘 새로 아저씨하나 왓거든",
          "speaker_id": "2",
          "time": "20210830 13:42"
        },
        {
          "id": "MDRW2100023376.1.23",
          "form": "노조위원장님이라고",
          "original_form": "노조위원장님이라고",
          "speaker_id": "2",
          "time": "20210830 13:42"
        },
        {
          "id": "MDRW2100023376.1.24",
          "form": "그분자리만든다고 더열심히청소함",
          "original_form": "그분자리만든다고 더열심히청소함",
          "speaker_id": "2",
          "time": "20210830 13:42"
        },
        {
          "id": "MDRW2100023376.1.25",
          "form": "엥 아그 다른사무실분",
          "original_form": "엥 아그 다른사무실분",
          "speaker_id": "1",
          "time": "20210830 13:42"
        },
        {
          "id": "MDRW2100023376.1.26",
          "form": "웅웅 그분..",
          "original_form": "웅웅 그분..",
          "speaker_id": "2",
          "time": "20210830 13:43"
        }
      ]
    }
  ]
