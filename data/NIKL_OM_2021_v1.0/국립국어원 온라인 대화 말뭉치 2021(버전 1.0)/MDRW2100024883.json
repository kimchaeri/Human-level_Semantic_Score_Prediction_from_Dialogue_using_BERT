      "utterance": [
        {
          "id": "MDRW2100024883.1.1",
          "form": "추석 선물 고르는건 어려운거같아요",
          "original_form": "추석 선물 고르는건 어려운거같아요",
          "speaker_id": "1",
          "time": "20210831 20:11"
        },
        {
          "id": "MDRW2100024883.1.2",
          "form": "항상숙제같죠ㅋㅋ",
          "original_form": "항상숙제같죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024883.1.3",
          "form": "매년",
          "original_form": "매년",
          "speaker_id": "2",
          "time": "20210831 20:19"
        },
        {
          "id": "MDRW2100024883.1.4",
          "form": "맞아요 ㅎ 요즘 플스 갖고싶었는데 맘 접었어요",
          "original_form": "맞아요 ㅎ 요즘 플스 갖고싶었는데 맘 접었어요",
          "speaker_id": "1",
          "time": "20210831 20:44"
        },
        {
          "id": "MDRW2100024883.1.5",
          "form": "ㅎㅎ 추석때 뭐선물하시나요?",
          "original_form": "ㅎㅎ 추석때 뭐선물하시나요?",
          "speaker_id": "2",
          "time": "20210831 21:56"
        },
        {
          "id": "MDRW2100024883.1.6",
          "form": "추석때 그게 고민이에요",
          "original_form": "추석때 그게 고민이에요",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024883.1.7",
          "form": "맘같아선 하기싫은데 ㅠ",
          "original_form": "맘같아선 하기싫은데 ㅠ",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024883.1.8",
          "form": "하긴 해야겠고 그래요",
          "original_form": "하긴 해야겠고 그래요",
          "speaker_id": "1",
          "time": "20210831 21:59"
        },
        {
          "id": "MDRW2100024883.1.9",
          "form": "ㅋㅋ제말이요",
          "original_form": "ㅋㅋ제말이요",
          "speaker_id": "2",
          "time": "20210831 22:09"
        },
        {
          "id": "MDRW2100024883.1.10",
          "form": "아니 왜이렇게들 선물 바라는지 모르겠어요 ㅎ",
          "original_form": "아니 왜이렇게들 선물 바라는지 모르겠어요 ㅎ",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100024883.1.11",
          "form": "좋은사람한테 주는건 아무렇지 않은데",
          "original_form": "좋은사람한테 주는건 아무렇지 않은데",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100024883.1.12",
          "form": "싫은사람 주는거라 영 그렇다죠",
          "original_form": "싫은사람 주는거라 영 그렇다죠",
          "speaker_id": "1",
          "time": "20210831 22:11"
        },
        {
          "id": "MDRW2100024883.1.13",
          "form": "ㅎㅎ맞아요 그리고 좋은맘에 주기보단 의무적인거 잇어서",
          "original_form": "ㅎㅎ맞아요 그리고 좋은맘에 주기보단 의무적인거 잇어서",
          "speaker_id": "2",
          "time": "20210901 21:40"
        },
        {
          "id": "MDRW2100024883.1.14",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100024883.1.15",
          "form": "사회생활 하기 어렵네요 ㅎ",
          "original_form": "사회생활 하기 어렵네요 ㅎ",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100024883.1.16",
          "form": "선물 고르는것도 어렵고",
          "original_form": "선물 고르는것도 어렵고",
          "speaker_id": "1",
          "time": "20210901 21:45"
        },
        {
          "id": "MDRW2100024883.1.17",
          "form": "ㅋㅋ그쳐 사회생활ㅠ",
          "original_form": "ㅋㅋ그쳐 사회생활ㅠ",
          "speaker_id": "2",
          "time": "20210901 22:07"
        },
        {
          "id": "MDRW2100024883.1.18",
          "form": "네네ㅋㅋㅋㅋ",
          "original_form": "네네ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100024883.1.19",
          "form": "최근에 갖고싶은게 생겼어요",
          "original_form": "최근에 갖고싶은게 생겼어요",
          "speaker_id": "1",
          "time": "20210901 22:12"
        },
        {
          "id": "MDRW2100024883.1.20",
          "form": "오 먼가요?",
          "original_form": "오 먼가요?",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100024883.1.21",
          "form": "춘익이 얼굴 쿠션이요!",
          "original_form": "춘익이 얼굴 쿠션이요!",
          "speaker_id": "1",
          "time": "20210901 23:01"
        },
        {
          "id": "MDRW2100024883.1.22",
          "form": "그건 뭐죠@",
          "original_form": "그건 뭐죠@",
          "speaker_id": "2",
          "time": "20210901 23:05"
        },
        {
          "id": "MDRW2100024883.1.23",
          "form": "카카오프렌즈 캐릭터인데",
          "original_form": "카카오프렌즈 캐릭터인데",
          "speaker_id": "1",
          "time": "20210901 23:11"
        },
        {
          "id": "MDRW2100024883.1.24",
          "form": "얼굴 쿠션이있어요",
          "original_form": "얼굴 쿠션이있어요",
