      "utterance": [
        {
          "id": "MDRW2100018313.1.1",
          "form": "켈로이드?,",
          "original_form": "켈로이드?,",
          "speaker_id": "2",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018313.1.2",
          "form": "아니요 그건 아닌데",
          "original_form": "아니요 그건 아닌데",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018313.1.3",
          "form": "안 아무네요 ㅠㅠ",
          "original_form": "안 아무네요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018313.1.4",
          "form": "잘못된건 아니에요?",
          "original_form": "잘못된건 아니에요?",
          "speaker_id": "2",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018313.1.5",
          "form": "모르겠어요 ㅠㅠ",
          "original_form": "모르겠어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018313.1.6",
          "form": "피 완전 많이나서",
          "original_form": "피 완전 많이나서",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018313.1.7",
          "form": "휴지가 꽉차요",
          "original_form": "휴지가 꽉차요",
          "speaker_id": "1",
          "time": "20210826 23:46"
        },
        {
          "id": "MDRW2100018313.1.8",
          "form": "병원 가보셔야되는거 아니에요?",
          "original_form": "병원 가보셔야되는거 아니에요?",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018313.1.9",
          "form": "근데 아프긴한데",
          "original_form": "근데 아프긴한데",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018313.1.10",
          "form": "크게 붓진 않아서..",
          "original_form": "크게 붓진 않아서..",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018313.1.11",
          "form": "병원 안가보세요? ㅜ",
          "original_form": "병원 안가보세요? ㅜ",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018313.1.12",
          "form": "아직은 좀 더 보려구요 ㅠㅠ",
          "original_form": "아직은 좀 더 보려구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018313.1.13",
          "form": "제 친구는 완전 곪아서",
          "original_form": "제 친구는 완전 곪아서",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018313.1.14",
          "form": "다 빼더라구요",
          "original_form": "다 빼더라구요",
          "speaker_id": "2",
          "time": "20210826 23:47"
        },
        {
          "id": "MDRW2100018313.1.15",
          "form": "저도 항상 안 아물어서",
          "original_form": "저도 항상 안 아물어서",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018313.1.16",
          "form": "이번이 3차시도에요 ㅠㅠ",
          "original_form": "이번이 3차시도에요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018313.1.17",
          "form": "알레르기일까요?",
          "original_form": "알레르기일까요?",
          "speaker_id": "2",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018313.1.18",
          "form": "그러게요 ㅠㅠ",
          "original_form": "그러게요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018313.1.19",
          "form": "원래 피부 살성이 안 좋긴 해요",
          "original_form": "원래 피부 살성이 안 좋긴 해요",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018313.1.20",
          "form": "에궁 소독하셨어요?",
          "original_form": "에궁 소독하셨어요?",
          "speaker_id": "2",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018313.1.21",
          "form": "아뇨 이제 하려구요.",
          "original_form": "아뇨 이제 하려구요.",
          "speaker_id": "1",
          "time": "20210826 23:48"
        },
        {
          "id": "MDRW2100018313.1.22",
          "form": "저 어제 손목 넘 아팠어요",
          "original_form": "저 어제 손목 넘 아팠어요",
          "speaker_id": "1",
          "time": "20210903 19:13"
        }
      ]
