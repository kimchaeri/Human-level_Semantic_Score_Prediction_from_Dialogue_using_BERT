      "utterance": [
        {
          "id": "MDRW2100037712.1.1",
          "form": "제가 성공한 베이킹은",
          "original_form": "제가 성공한 베이킹은",
          "speaker_id": "2",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100037712.1.2",
          "form": "바스크 치즈케이크인데요",
          "original_form": "바스크 치즈케이크인데요",
          "speaker_id": "2",
          "time": "20210901 16:53"
        },
        {
          "id": "MDRW2100037712.1.3",
          "form": "헐",
          "original_form": "헐",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100037712.1.4",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100037712.1.5",
          "form": "실패한 베이킹도 있나요?",
          "original_form": "실패한 베이킹도 있나요?",
          "speaker_id": "1",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100037712.1.6",
          "form": "아니오 치케가 저의 첫 베이킹이라,,",
          "original_form": "아니오 치케가 저의 첫 베이킹이라,,",
          "speaker_id": "2",
          "time": "20210901 16:59"
        },
        {
          "id": "MDRW2100037712.1.7",
          "form": "생지 해동에 실패해서 곤죽이 된 빵을 먹은 적은 있어요",
          "original_form": "생지 해동에 실패해서 곤죽이 된 빵을 먹은 적은 있어요",
          "speaker_id": "2",
          "time": "20210901 17:00"
        },
        {
          "id": "MDRW2100037712.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100037712.1.9",
          "form": "저는 요새 베이킹...음...",
          "original_form": "저는 요새 베이킹...음...",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100037712.1.10",
          "form": "브라우니 믹스 한 번 해먹은 적 있어요",
          "original_form": "브라우니 믹스 한 번 해먹은 적 있어요",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100037712.1.11",
          "form": "미국의 맛을 느끼고 싶어서..",
          "original_form": "미국의 맛을 느끼고 싶어서..",
          "speaker_id": "1",
          "time": "20210901 17:03"
        },
        {
          "id": "MDRW2100037712.1.12",
          "form": "재료를 사면 초콜렛칩이 1키로,,",
          "original_form": "재료를 사면 초콜렛칩이 1키로,,",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100037712.1.13",
          "form": "브라우니를 몇키로를 해야할지,,",
          "original_form": "브라우니를 몇키로를 해야할지,,",
          "speaker_id": "2",
          "time": "20210901 17:04"
        },
        {
          "id": "MDRW2100037712.1.14",
          "form": "ㅠㅠ조금만 사세요.....",
          "original_form": "ㅠㅠ조금만 사세요.....",
          "speaker_id": "1",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100037712.1.15",
          "form": "빵종류마다 필요한게 너무 달라서 필요재료가 적거나 단일인걸 위주로 깰거에요",
          "original_form": "빵종류마다 필요한게 너무 달라서 필요재료가 적거나 단일인걸 위주로 깰거에요",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100037712.1.16",
          "form": "마트 베이킹 코너 가면",
          "original_form": "마트 베이킹 코너 가면",
          "speaker_id": "1",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100037712.1.17",
          "form": "조금씩 포장된 거 팔아요!",
          "original_form": "조금씩 포장된 거 팔아요!",
          "speaker_id": "1",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100037712.1.18",
          "form": "제가 쿠팡서 주로 사다보니,,",
          "original_form": "제가 쿠팡서 주로 사다보니,,",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037712.1.19",
          "form": "재난지원금이 나오면 방산시장에라도 가려구요",
          "original_form": "재난지원금이 나오면 방산시장에라도 가려구요",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037712.1.20",
          "form": "암튼 재료문제때무넹 치아바타를 하려는데,,",
          "original_form": "암튼 재료문제때무넹 치아바타를 하려는데,,",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037712.1.21",
          "form": "치아바타 해보셨나요?",
          "original_form": "치아바타 해보셨나요?",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037712.1.22",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037712.1.23",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037712.1.24",
          "form": "아뇨 저는 사먹어만 봤어요 호호",
          "original_form": "아뇨 저는 사먹어만 봤어요 호호",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037712.1.25",
          "form": "이게 재료가 진자 간단해요 밀가루 올리브오일 소금 이스트??들어가던가?",
          "original_form": "이게 재료가 진자 간단해요 밀가루 올리브오일 소금 이스트??들어가던가?",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037712.1.26",
          "form": "문제는,, 반죽이,,",
          "original_form": "문제는,, 반죽이,,",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100037712.1.27",
          "form": "1차반죽하고 풀딩 4번",
          "original_form": "1차반죽하고 풀딩 4번",
          "speaker_id": "2",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037712.1.28",
          "form": "그 뒤에 24시간 반죽 재우고 다시 폴딩 -_-",
          "original_form": "그 뒤에 24시간 반죽 재우고 다시 폴딩 -_-",
          "speaker_id": "2",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037712.1.29",
          "form": "올리브오일이 왕창 들어가는데 그럼 그때마다 손씻으며 반죽하란건가!",
          "original_form": "올리브오일이 왕창 들어가는데 그럼 그때마다 손씻으며 반죽하란건가!",
          "speaker_id": "2",
          "time": "20210901 17:08"
        },
        {
          "id": "MDRW2100037712.1.30",
          "form": "그쳐.....",
          "original_form": "그쳐.....",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037712.1.31",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037712.1.32",
          "form": "그게 너무 귀찮아서 때려쳤어요..",
          "original_form": "그게 너무 귀찮아서 때려쳤어요..",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037712.1.33",
          "form": "휴지 시간 기다리는 것도 넘 귀찮...지루 .",
          "original_form": "휴지 시간 기다리는 것도 넘 귀찮...지루 .",
          "speaker_id": "1",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100037712.1.34",
          "form": "그리고 냉장고같은데 24시간 둘라면 그것도 공간이고",
          "original_form": "그리고 냉장고같은데 24시간 둘라면 그것도 공간이고",
          "speaker_id": "2",
