      "utterance": [
        {
          "id": "MDRW2100032668.1.1",
          "form": "웅 아침마다 하려고",
          "original_form": "웅 아침마다 하려고",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100032668.1.2",
          "form": "운동",
          "original_form": "운동",
          "speaker_id": "2",
          "time": "20210905 22:05"
        },
        {
          "id": "MDRW2100032668.1.3",
          "form": "다이어트 시작이니",
          "original_form": "다이어트 시작이니",
          "speaker_id": "1",
          "time": "20210905 22:06"
        },
        {
          "id": "MDRW2100032668.1.4",
          "form": "건강해지고",
          "original_form": "건강해지고",
          "speaker_id": "2",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100032668.1.5",
          "form": "빠지면 좋고",
          "original_form": "빠지면 좋고",
          "speaker_id": "2",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100032668.1.6",
          "form": "아님말고ㅋㅋㅋ",
          "original_form": "아님말고ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210905 22:07"
        },
        {
          "id": "MDRW2100032668.1.7",
          "form": "저녁운동은 아니야",
          "original_form": "저녁운동은 아니야",
          "speaker_id": "1",
          "time": "20210905 22:08"
        },
        {
          "id": "MDRW2100032668.1.8",
          "form": "도는거 지루해서ㅠ",
          "original_form": "도는거 지루해서ㅠ",
          "speaker_id": "2",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032668.1.9",
          "form": "왜 음악 들으면서 운동 하는것도 좋아",
          "original_form": "왜 음악 들으면서 운동 하는것도 좋아",
          "speaker_id": "1",
          "time": "20210905 22:11"
        },
        {
          "id": "MDRW2100032668.1.10",
          "form": "같은 것만보니까 지루행",
          "original_form": "같은 것만보니까 지루행",
          "speaker_id": "2",
          "time": "20210905 22:17"
        },
        {
          "id": "MDRW2100032668.1.11",
          "form": "암 생각도안하고 좋지 않아",
          "original_form": "암 생각도안하고 좋지 않아",
          "speaker_id": "1",
          "time": "20210905 22:27"
        },
        {
          "id": "MDRW2100032668.1.12",
          "form": "ㅋㅋㅋ내취향은 길거리 걷는거ㅠ",
          "original_form": "ㅋㅋㅋ내취향은 길거리 걷는거ㅠ",
          "speaker_id": "2",
          "time": "20210905 22:29"
        },
        {
          "id": "MDRW2100032668.1.13",
          "form": "그런 운동이 젤좋앙",
          "original_form": "그런 운동이 젤좋앙",
          "speaker_id": "2",
          "time": "20210905 22:30"
        },
        {
          "id": "MDRW2100032668.1.14",
          "form": "그래 너가 하고싶은 운동해",
          "original_form": "그래 너가 하고싶은 운동해",
          "speaker_id": "1",
          "time": "20210905 22:59"
        },
        {
          "id": "MDRW2100032668.1.15",
          "form": "고마웡",
          "original_form": "고마웡{emoji:❤}",
          "speaker_id": "2",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100032668.1.16",
          "form": "나중에 안할스돜ㅋㅋㅋㅠ",
          "original_form": "나중에 안할스돜ㅋㅋㅋㅠ",
          "speaker_id": "2",
          "time": "20210905 23:00"
        },
        {
          "id": "MDRW2100032668.1.17",
          "form": "왜 꾸준히해 몸 튼튼해 지게",
          "original_form": "왜 꾸준히해 몸 튼튼해 지게",
          "speaker_id": "1",
          "time": "20210905 23:02"
        },
        {
          "id": "MDRW2100032668.1.18",
          "form": "의지가 되려나ㅠㅋㅋㅋ",
          "original_form": "의지가 되려나ㅠㅋㅋㅋ",
          "speaker_id": "2",
