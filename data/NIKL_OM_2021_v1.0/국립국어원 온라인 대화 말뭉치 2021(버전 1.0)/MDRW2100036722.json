      "utterance": [
        {
          "id": "MDRW2100036722.1.1",
          "form": "다이어트면 식이조절하시나요?",
          "original_form": "다이어트면 식이조절하시나요?",
          "speaker_id": "1",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100036722.1.2",
          "form": "음 네네",
          "original_form": "음 네네",
          "speaker_id": "2",
          "time": "20210831 10:24"
        },
        {
          "id": "MDRW2100036722.1.3",
          "form": "지금 레디톡 48시간인가 이거 먹어요 ㅋㅋ",
          "original_form": "지금 레디톡 48시간인가 이거 먹어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 10:25"
        },
        {
          "id": "MDRW2100036722.1.4",
          "form": "그거 먹으면 식욕이 감소하나요?",
          "original_form": "그거 먹으면 식욕이 감소하나요?",
          "speaker_id": "1",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100036722.1.5",
          "form": "음 아뇨",
          "original_form": "음 아뇨",
          "speaker_id": "2",
          "time": "20210831 10:27"
        },
        {
          "id": "MDRW2100036722.1.6",
          "form": "네이버에 레디톡치면 나오는데",
          "original_form": "네이버에 레디톡치면 나오는데",
          "speaker_id": "2",
          "time": "20210831 10:28"
        },
        {
          "id": "MDRW2100036722.1.7",
          "form": "배는 고프군요",
          "original_form": "배는 고프군요",
          "speaker_id": "2",
          "time": "20210831 10:28"
        },
        {
          "id": "MDRW2100036722.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ 아 그러면 뭐 지방분해효소 이런거 들어잇나요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ 아 그러면 뭐 지방분해효소 이런거 들어잇나요",
          "speaker_id": "1",
          "time": "20210831 10:32"
        },
        {
          "id": "MDRW2100036722.1.9",
          "form": "아뇨 음 건강기능식품도 아니네요",
          "original_form": "아뇨 음 건강기능식품도 아니네요",
          "speaker_id": "2",
          "time": "20210831 10:36"
        },
        {
          "id": "MDRW2100036722.1.10",
          "form": "식품유형 과채음료ㄷㄷ",
          "original_form": "식품유형 과채음료ㄷㄷ",
          "speaker_id": "2",
          "time": "20210831 10:36"
        },
        {
          "id": "MDRW2100036722.1.11",
          "form": "헉ㅋㅋㅋㅋ 그냥 맛잇는 주스인가요",
          "original_form": "헉ㅋㅋㅋㅋ 그냥 맛잇는 주스인가요",
          "speaker_id": "1",
          "time": "20210831 10:50"
        },
        {
          "id": "MDRW2100036722.1.12",
          "form": "와 이거 보면볼수록 좀 사기네요",
          "original_form": "와 이거 보면볼수록 좀 사기네요",
          "speaker_id": "2",
          "time": "20210831 11:07"
        },
        {
          "id": "MDRW2100036722.1.13",
          "form": "이거 10개에 34200이라니 ㅋㅋ",
          "original_form": "이거 10개에 34200이라니 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:09"
        },
        {
          "id": "MDRW2100036722.1.14",
          "form": "헉 너무비싼대요",
          "original_form": "헉 너무비싼대요",
          "speaker_id": "1",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100036722.1.15",
          "form": "효과는 좀 보셨어요?",
          "original_form": "효과는 좀 보셨어요?",
          "speaker_id": "1",
          "time": "20210831 11:10"
        },
        {
          "id": "MDRW2100036722.1.16",
          "form": "10개세트중 아침꺼 한개 먹었어요 ㅋㅋ",
          "original_form": "10개세트중 아침꺼 한개 먹었어요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210831 11:17"
        },
        {
          "id": "MDRW2100036722.1.17",
          "form": "근데 배고프다는ㅜ3ㅜ",
          "original_form": "근데 배고프다는ㅜ3ㅜ",
          "speaker_id": "2",
          "time": "20210831 11:17"
        },
        {
          "id": "MDRW2100036722.1.18",
          "form": "헐 그거 끼니 대용이에요?",
          "original_form": "헐 그거 끼니 대용이에요?",
          "speaker_id": "1",
          "time": "20210831 12:26"
        },
        {
          "id": "MDRW2100036722.1.19",
          "form": "네네 이걸 하루 5개먹어요 세시간씩",
          "original_form": "네네 이걸 하루 5개먹어요 세시간씩",
          "speaker_id": "2",
          "time": "20210831 13:00"
        },
        {
          "id": "MDRW2100036722.1.20",
          "form": "이거 배송료도 있네요 ㄷㄷ",
          "original_form": "이거 배송료도 있네요 ㄷㄷ",
          "speaker_id": "2",
          "time": "20210831 13:00"
        },
        {
          "id": "MDRW2100036722.1.21",
          "form": "헐 다섯개씩 ㄷㄷ",
          "original_form": "헐 다섯개씩 ㄷㄷ",
          "speaker_id": "1",
          "time": "20210831 13:31"
        },
        {
          "id": "MDRW2100036722.1.22",
