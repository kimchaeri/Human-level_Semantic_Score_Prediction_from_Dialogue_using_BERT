      "utterance": [
        {
          "id": "MDRW2100015940.1.1",
          "form": "너는 남자친구 안사귀니?",
          "original_form": "너는 남자친구 안사귀니?",
          "speaker_id": "2",
          "time": "20210826 03:20"
        },
        {
          "id": "MDRW2100015940.1.2",
          "form": "못사귀는거야",
          "original_form": "못사귀는거야",
          "speaker_id": "1",
          "time": "20210826 03:20"
        },
        {
          "id": "MDRW2100015940.1.3",
          "form": "노력을 해봐",
          "original_form": "노력을 해봐",
          "speaker_id": "2",
          "time": "20210826 03:20"
        },
        {
          "id": "MDRW2100015940.1.4",
          "form": "노력한거 있어",
          "original_form": "노력한거 있어",
          "speaker_id": "1",
          "time": "20210826 03:20"
        },
        {
          "id": "MDRW2100015940.1.5",
          "form": "뭐했는데?",
          "original_form": "뭐했는데?",
          "speaker_id": "2",
          "time": "20210826 03:20"
        },
        {
          "id": "MDRW2100015940.1.6",
          "form": "우리학교랑 근처 학교 연합해서 하는 소개팅 신청했어",
          "original_form": "우리학교랑 근처 학교 연합해서 하는 소개팅 신청했어",
          "speaker_id": "1",
          "time": "20210826 03:21"
        },
        {
          "id": "MDRW2100015940.1.7",
          "form": "어떻게 하는건데?",
          "original_form": "어떻게 하는건데?",
          "speaker_id": "2",
          "time": "20210826 03:21"
        },
        {
          "id": "MDRW2100015940.1.8",
          "form": "말그대로 어플로 신청하면 조건에 맞는사람 매칭시켜줘",
          "original_form": "말그대로 어플로 신청하면 조건에 맞는사람 매칭시켜줘",
          "speaker_id": "1",
          "time": "20210826 03:21"
        },
        {
          "id": "MDRW2100015940.1.9",
          "form": "매칭 됐어?",
          "original_form": "매칭 됐어?",
          "speaker_id": "2",
          "time": "20210826 03:21"
        },
        {
          "id": "MDRW2100015940.1.10",
          "form": "아니 어제 신청만 했고, 이틀뒤에 결과 나와",
          "original_form": "아니 어제 신청만 했고, 이틀뒤에 결과 나와",
          "speaker_id": "1",
          "time": "20210826 03:22"
        },
        {
          "id": "MDRW2100015940.1.11",
          "form": "그거 돈 드는건 아니지?",
          "original_form": "그거 돈 드는건 아니지?",
          "speaker_id": "2",
          "time": "20210826 03:22"
        },
        {
          "id": "MDRW2100015940.1.12",
          "form": "최대 3명까지 매칭해주는데 1명은 무료야",
          "original_form": "최대 3명까지 매칭해주는데 1명은 무료야",
          "speaker_id": "1",
          "time": "20210826 03:22"
        },
        {
          "id": "MDRW2100015940.1.13",
          "form": "그래서 한명만 했어?",
          "original_form": "그래서 한명만 했어?",
          "speaker_id": "2",
          "time": "20210826 03:22"
        },
        {
          "id": "MDRW2100015940.1.14",
          "form": "아니 세명 했어",
          "original_form": "아니 세명 했어",
          "speaker_id": "1",
          "time": "20210826 03:22"
        },
        {
          "id": "MDRW2100015940.1.15",
          "form": "얼마 낸거야 그럼",
          "original_form": "얼마 낸거야 그럼",
          "speaker_id": "2",
          "time": "20210826 03:22"
        },
        {
          "id": "MDRW2100015940.1.16",
          "form": "2000원밖에 안냈어",
          "original_form": "2000원밖에 안냈어",
          "speaker_id": "1",
          "time": "20210826 03:22"
        },
        {
          "id": "MDRW2100015940.1.17",
          "form": "매칭되면 바로 만나는거야?",
          "original_form": "매칭되면 바로 만나는거야?",
          "speaker_id": "2",
          "time": "20210826 03:23"
        },
        {
          "id": "MDRW2100015940.1.18",
          "form": "아니지",
          "original_form": "아니지",
          "speaker_id": "1",
          "time": "20210826 03:23"
        },
        {
          "id": "MDRW2100015940.1.19",
          "form": "얘기 먼저 나눠보고 만나야지",
          "original_form": "얘기 먼저 나눠보고 만나야지",
          "speaker_id": "1",
          "time": "20210826 03:23"
        },
        {
          "id": "MDRW2100015940.1.20",
          "form": "이상한 애 걸리면 어떡해",
          "original_form": "이상한 애 걸리면 어떡해",
          "speaker_id": "2",
          "time": "20210826 03:23"
        },
        {
          "id": "MDRW2100015940.1.21",
          "form": "나는 우리학교 사람들만 받는다고 했어",
          "original_form": "나는 우리학교 사람들만 받는다고 했어",
          "speaker_id": "1",
          "time": "20210826 03:23"
        },
        {
          "id": "MDRW2100015940.1.22",
          "form": "그럼 그나마 다행이네",
          "original_form": "그럼 그나마 다행이네",
          "speaker_id": "2",
          "time": "20210826 03:23"
        },
        {
          "id": "MDRW2100015940.1.23",
          "form": "그렇지",
          "original_form": "그렇지",
          "speaker_id": "1",
          "time": "20210826 03:23"
        },
        {
          "id": "MDRW2100015940.1.24",
          "form": "나이 차이 많이 나는거 아니야?",
          "original_form": "나이 차이 많이 나는거 아니야?",
          "speaker_id": "2",
          "time": "20210826 03:24"
        },
        {
          "id": "MDRW2100015940.1.25",
          "form": "나는 21~23세 선택했어",
          "original_form": "나는 21~23세 선택했어",
          "speaker_id": "1",
          "time": "20210826 03:24"
        },
        {
          "id": "MDRW2100015940.1.26",
          "form": "신기하네 하는 사람 많은가봐?",
          "original_form": "신기하네 하는 사람 많은가봐?",
          "speaker_id": "2",
          "time": "20210826 03:24"
        },
        {
          "id": "MDRW2100015940.1.27",
          "form": "인기 엄청 많지",
          "original_form": "인기 엄청 많지",
          "speaker_id": "1",
          "time": "20210826 03:24"
        }
      ]
    }
  ]
