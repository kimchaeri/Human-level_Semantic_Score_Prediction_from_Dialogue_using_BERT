      "utterance": [
        {
          "id": "MDRW2100010736.1.1",
          "form": "ㅎㅎ 오늘은 야근후 닭모래집 튀김을 먹었어요",
          "original_form": "ㅎㅎ 오늘은 야근후 닭모래집 튀김을 먹었어요",
          "speaker_id": "2",
          "time": "20210823 21:56"
        },
        {
          "id": "MDRW2100010736.1.2",
          "form": "삼계탕 먹고 팠는데ㅜ",
          "original_form": "삼계탕 먹고 팠는데ㅜ",
          "speaker_id": "2",
          "time": "20210823 21:56"
        },
        {
          "id": "MDRW2100010736.1.3",
          "form": "가게서 드셨나요 아님 레토르트?",
          "original_form": "가게서 드셨나요 아님 레토르트?",
          "speaker_id": "2",
          "time": "20210823 21:57"
        },
        {
          "id": "MDRW2100010736.1.4",
          "form": "포장해와서 먹었어요 ㅎㅎㅎ",
          "original_form": "포장해와서 먹었어요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:06"
        },
        {
          "id": "MDRW2100010736.1.5",
          "form": "날씨가 쌀쌀해서 뜨끈한 국물이 계속 땡기는거 같아용",
          "original_form": "날씨가 쌀쌀해서 뜨끈한 국물이 계속 땡기는거 같아용",
          "speaker_id": "1",
          "time": "20210823 22:07"
        },
        {
          "id": "MDRW2100010736.1.6",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210823 22:07"
        },
        {
          "id": "MDRW2100010736.1.7",
          "form": "전 오늘 알탕이 당겼는데ㅜ",
          "original_form": "전 오늘 알탕이 당겼는데ㅜ",
          "speaker_id": "2",
          "time": "20210823 22:07"
        },
        {
          "id": "MDRW2100010736.1.8",
          "form": "대세를 따랐죠",
          "original_form": "대세를 따랐죠",
          "speaker_id": "2",
          "time": "20210823 22:07"
        },
        {
          "id": "MDRW2100010736.1.9",
          "form": "아무도 국물요리를 안 찾길래ㅜ",
          "original_form": "아무도 국물요리를 안 찾길래ㅜ",
          "speaker_id": "2",
          "time": "20210823 22:08"
        },
        {
          "id": "MDRW2100010736.1.10",
          "form": "워우 다들 건강하고 속이 든든하신가...",
          "original_form": "워우 다들 건강하고 속이 든든하신가...",
          "speaker_id": "1",
          "time": "20210823 22:09"
        },
        {
          "id": "MDRW2100010736.1.11",
          "form": "요즘같은날엔 삼계탕이쥬~",
          "original_form": "요즘같은날엔 삼계탕이쥬~",
          "speaker_id": "1",
          "time": "20210823 22:09"
        },
        {
          "id": "MDRW2100010736.1.12",
          "form": "ㅎㅎ 다들 지방간이 비대하죠",
          "original_form": "ㅎㅎ 다들 지방간이 비대하죠",
          "speaker_id": "2",
          "time": "20210823 22:09"
        },
        {
          "id": "MDRW2100010736.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:10"
        },
        {
          "id": "MDRW2100010736.1.14",
          "form": "제가 그나마 건강한 편ㅎ",
          "original_form": "제가 그나마 건강한 편ㅎ",
          "speaker_id": "2",
          "time": "20210823 22:10"
        },
        {
          "id": "MDRW2100010736.1.15",
          "form": "통새우버거는 아침에 드셨어요?",
          "original_form": "통새우버거는 아침에 드셨어요?",
          "speaker_id": "1",
          "time": "20210823 22:10"
        },
        {
          "id": "MDRW2100010736.1.16",
          "form": "안주발을 당기니까요",
          "original_form": "안주발을 당기니까요",
          "speaker_id": "2",
          "time": "20210823 22:10"
        },
        {
          "id": "MDRW2100010736.1.17",
          "form": "통새우 하루지나도 튼실하더이다",
          "original_form": "통새우 하루지나도 튼실하더이다",
          "speaker_id": "2",
          "time": "20210823 22:10"
        },
        {
          "id": "MDRW2100010736.1.18",
          "form": "생각보다 씹는맛이 있어 놀랐어요",
          "original_form": "생각보다 씹는맛이 있어 놀랐어요",
          "speaker_id": "2",
          "time": "20210823 22:10"
        },
        {
          "id": "MDRW2100010736.1.19",
          "form": "오늘 갈릭도 먹었어야 하는데 술배가 차서ㅜ",
          "original_form": "오늘 갈릭도 먹었어야 하는데 술배가 차서ㅜ",
          "speaker_id": "2",
          "time": "20210823 22:11"
        },
        {
          "id": "MDRW2100010736.1.20",
          "form": "그쳐그쳐 씹는맛이 있어요 ㅋㅋㅋ 이거 생각보다 되게 든든해서",
          "original_form": "그쳐그쳐 씹는맛이 있어요 ㅋㅋㅋ 이거 생각보다 되게 든든해서",
          "speaker_id": "1",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010736.1.21",
          "form": "저 한번은 급하게 먹고 움직여야하는 때가 있었는데",
          "original_form": "저 한번은 급하게 먹고 움직여야하는 때가 있었는데",
          "speaker_id": "1",
          "time": "20210823 22:17"
        },
        {
          "id": "MDRW2100010736.1.22",
          "form": "3분의1밖에 못먹고 포장해서 일보러 다녔거든요. 근데도 배 안고프고 저녁도 안먹었자나요 ㅋㅋㅋ",
          "original_form": "3분의1밖에 못먹고 포장해서 일보러 다녔거든요. 근데도 배 안고프고 저녁도 안먹었자나요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 22:18"
        },
        {
          "id": "MDRW2100010736.1.23",
          "form": "그건;; name1 위 크기가 문제 아닐까요",
          "original_form": "그건;; &name1& 위 크기가 문제 아닐까요",
          "speaker_id": "2",
          "time": "20210823 22:18"
        },
        {
          "id": "MDRW2100010736.1.24",
          "form": "뭐죠???",
          "original_form": "뭐죠???",
          "speaker_id": "2",
          "time": "20210823 22:18"
        },
        {
          "id": "MDRW2100010736.1.25",
          "form": "그건 듣고도 납득할 수 없어욧!",
          "original_form": "그건 듣고도 납득할 수 없어욧!",
          "speaker_id": "2",
          "time": "20210823 22:19"
        },
        {
          "id": "MDRW2100010736.1.26",
          "form": "원래 소식하시나요",
          "original_form": "원래 소식하시나요",
          "speaker_id": "2",
          "time": "20210823 22:19"
        },
        {
          "id": "MDRW2100010736.1.27",
          "form": "딱히 소식하지는 않는데~ 요즘 유튜브 보고 하면 많이 먹잖아요. 그정도는 아닌거 같아요 ㅎㅎㅎ",
          "original_form": "딱히 소식하지는 않는데~ 요즘 유튜브 보고 하면 많이 먹잖아요. 그정도는 아닌거 같아요 ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010736.1.28",
          "form": "먹고싶은 만큼 먹는데 예전보다 양이 줄긴했어요.",
          "original_form": "먹고싶은 만큼 먹는데 예전보다 양이 줄긴했어요.",
          "speaker_id": "1",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010736.1.29",
          "form": "그래도 피지스쿨 고구마피자는 4조각 먹어요 ㅋㅋㅋ 5조각까지도 가끔은 가넝한~#",
          "original_form": "그래도 피지스쿨 고구마피자는 4조각 먹어요 ㅋㅋㅋ 5조각까지도 가끔은 가넝한~#",
          "speaker_id": "1",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010736.1.30",
          "form": "아니 유튜브를 떠나서 너무 소식인데ㅜ",
          "original_form": "아니 유튜브를 떠나서 너무 소식인데ㅜ",
          "speaker_id": "2",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010736.1.31",
          "form": "피자스쿨 피자는 작잖아요!!",
          "original_form": "피자스쿨 피자는 작잖아요!!",
          "speaker_id": "2",
          "time": "20210823 22:22"
        },
        {
          "id": "MDRW2100010736.1.32",
          "form": "한창때는 그래도 한판 드셨겠지요ㅜ",
          "original_form": "한창때는 그래도 한판 드셨겠지요ㅜ",
          "speaker_id": "2",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010736.1.33",
          "form": "그렇다고 해주세요",
          "original_form": "그렇다고 해주세요",
          "speaker_id": "2",
          "time": "20210823 22:23"
        },
        {
          "id": "MDRW2100010736.1.34",
          "form": "ㅋㅋㅋㅋㅋ아니...",
          "original_form": "ㅋㅋㅋㅋㅋ아니...",
          "speaker_id": "1",
          "time": "20210823 22:25"
        },
        {
          "id": "MDRW2100010736.1.35",
          "form": "저 최고기록이 5조각이에요....",
          "original_form": "저 최고기록이 5조각이에요....",
          "speaker_id": "1",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010736.1.36",
          "form": "어릴때는 배터지듯먹고 토하구... 그랬어요.. 이젠 모대요..",
          "original_form": "어릴때는 배터지듯먹고 토하구... 그랬어요.. 이젠 모대요..",
          "speaker_id": "1",
          "time": "20210823 22:26"
        },
        {
          "id": "MDRW2100010736.1.37",
          "form": "라지 아니고 레귤러요ㅜ",
          "original_form": "라지 아니고 레귤러요ㅜ",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010736.1.38",
          "form": "name1 실망이에욧!",
          "original_form": "&name1& 실망이에욧!",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010736.1.39",
          "form": "나만 돼지야ㅜㅜㅜ",
          "original_form": "나만 돼지야ㅜㅜㅜ",
          "speaker_id": "2",
          "time": "20210823 22:27"
        },
        {
          "id": "MDRW2100010736.1.40",
          "form": "맛있게 먹는게 중요하지 얼만큼 먹느냐가 중요한것이 아닙니다아~~~",
          "original_form": "맛있게 먹는게 중요하지 얼만큼 먹느냐가 중요한것이 아닙니다아~~~",
          "speaker_id": "1",
          "time": "20210823 22:28"
        },
        {
          "id": "MDRW2100010736.1.41",
          "form": "근데 name2 ㅋㅋㅋㅋ 술배 찼다더니 취하셨나여 ㅋㅋㅋ",
          "original_form": "근데 &name2& ㅋㅋㅋㅋ 술배 찼다더니 취하셨나여 ㅋㅋㅋ",
          "speaker_id": "1",
