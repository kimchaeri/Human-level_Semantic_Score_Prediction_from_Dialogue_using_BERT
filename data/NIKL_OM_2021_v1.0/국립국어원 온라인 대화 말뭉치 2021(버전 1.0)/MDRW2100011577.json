      "utterance": [
        {
          "id": "MDRW2100011577.1.1",
          "form": "게임 하는거 있으세요?",
          "original_form": "게임 하는거 있으세요?",
          "speaker_id": "2",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100011577.1.2",
          "form": "저 피파온라인이요!",
          "original_form": "저 피파온라인이요!",
          "speaker_id": "1",
          "time": "20210824 14:09"
        },
        {
          "id": "MDRW2100011577.1.3",
          "form": "피파면 그 축구게임 맞나요?",
          "original_form": "피파면 그 축구게임 맞나요?",
          "speaker_id": "2",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100011577.1.4",
          "form": "네 맞아요! 배그는 예전에 했었어요",
          "original_form": "네 맞아요! 배그는 예전에 했었어요",
          "speaker_id": "1",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011577.1.5",
          "form": "오 저는 폰겜만 깔짝거리는 정도인데 ㅎㅎ",
          "original_form": "오 저는 폰겜만 깔짝거리는 정도인데 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011577.1.6",
          "form": "배그 중독성이 어마어마 하다고 들었어요",
          "original_form": "배그 중독성이 어마어마 하다고 들었어요",
          "speaker_id": "2",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011577.1.7",
          "form": "네 예전에 pc방 거의 살았던기억이ㅎㅎ",
          "original_form": "네 예전에 pc방 거의 살았던기억이ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011577.1.8",
          "form": "저는 넘 어려운건 못하겠더라고요",
          "original_form": "저는 넘 어려운건 못하겠더라고요",
          "speaker_id": "2",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011577.1.9",
          "form": "게임도 적성이 있어야 ㅋㅋ",
          "original_form": "게임도 적성이 있어야 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011577.1.10",
          "form": "저는 스타 이런게임 싫어해요",
          "original_form": "저는 스타 이런게임 싫어해요",
          "speaker_id": "1",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100011577.1.11",
          "form": "저는 딱 그 애니팡 정도 수준이에요 ㅋㅋ",
          "original_form": "저는 딱 그 애니팡 정도 수준이에요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100011577.1.12",
          "form": "애니팡ㅎㅎ 저도 예전에 했었네요",
          "original_form": "애니팡ㅎㅎ 저도 예전에 했었네요",
          "speaker_id": "1",
          "time": "20210824 14:30"
        },
        {
          "id": "MDRW2100011577.1.13",
          "form": "캐시퍼 라는 폰겜은 하고있어요",
          "original_form": "캐시퍼 라는 폰겜은 하고있어요",
          "speaker_id": "2",
          "time": "20210824 14:33"
        },
        {
          "id": "MDRW2100011577.1.14",
          "form": "게임이라 하기도 민망하다만",
          "original_form": "게임이라 하기도 민망하다만",
          "speaker_id": "2",
          "time": "20210824 14:33"
        },
        {
          "id": "MDRW2100011577.1.15",
          "form": "오 캐시퍼 처음 들어봤어요",
          "original_form": "오 캐시퍼 처음 들어봤어요",
