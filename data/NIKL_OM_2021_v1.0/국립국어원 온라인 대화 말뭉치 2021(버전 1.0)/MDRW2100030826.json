      },
      "utterance": [
        {
          "id": "MDRW2100030826.1.1",
          "form": "한지박물관 말고 종이박물관이었나",
          "original_form": "한지박물관 말고 종이박물관이었나",
          "speaker_id": "2",
          "time": "20210904 13:04"
        },
        {
          "id": "MDRW2100030826.1.2",
          "form": "그랬던거같기도? 하여튼",
          "original_form": "그랬던거같기도? 하여튼",
          "speaker_id": "1",
          "time": "20210904 13:05"
        },
        {
          "id": "MDRW2100030826.1.3",
          "form": "한지만든게 중요한거죠",
          "original_form": "한지만든게 중요한거죠",
          "speaker_id": "1",
          "time": "20210904 13:05"
        },
        {
          "id": "MDRW2100030826.1.4",
          "form": "부채도 만들었지 그걸로",
          "original_form": "부채도 만들었지 그걸로",
          "speaker_id": "2",
          "time": "20210904 13:05"
        },
        {
          "id": "MDRW2100030826.1.5",
          "form": "그랬는데 지금은",
          "original_form": "그랬는데 지금은",
          "speaker_id": "1",
          "time": "20210904 13:05"
        },
        {
          "id": "MDRW2100030826.1.6",
          "form": "어디갔는지 모르겠어요 ㅋㅋㅋ",
          "original_form": "어디갔는지 모르겠어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 13:05"
        },
        {
          "id": "MDRW2100030826.1.7",
          "form": "이사오면서 버렸나",
          "original_form": "이사오면서 버렸나",
          "speaker_id": "2",
          "time": "20210904 13:05"
        },
        {
          "id": "MDRW2100030826.1.8",
          "form": "ㅐ중간에 망가졌을지두...",
          "original_form": "ㅐ중간에 망가졌을지두...",
          "speaker_id": "1",
          "time": "20210904 13:05"
        },
        {
          "id": "MDRW2100030826.1.9",
          "form": "아깝긴 하네요",
          "original_form": "아깝긴 하네요",
          "speaker_id": "1",
          "time": "20210904 13:06"
        },
        {
          "id": "MDRW2100030826.1.10",
          "form": "그림도 그려서 보기 좋았는데",
          "original_form": "그림도 그려서 보기 좋았는데",
          "speaker_id": "2",
          "time": "20210904 13:06"
        },
        {
          "id": "MDRW2100030826.1.11",
          "form": "근데 엄청 잘 기억하구 계시네요",
          "original_form": "근데 엄청 잘 기억하구 계시네요",
          "speaker_id": "1",
          "time": "20210904 13:06"
        },
        {
          "id": "MDRW2100030826.1.12",
          "form": "저 어렸을때면 예전인데",
          "original_form": "저 어렸을때면 예전인데",
          "speaker_id": "1",
          "time": "20210904 13:06"
        },
        {
          "id": "MDRW2100030826.1.13",
          "form": "엄마 다 기억하지",
          "original_form": "엄마 다 기억하지",
          "speaker_id": "2",
          "time": "20210904 13:06"
        },
        {
          "id": "MDRW2100030826.1.14",
          "form": "중요한건 다 까먹으시면서...",
          "original_form": "중요한건 다 까먹으시면서...",
          "speaker_id": "1",
          "time": "20210904 13:06"
        },
        {
          "id": "MDRW2100030826.1.15",
          "form": "엄마가 언제 그래",
          "original_form": "엄마가 언제 그래",
          "speaker_id": "2",
          "time": "20210904 13:07"
        },
        {
          "id": "MDRW2100030826.1.16",
          "form": "ㅎㅎ 농담이에요",
          "original_form": "ㅎㅎ 농담이에요",
          "speaker_id": "1",
          "time": "20210904 13:07"
        },
        {
          "id": "MDRW2100030826.1.17",
          "form": "하여튼 체험해야 기억에 더 잘남는거같아요",
          "original_form": "하여튼 체험해야 기억에 더 잘남는거같아요",
          "speaker_id": "1",
          "time": "20210904 13:07"
        },
        {
          "id": "MDRW2100030826.1.18",
          "form": "다른건 다 까먹음",
          "original_form": "다른건 다 까먹음",
          "speaker_id": "1",
          "time": "20210904 13:07"
        },
        {
          "id": "MDRW2100030826.1.19",
          "form": "전쟁기념관 간것도 기억안나?",
          "original_form": "전쟁기념관 간것도 기억안나?",
          "speaker_id": "2",
          "time": "20210904 13:08"
        },
        {
          "id": "MDRW2100030826.1.20",
          "form": "간거만 기억나고",
          "original_form": "간거만 기억나고",
          "speaker_id": "1",
          "time": "20210904 13:08"
        },
        {
          "id": "MDRW2100030826.1.21",
          "form": "내용은 기억안나는 그런...",
          "original_form": "내용은 기억안나는 그런...",
          "speaker_id": "1",
          "time": "20210904 13:08"
        },
        {
          "id": "MDRW2100030826.1.22",
          "form": "체험 안해서 그런가보다",
          "original_form": "체험 안해서 그런가보다",
          "speaker_id": "2",
          "time": "20210904 13:08"
        },
        {
          "id": "MDRW2100030826.1.23",
          "form": "뭔가 물건으로 남으면 더 기억하게 되네요",
          "original_form": "뭔가 물건으로 남으면 더 기억하게 되네요",
          "speaker_id": "1",
          "time": "20210904 13:09"
        }
      ]
