      "utterance": [
        {
          "id": "MDRW2100023891.1.1",
          "form": "주로 누구랑 가는데요",
          "original_form": "주로 누구랑 가는데요",
          "speaker_id": "2",
          "time": "20210830 19:48"
        },
        {
          "id": "MDRW2100023891.1.2",
          "form": "저 주로 그때 그때 시간 되는 인간이랑 가요",
          "original_form": "저 주로 그때 그때 시간 되는 인간이랑 가요",
          "speaker_id": "1",
          "time": "20210830 19:48"
        },
        {
          "id": "MDRW2100023891.1.3",
          "form": "저 베이징만",
          "original_form": "저 베이징만",
          "speaker_id": "2",
          "time": "20210830 19:48"
        },
        {
          "id": "MDRW2100023891.1.4",
          "form": "인싸시구나",
          "original_form": "인싸시구나",
          "speaker_id": "2",
          "time": "20210830 19:48"
        },
        {
          "id": "MDRW2100023891.1.5",
          "form": "전 베이징 못가봤어요 ㅜㅜ",
          "original_form": "전 베이징 못가봤어요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 19:48"
        },
        {
          "id": "MDRW2100023891.1.6",
          "form": "상하이는 아세요?",
          "original_form": "상하이는 아세요?",
          "speaker_id": "1",
          "time": "20210830 19:48"
        },
        {
          "id": "MDRW2100023891.1.7",
          "form": "알죠알조ㅗ",
          "original_form": "알죠알조ㅗ",
          "speaker_id": "2",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023891.1.8",
          "form": "아 오타",
          "original_form": "아 오타",
          "speaker_id": "2",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023891.1.9",
          "form": "알죠알죠",
          "original_form": "알죠알죠",
          "speaker_id": "2",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023891.1.10",
          "form": "아마 항저우는 모르실 것 같은데..",
          "original_form": "아마 항저우는 모르실 것 같은데..",
          "speaker_id": "1",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023891.1.11",
          "form": "아시안게임 한 곳 근처?",
          "original_form": "아시안게임 한 곳 근처?",
          "speaker_id": "2",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023891.1.12",
          "form": "광저우쪽?",
          "original_form": "광저우쪽?",
          "speaker_id": "2",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023891.1.13",
          "form": "어~아니요",
          "original_form": "어~아니요",
          "speaker_id": "1",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023891.1.14",
          "form": "어디지 그럼",
          "original_form": "어디지 그럼",
          "speaker_id": "2",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023891.1.15",
          "form": "여긴 중국 저장성이구요 첸탕강 아래쪽에 있어요",
          "original_form": "여긴 중국 저장성이구요 첸탕강 아래쪽에 있어요",
          "speaker_id": "1",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023891.1.16",
          "form": "상하이 밑 쪽이요!",
          "original_form": "상하이 밑 쪽이요!",
          "speaker_id": "1",
          "time": "20210830 19:49"
        },
        {
          "id": "MDRW2100023891.1.17",
          "form": "워낙 넓어서 아리까리하네요",
          "original_form": "워낙 넓어서 아리까리하네요",
          "speaker_id": "2",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023891.1.18",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023891.1.19",
          "form": "여기서 우전 여행 했었거든요 너무 좋았어요",
          "original_form": "여기서 우전 여행 했었거든요 너무 좋았어요",
          "speaker_id": "1",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023891.1.20",
          "form": "역사적인 유적이 많나요",
          "original_form": "역사적인 유적이 많나요",
          "speaker_id": "2",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023891.1.21",
          "form": "그 중국 드라마에 보면 되게 옛날 느낌 나는 것들 있잖아요",
          "original_form": "그 중국 드라마에 보면 되게 옛날 느낌 나는 것들 있잖아요",
          "speaker_id": "1",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023891.1.22",
          "form": "그런 분위기를 느낄 수 있어요",
          "original_form": "그런 분위기를 느낄 수 있어요",
          "speaker_id": "1",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023891.1.23",
          "form": "오호 좋겟다",
          "original_form": "오호 좋겟다",
          "speaker_id": "2",
          "time": "20210830 19:50"
        },
        {
          "id": "MDRW2100023891.1.24",
          "form": "아 언뜻 세계테마기행 이런데서 봣을수도",
          "original_form": "아 언뜻 세계테마기행 이런데서 봣을수도",
          "speaker_id": "2",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023891.1.25",
          "form": "돈 내면 나룻배도 태워주더라구요 ㅋㅋㅋ",
          "original_form": "돈 내면 나룻배도 태워주더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023891.1.26",
          "form": "거기에 시후호가 되게 유명했는데 뭔가 그냥 호수였어요",
          "original_form": "거기에 시후호가 되게 유명했는데 뭔가 그냥 호수였어요",
          "speaker_id": "1",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023891.1.27",
          "form": "오오 상하이랑 같이 이어서 갔나요",
          "original_form": "오오 상하이랑 같이 이어서 갔나요",
          "speaker_id": "2",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023891.1.28",
          "form": "네네 맞아요 상하이에 좀 높은 건물 있대요",
          "original_form": "네네 맞아요 상하이에 좀 높은 건물 있대요",
          "speaker_id": "1",
          "time": "20210830 19:51"
        },
        {
          "id": "MDRW2100023891.1.29",
          "form": "외국인이 63빌딩이나 롯데타워 가면 이런 기분이구나 싶었어요",
          "original_form": "외국인이 63빌딩이나 롯데타워 가면 이런 기분이구나 싶었어요",
          "speaker_id": "1",
          "time": "20210830 19:52"
        },
        {
          "id": "MDRW2100023891.1.30",
          "form": "오 전망대 올라가셧나요",
          "original_form": "오 전망대 올라가셧나요",
          "speaker_id": "2",
          "time": "20210830 19:52"
        },
        {
          "id": "MDRW2100023891.1.31",
          "form": "네 전망대도 무슨 상하이타워 이런거였던 거 같은데",
          "original_form": "네 전망대도 무슨 상하이타워 이런거였던 거 같은데",
          "speaker_id": "1",
          "time": "20210830 19:52"
        },
        {
          "id": "MDRW2100023891.1.32",
          "form": "밤에 갔으면 야경 예뻤을텐데 아쉬웠어요",
          "original_form": "밤에 갔으면 야경 예뻤을텐데 아쉬웠어요",
          "speaker_id": "1",
          "time": "20210830 19:52"
        },
        {
          "id": "MDRW2100023891.1.33",
          "form": "보통 밤에가던데",
          "original_form": "보통 밤에가던데",
          "speaker_id": "2",
          "time": "20210830 19:53"
        },
        {
          "id": "MDRW2100023891.1.34",
          "form": "왜 낮에 가셧나용",
          "original_form": "왜 낮에 가셧나용",
          "speaker_id": "2",
          "time": "20210830 19:53"
        },
        {
          "id": "MDRW2100023891.1.35",
          "form": "그때 어학원 다녔을 때라서",
          "original_form": "그때 어학원 다녔을 때라서",
          "speaker_id": "1",
          "time": "20210830 19:53"
        },
        {
          "id": "MDRW2100023891.1.36",
          "form": "학교가 항저우사범대학교라 다시 돌아가려면 3시간 정도 걸려서요 ㅜㅜ",
          "original_form": "학교가 항저우사범대학교라 다시 돌아가려면 3시간 정도 걸려서요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210830 19:53"
        },
        {
          "id": "MDRW2100023891.1.37",
          "form": "제주도로 졸업 여행 갔을 때요",
          "original_form": "제주도로 졸업 여행 갔을 때요",
          "speaker_id": "1",
          "time": "20210830 19:59"
        }
      ]
    }
