      "utterance": [
        {
          "id": "MDRW2100044641.1.1",
          "form": "중고나라 거래 해봤나요",
          "original_form": "중고나라 거래 해봤나요",
          "speaker_id": "1",
          "time": "20210907 23:19"
        },
        {
          "id": "MDRW2100044641.1.2",
          "form": "중고거래는 딱 한번 해봤어요",
          "original_form": "중고거래는 딱 한번 해봤어요",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044641.1.3",
          "form": "당근마켓보다 중고나라가 좋은가요",
          "original_form": "당근마켓보다 중고나라가 좋은가요",
          "speaker_id": "2",
          "time": "20210907 23:20"
        },
        {
          "id": "MDRW2100044641.1.4",
          "form": "저는둘다해요",
          "original_form": "저는둘다해요",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044641.1.5",
          "form": "근거리당근 ㅈ원거리 중나",
          "original_form": "근거리당근 ㅈ원거리 중나",
          "speaker_id": "1",
          "time": "20210907 23:21"
        },
        {
          "id": "MDRW2100044641.1.6",
          "form": "최근에는 어떤 거래 하셨어요?",
          "original_form": "최근에는 어떤 거래 하셨어요?",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044641.1.7",
          "form": "아기분유요",
          "original_form": "아기분유요",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044641.1.8",
          "form": "요즘 아기 용품도 중고 거래 많이 하신다고 들었어요.",
          "original_form": "요즘 아기 용품도 중고 거래 많이 하신다고 들었어요.",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044641.1.9",
          "form": "네~~어릴수록 잠깐쓰는거라유용하드라구요",
          "original_form": "네~~어릴수록 잠깐쓰는거라유용하드라구요",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044641.1.10",
          "form": "저는 물건에 정드는 타입이라 중고 판매가 어렵더라구요ㅠㅠ",
          "original_form": "저는 물건에 정드는 타입이라 중고 판매가 어렵더라구요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:35"
        },
        {
          "id": "MDRW2100044641.1.11",
          "form": "아 저도중고는잘안팔게되요",
          "original_form": "아 저도중고는잘안팔게되요",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044641.1.12",
          "form": "정리를좀 하긴해야되는데",
          "original_form": "정리를좀 하긴해야되는데",
          "speaker_id": "1",
          "time": "20210907 23:38"
        },
        {
          "id": "MDRW2100044641.1.13",
          "form": "구매를 주로 하시는거군요!",
          "original_form": "구매를 주로 하시는거군요!",
          "speaker_id": "2",
          "time": "20210907 23:40"
        },
        {
          "id": "MDRW2100044641.1.14",
          "form": "새물건 안쓴건팔기도 하구요",
          "original_form": "새물건 안쓴건팔기도 하구요",
          "speaker_id": "1",
          "time": "20210907 23:41"
        },
        {
          "id": "MDRW2100044641.1.15",
          "form": "저는 요즘 사고싶은 게 많아서 고민입니다",
          "original_form": "저는 요즘 사고싶은 게 많아서 고민입니다",
