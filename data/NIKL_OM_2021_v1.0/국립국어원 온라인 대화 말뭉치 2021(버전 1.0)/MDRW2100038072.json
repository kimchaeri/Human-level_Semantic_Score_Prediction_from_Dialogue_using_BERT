      "utterance": [
        {
          "id": "MDRW2100038072.1.1",
          "form": "요새 계속 비와",
          "original_form": "요새 계속 비와",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100038072.1.2",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100038072.1.3",
          "form": "가끔은 좋아. 시원하거든",
          "original_form": "가끔은 좋아. 시원하거든",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100038072.1.4",
          "form": "새벽에 비오면 좋아",
          "original_form": "새벽에 비오면 좋아",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100038072.1.5",
          "form": "나는 집에서 소리 듣는거좋아해",
          "original_form": "나는 집에서 소리 듣는거좋아해",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100038072.1.6",
          "form": "어두컴컴해서 자기 딱 좋아",
          "original_form": "어두컴컴해서 자기 딱 좋아",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100038072.1.7",
          "form": "하지만 비오면 너무 꿉꿉하지?",
          "original_form": "하지만 비오면 너무 꿉꿉하지?",
          "speaker_id": "1",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100038072.1.8",
          "form": "천둥번개도 좋아해?",
          "original_form": "천둥번개도 좋아해?",
          "speaker_id": "2",
          "time": "20210901 22:57"
        },
        {
          "id": "MDRW2100038072.1.9",
          "form": "나는 천둥번개는 안쳤으먄 좋겠어",
          "original_form": "나는 천둥번개는 안쳤으먄 좋겠어",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038072.1.10",
          "form": "응 나는 천둥소리 좋아해",
          "original_form": "응 나는 천둥소리 좋아해",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038072.1.11",
          "form": "그냥 비만 오면 좋겠어",
          "original_form": "그냥 비만 오면 좋겠어",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038072.1.12",
          "form": "세상에 왜그래?",
          "original_form": "세상에 왜그래?",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038072.1.13",
          "form": "아 정말?",
          "original_form": "아 정말?",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038072.1.14",
          "form": "그럼 만나몀 내가 니 주위에서 우르릉 해줄게",
          "original_form": "그럼 만나몀 내가 니 주위에서 우르릉 해줄게",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038072.1.15",
          "form": "천둥소리 들으면 뭔가 편안해져",
          "original_form": "천둥소리 들으면 뭔가 편안해져",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038072.1.16",
          "form": "특이취향이구나",
          "original_form": "특이취향이구나",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038072.1.17",
          "form": "신기하다",
          "original_form": "신기하다",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038072.1.18",
          "form": "음 나는 자연의 소리를 좋아해!!",
          "original_form": "음 나는 자연의 소리를 좋아해!!",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038072.1.19",
          "form": "마치 뱃고동소리같잖아",
          "original_form": "마치 뱃고동소리같잖아",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038072.1.20",
          "form": "인위적인건 별로야",
          "original_form": "인위적인건 별로야",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038072.1.21",
          "form": "그것도... 아 그렇구나",
          "original_form": "그것도... 아 그렇구나",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038072.1.22",
          "form": "응 나 뱃고동소리도 좋아해",
          "original_form": "응 나 뱃고동소리도 좋아해",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038072.1.23",
          "form": "근데 인간의소리도 자연소리 아닐까?",
          "original_form": "근데 인간의소리도 자연소리 아닐까?",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038072.1.24",
          "form": "할아버지기침소리같은던 어때?",
          "original_form": "할아버지기침소리같은던 어때?",
          "speaker_id": "2",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038072.1.25",
          "form": "아니. 인위적이라구.",
          "original_form": "아니. 인위적이라구.",
          "speaker_id": "1",
          "time": "20210901 22:58"
        },
        {
          "id": "MDRW2100038072.1.26",
          "form": "한국적인 소리 모음집에 잇더라",
          "original_form": "한국적인 소리 모음집에 잇더라",
          "speaker_id": "2",
          "time": "20210901 22:58"
        }
      ]
