      "utterance": [
        {
          "id": "MDRW2100016824.1.1",
          "form": "종교 있으신가요?",
          "original_form": "종교 있으신가요?",
          "speaker_id": "1",
          "time": "20210826 15:37"
        },
        {
          "id": "MDRW2100016824.1.2",
          "form": "무교예요",
          "original_form": "무교예요",
          "speaker_id": "2",
          "time": "20210826 15:39"
        },
        {
          "id": "MDRW2100016824.1.3",
          "form": "저도 ㅎㅎ 원래는 있었는데",
          "original_form": "저도 ㅎㅎ 원래는 있었는데",
          "speaker_id": "1",
          "time": "20210826 15:41"
        },
        {
          "id": "MDRW2100016824.1.4",
          "form": "안가게 되더라구요",
          "original_form": "안가게 되더라구요",
          "speaker_id": "1",
          "time": "20210826 15:42"
        },
        {
          "id": "MDRW2100016824.1.5",
          "form": "절 가보신적 있으신가요",
          "original_form": "절 가보신적 있으신가요",
          "speaker_id": "2",
          "time": "20210826 15:44"
        },
        {
          "id": "MDRW2100016824.1.6",
          "form": "네네 저 원래 절 다녔어요 ㅎㅎ",
          "original_form": "네네 저 원래 절 다녔어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100016824.1.7",
          "form": "가보셨어요?",
          "original_form": "가보셨어요?",
          "speaker_id": "1",
          "time": "20210826 15:46"
        },
        {
          "id": "MDRW2100016824.1.8",
          "form": "봉은사 가봤어요!",
          "original_form": "봉은사 가봤어요!",
          "speaker_id": "2",
          "time": "20210826 15:47"
        },
        {
          "id": "MDRW2100016824.1.9",
          "form": "오오 어떠셨어요 ?",
          "original_form": "오오 어떠셨어요 ?",
          "speaker_id": "1",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016824.1.10",
          "form": "힐링하셨나요 ㅎㅎ",
          "original_form": "힐링하셨나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:48"
        },
        {
          "id": "MDRW2100016824.1.11",
          "form": "종소리가 평온을 주더라고요",
          "original_form": "종소리가 평온을 주더라고요",
          "speaker_id": "2",
          "time": "20210826 15:50"
        },
        {
          "id": "MDRW2100016824.1.12",
          "form": "아 맞아요 ㅎㅎ 엄청 조용한데",
          "original_form": "아 맞아요 ㅎㅎ 엄청 조용한데",
          "speaker_id": "1",
          "time": "20210826 15:52"
        },
        {
          "id": "MDRW2100016824.1.13",
          "form": "종소리만 나니까 좋아요 ㅎㅎ",
          "original_form": "종소리만 나니까 좋아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:53"
        },
        {
          "id": "MDRW2100016824.1.14",
          "form": "교회 가보셨나요?",
          "original_form": "교회 가보셨나요?",
          "speaker_id": "2",
          "time": "20210826 15:54"
        },
        {
          "id": "MDRW2100016824.1.15",
          "form": "저 교회는 한번도 안갔어요!",
          "original_form": "저 교회는 한번도 안갔어요!",
          "speaker_id": "1",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100016824.1.16",
          "form": "아 성당은 가봤네요 ㅎㅎ",
          "original_form": "아 성당은 가봤네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100016824.1.17",
          "form": "저는 초딩때 가봤어요",
          "original_form": "저는 초딩때 가봤어요",
          "speaker_id": "2",
          "time": "20210826 15:55"
        },
        {
          "id": "MDRW2100016824.1.18",
          "form": "아 초딩때 ㅋㅋ 맞아요 막 맛있는거주고",
          "original_form": "아 초딩때 ㅋㅋ 맞아요 막 맛있는거주고",
          "speaker_id": "1",
          "time": "20210826 15:56"
        },
        {
          "id": "MDRW2100016824.1.19",
          "form": "이랬던거같은데",
          "original_form": "이랬던거같은데",
          "speaker_id": "1",
          "time": "20210826 15:56"
        }
      ]
