        },
        {
          "id": "MDRW2100015179.1.2",
          "form": "아 누구 노래에요??",
          "original_form": "아 누구 노래에요??",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015179.1.3",
          "form": "어떤 작품 보시나요?",
          "original_form": "어떤 작품 보시나요?",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015179.1.4",
          "form": "아 저는 최근에 블랙위도우?",
          "original_form": "아 저는 최근에 블랙위도우?",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015179.1.5",
          "form": "혹시 마블영화 보시나요? ㅋㅋ",
          "original_form": "혹시 마블영화 보시나요? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015179.1.6",
          "form": "어제 봤어요 ㅋㅋㅋ",
          "original_form": "어제 봤어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015179.1.7",
          "form": "블랙위도우가 넷플에 있어요?",
          "original_form": "블랙위도우가 넷플에 있어요?",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100015179.1.8",
          "form": "아 아니요 저는 네이버시리즈에서 봤어요",
          "original_form": "아 아니요 저는 네이버시리즈에서 봤어요",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015179.1.9",
          "form": "아 ㅋㅋ",
          "original_form": "아 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015179.1.10",
          "form": "저도 네이버 시리즈에서 봤는데!!",
          "original_form": "저도 네이버 시리즈에서 봤는데!!",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015179.1.11",
          "form": "스칼렛요한슨 너무 이쁘더라구요",
          "original_form": "스칼렛요한슨 너무 이쁘더라구요",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015179.1.12",
          "form": "근데 기대했던 것보단 조금 ㅜㅜ 그랬어요",
          "original_form": "근데 기대했던 것보단 조금 ㅜㅜ 그랬어요",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015179.1.13",
          "form": "오 바로 어제보셨구나",
          "original_form": "오 바로 어제보셨구나",
          "speaker_id": "1",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015179.1.14",
          "form": "아직 끝까지 못봤어요..ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아직 끝까지 못봤어요..ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015179.1.15",
          "form": "집중력 부족..",
          "original_form": "집중력 부족..",
          "speaker_id": "2",
          "time": "20210825 22:09"
        },
        {
          "id": "MDRW2100015179.1.16",
          "form": "저도 그냥 쏘쏘 너무 기대해서 그런가 ㅋㅋㅋ",
          "original_form": "저도 그냥 쏘쏘 너무 기대해서 그런가 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015179.1.17",
          "form": "아 ㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015179.1.18",
          "form": "크루엘라는 보셨나요?",
          "original_form": "크루엘라는 보셨나요?",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015179.1.19",
          "form": "그쵸 ㅠㅠ 그냥 크루엘라를 다운받을 걸 그랬나봐요",
          "original_form": "그쵸 ㅠㅠ 그냥 크루엘라를 다운받을 걸 그랬나봐요",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015179.1.20",
          "form": "아 대박 우리 둘이 진짜 잘통하네요",
          "original_form": "아 대박 우리 둘이 진짜 잘통하네요",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015179.1.21",
          "form": "저도 고민했는데 ㅋㅋㅋㅋㅋ",
          "original_form": "저도 고민했는데 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015179.1.22",
          "form": "그두개가 상위권이더라고요",
          "original_form": "그두개가 상위권이더라고요",
          "speaker_id": "1",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015179.1.23",
          "form": "가격 때문에 블랙위도우 고른 나를 욕해야죠..",
          "original_form": "가격 때문에 블랙위도우 고른 나를 욕해야죠..",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015179.1.24",
          "form": "만원과 이만원..",
          "original_form": "만원과 이만원..",
          "speaker_id": "2",
          "time": "20210825 22:10"
        },
        {
          "id": "MDRW2100015179.1.25",
          "form": "ㅋㅋㅋㅋㅋ아 ㅋㅋ저는 네페 등록하니까 영화보는쿠폰주더라고요",
          "original_form": "ㅋㅋㅋㅋㅋ아 ㅋㅋ저는 네페 등록하니까 영화보는쿠폰주더라고요",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015179.1.26",
          "form": "저도 그걸로 봤어요..ㅋㅋ",
          "original_form": "저도 그걸로 봤어요..ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015179.1.27",
          "form": "그래서 만원짜리가 더 손해인 느낌이였어요..",
          "original_form": "그래서 만원짜리가 더 손해인 느낌이였어요..",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015179.1.28",
          "form": "아하 ㅋㅋㅋㅋㅋ저도 담달에는 그걸로 크루엘라 봐야겠어요 ㅋㅋㅋㅋㅋ",
          "original_form": "아하 ㅋㅋㅋㅋㅋ저도 담달에는 그걸로 크루엘라 봐야겠어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015179.1.29",
          "form": "크루엘라 진짜 재밌대요",
          "original_form": "크루엘라 진짜 재밌대요",
          "speaker_id": "2",
          "time": "20210825 22:11"
        },
        {
          "id": "MDRW2100015179.1.30",
          "form": "저 그거 영화관에서 보려고 했는데 못봤어요 ㅜㅜ",
          "original_form": "저 그거 영화관에서 보려고 했는데 못봤어요 ㅜㅜ",
