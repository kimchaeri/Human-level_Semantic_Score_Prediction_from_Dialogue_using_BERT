      "utterance": [
        {
          "id": "MDRW2100022313.1.1",
          "form": "왜 그렇습니까",
          "original_form": "왜 그렇습니까",
          "speaker_id": "2",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022313.1.2",
          "form": "순살 먹는게 잘못됐나요 ㅜㅜ",
          "original_form": "순살 먹는게 잘못됐나요 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022313.1.3",
          "form": "그럼 가족들하고 먹을 때도 순살만드사요",
          "original_form": "그럼 가족들하고 먹을 때도 순살만드사요",
          "speaker_id": "1",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022313.1.4",
          "form": "아니요",
          "original_form": "아니요",
          "speaker_id": "1",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022313.1.5",
          "form": "네 순살먹어요",
          "original_form": "네 순살먹어요",
          "speaker_id": "2",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022313.1.6",
          "form": "이게 안쳐져서ㅋㅋㅋ",
          "original_form": "이게 안쳐져서ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022313.1.7",
          "form": "뼈닭 시키면 누나가 골라 먹는게 얄미워서요",
          "original_form": "뼈닭 시키면 누나가 골라 먹는게 얄미워서요",
          "speaker_id": "2",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022313.1.8",
          "form": "맨손으로 쩝쩝거린 손으로 뒤적거리면 화가 나요",
          "original_form": "맨손으로 쩝쩝거린 손으로 뒤적거리면 화가 나요",
          "speaker_id": "2",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022313.1.9",
          "form": "누나도 닭다리만드시닌요",
          "original_form": "누나도 닭다리만드시닌요",
          "speaker_id": "1",
          "time": "20210829 12:28"
        },
        {
          "id": "MDRW2100022313.1.10",
          "form": "아..  뭔지알아요",
          "original_form": "아..  뭔지알아요",
          "speaker_id": "1",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022313.1.11",
          "form": "누나는 닭다리살 젤 먼저 집어가요",
          "original_form": "누나는 닭다리살 젤 먼저 집어가요",
          "speaker_id": "2",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022313.1.12",
          "form": "한마리닭인데 닭다리 좋아하면",
          "original_form": "한마리닭인데 닭다리 좋아하면",
          "speaker_id": "1",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022313.1.13",
          "form": "그래서 제가 집은건 바로 먹으라고 해요",
          "original_form": "그래서 제가 집은건 바로 먹으라고 해요",
          "speaker_id": "2",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022313.1.14",
          "form": "치킨더홈을 시켜보세요",
          "original_form": "치킨더홈을 시켜보세요",
          "speaker_id": "1",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022313.1.15",
          "form": "다리 4개줘오",
          "original_form": "다리 4개줘오",
          "speaker_id": "1",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022313.1.16",
          "form": "오 진짜요?ㅋㅋㅋ",
          "original_form": "오 진짜요?ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022313.1.17",
          "form": "그래도 그거 안시킬래요",
          "original_form": "그래도 그거 안시킬래요",
          "speaker_id": "2",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022313.1.18",
          "form": "누나가 혼자 닭다리 다 차지할걸요",
          "original_form": "누나가 혼자 닭다리 다 차지할걸요",
          "speaker_id": "2",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022313.1.19",
          "form": "그래요 그럼",
          "original_form": "그래요 그럼",
          "speaker_id": "1",
          "time": "20210829 12:29"
        },
        {
          "id": "MDRW2100022313.1.20",
          "form": "기분나빠요 누나 ㅜ",
          "original_form": "기분나빠요 누나 ㅜ",
          "speaker_id": "2",
