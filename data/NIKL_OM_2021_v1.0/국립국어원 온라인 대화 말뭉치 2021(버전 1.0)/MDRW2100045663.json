      "utterance": [
        {
          "id": "MDRW2100045663.1.1",
          "form": "생선이 들어가나요?",
          "original_form": "생선이 들어가나요?",
          "speaker_id": "2",
          "time": "20210908 04:59"
        },
        {
          "id": "MDRW2100045663.1.2",
          "form": "네 생선 들어가요",
          "original_form": "네 생선 들어가요",
          "speaker_id": "1",
          "time": "20210908 05:00"
        },
        {
          "id": "MDRW2100045663.1.3",
          "form": "으하하하 껌이죠",
          "original_form": "으하하하 껌이죠",
          "speaker_id": "2",
          "time": "20210908 05:01"
        },
        {
          "id": "MDRW2100045663.1.4",
          "form": "정답! 스시!!!",
          "original_form": "정답! 스시!!!",
          "speaker_id": "2",
          "time": "20210908 05:02"
        },
        {
          "id": "MDRW2100045663.1.5",
          "form": "정답!!! ㅋㅋㅋㅋㅋㅋ",
          "original_form": "정답!!! ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:02"
        },
        {
          "id": "MDRW2100045663.1.6",
          "form": "연어초밥이 먹고싶엇어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "연어초밥이 먹고싶엇어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:02"
        },
        {
          "id": "MDRW2100045663.1.7",
          "form": "대박......저도 연어초밥 너무 먹고싶어요",
          "original_form": "대박......저도 연어초밥 너무 먹고싶어요",
          "speaker_id": "2",
          "time": "20210908 05:02"
        },
        {
          "id": "MDRW2100045663.1.8",
          "form": "와사비도 잘 드세요?",
          "original_form": "와사비도 잘 드세요?",
          "speaker_id": "2",
          "time": "20210908 05:02"
        },
        {
          "id": "MDRW2100045663.1.9",
          "form": "네 와사비 좋아해요",
          "original_form": "네 와사비 좋아해요",
          "speaker_id": "1",
          "time": "20210908 05:03"
        },
        {
          "id": "MDRW2100045663.1.10",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210908 05:03"
        },
        {
          "id": "MDRW2100045663.1.11",
          "form": "헉 저는 아직 와사비공격에 버티지 못해요",
          "original_form": "헉 저는 아직 와사비공격에 버티지 못해요",
          "speaker_id": "2",
          "time": "20210908 05:04"
        },
        {
          "id": "MDRW2100045663.1.12",
          "form": "ㅋㅋㅋ초밥에 찔끔 든 건 먹는데 왕창은 못 먹고 있어요",
          "original_form": "ㅋㅋㅋ초밥에 찔끔 든 건 먹는데 왕창은 못 먹고 있어요",
          "speaker_id": "2",
          "time": "20210908 05:04"
        },
        {
          "id": "MDRW2100045663.1.13",
          "form": "ㅋㅋㅋㅋㅋ전 와사비 한움큼이 아니면 ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ전 와사비 한움큼이 아니면 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:04"
        },
        {
          "id": "MDRW2100045663.1.14",
          "form": "잘먹어요 ㅋㅋㅋㅋㅋ",
          "original_form": "잘먹어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:04"
        },
        {
          "id": "MDRW2100045663.1.15",
          "form": "우와 매운거 잘 드시나요?",
          "original_form": "우와 매운거 잘 드시나요?",
          "speaker_id": "2",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100045663.1.16",
          "form": "매운거는 잘 못먹어요",
          "original_form": "매운거는 잘 못먹어요",
          "speaker_id": "1",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100045663.1.17",
          "form": "좋아는 하는데 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "좋아는 하는데 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100045663.1.18",
          "form": "근데 와사비는 우째 그리 잘 드세요",
          "original_form": "근데 와사비는 우째 그리 잘 드세요",
          "speaker_id": "2",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100045663.1.19",
          "form": "아 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100045663.1.20",
          "form": "곧잘 먹고 괴로워하시는 ㅋㅋㅋㅋ편이구나",
          "original_form": "곧잘 먹고 괴로워하시는 ㅋㅋㅋㅋ편이구나",
          "speaker_id": "2",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100045663.1.21",
          "form": "네 ㅋㅋㅋㅋㅋ정답",
          "original_form": "네 ㅋㅋㅋㅋㅋ정답",
          "speaker_id": "1",
          "time": "20210908 05:05"
        },
        {
          "id": "MDRW2100045663.1.22",
          "form": "매운거 잘드세요?",
          "original_form": "매운거 잘드세요?",
          "speaker_id": "1",
          "time": "20210908 05:06"
        },
        {
          "id": "MDRW2100045663.1.23",
          "form": "와사비말고",
          "original_form": "와사비말고",
          "speaker_id": "1",
          "time": "20210908 05:06"
        },
        {
          "id": "MDRW2100045663.1.24",
          "form": "네! 매운거는 잘 먹는데 전 반대로 와사비를 잘 못 먹어요",
          "original_form": "네! 매운거는 잘 먹는데 전 반대로 와사비를 잘 못 먹어요",
          "speaker_id": "2",
          "time": "20210908 05:06"
        },
        {
          "id": "MDRW2100045663.1.25",
          "form": "왠지 모르겠는데 와사비공격을 못 참겠어요 ㅋㅋㅋㅋ",
          "original_form": "왠지 모르겠는데 와사비공격을 못 참겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045663.1.26",
          "form": "코를 쏴서 그런가",
          "original_form": "코를 쏴서 그런가",
          "speaker_id": "2",
          "time": "20210908 05:07"
        },
        {
          "id": "MDRW2100045663.1.27",
          "form": "신기하네요 매운맛은 잘먹는데 와사비를 잘 못드시다니",
          "original_form": "신기하네요 매운맛은 잘먹는데 와사비를 잘 못드시다니",
          "speaker_id": "1",
          "time": "20210908 05:07"
        }
      ]
