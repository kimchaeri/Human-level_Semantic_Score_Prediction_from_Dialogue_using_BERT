      "utterance": [
        {
          "id": "MDRW2100026653.1.1",
          "form": "오 나 말할거 많아",
          "original_form": "오 나 말할거 많아",
          "speaker_id": "2",
          "time": "20210901 23:25"
        },
        {
          "id": "MDRW2100026653.1.2",
          "form": "나 푸들키우거든",
          "original_form": "나 푸들키우거든",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026653.1.3",
          "form": "오 푸들 귀엽겠다!",
          "original_form": "오 푸들 귀엽겠다!",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026653.1.4",
          "form": "나만 반려동물 없어",
          "original_form": "나만 반려동물 없어",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026653.1.5",
          "form": "푸들자랑 좀 해주라 ㅋㅋㅋ",
          "original_form": "푸들자랑 좀 해주라 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026653.1.6",
          "form": "근데 내가 키워서 하는 말이 아니라 강아지 키울때 푸들이 좋아",
          "original_form": "근데 내가 키워서 하는 말이 아니라 강아지 키울때 푸들이 좋아",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026653.1.7",
          "form": "애교 많고 사람도 좋아하고",
          "original_form": "애교 많고 사람도 좋아하고",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100026653.1.8",
          "form": "그리고 귀여워 ㅋㅋㅋㅋ",
          "original_form": "그리고 귀여워 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:34"
        },
        {
          "id": "MDRW2100026653.1.9",
          "form": "귀여운거 중요하다 ㅋㅋㅋㅋㅋ\n푸들이 그렇게 똑똑하다면서???",
          "original_form": "귀여운거 중요하다 ㅋㅋㅋㅋㅋ\n푸들이 그렇게 똑똑하다면서???",
          "speaker_id": "1",
          "time": "20210901 23:35"
        },
        {
          "id": "MDRW2100026653.1.10",
          "form": "응 완전 귀여워ㅠㅠ 푸들 진짜 짱이야.. 털도 안빠지구 활발해서 초심자가 키우기에도 좋아!!",
          "original_form": "응 완전 귀여워ㅠㅠ 푸들 진짜 짱이야.. 털도 안빠지구 활발해서 초심자가 키우기에도 좋아!!",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026653.1.11",
          "form": "나 졸려서 먼저 자러갈게!! 일어나서 톡할게!! 푸들짱!",
          "original_form": "나 졸려서 먼저 자러갈게!! 일어나서 톡할게!! 푸들짱!",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026653.1.12",
          "form": "응 잘자~~~ 푸들 얘기 또 해줘",
          "original_form": "응 잘자~~~ 푸들 얘기 또 해줘",
          "speaker_id": "1",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026653.1.13",
          "form": "일어났당 푸들은 그리고 셀프미용해도 곱슬거려서 괜찮아 ㅋㅋㅋ 잘 못 깎아도 티가 안나!",
          "original_form": "일어났당 푸들은 그리고 셀프미용해도 곱슬거려서 괜찮아 ㅋㅋㅋ 잘 못 깎아도 티가 안나!",
          "speaker_id": "2",
          "time": "20210902 09:59"
        },
        {
          "id": "MDRW2100026653.1.14",
          "form": "앜ㅋㅋㅋㅋㅋ",
          "original_form": "앜ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:07"
        },
        {
          "id": "MDRW2100026653.1.15",
          "form": "잘잤구나 ㅋㅋㅋ 털 잘못 깎을 때도 있어??",
          "original_form": "잘잤구나 ㅋㅋㅋ 털 잘못 깎을 때도 있어??",
          "speaker_id": "1",
          "time": "20210902 10:08"
        },
        {
          "id": "MDRW2100026653.1.16",
          "form": "초보는 그 뭐지 일정하게 털 깎기가 약간 어려웡",
          "original_form": "초보는 그 뭐지 일정하게 털 깎기가 약간 어려웡",
          "speaker_id": "2",
          "time": "20210902 10:11"
        },
        {
          "id": "MDRW2100026653.1.17",
          "form": "그게 티가 잘 안낭ㅋㅋㅋㅋ",
          "original_form": "그게 티가 잘 안낭ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 10:11"
        },
        {
          "id": "MDRW2100026653.1.18",
          "form": "오 좋다",
          "original_form": "오 좋다",
          "speaker_id": "1",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100026653.1.19",
          "form": "강아지들이 털 깎으면 우울해한다고 하던데",
          "original_form": "강아지들이 털 깎으면 우울해한다고 하던데",
          "speaker_id": "1",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100026653.1.20",
          "form": "티 안나면 강아지도 모를 거 아냐 ㅋㅋㅋ",
          "original_form": "티 안나면 강아지도 모를 거 아냐 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:12"
        },
        {
          "id": "MDRW2100026653.1.21",
          "form": "엄청 닭백숙처럼 깎으면 우울해행",
          "original_form": "엄청 닭백숙처럼 깎으면 우울해행",
          "speaker_id": "2",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100026653.1.22",
          "form": "적당히 털 남기면 괜차나",
          "original_form": "적당히 털 남기면 괜차나",
          "speaker_id": "2",
          "time": "20210902 10:13"
        },
        {
          "id": "MDRW2100026653.1.23",
          "form": "에고 ㅋㅋㅋㅋㅋ",
          "original_form": "에고 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100026653.1.24",
          "form": "푸들은 털색 무슨 색이야??",
          "original_form": "푸들은 털색 무슨 색이야??",
          "speaker_id": "1",
          "time": "20210902 10:14"
        },
        {
          "id": "MDRW2100026653.1.25",
          "form": "갈색!!",
          "original_form": "갈색!!",
          "speaker_id": "2",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100026653.1.26",
          "form": "타버린식빵색!",
          "original_form": "타버린식빵색!{emoji:♡}",
          "speaker_id": "2",
          "time": "20210902 10:16"
        },
        {
          "id": "MDRW2100026653.1.27",
          "form": "와! 진짜 귀엽겠다!!!",
          "original_form": "와! 진짜 귀엽겠다!!!",
          "speaker_id": "1",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100026653.1.28",
          "form": "털찌면 곰돌이 같은 색이네 ㅋㅋㅋㅋ 이쁘겠다 ㅋㅋㅋ",
          "original_form": "털찌면 곰돌이 같은 색이네 ㅋㅋㅋㅋ 이쁘겠다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100026653.1.29",
          "form": "같이산지 얼마나 됐어??",
          "original_form": "같이산지 얼마나 됐어??",
          "speaker_id": "1",
          "time": "20210902 10:18"
        },
        {
          "id": "MDRW2100026653.1.30",
          "form": "응응 10년 됐다앙",
          "original_form": "응응 10년 됐다앙",
          "speaker_id": "2",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100026653.1.31",
          "form": "진짜 시간이 너무 빨라",
          "original_form": "진짜 시간이 너무 빨라",
          "speaker_id": "2",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100026653.1.32",
          "form": "벌써 1년이 아니라 벌써 10년이야",
          "original_form": "벌써 1년이 아니라 벌써 10년이야",
          "speaker_id": "2",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100026653.1.33",
          "form": "와 애기때 모습이 아직도 눈에 선할텐데",
          "original_form": "와 애기때 모습이 아직도 눈에 선할텐데",
          "speaker_id": "1",
          "time": "20210902 10:19"
        },
        {
          "id": "MDRW2100026653.1.34",
          "form": "응 맞아ㅠㅠ 완전 그래",
          "original_form": "응 맞아ㅠㅠ 완전 그래",
          "speaker_id": "2",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100026653.1.35",
          "form": "푸들이 사람처럼 똑똑하다던데",
          "original_form": "푸들이 사람처럼 똑똑하다던데",
          "speaker_id": "1",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100026653.1.36",
          "form": "내눈엔 애기야 지금도ㅠㅠ",
          "original_form": "내눈엔 애기야 지금도ㅠㅠ",
          "speaker_id": "2",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100026653.1.37",
          "form": "아 그거 ㅋㅋㅋ 좀 애가 눈치가 빤해",
          "original_form": "아 그거 ㅋㅋㅋ 좀 애가 눈치가 빤해",
          "speaker_id": "2",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100026653.1.38",
          "form": "너무 웃겨",
          "original_form": "너무 웃겨",
          "speaker_id": "2",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100026653.1.39",
          "form": "평생 애기지 진짜 ㅋㅋㅋ",
          "original_form": "평생 애기지 진짜 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100026653.1.40",
          "form": "짠",
          "original_form": "짠",
          "speaker_id": "2",
          "time": "20210902 10:20"
        },
        {
          "id": "MDRW2100026653.1.41",
          "form": "눈치도 잘 보고 잔머리 잘 굴려",
          "original_form": "눈치도 잘 보고 잔머리 잘 굴려",
