      "utterance": [
        {
          "id": "MDRW2100023732.1.1",
          "form": "애들은 학교 잘 갔나요",
          "original_form": "애들은 학교 잘 갔나요",
          "speaker_id": "1",
          "time": "20210831 13:57"
