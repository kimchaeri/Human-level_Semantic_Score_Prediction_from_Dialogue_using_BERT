      "utterance": [
        {
          "id": "MDRW2100038313.1.1",
          "form": "혹시 대학생이신가요?",
          "original_form": "혹시 대학생이신가요?",
          "speaker_id": "2",
          "time": "20210902 09:38"
        },
        {
          "id": "MDRW2100038313.1.2",
          "form": "아니요 저는 대학원생이랍니다!",
          "original_form": "아니요 저는 대학원생이랍니다!",
          "speaker_id": "1",
          "time": "20210902 09:38"
        },
        {
          "id": "MDRW2100038313.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210902 09:39"
        },
        {
          "id": "MDRW2100038313.1.4",
          "form": "앗 저는 사회초년생... 지금은 출근했지만 열심히 월급루팡 중이지요",
          "original_form": "앗 저는 사회초년생... 지금은 출근했지만 열심히 월급루팡 중이지요",
          "speaker_id": "2",
          "time": "20210902 09:44"
        },
        {
          "id": "MDRW2100038313.1.5",
          "form": "대학원생 동아리 같은 건 없겠죠...? 대학원생들... 바빠서...",
          "original_form": "대학원생 동아리 같은 건 없겠죠...? 대학원생들... 바빠서...",
          "speaker_id": "2",
          "time": "20210902 09:45"
        },
        {
          "id": "MDRW2100038313.1.6",
          "form": "대학원생들은 아마 동호회에 들어갈 거 같아욬ㅋㅋㅋ",
          "original_form": "대학원생들은 아마 동호회에 들어갈 거 같아욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:46"
        },
        {
          "id": "MDRW2100038313.1.7",
          "form": "학생과 직장인의 중간자 느낌이라,,,",
          "original_form": "학생과 직장인의 중간자 느낌이라,,,",
          "speaker_id": "1",
          "time": "20210902 09:46"
        },
        {
          "id": "MDRW2100038313.1.8",
          "form": "우와 사회초년생이시구나!",
          "original_form": "우와 사회초년생이시구나!",
          "speaker_id": "1",
          "time": "20210902 09:46"
        },
        {
          "id": "MDRW2100038313.1.9",
          "form": "아 동호회~~",
          "original_form": "아 동호회~~",
          "speaker_id": "2",
          "time": "20210902 09:46"
        },
        {
          "id": "MDRW2100038313.1.10",
          "form": "다음 주제는 일/직업으로",
          "original_form": "다음 주제는 일/직업으로",
          "speaker_id": "1",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038313.1.11",
          "form": "해야겠어요 ㅋㅋㅋㅋ",
          "original_form": "해야겠어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038313.1.12",
          "form": "부럽습니다 저도 직장인 되려고 대학원 들어온거라...!!!",
          "original_form": "부럽습니다 저도 직장인 되려고 대학원 들어온거라...!!!",
          "speaker_id": "1",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038313.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ좋아요ㅋㅋ  지금은 name1님의 학교생활 이야기 해요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ좋아요ㅋㅋ  지금은 &name1&님의 학교생활 이야기 해요",
          "speaker_id": "2",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038313.1.14",
          "form": "지금쯤 개강하셨나요?",
          "original_form": "지금쯤 개강하셨나요?",
          "speaker_id": "2",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038313.1.15",
          "form": "네네 근데 뭐랄까요 저희 전공은",
          "original_form": "네네 근데 뭐랄까요 저희 전공은",
          "speaker_id": "1",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038313.1.16",
          "form": "개강이랑 방학이랑 차이가 없어요,,,",
          "original_form": "개강이랑 방학이랑 차이가 없어요,,,",
          "speaker_id": "1",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038313.1.17",
          "form": "월화수목금 출근이라...!!",
          "original_form": "월화수목금 출근이라...!!",
          "speaker_id": "1",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038313.1.18",
          "form": "어떻게 그럴 수가 있죠",
          "original_form": "어떻게 그럴 수가 있죠",
          "speaker_id": "2",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038313.1.19",
          "form": "방학에도 그렇게 출근 하나요",
          "original_form": "방학에도 그렇게 출근 하나요",
          "speaker_id": "2",
          "time": "20210902 09:47"
        },
        {
          "id": "MDRW2100038313.1.20",
          "form": "네,,,,",
          "original_form": "네,,,,",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038313.1.21",
          "form": "10시 출근 8시 퇴근이랍니다",
          "original_form": "10시 출근 8시 퇴근이랍니다",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038313.1.22",
          "form": "너무하는데요...",
          "original_form": "너무하는데요...",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038313.1.23",
          "form": "나름 월급도 받아요! 코딱지만하지만...",
          "original_form": "나름 월급도 받아요! 코딱지만하지만...",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038313.1.24",
          "form": "그쵸!!!!",
          "original_form": "그쵸!!!!",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038313.1.25",
          "form": "대학원생은 인권 사각지대에 놓여있어요....",
          "original_form": "대학원생은 인권 사각지대에 놓여있어요....",
          "speaker_id": "1",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038313.1.26",
          "form": "혹시 전공이 어느 쪽이세요",
          "original_form": "혹시 전공이 어느 쪽이세요",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038313.1.27",
          "form": "??",
          "original_form": "??",
          "speaker_id": "2",
          "time": "20210902 09:48"
        },
        {
          "id": "MDRW2100038313.1.28",
          "form": "저는 유기화학 전공하고 있습니다,,,",
          "original_form": "저는 유기화학 전공하고 있습니다,,,",
          "speaker_id": "1",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100038313.1.29",
          "form": "name2님 전공은 어떻게 되세요~?",
          "original_form": "&name2&님 전공은 어떻게 되세요~?",
          "speaker_id": "1",
          "time": "20210902 10:26"
        },
        {
          "id": "MDRW2100038313.1.30",
          "form": "유기화학이 대체 뭔가요...  저는 문과예요",
          "original_form": "유기화학이 대체 뭔가요...  저는 문과예요",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100038313.1.31",
          "form": "유기... 화학",
          "original_form": "유기... 화학",
          "speaker_id": "2",
          "time": "20210902 10:32"
        },
        {
          "id": "MDRW2100038313.1.32",
          "form": "화학을 갖다 버리면 유기화학 인가요",
          "original_form": "화학을 갖다 버리면 유기화학 인가요",
          "speaker_id": "2",
          "time": "20210902 10:33"
        },
        {
          "id": "MDRW2100038313.1.33",
          "form": "아낰ㅋㅋㅋㅋㅋㅋ",
          "original_form": "아낰ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:12"
        },
        {
          "id": "MDRW2100038313.1.34",
          "form": "정말 기발한 생각이에욬ㅋㅋㅋ",
          "original_form": "정말 기발한 생각이에욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 11:13"
        },
        {
          "id": "MDRW2100038313.1.35",
          "form": "유기화학이 뭐냐면요",
          "original_form": "유기화학이 뭐냐면요",
          "speaker_id": "1",
          "time": "20210902 11:16"
        },
        {
          "id": "MDRW2100038313.1.36",
          "form": "탄소랑 수소로 이루어진 물질을 가지고 합성하는거예요!",
          "original_form": "탄소랑 수소로 이루어진 물질을 가지고 합성하는거예요!",
          "speaker_id": "1",
