      "utterance": [
        {
          "id": "MDRW2100041732.1.1",
          "form": "도수 치료로 둘어간다네#",
          "original_form": "도수 치료로 둘어간다네#",
          "speaker_id": "2",
          "time": "20210906 15:11"
        },
        {
          "id": "MDRW2100041732.1.2",
          "form": "그래서 보험금 하나도 안 나온다 함",
          "original_form": "그래서 보험금 하나도 안 나온다 함",
          "speaker_id": "2",
          "time": "20210906 15:11"
        },
        {
          "id": "MDRW2100041732.1.3",
          "form": "너무 화가 난다..",
          "original_form": "너무 화가 난다..",
          "speaker_id": "2",
          "time": "20210906 15:11"
        },
        {
          "id": "MDRW2100041732.1.4",
          "form": "으응 헉",
          "original_form": "으응 헉",
          "speaker_id": "1",
          "time": "20210906 15:11"
        },
        {
          "id": "MDRW2100041732.1.5",
          "form": "하나도 안나온다고??????",
          "original_form": "하나도 안나온다고??????",
          "speaker_id": "1",
          "time": "20210906 15:11"
        },
        {
          "id": "MDRW2100041732.1.6",
          "form": "아니 그런게 어디있노?",
          "original_form": "아니 그런게 어디있노?",
          "speaker_id": "1",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041732.1.7",
          "form": "병원에서도수 치료란 얘기 없었거든",
          "original_form": "병원에서도수 치료란 얘기 없었거든",
          "speaker_id": "2",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041732.1.8",
          "form": "마치 도 하고 햇는데",
          "original_form": "마치 도 하고 햇는데",
          "speaker_id": "2",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041732.1.9",
          "form": "수술로 안 들어가는데",
          "original_form": "수술로 안 들어가는데",
          "speaker_id": "2",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041732.1.10",
          "form": "응응 아",
          "original_form": "응응 아",
          "speaker_id": "1",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041732.1.11",
          "form": "보험금만 내고 응",
          "original_form": "보험금만 내고 응",
          "speaker_id": "2",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041732.1.12",
          "form": "하나도 안나오는 건 좀 심한데?",
          "original_form": "하나도 안나오는 건 좀 심한데?",
          "speaker_id": "1",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041732.1.13",
          "form": "받아먹지도 못하고",
          "original_form": "받아먹지도 못하고",
          "speaker_id": "2",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041732.1.14",
          "form": "그러니깐은",
          "original_form": "그러니깐은",
          "speaker_id": "1",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041732.1.15",
          "form": "나 너무 화났음",
          "original_form": "나 너무 화났음",
          "speaker_id": "2",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041732.1.16",
          "form": "실비만 25만원씨",
          "original_form": "실비만 25만원씨",
          "speaker_id": "2",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041732.1.17",
          "form": "그렇지 뭐 그게 보험이고",
          "original_form": "그렇지 뭐 그게 보험이고",
          "speaker_id": "1",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041732.1.18",
          "form": "장난치나 ㅠㅠ 아이고",
          "original_form": "장난치나 ㅠㅠ 아이고",
          "speaker_id": "1",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041732.1.19",
          "form": "거의 200 나왔는데",
          "original_form": "거의 200 나왔는데",
          "speaker_id": "2",
          "time": "20210906 15:12"
        },
        {
          "id": "MDRW2100041732.1.20",
          "form": "80만원 들어왔음",
          "original_form": "80만원 들어왔음",
          "speaker_id": "2",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100041732.1.21",
          "form": "짜증나지./쌩돈 나가게 생겼는데",
          "original_form": "짜증나지./쌩돈 나가게 생겼는데",
          "speaker_id": "1",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100041732.1.22",
          "form": "아 ㅠㅠ",
          "original_form": "아 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100041732.1.23",
          "form": "진짜 현 타오더라",
          "original_form": "진짜 현 타오더라",
          "speaker_id": "2",
          "time": "20210906 15:13"
        },
        {
          "id": "MDRW2100041732.1.24",
          "form": "아 진짜 현타온다",
          "original_form": "아 진짜 현타온다",
          "speaker_id": "1",
          "time": "20210906 15:13"
        }
