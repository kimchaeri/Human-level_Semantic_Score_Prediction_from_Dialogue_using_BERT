      "utterance": [
        {
          "id": "MDRW2100029375.1.1",
          "form": "헉 ㅠ죄송해요 날라갔네요...",
          "original_form": "헉 ㅠ죄송해요 날라갔네요...",
          "speaker_id": "1",
          "time": "20210903 03:31"
        },
        {
          "id": "MDRW2100029375.1.2",
          "form": "완전 강추합니다",
          "original_form": "완전 강추합니다",
          "speaker_id": "1",
          "time": "20210903 03:32"
        },
        {
          "id": "MDRW2100029375.1.3",
          "form": "성수에 유가츠도 완전 맛있어요!",
          "original_form": "성수에 유가츠도 완전 맛있어요!",
          "speaker_id": "1",
          "time": "20210903 03:32"
        },
        {
          "id": "MDRW2100029375.1.4",
          "form": "ㅋㅋㅋㅋㄱ엄청난 맛집지도를 만들고계신것같아요",
          "original_form": "ㅋㅋㅋㅋㄱ엄청난 맛집지도를 만들고계신것같아요",
          "speaker_id": "2",
          "time": "20210903 03:32"
        },
        {
          "id": "MDRW2100029375.1.5",
          "form": "ㅋㅋㅋㅋㅋ여기에 사진 첨부 기능만 있으면 제 리스트 캡쳐해서 보내드리고 싶어요 ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ여기에 사진 첨부 기능만 있으면 제 리스트 캡쳐해서 보내드리고 싶어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 03:33"
        },
        {
          "id": "MDRW2100029375.1.6",
          "form": "혹시 문래 말고 또 어디 자주 가세요?",
          "original_form": "혹시 문래 말고 또 어디 자주 가세요?",
          "speaker_id": "1",
          "time": "20210903 03:33"
        },
        {
          "id": "MDRW2100029375.1.7",
          "form": "ㅋㅋㅋㅋㄱㅋ저 집순이라서 잘 안나가요ㅠ",
          "original_form": "ㅋㅋㅋㅋㄱㅋ저 집순이라서 잘 안나가요ㅠ",
          "speaker_id": "2",
          "time": "20210903 03:33"
        },
        {
          "id": "MDRW2100029375.1.8",
          "form": "헉 ㅠㅠㅠㅠㅠ",
          "original_form": "헉 ㅠㅠㅠㅠㅠ",
          "speaker_id": "1",
          "time": "20210903 03:34"
        },
        {
          "id": "MDRW2100029375.1.9",
          "form": "친구들이 어디 맛있다! 하면 나가는편,,,",
          "original_form": "친구들이 어디 맛있다! 하면 나가는편,,,",
          "speaker_id": "2",
          "time": "20210903 03:34"
        },
        {
          "id": "MDRW2100029375.1.10",
          "form": "name1은 멀리까지 가시나요?!",
          "original_form": "&name1&은 멀리까지 가시나요?!",
          "speaker_id": "2",
          "time": "20210903 03:34"
        },
        {
          "id": "MDRW2100029375.1.11",
          "form": "저는 제가 주도해서 다니거든요!!",
          "original_form": "저는 제가 주도해서 다니거든요!!",
          "speaker_id": "1",
          "time": "20210903 03:34"
        },
        {
          "id": "MDRW2100029375.1.12",
          "form": "성수랑 문래는 멀지않나요?!",
          "original_form": "성수랑 문래는 멀지않나요?!",
          "speaker_id": "2",
          "time": "20210903 03:34"
        },
        {
          "id": "MDRW2100029375.1.13",
          "form": "제가 성수랑 문래 중간에 살아요!!",
          "original_form": "제가 성수랑 문래 중간에 살아요!!",
          "speaker_id": "1",
          "time": "20210903 03:34"
        },
        {
          "id": "MDRW2100029375.1.14",
          "form": "학교 앞에서 자취를 하고 있는데 모든 곳이 가까워요",
          "original_form": "학교 앞에서 자취를 하고 있는데 모든 곳이 가까워요",
          "speaker_id": "1",
          "time": "20210903 03:34"
        },
        {
          "id": "MDRW2100029375.1.15",
          "form": "아 딱 맛집이 많은 곳!!",
          "original_form": "아 딱 맛집이 많은 곳!!",
          "speaker_id": "2",
          "time": "20210903 03:35"
        },
        {
          "id": "MDRW2100029375.1.16",
          "form": "맞아요!!!접근성이 너무 좋아섯",
          "original_form": "맞아요!!!접근성이 너무 좋아섯",
          "speaker_id": "1",
          "time": "20210903 03:36"
        },
        {
          "id": "MDRW2100029375.1.17",
          "form": "서울 여기저기 열심히 다니고 있어요!",
          "original_form": "서울 여기저기 열심히 다니고 있어요!",
          "speaker_id": "1",
          "time": "20210903 03:37"
        },
        {
          "id": "MDRW2100029375.1.18",
          "form": "2호선이 맛집이많죠,,,",
          "original_form": "2호선이 맛집이많죠,,,",
          "speaker_id": "2",
          "time": "20210903 03:37"
        },
        {
          "id": "MDRW2100029375.1.19",
          "form": "아 막상 뱉으려니 안나오는",
          "original_form": "아 막상 뱉으려니 안나오는",
          "speaker_id": "2",
          "time": "20210903 03:38"
        },
        {
