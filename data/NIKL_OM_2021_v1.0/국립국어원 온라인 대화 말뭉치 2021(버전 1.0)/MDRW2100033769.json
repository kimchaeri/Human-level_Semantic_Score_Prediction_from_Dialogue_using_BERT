        {
          "id": "MDRW2100033769.1.1",
          "form": "중식 좋아하세요?",
          "original_form": "중식 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 01:19"
        },
        {
          "id": "MDRW2100033769.1.2",
          "form": "없어서..못먹어요",
          "original_form": "없어서..못먹어요",
          "speaker_id": "2",
          "time": "20210825 01:20"
        },
        {
          "id": "MDRW2100033769.1.3",
          "form": "name1님은 짬뽕 짜장 둘 중 어느것?!",
          "original_form": "&name1&님은 짬뽕 짜장 둘 중 어느것?!",
          "speaker_id": "2",
          "time": "20210825 01:20"
        },
        {
          "id": "MDRW2100033769.1.4",
          "form": "저는 짬뽕이요ㅋㅋㅋㅋ",
          "original_form": "저는 짬뽕이요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:24"
        },
        {
          "id": "MDRW2100033769.1.5",
          "form": "국물을 포기못하죠",
          "original_form": "국물을 포기못하죠",
          "speaker_id": "1",
          "time": "20210825 01:24"
        },
        {
          "id": "MDRW2100033769.1.6",
          "form": "아 저도요",
          "original_form": "아 저도요",
          "speaker_id": "2",
          "time": "20210825 01:25"
        },
        {
          "id": "MDRW2100033769.1.7",
          "form": "매운 고추짬뽕 시키면 얼큰 그자체..",
          "original_form": "매운 고추짬뽕 시키면 얼큰 그자체..",
          "speaker_id": "2",
          "time": "20210825 01:25"
        },
        {
          "id": "MDRW2100033769.1.8",
          "form": "냉면이나 찌개류는 어떤 거 좋아하시나요?",
          "original_form": "냉면이나 찌개류는 어떤 거 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 01:26"
        },
        {
          "id": "MDRW2100033769.1.9",
          "form": "전 비냉이요ㅋㅋㅋ",
          "original_form": "전 비냉이요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100033769.1.10",
          "form": "찌개는 된찌 김찌?",
          "original_form": "찌개는 된찌 김찌?",
          "speaker_id": "1",
          "time": "20210825 01:34"
        },
        {
          "id": "MDRW2100033769.1.11",
          "form": "오 저도 비냉! 전 고기먹을땐 된찌 밥이랑은 고기많은 김찌요ㅎㅎ",
          "original_form": "오 저도 비냉! 전 고기먹을땐 된찌 밥이랑은 고기많은 김찌요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 01:37"
        },
        {
          "id": "MDRW2100033769.1.12",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210825 01:37"
        },
        {
          "id": "MDRW2100033769.1.13",
          "form": "ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:41"
        },
        {
          "id": "MDRW2100033769.1.14",
          "form": "제취향을",
          "original_form": "제취향을",
          "speaker_id": "1",
          "time": "20210825 01:41"
        },
        {
          "id": "MDRW2100033769.1.15",
          "form": "적어두셨나요?",
          "original_form": "적어두셨나요?",
          "speaker_id": "1",
          "time": "20210825 01:41"
        },
        {
          "id": "MDRW2100033769.1.16",
          "form": "아ㅋㅋㅋㅋㅋㅋ뭔가 웃기네여",
          "original_form": "아ㅋㅋㅋㅋㅋㅋ뭔가 웃기네여",
          "speaker_id": "2",
          "time": "20210825 01:45"
        },
        {
          "id": "MDRW2100033769.1.17",
          "form": "name1님은 맛집탐방 좋아하시나요?",
          "original_form": "&name1&님은 맛집탐방 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210825 01:46"
        },
        {
          "id": "MDRW2100033769.1.18",
          "form": "네네 너무좋아하죠ㅋㅋㅋㅋㅋ",
          "original_form": "네네 너무좋아하죠ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 01:50"
        },
        {
          "id": "MDRW2100033769.1.19",
          "form": "오 저는 맛집가는건 좋은데 줄서는건 싫어요",
          "original_form": "오 저는 맛집가는건 좋은데 줄서는건 싫어요",
          "speaker_id": "2",
          "time": "20210825 01:53"
        },
        {
          "id": "MDRW2100033769.1.20",
          "form": "아이러니한..ㅠ",
          "original_form": "아이러니한..ㅠ",
          "speaker_id": "2",
          "time": "20210825 01:53"
        },
        {
          "id": "MDRW2100033769.1.21",
          "form": "ㅋㅋㅋㅋ 모두의 딜레마 ㅡㅏ",
          "original_form": "ㅋㅋㅋㅋ 모두의 딜레마 ㅡㅏ",
          "speaker_id": "1",
          "time": "20210825 02:00"
        },
        {
          "id": "MDRW2100033769.1.22",
          "form": "맛집은 줄이너무길죠ㅠㅠ",
          "original_form": "맛집은 줄이너무길죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 02:00"
        },
        {
          "id": "MDRW2100033769.1.23",
          "form": "그니까요..ㅠ아쉬운",
          "original_form": "그니까요..ㅠ아쉬운",
          "speaker_id": "2",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100033769.1.24",
          "form": "집에서 요리도 자주 해드시나요?",
          "original_form": "집에서 요리도 자주 해드시나요?",
          "speaker_id": "2",
          "time": "20210825 14:19"
        },
        {
          "id": "MDRW2100033769.1.25",
          "form": "ㅋㅋㅋ네네 가끔이요",
          "original_form": "ㅋㅋㅋ네네 가끔이요",
          "speaker_id": "1",
          "time": "20210825 14:29"
        },
        {
          "id": "MDRW2100033769.1.26",
          "form": "샤브샤브정도?",
          "original_form": "샤브샤브정도?",
