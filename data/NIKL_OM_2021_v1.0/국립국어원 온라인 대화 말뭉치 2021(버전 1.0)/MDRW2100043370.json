      "utterance": [
        {
          "id": "MDRW2100043370.1.1",
          "form": "요즘 날이 추워져요",
          "original_form": "요즘 날이 추워져요",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043370.1.2",
          "form": "여름진짜 금방가죠",
          "original_form": "여름진짜 금방가죠",
          "speaker_id": "2",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043370.1.3",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043370.1.4",
          "form": "그런데 모기는 나오더라구여",
          "original_form": "그런데 모기는 나오더라구여",
          "speaker_id": "2",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043370.1.5",
          "form": "맞아요 저도어제 모기때문에",
          "original_form": "맞아요 저도어제 모기때문에",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043370.1.6",
          "form": "잠못잤어요",
          "original_form": "잠못잤어요",
          "speaker_id": "1",
          "time": "20210907 18:26"
        },
        {
          "id": "MDRW2100043370.1.7",
          "form": "저는 3방 물렸어요",
          "original_form": "저는 3방 물렸어요",
          "speaker_id": "2",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043370.1.8",
          "form": "어떡해 간지럽겠다",
          "original_form": "어떡해 간지럽겠다",
          "speaker_id": "1",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043370.1.9",
          "form": "약사러가려구요",
          "original_form": "약사러가려구요",
          "speaker_id": "2",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043370.1.10",
          "form": "모기 너무 싫어요 ㅜ",
          "original_form": "모기 너무 싫어요 ㅜ",
          "speaker_id": "1",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043370.1.11",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043370.1.12",
          "form": "추운대 나오니까 더 쌔보이구",
          "original_form": "추운대 나오니까 더 쌔보이구",
          "speaker_id": "1",
          "time": "20210907 18:27"
        },
        {
          "id": "MDRW2100043370.1.13",
          "form": "더 싫죠 가을모기",
          "original_form": "더 싫죠 가을모기",
          "speaker_id": "2",
          "time": "20210907 18:28"
        },
        {
