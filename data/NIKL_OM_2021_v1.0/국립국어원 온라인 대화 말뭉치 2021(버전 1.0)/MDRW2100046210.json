          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046210.1.4",
          "form": "롤링페이퍼처럼 쓰면 되지 않을까요?",
          "original_form": "롤링페이퍼처럼 쓰면 되지 않을까요?",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046210.1.5",
          "form": "맞아요ㅠㅠ",
          "original_form": "맞아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046210.1.6",
          "form": "근디 교수님 기분이 좋아야할텐데ㅠㅠ",
          "original_form": "근디 교수님 기분이 좋아야할텐데ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046210.1.7",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100046210.1.8",
          "form": "괜히 무안하게",
          "original_form": "괜히 무안하게",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046210.1.9",
          "form": "반응하면 안되는데 ㅠㅠ",
          "original_form": "반응하면 안되는데 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046210.1.10",
          "form": "맞아요 그게 걱정이에요",
          "original_form": "맞아요 그게 걱정이에요",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
