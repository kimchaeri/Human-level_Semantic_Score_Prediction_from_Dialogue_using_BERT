      "utterance": [
        {
          "id": "MDRW2100036974.1.1",
          "form": "오늘 점심 어떤거드셨어요",
          "original_form": "오늘 점심 어떤거드셨어요",
          "speaker_id": "1",
          "time": "20210831 16:04"
        },
        {
          "id": "MDRW2100036974.1.2",
          "form": "월말이여서 오늘까지써야 하는 쿠폰때문에",
          "original_form": "월말이여서 오늘까지써야 하는 쿠폰때문에",
          "speaker_id": "2",
          "time": "20210831 16:05"
        },
        {
          "id": "MDRW2100036974.1.3",
          "form": "롯데리아요",
          "original_form": "롯데리아요",
          "speaker_id": "2",
          "time": "20210831 16:06"
        },
        {
          "id": "MDRW2100036974.1.4",
          "form": "롯데리아 뭐드셨어요?",
          "original_form": "롯데리아 뭐드셨어요?",
          "speaker_id": "1",
          "time": "20210831 16:06"
        },
        {
          "id": "MDRW2100036974.1.5",
          "form": "핫크리스피버거 세투요",
          "original_form": "핫크리스피버거 세투요",
          "speaker_id": "2",
          "time": "20210831 16:21"
        },
        {
          "id": "MDRW2100036974.1.6",
          "form": "name1님은 맛점하셨나요?",
          "original_form": "&name1&님은 맛점하셨나요?",
          "speaker_id": "2",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100036974.1.7",
          "form": "핫크리스피맛있죠ㅜㅜ원래롯리가장좋아하는 버거신가요",
          "original_form": "핫크리스피맛있죠ㅜㅜ원래롯리가장좋아하는 버거신가요",
          "speaker_id": "1",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100036974.1.8",
          "form": "네 전 짜장먹었어요",
          "original_form": "네 전 짜장먹었어요",
          "speaker_id": "1",
          "time": "20210831 16:22"
        },
        {
          "id": "MDRW2100036974.1.9",
          "form": "버거킹을 진짜 좋아하지만 가까이에 없어서",
          "original_form": "버거킹을 진짜 좋아하지만 가까이에 없어서",
          "speaker_id": "2",
          "time": "20210831 16:23"
        },
        {
          "id": "MDRW2100036974.1.10",
          "form": "아 저도 햄버거는 다사랑이져",
          "original_form": "아 저도 햄버거는 다사랑이져",
          "speaker_id": "1",
          "time": "20210831 16:23"
        },
        {
          "id": "MDRW2100036974.1.11",
          "form": "며칠전부터 간짜 땡겼는데.",
          "original_form": "며칠전부터 간짜 땡겼는데.",
          "speaker_id": "2",
          "time": "20210831 16:23"
        },
        {
          "id": "MDRW2100036974.1.12",
          "form": "낼모래는 간짜 먹어야겠어요",
          "original_form": "낼모래는 간짜 먹어야겠어요",
          "speaker_id": "2",
          "time": "20210831 16:24"
        },
        {
          "id": "MDRW2100036974.1.13",
          "form": "오늘쿠폰써야해서 롯리를가셨군여",
          "original_form": "오늘쿠폰써야해서 롯리를가셨군여",
          "speaker_id": "1",
          "time": "20210831 16:24"
        },
        {
          "id": "MDRW2100036974.1.14",
          "form": "버거킹 최애메뉴뭐에요",
          "original_form": "버거킹 최애메뉴뭐에요",
          "speaker_id": "1",
          "time": "20210831 16:24"
        },
        {
          "id": "MDRW2100036974.1.15",
          "form": "쉬림프 와퍼요",
          "original_form": "쉬림프 와퍼요",
          "speaker_id": "2",
          "time": "20210831 16:47"
        },
        {
          "id": "MDRW2100036974.1.16",
          "form": "오 저도 쉬림프들어간거 좋아해요",
          "original_form": "오 저도 쉬림프들어간거 좋아해요",
          "speaker_id": "1",
          "time": "20210831 16:47"
        },
        {
          "id": "MDRW2100036974.1.17",
          "form": "몬스타버건가 그거",
          "original_form": "몬스타버건가 그거",
          "speaker_id": "1",
          "time": "20210831 16:47"
        },
        {
          "id": "MDRW2100036974.1.18",
          "form": "이번에 버거킹에서 청양이 들어간 너겟나온다던데",
          "original_form": "이번에 버거킹에서 청양이 들어간 너겟나온다던데",
          "speaker_id": "2",
          "time": "20210831 16:54"
        },
        {
          "id": "MDRW2100036974.1.19",
          "form": "오 뭔가시도하나보네요",
          "original_form": "오 뭔가시도하나보네요",
