      "utterance": [
        {
          "id": "MDRW2100014678.1.1",
          "form": "생물 버리고 어떤 일해?",
          "original_form": "생물 버리고 어떤 일해?",
          "speaker_id": "1",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014678.1.2",
          "form": "그냥 회사원이지 뭐",
          "original_form": "그냥 회사원이지 뭐",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014678.1.3",
          "form": "넌 무슨일해?",
          "original_form": "넌 무슨일해?",
          "speaker_id": "2",
          "time": "20210825 20:15"
        },
        {
          "id": "MDRW2100014678.1.4",
          "form": "난 그냥 일용잡부야...",
          "original_form": "난 그냥 일용잡부야...",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014678.1.5",
          "form": "ㅋㅋㅋ 회사는 재밌어?",
          "original_form": "ㅋㅋㅋ 회사는 재밌어?",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014678.1.6",
          "form": "밥은 잘 나오고?",
          "original_form": "밥은 잘 나오고?",
          "speaker_id": "1",
          "time": "20210825 20:16"
        },
        {
          "id": "MDRW2100014678.1.7",
          "form": "일용잡부는 뭐야ㅋㅋㅋㅋㅋ 건설노동자 같은거야?",
          "original_form": "일용잡부는 뭐야ㅋㅋㅋㅋㅋ 건설노동자 같은거야?",
          "speaker_id": "2",
          "time": "20210825 20:17"
        },
        {
          "id": "MDRW2100014678.1.8",
          "form": "ㅋㅋㅋ 지금 백수야..",
          "original_form": "ㅋㅋㅋ 지금 백수야..",
          "speaker_id": "1",
          "time": "20210825 20:18"
        },
        {
          "id": "MDRW2100014678.1.9",
          "form": "재취직 어렵네... 큰맘먹고 막일 나갔는데\r\n얼굴이 너무 곱다고 집에가래..",
          "original_form": "재취직 어렵네... 큰맘먹고 막일 나갔는데\r\n얼굴이 너무 곱다고 집에가래..",
          "speaker_id": "1",
          "time": "20210825 20:19"
        },
        {
          "id": "MDRW2100014678.1.10",
          "form": "너무 도련님스타일이라서 일 잘 못할거같아보였나",
          "original_form": "너무 도련님스타일이라서 일 잘 못할거같아보였나",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014678.1.11",
          "form": "나는 그냥 뭐",
          "original_form": "나는 그냥 뭐",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014678.1.12",
          "form": "돈주니까 다녀",
          "original_form": "돈주니까 다녀",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014678.1.13",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 20:20"
        },
        {
          "id": "MDRW2100014678.1.14",
          "form": "ㅋㅋㅋ 다 그렇지.. \r\n자아실현은 취미로 하는거지..\r\n일은 그저 돈인거지..",
          "original_form": "ㅋㅋㅋ 다 그렇지.. \r\n자아실현은 취미로 하는거지..\r\n일은 그저 돈인거지..",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014678.1.15",
          "form": "난 진짜 아무데느 취직 할라고 이력서도 위조함.. ㅋㅋ",
          "original_form": "난 진짜 아무데느 취직 할라고 이력서도 위조함.. ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014678.1.16",
          "form": "너무 고스펙이라 막일은 안써주더라.. \r\n막이래..",
          "original_form": "너무 고스펙이라 막일은 안써주더라.. \r\n막이래..",
          "speaker_id": "1",
          "time": "20210825 20:21"
        },
        {
          "id": "MDRW2100014678.1.17",
          "form": "재수엄겟다.. 적당히 해야지.. ㅎㅎ",
          "original_form": "재수엄겟다.. 적당히 해야지.. ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014678.1.18",
          "form": "퇴사하고싶은데 재취업힘들거같아서 그냥 참음..ㅎㅎ",
          "original_form": "퇴사하고싶은데 재취업힘들거같아서 그냥 참음..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014678.1.19",
          "form": "요즘 진짜 안힘든사람이 없는듯ㅠㅠ",
          "original_form": "요즘 진짜 안힘든사람이 없는듯ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 20:23"
        },
        {
          "id": "MDRW2100014678.1.20",
          "form": "맞어.. 참어..ㅋㅋㅋㅋ \r\n지금 회사 오래다님?",
          "original_form": "맞어.. 참어..ㅋㅋㅋㅋ \r\n지금 회사 오래다님?",
          "speaker_id": "1",
          "time": "20210825 20:25"
        },
        {
          "id": "MDRW2100014678.1.21",
          "form": "아니 한번 이직하고 지금 회사는 이제 딱 2년됐네",
          "original_form": "아니 한번 이직하고 지금 회사는 이제 딱 2년됐네",
          "speaker_id": "2",
          "time": "20210825 20:26"
        },
        {
          "id": "MDRW2100014678.1.22",
          "form": "오.. 이직한번했구나.. 능력자네..",
          "original_form": "오.. 이직한번했구나.. 능력자네..",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014678.1.23",
          "form": "연봉 올리면서 이직하는사람이 진짜 능력자",
          "original_form": "연봉 올리면서 이직하는사람이 진짜 능력자",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014678.1.24",
          "form": "나는 조건만 좀 좋아지고 연봉은 비슷ㅠㅠㅠ",
          "original_form": "나는 조건만 좀 좋아지고 연봉은 비슷ㅠㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 20:31"
        },
        {
          "id": "MDRW2100014678.1.25",
          "form": "그래도 돈주니까 열심히 다니는걸루...",
          "original_form": "그래도 돈주니까 열심히 다니는걸루...",
          "speaker_id": "2",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014678.1.26",
          "form": "월급쟁이 다 필요엄다..",
          "original_form": "월급쟁이 다 필요엄다..",
          "speaker_id": "1",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014678.1.27",
          "form": "그냥 집에서 가까운게 최고야.. ㅋㅋㅋ",
          "original_form": "그냥 집에서 가까운게 최고야.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 20:32"
        },
        {
          "id": "MDRW2100014678.1.28",
          "form": "맞아ㅋㅋㅋ 초엘리트 아니고서는 사람사는거 다 비슷하고",
          "original_form": "맞아ㅋㅋㅋ 초엘리트 아니고서는 사람사는거 다 비슷하고",
          "speaker_id": "2",
