      "utterance": [
        {
          "id": "MDRW2100019340.1.1",
          "form": "오 웨이팅",
          "original_form": "오 웨이팅",
          "speaker_id": "2",
          "time": "20210827 17:49"
        },
        {
          "id": "MDRW2100019340.1.2",
          "form": "어디있어??",
          "original_form": "어디있어??",
          "speaker_id": "2",
          "time": "20210827 17:49"
        },
        {
          "id": "MDRW2100019340.1.3",
          "form": "라디오회관에서 더 올라가면있어",
          "original_form": "라디오회관에서 더 올라가면있어",
          "speaker_id": "1",
          "time": "20210827 17:50"
        },
        {
          "id": "MDRW2100019340.1.4",
          "form": "거기도 모츠나베가 있는데",
          "original_form": "거기도 모츠나베가 있는데",
          "speaker_id": "1",
          "time": "20210827 17:50"
        },
        {
          "id": "MDRW2100019340.1.5",
          "form": "안타깝게도 4명이서 갔을때",
          "original_form": "안타깝게도 4명이서 갔을때",
          "speaker_id": "1",
          "time": "20210827 17:50"
        },
        {
          "id": "MDRW2100019340.1.6",
          "form": "3명이 토리도노야가 더 맛있다고 평가했어",
          "original_form": "3명이 토리도노야가 더 맛있다고 평가했어",
          "speaker_id": "1",
          "time": "20210827 17:50"
        },
        {
          "id": "MDRW2100019340.1.7",
          "form": "라디오회관을 뽑은 1명은 나야",
          "original_form": "라디오회관을 뽑은 1명은 나야",
          "speaker_id": "1",
          "time": "20210827 17:51"
        },
        {
          "id": "MDRW2100019340.1.8",
          "form": "",
          "original_form": "",
          "speaker_id": "1",
          "time": "20210827 17:51"
        },
        {
          "id": "MDRW2100019340.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:51"
        },
        {
          "id": "MDRW2100019340.1.10",
          "form": "라디오회관 찐팬이네",
          "original_form": "라디오회관 찐팬이네",
          "speaker_id": "2",
          "time": "20210827 17:51"
        },
        {
          "id": "MDRW2100019340.1.11",
          "form": "라디오회관이랑",
          "original_form": "라디오회관이랑",
          "speaker_id": "2",
          "time": "20210827 17:51"
        },
        {
          "id": "MDRW2100019340.1.12",
          "form": "비슷한 컨셉이야?",
          "original_form": "비슷한 컨셉이야?",
          "speaker_id": "2",
          "time": "20210827 17:51"
        },
        {
          "id": "MDRW2100019340.1.13",
          "form": "음식들이?",
          "original_form": "음식들이?",
          "speaker_id": "2",
          "time": "20210827 17:52"
        },
        {
          "id": "MDRW2100019340.1.14",
          "form": "거기도 일식인데",
          "original_form": "거기도 일식인데",
          "speaker_id": "1",
          "time": "20210827 17:54"
        },
        {
          "id": "MDRW2100019340.1.15",
          "form": "라디오회관보다 일본 현지음식느낌",
          "original_form": "라디오회관보다 일본 현지음식느낌",
          "speaker_id": "1",
          "time": "20210827 17:55"
        },
        {
          "id": "MDRW2100019340.1.16",
          "form": "주방장님도 일본분이심",
          "original_form": "주방장님도 일본분이심",
          "speaker_id": "1",
          "time": "20210827 17:55"
        },
        {
          "id": "MDRW2100019340.1.17",
          "form": "오?",
          "original_form": "오?",
          "speaker_id": "2",
          "time": "20210827 17:55"
        },
        {
          "id": "MDRW2100019340.1.18",
          "form": "색다르네",
          "original_form": "색다르네",
          "speaker_id": "2",
          "time": "20210827 17:55"
        },
        {
          "id": "MDRW2100019340.1.19",
          "form": "꼭한번 가보자",
          "original_form": "꼭한번 가보자",
          "speaker_id": "2",
          "time": "20210827 17:55"
        },
        {
          "id": "MDRW2100019340.1.20",
          "form": "ㅇㅇ 인테리어도 좀더 일본느낌이고",
          "original_form": "ㅇㅇ 인테리어도 좀더 일본느낌이고",
          "speaker_id": "1",
          "time": "20210827 17:55"
        },
        {
          "id": "MDRW2100019340.1.21",
          "form": "거긴 사케도있어",
          "original_form": "거긴 사케도있어",
          "speaker_id": "1",
          "time": "20210827 17:55"
        },
        {
          "id": "MDRW2100019340.1.22",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:56"
        },
        {
          "id": "MDRW2100019340.1.23",
          "form": "라디오회관은 약간",
          "original_form": "라디오회관은 약간",
          "speaker_id": "2",
          "time": "20210827 17:56"
        },
        {
          "id": "MDRW2100019340.1.24",
          "form": "술을 파는 집인데",
          "original_form": "술을 파는 집인데",
          "speaker_id": "2",
          "time": "20210827 17:56"
        },
        {
          "id": "MDRW2100019340.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 17:56"
        },
        {
          "id": "MDRW2100019340.1.26",
          "form": "거긴어때",
          "original_form": "거긴어때",
          "speaker_id": "2",
          "time": "20210827 17:56"
        },
        {
          "id": "MDRW2100019340.1.27",
          "form": "거기도 주점이야",
          "original_form": "거기도 주점이야",
          "speaker_id": "1",
          "time": "20210827 18:01"
        },
        {
          "id": "MDRW2100019340.1.28",
          "form": "주점은 가능하네",
          "original_form": "주점은 가능하네",
          "speaker_id": "1",
          "time": "20210827 18:01"
        },
        {
          "id": "MDRW2100019340.1.29",
          "form": "오호",
          "original_form": "오호",
          "speaker_id": "2",
          "time": "20210828 09:01"
        },
        {
          "id": "MDRW2100019340.1.30",
          "form": "거기는 무슨 메뉴가",
          "original_form": "거기는 무슨 메뉴가",
          "speaker_id": "2",
          "time": "20210828 09:01"
        },
        {
          "id": "MDRW2100019340.1.31",
          "form": "주력이야?",
          "original_form": "주력이야?",
          "speaker_id": "2",
          "time": "20210828 09:01"
        },
        {
          "id": "MDRW2100019340.1.32",
          "form": "여기는 모츠나베가 주력이고",
          "original_form": "여기는 모츠나베가 주력이고",
          "speaker_id": "1",
          "time": "20210828 13:30"
        },
        {
          "id": "MDRW2100019340.1.33",
          "form": "안먹어본 메뉴가 많아",
          "original_form": "안먹어본 메뉴가 많아",
          "speaker_id": "1",
          "time": "20210828 13:30"
        },
        {
          "id": "MDRW2100019340.1.34",
          "form": "더 일본음식에 가까운 느낌이야",
          "original_form": "더 일본음식에 가까운 느낌이야",
          "speaker_id": "1",
          "time": "20210828 13:30"
        },
        {
          "id": "MDRW2100019340.1.35",
          "form": "오옷",
          "original_form": "오옷",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100019340.1.36",
          "form": "여기도 여러번 가서",
          "original_form": "여기도 여러번 가서",
          "speaker_id": "2",
          "time": "20210828 14:29"
        },
        {
          "id": "MDRW2100019340.1.37",
          "form": "다양하게 먹어봐야겠네",
          "original_form": "다양하게 먹어봐야겠네",
          "speaker_id": "2",
          "time": "20210828 14:30"
        },
        {
          "id": "MDRW2100019340.1.38",
          "form": "그동안",
          "original_form": "그동안",
          "speaker_id": "2",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100019340.1.39",
          "form": "정통 일본음식은 별로",
          "original_form": "정통 일본음식은 별로",
          "speaker_id": "2",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100019340.1.40",
          "form": "안먹어본거같아",
          "original_form": "안먹어본거같아",
          "speaker_id": "2",
          "time": "20210828 14:31"
        },
        {
          "id": "MDRW2100019340.1.41",
          "form": "일본여행 안가봤나",
          "original_form": "일본여행 안가봤나",
          "speaker_id": "1",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100019340.1.42",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100019340.1.43",
          "form": "비교하려면 한번쯤은 가봐야해",
          "original_form": "비교하려면 한번쯤은 가봐야해",
          "speaker_id": "1",
          "time": "20210829 12:37"
        },
        {
          "id": "MDRW2100019340.1.44",
          "form": "그치만 나의 라디오회관 버리지말아",
          "original_form": "그치만 나의 라디오회관 버리지말아{emoji:•}{emoji:•}{emoji:•}",
          "speaker_id": "1",
          "time": "20210829 12:38"
        },
        {
          "id": "MDRW2100019340.1.45",
          "form": "마지막 일본여행이",
          "original_form": "마지막 일본여행이",
          "speaker_id": "2",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100019340.1.46",
          "form": "초등학교 5학년이었다 한다",
          "original_form": "초등학교 5학년이었다 한다",
          "speaker_id": "2",
          "time": "20210829 12:43"
        },
        {
          "id": "MDRW2100019340.1.47",
          "form": "희미해진 기억",
          "original_form": "희미해진 기억",
          "speaker_id": "2",
