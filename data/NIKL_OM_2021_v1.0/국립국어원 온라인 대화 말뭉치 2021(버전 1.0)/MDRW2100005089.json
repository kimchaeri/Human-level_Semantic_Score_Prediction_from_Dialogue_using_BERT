      "utterance": [
        {
          "id": "MDRW2100005089.1.1",
          "form": "혹시 마라탕 맛집 추천해주실 수 있나요??",
          "original_form": "혹시 마라탕 맛집 추천해주실 수 있나요??",
          "speaker_id": "1",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005089.1.2",
          "form": "라와랑 신미방 맛있게 먹었는데",
          "original_form": "라와랑 신미방 맛있게 먹었는데",
          "speaker_id": "2",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005089.1.3",
          "form": "지점이 있을지 모르겠네요",
          "original_form": "지점이 있을지 모르겠네요",
          "speaker_id": "2",
          "time": "20210821 20:30"
        },
        {
          "id": "MDRW2100005089.1.4",
          "form": "오 라와는 먹어봤는데",
          "original_form": "오 라와는 먹어봤는데",
          "speaker_id": "1",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005089.1.5",
          "form": "신미방은 처음 듣는거같아요",
          "original_form": "신미방은 처음 듣는거같아요",
          "speaker_id": "1",
          "time": "20210821 20:38"
        },
        {
          "id": "MDRW2100005089.1.6",
          "form": "가격대비 양이랑 맛이 괜찮더라고요",
          "original_form": "가격대비 양이랑 맛이 괜찮더라고요",
          "speaker_id": "2",
          "time": "20210821 20:40"
        },
        {
          "id": "MDRW2100005089.1.7",
          "form": "마라탕 말고 즐겨 드시는 음식 있으세요?",
          "original_form": "마라탕 말고 즐겨 드시는 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210821 20:48"
        },
        {
          "id": "MDRW2100005089.1.8",
          "form": "간편하게는 냉동과일에 요거트 부어먹어요!",
          "original_form": "간편하게는 냉동과일에 요거트 부어먹어요!",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005089.1.9",
          "form": "냉동 과일 괜찮나요?",
          "original_form": "냉동 과일 괜찮나요?",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005089.1.10",
          "form": "한번도 안 먹어 봐서",
          "original_form": "한번도 안 먹어 봐서",
          "speaker_id": "2",
          "time": "20210821 20:55"
        },
        {
          "id": "MDRW2100005089.1.11",
          "form": "저도  처음엔 당도도 그렇고 걱정했는데",
          "original_form": "저도  처음엔 당도도 그렇고 걱정했는데",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005089.1.12",
          "form": "생각보다 괜찮더라구요",
          "original_form": "생각보다 괜찮더라구요",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005089.1.13",
          "form": "과일도 오래 두고 먹을수있어서 좋구요!",
          "original_form": "과일도 오래 두고 먹을수있어서 좋구요!",
          "speaker_id": "1",
          "time": "20210821 20:56"
        },
        {
          "id": "MDRW2100005089.1.14",
          "form": "오래 두고 먹을 수 있는 건 좋네요",
          "original_form": "오래 두고 먹을 수 있는 건 좋네요",
          "speaker_id": "2",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005089.1.15",
          "form": "과일 변색되거나 섞으면 난감해요ㅜㅜ",
          "original_form": "과일 변색되거나 섞으면 난감해요ㅜㅜ",
          "speaker_id": "2",
          "time": "20210821 20:57"
        },
        {
          "id": "MDRW2100005089.1.16",
          "form": "맞아요ㅜ",
          "original_form": "맞아요ㅜ",
          "speaker_id": "1",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005089.1.17",
          "form": "그래서 참 과일 먹기가 힘들죠ㅜ",
          "original_form": "그래서 참 과일 먹기가 힘들죠ㅜ",
          "speaker_id": "1",
          "time": "20210821 21:04"
        },
        {
          "id": "MDRW2100005089.1.18",
          "form": "그래도 이번 여름엔 제철 과일 조금씩 사다 먹었어요",
          "original_form": "그래도 이번 여름엔 제철 과일 조금씩 사다 먹었어요",
          "speaker_id": "2",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005089.1.19",
          "form": "살구랑 천도복숭아를 제일 많이 먹은 거 같아요",
          "original_form": "살구랑 천도복숭아를 제일 많이 먹은 거 같아요",
          "speaker_id": "2",
          "time": "20210821 21:07"
        },
        {
          "id": "MDRW2100005089.1.20",
          "form": "어떤 과일 좋아하세요?",
          "original_form": "어떤 과일 좋아하세요?",
          "speaker_id": "2",
          "time": "20210821 21:08"
        },
        {
          "id": "MDRW2100005089.1.21",
          "form": "저는 제철과일에 그렇게 감흥이 없었는데",
          "original_form": "저는 제철과일에 그렇게 감흥이 없었는데",
          "speaker_id": "1",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005089.1.22",
          "form": "이번엔 체리도 먹고",
          "original_form": "이번엔 체리도 먹고",
          "speaker_id": "1",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005089.1.23",
          "form": "복숭아도 사자먹었어요",
          "original_form": "복숭아도 사자먹었어요",
          "speaker_id": "1",
          "time": "20210821 21:09"
        },
        {
          "id": "MDRW2100005089.1.24",
          "form": "체리도 맛있죠",
          "original_form": "체리도 맛있죠",
          "speaker_id": "2",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005089.1.25",
          "form": "요거트에는 어떤 과일 드세요?",
          "original_form": "요거트에는 어떤 과일 드세요?",
          "speaker_id": "2",
          "time": "20210821 21:11"
        },
        {
          "id": "MDRW2100005089.1.26",
          "form": "망고도 좋고",
          "original_form": "망고도 좋고",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005089.1.27",
          "form": "블루베리도 좋아요!",
          "original_form": "블루베리도 좋아요!",
          "speaker_id": "1",
          "time": "20210821 21:12"
        },
        {
          "id": "MDRW2100005089.1.28",
          "form": "생과로는 체리랑 바나나 요! ㅎㅎ",
          "original_form": "생과로는 체리랑 바나나 요! ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 21:13"
        },
        {
          "id": "MDRW2100005089.1.29",
          "form": "요거트랑 같이 먹어볼게요",
          "original_form": "요거트랑 같이 먹어볼게요",
          "speaker_id": "2",
          "time": "20210821 21:17"
        },
        {
          "id": "MDRW2100005089.1.30",
          "form": "감사해요!",
          "original_form": "감사해요!",
          "speaker_id": "2",
          "time": "20210821 21:17"
        }
      ]
    }
