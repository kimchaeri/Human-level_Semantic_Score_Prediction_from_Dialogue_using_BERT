        {
          "id": "MDRW2100039085.1.1",
          "form": "여행조아하세요?",
          "original_form": "여행조아하세요?",
          "speaker_id": "2",
          "time": "20210903 09:41"
        },
        {
          "id": "MDRW2100039085.1.2",
          "form": "네 좋아해요 ㅋㅋㅋ",
          "original_form": "네 좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:42"
        },
        {
          "id": "MDRW2100039085.1.3",
          "form": "어디가셧어용 올핸",
          "original_form": "어디가셧어용 올핸",
          "speaker_id": "2",
          "time": "20210903 09:42"
        },
        {
          "id": "MDRW2100039085.1.4",
          "form": "저는 4월쯤에 태백 갔다왔어요 ㅋㅋㅋ",
          "original_form": "저는 4월쯤에 태백 갔다왔어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100039085.1.5",
          "form": "name2님은 올해 어디 다녀오셨어요??",
          "original_form": "&name2&님은 올해 어디 다녀오셨어요??",
          "speaker_id": "1",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100039085.1.6",
          "form": "멋지다태백",
          "original_form": "멋지다태백",
          "speaker_id": "2",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100039085.1.7",
          "form": "산을 타신건가요 ...???",
          "original_form": "산을 타신건가요 ...???",
          "speaker_id": "2",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100039085.1.8",
          "form": "전 부안ㅇ요",
          "original_form": "전 부안ㅇ요",
          "speaker_id": "2",
          "time": "20210903 09:43"
        },
        {
          "id": "MDRW2100039085.1.9",
          "form": "산은 못탔어요ㅠㅠㅠ 동굴 들어갔다왔어요 ㅋㅋㅋ",
          "original_form": "산은 못탔어요ㅠㅠㅠ 동굴 들어갔다왔어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100039085.1.10",
          "form": "부안엔 뭐가 유명해요???",
          "original_form": "부안엔 뭐가 유명해요???",
          "speaker_id": "1",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100039085.1.11",
          "form": "아항 동굴이잇구낭",
          "original_form": "아항 동굴이잇구낭",
          "speaker_id": "2",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100039085.1.12",
          "form": "부안...채석강 ? 봣어요 ㅋㅋ 바다",
          "original_form": "부안...채석강 ? 봣어요 ㅋㅋ 바다",
          "speaker_id": "2",
          "time": "20210903 09:44"
        },
        {
          "id": "MDRW2100039085.1.13",
          "form": "태백 추천하나요",
          "original_form": "태백 추천하나요",
          "speaker_id": "2",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100039085.1.14",
          "form": "와 채석강이면 배타고 들어가신 걸까요?",
          "original_form": "와 채석강이면 배타고 들어가신 걸까요?",
          "speaker_id": "1",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100039085.1.15",
          "form": "네 태백 추천드려요 ㅋㅋㅋ 다른 곳 보다 벚꽃이 늦게 져서 4월 중순에 가서 벚꽃 실컷 보고왔어요 ㅋㅋㅋ",
          "original_form": "네 태백 추천드려요 ㅋㅋㅋ 다른 곳 보다 벚꽃이 늦게 져서 4월 중순에 가서 벚꽃 실컷 보고왔어요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:45"
        },
        {
          "id": "MDRW2100039085.1.16",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "2",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100039085.1.17",
          "form": "아뇨 그냥 걸어서봣어요 ㅋㅋㅋ",
          "original_form": "아뇨 그냥 걸어서봣어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100039085.1.18",
          "form": "오 ...조아용 또볼만한데잇나용",
          "original_form": "오 ...조아용 또볼만한데잇나용",
          "speaker_id": "2",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100039085.1.19",
          "form": "오 뭍에서 바로 보이는군요 좋네요",
          "original_form": "오 뭍에서 바로 보이는군요 좋네요",
          "speaker_id": "1",
          "time": "20210903 09:46"
        },
        {
          "id": "MDRW2100039085.1.20",
          "form": "넹넹 ㅋㅋ 멋지던데용",
          "original_form": "넹넹 ㅋㅋ 멋지던데용",
          "speaker_id": "2",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100039085.1.21",
          "form": "사실 제가 당일치기로 가서 동굴이랑 고대화석인가 있는 구문소라고 계곡같은 곳만 둘러보고왔는데요 가볍게 산책하는 거 좋아하시면 구문소 괜찮아요 ㅋㅋㅋ",
          "original_form": "사실 제가 당일치기로 가서 동굴이랑 고대화석인가 있는 구문소라고 계곡같은 곳만 둘러보고왔는데요 가볍게 산책하는 거 좋아하시면 구문소 괜찮아요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:47"
        },
        {
          "id": "MDRW2100039085.1.22",
          "form": "추천 감사용 ㅋㅋㅋ",
          "original_form": "추천 감사용 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 09:48"
        },
        {
          "id": "MDRW2100039085.1.23",
          "form": "와 지금알았는데 채석강이 조선 수군 근거지였네요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "와 지금알았는데 채석강이 조선 수군 근거지였네요 ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:48"
        },
        {
          "id": "MDRW2100039085.1.24",
          "form": "아 그래요? ㅋㅋㅋ 저도 첨알앗어요",
          "original_form": "아 그래요? ㅋㅋㅋ 저도 첨알앗어요",
          "speaker_id": "2",
          "time": "20210903 09:48"
        },
        {
          "id": "MDRW2100039085.1.25",
          "form": "되게 멋진 곳을 근거지로 뒀네요 조상님들이 ㅋㅋㅋㅋ",
          "original_form": "되게 멋진 곳을 근거지로 뒀네요 조상님들이 ㅋㅋㅋㅋ",
