      "utterance": [
        {
          "id": "MDRW2100011590.1.1",
          "form": "이준기 나온 거요?",
          "original_form": "이준기 나온 거요?",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011590.1.2",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011590.1.3",
          "form": "이준기랑 서예지",
          "original_form": "이준기랑 서예지",
          "speaker_id": "1",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011590.1.4",
          "form": "최민수도요",
          "original_form": "최민수도요",
          "speaker_id": "1",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011590.1.5",
          "form": "지나가면서만 보고 제대로 보진않았어요",
          "original_form": "지나가면서만 보고 제대로 보진않았어요",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011590.1.6",
          "form": "저도 본방할 때는 관심도 없어서",
          "original_form": "저도 본방할 때는 관심도 없어서",
          "speaker_id": "1",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011590.1.7",
          "form": "이런거 있는지도 몰랐는데",
          "original_form": "이런거 있는지도 몰랐는데",
          "speaker_id": "1",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011590.1.8",
          "form": "넷플릭스에 있더라구요",
          "original_form": "넷플릭스에 있더라구요",
          "speaker_id": "1",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011590.1.9",
          "form": "아 넷플릭스로 보시는군요",
          "original_form": "아 넷플릭스로 보시는군요",
          "speaker_id": "2",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011590.1.10",
          "form": "네 볼게 없어서 ㅜㅠ",
          "original_form": "네 볼게 없어서 ㅜㅠ",
          "speaker_id": "1",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011590.1.11",
          "form": "첨엔 재미없었는데 이제 나름 재밌어졌어요",
          "original_form": "첨엔 재미없었는데 이제 나름 재밌어졌어요",
          "speaker_id": "1",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011590.1.12",
          "form": "서예지는 어떤가요?",
          "original_form": "서예지는 어떤가요?",
          "speaker_id": "2",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011590.1.13",
          "form": "시끄럽던데",
          "original_form": "시끄럽던데",
          "speaker_id": "2",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011590.1.14",
          "form": "전 원래 서예지는 별로 제 스타일 아니었어서 ㅎㅎ",
          "original_form": "전 원래 서예지는 별로 제 스타일 아니었어서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011590.1.15",
          "form": "근데 아무래도 알고보니까",
          "original_form": "근데 아무래도 알고보니까",
          "speaker_id": "1",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011590.1.16",
          "form": "배역이랑 좀 갭이 있긴해요",
          "original_form": "배역이랑 좀 갭이 있긴해요",
          "speaker_id": "1",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011590.1.17",
          "form": "어떻게요??",
          "original_form": "어떻게요??",
          "speaker_id": "2",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011590.1.18",
          "form": "배역은 완전 정의의 사도 그런 느낌?",
          "original_form": "배역은 완전 정의의 사도 그런 느낌?",
          "speaker_id": "1",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011590.1.19",
          "form": "근데 저는 이준기는 괜찮게 생각해서",
          "original_form": "근데 저는 이준기는 괜찮게 생각해서",
          "speaker_id": "1",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011590.1.20",
          "form": "그냥저냥 틀어놓을만 한 것 같아요",
          "original_form": "그냥저냥 틀어놓을만 한 것 같아요",
          "speaker_id": "1",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011590.1.21",
          "form": "이준기 연기 잘하더라구요",
          "original_form": "이준기 연기 잘하더라구요",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011590.1.22",
          "form": "네 엄청 노력파같아요",
          "original_form": "네 엄청 노력파같아요",
          "speaker_id": "1",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011590.1.23",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011590.1.24",
          "form": "나이도 안먹는 느낌 ㅋㅋㅋ",
          "original_form": "나이도 안먹는 느낌 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 14:21"
        },
        {
          "id": "MDRW2100011590.1.25",
          "form": "부럽네요",
          "original_form": "부럽네요",
          "speaker_id": "2",
          "time": "20210824 14:21"
        }
      ]
    }
