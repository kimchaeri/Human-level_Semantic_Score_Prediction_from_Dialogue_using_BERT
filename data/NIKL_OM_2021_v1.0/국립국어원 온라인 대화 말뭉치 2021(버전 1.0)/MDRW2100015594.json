      "utterance": [
        {
          "id": "MDRW2100015594.1.1",
          "form": "최근에 본 공연이나 전시회 있어요?",
          "original_form": "최근에 본 공연이나 전시회 있어요?",
          "speaker_id": "2",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015594.1.2",
          "form": "아니요 ㅋㅋ근데 좋아해요 그런거 보세요?",
          "original_form": "아니요 ㅋㅋ근데 좋아해요 그런거 보세요?",
          "speaker_id": "1",
          "time": "20210825 23:40"
        },
        {
          "id": "MDRW2100015594.1.3",
          "form": "최근에는 거의못봤어요 ㅠ",
          "original_form": "최근에는 거의못봤어요 ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015594.1.4",
          "form": "저도 좋아해요!",
          "original_form": "저도 좋아해요!",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015594.1.5",
          "form": "예술의 전당 자주 갔고 전시관도 자주가요~!",
          "original_form": "예술의 전당 자주 갔고 전시관도 자주가요~!",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015594.1.6",
          "form": "대학로연극예전에 몇번봤어요",
          "original_form": "대학로연극예전에 몇번봤어요",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015594.1.7",
          "form": "옥탑방고양이?",
          "original_form": "옥탑방고양이?",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015594.1.8",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015594.1.9",
          "form": "전시관좋죠",
          "original_form": "전시관좋죠",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015594.1.10",
          "form": "보통 작품 봐요...ㅋㅋㅋ",
          "original_form": "보통 작품 봐요...ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015594.1.11",
          "form": "온라인으로 마술 공연 봤는데 그것도 좋았어요!",
          "original_form": "온라인으로 마술 공연 봤는데 그것도 좋았어요!",
          "speaker_id": "2",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015594.1.12",
          "form": "오 저도 미술관 가봤는덕",
          "original_form": "오 저도 미술관 가봤는덕",
          "speaker_id": "1",
          "time": "20210825 23:41"
        },
        {
          "id": "MDRW2100015594.1.13",
          "form": "막 특별하게 훅 오는감정은 크게 못느꼈어요",
          "original_form": "막 특별하게 훅 오는감정은 크게 못느꼈어요",
          "speaker_id": "1",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015594.1.14",
          "form": "오 그런것도 있나요??",
          "original_form": "오 그런것도 있나요??",
          "speaker_id": "1",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015594.1.15",
          "form": "미술은.. 공부하고 봐야 보이더라구요 ㅎㅎ",
          "original_form": "미술은.. 공부하고 봐야 보이더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015594.1.16",
          "form": "네 최현우 마술사가 했었는데 진짜 너무 좋았어요",
          "original_form": "네 최현우 마술사가 했었는데 진짜 너무 좋았어요",
          "speaker_id": "2",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015594.1.17",
          "form": "맞아요...느낌만으로 충분하지않은",
          "original_form": "맞아요...느낌만으로 충분하지않은",
          "speaker_id": "1",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015594.1.18",
          "form": "최현우는 마법사잖아요",
          "original_form": "최현우는 마법사잖아요",
          "speaker_id": "1",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015594.1.19",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210825 23:42"
        },
        {
          "id": "MDRW2100015594.1.20",
          "form": "전 애인한테 설명 많이 해줬는데.. 씁쓸하네요 ㅋㅋ",
          "original_form": "전 애인한테 설명 많이 해줬는데.. 씁쓸하네요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015594.1.21",
          "form": "호그와트출신아닌가요 최현우씨 ㅋㅋ",
          "original_form": "호그와트출신아닌가요 최현우씨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015594.1.22",
          "form": "아 ㅋㅋㅋㅋㅋ",
          "original_form": "아 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015594.1.23",
          "form": "최현우씨 진짜 소름돋게 해요 자꾸 ㅜㅜ",
          "original_form": "최현우씨 진짜 소름돋게 해요 자꾸 ㅜㅜ",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015594.1.24",
          "form": "그쵸 ㅋㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015594.1.25",
          "form": "진짜 너무 신기해요",
          "original_form": "진짜 너무 신기해요",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015594.1.26",
          "form": "저 진짜 놀라서 노트북 집어던질 뻔 했어요",
          "original_form": "저 진짜 놀라서 노트북 집어던질 뻔 했어요",
          "speaker_id": "2",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015594.1.27",
          "form": "저도 보고싶네요",
          "original_form": "저도 보고싶네요",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015594.1.28",
          "form": "하나도 트릭? 모르겠어요",
          "original_form": "하나도 트릭? 모르겠어요",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015594.1.29",
          "form": "예능 나온것만 봐도",
          "original_form": "예능 나온것만 봐도",
          "speaker_id": "1",
          "time": "20210825 23:43"
        },
        {
          "id": "MDRW2100015594.1.30",
          "form": "그것도 시청자 참여형으로 마술 공연 해주셔서 더 좋았어요",
          "original_form": "그것도 시청자 참여형으로 마술 공연 해주셔서 더 좋았어요",
          "speaker_id": "2",
          "time": "20210825 23:44"
        },
        {
          "id": "MDRW2100015594.1.31",
          "form": "온라인 마술공연은 더 저렴한가요?",
          "original_form": "온라인 마술공연은 더 저렴한가요?",
