      "utterance": [
        {
          "id": "MDRW2100010467.1.1",
          "form": "여주 정치하는 게 너무 멋있어요",
          "original_form": "여주 정치하는 게 너무 멋있어요",
          "speaker_id": "2",
          "time": "20210823 18:36"
        },
        {
          "id": "MDRW2100010467.1.2",
          "form": "꺄~",
          "original_form": "꺄~",
          "speaker_id": "2",
          "time": "20210823 18:36"
        },
        {
          "id": "MDRW2100010467.1.3",
          "form": "맞아요 맞아요!!!!",
          "original_form": "맞아요 맞아요!!!!",
          "speaker_id": "1",
          "time": "20210823 18:36"
        },
        {
          "id": "MDRW2100010467.1.4",
          "form": "그러면",
          "original_form": "그러면",
          "speaker_id": "1",
          "time": "20210823 18:36"
        },
        {
          "id": "MDRW2100010467.1.5",
          "form": "악역의 엔딩은 죽음뿐도 혹시 보셨어요?!",
          "original_form": "악역의 엔딩은 죽음뿐도 혹시 보셨어요?!",
          "speaker_id": "1",
          "time": "20210823 18:36"
        },
        {
          "id": "MDRW2100010467.1.6",
          "form": "똑똑한 여캐들이 좋아요ㅎㅎ",
          "original_form": "똑똑한 여캐들이 좋아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 18:36"
        },
        {
          "id": "MDRW2100010467.1.7",
          "form": "아 정말 명작!",
          "original_form": "아 정말 명작!",
          "speaker_id": "2",
          "time": "20210823 18:36"
        },
        {
          "id": "MDRW2100010467.1.8",
          "form": "꺄어어아악",
          "original_form": "꺄어어아악",
          "speaker_id": "1",
          "time": "20210823 18:36"
        },
        {
          "id": "MDRW2100010467.1.9",
          "form": "녹음의 관도 보시나요?!!!",
          "original_form": "녹음의 관도 보시나요?!!!",
          "speaker_id": "1",
          "time": "20210823 18:37"
        },
        {
          "id": "MDRW2100010467.1.10",
          "form": "마지막에 제목 상기될 땨 완전 전율",
          "original_form": "마지막에 제목 상기될 땨 완전 전율",
          "speaker_id": "2",
          "time": "20210823 18:37"
        },
        {
          "id": "MDRW2100010467.1.11",
          "form": "그것도 여주 똑똑하던데",
          "original_form": "그것도 여주 똑똑하던데",
          "speaker_id": "1",
          "time": "20210823 18:37"
        },
        {
          "id": "MDRW2100010467.1.12",
          "form": "녹음의관 완결까지는 안 봤고 중간에 하차했던 거 같아요",
          "original_form": "녹음의관 완결까지는 안 봤고 중간에 하차했던 거 같아요",
          "speaker_id": "2",
          "time": "20210823 18:37"
        },
        {
          "id": "MDRW2100010467.1.13",
          "form": "악엔죽 진짜 똑똑해요",
          "original_form": "악엔죽 진짜 똑똑해요",
          "speaker_id": "1",
          "time": "20210823 18:37"
        },
        {
          "id": "MDRW2100010467.1.14",
          "form": "연재 중일 때 봐서 그랬나",
          "original_form": "연재 중일 때 봐서 그랬나",
          "speaker_id": "2",
          "time": "20210823 18:37"
        },
        {
          "id": "MDRW2100010467.1.15",
          "form": "녹음의 관 웹툰이 잘 나왔더라고요",
          "original_form": "녹음의 관 웹툰이 잘 나왔더라고요",
          "speaker_id": "1",
          "time": "20210823 18:37"
        },
        {
          "id": "MDRW2100010467.1.16",
          "form": "악엔죽은 소설이 워낙 잘써져있어서",
          "original_form": "악엔죽은 소설이 워낙 잘써져있어서",
          "speaker_id": "1",
          "time": "20210823 18:38"
        },
        {
          "id": "MDRW2100010467.1.17",
          "form": "나중에 보게 되면 웹툰으로 달려야 겠어요",
          "original_form": "나중에 보게 되면 웹툰으로 달려야 겠어요",
          "speaker_id": "2",
          "time": "20210823 18:38"
        },
        {
          "id": "MDRW2100010467.1.18",
          "form": "웹툰이 못만든게 아닌데 소설이 더 좋아요",
          "original_form": "웹툰이 못만든게 아닌데 소설이 더 좋아요",
          "speaker_id": "1",
          "time": "20210823 18:38"
        },
        {
          "id": "MDRW2100010467.1.19",
          "form": "혹시 이번생은 가주가 되겠습니다도 보셨나요?!",
          "original_form": "혹시 이번생은 가주가 되겠습니다도 보셨나요?!",
          "speaker_id": "1",
          "time": "20210823 18:38"
        },
        {
          "id": "MDRW2100010467.1.20",
          "form": "그건 안 봤던 거 같아요",
          "original_form": "그건 안 봤던 거 같아요",
          "speaker_id": "2",
          "time": "20210823 18:39"
        }
      ]
