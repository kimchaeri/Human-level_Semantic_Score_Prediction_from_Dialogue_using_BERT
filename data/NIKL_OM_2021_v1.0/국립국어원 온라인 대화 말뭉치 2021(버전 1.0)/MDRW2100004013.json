      "utterance": [
        {
          "id": "MDRW2100004013.1.1",
          "form": "점심 먹었어 ?",
          "original_form": "점심 먹었어 ?",
          "speaker_id": "1",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100004013.1.2",
          "form": "아니 아직",
          "original_form": "아니 아직",
          "speaker_id": "2",
          "time": "20210821 14:32"
        },
        {
          "id": "MDRW2100004013.1.3",
          "form": "음~ 먹을 계획은 있어?",
          "original_form": "음~ 먹을 계획은 있어?",
          "speaker_id": "1",
          "time": "20210821 14:33"
        },
        {
          "id": "MDRW2100004013.1.4",
          "form": "식사대신 음료 마시려고",
          "original_form": "식사대신 음료 마시려고",
          "speaker_id": "2",
          "time": "20210821 14:34"
        },
        {
          "id": "MDRW2100004013.1.5",
          "form": "좋아하는 차나 커피 있어?",
          "original_form": "좋아하는 차나 커피 있어?",
          "speaker_id": "2",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100004013.1.6",
          "form": "아하~ 어떤거?",
          "original_form": "아하~ 어떤거?",
          "speaker_id": "1",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100004013.1.7",
          "form": "난 자스민 좋아해 ㅎㅎ 커피도 좋아하고",
          "original_form": "난 자스민 좋아해 ㅎㅎ 커피도 좋아하고",
          "speaker_id": "1",
          "time": "20210821 14:35"
        },
        {
          "id": "MDRW2100004013.1.8",
          "form": "녹차라떼 식사대신 마시려고~",
          "original_form": "녹차라떼 식사대신 마시려고~",
          "speaker_id": "2",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100004013.1.9",
          "form": "녹차라떼 좋아하는구나",
          "original_form": "녹차라떼 좋아하는구나",
          "speaker_id": "1",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100004013.1.10",
          "form": "나도 자스민차 좋아해!",
          "original_form": "나도 자스민차 좋아해!",
          "speaker_id": "2",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100004013.1.11",
          "form": "커피는 잠이 안 와서 가끔 마셔ㅎㅎ",
          "original_form": "커피는 잠이 안 와서 가끔 마셔ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 14:36"
        },
        {
          "id": "MDRW2100004013.1.12",
          "form": "아하 ㅎㅎ",
          "original_form": "아하 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004013.1.13",
          "form": "그럼 지금 카페?!",
          "original_form": "그럼 지금 카페?!",
          "speaker_id": "1",
          "time": "20210821 14:38"
        },
        {
          "id": "MDRW2100004013.1.14",
          "form": "응~카페 테이크아웃 하려고!",
          "original_form": "응~카페 테이크아웃 하려고!",
          "speaker_id": "2",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100004013.1.15",
          "form": "거기도 비 와?",
          "original_form": "거기도 비 와?",
          "speaker_id": "1",
          "time": "20210821 14:39"
        },
        {
          "id": "MDRW2100004013.1.16",
          "form": "여기는 비가 그쳤어! ",
          "original_form": "여기는 비가 그쳤어! ",
          "speaker_id": "2",
          "time": "20210821 14:40"
        }
      ]
