        {
          "id": "MDRW2100022442.1.1",
          "form": "백신맞으셨나요?",
          "original_form": "백신맞으셨나요?",
          "speaker_id": "1",
          "time": "20210829 14:07"
        },
        {
          "id": "MDRW2100022442.1.2",
          "form": "아직이요 맞으셨나요?",
          "original_form": "아직이요 맞으셨나요?",
          "speaker_id": "2",
          "time": "20210829 14:10"
        },
        {
          "id": "MDRW2100022442.1.3",
          "form": "사전예약 했습니다",
          "original_form": "사전예약 했습니다",
          "speaker_id": "1",
          "time": "20210829 14:14"
        },
        {
          "id": "MDRW2100022442.1.4",
          "form": "아항 그러셨군요",
          "original_form": "아항 그러셨군요",
          "speaker_id": "2",
          "time": "20210829 14:17"
        },
        {
          "id": "MDRW2100022442.1.5",
          "form": "예약 하셨나요?",
          "original_form": "예약 하셨나요?",
          "speaker_id": "1",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022442.1.6",
          "form": "넵",
          "original_form": "넵",
          "speaker_id": "2",
          "time": "20210829 14:20"
        },
        {
          "id": "MDRW2100022442.1.7",
          "form": "다음달이요!",
          "original_form": "다음달이요!",
          "speaker_id": "2",
          "time": "20210829 14:21"
        },
        {
          "id": "MDRW2100022442.1.8",
          "form": "백신 안정해졌죠ㅜ",
          "original_form": "백신 안정해졌죠ㅜ",
          "speaker_id": "1",
          "time": "20210829 14:22"
        },
        {
          "id": "MDRW2100022442.1.9",
          "form": "네 그래도 불안하긴하네요ㅠ",
          "original_form": "네 그래도 불안하긴하네요ㅠ",
          "speaker_id": "2",
          "time": "20210829 14:25"
        },
        {
          "id": "MDRW2100022442.1.10",
          "form": "맞아요 부작용ㅠㅠ",
          "original_form": "맞아요 부작용ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 14:29"
        },
        {
          "id": "MDRW2100022442.1.11",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210829 14:34"
        },
        {
          "id": "MDRW2100022442.1.12",
          "form": "사람 죽기도하고ㅠㅠ",
          "original_form": "사람 죽기도하고ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 14:35"
        },
        {
          "id": "MDRW2100022442.1.13",
          "form": "심리가 내가 아닐거라는 보장은 없지않아? 그래도.. 괜찮으려나 이게 반복되는거 같아요",
          "original_form": "심리가 내가 아닐거라는 보장은 없지않아? 그래도.. 괜찮으려나 이게 반복되는거 같아요",
          "speaker_id": "2",
          "time": "20210829 14:35"
        },
        {
          "id": "MDRW2100022442.1.14",
          "form": "맞아요ㅠ",
          "original_form": "맞아요ㅠ",
          "speaker_id": "2",
          "time": "20210829 14:35"
        },
        {
          "id": "MDRW2100022442.1.15",
