      "utterance": [
        {
          "id": "MDRW2100045408.1.1",
          "form": "아이유 좋아하세요?",
          "original_form": "아이유 좋아하세요?",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045408.1.2",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045408.1.3",
          "form": "그럼 이번 신곡 들으셨어요?",
          "original_form": "그럼 이번 신곡 들으셨어요?",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045408.1.4",
          "form": "곡이 새로 나왔나요?",
          "original_form": "곡이 새로 나왔나요?",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045408.1.5",
          "form": "라일락 안들어보셨어요?",
          "original_form": "라일락 안들어보셨어요?",
          "speaker_id": "1",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045408.1.6",
          "form": "아 그건 들었어요",
          "original_form": "아 그건 들었어요",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045408.1.7",
          "form": "또 새로 나온 줄 알았어요",
          "original_form": "또 새로 나온 줄 알았어요",
          "speaker_id": "2",
          "time": "20210908 02:21"
        },
        {
          "id": "MDRW2100045408.1.8",
          "form": "아 ㅋㅋㅋㅋㅋ 그건 아니예요",
          "original_form": "아 ㅋㅋㅋㅋㅋ 그건 아니예요",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045408.1.9",
          "form": "신곡치고 좀 오래되긴했죠",
          "original_form": "신곡치고 좀 오래되긴했죠",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045408.1.10",
          "form": "라일락 노래 좋던데요",
          "original_form": "라일락 노래 좋던데요",
          "speaker_id": "2",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045408.1.11",
          "form": "그쵸?? 그거랑 이번 앨범 노래들 다 좋아요",
          "original_form": "그쵸?? 그거랑 이번 앨범 노래들 다 좋아요",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045408.1.12",
          "form": "아 그 코인인가 그것도 좋았어요",
          "original_form": "아 그 코인인가 그것도 좋았어요",
          "speaker_id": "2",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045408.1.13",
          "form": "코인... 뮤비가 복지죠",
          "original_form": "코인... 뮤비가 복지죠",
          "speaker_id": "1",
          "time": "20210908 02:22"
        },
        {
          "id": "MDRW2100045408.1.14",
          "form": "아 뮤지는 아직 안 봤어요",
          "original_form": "아 뮤지는 아직 안 봤어요",
          "speaker_id": "2",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045408.1.15",
          "form": "헐 왜요? 뮤비한번 봐주세요 ㅠㅠ",
          "original_form": "헐 왜요? 뮤비한번 봐주세요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045408.1.16",
          "form": "음악만 듣다보니까 뮤비를 못 봤네요",
          "original_form": "음악만 듣다보니까 뮤비를 못 봤네요",
          "speaker_id": "2",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045408.1.17",
          "form": "한번 보긴할게요",
          "original_form": "한번 보긴할게요",
          "speaker_id": "2",
          "time": "20210908 02:23"
        },
        {
          "id": "MDRW2100045408.1.18",
          "form": "꼭입니다 ㅠㅠ 진짜 아이유님 완전 매력터져요 ㅠㅠ",
          "original_form": "꼭입니다 ㅠㅠ 진짜 아이유님 완전 매력터져요 ㅠㅠ",
