      "utterance": [
        {
          "id": "MDRW2100036491.1.1",
          "form": "너 러그 샀어?",
          "original_form": "너 러그 샀어?",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036491.1.2",
          "form": "아 아직 고민중이구나",
          "original_form": "아 아직 고민중이구나",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036491.1.3",
          "form": "너는 추천하는편이니?",
          "original_form": "너는 추천하는편이니?",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036491.1.4",
          "form": "마음에 드는 디자인이라도 있어?",
          "original_form": "마음에 드는 디자인이라도 있어?",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036491.1.5",
          "form": "음 살다가 하나씩 장만하는골 추천해",
          "original_form": "음 살다가 하나씩 장만하는골 추천해",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036491.1.6",
          "form": "음 아니 사실 뭔가 쏙 맘에드는건 엊ㅅ어",
          "original_form": "음 아니 사실 뭔가 쏙 맘에드는건 엊ㅅ어",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036491.1.7",
          "form": "나는 러그에 좀 늦게 꽂혔어",
          "original_form": "나는 러그에 좀 늦게 꽂혔어",
          "speaker_id": "2",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036491.1.8",
          "form": "그렇지? 필요하면 내가 사게될꺼야",
          "original_form": "그렇지? 필요하면 내가 사게될꺼야",
          "speaker_id": "1",
          "time": "20210830 23:45"
        },
        {
          "id": "MDRW2100036491.1.9",
          "form": "아 그럼 일단 굳이 찾지머",
          "original_form": "아 그럼 일단 굳이 찾지머",
          "speaker_id": "2",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036491.1.10",
          "form": "마",
          "original_form": "마",
          "speaker_id": "2",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036491.1.11",
          "form": "말고 나는 밥을 얼릴 용기를 사야해",
          "original_form": "말고 나는 밥을 얼릴 용기를 사야해",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036491.1.12",
          "form": "자립심이 있고 멋있두나",
          "original_form": "자립심이 있고 멋있두나",
          "speaker_id": "2",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036491.1.13",
          "form": "아 그런 꼭 필요한 것부터 사야지",
          "original_form": "아 그런 꼭 필요한 것부터 사야지",
          "speaker_id": "2",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036491.1.14",
          "form": "봐 둔 용기는 있니?",
          "original_form": "봐 둔 용기는 있니?",
          "speaker_id": "2",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036491.1.15",
          "form": "그치? 근데 구매하려니까 돈아깝더라",
          "original_form": "그치? 근데 구매하려니까 돈아깝더라",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036491.1.16",
          "form": "응 육천구백원하는데",
          "original_form": "응 육천구백원하는데",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036491.1.17",
          "form": "아직 밥을 안해먹은거야?",
          "original_form": "아직 밥을 안해먹은거야?",
          "speaker_id": "2",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036491.1.18",
          "form": "12개를 팔길래 찜해뒀어",
          "original_form": "12개를 팔길래 찜해뒀어",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036491.1.19",
          "form": "응 아직 매일 사먹고있어",
          "original_form": "응 아직 매일 사먹고있어",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036491.1.20",
          "form": "12개 많은데 반찬도 담아두묜 되겠다",
          "original_form": "12개 많은데 반찬도 담아두묜 되겠다",
          "speaker_id": "2",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036491.1.21",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "2",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036491.1.22",
          "form": "부자구나",
          "original_form": "부자구나",
          "speaker_id": "2",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036491.1.23",
          "form": "그치? 나도 그렇게 생각해",
          "original_form": "그치? 나도 그렇게 생각해",
          "speaker_id": "1",
          "time": "20210830 23:46"
        },
        {
          "id": "MDRW2100036491.1.24",
          "form": "나 최근에 아이허브에서 뭘 샀어",
          "original_form": "나 최근에 아이허브에서 뭘 샀어",
          "speaker_id": "2",
          "time": "20210830 23:50"
        }
      ]
