      "utterance": [
        {
          "id": "MDRW2100046872.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20211108 09:08"
        },
        {
          "id": "MDRW2100046872.1.2",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "2",
          "time": "20211108 10:00"
        },
        {
          "id": "MDRW2100046872.1.3",
          "form": "혹시 틱톡 이용해보셨나요?",
          "original_form": "혹시 틱톡 이용해보셨나요?",
          "speaker_id": "2",
          "time": "20211108 10:00"
        },
        {
          "id": "MDRW2100046872.1.4",
          "form": "네 틱톡 사용해봤어요",
          "original_form": "네 틱톡 사용해봤어요",
          "speaker_id": "1",
          "time": "20211108 10:05"
        },
        {
          "id": "MDRW2100046872.1.5",
          "form": "오 자주 이용하시나요?",
          "original_form": "오 자주 이용하시나요?",
          "speaker_id": "2",
          "time": "20211108 10:05"
        },
        {
          "id": "MDRW2100046872.1.6",
          "form": "근데 요즘 개인정보유출이슈가 있지않나요",
          "original_form": "근데 요즘 개인정보유출이슈가 있지않나요",
          "speaker_id": "1",
          "time": "20211108 10:05"
        },
        {
          "id": "MDRW2100046872.1.7",
          "form": "항상 있었던 거 같은 느낌인데",
          "original_form": "항상 있었던 거 같은 느낌인데",
          "speaker_id": "2",
          "time": "20211108 10:06"
        },
        {
          "id": "MDRW2100046872.1.8",
          "form": "중국 어플이어서",
          "original_form": "중국 어플이어서",
          "speaker_id": "2",
          "time": "20211108 10:06"
        },
        {
          "id": "MDRW2100046872.1.9",
          "form": "ㅋㅋㅋㅋㅋ ㅠㅠ",
          "original_form": "ㅋㅋㅋㅋㅋ ㅠㅠ",
          "speaker_id": "2",
          "time": "20211108 10:06"
        },
        {
          "id": "MDRW2100046872.1.10",
          "form": "전 그래서 안깔았어요....ㅎ",
          "original_form": "전 그래서 안깔았어요....ㅎ",
          "speaker_id": "2",
          "time": "20211108 10:06"
        },
        {
          "id": "MDRW2100046872.1.11",
          "form": "요즘은 유튜브에 틱톡영상이 많이 올라오더라구요",
          "original_form": "요즘은 유튜브에 틱톡영상이 많이 올라오더라구요",
          "speaker_id": "1",
          "time": "20211108 10:21"
        },
        {
          "id": "MDRW2100046872.1.12",
          "form": "맞아요 아예 틱톡처럼",
          "original_form": "맞아요 아예 틱톡처럼",
          "speaker_id": "2",
          "time": "20211108 10:21"
        },
        {
          "id": "MDRW2100046872.1.13",
          "form": "릴스라고 만들었더라구요 ㅋㅋㅋㅋ",
          "original_form": "릴스라고 만들었더라구요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 10:21"
        },
        {
          "id": "MDRW2100046872.1.14",
          "form": "저도 처음에는 엥 이게 뭐지 별루다",
          "original_form": "저도 처음에는 엥 이게 뭐지 별루다",
          "speaker_id": "2",
          "time": "20211108 10:22"
        },
        {
          "id": "MDRW2100046872.1.15",
          "form": "아 릴스는 인스타구나",
          "original_form": "아 릴스는 인스타구나",
          "speaker_id": "2",
          "time": "20211108 10:22"
        },
        {
          "id": "MDRW2100046872.1.16",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 10:22"
        },
        {
          "id": "MDRW2100046872.1.17",
          "form": "유튜브는 쇼츠라고 하네요 ㅠㅠ ㅋㅋㅋ",
          "original_form": "유튜브는 쇼츠라고 하네요 ㅠㅠ ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20211108 10:22"
        },
        {
          "id": "MDRW2100046872.1.18",
          "form": "여튼 첨엔 아 틱톡같아졌네 별로다..",
          "original_form": "여튼 첨엔 아 틱톡같아졌네 별로다..",
          "speaker_id": "2",
          "time": "20211108 10:22"
        },
        {
          "id": "MDRW2100046872.1.19",
          "form": "맞아요 릴스는 인스타가 원조죠",
          "original_form": "맞아요 릴스는 인스타가 원조죠",
          "speaker_id": "1",
          "time": "20211108 10:22"
        },
        {
          "id": "MDRW2100046872.1.20",
          "form": "했는데",
          "original_form": "했는데",
          "speaker_id": "2",
          "time": "20211108 10:22"
        },
        {
          "id": "MDRW2100046872.1.21",
          "form": "이젠 쇼츠를 더 많이 보는거같아요 ㅎ",
          "original_form": "이젠 쇼츠를 더 많이 보는거같아요 ㅎ",
          "speaker_id": "2",
          "time": "20211108 10:22"
        },
        {
          "id": "MDRW2100046872.1.22",
          "form": "ㅎㅎ 네네",
          "original_form": "ㅎㅎ 네네",
          "speaker_id": "2",
          "time": "20211108 10:22"
        },
        {
          "id": "MDRW2100046872.1.23",
          "form": "헷갈렸어요 ㅠ",
          "original_form": "헷갈렸어요 ㅠ",
          "speaker_id": "2",
          "time": "20211108 10:22"
        },
        {
          "id": "MDRW2100046872.1.24",
          "form": "요즘은 쇼츠영상만 전문적으로올리는 유튜버들도",
          "original_form": "요즘은 쇼츠영상만 전문적으로올리는 유튜버들도",
          "speaker_id": "1",
          "time": "20211108 10:23"
        },
        {
          "id": "MDRW2100046872.1.25",
          "form": "있더라구요",
          "original_form": "있더라구요",
          "speaker_id": "1",
          "time": "20211108 10:23"
        },
        {
          "id": "MDRW2100046872.1.26",
          "form": "맞아요 그게 은근 조회수가 잘나오는 거 같더라고요",
          "original_form": "맞아요 그게 은근 조회수가 잘나오는 거 같더라고요",
          "speaker_id": "2",
          "time": "20211108 10:23"
        },
        {
          "id": "MDRW2100046872.1.27",
          "form": "점점 사람들이 짧은 영상에 익숙해져가는 느낌 ㅎ\r\n",
          "original_form": "점점 사람들이 짧은 영상에 익숙해져가는 느낌 ㅎ\r\n",
          "speaker_id": "2",
          "time": "20211108 10:24"
        }
      ]
