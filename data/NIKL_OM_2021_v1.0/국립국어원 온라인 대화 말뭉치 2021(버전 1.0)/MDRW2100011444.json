      "utterance": [
        {
          "id": "MDRW2100011444.1.1",
          "form": "이번 올림픽 보셨나요?",
          "original_form": "이번 올림픽 보셨나요?",
          "speaker_id": "2",
          "time": "20210824 12:43"
        },
        {
          "id": "MDRW2100011444.1.2",
          "form": "네 당연 봤죠!",
          "original_form": "네 당연 봤죠!",
          "speaker_id": "1",
          "time": "20210824 12:46"
        },
        {
          "id": "MDRW2100011444.1.3",
          "form": "어떤 경기 제일 재밌게보셨나요?",
          "original_form": "어떤 경기 제일 재밌게보셨나요?",
          "speaker_id": "2",
          "time": "20210824 12:58"
        },
        {
          "id": "MDRW2100011444.1.4",
          "form": "저는 배구랑 양궁 너무 재밌게봤어요~",
          "original_form": "저는 배구랑 양궁 너무 재밌게봤어요~",
          "speaker_id": "2",
          "time": "20210824 12:59"
        },
        {
          "id": "MDRW2100011444.1.5",
          "form": "오저도 배구랑 양궁!",
          "original_form": "오저도 배구랑 양궁!",
          "speaker_id": "1",
          "time": "20210824 13:18"
        },
        {
          "id": "MDRW2100011444.1.6",
          "form": "김연경 선수 대박이에요",
          "original_form": "김연경 선수 대박이에요",
          "speaker_id": "1",
          "time": "20210824 13:18"
        },
        {
          "id": "MDRW2100011444.1.7",
          "form": "맞아요 .. 너무 멋있더라구요 ㅠㅠ",
          "original_form": "맞아요 .. 너무 멋있더라구요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100011444.1.8",
          "form": "배구 한일전때 대박이었어요",
          "original_form": "배구 한일전때 대박이었어요",
          "speaker_id": "2",
          "time": "20210824 14:17"
        },
        {
          "id": "MDRW2100011444.1.9",
          "form": "아 다시생각해도 떨려요 지고있었는데",
          "original_form": "아 다시생각해도 떨려요 지고있었는데",
          "speaker_id": "1",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011444.1.10",
          "form": "진짜 한편의 드라마였어요 ....",
          "original_form": "진짜 한편의 드라마였어요 ....",
          "speaker_id": "2",
          "time": "20210824 14:19"
        },
        {
          "id": "MDRW2100011444.1.11",
          "form": "실시간으로 본게 정말 최고였어요 ㅠ",
          "original_form": "실시간으로 본게 정말 최고였어요 ㅠ",
          "speaker_id": "2",
          "time": "20210824 14:20"
        },
        {
          "id": "MDRW2100011444.1.12",
          "form": "안산선수도 대박이었죠",
          "original_form": "안산선수도 대박이었죠",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011444.1.13",
          "form": "너무 멋져요 3관왕 ..ㅠㅠ 엄청 차분하게 경기 잘하더라구요!",
          "original_form": "너무 멋져요 3관왕 ..ㅠㅠ 엄청 차분하게 경기 잘하더라구요!",
          "speaker_id": "2",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100011444.1.14",
          "form": "진짜 실력이 넘사벽...",
          "original_form": "진짜 실력이 넘사벽...",
          "speaker_id": "1",
          "time": "20210824 14:28"
        },
        {
          "id": "MDRW2100011444.1.15",
          "form": "너무 멋있어요 다음 올림픽도 기대돼요!",
          "original_form": "너무 멋있어요 다음 올림픽도 기대돼요!",
