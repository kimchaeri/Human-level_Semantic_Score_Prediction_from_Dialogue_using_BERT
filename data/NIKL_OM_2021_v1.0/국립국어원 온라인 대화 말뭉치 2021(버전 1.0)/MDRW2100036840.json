      "utterance": [
        {
          "id": "MDRW2100036840.1.1",
          "form": "비유로 하자면 음 모르겠어요",
          "original_form": "비유로 하자면 음 모르겠어요",
          "speaker_id": "1",
          "time": "20210831 13:30"
        },
        {
          "id": "MDRW2100036840.1.2",
          "form": "뮤지컬이나 다른 연극 보신 적 있으신가요?",
          "original_form": "뮤지컬이나 다른 연극 보신 적 있으신가요?",
          "speaker_id": "2",
          "time": "20210831 13:40"
        },
        {
          "id": "MDRW2100036840.1.3",
          "form": "저는 이상하게",
          "original_form": "저는 이상하게",
          "speaker_id": "2",
          "time": "20210831 13:40"
        },
        {
          "id": "MDRW2100036840.1.4",
          "form": "대학로 연극 감성이 싫더라구요...",
          "original_form": "대학로 연극 감성이 싫더라구요...",
          "speaker_id": "2",
          "time": "20210831 13:41"
        },
        {
          "id": "MDRW2100036840.1.5",
          "form": "한두번 정도요?",
          "original_form": "한두번 정도요?",
          "speaker_id": "1",
          "time": "20210831 13:41"
        },
        {
          "id": "MDRW2100036840.1.6",
          "form": "대학로 감성이 어떤거에요?",
          "original_form": "대학로 감성이 어떤거에요?",
          "speaker_id": "1",
          "time": "20210831 13:41"
        },
        {
          "id": "MDRW2100036840.1.7",
          "form": "예를 들자면",
          "original_form": "예를 들자면",
          "speaker_id": "2",
          "time": "20210831 13:41"
        },
        {
          "id": "MDRW2100036840.1.8",
          "form": "극적인 하룻밤",
          "original_form": "극적인 하룻밤",
          "speaker_id": "2",
          "time": "20210831 13:41"
        },
        {
          "id": "MDRW2100036840.1.9",
          "form": "약간 이런 느낌의 연극?",
          "original_form": "약간 이런 느낌의 연극?",
          "speaker_id": "2",
          "time": "20210831 13:41"
        },
        {
          "id": "MDRW2100036840.1.10",
          "form": "아아 그 줄거리에",
          "original_form": "아아 그 줄거리에",
          "speaker_id": "1",
          "time": "20210831 13:42"
        },
        {
          "id": "MDRW2100036840.1.11",
          "form": "뭔가 촌스러운...연애 느낌",
          "original_form": "뭔가 촌스러운...연애 느낌",
          "speaker_id": "2",
          "time": "20210831 13:42"
        },
        {
          "id": "MDRW2100036840.1.12",
          "form": "하룻밤이면 꼭 극적이고 그런가봐요",
          "original_form": "하룻밤이면 꼭 극적이고 그런가봐요",
          "speaker_id": "1",
          "time": "20210831 13:42"
        },
        {
          "id": "MDRW2100036840.1.13",
          "form": "너무 전형적인 스테레오타입에 여자 남자를 욱여넣는 느낌이요",
          "original_form": "너무 전형적인 스테레오타입에 여자 남자를 욱여넣는 느낌이요",
          "speaker_id": "2",
          "time": "20210831 13:42"
        },
        {
          "id": "MDRW2100036840.1.14",
          "form": "좀 오버해서요?",
          "original_form": "좀 오버해서요?",
          "speaker_id": "1",
          "time": "20210831 13:42"
        },
        {
          "id": "MDRW2100036840.1.15",
          "form": "네네 오버하는 것도 있고",
          "original_form": "네네 오버하는 것도 있고",
          "speaker_id": "2",
          "time": "20210831 13:42"
        },
        {
          "id": "MDRW2100036840.1.16",
          "form": "촌스럽달까요...음",
          "original_form": "촌스럽달까요...음",
          "speaker_id": "2",
          "time": "20210831 13:42"
        },
        {
          "id": "MDRW2100036840.1.17",
          "form": "모름지기 여자는 이럴 것이다,",
          "original_form": "모름지기 여자는 이럴 것이다,",
          "speaker_id": "2",
          "time": "20210831 13:42"
        },
        {
          "id": "MDRW2100036840.1.18",
          "form": "남자는 저럴 것이다",
          "original_form": "남자는 저럴 것이다",
          "speaker_id": "2",
          "time": "20210831 13:42"
        },
        {
          "id": "MDRW2100036840.1.19",
          "form": "이런 플롯인게 너무 오그라들고",
          "original_form": "이런 플롯인게 너무 오그라들고",
          "speaker_id": "2",
          "time": "20210831 13:43"
        },
        {
          "id": "MDRW2100036840.1.20",
          "form": "별로더라구요 ㅎㅎㅎ",
          "original_form": "별로더라구요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 13:43"
        },
        {
          "id": "MDRW2100036840.1.21",
          "form": "네 그런거 엄청 많은듯해요",
          "original_form": "네 그런거 엄청 많은듯해요",
          "speaker_id": "1",
          "time": "20210831 13:43"
        },
        {
          "id": "MDRW2100036840.1.22",
          "form": "그쵸 그래서 한국 영화도",
          "original_form": "그쵸 그래서 한국 영화도",
          "speaker_id": "2",
          "time": "20210831 13:43"
        },
        {
          "id": "MDRW2100036840.1.23",
          "form": "좋아하기 힘들어요....",
          "original_form": "좋아하기 힘들어요....",
          "speaker_id": "2",
          "time": "20210831 13:43"
        },
        {
          "id": "MDRW2100036840.1.24",
          "form": "노란 장판 감성이라고 해야하나",
          "original_form": "노란 장판 감성이라고 해야하나",
          "speaker_id": "2",
          "time": "20210831 13:43"
        },
        {
          "id": "MDRW2100036840.1.25",
          "form": "글고 막 사랑은 엄청나게 강렬하고 숭고한",
          "original_form": "글고 막 사랑은 엄청나게 강렬하고 숭고한",
          "speaker_id": "1",
          "time": "20210831 13:43"
        },
        {
          "id": "MDRW2100036840.1.26",
          "form": "그런게 저는 그닥 와닿지않아서요",
          "original_form": "그런게 저는 그닥 와닿지않아서요",
          "speaker_id": "1",
          "time": "20210831 14:00"
        },
        {
          "id": "MDRW2100036840.1.27",
          "form": "원래 본인 감정의 진폭이 큰 사람들이 있는거같아요",
          "original_form": "원래 본인 감정의 진폭이 큰 사람들이 있는거같아요",
          "speaker_id": "1",
          "time": "20210831 14:00"
        },
        {
          "id": "MDRW2100036840.1.28",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210831 14:01"
        },
        {
          "id": "MDRW2100036840.1.29",
          "form": "그리고 연애에 목숨 거는 것도",
          "original_form": "그리고 연애에 목숨 거는 것도",
          "speaker_id": "2",
          "time": "20210831 14:01"
        },
        {
          "id": "MDRW2100036840.1.30",
          "form": "그리 건강해보이지 않아서....!!!!",
          "original_form": "그리 건강해보이지 않아서....!!!!",
          "speaker_id": "2",
          "time": "20210831 14:01"
        },
        {
          "id": "MDRW2100036840.1.31",
          "form": "네 그거도 그래요\n막 나는 사랑받고 있어 강조하는 사람들요 ㅜ",
          "original_form": "네 그거도 그래요\n막 나는 사랑받고 있어 강조하는 사람들요 ㅜ",
          "speaker_id": "1",
          "time": "20210831 14:01"
        }
      ]
