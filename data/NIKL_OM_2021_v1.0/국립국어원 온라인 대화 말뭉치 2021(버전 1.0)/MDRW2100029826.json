      "utterance": [
        {
          "id": "MDRW2100029826.1.1",
          "form": "회 좋아하시나요",
          "original_form": "회 좋아하시나요",
          "speaker_id": "2",
          "time": "20210903 15:16"
        },
        {
          "id": "MDRW2100029826.1.2",
          "form": "앗 아뇨 안익힌 것보다는 익힌 생선을 더 좋아하는 편입니다ㅎㅎ",
          "original_form": "앗 아뇨 안익힌 것보다는 익힌 생선을 더 좋아하는 편입니다ㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100029826.1.3",
          "form": "회 좋아하시나요?",
          "original_form": "회 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210903 19:51"
        },
        {
          "id": "MDRW2100029826.1.4",
          "form": "저는 반대...",
          "original_form": "저는 반대...",
          "speaker_id": "2",
          "time": "20210903 19:55"
        },
        {
          "id": "MDRW2100029826.1.5",
          "form": "ㅎㅎㅎㅎ",
          "original_form": "ㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100029826.1.6",
          "form": "마라탕은 좋아하시나요??",
          "original_form": "마라탕은 좋아하시나요??",
          "speaker_id": "1",
          "time": "20210903 19:57"
        },
        {
          "id": "MDRW2100029826.1.7",
          "form": "아니오......",
          "original_form": "아니오......",
          "speaker_id": "2",
          "time": "20210903 21:03"
        },
        {
          "id": "MDRW2100029826.1.8",
          "form": "앟ㅎㅎㅎ 그럼 다른 좋아하는 음식 어떤 거 있으신가요??",
          "original_form": "앟ㅎㅎㅎ 그럼 다른 좋아하는 음식 어떤 거 있으신가요??",
          "speaker_id": "1",
          "time": "20210904 00:04"
        },
        {
          "id": "MDRW2100029826.1.9",
          "form": "떡볶이요 신전!!!",
          "original_form": "떡볶이요 신전!!!",
          "speaker_id": "2",
          "time": "20210904 00:33"
        },
        {
          "id": "MDRW2100029826.1.10",
          "form": "헉 신전 엄청 맛있죠..!!!",
          "original_form": "헉 신전 엄청 맛있죠..!!!",
          "speaker_id": "1",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100029826.1.11",
          "form": "맵기단계 어느정도로 해서 드세요?",
          "original_form": "맵기단계 어느정도로 해서 드세요?",
          "speaker_id": "1",
          "time": "20210904 12:33"
        },
        {
          "id": "MDRW2100029826.1.12",
          "form": "3해야 되는거 아닙니까",
          "original_form": "3해야 되는거 아닙니까",
          "speaker_id": "2",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100029826.1.13",
          "form": "한국인이면",
          "original_form": "한국인이면",
          "speaker_id": "2",
          "time": "20210904 14:02"
        },
        {
          "id": "MDRW2100029826.1.14",
          "form": "엇ㅋㅋㅋ 맵고수셨네요",
          "original_form": "엇ㅋㅋㅋ 맵고수셨네요",
          "speaker_id": "1",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100029826.1.15",
          "form": "전 힘줘봐야 2단계인데ㅋㅋㅋ",
          "original_form": "전 힘줘봐야 2단계인데ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 15:45"
        },
        {
          "id": "MDRW2100029826.1.16",
          "form": "근데 그게 지점마다 좀 차이가 있는듯해요",
          "original_form": "근데 그게 지점마다 좀 차이가 있는듯해요",
          "speaker_id": "2",
          "time": "20210904 15:53"
        },
        {
          "id": "MDRW2100029826.1.17",
          "form": "그렇긴하지만 아무리 덜맵게 만들어도 3단계 드시는거면 맵고수신 것 같아요ㅎㅎ",
          "original_form": "그렇긴하지만 아무리 덜맵게 만들어도 3단계 드시는거면 맵고수신 것 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 16:59"
        },
        {
          "id": "MDRW2100029826.1.18",
          "form": "맵부심이 있긴 합니당",
          "original_form": "맵부심이 있긴 합니당",
          "speaker_id": "2",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100029826.1.19",
          "form": "근데 본가때는 힘들었는데",
          "original_form": "근데 본가때는 힘들었는데",
          "speaker_id": "2",
          "time": "20210904 17:00"
        },
        {
          "id": "MDRW2100029826.1.20",
          "form": "앟 본가에서 드실때는 더 매웠었나 보네요ㅎㅎ",
          "original_form": "앟 본가에서 드실때는 더 매웠었나 보네요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210904 20:37"
        },
        {
          "id": "MDRW2100029826.1.21",
          "form": "네 그때는 3먹으면서 물2리터는",
          "original_form": "네 그때는 3먹으면서 물2리터는",
          "speaker_id": "2",
          "time": "20210904 21:39"
        },
        {
          "id": "MDRW2100029826.1.22",
          "form": "지금은 거의 안먹고",
          "original_form": "지금은 거의 안먹고",
          "speaker_id": "2",
          "time": "20210904 21:39"
        },
        {
          "id": "MDRW2100029826.1.23",
          "form": "후후로 가능입니다",
          "original_form": "후후로 가능입니다",
          "speaker_id": "2",
          "time": "20210904 21:39"
        },
        {
          "id": "MDRW2100029826.1.24",
          "form": "대단하시네요ㅋㅋㅋ",
          "original_form": "대단하시네요ㅋㅋㅋ",
          "speaker_id": "1",
