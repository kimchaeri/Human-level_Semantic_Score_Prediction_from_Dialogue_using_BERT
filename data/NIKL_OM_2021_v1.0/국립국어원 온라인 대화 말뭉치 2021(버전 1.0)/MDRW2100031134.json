      "utterance": [
        {
          "id": "MDRW2100031134.1.1",
          "form": "집안일은 하시나여",
          "original_form": "집안일은 하시나여",
          "speaker_id": "1",
          "time": "20210904 17:16"
        },
        {
          "id": "MDRW2100031134.1.2",
          "form": "네 합니다~",
          "original_form": "네 합니다~",
          "speaker_id": "2",
          "time": "20210904 17:24"
        },
        {
          "id": "MDRW2100031134.1.3",
          "form": "오 뭐뭐하시나여",
          "original_form": "오 뭐뭐하시나여",
          "speaker_id": "1",
          "time": "20210904 19:17"
        },
        {
          "id": "MDRW2100031134.1.4",
          "form": "다하는거같아요 분리수거빼고요",
          "original_form": "다하는거같아요 분리수거빼고요",
          "speaker_id": "2",
          "time": "20210904 19:19"
        },
        {
          "id": "MDRW2100031134.1.5",
          "form": "헛...전 안하는데...ㅜㅜ",
          "original_form": "헛...전 안하는데...ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 19:25"
        },
        {
          "id": "MDRW2100031134.1.6",
          "form": "둘이서 살아서 같이해야되요ㅋㅋㅋ",
          "original_form": "둘이서 살아서 같이해야되요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 19:26"
        },
        {
          "id": "MDRW2100031134.1.7",
          "form": "헛 결혼하셨나용",
          "original_form": "헛 결혼하셨나용",
          "speaker_id": "1",
          "time": "20210904 20:03"
        },
        {
          "id": "MDRW2100031134.1.8",
          "form": "네네~ 결혼했어용ㅋㅋㅋ",
          "original_form": "네네~ 결혼했어용ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100031134.1.9",
          "form": "오 신혼이신가요?",
          "original_form": "오 신혼이신가요?",
          "speaker_id": "1",
          "time": "20210906 07:35"
        },
        {
          "id": "MDRW2100031134.1.10",
          "form": "네 1년안됫어요ㅋㅋ 집에 잇을때는 전혀 안했어요",
          "original_form": "네 1년안됫어요ㅋㅋ 집에 잇을때는 전혀 안했어요",
          "speaker_id": "2",
          "time": "20210906 11:39"
        },
        {
          "id": "MDRW2100031134.1.11",
          "form": "오 결혼하고 가족 생기니까 좋나용?",
          "original_form": "오 결혼하고 가족 생기니까 좋나용?",
          "speaker_id": "1",
          "time": "20210906 11:41"
        },
        {
          "id": "MDRW2100031134.1.12",
          "form": "안하던 집안일은 잘몰라서 서툴렀는대 지금은 조금 적응했어요 좋을때도있고 아닐때도있고 그래요~ㅋ",
          "original_form": "안하던 집안일은 잘몰라서 서툴렀는대 지금은 조금 적응했어요 좋을때도있고 아닐때도있고 그래요~ㅋ",
          "speaker_id": "2",
          "time": "20210906 11:44"
        },
        {
          "id": "MDRW2100031134.1.13",
          "form": "이런...요즘은 다 같이하니까요ㅋ 전 안하지만ㅋ",
          "original_form": "이런...요즘은 다 같이하니까요ㅋ 전 안하지만ㅋ",
          "speaker_id": "1",
          "time": "20210906 11:45"
        },
        {
          "id": "MDRW2100031134.1.14",
          "form": "전 로봇청소기를 가지고싶더라구요 편하다던데",
          "original_form": "전 로봇청소기를 가지고싶더라구요 편하다던데",
          "speaker_id": "2",
          "time": "20210906 15:18"
        },
        {
          "id": "MDRW2100031134.1.15",
          "form": "오 저 있어요. 좋죠ㅋ",
          "original_form": "오 저 있어요. 좋죠ㅋ",
          "speaker_id": "1",
          "time": "20210906 15:32"
        },
        {
          "id": "MDRW2100031134.1.16",
          "form": "부럽네요 식기세척기 이모님은 있는데 로봇청소기도 꼭 구매하려구요",
          "original_form": "부럽네요 식기세척기 이모님은 있는데 로봇청소기도 꼭 구매하려구요",
          "speaker_id": "2",
          "time": "20210906 15:41"
        },
        {
          "id": "MDRW2100031134.1.17",
          "form": "식기세척기도 있는데 안써봐서..",
          "original_form": "식기세척기도 있는데 안써봐서..",
          "speaker_id": "1",
          "time": "20210906 15:48"
        }
      ]
