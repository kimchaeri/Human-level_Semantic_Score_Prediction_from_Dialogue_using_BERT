      "utterance": [
        {
          "id": "MDRW2100018187.1.1",
          "form": "저 오늘 병원 2군데나 갔다왔어요",
          "original_form": "저 오늘 병원 2군데나 갔다왔어요",
          "speaker_id": "1",
          "time": "20210826 23:15"
        },
        {
          "id": "MDRW2100018187.1.2",
          "form": "어제 부대찌개 먹었더니 배아프네요",
          "original_form": "어제 부대찌개 먹었더니 배아프네요",
          "speaker_id": "2",
          "time": "20210827 14:37"
        },
        {
          "id": "MDRW2100018187.1.3",
          "form": "헉 배탈나셨어요?",
          "original_form": "헉 배탈나셨어요?",
          "speaker_id": "1",
          "time": "20210827 14:37"
        },
        {
          "id": "MDRW2100018187.1.4",
          "form": "그런 것 같아요",
          "original_form": "그런 것 같아요",
          "speaker_id": "2",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018187.1.5",
          "form": "계속 화장실 가네요",
          "original_form": "계속 화장실 가네요",
          "speaker_id": "2",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018187.1.6",
          "form": "혹시 식중독은 아니겠죠?",
          "original_form": "혹시 식중독은 아니겠죠?",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018187.1.7",
          "form": "그건 아닌거 같아요",
          "original_form": "그건 아닌거 같아요",
          "speaker_id": "2",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018187.1.8",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018187.1.9",
          "form": "요즘 난리잖아요",
          "original_form": "요즘 난리잖아요",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018187.1.10",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018187.1.11",
          "form": "그 20대도 죽었다하고",
          "original_form": "그 20대도 죽었다하고",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018187.1.12",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "2",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018187.1.13",
          "form": "그 정도는 아니에여",
          "original_form": "그 정도는 아니에여",
          "speaker_id": "2",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018187.1.14",
          "form": "다행입니다",
          "original_form": "다행입니다",
          "speaker_id": "1",
          "time": "20210827 14:38"
        },
        {
          "id": "MDRW2100018187.1.15",
          "form": "그럼 왜 탈났을까요?",
          "original_form": "그럼 왜 탈났을까요?",
          "speaker_id": "1",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100018187.1.16",
          "form": "아마 매워서 그런거 같아요",
          "original_form": "아마 매워서 그런거 같아요",
          "speaker_id": "2",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100018187.1.17",
          "form": "아 매웠군요",
          "original_form": "아 매웠군요",
          "speaker_id": "1",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100018187.1.18",
          "form": "네 좀 맵고 자극적이었어요",
          "original_form": "네 좀 맵고 자극적이었어요",
          "speaker_id": "2",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100018187.1.19",
          "form": "약 드셨어요?",
          "original_form": "약 드셨어요?",
          "speaker_id": "1",
          "time": "20210827 14:39"
        },
        {
          "id": "MDRW2100018187.1.20",
