      "utterance": [
        {
          "id": "MDRW2100035171.1.1",
          "form": "거기 날씨는 어떤가요?",
          "original_form": "거기 날씨는 어떤가요?",
          "speaker_id": "1",
          "time": "20210829 09:38"
        },
        {
          "id": "MDRW2100035171.1.2",
          "form": "여기는 그냥 흐리기만 해용",
          "original_form": "여기는 그냥 흐리기만 해용",
          "speaker_id": "2",
          "time": "20210829 09:38"
        },
        {
          "id": "MDRW2100035171.1.3",
          "form": "새벽에 비 조금 왔어요!",
          "original_form": "새벽에 비 조금 왔어요!",
          "speaker_id": "2",
          "time": "20210829 09:38"
        },
        {
          "id": "MDRW2100035171.1.4",
          "form": "여기는 여전히 비와요. 해를 계속 못보네요 ㅠ",
          "original_form": "여기는 여전히 비와요. 해를 계속 못보네요 ㅠ",
          "speaker_id": "1",
          "time": "20210829 09:39"
        },
        {
          "id": "MDRW2100035171.1.5",
          "form": "아이구ㅠㅠ저는 경기도인데",
          "original_form": "아이구ㅠㅠ저는 경기도인데",
          "speaker_id": "2",
          "time": "20210829 09:39"
        },
        {
          "id": "MDRW2100035171.1.6",
          "form": "어디세용?",
          "original_form": "어디세용?",
          "speaker_id": "2",
          "time": "20210829 09:39"
        },
        {
          "id": "MDRW2100035171.1.7",
          "form": "여긴 대전이예요~ 이시기에 이렇게 장기간 해 못보는건 처음은것 같아요",
          "original_form": "여긴 대전이예요~ 이시기에 이렇게 장기간 해 못보는건 처음은것 같아요",
          "speaker_id": "1",
          "time": "20210829 09:40"
        },
        {
          "id": "MDRW2100035171.1.8",
          "form": "오 그러시구낭",
          "original_form": "오 그러시구낭",
          "speaker_id": "2",
          "time": "20210829 09:40"
        },
        {
          "id": "MDRW2100035171.1.9",
          "form": "저도 오늘 잠깐 장보러 나가볼까 했는데",
          "original_form": "저도 오늘 잠깐 장보러 나가볼까 했는데",
          "speaker_id": "2",
          "time": "20210829 09:40"
        },
        {
          "id": "MDRW2100035171.1.10",
          "form": "날이 이래서 언제 비올지도 모르고",
          "original_form": "날이 이래서 언제 비올지도 모르고",
          "speaker_id": "2",
          "time": "20210829 09:40"
        },
        {
          "id": "MDRW2100035171.1.11",
          "form": "날도 추워지지 않았나요?",
          "original_form": "날도 추워지지 않았나요?",
          "speaker_id": "1",
          "time": "20210829 09:40"
        },
        {
          "id": "MDRW2100035171.1.12",
          "form": "못나갈것같아요ㅠㅠ",
          "original_form": "못나갈것같아요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 09:41"
        },
        {
          "id": "MDRW2100035171.1.13",
          "form": "네네 이불도 바꿨어용",
          "original_form": "네네 이불도 바꿨어용",
          "speaker_id": "2",
          "time": "20210829 09:41"
        },
        {
          "id": "MDRW2100035171.1.14",
          "form": "저도 이불 바꿔야겠네요 ㅎㅎ",
          "original_form": "저도 이불 바꿔야겠네요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 09:43"
        },
        {
          "id": "MDRW2100035171.1.15",
          "form": "이러다 확 겨울이 오겠죠?",
          "original_form": "이러다 확 겨울이 오겠죠?",
          "speaker_id": "1",
          "time": "20210829 09:43"
        },
        {
          "id": "MDRW2100035171.1.16",
          "form": "추석전에 아주 잠깐 다시 더웠다가",
          "original_form": "추석전에 아주 잠깐 다시 더웠다가",
          "speaker_id": "2",
          "time": "20210829 09:43"
        },
        {
          "id": "MDRW2100035171.1.17",
          "form": "급 추워지고 바로 겨울시작일것같아요ㅋㅋㅋ",
          "original_form": "급 추워지고 바로 겨울시작일것같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 09:43"
        },
        {
          "id": "MDRW2100035171.1.18",
          "form": "요즘 계절이 중간이 없네용",
          "original_form": "요즘 계절이 중간이 없네용",
          "speaker_id": "2",
          "time": "20210829 09:44"
        },
        {
          "id": "MDRW2100035171.1.19",
          "form": "옷 정리도 해야되겠는데요 ㅠ",
          "original_form": "옷 정리도 해야되겠는데요 ㅠ",
          "speaker_id": "1",
          "time": "20210829 09:46"
        },
        {
          "id": "MDRW2100035171.1.20",
          "form": "전 다시 날이 더워질까봐 이도저도 못하고있어요 ㅡㅠㅠㅠㅋ",
          "original_form": "전 다시 날이 더워질까봐 이도저도 못하고있어요 ㅡㅠㅠㅠㅋ",
          "speaker_id": "2",
          "time": "20210829 09:46"
        },
        {
          "id": "MDRW2100035171.1.21",
          "form": "아.. 맞아요 그럴수도 있어요. 미리 정리할 필요는 없겠군요 ㅎㅎ",
          "original_form": "아.. 맞아요 그럴수도 있어요. 미리 정리할 필요는 없겠군요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 09:48"
        }
      ]
