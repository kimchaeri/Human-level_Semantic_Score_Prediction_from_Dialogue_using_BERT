      "utterance": [
        {
          "id": "MDRW2100016867.1.1",
          "form": "연애중이세요?",
          "original_form": "연애중이세요?",
          "speaker_id": "2",
          "time": "20210826 15:59"
        },
        {
          "id": "MDRW2100016867.1.2",
          "form": "전 저만 사랑해서.. ㅋㅋㅋ",
          "original_form": "전 저만 사랑해서.. ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016867.1.3",
          "form": "연애 얼마나 라셧어여?",
          "original_form": "연애 얼마나 라셧어여?",
          "speaker_id": "1",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016867.1.4",
          "form": "하",
          "original_form": "하",
          "speaker_id": "1",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016867.1.5",
          "form": "럽마이셀프",
          "original_form": "럽마이셀프",
          "speaker_id": "2",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016867.1.6",
          "form": "오년째구 내년말에 결혼해요",
          "original_form": "오년째구 내년말에 결혼해요",
          "speaker_id": "2",
          "time": "20210826 16:00"
        },
        {
          "id": "MDRW2100016867.1.7",
          "form": "남들이 안말려요? ㅋㅋㅋ 결혼하지 말라고..",
          "original_form": "남들이 안말려요? ㅋㅋㅋ 결혼하지 말라고..",
          "speaker_id": "1",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016867.1.8",
          "form": "애낳은부부는",
          "original_form": "애낳은부부는",
          "speaker_id": "2",
          "time": "20210826 16:01"
        },
        {
          "id": "MDRW2100016867.1.9",
          "form": "이백프로말리져",
          "original_form": "이백프로말리져",
          "speaker_id": "2",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016867.1.10",
          "form": "애낳기전까진 좋데여",
          "original_form": "애낳기전까진 좋데여",
          "speaker_id": "2",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016867.1.11",
          "form": "근데 5년이면 뭐 부부아닌가여?",
          "original_form": "근데 5년이면 뭐 부부아닌가여?",
          "speaker_id": "1",
          "time": "20210826 16:02"
        },
        {
          "id": "MDRW2100016867.1.12",
          "form": "설렘 이런거 별로 없을꺼 같아요",
          "original_form": "설렘 이런거 별로 없을꺼 같아요",
          "speaker_id": "1",
          "time": "20210826 16:03"
        },
        {
          "id": "MDRW2100016867.1.13",
          "form": "ㅋㅋ저도 그런줄알앗는데",
          "original_form": "ㅋㅋ저도 그런줄알앗는데",
          "speaker_id": "2",
          "time": "20210826 16:04"
        },
        {
          "id": "MDRW2100016867.1.14",
          "form": "2년만만나도 장수커플같앳는데",
          "original_form": "2년만만나도 장수커플같앳는데",
          "speaker_id": "2",
          "time": "20210826 16:04"
        },
        {
          "id": "MDRW2100016867.1.15",
          "form": "걍 시간이 빨리 흐른거에요",
          "original_form": "걍 시간이 빨리 흐른거에요",
          "speaker_id": "2",
          "time": "20210826 16:04"
        },
        {
          "id": "MDRW2100016867.1.16",
          "form": "좋아쥭음ㅋㅋㅋㅋ",
          "original_form": "좋아쥭음ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 16:05"
        },
        {
          "id": "MDRW2100016867.1.17",
          "form": "그러니까 결혼하겠쥬... 퉷퉤.. ㅋㅋㅋㅋㅋ",
          "original_form": "그러니까 결혼하겠쥬... 퉷퉤.. ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016867.1.18",
          "form": "다 자기 짝이 잇더라구요\r\n혼자지내는것도 세상 편하고 좋고",
          "original_form": "다 자기 짝이 잇더라구요\r\n혼자지내는것도 세상 편하고 좋고",
          "speaker_id": "2",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016867.1.19",
          "form": "누가 먼저 사귀자고 고백했어여?",
          "original_form": "누가 먼저 사귀자고 고백했어여?",
          "speaker_id": "1",
          "time": "20210826 16:06"
        },
        {
          "id": "MDRW2100016867.1.20",
          "form": "제가 햇죠",
          "original_form": "제가 햇죠",
          "speaker_id": "2",
          "time": "20210826 16:07"
        },
        {
          "id": "MDRW2100016867.1.21",
          "form": "성격이 엄청 털털하싱가보네요..",
          "original_form": "성격이 엄청 털털하싱가보네요..",
          "speaker_id": "1",
          "time": "20210826 16:09"
        }
      ]
