      "utterance": [
        {
          "id": "MDRW2100018981.1.1",
          "form": "요즘 화장품 구매하신것 있나요?",
          "original_form": "요즘 화장품 구매하신것 있나요?",
          "speaker_id": "2",
          "time": "20210827 14:22"
        },
        {
          "id": "MDRW2100018981.1.2",
          "form": "저 최근에 폼클렌징과 선크림을 샀답니다",
          "original_form": "저 최근에 폼클렌징과 선크림을 샀답니다",
          "speaker_id": "1",
          "time": "20210827 14:23"
        },
        {
          "id": "MDRW2100018981.1.3",
          "form": "이사를 했는데 잊고 안가져왔더라구요",
          "original_form": "이사를 했는데 잊고 안가져왔더라구요",
          "speaker_id": "1",
          "time": "20210827 14:23"
        },
        {
          "id": "MDRW2100018981.1.4",
          "form": "아 이사하셨군요 바쁘셨겠어요",
          "original_form": "아 이사하셨군요 바쁘셨겠어요",
          "speaker_id": "2",
          "time": "20210827 14:23"
        },
        {
          "id": "MDRW2100018981.1.5",
          "form": "선크림 제품 뭔지 물어봐도 될까요?",
          "original_form": "선크림 제품 뭔지 물어봐도 될까요?",
          "speaker_id": "2",
          "time": "20210827 14:24"
        },
        {
          "id": "MDRW2100018981.1.6",
          "form": "네 그래서 아직 짐이 엉망이에요ㅋㅋㅋ",
          "original_form": "네 그래서 아직 짐이 엉망이에요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 14:24"
        },
        {
          "id": "MDRW2100018981.1.7",
          "form": "선크림 닥터지 그린이요",
          "original_form": "선크림 닥터지 그린이요",
          "speaker_id": "1",
          "time": "20210827 14:24"
        },
        {
          "id": "MDRW2100018981.1.8",
          "form": "저도 새로 사야되는데 고민이이요",
          "original_form": "저도 새로 사야되는데 고민이이요",
          "speaker_id": "2",
          "time": "20210827 14:24"
        },
        {
          "id": "MDRW2100018981.1.9",
          "form": "무기자차가 눈에 안따갑더라구요",
          "original_form": "무기자차가 눈에 안따갑더라구요",
          "speaker_id": "1",
          "time": "20210827 14:24"
        },
        {
          "id": "MDRW2100018981.1.10",
          "form": "무기자차가 환경에도 좋다고하더라구요",
          "original_form": "무기자차가 환경에도 좋다고하더라구요",
          "speaker_id": "2",
          "time": "20210827 14:25"
        },
        {
          "id": "MDRW2100018981.1.11",
          "form": "닥터지그린 안써봤는데 눈안따갑다니 사봐야겠네요",
          "original_form": "닥터지그린 안써봤는데 눈안따갑다니 사봐야겠네요",
          "speaker_id": "2",
          "time": "20210827 14:25"
        },
        {
          "id": "MDRW2100018981.1.12",
          "form": "아 환경에도 좋나요? 굿굿..!",
          "original_form": "아 환경에도 좋나요? 굿굿..!",
          "speaker_id": "1",
          "time": "20210827 14:27"
        },
        {
          "id": "MDRW2100018981.1.13",
          "form": "양이 좀 작은거만 빼면 만족해요",
          "original_form": "양이 좀 작은거만 빼면 만족해요",
          "speaker_id": "1",
          "time": "20210827 14:27"
        },
        {
          "id": "MDRW2100018981.1.14",
          "form": "아 건성이시면 당기실 거예요 ㅠㅠ",
          "original_form": "아 건성이시면 당기실 거예요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 14:27"
        },
        {
          "id": "MDRW2100018981.1.15",
          "form": "괜찮아요 지복합이라서 딱이네요",
          "original_form": "괜찮아요 지복합이라서 딱이네요",
          "speaker_id": "2",
          "time": "20210827 14:27"
        },
        {
          "id": "MDRW2100018981.1.16",
          "form": "아이오페쓰는데 너무 기름져서ㅠ",
          "original_form": "아이오페쓰는데 너무 기름져서ㅠ",
          "speaker_id": "2",
          "time": "20210827 14:28"
        },
        {
          "id": "MDRW2100018981.1.17",
          "form": "마스크쓰고나서 기미가 생겨서 선크림이 중요해졌어요",
          "original_form": "마스크쓰고나서 기미가 생겨서 선크림이 중요해졌어요",
          "speaker_id": "2",
          "time": "20210827 14:30"
        },
        {
          "id": "MDRW2100018981.1.18",
          "form": "아 저두요!!!",
          "original_form": "아 저두요!!!",
          "speaker_id": "1",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100018981.1.19",
          "form": "마스크쓰고 등산갔더니 바로 기미가 생겼어요 ㅠㅠ",
          "original_form": "마스크쓰고 등산갔더니 바로 기미가 생겼어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 14:46"
        },
        {
          "id": "MDRW2100018981.1.20",
          "form": "그뒤로 모자랑 선크림 꼭꼭 씁니다",
          "original_form": "그뒤로 모자랑 선크림 꼭꼭 씁니다",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100018981.1.21",
          "form": "네 기미 무서워요",
          "original_form": "네 기미 무서워요",
          "speaker_id": "2",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100018981.1.22",
          "form": "기미 피부과 시술이 엄청 아프다고 들어서ㅠ",
          "original_form": "기미 피부과 시술이 엄청 아프다고 들어서ㅠ",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100018981.1.23",
          "form": "우리 열심히 피부관리해요~",
          "original_form": "우리 열심히 피부관리해요~",
          "speaker_id": "2",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100018981.1.24",
          "form": "헉 아프구나.. 가격도 엄청 비싸던데 ㅠㅠ",
          "original_form": "헉 아프구나.. 가격도 엄청 비싸던데 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100018981.1.25",
          "form": "미리미리 예방해야겠네요",
          "original_form": "미리미리 예방해야겠네요",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
