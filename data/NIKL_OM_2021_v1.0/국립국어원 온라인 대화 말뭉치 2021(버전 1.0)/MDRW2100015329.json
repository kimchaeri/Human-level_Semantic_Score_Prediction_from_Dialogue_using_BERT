      "utterance": [
        {
          "id": "MDRW2100015329.1.1",
          "form": "피자는 브랜드피자랑 화덕피자중에",
          "original_form": "피자는 브랜드피자랑 화덕피자중에",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015329.1.2",
          "form": "어떤거좋아하세요?",
          "original_form": "어떤거좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 22:42"
        },
        {
          "id": "MDRW2100015329.1.3",
          "form": "저는 브랜드를 더 좋아하는 것 같아요! 풍부한 토핑 ㅋㅋㅋ",
          "original_form": "저는 브랜드를 더 좋아하는 것 같아요! 풍부한 토핑 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015329.1.4",
          "form": "요리하시는 거 좋아하세요??",
          "original_form": "요리하시는 거 좋아하세요??",
          "speaker_id": "2",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015329.1.5",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015329.1.6",
          "form": "이것저것 해먹는게 재밌어요",
          "original_form": "이것저것 해먹는게 재밌어요",
          "speaker_id": "1",
          "time": "20210825 22:43"
        },
        {
          "id": "MDRW2100015329.1.7",
          "form": "제일 자주 해드시는 음식이 뭔가요??",
          "original_form": "제일 자주 해드시는 음식이 뭔가요??",
          "speaker_id": "2",
          "time": "20210825 22:44"
        },
        {
          "id": "MDRW2100015329.1.8",
          "form": "닭도리탕이요",
          "original_form": "닭도리탕이요",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015329.1.9",
          "form": "주에 한번이상은 먹는거 같아요",
          "original_form": "주에 한번이상은 먹는거 같아요",
          "speaker_id": "1",
          "time": "20210825 22:54"
        },
        {
          "id": "MDRW2100015329.1.10",
          "form": "제 최애음식 중 하난데 닭도리탕 ㅠㅠ",
          "original_form": "제 최애음식 중 하난데 닭도리탕 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 22:56"
        },
        {
          "id": "MDRW2100015329.1.11",
          "form": "포슬포슬한 감자 으깨고 닭고기 발라서 양념에 밥이랑 비벼먹으면",
          "original_form": "포슬포슬한 감자 으깨고 닭고기 발라서 양념에 밥이랑 비벼먹으면",
          "speaker_id": "2",
          "time": "20210825 22:57"
        },
        {
          "id": "MDRW2100015329.1.12",
          "form": "크...완전 최고죠",
          "original_form": "크...완전 최고죠",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015329.1.13",
          "form": "볶음밥 해먹으면 최고예요",
          "original_form": "볶음밥 해먹으면 최고예요",
          "speaker_id": "1",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015329.1.14",
          "form": "저도 요리 잘하고싶네요ㅠ",
          "original_form": "저도 요리 잘하고싶네요ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:01"
        },
        {
          "id": "MDRW2100015329.1.15",
          "form": "전 그나마 하는게 간편식....ㅎ",
          "original_form": "전 그나마 하는게 간편식....ㅎ",
          "speaker_id": "2",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015329.1.16",
          "form": "간편식 저도 자주 먹어요",
          "original_form": "간편식 저도 자주 먹어요",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015329.1.17",
          "form": "재료관리가.. 힘들어서",
          "original_form": "재료관리가.. 힘들어서",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015329.1.18",
          "form": "쟁겨놓고 가끔먹어요",
          "original_form": "쟁겨놓고 가끔먹어요",
          "speaker_id": "1",
          "time": "20210825 23:02"
        },
        {
          "id": "MDRW2100015329.1.19",
          "form": "맞아요 재료관리 너무 어려워요",
          "original_form": "맞아요 재료관리 너무 어려워요",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015329.1.20",
          "form": "자주 해먹는거도아니다보니까ㅠ",
          "original_form": "자주 해먹는거도아니다보니까ㅠ",
          "speaker_id": "2",
          "time": "20210825 23:03"
        },
        {
          "id": "MDRW2100015329.1.21",
          "form": "야채는 어찌나 빨리 썩는지...ㅠ",
          "original_form": "야채는 어찌나 빨리 썩는지...ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:06"
        },
        {
          "id": "MDRW2100015329.1.22",
          "form": "조금만 안보면 색변해있어요ㅠㅠ",
          "original_form": "조금만 안보면 색변해있어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210825 23:07"
        },
        {
          "id": "MDRW2100015329.1.23",
          "form": "그니까요ㅠㅠ",
          "original_form": "그니까요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015329.1.24",
          "form": "그렇다고 냉동야채 사놓기는 싫구",
          "original_form": "그렇다고 냉동야채 사놓기는 싫구",
          "speaker_id": "2",
          "time": "20210825 23:09"
        },
        {
          "id": "MDRW2100015329.1.25",
          "form": "냉동야채는 너무 맛도 영양도 없는거 같아사 더싫죠ㅠ",
          "original_form": "냉동야채는 너무 맛도 영양도 없는거 같아사 더싫죠ㅠ",
          "speaker_id": "1",
          "time": "20210825 23:10"
        }
      ]
    }
  ]
