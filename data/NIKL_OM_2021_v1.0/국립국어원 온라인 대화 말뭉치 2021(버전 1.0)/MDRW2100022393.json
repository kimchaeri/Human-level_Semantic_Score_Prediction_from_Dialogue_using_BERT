      "utterance": [
        {
          "id": "MDRW2100022393.1.1",
          "form": "뮤지컬조아하시나요",
          "original_form": "뮤지컬조아하시나요",
          "speaker_id": "1",
          "time": "20210829 13:21"
        },
        {
          "id": "MDRW2100022393.1.2",
          "form": "보고싶은데 아직 기회가 없네요ㅠ",
          "original_form": "보고싶은데 아직 기회가 없네요ㅠ",
          "speaker_id": "2",
          "time": "20210829 13:22"
        },
        {
          "id": "MDRW2100022393.1.3",
          "form": "좋아하시나용?",
          "original_form": "좋아하시나용?",
          "speaker_id": "2",
          "time": "20210829 13:22"
        },
        {
          "id": "MDRW2100022393.1.4",
          "form": "저도아직 한번도ㅠ",
          "original_form": "저도아직 한번도ㅠ",
          "speaker_id": "1",
          "time": "20210829 13:23"
        },
        {
          "id": "MDRW2100022393.1.5",
          "form": "뮤지컬이 가격이 있다보니 쉽게 보기 힘드네요ㅠ",
          "original_form": "뮤지컬이 가격이 있다보니 쉽게 보기 힘드네요ㅠ",
          "speaker_id": "2",
          "time": "20210829 13:24"
        },
        {
          "id": "MDRW2100022393.1.6",
          "form": "스타배우 나오면 가격도엄청뛰더라구요",
          "original_form": "스타배우 나오면 가격도엄청뛰더라구요",
          "speaker_id": "1",
          "time": "20210829 13:25"
        },
        {
          "id": "MDRW2100022393.1.7",
          "form": "맞아요!",
          "original_form": "맞아요!",
          "speaker_id": "2",
          "time": "20210829 13:28"
        },
        {
          "id": "MDRW2100022393.1.8",
          "form": "다녀온 친구말에 의하면 멀면 만원경도 필수라더라구요",
          "original_form": "다녀온 친구말에 의하면 멀면 만원경도 필수라더라구요",
          "speaker_id": "2",
          "time": "20210829 13:28"
        },
        {
          "id": "MDRW2100022393.1.9",
          "form": "오페라글라스 맞나요ㅎㅎ",
          "original_form": "오페라글라스 맞나요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 13:29"
        },
        {
          "id": "MDRW2100022393.1.10",
          "form": "앗 그런 고급진 명칭이 있군욧",
          "original_form": "앗 그런 고급진 명칭이 있군욧",
          "speaker_id": "2",
          "time": "20210829 13:30"
        },
        {
          "id": "MDRW2100022393.1.11",
          "form": "한번 보러가고싶네요ㅎ",
          "original_form": "한번 보러가고싶네요ㅎ",
          "speaker_id": "1",
          "time": "20210829 13:33"
        },
        {
          "id": "MDRW2100022393.1.12",
          "form": "저두요ㅋㅋ",
          "original_form": "저두요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 13:36"
        },
        {
          "id": "MDRW2100022393.1.13",
          "form": "다시 하는곳도 있다던데 지방은 웁니닷..",
          "original_form": "다시 하는곳도 있다던데 지방은 웁니닷..",
          "speaker_id": "2",
          "time": "20210829 13:37"
        },
        {
          "id": "MDRW2100022393.1.14",
          "form": "ㅠㅠ 헤드윅 보고싶네요",
          "original_form": "ㅠㅠ 헤드윅 보고싶네요",
          "speaker_id": "1",
          "time": "20210829 13:39"
        },
        {
          "id": "MDRW2100022393.1.15",
          "form": "많이 들어본거 같아요!",
          "original_form": "많이 들어본거 같아요!",
          "speaker_id": "2",
          "time": "20210829 13:40"
        },
        {
          "id": "MDRW2100022393.1.16",
          "form": "어제 놀토에 마리앙투아네뜨 배우들 나와서",
          "original_form": "어제 놀토에 마리앙투아네뜨 배우들 나와서",
          "speaker_id": "2",
          "time": "20210829 13:41"
        },
        {
          "id": "MDRW2100022393.1.17",
          "form": "전 그게 보고싶네요",
          "original_form": "전 그게 보고싶네요",
          "speaker_id": "2",
          "time": "20210829 13:41"
        },
        {
          "id": "MDRW2100022393.1.18",
          "form": "오 그 김소현 이석훈ㅎㅎ",
          "original_form": "오 그 김소현 이석훈ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 13:48"
        }
      ]
