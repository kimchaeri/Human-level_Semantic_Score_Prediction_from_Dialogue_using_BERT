      "utterance": [
        {
          "id": "MDRW2100037081.1.1",
          "form": "안녕하세요~ 식사하셨나요?",
          "original_form": "안녕하세요~ 식사하셨나요?",
          "speaker_id": "1",
          "time": "20210831 19:06"
        },
        {
          "id": "MDRW2100037081.1.2",
          "form": "퇴근중이예요~",
          "original_form": "퇴근중이예요~",
          "speaker_id": "2",
          "time": "20210831 19:06"
        },
        {
          "id": "MDRW2100037081.1.3",
          "form": "집가서 먹으려구요",
          "original_form": "집가서 먹으려구요",
          "speaker_id": "2",
          "time": "20210831 19:06"
        },
        {
          "id": "MDRW2100037081.1.4",
          "form": "저녁메뉴 정하셨어요?",
          "original_form": "저녁메뉴 정하셨어요?",
          "speaker_id": "2",
          "time": "20210831 19:06"
        },
        {
          "id": "MDRW2100037081.1.5",
          "form": "저는 좀 전에 미역국이랑 먹었어요",
          "original_form": "저는 좀 전에 미역국이랑 먹었어요",
          "speaker_id": "1",
          "time": "20210831 19:06"
        },
        {
          "id": "MDRW2100037081.1.6",
          "form": "뭐 드실 예정이세요?",
          "original_form": "뭐 드실 예정이세요?",
          "speaker_id": "1",
          "time": "20210831 19:07"
        },
        {
          "id": "MDRW2100037081.1.7",
          "form": "아직 못정했어요!",
          "original_form": "아직 못정했어요!",
          "speaker_id": "2",
          "time": "20210831 19:08"
        },
        {
          "id": "MDRW2100037081.1.8",
          "form": "아마 집에 있는걸로 먹을것같아요~",
          "original_form": "아마 집에 있는걸로 먹을것같아요~",
          "speaker_id": "2",
          "time": "20210831 19:08"
        },
        {
          "id": "MDRW2100037081.1.9",
          "form": "혼밥 하시나요?",
          "original_form": "혼밥 하시나요?",
          "speaker_id": "1",
          "time": "20210831 19:09"
        },
        {
          "id": "MDRW2100037081.1.10",
          "form": "가족들이랑 먹으려구요ㅎㅎ",
          "original_form": "가족들이랑 먹으려구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210831 20:09"
        },
        {
          "id": "MDRW2100037081.1.11",
          "form": "집와보니 김치부침개 있네요~",
          "original_form": "집와보니 김치부침개 있네요~",
          "speaker_id": "2",
          "time": "20210831 20:09"
        },
        {
          "id": "MDRW2100037081.1.12",
          "form": "비오는 날은 김치부침개죠!",
          "original_form": "비오는 날은 김치부침개죠!",
          "speaker_id": "1",
          "time": "20210831 20:21"
        },
        {
          "id": "MDRW2100037081.1.13",
          "form": "너무 맛있을 거 같아요~",
          "original_form": "너무 맛있을 거 같아요~",
          "speaker_id": "1",
          "time": "20210831 20:21"
        },
        {
          "id": "MDRW2100037081.1.14",
          "form": "김치 큼직하게 넣었더니 맛읶네용",
          "original_form": "김치 큼직하게 넣었더니 맛읶네용",
          "speaker_id": "2",
          "time": "20210831 20:32"
        },
        {
          "id": "MDRW2100037081.1.15",
          "form": "김치전은 김치가 씹혀야 맛있는 거 같아요ㅎㅎ",
          "original_form": "김치전은 김치가 씹혀야 맛있는 거 같아요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210831 20:38"
        },
        {
          "id": "MDRW2100037081.1.16",
          "form": "김치전이랑 파전 중에 어떤 걸 더 좋아하시나요?",
          "original_form": "김치전이랑 파전 중에 어떤 걸 더 좋아하시나요?",
          "speaker_id": "1",
          "time": "20210831 20:39"
        },
        {
          "id": "MDRW2100037081.1.17",
          "form": "음 우열을 못가리겠는데요ㅋㅋㅋ",
          "original_form": "음 우열을 못가리겠는데요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 08:51"
        },
        {
          "id": "MDRW2100037081.1.18",
          "form": "어떤거 고르시겠어요?",
          "original_form": "어떤거 고르시겠어요?",
          "speaker_id": "2",
          "time": "20210901 08:51"
        },
        {
          "id": "MDRW2100037081.1.19",
          "form": "저는 김치전 더 좋아해요ㅎㅎ",
          "original_form": "저는 김치전 더 좋아해요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 14:43"
        },
        {
          "id": "MDRW2100037081.1.20",
          "form": "김치전에 간장 찍어드시나요? 그냥 드시나요?",
          "original_form": "김치전에 간장 찍어드시나요? 그냥 드시나요?",
          "speaker_id": "2",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100037081.1.21",
          "form": "저는 그냥 먹어요!",
          "original_form": "저는 그냥 먹어요!",
          "speaker_id": "1",
          "time": "20210901 18:46"
        },
        {
          "id": "MDRW2100037081.1.22",
          "form": "name2 님은요?",
          "original_form": "&name2& 님은요?",
