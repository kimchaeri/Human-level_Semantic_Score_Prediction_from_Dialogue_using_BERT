      "utterance": [
        {
          "id": "MDRW2100004948.1.1",
          "form": "포스코는 아니고ㅎ 포스코와 연관된 선박회사에요ㅎ",
          "original_form": "포스코는 아니고ㅎ 포스코와 연관된 선박회사에요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:31"
        },
        {
          "id": "MDRW2100004948.1.2",
          "form": "name1님든 직업이 어떻게되세요?ㅎ",
          "original_form": "&name1&님든 직업이 어떻게되세요?ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004948.1.3",
          "form": "선박회사면 좋은곳이겠어요ㅎ",
          "original_form": "선박회사면 좋은곳이겠어요ㅎ",
          "speaker_id": "1",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004948.1.4",
          "form": "에이 세상 제일짠게 선박회사에요ㅎㅎ",
          "original_form": "에이 세상 제일짠게 선박회사에요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004948.1.5",
          "form": "전 원자력 관련일해요",
          "original_form": "전 원자력 관련일해요",
          "speaker_id": "1",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004948.1.6",
          "form": "쬐끄만한 회사에서",
          "original_form": "쬐끄만한 회사에서",
          "speaker_id": "1",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004948.1.7",
          "form": "그나마 급여는 좋은데 워낙 이력배치를 안해주니",
          "original_form": "그나마 급여는 좋은데 워낙 이력배치를 안해주니",
          "speaker_id": "2",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004948.1.8",
          "form": "우와ㅎㅎ 대단하시네요ㅎ",
          "original_form": "우와ㅎㅎ 대단하시네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:32"
        },
        {
          "id": "MDRW2100004948.1.9",
          "form": "발전소에서 일하시는거에요?",
          "original_form": "발전소에서 일하시는거에요?",
          "speaker_id": "2",
          "time": "20210821 19:33"
        },
        {
          "id": "MDRW2100004948.1.10",
          "form": "요즘 이직도 힘들고 먹고 살기 힘드니 직장있는것도 행복한거같아요",
          "original_form": "요즘 이직도 힘들고 먹고 살기 힘드니 직장있는것도 행복한거같아요",
          "speaker_id": "1",
          "time": "20210821 19:33"
        },
        {
          "id": "MDRW2100004948.1.11",
          "form": "맞아요ㅎ 매일 업무 스트레스에...",
          "original_form": "맞아요ㅎ 매일 업무 스트레스에...",
          "speaker_id": "2",
          "time": "20210821 19:33"
        },
        {
          "id": "MDRW2100004948.1.12",
          "form": "이직하고싶어도 나이가 있으니 못하죠",
          "original_form": "이직하고싶어도 나이가 있으니 못하죠",
          "speaker_id": "2",
          "time": "20210821 19:33"
        },
        {
          "id": "MDRW2100004948.1.13",
          "form": "쥐꼬리 월급인데 ㅋ 걍 요즘 스트레스는 없어서",
          "original_form": "쥐꼬리 월급인데 ㅋ 걍 요즘 스트레스는 없어서",
          "speaker_id": "1",
          "time": "20210821 19:34"
        },
        {
          "id": "MDRW2100004948.1.14",
          "form": "그려려니 하고 다니고있어요",
          "original_form": "그려려니 하고 다니고있어요",
          "speaker_id": "1",
          "time": "20210821 19:34"
        },
        {
          "id": "MDRW2100004948.1.15",
          "form": "월급은 내꺼아니잖아요ㅎ 카드사껀가? 아니면 세금꺼죠뭐ㅎ",
          "original_form": "월급은 내꺼아니잖아요ㅎ 카드사껀가? 아니면 세금꺼죠뭐ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:34"
        },
        {
          "id": "MDRW2100004948.1.16",
          "form": "ㅎㅎ 들어왔다 바로 나가는 ㅋ",
          "original_form": "ㅎㅎ 들어왔다 바로 나가는 ㅋ",
          "speaker_id": "1",
          "time": "20210821 19:35"
        },
        {
          "id": "MDRW2100004948.1.17",
          "form": "내통장은 임시거처",
          "original_form": "내통장은 임시거처",
          "speaker_id": "1",
          "time": "20210821 19:35"
        },
        {
          "id": "MDRW2100004948.1.18",
          "form": "그래도 행복해요 지금 이시기에 월급을 꼬박꼬박나온다는게",
          "original_form": "그래도 행복해요 지금 이시기에 월급을 꼬박꼬박나온다는게",
          "speaker_id": "2",
          "time": "20210821 19:35"
        },
        {
          "id": "MDRW2100004948.1.19",
          "form": "저도요 짤리지만 않길 바라고있어요",
          "original_form": "저도요 짤리지만 않길 바라고있어요",
          "speaker_id": "1",
          "time": "20210821 19:35"
        },
        {
          "id": "MDRW2100004948.1.20",
          "form": "통장은 우리꺼아닌게 확실해요ㅎㅎ 명의만 빌려준것뿐ㅎ",
          "original_form": "통장은 우리꺼아닌게 확실해요ㅎㅎ 명의만 빌려준것뿐ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:35"
        },
        {
          "id": "MDRW2100004948.1.21",
          "form": "취업이 요즘 더 힘들겟어요ㅠ",
          "original_form": "취업이 요즘 더 힘들겟어요ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:35"
        },
        {
          "id": "MDRW2100004948.1.22",
          "form": "외벌이라 힘드시겠어요",
          "original_form": "외벌이라 힘드시겠어요",
          "speaker_id": "1",
          "time": "20210821 19:35"
        },
        {
          "id": "MDRW2100004948.1.23",
          "form": "그렇죠 외벌이라ㅠㅠ",
          "original_form": "그렇죠 외벌이라ㅠㅠ",
          "speaker_id": "2",
          "time": "20210821 19:36"
        },
        {
          "id": "MDRW2100004948.1.24",
          "form": "야근도 하고 그러나요?",
          "original_form": "야근도 하고 그러나요?",
          "speaker_id": "1",
          "time": "20210821 19:36"
        },
        {
          "id": "MDRW2100004948.1.25",
          "form": "급여는 한정적잊데ㅠ",
          "original_form": "급여는 한정적잊데ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:36"
        },
        {
          "id": "MDRW2100004948.1.26",
          "form": "쓸때는 많고",
          "original_form": "쓸때는 많고",
          "speaker_id": "2",
          "time": "20210821 19:36"
        },
        {
          "id": "MDRW2100004948.1.27",
          "form": "야근도해요ㅠ",
          "original_form": "야근도해요ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:36"
        },
        {
          "id": "MDRW2100004948.1.28",
          "form": "육아에 돈이 생각보다 많이 들어가더라구요",
          "original_form": "육아에 돈이 생각보다 많이 들어가더라구요",
          "speaker_id": "1",
          "time": "20210821 19:36"
        },
        {
          "id": "MDRW2100004948.1.29",
          "form": "예전에는 일을 정말 열씸히해서 주말에도 나가고 야근도 하고 그랬는데",
          "original_form": "예전에는 일을 정말 열씸히해서 주말에도 나가고 야근도 하고 그랬는데",
          "speaker_id": "2",
          "time": "20210821 19:36"
        },
        {
          "id": "MDRW2100004948.1.30",
          "form": "투잡을 뛸수도 없고",
          "original_form": "투잡을 뛸수도 없고",
          "speaker_id": "1",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004948.1.31",
          "form": "요즘은 아이가 말을 잘하다보니",
          "original_form": "요즘은 아이가 말을 잘하다보니",
          "speaker_id": "2",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004948.1.32",
          "form": "맞아요ㅎ",
          "original_form": "맞아요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004948.1.33",
          "form": "투잡을 뛸시간이없어요ㅠ",
          "original_form": "투잡을 뛸시간이없어요ㅠ",
          "speaker_id": "2",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004948.1.34",
          "form": "그럼 기술직인건가요?",
          "original_form": "그럼 기술직인건가요?",
          "speaker_id": "1",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004948.1.35",
          "form": "사무직?",
          "original_form": "사무직?",
          "speaker_id": "1",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004948.1.36",
          "form": "직장 집 직장 집이네요ㅎ",
          "original_form": "직장 집 직장 집이네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004948.1.37",
          "form": "사무직인데 현장직?ㅎ",
          "original_form": "사무직인데 현장직?ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:37"
        },
        {
          "id": "MDRW2100004948.1.38",
          "form": "아 왔다갔다 하시는군요",
          "original_form": "아 왔다갔다 하시는군요",
          "speaker_id": "1",
          "time": "20210821 19:38"
        },
        {
          "id": "MDRW2100004948.1.39",
          "form": "선박 안전관리자라ㅎㅎ",
          "original_form": "선박 안전관리자라ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:38"
        },
        {
          "id": "MDRW2100004948.1.40",
          "form": "선박에 왔다갔다 하고있어요ㅎㅎ",
          "original_form": "선박에 왔다갔다 하고있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 19:38"
        },
        {
          "id": "MDRW2100004948.1.41",
          "form": "대충 알듯도 해요",
          "original_form": "대충 알듯도 해요",
          "speaker_id": "1",
          "time": "20210821 19:38"
        },
        {
          "id": "MDRW2100004948.1.42",
          "form": "그래서 맨날 논땡이 쳐요ㅎㅎㅎㅎㅎㅎㅎ 사무실에서 졸리면 배나갔다올께요~^^",
          "original_form": "그래서 맨날 논땡이 쳐요ㅎㅎㅎㅎㅎㅎㅎ 사무실에서 졸리면 배나갔다올께요~^^",
          "speaker_id": "2",
          "time": "20210821 19:38"
        },
        {
          "id": "MDRW2100004948.1.43",
          "form": "친구도 포스코 다니는데 현장 가고 그러더라구요",
          "original_form": "친구도 포스코 다니는데 현장 가고 그러더라구요",
          "speaker_id": "1",
          "time": "20210821 19:39"
        },
        {
          "id": "MDRW2100004948.1.44",
          "form": "여잔데 관리직이라고 봐야될지",
          "original_form": "여잔데 관리직이라고 봐야될지",
          "speaker_id": "1",
          "time": "20210821 19:39"
        },
        {
          "id": "MDRW2100004948.1.45",
          "form": "글죠ㅎ 현장안나갈수가없어요ㅎ",
          "original_form": "글죠ㅎ 현장안나갈수가없어요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:39"
        },
        {
          "id": "MDRW2100004948.1.46",
          "form": "관리직이지 않을까요?",
          "original_form": "관리직이지 않을까요?",
          "speaker_id": "2",
          "time": "20210821 19:39"
        },
        {
          "id": "MDRW2100004948.1.47",
          "form": "석사니까 그렇겠죠?",
          "original_form": "석사니까 그렇겠죠?",
          "speaker_id": "1",
          "time": "20210821 19:39"
        },
        {
          "id": "MDRW2100004948.1.48",
          "form": "그래도 포스코 다니면 공부잘하셨겟네요ㅎ",
          "original_form": "그래도 포스코 다니면 공부잘하셨겟네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:39"
        },
        {
          "id": "MDRW2100004948.1.49",
          "form": "우와ㅎ",
          "original_form": "우와ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:39"
        },
        {
          "id": "MDRW2100004948.1.50",
          "form": "방금쓸려고했었는데ㅎ",
          "original_form": "방금쓸려고했었는데ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:39"
        },
        {
          "id": "MDRW2100004948.1.51",
          "form": "연대 졸업하고 서울대석사",
          "original_form": "연대 졸업하고 서울대석사",
          "speaker_id": "1",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004948.1.52",
          "form": "엘리트 코스죠ㅎㅎ",
          "original_form": "엘리트 코스죠ㅎㅎ",
          "speaker_id": "1",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004948.1.53",
          "form": "우왐ㅎㅎ 엘리트네요ㅎ",
          "original_form": "우왐ㅎㅎ 엘리트네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004948.1.54",
          "form": "부럽네요ㅎ",
          "original_form": "부럽네요ㅎ",
          "speaker_id": "2",
          "time": "20210821 19:40"
        },
        {
          "id": "MDRW2100004948.1.55",
          "form": "저도요",
          "original_form": "저도요",
          "speaker_id": "1",
          "time": "20210821 19:40"
        },
        {
