      "utterance": [
        {
          "id": "MDRW2100011933.1.1",
          "form": "스포츠좋아하시나요",
          "original_form": "스포츠좋아하시나요",
          "speaker_id": "2",
          "time": "20210824 20:47"
        },
        {
          "id": "MDRW2100011933.1.2",
          "form": "자주 챙겨 보지는 않는데",
          "original_form": "자주 챙겨 보지는 않는데",
          "speaker_id": "1",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100011933.1.3",
          "form": "이번 올림픽은 재밌게 봤어요",
          "original_form": "이번 올림픽은 재밌게 봤어요",
          "speaker_id": "1",
          "time": "20210824 20:49"
        },
        {
          "id": "MDRW2100011933.1.4",
          "form": "오 올림픽 너무재밌었죠",
          "original_form": "오 올림픽 너무재밌었죠",
          "speaker_id": "2",
          "time": "20210824 20:50"
        },
        {
          "id": "MDRW2100011933.1.5",
          "form": "어떤 경기 재밌게 보셨나요?",
          "original_form": "어떤 경기 재밌게 보셨나요?",
          "speaker_id": "1",
          "time": "20210824 20:53"
        },
        {
          "id": "MDRW2100011933.1.6",
          "form": "여자배구요!",
          "original_form": "여자배구요!",
          "speaker_id": "2",
          "time": "20210824 20:53"
        },
        {
          "id": "MDRW2100011933.1.7",
          "form": "아 저도 너무 재밌게 봤어요!",
          "original_form": "아 저도 너무 재밌게 봤어요!",
          "speaker_id": "1",
          "time": "20210824 20:56"
        },
        {
          "id": "MDRW2100011933.1.8",
          "form": "생방송으로 보셨나요?",
          "original_form": "생방송으로 보셨나요?",
          "speaker_id": "1",
          "time": "20210824 20:56"
        },
        {
          "id": "MDRW2100011933.1.9",
          "form": "아뇨 재방송으로ㅠㅠ",
          "original_form": "아뇨 재방송으로ㅠㅠ",
          "speaker_id": "2",
          "time": "20210824 20:58"
        },
        {
          "id": "MDRW2100011933.1.10",
          "form": "시간대가 왔다갔다하고 평일 경기가 많아서 생방송으로 보기 힘들었던 거 같아요",
          "original_form": "시간대가 왔다갔다하고 평일 경기가 많아서 생방송으로 보기 힘들었던 거 같아요",
          "speaker_id": "1",
          "time": "20210824 20:59"
        },
        {
          "id": "MDRW2100011933.1.11",
          "form": "마자요 아침시간대도 많고ㅠ",
          "original_form": "마자요 아침시간대도 많고ㅠ",
          "speaker_id": "2",
          "time": "20210824 20:59"
        },
        {
          "id": "MDRW2100011933.1.12",
          "form": "막 회사나 학교에서 몰래 보시는 분들도 있었다고 하더라고요ㅋㅋㅋㅋ",
          "original_form": "막 회사나 학교에서 몰래 보시는 분들도 있었다고 하더라고요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210824 21:00"
        },
        {
          "id": "MDRW2100011933.1.13",
          "form": "그만큼 너무 재밌었어요",
          "original_form": "그만큼 너무 재밌었어요",
          "speaker_id": "1",
          "time": "20210824 21:00"
        },
        {
          "id": "MDRW2100011933.1.14",
          "form": "오우 월루ㅎㅎ",
          "original_form": "오우 월루ㅎㅎ",
          "speaker_id": "2",
          "time": "20210824 21:00"
        },
        {
          "id": "MDRW2100011933.1.15",
          "form": "저는 방학이라 마음껏 봤습니다ㅋㅋㅋ",
