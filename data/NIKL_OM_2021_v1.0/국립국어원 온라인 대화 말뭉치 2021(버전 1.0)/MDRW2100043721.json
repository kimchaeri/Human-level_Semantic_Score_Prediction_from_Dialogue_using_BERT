      "utterance": [
        {
          "id": "MDRW2100043721.1.1",
          "form": "여기는 아침부터 비가 오네요 ㅠ",
          "original_form": "여기는 아침부터 비가 오네요 ㅠ",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043721.1.2",
          "form": "거긴 날씨가 어떤가요?",
          "original_form": "거긴 날씨가 어떤가요?",
          "speaker_id": "2",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043721.1.3",
          "form": "여기도 비 오다가 오후에 그쳤네요",
          "original_form": "여기도 비 오다가 오후에 그쳤네요",
          "speaker_id": "1",
          "time": "20210907 20:12"
        },
        {
          "id": "MDRW2100043721.1.4",
          "form": "지금은 안내리나요?",
          "original_form": "지금은 안내리나요?",
          "speaker_id": "2",
          "time": "20210907 20:13"
        },
        {
          "id": "MDRW2100043721.1.5",
          "form": "에 지금은 그쳤어요",
          "original_form": "에 지금은 그쳤어요",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043721.1.6",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043721.1.7",
          "form": "대신 쌀쌀해졌어요",
          "original_form": "대신 쌀쌀해졌어요",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043721.1.8",
          "form": "여긴 지금도 가늘게 내리고 있어요",
          "original_form": "여긴 지금도 가늘게 내리고 있어요",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043721.1.9",
          "form": "맞아요 확 추워졌더라구요",
          "original_form": "맞아요 확 추워졌더라구요",
          "speaker_id": "2",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043721.1.10",
          "form": "감기 조심하세요",
          "original_form": "감기 조심하세요",
          "speaker_id": "1",
          "time": "20210907 20:14"
        },
        {
          "id": "MDRW2100043721.1.11",
          "form": "그쵸 ㅠ 앞으로는 긴팔 입어야겠어요",
          "original_form": "그쵸 ㅠ 앞으로는 긴팔 입어야겠어요",
          "speaker_id": "2",
          "time": "20210907 20:15"
        },
        {
          "id": "MDRW2100043721.1.12",
          "form": "이제 슬슬 옷정리를 해야겠어요",
          "original_form": "이제 슬슬 옷정리를 해야겠어요",
          "speaker_id": "1",
          "time": "20210907 20:16"
        },
        {
          "id": "MDRW2100043721.1.13",
          "form": "맞아요..",
          "original_form": "맞아요..",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043721.1.14",
          "form": "날씨가 너무 갑작스럽게 바뀐거같아요",
          "original_form": "날씨가 너무 갑작스럽게 바뀐거같아요",
          "speaker_id": "2",
          "time": "20210907 20:17"
        },
        {
          "id": "MDRW2100043721.1.15",
          "form": "맞아요 갑자기 가을이 된듯",
          "original_form": "맞아요 갑자기 가을이 된듯",
          "speaker_id": "1",
          "time": "20210907 20:18"
        }
