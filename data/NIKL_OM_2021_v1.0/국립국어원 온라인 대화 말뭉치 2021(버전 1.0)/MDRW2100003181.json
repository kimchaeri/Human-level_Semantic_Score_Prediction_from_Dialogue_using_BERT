      "utterance": [
        {
          "id": "MDRW2100003181.1.1",
          "form": "오늘 베라 그렌드 사이즈 사왔다ㅋㅋ",
          "original_form": "오늘 베라 그렌드 사이즈 사왔다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:13"
        },
        {
          "id": "MDRW2100003181.1.2",
          "form": "그랜드 사이즈도 있어?",
          "original_form": "그랜드 사이즈도 있어?",
          "speaker_id": "1",
          "time": "20210820 20:13"
        },
        {
          "id": "MDRW2100003181.1.3",
          "form": "첨 들어봄 ㅋㅋ",
          "original_form": "첨 들어봄 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 20:13"
        },
        {
          "id": "MDRW2100003181.1.4",
          "form": "정가 39900원 짜리",
          "original_form": "정가 39900원 짜리",
          "speaker_id": "2",
          "time": "20210820 20:14"
        },
        {
          "id": "MDRW2100003181.1.5",
          "form": "크기가 음쓰통만함ㅋㅋㅋ",
          "original_form": "크기가 음쓰통만함ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:14"
        },
        {
          "id": "MDRW2100003181.1.6",
          "form": "그만한것도 있구나 ㅋㅋㅋ 우리집은 그만한건 넣을곳도 없 ㅋㅋㅋ",
          "original_form": "그만한것도 있구나 ㅋㅋㅋ 우리집은 그만한건 넣을곳도 없 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 20:23"
        },
        {
          "id": "MDRW2100003181.1.7",
          "form": "한 10가지 맛 고르나?",
          "original_form": "한 10가지 맛 고르나?",
          "speaker_id": "1",
          "time": "20210820 20:23"
        },
        {
          "id": "MDRW2100003181.1.8",
          "form": "9가지 맛ㅋㅋ",
          "original_form": "9가지 맛ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:25"
        },
        {
          "id": "MDRW2100003181.1.9",
          "form": "뭐뭐 샀니",
          "original_form": "뭐뭐 샀니",
          "speaker_id": "1",
          "time": "20210820 20:27"
        },
        {
          "id": "MDRW2100003181.1.10",
          "form": "아몬드봉봉 사랑에빠진딸기 쿠엔크 엄마는외계인 등등ㅋㅋㅋ",
          "original_form": "아몬드봉봉 사랑에빠진딸기 쿠엔크 엄마는외계인 등등ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:28"
        },
        {
          "id": "MDRW2100003181.1.11",
          "form": "자모카 사고싶었는디 없드라",
          "original_form": "자모카 사고싶었는디 없드라",
          "speaker_id": "2",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003181.1.12",
          "form": "자모카가 뭔디 ㅋㅋㅋ",
          "original_form": "자모카가 뭔디 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003181.1.13",
          "form": "커피맛베이스에 중간중간 초코 박힌? 뭐 그런거ㅋㅋ",
          "original_form": "커피맛베이스에 중간중간 초코 박힌? 뭐 그런거ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:29"
        },
        {
          "id": "MDRW2100003181.1.14",
          "form": "그런맛이 있군여 ㅋㅋ",
          "original_form": "그런맛이 있군여 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210820 20:30"
        },
        {
          "id": "MDRW2100003181.1.15",
          "form": "응 맛있음 애플민트에 이어 내 최애중의 하나여ㅋㅋ",
          "original_form": "응 맛있음 애플민트에 이어 내 최애중의 하나여ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:33"
        },
        {
          "id": "MDRW2100003181.1.16",
          "form": "블랙 소르베? 레몬 소르베? 먹고싶은데 그건 단독으로 먹어야지 다른거랑 같이 포장하기가 좀...",
          "original_form": "블랙 소르베? 레몬 소르베? 먹고싶은데 그건 단독으로 먹어야지 다른거랑 같이 포장하기가 좀...",
          "speaker_id": "1",
          "time": "20210820 20:38"
        },
        {
          "id": "MDRW2100003181.1.17",
          "form": "그게 맛있대?",
          "original_form": "그게 맛있대?",
          "speaker_id": "2",
          "time": "20210820 20:39"
        },
        {
          "id": "MDRW2100003181.1.18",
          "form": "난 그거 좋아해 엄청 상큼",
          "original_form": "난 그거 좋아해 엄청 상큼",
          "speaker_id": "1",
          "time": "20210820 20:42"
        },
        {
          "id": "MDRW2100003181.1.19",
          "form": "어 먹어보고싶다 나 샤베트류 겁나 좋아함ㅋㅋ",
          "original_form": "어 먹어보고싶다 나 샤베트류 겁나 좋아함ㅋㅋ",
          "speaker_id": "2",
          "time": "20210820 20:43"
        },
        {
          "id": "MDRW2100003181.1.20",
          "form": "저녁 메뉴 정했니",
          "original_form": "저녁 메뉴 정했니",
