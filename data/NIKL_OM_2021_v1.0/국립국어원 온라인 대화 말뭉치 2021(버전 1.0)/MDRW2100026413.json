      "utterance": [
        {
          "id": "MDRW2100026413.1.1",
          "form": "가족과 함께 사세요?",
          "original_form": "가족과 함께 사세요?",
          "speaker_id": "2",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026413.1.2",
          "form": "네 아직 가족과 살고있습니다 ㅎㅎ",
          "original_form": "네 아직 가족과 살고있습니다 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026413.1.3",
          "form": "가족과 사시나요 ㅎㅎ",
          "original_form": "가족과 사시나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:13"
        },
        {
          "id": "MDRW2100026413.1.4",
          "form": "네네 저두요",
          "original_form": "네네 저두요",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026413.1.5",
          "form": "집안일은 많지만 그래도좋아요",
          "original_form": "집안일은 많지만 그래도좋아요",
          "speaker_id": "2",
          "time": "20210901 22:21"
        },
        {
          "id": "MDRW2100026413.1.6",
          "form": "맞아요 ㅎㅎ 저도 장점이 더 많은거같아여",
          "original_form": "맞아요 ㅎㅎ 저도 장점이 더 많은거같아여",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026413.1.7",
          "form": "그쵸 ㅋㅋ안정감",
          "original_form": "그쵸 ㅋㅋ안정감",
          "speaker_id": "2",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026413.1.8",
          "form": "마자여 ㅋㅋㅋ 따순밥먹고.. 안정감있고 ㅎㅎ",
          "original_form": "마자여 ㅋㅋㅋ 따순밥먹고.. 안정감있고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210901 22:22"
        },
        {
          "id": "MDRW2100026413.1.9",
          "form": "그렇습니다ㅜ 다함께사는게최고죠",
          "original_form": "그렇습니다ㅜ 다함께사는게최고죠",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026413.1.10",
          "form": "독립무서워요",
          "original_form": "독립무서워요",
          "speaker_id": "2",
          "time": "20210901 22:23"
        },
        {
          "id": "MDRW2100026413.1.11",
          "form": "마자여ㅋㅋㅋ 독립 평생못할거같아여..ㅎㅎ...",
          "original_form": "마자여ㅋㅋㅋ 독립 평생못할거같아여..ㅎㅎ...",
          "speaker_id": "1",
          "time": "20210901 22:24"
        },
        {
          "id": "MDRW2100026413.1.12",
          "form": "저도요 ㅋㅋ",
          "original_form": "저도요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100026413.1.13",
          "form": "결혼은 언제할런지",
          "original_form": "결혼은 언제할런지",
          "speaker_id": "2",
          "time": "20210901 22:45"
        },
        {
          "id": "MDRW2100026413.1.14",
          "form": "맞아요 ㅎㅎ근데 저는 안할거같아요..ㅎㅠㅠㅋㅋ",
          "original_form": "맞아요 ㅎㅎ근데 저는 안할거같아요..ㅎㅠㅠㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 09:40"
        },
        {
          "id": "MDRW2100026413.1.15",
          "form": "저도 ㅋㅋ잘모르겠어요",
          "original_form": "저도 ㅋㅋ잘모르겠어요",
          "speaker_id": "2",
          "time": "20210902 09:41"
        },
        {
          "id": "MDRW2100026413.1.16",
          "form": "ㅋㅋㅋㅋ우선 현재 생활에 넘 만족중이어서 한동안은 지속할거같아여 ㅎㅎ",
          "original_form": "ㅋㅋㅋㅋ우선 현재 생활에 넘 만족중이어서 한동안은 지속할거같아여 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100026413.1.17",
          "form": "집안일은 많이하시나요?",
          "original_form": "집안일은 많이하시나요?",
          "speaker_id": "1",
          "time": "20210902 09:42"
        },
        {
          "id": "MDRW2100026413.1.18",
          "form": "넹 ㅠ 많이하는편이에요",
          "original_form": "넹 ㅠ 많이하는편이에요",
          "speaker_id": "2",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100026413.1.19",
          "form": "더워서 힘들었는데 오늘은 선선해서 좋네요",
          "original_form": "더워서 힘들었는데 오늘은 선선해서 좋네요",
          "speaker_id": "2",
          "time": "20210902 09:43"
        },
        {
          "id": "MDRW2100026413.1.20",
          "form": "ㅋㅋㅋ오 그러시구나 ㅠㅠㅋㅋ 전 저녁같이 차리고 설거지 빨래 정도 해여 ㅠㅠ요즘 선선해져서 다행이네요",
