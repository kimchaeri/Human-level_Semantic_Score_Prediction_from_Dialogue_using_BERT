      "utterance": [
        {
          "id": "MDRW2100025340.1.1",
          "form": "안녕하세요 아침식사는 하셨나요?",
          "original_form": "안녕하세요 아침식사는 하셨나요?",
          "speaker_id": "1",
          "time": "20210901 09:38"
        },
        {
          "id": "MDRW2100025340.1.2",
          "form": "네! 먹었습니다!",
          "original_form": "네! 먹었습니다!",
          "speaker_id": "2",
          "time": "20210901 09:46"
        },
        {
          "id": "MDRW2100025340.1.3",
          "form": "아침 드셨나요??",
          "original_form": "아침 드셨나요??",
          "speaker_id": "2",
          "time": "20210901 09:46"
        },
        {
          "id": "MDRW2100025340.1.4",
          "form": "아니요 저는 안챙겨 먹어요 ㅠㅠ",
          "original_form": "아니요 저는 안챙겨 먹어요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 09:46"
        },
        {
          "id": "MDRW2100025340.1.5",
          "form": "출근하셨나요?",
          "original_form": "출근하셨나요?",
          "speaker_id": "1",
          "time": "20210901 09:47"
        },
        {
          "id": "MDRW2100025340.1.6",
          "form": "요즘엔 아침 안먹는 사람도 많더라구요",
          "original_form": "요즘엔 아침 안먹는 사람도 많더라구요",
          "speaker_id": "2",
          "time": "20210901 09:48"
        },
        {
          "id": "MDRW2100025340.1.7",
          "form": "학생이라 출근은 안하고... 곧 수업이라 수업 준비중입니다ㅎㅎ",
          "original_form": "학생이라 출근은 안하고... 곧 수업이라 수업 준비중입니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210901 09:48"
        },
        {
          "id": "MDRW2100025340.1.8",
          "form": "요즘 수업은줌으로 하나요 학교를 가나요?",
          "original_form": "요즘 수업은줌으로 하나요 학교를 가나요?",
          "speaker_id": "1",
          "time": "20210901 09:53"
        },
        {
          "id": "MDRW2100025340.1.9",
          "form": "줌으로 해요!",
          "original_form": "줌으로 해요!",
          "speaker_id": "2",
          "time": "20210901 09:56"
        },
        {
          "id": "MDRW2100025340.1.10",
          "form": "아그래서 아침밥을 먹는군요",
          "original_form": "아그래서 아침밥을 먹는군요",
          "speaker_id": "1",
          "time": "20210901 09:57"
        },
        {
          "id": "MDRW2100025340.1.11",
          "form": "좋아하는 음식 있으세요?",
          "original_form": "좋아하는 음식 있으세요?",
          "speaker_id": "1",
          "time": "20210901 09:57"
        },
        {
          "id": "MDRW2100025340.1.12",
          "form": "네 저는 떡볶이 좋아합니다!",
          "original_form": "네 저는 떡볶이 좋아합니다!",
          "speaker_id": "2",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100025340.1.13",
          "form": "어떤 음식 좋아하세요??",
          "original_form": "어떤 음식 좋아하세요??",
          "speaker_id": "2",
          "time": "20210901 10:00"
        },
        {
          "id": "MDRW2100025340.1.14",
          "form": "헐 저도",
          "original_form": "헐 저도",
          "speaker_id": "1",
          "time": "20210901 10:01"
        },
        {
          "id": "MDRW2100025340.1.15",
          "form": "떡볶이 좋아해요",
          "original_form": "떡볶이 좋아해요",
          "speaker_id": "1",
          "time": "20210901 10:01"
        },
        {
          "id": "MDRW2100025340.1.16",
          "form": "요즘은 국밥도 좋더라구요",
          "original_form": "요즘은 국밥도 좋더라구요",
          "speaker_id": "1",
          "time": "20210901 10:01"
        },
        {
          "id": "MDRW2100025340.1.17",
          "form": "떡볶이 어디가 좋나요",
          "original_form": "떡볶이 어디가 좋나요",
          "speaker_id": "1",
          "time": "20210901 10:01"
        },
        {
          "id": "MDRW2100025340.1.18",
          "form": "전 요즘 로제에 빠져서",
          "original_form": "전 요즘 로제에 빠져서",
          "speaker_id": "2",
          "time": "20210901 10:01"
        },
        {
          "id": "MDRW2100025340.1.19",
          "form": "로제 떡볶이 자주 시켜먹어요!",
          "original_form": "로제 떡볶이 자주 시켜먹어요!",
          "speaker_id": "2",
          "time": "20210901 10:01"
        },
        {
          "id": "MDRW2100025340.1.20",
          "form": "요즘 날씨 쌀쌀해져서 국밥도 너무 땡기더라구요",
          "original_form": "요즘 날씨 쌀쌀해져서 국밥도 너무 땡기더라구요",
          "speaker_id": "2",
          "time": "20210901 10:02"
        },
        {
          "id": "MDRW2100025340.1.21",
          "form": "저는 로제는 너무 물리더라구요 ㅠㅠ",
          "original_form": "저는 로제는 너무 물리더라구요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210901 10:05"
        },
        {
          "id": "MDRW2100025340.1.22",
          "form": "저는 주로 기본을 먹어요",
          "original_form": "저는 주로 기본을 먹어요",
          "speaker_id": "1",
          "time": "20210901 10:05"
        },
        {
          "id": "MDRW2100025340.1.23",
          "form": "국밥진짜 너무 맛있어요 ㅋㅋ",
          "original_form": "국밥진짜 너무 맛있어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 10:05"
        },
        {
          "id": "MDRW2100025340.1.24",
          "form": "맞아요 어떤 국밥 제일 좋아하세요???",
          "original_form": "맞아요 어떤 국밥 제일 좋아하세요???",
          "speaker_id": "2",
          "time": "20210901 10:05"
        },
        {
          "id": "MDRW2100025340.1.25",
          "form": "저는돼지국밥이랑 콩나물국밥좋아해요!",
          "original_form": "저는돼지국밥이랑 콩나물국밥좋아해요!",
          "speaker_id": "1",
          "time": "20210901 10:06"
        }
      ]
    }
  ]
