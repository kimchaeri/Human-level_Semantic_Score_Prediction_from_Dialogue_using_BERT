      "utterance": [
        {
          "id": "MDRW2100014119.1.1",
          "form": "name2님은 백신 맞으셨나요~?",
          "original_form": "&name2&님은 백신 맞으셨나요~?",
          "speaker_id": "1",
          "time": "20210825 17:21"
        },
        {
          "id": "MDRW2100014119.1.2",
          "form": "저는 1차 맞았어요!",
          "original_form": "저는 1차 맞았어요!",
          "speaker_id": "2",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014119.1.3",
          "form": "name1님은 맞으셨나료??",
          "original_form": "&name1&님은 맞으셨나료??",
          "speaker_id": "2",
          "time": "20210825 19:14"
        },
        {
          "id": "MDRW2100014119.1.4",
          "form": "내일 1차 맞으러 가요!",
          "original_form": "내일 1차 맞으러 가요!",
          "speaker_id": "1",
          "time": "20210825 19:23"
        },
        {
          "id": "MDRW2100014119.1.5",
          "form": "어떤 백신 맞으셨어요?",
          "original_form": "어떤 백신 맞으셨어요?",
          "speaker_id": "1",
          "time": "20210825 19:24"
        },
        {
          "id": "MDRW2100014119.1.6",
          "form": "저는 화이자요!",
          "original_form": "저는 화이자요!",
          "speaker_id": "2",
          "time": "20210825 19:28"
        },
        {
          "id": "MDRW2100014119.1.7",
          "form": "오 저도 화이자 맞아요",
          "original_form": "오 저도 화이자 맞아요",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014119.1.8",
          "form": "백신 맞고 컨디션 괜찮으셨나요?",
          "original_form": "백신 맞고 컨디션 괜찮으셨나요?",
          "speaker_id": "1",
          "time": "20210825 19:51"
        },
        {
          "id": "MDRW2100014119.1.9",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210825 21:38"
        },
        {
          "id": "MDRW2100014119.1.10",
          "form": "팔만 좀 뻐근하고 다른덴 괜찮았어요",
          "original_form": "팔만 좀 뻐근하고 다른덴 괜찮았어요",
          "speaker_id": "2",
          "time": "20210825 21:39"
        },
        {
          "id": "MDRW2100014119.1.11",
          "form": "오 다행이네요",
          "original_form": "오 다행이네요",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100014119.1.12",
          "form": "저도 별 증상 없었으면 좋겠어요",
          "original_form": "저도 별 증상 없었으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210826 15:19"
        },
        {
          "id": "MDRW2100014119.1.13",
          "form": "곧 맞으러 가는데 약간 떨리네요 ㅋㅋㅋㅋㅋ",
          "original_form": "곧 맞으러 가는데 약간 떨리네요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 15:20"
        },
        {
          "id": "MDRW2100014119.1.14",
          "form": "으악",
          "original_form": "으악",
          "speaker_id": "2",
          "time": "20210829 20:07"
        },
        {
          "id": "MDRW2100014119.1.15",
          "form": "괜찮으셨나요?",
          "original_form": "괜찮으셨나요?",
          "speaker_id": "2",
          "time": "20210829 20:07"
        },
        {
          "id": "MDRW2100014119.1.16",
          "form": "네 ㅎㅎ 다행히 첫날 팔통증+열 살짝 있다가 둘째날부터 괜찮아졌어요",
          "original_form": "네 ㅎㅎ 다행히 첫날 팔통증+열 살짝 있다가 둘째날부터 괜찮아졌어요",
          "speaker_id": "1",
          "time": "20210829 20:08"
        },
        {
          "id": "MDRW2100014119.1.17",
          "form": "아이고",
          "original_form": "아이고",
          "speaker_id": "2",
          "time": "20210829 20:09"
        },
        {
          "id": "MDRW2100014119.1.18",
          "form": "다행이네요",
          "original_form": "다행이네요",
          "speaker_id": "2",
          "time": "20210829 20:09"
        },
        {
          "id": "MDRW2100014119.1.19",
          "form": "곧 2차인데 벌써 걱정돼요ㅜ",
          "original_form": "곧 2차인데 벌써 걱정돼요ㅜ",
          "speaker_id": "2",
          "time": "20210829 20:09"
        },
        {
          "id": "MDRW2100014119.1.20",
          "form": "헉 언제 맞으세요?",
          "original_form": "헉 언제 맞으세요?",
          "speaker_id": "1",
          "time": "20210829 20:09"
        },
        {
          "id": "MDRW2100014119.1.21",
          "form": "저 다음주에 맞아요ㅜ",
          "original_form": "저 다음주에 맞아요ㅜ",
          "speaker_id": "2",
          "time": "20210829 20:53"
        },
        {
          "id": "MDRW2100014119.1.22",
          "form": "정말 얼마 안남으셨네요... 아이구",
          "original_form": "정말 얼마 안남으셨네요... 아이구",
          "speaker_id": "1",
          "time": "20210829 21:03"
        },
        {
          "id": "MDRW2100014119.1.23",
          "form": "집에 타이레놀 꼭 준비해두셔요 ㅠ",
          "original_form": "집에 타이레놀 꼭 준비해두셔요 ㅠ",
