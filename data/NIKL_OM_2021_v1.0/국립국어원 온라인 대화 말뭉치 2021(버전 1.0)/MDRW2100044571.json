      "utterance": [
        {
          "id": "MDRW2100044571.1.1",
          "form": "오늘 흠...",
          "original_form": "오늘 흠...",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044571.1.2",
          "form": "발카가 문제다",
          "original_form": "발카가 문제다",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044571.1.3",
          "form": "카감은 각송해야한다",
          "original_form": "카감은 각송해야한다",
          "speaker_id": "2",
          "time": "20210907 23:06"
        },
        {
          "id": "MDRW2100044571.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ흠",
          "original_form": "ㅋㅋㅋㅋㅋㅋ흠",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044571.1.5",
          "form": "다른 계급 전부 춤춤??",
          "original_form": "다른 계급 전부 춤춤??",
          "speaker_id": "1",
          "time": "20210907 23:07"
        },
        {
          "id": "MDRW2100044571.1.6",
          "form": "ㅇㅇ다 버여줫어",
          "original_form": "ㅇㅇ다 버여줫어",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044571.1.7",
          "form": "오늘도 느끼는 건데",
          "original_form": "오늘도 느끼는 건데",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044571.1.8",
          "form": "오오 전부",
          "original_form": "오오 전부",
          "speaker_id": "1",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044571.1.9",
          "form": "인터뷰 하는 언니들 왜 맨날 눈시울 붉어? ㅋㅋ큐ㅠㅠ",
          "original_form": "인터뷰 하는 언니들 왜 맨날 눈시울 붉어? ㅋㅋ큐ㅠㅠ",
          "speaker_id": "2",
          "time": "20210907 23:08"
        },
        {
          "id": "MDRW2100044571.1.10",
          "form": "아뭔데똨ㅋㅋㅋㅋㅋ",
          "original_form": "아뭔데똨ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044571.1.11",
          "form": "감동익음???",
          "original_form": "감동익음???",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044571.1.12",
          "form": "ㅋㅋㅋㅋ감동은 아닌데",
          "original_form": "ㅋㅋㅋㅋ감동은 아닌데",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044571.1.13",
          "form": "다들 센 언니같지만 여린듯ㅋㅋㅋ",
          "original_form": "다들 센 언니같지만 여린듯ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044571.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:09"
        },
        {
          "id": "MDRW2100044571.1.15",
          "form": "가비 생얼 완전 순함",
          "original_form": "가비 생얼 완전 순함",
          "speaker_id": "1",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044571.1.16",
          "form": "맞음 ㅋㅋㅋㅋ진짜 주변에 있을 것 같은",
          "original_form": "맞음 ㅋㅋㅋㅋ진짜 주변에 있을 것 같은",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044571.1.17",
          "form": "그런 느낌 몬지아니",
          "original_form": "그런 느낌 몬지아니",
          "speaker_id": "2",
          "time": "20210907 23:10"
        },
        {
          "id": "MDRW2100044571.1.18",
          "form": "알지알짘ㅋㅋㅋㅋ",
          "original_form": "알지알짘ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044571.1.19",
          "form": "아 기대된다 이번에도 두시간하려나",
          "original_form": "아 기대된다 이번에도 두시간하려나",
          "speaker_id": "1",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044571.1.20",
          "form": "ㅋㅋㅋㅋㅋㅋ벌써 거의 1시간 되가긴 하는데",
          "original_form": "ㅋㅋㅋㅋㅋㅋ벌써 거의 1시간 되가긴 하는데",
          "speaker_id": "2",
          "time": "20210907 23:11"
        },
        {
          "id": "MDRW2100044571.1.21",
          "form": "아직 덜 보여주긴 함ㅋㅋㅋ",
          "original_form": "아직 덜 보여주긴 함ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044571.1.22",
          "form": "오오 그거 계급마다 풀로보여줄라나",
          "original_form": "오오 그거 계급마다 풀로보여줄라나",
          "speaker_id": "1",
          "time": "20210907 23:12"
        },
        {
          "id": "MDRW2100044571.1.23",
          "form": "음 계급마다 다 이슈가 잇어 ㅋㅋㅋㅋ",
          "original_form": "음 계급마다 다 이슈가 잇어 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044571.1.24",
          "form": "오 기대됨",
          "original_form": "오 기대됨",
          "speaker_id": "1",
          "time": "20210907 23:13"
        },
        {
          "id": "MDRW2100044571.1.25",
          "form": "ㅎ ㅏ 스포 안할라니까 힘드노 ㅋㅋㅋ",
          "original_form": "ㅎ ㅏ 스포 안할라니까 힘드노 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044571.1.26",
          "form": "중간에 아이키 좀 멋졋다 오늘",
          "original_form": "중간에 아이키 좀 멋졋다 오늘",
          "speaker_id": "2",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044571.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ아 빨리봐야겟더",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ아 빨리봐야겟더",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044571.1.28",
          "form": "오 아이키 드디어 뭐나오나",
          "original_form": "오 아이키 드디어 뭐나오나",
          "speaker_id": "1",
          "time": "20210907 23:14"
        },
        {
          "id": "MDRW2100044571.1.29",
          "form": "춤 말고 ㅋㅋㅋㅋ",
          "original_form": "춤 말고 ㅋㅋㅋㅋ",
