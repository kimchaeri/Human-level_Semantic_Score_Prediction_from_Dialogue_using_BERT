      "utterance": [
        {
          "id": "MDRW2100044652.1.1",
          "form": "대하 머리구이 드세요?",
          "original_form": "대하 머리구이 드세요?",
          "speaker_id": "2",
          "time": "20210907 23:22"
        },
        {
          "id": "MDRW2100044652.1.2",
          "form": "튀긴건 먹어요 새우깡맛나져 ㅋㅋ",
          "original_form": "튀긴건 먹어요 새우깡맛나져 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044652.1.3",
          "form": "크 튀긴거 마자여 머리튀김",
          "original_form": "크 튀긴거 마자여 머리튀김",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044652.1.4",
          "form": "묵고싶어요",
          "original_form": "묵고싶어요",
          "speaker_id": "2",
          "time": "20210907 23:23"
        },
        {
          "id": "MDRW2100044652.1.5",
          "form": "술조아하세요?",
          "original_form": "술조아하세요?",
          "speaker_id": "2",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044652.1.6",
          "form": "소맥 가끔 마셔요\n술은 소맥이죠ㅋ",
          "original_form": "소맥 가끔 마셔요\n술은 소맥이죠ㅋ",
          "speaker_id": "1",
          "time": "20210907 23:24"
        },
        {
          "id": "MDRW2100044652.1.7",
          "form": "ㅋㅋㅋㅋ 안주는요,",
          "original_form": "ㅋㅋㅋㅋ 안주는요,",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044652.1.8",
          "form": "?",
          "original_form": "?",
          "speaker_id": "2",
          "time": "20210907 23:25"
        },
        {
          "id": "MDRW2100044652.1.9",
          "form": "육회가 갑자기 생각나네요ㅋㅋ",
          "original_form": "육회가 갑자기 생각나네요ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:27"
        },
        {
          "id": "MDRW2100044652.1.10",
          "form": "오육회... 예술이죠",
          "original_form": "오육회... 예술이죠",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044652.1.11",
          "form": "육사시미도",
          "original_form": "육사시미도",
          "speaker_id": "2",
          "time": "20210907 23:56"
        },
        {
          "id": "MDRW2100044652.1.12",
          "form": "맞아요 술이 술술들어가져ㅋㅋ",
          "original_form": "맞아요 술이 술술들어가져ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 23:57"
        },
        {
          "id": "MDRW2100044652.1.13",
          "form": "자주드시나여",
          "original_form": "자주드시나여",
          "speaker_id": "2",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044652.1.14",
          "form": "전엔 친구들이랑 먹엇엇는데 지금은 못만나니ㅠ",
          "original_form": "전엔 친구들이랑 먹엇엇는데 지금은 못만나니ㅠ",
          "speaker_id": "1",
          "time": "20210907 23:58"
        },
        {
          "id": "MDRW2100044652.1.15",
          "form": "ㅜㅜㅋㅋㅋ 혼자드시나요",
          "original_form": "ㅜㅜㅋㅋㅋ 혼자드시나요",
          "speaker_id": "2",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044652.1.16",
          "form": "그래서 안먹은지 오래됏네요 ㅠ",
          "original_form": "그래서 안먹은지 오래됏네요 ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:00"
        },
        {
          "id": "MDRW2100044652.1.17",
          "form": "흑흑 ㅠㅠ아쉽네여 코로나ㅠㅠ",
