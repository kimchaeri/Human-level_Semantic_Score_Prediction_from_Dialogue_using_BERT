        },
        {
          "id": "MDRW2100044121.1.2",
          "form": "얼굴보니 알겠어요",
          "original_form": "얼굴보니 알겠어요",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044121.1.3",
          "form": "용의눈물",
          "original_form": "용의눈물",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044121.1.4",
          "form": "ㅋㅋㅋ  유동근 나온거",
          "original_form": "ㅋㅋㅋ  유동근 나온거",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044121.1.5",
          "form": "이건 96.11~98.5 엄청 길게함",
          "original_form": "이건 96.11~98.5 엄청 길게함",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044121.1.6",
          "form": "보진 않았지만 기억 나네요",
          "original_form": "보진 않았지만 기억 나네요",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044121.1.7",
          "form": "159부작",
          "original_form": "159부작",
          "speaker_id": "2",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044121.1.8",
          "form": "인기 많았자나요",
          "original_form": "인기 많았자나요",
          "speaker_id": "1",
          "time": "20210907 21:39"
        },
        {
          "id": "MDRW2100044121.1.9",
          "form": "엉 우리아버지 열심히 보셨다",
          "original_form": "엉 우리아버지 열심히 보셨다",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044121.1.10",
          "form": "와 진짜 기네요ㅋ 다시 예전으로 돌아감",
          "original_form": "와 진짜 기네요ㅋ 다시 예전으로 돌아감",
          "speaker_id": "1",
          "time": "20210907 21:40"
        },
        {
          "id": "MDRW2100044121.1.11",
          "form": "유동근이 연기를 너무 잘해서",
          "original_form": "유동근이 연기를 너무 잘해서",
          "speaker_id": "2",
          "time": "20210907 21:40"
        },
        {
