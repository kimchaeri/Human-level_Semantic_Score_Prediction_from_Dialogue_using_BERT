      "utterance": [
        {
          "id": "MDRW2100037752.1.1",
          "form": "먹고 뭐가 제일 맛있었는지 알려줘ㅋㅋㅋ",
          "original_form": "먹고 뭐가 제일 맛있었는지 알려줘ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100037752.1.2",
          "form": "얍얍",
          "original_form": "얍얍",
          "speaker_id": "2",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100037752.1.3",
          "form": "레몬존맛",
          "original_form": "레몬존맛",
          "speaker_id": "2",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100037752.1.4",
          "form": "레몬이 그",
          "original_form": "레몬이 그",
          "speaker_id": "2",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100037752.1.5",
          "form": "글래이즈 도넛처럼",
          "original_form": "글래이즈 도넛처럼",
          "speaker_id": "2",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100037752.1.6",
          "form": "마들렌에 설탕 시럽 뿌려서 굳힌 느낌인데",
          "original_form": "마들렌에 설탕 시럽 뿌려서 굳힌 느낌인데",
          "speaker_id": "2",
          "time": "20210901 17:47"
        },
        {
          "id": "MDRW2100037752.1.7",
          "form": "진짜 상큼해",
          "original_form": "진짜 상큼해",
          "speaker_id": "2",
          "time": "20210901 17:48"
        },
        {
          "id": "MDRW2100037752.1.8",
          "form": "오 나도 레몬 궁금했어",
          "original_form": "오 나도 레몬 궁금했어",
          "speaker_id": "1",
          "time": "20210901 17:49"
        },
        {
          "id": "MDRW2100037752.1.9",
          "form": "다들 맛있다고 하더라고",
          "original_form": "다들 맛있다고 하더라고",
          "speaker_id": "1",
          "time": "20210901 17:50"
        },
        {
          "id": "MDRW2100037752.1.10",
          "form": "완전 맛있더라",
          "original_form": "완전 맛있더라",
          "speaker_id": "2",
          "time": "20210901 17:50"
        },
        {
          "id": "MDRW2100037752.1.11",
          "form": "신 맛은 안 나?",
          "original_form": "신 맛은 안 나?",
          "speaker_id": "1",
          "time": "20210901 17:51"
        },
        {
          "id": "MDRW2100037752.1.12",
          "form": "시지는 않아",
          "original_form": "시지는 않아",
          "speaker_id": "2",
          "time": "20210901 17:51"
        },
        {
          "id": "MDRW2100037752.1.13",
          "form": "나 가로수길가서 샀는데",
          "original_form": "나 가로수길가서 샀는데",
          "speaker_id": "2",
          "time": "20210901 17:51"
        },
        {
          "id": "MDRW2100037752.1.14",
          "form": "내가 산곳 맛있더라",
          "original_form": "내가 산곳 맛있더라",
          "speaker_id": "2",
          "time": "20210901 17:51"
        },
        {
          "id": "MDRW2100037752.1.15",
          "form": "나중에 같이가서 사오자!",
          "original_form": "나중에 같이가서 사오자!",
          "speaker_id": "2",
          "time": "20210901 17:51"
        },
        {
          "id": "MDRW2100037752.1.16",
          "form": "좋아!!",
          "original_form": "좋아!!",
          "speaker_id": "1",
          "time": "20210901 17:52"
        },
        {
          "id": "MDRW2100037752.1.17",
          "form": "예에!",
          "original_form": "예에!",
          "speaker_id": "2",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100037752.1.18",
          "form": "저번에 점심 약속 있었을 때 사온 거야?",
          "original_form": "저번에 점심 약속 있었을 때 사온 거야?",
          "speaker_id": "1",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100037752.1.19",
          "form": "엉엉",
          "original_form": "엉엉",
          "speaker_id": "2",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100037752.1.20",
          "form": "어제 사왔어",
          "original_form": "어제 사왔어",
          "speaker_id": "2",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100037752.1.21",
          "form": "마들렌 파는곳 앞에",
          "original_form": "마들렌 파는곳 앞에",
          "speaker_id": "2",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100037752.1.22",
          "form": "크로와상반죽으로 만든",
          "original_form": "크로와상반죽으로 만든",
          "speaker_id": "2",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100037752.1.23",
          "form": "컵케이크 파는곳도 있어!",
          "original_form": "컵케이크 파는곳도 있어!",
          "speaker_id": "2",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100037752.1.24",
          "form": "거긴 못갔는데 나중에 거기도 가자",
          "original_form": "거긴 못갔는데 나중에 거기도 가자",
          "speaker_id": "2",
          "time": "20210901 17:53"
        },
        {
          "id": "MDRW2100037752.1.25",
          "form": "나 카페 간곳도 이쁘고 케이크 맛있더라",
          "original_form": "나 카페 간곳도 이쁘고 케이크 맛있더라",
          "speaker_id": "2",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100037752.1.26",
          "form": "거기 말고도 맛있는 디저트 카페 많아보이더라",
          "original_form": "거기 말고도 맛있는 디저트 카페 많아보이더라",
          "speaker_id": "2",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100037752.1.27",
          "form": "와 역시 가로수길인가",
          "original_form": "와 역시 가로수길인가",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100037752.1.28",
          "form": "뭐가 많구만",
          "original_form": "뭐가 많구만",
          "speaker_id": "1",
          "time": "20210901 17:54"
        },
        {
          "id": "MDRW2100037752.1.29",
          "form": "마자!",
          "original_form": "마자!",
          "speaker_id": "2",
          "time": "20210901 17:55"
        },
        {
          "id": "MDRW2100037752.1.30",
          "form": "맛집도 많은것 같더라",
          "original_form": "맛집도 많은것 같더라",
          "speaker_id": "2",
          "time": "20210901 17:55"
        },
        {
          "id": "MDRW2100037752.1.31",
          "form": "압구정도 이쁜 카페 많더랴!",
          "original_form": "압구정도 이쁜 카페 많더랴!",
          "speaker_id": "2",
          "time": "20210901 17:55"
        },
        {
          "id": "MDRW2100037752.1.32",
          "form": "나 한번도 안 가봤어",
          "original_form": "나 한번도 안 가봤어",
          "speaker_id": "1",
          "time": "20210901 17:55"
        },
        {
          "id": "MDRW2100037752.1.33",
          "form": "나 한번 가봤어",
          "original_form": "나 한번 가봤어",
          "speaker_id": "2",
          "time": "20210901 17:55"
        },
        {
          "id": "MDRW2100037752.1.34",
          "form": "그 도산공원쪽",
          "original_form": "그 도산공원쪽",
          "speaker_id": "2",
          "time": "20210901 17:55"
        },
        {
          "id": "MDRW2100037752.1.35",
          "form": "카페도 많고 편집샵도 많고",
          "original_form": "카페도 많고 편집샵도 많고",
          "speaker_id": "2",
          "time": "20210901 17:55"
        }
      ]
    }
  ]
