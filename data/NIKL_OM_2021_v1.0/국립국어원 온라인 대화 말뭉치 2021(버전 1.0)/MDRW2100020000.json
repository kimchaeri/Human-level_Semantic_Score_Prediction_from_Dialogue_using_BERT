      "utterance": [
        {
          "id": "MDRW2100020000.1.1",
          "form": "ㅋㅋㅋㅋㅋ좀 마음고생을 하고 나서 깨닫게 된 것 같시야",
          "original_form": "ㅋㅋㅋㅋㅋ좀 마음고생을 하고 나서 깨닫게 된 것 같시야",
          "speaker_id": "2",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100020000.1.2",
          "form": "상대방한테 그걸 요구하지 않을 수 있게된다는게",
          "original_form": "상대방한테 그걸 요구하지 않을 수 있게된다는게",
          "speaker_id": "1",
          "time": "20210827 23:11"
        },
        {
          "id": "MDRW2100020000.1.3",
          "form": "정말 건강한 마음가짐같아요",
          "original_form": "정말 건강한 마음가짐같아요",
          "speaker_id": "1",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100020000.1.4",
          "form": "아마 전 앞으로도 동성포함 비연애일 것 같지만",
          "original_form": "아마 전 앞으로도 동성포함 비연애일 것 같지만",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100020000.1.5",
          "form": "저와 의견이 다른 분들은 응원해주고싶어ㅛ",
          "original_form": "저와 의견이 다른 분들은 응원해주고싶어ㅛ",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100020000.1.6",
          "form": "어요",
          "original_form": "어요",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100020000.1.7",
          "form": "제가 생각한 방식이랑은 또다른 방식으로",
          "original_form": "제가 생각한 방식이랑은 또다른 방식으로",
          "speaker_id": "2",
          "time": "20210827 23:12"
        },
        {
          "id": "MDRW2100020000.1.8",
          "form": "답을 찾는 분들도 계실거고",
          "original_form": "답을 찾는 분들도 계실거고",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100020000.1.9",
          "form": "솔직히 여성들 간의 사랑이 대두된 지",
          "original_form": "솔직히 여성들 간의 사랑이 대두된 지",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100020000.1.10",
          "form": "얼마 안 됐잖아요",
          "original_form": "얼마 안 됐잖아요",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100020000.1.11",
          "form": "그놈의 비엘은 아주 살판났드만ㅡㅡ",
          "original_form": "그놈의 비엘은 아주 살판났드만ㅡㅡ",
          "speaker_id": "2",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100020000.1.12",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100020000.1.13",
          "form": "여자들간의 사랑도 존중받는게 우선인거같아요",
          "original_form": "여자들간의 사랑도 존중받는게 우선인거같아요",
          "speaker_id": "1",
          "time": "20210827 23:13"
        },
        {
          "id": "MDRW2100020000.1.14",
          "form": "우리끼리 검열하기보단..",
          "original_form": "우리끼리 검열하기보단..",
          "speaker_id": "1",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100020000.1.15",
          "form": "연인들 사이에서 치이는 사람은 좀 배려해주되ㅋㅋㅋㅋ",
          "original_form": "연인들 사이에서 치이는 사람은 좀 배려해주되ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 23:14"
        },
        {
          "id": "MDRW2100020000.1.16",
          "form": "맞아욬ㅋㅋㅋㅋㅋㅋ 비연애입니다 써붙이고다니고싶어요..",
          "original_form": "맞아욬ㅋㅋㅋㅋㅋㅋ 비연애입니다 써붙이고다니고싶어요..",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020000.1.17",
          "form": "본인들끼리 사랑을 연구해보겠다고 한다면 말리진 않을 것 같습니다",
          "original_form": "본인들끼리 사랑을 연구해보겠다고 한다면 말리진 않을 것 같습니다",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020000.1.18",
          "form": "ㅋㅋㅋㅋㅋ저두요",
          "original_form": "ㅋㅋㅋㅋㅋ저두요",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020000.1.19",
          "form": "저야 이렇게 됐다 쳐도",
          "original_form": "저야 이렇게 됐다 쳐도",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020000.1.20",
          "form": "본인들끼맄ㅋㅋㅋㅋㅋㅋ",
          "original_form": "본인들끼맄ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020000.1.21",
          "form": "ㅋㅋㅋㅋ남들이 제 머리를 열어보지 않는 한은",
          "original_form": "ㅋㅋㅋㅋ남들이 제 머리를 열어보지 않는 한은",
          "speaker_id": "2",
          "time": "20210827 23:15"
        },
        {
          "id": "MDRW2100020000.1.22",
          "form": "제가 비연애인걸 모르잖아요",
          "original_form": "제가 비연애인걸 모르잖아요",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100020000.1.23",
          "form": "맞아요 막 플러팅하고.. 연애 전시하고..",
          "original_form": "맞아요 막 플러팅하고.. 연애 전시하고..",
          "speaker_id": "1",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100020000.1.24",
          "form": "그러니까요 흑",
          "original_form": "그러니까요 흑",
          "speaker_id": "2",
          "time": "20210827 23:16"
        },
        {
          "id": "MDRW2100020000.1.25",
          "form": "환/장하것습니다",
          "original_form": "환/장하것습니다",
          "speaker_id": "1",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020000.1.26",
          "form": "name1님은 친구분이랑 다시 회복하실 수 없을까요?",
          "original_form": "&name1&님은 친구분이랑 다시 회복하실 수 없을까요?",
          "speaker_id": "2",
          "time": "20210827 23:17"
        },
        {
          "id": "MDRW2100020000.1.27",
          "form": "친구분이 대화가 된다믄 괜찮을 것 같은디",
          "original_form": "친구분이 대화가 된다믄 괜찮을 것 같은디",
