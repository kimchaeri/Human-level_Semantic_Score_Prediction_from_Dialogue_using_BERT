      "utterance": [
        {
          "id": "MDRW2100015978.1.1",
          "form": "너는 무슨 음식을 제일 좋아해?",
          "original_form": "너는 무슨 음식을 제일 좋아해?",
          "speaker_id": "2",
          "time": "20210826 04:39"
        },
        {
          "id": "MDRW2100015978.1.2",
          "form": "나는 지금 연어회가먹고싶어",
          "original_form": "나는 지금 연어회가먹고싶어",
          "speaker_id": "1",
          "time": "20210826 04:39"
        },
        {
          "id": "MDRW2100015978.1.3",
          "form": "나는 지금 떡볶이가 너무 먹고싶다",
          "original_form": "나는 지금 떡볶이가 너무 먹고싶다",
          "speaker_id": "2",
          "time": "20210826 04:40"
        },
        {
          "id": "MDRW2100015978.1.4",
          "form": "나도 떡볶이 좋아해",
          "original_form": "나도 떡볶이 좋아해",
          "speaker_id": "1",
          "time": "20210826 04:40"
        },
        {
          "id": "MDRW2100015978.1.5",
          "form": "로제 떡볶이 먹어봤어?",
          "original_form": "로제 떡볶이 먹어봤어?",
          "speaker_id": "2",
          "time": "20210826 04:40"
        },
        {
          "id": "MDRW2100015978.1.6",
          "form": "한 번 먹어봤어! 엽기떡볶이에서",
          "original_form": "한 번 먹어봤어! 엽기떡볶이에서",
          "speaker_id": "1",
          "time": "20210826 04:41"
        },
        {
          "id": "MDRW2100015978.1.7",
          "form": "오 배떡 에서 파는 로제 떡볶이 도 맛있더라고",
          "original_form": "오 배떡 에서 파는 로제 떡볶이 도 맛있더라고",
          "speaker_id": "2",
          "time": "20210826 04:41"
        },
        {
          "id": "MDRW2100015978.1.8",
          "form": "배떡에 신메뉴도 있다던데",
          "original_form": "배떡에 신메뉴도 있다던데",
          "speaker_id": "1",
          "time": "20210826 04:41"
        },
        {
          "id": "MDRW2100015978.1.9",
          "form": "아 떡볶이에 핫도그 찍어먹으면 맛있대",
          "original_form": "아 떡볶이에 핫도그 찍어먹으면 맛있대",
          "speaker_id": "2",
          "time": "20210826 04:42"
        },
        {
          "id": "MDRW2100015978.1.10",
          "form": "하긴 어울릴것 같긴해  해먹어봤어?",
          "original_form": "하긴 어울릴것 같긴해  해먹어봤어?",
          "speaker_id": "1",
          "time": "20210826 04:42"
        },
        {
          "id": "MDRW2100015978.1.11",
          "form": "나도 아직 안먹어봐서 다음에 시도해보려고",
          "original_form": "나도 아직 안먹어봐서 다음에 시도해보려고",
          "speaker_id": "2",
          "time": "20210826 04:43"
        },
        {
          "id": "MDRW2100015978.1.12",
          "form": "핫도그 하니까 통모짜먹고싶다",
          "original_form": "핫도그 하니까 통모짜먹고싶다",
          "speaker_id": "1",
          "time": "20210826 04:43"
        },
        {
          "id": "MDRW2100015978.1.13",
          "form": "아 명랑 핫도그 맛있는데",
          "original_form": "아 명랑 핫도그 맛있는데",
          "speaker_id": "2",
          "time": "20210826 04:43"
        },
        {
          "id": "MDRW2100015978.1.14",
          "form": "소스가 다양해서맛있어",
          "original_form": "소스가 다양해서맛있어",
          "speaker_id": "1",
          "time": "20210826 04:44"
        },
        {
          "id": "MDRW2100015978.1.15",
          "form": "칠리소스에 치즈 가루 잔뜩 뿌려먹고싶다",
          "original_form": "칠리소스에 치즈 가루 잔뜩 뿌려먹고싶다",
          "speaker_id": "2",
          "time": "20210826 04:44"
        },
        {
          "id": "MDRW2100015978.1.16",
          "form": "먹으러가자",
          "original_form": "먹으러가자",
          "speaker_id": "1",
          "time": "20210826 04:44"
        }
      ]
