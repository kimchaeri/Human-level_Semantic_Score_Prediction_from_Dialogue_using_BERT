      "utterance": [
        {
          "id": "MDRW2100000554.1.1",
          "form": "동호회 다니는데 있으세요?",
          "original_form": "동호회 다니는데 있으세요?",
          "speaker_id": "2",
          "time": "20210820 21:06"
        },
        {
          "id": "MDRW2100000554.1.2",
          "form": "동호회는 다니는 곳이 없어요ㅜ name2님은 있으세요?",
          "original_form": "동호회는 다니는 곳이 없어요ㅜ &name2&님은 있으세요?",
          "speaker_id": "1",
          "time": "20210820 23:03"
        },
        {
          "id": "MDRW2100000554.1.3",
          "form": "동호회까지는 아니구 친구들이랑 모임 하고있어요",
          "original_form": "동호회까지는 아니구 친구들이랑 모임 하고있어요",
          "speaker_id": "2",
          "time": "20210820 23:05"
        },
        {
          "id": "MDRW2100000554.1.4",
          "form": "술모임에 운동 모임이라 모임명은 주짓수입니다",
          "original_form": "술모임에 운동 모임이라 모임명은 주짓수입니다",
          "speaker_id": "2",
          "time": "20210820 23:05"
        },
        {
          "id": "MDRW2100000554.1.5",
          "form": "ㅋㅋㅋㅋ오ㅋㅋ저는 코로나 끝나고 나면 볼링 동호회를 들어가볼까해요",
          "original_form": "ㅋㅋㅋㅋ오ㅋㅋ저는 코로나 끝나고 나면 볼링 동호회를 들어가볼까해요",
          "speaker_id": "1",
          "time": "20210820 23:14"
        },
        {
          "id": "MDRW2100000554.1.6",
          "form": "볼링 몇점정도 치세요?",
          "original_form": "볼링 몇점정도 치세요?",
          "speaker_id": "2",
          "time": "20210820 23:23"
        },
        {
          "id": "MDRW2100000554.1.7",
          "form": "볼링 평균 130정도요ㅋㅋㅋ최고로 나왔을땐 177...",
          "original_form": "볼링 평균 130정도요ㅋㅋㅋ최고로 나왔을땐 177...",
          "speaker_id": "1",
          "time": "20210820 23:24"
        },
        {
          "id": "MDRW2100000554.1.8",
          "form": "오...",
          "original_form": "오...",
          "speaker_id": "2",
          "time": "20210820 23:27"
        },
        {
          "id": "MDRW2100000554.1.9",
          "form": "저는 70점대였던것같은데",
          "original_form": "저는 70점대였던것같은데",
          "speaker_id": "2",
          "time": "20210820 23:27"
        },
        {
          "id": "MDRW2100000554.1.10",
          "form": "쌉고수시네여",
          "original_form": "쌉고수시네여",
          "speaker_id": "2",
          "time": "20210820 23:27"
        },
        {
          "id": "MDRW2100000554.1.11",
          "form": "ㅋㅋㅋ볼링은 조금 합니다!ㅋㅋ",
          "original_form": "ㅋㅋㅋ볼링은 조금 합니다!ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 00:00"
        },
        {
          "id": "MDRW2100000554.1.12",
          "form": "볼링 동호회 예전에 다녀본적 있어요?",
          "original_form": "볼링 동호회 예전에 다녀본적 있어요?",
          "speaker_id": "2",
          "time": "20210821 12:56"
        },
        {
          "id": "MDRW2100000554.1.13",
          "form": "아뇽ㅋㅋㅋ동호회는 아닌데 지인들끼리 소소하게 다닌적은 있죠",
          "original_form": "아뇽ㅋㅋㅋ동호회는 아닌데 지인들끼리 소소하게 다닌적은 있죠",
          "speaker_id": "1",
          "time": "20210821 13:17"
        },
        {
          "id": "MDRW2100000554.1.14",
          "form": "그정도면 인정해드릴게요",
          "original_form": "그정도면 인정해드릴게요",
          "speaker_id": "2",
          "time": "20210821 13:18"
        },
        {
          "id": "MDRW2100000554.1.15",
          "form": "근데 취미도 약간 으르신 취미시네요",
          "original_form": "근데 취미도 약간 으르신 취미시네요",
          "speaker_id": "2",
          "time": "20210821 13:19"
        },
        {
          "id": "MDRW2100000554.1.16",
          "form": "볼링을 좋아하는 곰국 매니아",
          "original_form": "볼링을 좋아하는 곰국 매니아",
          "speaker_id": "2",
          "time": "20210821 13:19"
        },
        {
          "id": "MDRW2100000554.1.17",
          "form": "아니!! 볼링이 왜 으르신취미에요!!!",
          "original_form": "아니!! 볼링이 왜 으르신취미에요!!!",
          "speaker_id": "1",
          "time": "20210821 13:23"
        },
        {
          "id": "MDRW2100000554.1.18",
          "form": "락볼링장에 얼마나 젊은사람들이 많은데!",
          "original_form": "락볼링장에 얼마나 젊은사람들이 많은데!",
          "speaker_id": "1",
          "time": "20210821 13:24"
        },
        {
          "id": "MDRW2100000554.1.19",
          "form": "락볼링장이라니",
          "original_form": "락볼링장이라니",
          "speaker_id": "2",
          "time": "20210821 13:27"
        },
        {
          "id": "MDRW2100000554.1.20",
          "form": "그거 13년도에 유행한거 아닙니까!",
          "original_form": "그거 13년도에 유행한거 아닙니까!",
          "speaker_id": "2",
          "time": "20210821 13:28"
        },
        {
          "id": "MDRW2100000554.1.21",
          "form": "에? 볼링장 언제 가봤나요ㅋㅋㅋ",
          "original_form": "에? 볼링장 언제 가봤나요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 13:31"
        },
        {
          "id": "MDRW2100000554.1.22",
          "form": "코로나 유행 전 2019년도에 이대 락볼링장에 1020대가 수두룩했는데!",
          "original_form": "코로나 유행 전 2019년도에 이대 락볼링장에 1020대가 수두룩했는데!",
          "speaker_id": "1",
          "time": "20210821 13:32"
        },
        {
          "id": "MDRW2100000554.1.23",
          "form": "제 기억속의 볼링장은",
          "original_form": "제 기억속의 볼링장은",
          "speaker_id": "2",
          "time": "20210821 13:34"
        },
        {
          "id": "MDRW2100000554.1.24",
          "form": "아저씨 아주머니들이 손에 가재 장갑 끼고",
          "original_form": "아저씨 아주머니들이 손에 가재 장갑 끼고",
          "speaker_id": "2",
          "time": "20210821 13:35"
        },
        {
          "id": "MDRW2100000554.1.25",
          "form": "스핀 휙휙 넣으면서 하는 그런 볼링장이었는데!",
          "original_form": "스핀 휙휙 넣으면서 하는 그런 볼링장이었는데!",
          "speaker_id": "2",
          "time": "20210821 13:35"
        },
        {
          "id": "MDRW2100000554.1.26",
          "form": "아이코 어르신이 요기계셨네여ㅋㅋㅋㅋ",
          "original_form": "아이코 어르신이 요기계셨네여ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 13:39"
        },
        {
          "id": "MDRW2100000554.1.27",
          "form": "치돈어르신도 볼링동호회 어때여",
          "original_form": "치돈어르신도 볼링동호회 어때여",
          "speaker_id": "1",
          "time": "20210821 13:40"
        },
        {
          "id": "MDRW2100000554.1.28",
          "form": "운동좋아하시는거같은데",
          "original_form": "운동좋아하시는거같은데",
          "speaker_id": "1",
          "time": "20210821 13:40"
        },
        {
          "id": "MDRW2100000554.1.29",
          "form": "볼링은 좀 그래요...",
          "original_form": "볼링은 좀 그래요...",
          "speaker_id": "2",
          "time": "20210821 13:42"
        },
        {
          "id": "MDRW2100000554.1.30",
          "form": "볼링은 길쭉하고 잘생기고 예쁜 사람이 치는거 아닌가여",
          "original_form": "볼링은 길쭉하고 잘생기고 예쁜 사람이 치는거 아닌가여",
          "speaker_id": "2",
          "time": "20210821 13:42"
        },
        {
          "id": "MDRW2100000554.1.31",
          "form": "그렇지도않아요ㅋㅋㅋㅋ",
          "original_form": "그렇지도않아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 13:51"
        },
        {
          "id": "MDRW2100000554.1.32",
          "form": "동호회나 모임 갖는다고하면 어떤 곳 들어가고픈가오",
          "original_form": "동호회나 모임 갖는다고하면 어떤 곳 들어가고픈가오",
          "speaker_id": "1",
          "time": "20210821 13:53"
        },
        {
          "id": "MDRW2100000554.1.33",
          "form": "요즘 등산이 땡기네요",
          "original_form": "요즘 등산이 땡기네요",
          "speaker_id": "2",
          "time": "20210821 13:54"
        },
        {
          "id": "MDRW2100000554.1.34",
          "form": "등산동호회?",
          "original_form": "등산동호회?",
          "speaker_id": "1",
          "time": "20210821 13:55"
        },
        {
          "id": "MDRW2100000554.1.35",
          "form": "등산동호회 엄마아빠 나이대 분들이 많던데!",
          "original_form": "등산동호회 엄마아빠 나이대 분들이 많던데!",
          "speaker_id": "1",
          "time": "20210821 13:55"
        },
        {
          "id": "MDRW2100000554.1.36",
          "form": "등산하고 국밥먹고 볼링치러 많이 가시죠",
          "original_form": "등산하고 국밥먹고 볼링치러 많이 가시죠",
          "speaker_id": "2",
          "time": "20210821 13:56"
        },
        {
          "id": "MDRW2100000554.1.37",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ국밥 볼링",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ국밥 볼링",
          "speaker_id": "1",
          "time": "20210821 13:58"
        },
        {
          "id": "MDRW2100000554.1.38",
          "form": "국밥동호회도 있으려나...",
          "original_form": "국밥동호회도 있으려나...",
