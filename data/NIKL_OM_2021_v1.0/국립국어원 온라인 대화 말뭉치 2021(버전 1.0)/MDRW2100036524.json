      "utterance": [
        {
          "id": "MDRW2100036524.1.1",
          "form": "너 언제자?",
          "original_form": "너 언제자?",
          "speaker_id": "2",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100036524.1.2",
          "form": "건강을 위해선 수면이 중요하대",
          "original_form": "건강을 위해선 수면이 중요하대",
          "speaker_id": "2",
          "time": "20210831 00:06"
        },
        {
          "id": "MDRW2100036524.1.3",
          "form": "나 머리를 자르고싶어",
          "original_form": "나 머리를 자르고싶어",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100036524.1.4",
          "form": "추천하는\n미용실있어?",
          "original_form": "추천하는\n미용실있어?",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100036524.1.5",
          "form": "정말?-",
          "original_form": "정말?-",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100036524.1.6",
          "form": "아니 난 업ㄱ어",
          "original_form": "아니 난 업ㄱ어",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100036524.1.7",
          "form": "미용실을 자주 안가",
          "original_form": "미용실을 자주 안가",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100036524.1.8",
          "form": "단발로 머리 자를거야?",
          "original_form": "단발로 머리 자를거야?",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100036524.1.9",
          "form": "단골 미용실 만들기 너무 힘들다",
          "original_form": "단골 미용실 만들기 너무 힘들다",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100036524.1.10",
          "form": "겨울이라 추울텐데...",
          "original_form": "겨울이라 추울텐데...",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100036524.1.11",
          "form": "그리고 요즘 미용실 너무 비싸",
          "original_form": "그리고 요즘 미용실 너무 비싸",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100036524.1.12",
          "form": "그렇지않니?",
          "original_form": "그렇지않니?",
          "speaker_id": "1",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100036524.1.13",
          "form": "하긴 단발이 더 좋긴 해",
          "original_form": "하긴 단발이 더 좋긴 해",
          "speaker_id": "2",
          "time": "20210831 00:11"
        },
        {
          "id": "MDRW2100036524.1.14",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036524.1.15",
          "form": "너는 지금 머리길이가 어느정도야?",
          "original_form": "너는 지금 머리길이가 어느정도야?",
          "speaker_id": "1",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036524.1.16",
          "form": "삼천원이었르면 졸겠어",
          "original_form": "삼천원이었르면 졸겠어",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036524.1.17",
          "form": "나는 쇄골아래야",
          "original_form": "나는 쇄골아래야",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036524.1.18",
          "form": "너는?",
          "original_form": "너는?",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036524.1.19",
          "form": "삼천원이면 매일갈꺼야",
          "original_form": "삼천원이면 매일갈꺼야",
          "speaker_id": "1",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036524.1.20",
          "form": "나는 가슴까지와",
          "original_form": "나는 가슴까지와",
          "speaker_id": "1",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036524.1.21",
          "form": "머리감겨달라할거야?",
          "original_form": "머리감겨달라할거야?",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036524.1.22",
          "form": "길구나",
          "original_form": "길구나",
          "speaker_id": "2",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036524.1.23",
          "form": "응 당연하지",
          "original_form": "응 당연하지",
          "speaker_id": "1",
          "time": "20210831 00:12"
        },
        {
          "id": "MDRW2100036524.1.24",
          "form": "머리빠지면 감당이안돼",
          "original_form": "머리빠지면 감당이안돼",
          "speaker_id": "2",
          "time": "20210831 00:12"
        }
      ]
    }
