      "utterance": [
        {
          "id": "MDRW2100042791.1.1",
          "form": "대전에는 성심당 ? 유명하져",
          "original_form": "대전에는 성심당 ? 유명하져",
          "speaker_id": "1",
          "time": "20210907 09:11"
        },
        {
          "id": "MDRW2100042791.1.2",
          "form": "저도 따뜻한커피한잔해야겠네요",
          "original_form": "저도 따뜻한커피한잔해야겠네요",
          "speaker_id": "2",
          "time": "20210907 09:11"
        },
        {
          "id": "MDRW2100042791.1.3",
          "form": "네..완전유명해요",
          "original_form": "네..완전유명해요",
          "speaker_id": "2",
          "time": "20210907 09:11"
        },
        {
          "id": "MDRW2100042791.1.4",
          "form": "한두번 맛있게 먹었어요",
          "original_form": "한두번 맛있게 먹었어요",
          "speaker_id": "1",
          "time": "20210907 09:12"
        },
        {
          "id": "MDRW2100042791.1.5",
          "form": "그동네 분들은 안가시겠져",
          "original_form": "그동네 분들은 안가시겠져",
          "speaker_id": "1",
          "time": "20210907 09:12"
        },
        {
          "id": "MDRW2100042791.1.6",
          "form": "네..",
          "original_form": "네..",
          "speaker_id": "2",
          "time": "20210907 09:12"
        },
        {
          "id": "MDRW2100042791.1.7",
          "form": "전 튀긴거라 좀느끼해서 다른빵사먹어요",
          "original_form": "전 튀긴거라 좀느끼해서 다른빵사먹어요",
          "speaker_id": "2",
          "time": "20210907 09:12"
        },
        {
          "id": "MDRW2100042791.1.8",
          "form": "다른빵도 맛있나요",
          "original_form": "다른빵도 맛있나요",
          "speaker_id": "1",
          "time": "20210907 09:13"
        },
        {
          "id": "MDRW2100042791.1.9",
          "form": "부추고로케빵 이런거도 많이 사시던대",
          "original_form": "부추고로케빵 이런거도 많이 사시던대",
          "speaker_id": "1",
          "time": "20210907 09:13"
        },
        {
          "id": "MDRW2100042791.1.10",
          "form": "전계피빵 좋아해요..쫀득거려서",
          "original_form": "전계피빵 좋아해요..쫀득거려서",
          "speaker_id": "2",
          "time": "20210907 09:13"
        },
        {
          "id": "MDRW2100042791.1.11",
          "form": "맞아요..부추빵도 많이사요",
          "original_form": "맞아요..부추빵도 많이사요",
          "speaker_id": "2",
          "time": "20210907 09:13"
        },
        {
          "id": "MDRW2100042791.1.12",
          "form": "잘아시네요!!!",
          "original_form": "잘아시네요!!!",
          "speaker_id": "2",
          "time": "20210907 09:13"
        },
        {
          "id": "MDRW2100042791.1.13",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210907 09:13"
        },
        {
          "id": "MDRW2100042791.1.14",
          "form": "저는 호밀빵 곡물빵 좋아해서",
          "original_form": "저는 호밀빵 곡물빵 좋아해서",
          "speaker_id": "1",
          "time": "20210907 09:14"
        },
        {
          "id": "MDRW2100042791.1.15",
          "form": "건강식이네요",
          "original_form": "건강식이네요",
          "speaker_id": "2",
          "time": "20210907 09:14"
        },
        {
          "id": "MDRW2100042791.1.16",
          "form": "맛이 강하면 잘 안먹게 되더라구요",
          "original_form": "맛이 강하면 잘 안먹게 되더라구요",
          "speaker_id": "1",
          "time": "20210907 09:14"
        },
        {
          "id": "MDRW2100042791.1.17",
          "form": "담백한맛~~",
          "original_form": "담백한맛~~",
          "speaker_id": "2",
          "time": "20210907 09:14"
        },
        {
          "id": "MDRW2100042791.1.18",
          "form": "빵을 간식으론 잘 안먹어요",
          "original_form": "빵을 간식으론 잘 안먹어요",
          "speaker_id": "1",
          "time": "20210907 09:14"
        },
        {
          "id": "MDRW2100042791.1.19",
          "form": "주식으로 드시나봐요",
          "original_form": "주식으로 드시나봐요",
          "speaker_id": "2",
          "time": "20210907 09:14"
        },
        {
          "id": "MDRW2100042791.1.20",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 09:14"
        },
        {
          "id": "MDRW2100042791.1.21",
          "form": "최근 배달 음식은 뭐 드셨어요",
          "original_form": "최근 배달 음식은 뭐 드셨어요",
          "speaker_id": "1",
          "time": "20210907 09:15"
        },
        {
          "id": "MDRW2100042791.1.22",
          "form": "주식으로 드시기엔 호밀빵이좋죠",
          "original_form": "주식으로 드시기엔 호밀빵이좋죠",
