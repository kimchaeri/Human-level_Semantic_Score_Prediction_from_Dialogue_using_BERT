      "utterance": [
        {
          "id": "MDRW2100007322.1.1",
          "form": "맞아요ㅋㅋㅋㅋㅋㅋㅜㅜ 너무 어려워요",
          "original_form": "맞아요ㅋㅋㅋㅋㅋㅋㅜㅜ 너무 어려워요",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007322.1.2",
          "form": "맨날 15분하고 누워있어요",
          "original_form": "맨날 15분하고 누워있어요",
          "speaker_id": "2",
          "time": "20210822 01:22"
        },
        {
          "id": "MDRW2100007322.1.3",
          "form": "저랑 되게 비슷하시다 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저랑 되게 비슷하시다 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 01:23"
        },
        {
          "id": "MDRW2100007322.1.4",
          "form": "피트니스복싱 잘 맞으실 것 같아요",
          "original_form": "피트니스복싱 잘 맞으실 것 같아요",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007322.1.5",
          "form": "ㅋㅋㅋㅋㅋㅋ한번 사봐야겠어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ한번 사봐야겠어요",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007322.1.6",
          "form": "당근마켓에서 한번 찾아봐보셔요",
          "original_form": "당근마켓에서 한번 찾아봐보셔요",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007322.1.7",
          "form": "저렴하게 구매해서 해보면 좋잖아요~",
          "original_form": "저렴하게 구매해서 해보면 좋잖아요~",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007322.1.8",
          "form": "오징어의 끈기 보여드리겠습니다",
          "original_form": "오징어의 끈기{emoji:✊} 보여드리겠습니다",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007322.1.9",
          "form": "맞아요 당근마켓 넘 좋아요... 싸고.. 환경도 아끼고~",
          "original_form": "맞아요 당근마켓 넘 좋아요... 싸고.. 환경도 아끼고~",
          "speaker_id": "2",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007322.1.10",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 최근에 2가 나와서 1은 2만원대면 구매가능할거에요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 최근에 2가 나와서 1은 2만원대면 구매가능할거에요",
          "speaker_id": "1",
          "time": "20210822 01:24"
        },
        {
          "id": "MDRW2100007322.1.11",
          "form": "헐! 스위치 치고는 엄청 싸네요",
          "original_form": "헐! 스위치 치고는 엄청 싸네요",
          "speaker_id": "2",
          "time": "20210822 01:25"
        },
        {
          "id": "MDRW2100007322.1.12",
          "form": "네 링피트는 되게 비싼데",
          "original_form": "네 링피트는 되게 비싼데",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007322.1.13",
          "form": "그거에 비해서 되게 싸요~",
          "original_form": "그거에 비해서 되게 싸요~",
          "speaker_id": "1",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007322.1.14",
          "form": "링피트 예전에 8마넌 대 하던 때 꽂혀서...",
          "original_form": "링피트 예전에 8마넌 대 하던 때 꽂혀서...",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007322.1.15",
          "form": "7만 5처넌 중고로 구매했었어요",
          "original_form": "7만 5처넌 중고로 구매했었어요",
          "speaker_id": "2",
          "time": "20210822 01:26"
        },
        {
          "id": "MDRW2100007322.1.16",
          "form": "쫌만 더 참다 살걸...ㅜㅜ 흑",
          "original_form": "쫌만 더 참다 살걸...ㅜㅜ 흑",
          "speaker_id": "2",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007322.1.17",
          "form": "링피트가 한창 유행했을ㄸ거",
          "original_form": "링피트가 한창 유행했을ㄸ거",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007322.1.18",
          "form": "때 정말 비쌌죠",
          "original_form": "때 정말 비쌌죠",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007322.1.19",
          "form": "신품 품절대란 나서 온라인에서 두배가격으로 팔기도 하고",
          "original_form": "신품 품절대란 나서 온라인에서 두배가격으로 팔기도 하고",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007322.1.20",
          "form": "뭐 지금도 비싸지만요",
          "original_form": "뭐 지금도 비싸지만요",
          "speaker_id": "1",
          "time": "20210822 01:27"
        },
        {
          "id": "MDRW2100007322.1.21",
          "form": "맞아요...",
          "original_form": "맞아요...",
          "speaker_id": "2",
          "time": "20210822 01:28"
        },
        {
          "id": "MDRW2100007322.1.22",
          "form": "저는 올해 초? 즈음에",
          "original_form": "저는 올해 초? 즈음에",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007322.1.23",
          "form": "롯데마트 온라인몰에서 구매했었어요 링피트",
          "original_form": "롯데마트 온라인몰에서 구매했었어요 링피트",
          "speaker_id": "1",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007322.1.24",
          "form": "오!",
          "original_form": "오!",
          "speaker_id": "2",
          "time": "20210822 01:29"
        },
        {
          "id": "MDRW2100007322.1.25",
          "form": "맞다! 저희쪽 홈플에는 링피트 칩 할인 행사하던데",
          "original_form": "맞다! 저희쪽 홈플에는 링피트 칩 할인 행사하던데",
          "speaker_id": "2",
