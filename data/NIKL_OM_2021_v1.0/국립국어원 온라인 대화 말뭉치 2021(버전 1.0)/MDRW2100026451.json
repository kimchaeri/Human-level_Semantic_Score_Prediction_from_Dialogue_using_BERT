      "utterance": [
        {
          "id": "MDRW2100026451.1.1",
          "form": "네 이제 유산균 쇼핑해야겠어욬ㅋㅋ",
          "original_form": "네 이제 유산균 쇼핑해야겠어욬ㅋㅋ",
          "speaker_id": "1",
          "time": "20210901 22:19"
        },
        {
          "id": "MDRW2100026451.1.2",
          "form": "쇼핑할 때 주로 이용하는 사이트 있나요?",
          "original_form": "쇼핑할 때 주로 이용하는 사이트 있나요?",
          "speaker_id": "2",
          "time": "20210902 19:29"
        },
        {
          "id": "MDRW2100026451.1.3",
          "form": "저 지마켓 옥션 제일많이써요",
          "original_form": "저 지마켓 옥션 제일많이써요",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100026451.1.4",
          "form": "그담 쿠팡 11번가 가끔쓰구요",
          "original_form": "그담 쿠팡 11번가 가끔쓰구요",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100026451.1.5",
          "form": "신세계몰도 종종 지에스샵도 종종",
          "original_form": "신세계몰도 종종 지에스샵도 종종",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100026451.1.6",
          "form": "엄청 많죠 쇼핑하는 사이트 ㅋㅋㅋ",
          "original_form": "엄청 많죠 쇼핑하는 사이트 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 19:50"
        },
        {
          "id": "MDRW2100026451.1.7",
          "form": "정말 다양하게 이용하시네요 전 정해진 몇 곳만 사는 것 같아요",
          "original_form": "정말 다양하게 이용하시네요 전 정해진 몇 곳만 사는 것 같아요",
          "speaker_id": "2",
          "time": "20210902 19:52"
        },
        {
          "id": "MDRW2100026451.1.8",
          "form": "최근에 지에스샵이 적립금 잘 줘서 구매를 자주 했어요",
          "original_form": "최근에 지에스샵이 적립금 잘 줘서 구매를 자주 했어요",
          "speaker_id": "2",
          "time": "20210902 19:53"
        },
        {
          "id": "MDRW2100026451.1.9",
          "form": "전엄청 많이써요^^; 지구도쓰고 개인몰도사고...",
          "original_form": "전엄청 많이써요^^; 지구도쓰고 개인몰도사고...",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100026451.1.10",
          "form": "ㅎㅎ맞아요 저도 그래서 지에스샵 진짜 잘이용했네요^^",
          "original_form": "ㅎㅎ맞아요 저도 그래서 지에스샵 진짜 잘이용했네요^^",
          "speaker_id": "1",
          "time": "20210902 20:54"
        },
        {
          "id": "MDRW2100026451.1.11",
          "form": "쇼핑 고수이신 것 같네요",
          "original_form": "쇼핑 고수이신 것 같네요",
          "speaker_id": "2",
          "time": "20210902 20:57"
        },
        {
          "id": "MDRW2100026451.1.12",
          "form": "고수는 아니에요ㅜㅜ",
          "original_form": "고수는 아니에요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100026451.1.13",
          "form": "뭘하나 사려고하면 검색에 검색을 거쳐서",
          "original_form": "뭘하나 사려고하면 검색에 검색을 거쳐서",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100026451.1.14",
          "form": "아주 피곤하죠ㅠ",
          "original_form": "아주 피곤하죠ㅠ",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100026451.1.15",
          "form": "제품고르기도 오래걸려~",
          "original_form": "제품고르기도 오래걸려~",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100026451.1.16",
          "form": "가격비교도 오래걸려~",
          "original_form": "가격비교도 오래걸려~",
          "speaker_id": "1",
          "time": "20210902 22:45"
        },
        {
          "id": "MDRW2100026451.1.17",
          "form": "그렇게 정착하고나면 못바꾸겠더라구요 ㅎㅎ",
          "original_form": "그렇게 정착하고나면 못바꾸겠더라구요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100026451.1.18",
          "form": "저도 그래요 특히 영양제는 바꾸기가 쉽지 않죠",
          "original_form": "저도 그래요 특히 영양제는 바꾸기가 쉽지 않죠",
          "speaker_id": "2",
          "time": "20210903 07:23"
        },
        {
          "id": "MDRW2100026451.1.19",
          "form": "맞아요 꾸준히먹어야 효과있으니까요^^",
          "original_form": "맞아요 꾸준히먹어야 효과있으니까요^^",
