      "utterance": [
        {
          "id": "MDRW2100032933.1.1",
          "form": "물로 해장 때립니다 ㅎㅎ",
          "original_form": "물로 해장 때립니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210906 09:19"
        },
        {
          "id": "MDRW2100032933.1.2",
          "form": "출근하셨나요",
          "original_form": "출근하셨나요",
          "speaker_id": "2",
          "time": "20210906 09:19"
        },
        {
          "id": "MDRW2100032933.1.3",
          "form": "네 월요일..힘드네요....ㅋㅋㅋ헐\n 물로 해장이 되나요?",
          "original_form": "네 월요일..힘드네요....ㅋㅋㅋ헐\n 물로 해장이 되나요?",
          "speaker_id": "1",
          "time": "20210906 09:27"
        },
        {
          "id": "MDRW2100032933.1.4",
          "form": "ㅋㅋ어느정도 돌아왔습니다",
          "original_form": "ㅋㅋ어느정도 돌아왔습니다",
          "speaker_id": "2",
          "time": "20210906 10:53"
        },
        {
          "id": "MDRW2100032933.1.5",
          "form": "ㅁㅋㅋㅋㅋㅋㅋㅋㅋ점심은 뭐드시나요",
          "original_form": "ㅁㅋㅋㅋㅋㅋㅋㅋㅋ점심은 뭐드시나요",
          "speaker_id": "1",
          "time": "20210906 12:43"
        },
        {
          "id": "MDRW2100032933.1.6",
          "form": "순대국으로",
          "original_form": "순대국으로",
          "speaker_id": "2",
          "time": "20210906 12:45"
        },
        {
          "id": "MDRW2100032933.1.7",
          "form": "해장 완료!!!",
          "original_form": "해장 완료!!!",
          "speaker_id": "2",
          "time": "20210906 12:45"
        },
        {
          "id": "MDRW2100032933.1.8",
          "form": "점심 뭐드세요",
          "original_form": "점심 뭐드세요",
          "speaker_id": "2",
          "time": "20210906 12:46"
        },
        {
          "id": "MDRW2100032933.1.9",
          "form": "오늘 구내식당 메뉴가 다 별로였어요ㅠㅠ",
          "original_form": "오늘 구내식당 메뉴가 다 별로였어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 12:46"
        },
        {
          "id": "MDRW2100032933.1.10",
          "form": "모둠돈가스 먹었는데 지금 너무 느끼해서 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "모둠돈가스 먹었는데 지금 너무 느끼해서 ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:46"
        },
        {
          "id": "MDRW2100032933.1.11",
          "form": "어떻게해야할지 고민중입니다 ㅋㅋㅋ",
          "original_form": "어떻게해야할지 고민중입니다 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210906 12:46"
        },
        {
          "id": "MDRW2100032933.1.12",
          "form": "그럴땐 탄산을 딱",
          "original_form": "그럴땐 탄산을 딱",
          "speaker_id": "2",
          "time": "20210906 12:47"
        },
        {
          "id": "MDRW2100032933.1.13",
          "form": "아악 그쵸 탄산이 딱인데",
          "original_form": "아악 그쵸 탄산이 딱인데",
          "speaker_id": "1",
          "time": "20210906 12:48"
        },
        {
          "id": "MDRW2100032933.1.14",
          "form": "위가약해서 또 탄산을 못먹어요ㅠㅠ",
          "original_form": "위가약해서 또 탄산을 못먹어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210906 12:48"
        },
        {
          "id": "MDRW2100032933.1.15",
          "form": "아 위가 약하시군요ㅠ",
          "original_form": "아 위가 약하시군요ㅠ",
          "speaker_id": "2",
          "time": "20210906 12:49"
        },
        {
          "id": "MDRW2100032933.1.16",
          "form": "그럼 우유먹고 탄산드세요",
          "original_form": "그럼 우유먹고 탄산드세요",
          "speaker_id": "2",
          "time": "20210906 12:49"
        },
        {
          "id": "MDRW2100032933.1.17",
          "form": "우유로 코팅",
          "original_form": "우유로 코팅",
          "speaker_id": "2",
          "time": "20210906 12:49"
        },
        {
          "id": "MDRW2100032933.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
