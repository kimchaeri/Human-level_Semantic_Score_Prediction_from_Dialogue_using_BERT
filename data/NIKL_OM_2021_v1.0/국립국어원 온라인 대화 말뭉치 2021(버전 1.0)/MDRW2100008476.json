      "utterance": [
        {
          "id": "MDRW2100008476.1.1",
          "form": "전 이제 배달음식 진짜 끊게요",
          "original_form": "전 이제 배달음식 진짜 끊게요",
          "speaker_id": "2",
          "time": "20210822 16:19"
        },
        {
          "id": "MDRW2100008476.1.2",
          "form": "자주시켜드셨어요?",
          "original_form": "자주시켜드셨어요?",
          "speaker_id": "1",
          "time": "20210822 16:22"
        },
        {
          "id": "MDRW2100008476.1.3",
          "form": "배민 주식이라도 살걸 싶을정도로요 ㅋㅋ",
          "original_form": "배민 주식이라도 살걸 싶을정도로요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:31"
        },
        {
          "id": "MDRW2100008476.1.4",
          "form": "주로 뭐시켜드세요?ㅎ",
          "original_form": "주로 뭐시켜드세요?ㅎ",
          "speaker_id": "1",
          "time": "20210822 16:32"
        },
        {
          "id": "MDRW2100008476.1.5",
          "form": "피자랑",
          "original_form": "피자랑",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008476.1.6",
          "form": "분식이랑",
          "original_form": "분식이랑",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008476.1.7",
          "form": "와플이랑",
          "original_form": "와플이랑",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008476.1.8",
          "form": "요새 없는게 없어오 ㅋ",
          "original_form": "요새 없는게 없어오 ㅋ",
          "speaker_id": "2",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008476.1.9",
          "form": "와플도 배달이 돼요?ㅎ",
          "original_form": "와플도 배달이 돼요?ㅎ",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008476.1.10",
          "form": "커피도 오니 요즘은ㅋ",
          "original_form": "커피도 오니 요즘은ㅋ",
          "speaker_id": "1",
          "time": "20210822 16:34"
        },
        {
          "id": "MDRW2100008476.1.11",
          "form": "피자는 어디꺼 시켜드세요?",
          "original_form": "피자는 어디꺼 시켜드세요?",
          "speaker_id": "1",
          "time": "20210822 16:35"
        },
        {
          "id": "MDRW2100008476.1.12",
          "form": "와플칸 맛있어요",
          "original_form": "와플칸 맛있어요",
          "speaker_id": "2",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008476.1.13",
          "form": "피자는 여기저기 다 찔러본거같아요 ㅋㅋ",
          "original_form": "피자는 여기저기 다 찔러본거같아요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:36"
        },
        {
          "id": "MDRW2100008476.1.14",
          "form": "ㅎㅎ 혼자 시켜드세요? 전다못먹어서 자주못시키겠던데",
          "original_form": "ㅎㅎ 혼자 시켜드세요? 전다못먹어서 자주못시키겠던데",
          "speaker_id": "1",
          "time": "20210822 16:37"
        },
        {
          "id": "MDRW2100008476.1.15",
          "form": "저는 사실엄청 대식가라서요 사실 위도 줄여야해요",
          "original_form": "저는 사실엄청 대식가라서요 사실 위도 줄여야해요",
          "speaker_id": "2",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008476.1.16",
          "form": "웬만한 남자보다 많이먹어서요",
          "original_form": "웬만한 남자보다 많이먹어서요",
          "speaker_id": "2",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008476.1.17",
          "form": "아하 ㅎ 잘드시는군요",
          "original_form": "아하 ㅎ 잘드시는군요",
          "speaker_id": "1",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008476.1.18",
          "form": "그럼 피자한판은  거뜬히?",
          "original_form": "그럼 피자한판은  거뜬히?",
          "speaker_id": "1",
          "time": "20210822 16:41"
        },
        {
          "id": "MDRW2100008476.1.19",
          "form": "거뜬하죠 자랑은 아니지만",
          "original_form": "거뜬하죠 자랑은 아니지만",
          "speaker_id": "2",
          "time": "20210822 16:47"
        },
        {
          "id": "MDRW2100008476.1.20",
          "form": "햄버거 기본 두세트시키는 사람이라",
          "original_form": "햄버거 기본 두세트시키는 사람이라",
          "speaker_id": "2",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008476.1.21",
          "form": "오ㅎ  햄버거는 어디꺼 맛있나요",
          "original_form": "오ㅎ  햄버거는 어디꺼 맛있나요",
          "speaker_id": "1",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008476.1.22",
          "form": "버거킹 주로 가는데",
          "original_form": "버거킹 주로 가는데",
          "speaker_id": "1",
          "time": "20210822 16:48"
        },
        {
          "id": "MDRW2100008476.1.23",
          "form": "저도 버거킹이랑",
          "original_form": "저도 버거킹이랑",
          "speaker_id": "2",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008476.1.24",
          "form": "맘스터치도 맛있어요",
          "original_form": "맘스터치도 맛있어요",
          "speaker_id": "2",
          "time": "20210822 16:51"
        },
        {
          "id": "MDRW2100008476.1.25",
          "form": "맘스터치는 싸이버거 먹어요",
          "original_form": "맘스터치는 싸이버거 먹어요",
          "speaker_id": "1",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008476.1.26",
          "form": "자꾸 싸고 양많은거 찾아서 버거킹 앱 깔아서 많이 먹었네요",
          "original_form": "자꾸 싸고 양많은거 찾아서 버거킹 앱 깔아서 많이 먹었네요",
          "speaker_id": "2",
          "time": "20210822 16:52"
        },
        {
          "id": "MDRW2100008476.1.27",
          "form": "할인의 늪에빠져서 ㅋㅋ",
          "original_form": "할인의 늪에빠져서 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 16:52"
        }
      ]
    }
