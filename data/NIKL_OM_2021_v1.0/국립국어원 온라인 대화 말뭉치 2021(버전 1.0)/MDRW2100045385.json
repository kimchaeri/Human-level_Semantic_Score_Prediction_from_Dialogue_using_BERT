      "utterance": [
        {
          "id": "MDRW2100045385.1.1",
          "form": "컨프롱이라도 낮에 봐야하나..",
          "original_form": "컨프롱이라도 낮에 봐야하나..",
          "speaker_id": "2",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045385.1.2",
          "form": "아 그거 아직",
          "original_form": "아 그거 아직",
          "speaker_id": "2",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045385.1.3",
          "form": "스케줄 안떴어",
          "original_form": "스케줄 안떴어",
          "speaker_id": "2",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045385.1.4",
          "form": "ㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅇㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045385.1.5",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045385.1.6",
          "form": "내놔 빨리ㅡㅡ",
          "original_form": "내놔 빨리ㅡㅡ",
          "speaker_id": "2",
          "time": "20210908 02:13"
        },
        {
          "id": "MDRW2100045385.1.7",
          "form": "보고싶어 그거도",
          "original_form": "보고싶어 그거도",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.8",
          "form": "아 헤드윅",
          "original_form": "아 헤드윅",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그게",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ그게",
          "speaker_id": "2",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.10",
          "form": "오슷나올거같은데",
          "original_form": "오슷나올거같은데",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.11",
          "form": "초연작인데다",
          "original_form": "초연작인데다",
          "speaker_id": "2",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.12",
          "form": "티켓팅 미리부탁..",
          "original_form": "티켓팅 미리부탁..",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.13",
          "form": "30분 어치가 코로나때문에 썰려서",
          "original_form": "30분 어치가 코로나때문에 썰려서",
          "speaker_id": "2",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.14",
          "form": "드립니다",
          "original_form": "드립니다",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.15",
          "form": "아하...",
          "original_form": "아하...",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.16",
          "form": "왜.........",
          "original_form": "왜.........",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.17",
          "form": "처음보시면 좀 ??? 되실수도 잇는데",
          "original_form": "처음보시면 좀 ??? 되실수도 잇는데",
          "speaker_id": "2",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.18",
          "form": "제가 최대한 설명충은.. 해드리겟습니다",
          "original_form": "제가 최대한 설명충은.. 해드리겟습니다",
          "speaker_id": "2",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.19",
          "form": "네네ㅔㄴㅇ",
          "original_form": "네네ㅔㄴㅇ",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.20",
          "form": "헤드윅 오슷도 티켓팅 필요해...?",
          "original_form": "헤드윅 오슷도 티켓팅 필요해...?",
          "speaker_id": "2",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.21",
          "form": "같이볼수있ㅇ면 보아여",
          "original_form": "같이볼수있ㅇ면 보아여",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.22",
          "form": "아님 아얘",
          "original_form": "아님 아얘",
          "speaker_id": "2",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.23",
          "form": "ㅇㅇ.. 재고포비아",
          "original_form": "ㅇㅇ.. 재고포비아",
          "speaker_id": "1",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.24",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "2",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.25",
          "form": "같이 봐도 되겟더",
          "original_form": "같이 봐도 되겟더",
          "speaker_id": "2",
          "time": "20210908 02:14"
        },
        {
          "id": "MDRW2100045385.1.26",
          "form": "같이 볼 장소가",
          "original_form": "같이 볼 장소가",
          "speaker_id": "2",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045385.1.27",
          "form": "애매하긴 한데",
          "original_form": "애매하긴 한데",
          "speaker_id": "2",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045385.1.28",
          "form": "아님 전에 킹덤 볼때처럼 봐도 되고..",
          "original_form": "아님 전에 킹덤 볼때처럼 봐도 되고..",
          "speaker_id": "2",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045385.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045385.1.30",
          "form": "작업실",
          "original_form": "작업실",
          "speaker_id": "1",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045385.1.31",
          "form": "에서 봐여",
          "original_form": "에서 봐여",
          "speaker_id": "1",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045385.1.32",
          "form": "ㅋㅋㅋㅋㅋㅋ아 그래도되지",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아 그래도되지",
          "speaker_id": "1",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045385.1.33",
          "form": "마자 킹덤",
          "original_form": "마자 킹덤",
          "speaker_id": "1",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045385.1.34",
          "form": "아 왓챠도",
          "original_form": "아 왓챠도",
          "speaker_id": "2",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045385.1.35",
          "form": "그렇게보는거재밌었어",
          "original_form": "그렇게보는거재밌었어",
          "speaker_id": "1",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045385.1.36",
          "form": "티비재생 되나...?",
          "original_form": "티비재생 되나...?",
          "speaker_id": "2",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045385.1.37",
          "form": "왓챠가",
          "original_form": "왓챠가",
          "speaker_id": "1",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045385.1.38",
          "form": "같이보기가되는데",
          "original_form": "같이보기가되는데",
          "speaker_id": "1",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045385.1.39",
          "form": "서비스되는거만 가능일거야",
          "original_form": "서비스되는거만 가능일거야",
          "speaker_id": "1",
          "time": "20210908 02:15"
        },
        {
          "id": "MDRW2100045385.1.40",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "2",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045385.1.41",
          "form": "일단 올라와봐야 알겟네...",
          "original_form": "일단 올라와봐야 알겟네...",
          "speaker_id": "2",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045385.1.42",
          "form": "재밌는건 알아가지고",
          "original_form": "재밌는건 알아가지고",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045385.1.43",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045385.1.44",
          "form": "나중에 달려도 좋고",
          "original_form": "나중에 달려도 좋고",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045385.1.45",
          "form": "저번에 보면서 달리니까 재밌더라고여",
          "original_form": "저번에 보면서 달리니까 재밌더라고여",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045385.1.46",
          "form": "캉덤",
          "original_form": "캉덤",
          "speaker_id": "1",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045385.1.47",
          "form": "ㅋㅋㅋㅋㅋㅋ좋아여",
          "original_form": "ㅋㅋㅋㅋㅋㅋ좋아여",
          "speaker_id": "2",
          "time": "20210908 02:16"
        },
        {
          "id": "MDRW2100045385.1.48",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210908 02:22"
        }
      ]
