      "utterance": [
        {
          "id": "MDRW2100006038.1.1",
          "form": "괌 구석구석 여행하셨나봐요",
          "original_form": "괌 구석구석 여행하셨나봐요",
          "speaker_id": "1",
          "time": "20210907 19:31"
        },
        {
          "id": "MDRW2100006038.1.2",
          "form": "일부러는 아니구요 좋다해서 간거였어요ㅋㅋ",
          "original_form": "일부러는 아니구요 좋다해서 간거였어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006038.1.3",
          "form": "괌은 가족여행으로 가서ㅎㅎ",
          "original_form": "괌은 가족여행으로 가서ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:01"
        },
        {
          "id": "MDRW2100006038.1.4",
          "form": "저도요... 아이 있으시나요?",
          "original_form": "저도요... 아이 있으시나요?",
          "speaker_id": "1",
          "time": "20210907 21:03"
        },
        {
          "id": "MDRW2100006038.1.5",
          "form": "아뇨 혼잡니다ㅎㅎ",
          "original_form": "아뇨 혼잡니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006038.1.6",
          "form": "나이가 어떻게 되세요?",
          "original_form": "나이가 어떻게 되세요?",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006038.1.7",
          "form": "30대 후반입니나",
          "original_form": "30대 후반입니나",
          "speaker_id": "1",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100006038.1.8",
          "form": "아하 전 30초입니다ㅎㅎ",
          "original_form": "아하 전 30초입니다ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 21:30"
        },
        {
          "id": "MDRW2100006038.1.9",
          "form": "애 있으면 여행 자주 못갈것같네요ㅠ",
          "original_form": "애 있으면 여행 자주 못갈것같네요ㅠ",
          "speaker_id": "2",
          "time": "20210907 21:31"
        },
        {
          "id": "MDRW2100006038.1.10",
          "form": "그렇죠 아무래도 시간.장소에 제약이 생기더라구요",
          "original_form": "그렇죠 아무래도 시간.장소에 제약이 생기더라구요",
          "speaker_id": "1",
          "time": "20210907 21:45"
        },
        {
          "id": "MDRW2100006038.1.11",
          "form": "괌은 언제가셨나요?",
          "original_form": "괌은 언제가셨나요?",
          "speaker_id": "1",
          "time": "20210908 10:07"
        },
        {
          "id": "MDRW2100006038.1.12",
          "form": "가물가물하네요 16년도인가 15년도인가ㅎㅎ",
          "original_form": "가물가물하네요 16년도인가 15년도인가ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:09"
        },
        {
          "id": "MDRW2100006038.1.13",
          "form": "가족여행이면 부모님 모시고??",
          "original_form": "가족여행이면 부모님 모시고??",
          "speaker_id": "1",
          "time": "20210908 10:11"
        },
        {
          "id": "MDRW2100006038.1.14",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "2",
          "time": "20210908 10:15"
        },
        {
          "id": "MDRW2100006038.1.15",
          "form": "몇명이서 갔나요?",
          "original_form": "몇명이서 갔나요?",
          "speaker_id": "1",
          "time": "20210908 10:16"
        },
        {
          "id": "MDRW2100006038.1.16",
          "form": "저희 가족 넷이요ㅎㅎ",
          "original_form": "저희 가족 넷이요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100006038.1.17",
          "form": "괌 가셨을 때 뭐하셨어요?",
          "original_form": "괌 가셨을 때 뭐하셨어요?",
          "speaker_id": "2",
          "time": "20210908 10:26"
        },
        {
          "id": "MDRW2100006038.1.18",
          "form": "돌핀투어 하셨나요? ㅎ",
          "original_form": "돌핀투어 하셨나요? ㅎ",
          "speaker_id": "1",
          "time": "20210908 10:31"
        },
        {
          "id": "MDRW2100006038.1.19",
          "form": "오 네네 그거했어요ㅋㅋ",
          "original_form": "오 네네 그거했어요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 10:31"
        }
      ]
