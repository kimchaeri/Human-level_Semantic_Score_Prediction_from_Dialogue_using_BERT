      "utterance": [
        {
          "id": "MDRW2100026640.1.1",
          "form": "ㅋㅋㅋㅋ정말 많은걸 남긴 프로야",
          "original_form": "ㅋㅋㅋㅋ정말 많은걸 남긴 프로야",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026640.1.2",
          "form": "언제탈프듀하지",
          "original_form": "언제탈프듀하지",
          "speaker_id": "1",
          "time": "20210901 23:24"
        },
        {
          "id": "MDRW2100026640.1.3",
          "form": "세대 물갈이 해도... 프듀가 남긴 영향은 계속 될 듯",
          "original_form": "세대 물갈이 해도... 프듀가 남긴 영향은 계속 될 듯",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026640.1.4",
          "form": "ㅇㅇㅇㅇ",
          "original_form": "ㅇㅇㅇㅇ",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026640.1.5",
          "form": "정병 문화가 너무 퍼졌어",
          "original_form": "정병 문화가 너무 퍼졌어",
          "speaker_id": "2",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026640.1.6",
          "form": "죽을때도",
          "original_form": "죽을때도",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026640.1.7",
          "form": "프듀2 이러고 죽을듯",
          "original_form": "프듀2 이러고 죽을듯",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026640.1.8",
          "form": "정병들은",
          "original_form": "정병들은",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026640.1.9",
          "form": "그리고 갠팬문화",
          "original_form": "그리고 갠팬문화",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026640.1.10",
          "form": "너무심해짐",
          "original_form": "너무심해짐",
          "speaker_id": "1",
          "time": "20210901 23:26"
        },
        {
          "id": "MDRW2100026640.1.11",
          "form": "프듀2 이러면서 눈 감는다니ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "프듀2 이러면서 눈 감는다니ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026640.1.12",
          "form": "개인팬 문화랑 정병들..",
          "original_form": "개인팬 문화랑 정병들..",
          "speaker_id": "2",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026640.1.13",
          "form": "ㅋㅋㄱㅋㅋㅋㅋ 거긴 팬덤도",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋ 거긴 팬덤도",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026640.1.14",
          "form": "유지될때 철저하게 갠팬위주더라",
          "original_form": "유지될때 철저하게 갠팬위주더라",
          "speaker_id": "1",
          "time": "20210901 23:29"
        },
        {
          "id": "MDRW2100026640.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ서바이벌 구조상 어쩔수 없긴 하지만",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ서바이벌 구조상 어쩔수 없긴 하지만",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026640.1.16",
          "form": "ㅇㅇㅇ그렇지만",
          "original_form": "ㅇㅇㅇ그렇지만",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026640.1.17",
          "form": "주작 밝혀지면서",
          "original_form": "주작 밝혀지면서",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026640.1.18",
          "form": "팀되면 좀 뭉칠줄알았는데",
          "original_form": "팀되면 좀 뭉칠줄알았는데",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026640.1.19",
          "form": "나는 아니고 너는 맞다",
          "original_form": "나는 아니고 너는 맞다",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026640.1.20",
          "form": "선긋기하면서",
          "original_form": "선긋기하면서",
          "speaker_id": "2",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026640.1.21",
          "form": "ㅋㅋㄱㅋㅋㅋㅋ 아그리고",
          "original_form": "ㅋㅋㄱㅋㅋㅋㅋ 아그리고",
          "speaker_id": "1",
          "time": "20210901 23:30"
        },
        {
          "id": "MDRW2100026640.1.22",
          "form": "어차피 2 성진우인가",
          "original_form": "어차피 2 성진우인가",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026640.1.23",
          "form": "성현우 떨어트려서 조작맞는데",
          "original_form": "성현우 떨어트려서 조작맞는데",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026640.1.24",
          "form": "프듀4는 애초에 그룹 활동한 시간도 거의 없고..",
          "original_form": "프듀4는 애초에 그룹 활동한 시간도 거의 없고..",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026640.1.25",
          "form": "자꾸 강동호주작가담이라그러고",
          "original_form": "자꾸 강동호주작가담이라그러고",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026640.1.26",
          "form": "ㅋㅋㄱㅋㅋㅋ가담이니까 주작아니라고 뇌절하는데",
          "original_form": "ㅋㅋㄱㅋㅋㅋ가담이니까 주작아니라고 뇌절하는데",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026640.1.27",
          "form": "응 아니야",
          "original_form": "응 아니야",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026640.1.28",
          "form": "주작이야",
          "original_form": "주작이야",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100026640.1.29",
          "form": "뇌절들은 진짜 혼나야해",
          "original_form": "뇌절들은 진짜 혼나야해",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026640.1.30",
          "form": "그게 사실이든 아니든",
          "original_form": "그게 사실이든 아니든",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100026640.1.31",
          "form": "피디픽이주작아닌가요",
