      "utterance": [
        {
          "id": "MDRW2100031814.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210905 01:18"
        },
        {
          "id": "MDRW2100031814.1.2",
          "form": "안녕하세요 최근에 누군가에게 선물 주신 거 있으세요?",
          "original_form": "안녕하세요 최근에 누군가에게 선물 주신 거 있으세요?",
          "speaker_id": "2",
          "time": "20210905 01:19"
        },
        {
          "id": "MDRW2100031814.1.3",
          "form": "와이프한테 선물로 현찰 줬어요 ㅎㅎ",
          "original_form": "와이프한테 선물로 현찰 줬어요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 01:41"
        },
        {
          "id": "MDRW2100031814.1.4",
          "form": "와!! 와이프분께서 좋아하셨을거같아요!",
          "original_form": "와!! 와이프분께서 좋아하셨을거같아요!",
          "speaker_id": "2",
          "time": "20210905 01:44"
        },
        {
          "id": "MDRW2100031814.1.5",
          "form": "어떤 물질적인 선물도 좋지만",
          "original_form": "어떤 물질적인 선물도 좋지만",
          "speaker_id": "2",
          "time": "20210905 01:44"
        },
        {
          "id": "MDRW2100031814.1.6",
          "form": "현찰이 원하는 물건을 구매할 수 있어도 좋기도 하잖아요",
          "original_form": "현찰이 원하는 물건을 구매할 수 있어도 좋기도 하잖아요",
          "speaker_id": "2",
          "time": "20210905 01:44"
        },
        {
          "id": "MDRW2100031814.1.7",
          "form": "그런 고차원의 의미는 아니고ㅋㅋ",
          "original_form": "그런 고차원의 의미는 아니고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:47"
        },
        {
          "id": "MDRW2100031814.1.8",
          "form": "그냥 돈 생겨서 ㅎㅎ",
          "original_form": "그냥 돈 생겨서 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 01:47"
        },
        {
          "id": "MDRW2100031814.1.9",
          "form": "앟 용돈이었구나",
          "original_form": "앟 용돈이었구나",
          "speaker_id": "2",
          "time": "20210905 01:48"
        },
        {
          "id": "MDRW2100031814.1.10",
          "form": "그럼 와이프분은 그거로 아직 뭐 안사셨어요?",
          "original_form": "그럼 와이프분은 그거로 아직 뭐 안사셨어요?",
          "speaker_id": "2",
          "time": "20210905 01:49"
        },
        {
          "id": "MDRW2100031814.1.11",
          "form": "네 내일 네일받는다고ㅋㅋ",
          "original_form": "네 내일 네일받는다고ㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 01:49"
        },
        {
          "id": "MDRW2100031814.1.12",
          "form": "헉 네일 너무 예쁠거같아요",
          "original_form": "헉 네일 너무 예쁠거같아요",
          "speaker_id": "2",
          "time": "20210905 01:50"
        },
        {
          "id": "MDRW2100031814.1.13",
          "form": "함께 네일 받아보신 적 있으세요?",
          "original_form": "함께 네일 받아보신 적 있으세요?",
          "speaker_id": "2",
          "time": "20210905 01:50"
        },
        {
          "id": "MDRW2100031814.1.14",
          "form": "기분 전환으로라도..",
          "original_form": "기분 전환으로라도..",
          "speaker_id": "2",
          "time": "20210905 01:51"
        },
        {
          "id": "MDRW2100031814.1.15",
          "form": "ㅋㅋ제 취향은 아니라...",
          "original_form": "ㅋㅋ제 취향은 아니라...",
          "speaker_id": "1",
          "time": "20210905 01:52"
        },
        {
          "id": "MDRW2100031814.1.16",
          "form": "그럴 수 있죠... 보통 남성분들이 많이 받지는 않으시니까요",
          "original_form": "그럴 수 있죠... 보통 남성분들이 많이 받지는 않으시니까요",
          "speaker_id": "2",
          "time": "20210905 01:55"
        },
        {
          "id": "MDRW2100031814.1.17",
          "form": "깜빡 잠들었네요",
          "original_form": "깜빡 잠들었네요",
          "speaker_id": "1",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100031814.1.18",
          "form": "name2님은 최근에",
          "original_form": "&name2&님은 최근에",
          "speaker_id": "1",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100031814.1.19",
          "form": "뭐 선물하셨나요",
          "original_form": "뭐 선물하셨나요",
          "speaker_id": "1",
          "time": "20210905 07:49"
        },
        {
          "id": "MDRW2100031814.1.20",
          "form": "저는 꽃다발 사서 친구 선물해준적 있어요!",
          "original_form": "저는 꽃다발 사서 친구 선물해준적 있어요!",
          "speaker_id": "2",
          "time": "20210905 11:28"
        },
        {
          "id": "MDRW2100031814.1.21",
          "form": "친구 생일이었나요 ㅎㅎ",
          "original_form": "친구 생일이었나요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210905 11:41"
        },
        {
          "id": "MDRW2100031814.1.22",
          "form": "네 ㅎㅎ 친구 생일겸해서 꽃다발이랑 컵 선물 해줬어요",
          "original_form": "네 ㅎㅎ 친구 생일겸해서 꽃다발이랑 컵 선물 해줬어요",
