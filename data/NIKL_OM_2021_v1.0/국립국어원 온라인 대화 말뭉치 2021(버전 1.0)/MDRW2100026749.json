      "utterance": [
        {
          "id": "MDRW2100026749.1.1",
          "form": "쇼핑..좋아하시나요\n저는 주로 인터넷쇼핑몰에서 물건을 구입합니다.",
          "original_form": "쇼핑..좋아하시나요\n저는 주로 인터넷쇼핑몰에서 물건을 구입합니다.",
          "speaker_id": "1",
          "time": "20210901 23:53"
        },
        {
          "id": "MDRW2100026749.1.2",
          "form": "너무좋아요 쇼핑",
          "original_form": "너무좋아요 쇼핑",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026749.1.3",
          "form": "주로 아이쇼핑이 특기입니다",
          "original_form": "주로 아이쇼핑이 특기입니다",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026749.1.4",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026749.1.5",
          "form": "저도 그래요",
          "original_form": "저도 그래요",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026749.1.6",
          "form": "특가뜨면 눈으로 삽니다",
          "original_form": "특가뜨면 눈으로 삽니다",
          "speaker_id": "1",
          "time": "20210901 23:54"
        },
        {
          "id": "MDRW2100026749.1.7",
          "form": "주로 사는건 물티슈...세일하면 삽니다...",
          "original_form": "주로 사는건 물티슈...세일하면 삽니다...",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026749.1.8",
          "form": "청소도 하고 막쓰기 좋아서요",
          "original_form": "청소도 하고 막쓰기 좋아서요",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026749.1.9",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㄱㅋ",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026749.1.10",
          "form": "물티슈는 꼭 필요해요",
          "original_form": "물티슈는 꼭 필요해요",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026749.1.11",
          "form": "저는 당근도 많이해요",
          "original_form": "저는 당근도 많이해요",
          "speaker_id": "2",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026749.1.12",
          "form": "인터넷으로 10개.20개 박스로\n 싸게 사다가 마트에서 1000원짜리 사면\n뭔가 돈 아까워요",
          "original_form": "인터넷으로 10개.20개 박스로\n 싸게 사다가 마트에서 1000원짜리 사면\n뭔가 돈 아까워요",
          "speaker_id": "1",
          "time": "20210901 23:55"
        },
        {
          "id": "MDRW2100026749.1.13",
          "form": "당근은 아직 못 해봤습니다",
          "original_form": "당근은 아직 못 해봤습니다",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026749.1.14",
          "form": "말은 많이 들었는데",
          "original_form": "말은 많이 들었는데",
          "speaker_id": "1",
          "time": "20210901 23:56"
        },
        {
          "id": "MDRW2100026749.1.15",
          "form": "중고거래이다보니",
          "original_form": "중고거래이다보니",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026749.1.16",
          "form": "재미있어요 저렴해서",
          "original_form": "재미있어요 저렴해서",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026749.1.17",
          "form": "중고나라는 이용해봤어요",
          "original_form": "중고나라는 이용해봤어요",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026749.1.18",
          "form": "직거래라 믿음도 간다더군요 당근은",
          "original_form": "직거래라 믿음도 간다더군요 당근은",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026749.1.19",
          "form": "거긴 사기가많아서 믿을수가 없어요",
          "original_form": "거긴 사기가많아서 믿을수가 없어요",
          "speaker_id": "2",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026749.1.20",
          "form": "맞아요 사기꾼들이 너무많습니다.",
          "original_form": "맞아요 사기꾼들이 너무많습니다.",
          "speaker_id": "1",
          "time": "20210901 23:57"
        },
        {
          "id": "MDRW2100026749.1.21",
          "form": "동네주민이라는 생각에 중고라도 상태가 좋아요",
          "original_form": "동네주민이라는 생각에 중고라도 상태가 좋아요",
          "speaker_id": "2",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026749.1.22",
          "form": "오 그렇군요",
          "original_form": "오 그렇군요",
          "speaker_id": "1",
          "time": "20210901 23:58"
        },
        {
          "id": "MDRW2100026749.1.23",
          "form": "최근에 구매하신건 물티슈",
          "original_form": "최근에 구매하신건 물티슈",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026749.1.24",
          "form": "말고는 없나요?",
          "original_form": "말고는 없나요?",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026749.1.25",
          "form": "전 핫딜뜨면",
          "original_form": "전 핫딜뜨면",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026749.1.26",
          "form": "안필요해도 사게되네요",
          "original_form": "안필요해도 사게되네요",
          "speaker_id": "2",
          "time": "20210901 23:59"
        },
        {
          "id": "MDRW2100026749.1.27",
          "form": "아 저는 최근에 마스크를...또",
          "original_form": "아 저는 최근에 마스크를...또",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026749.1.28",
          "form": "진짜 집에 한1000장은 있는거같아요",
          "original_form": "진짜 집에 한1000장은 있는거같아요",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026749.1.29",
          "form": "......국내산 핫딜만뜨면...주체를 못하네요",
          "original_form": "......국내산 핫딜만뜨면...주체를 못하네요",
          "speaker_id": "1",
          "time": "20210902 00:00"
        },
        {
          "id": "MDRW2100026749.1.30",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 너무웃겨요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ 너무웃겨요",
          "speaker_id": "2",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026749.1.31",
          "form": "진심 빵 터졌습니다",
          "original_form": "진심 빵 터졌습니다",
          "speaker_id": "2",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026749.1.32",
          "form": "거기에 선착순 1000세트 이런거뜨면...",
          "original_form": "거기에 선착순 1000세트 이런거뜨면...",
          "speaker_id": "1",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026749.1.33",
          "form": "일단사고봅니다....",
          "original_form": "일단사고봅니다....",
          "speaker_id": "1",
          "time": "20210902 00:01"
        },
        {
          "id": "MDRW2100026749.1.34",
          "form": "할인 소식 차단하세요 ㅎㅎㅎ",
          "original_form": "할인 소식 차단하세요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210902 00:02"
        }
      ]
