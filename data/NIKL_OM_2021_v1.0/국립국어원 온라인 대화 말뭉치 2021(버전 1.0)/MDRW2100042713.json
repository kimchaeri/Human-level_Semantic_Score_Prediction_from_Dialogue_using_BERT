      "utterance": [
        {
          "id": "MDRW2100042713.1.1",
          "form": "오늘아침먹었나요?",
          "original_form": "오늘아침먹었나요?",
          "speaker_id": "1",
          "time": "20210907 07:20"
        },
        {
          "id": "MDRW2100042713.1.2",
          "form": "아니요~ 오늘은 공복으로 나왔어요!",
          "original_form": "아니요~ 오늘은 공복으로 나왔어요!",
          "speaker_id": "2",
          "time": "20210907 07:43"
        },
        {
          "id": "MDRW2100042713.1.3",
          "form": "드셨어요?",
          "original_form": "드셨어요?",
          "speaker_id": "2",
          "time": "20210907 07:43"
        },
        {
          "id": "MDRW2100042713.1.4",
          "form": "저도 어제까지 계속 공복이었는데",
          "original_form": "저도 어제까지 계속 공복이었는데",
          "speaker_id": "1",
          "time": "20210907 07:44"
        },
        {
          "id": "MDRW2100042713.1.5",
          "form": "오늘 아침 먹었어요",
          "original_form": "오늘 아침 먹었어요",
          "speaker_id": "1",
          "time": "20210907 07:44"
        },
        {
          "id": "MDRW2100042713.1.6",
          "form": "샌드위치",
          "original_form": "샌드위치",
          "speaker_id": "1",
          "time": "20210907 07:44"
        },
        {
          "id": "MDRW2100042713.1.7",
          "form": "오 미리 사두셨나보네요",
          "original_form": "오 미리 사두셨나보네요",
          "speaker_id": "2",
          "time": "20210907 07:45"
        },
        {
          "id": "MDRW2100042713.1.8",
          "form": "저는 회사가서 라떼나 마시고",
          "original_form": "저는 회사가서 라떼나 마시고",
          "speaker_id": "2",
          "time": "20210907 07:45"
        },
        {
          "id": "MDRW2100042713.1.9",
          "form": "군것질할듯요",
          "original_form": "군것질할듯요",
          "speaker_id": "2",
          "time": "20210907 07:45"
        },
        {
          "id": "MDRW2100042713.1.10",
          "form": "편의점오늘사서먹었어요ㅜㅜ너무배고파서요",
          "original_form": "편의점오늘사서먹었어요ㅜㅜ너무배고파서요",
          "speaker_id": "1",
          "time": "20210907 09:38"
        },
        {
          "id": "MDRW2100042713.1.11",
          "form": "아 그쵸 사실안먹고가면 너무 배고파요",
          "original_form": "아 그쵸 사실안먹고가면 너무 배고파요",
          "speaker_id": "2",
          "time": "20210907 09:39"
        },
        {
          "id": "MDRW2100042713.1.12",
          "form": "네 맞아요ㅠㅠ너무 힘들어요",
          "original_form": "네 맞아요ㅠㅠ너무 힘들어요",
          "speaker_id": "1",
          "time": "20210907 09:52"
        },
        {
          "id": "MDRW2100042713.1.13",
          "form": "점심은 뭐드세요",
          "original_form": "점심은 뭐드세요",
          "speaker_id": "2",
          "time": "20210907 10:22"
        },
        {
          "id": "MDRW2100042713.1.14",
          "form": "낼은 새우튀김먹어요",
          "original_form": "낼은 새우튀김먹어요",
          "speaker_id": "1",
          "time": "20210907 17:05"
        },
        {
          "id": "MDRW2100042713.1.15",
          "form": "저는 함박스테이크먹었어요",
          "original_form": "저는 함박스테이크먹었어요",
          "speaker_id": "1",
          "time": "20210907 17:05"
        },
        {
          "id": "MDRW2100042713.1.16",
          "form": "오 외식하시나요?",
          "original_form": "오 외식하시나요?",
          "speaker_id": "2",
          "time": "20210907 17:54"
        },
        {
          "id": "MDRW2100042713.1.17",
          "form": "저는 회사식당가요",
          "original_form": "저는 회사식당가요",
          "speaker_id": "2",
          "time": "20210907 17:55"
        },
        {
          "id": "MDRW2100042713.1.18",
          "form": "아니용 낼 반찬메뉴에여ㅋㅋㄱ",
          "original_form": "아니용 낼 반찬메뉴에여ㅋㅋㄱ",
          "speaker_id": "1",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100042713.1.19",
          "form": "반찬중에서 그나마 괜찮은게 새우튀김나와요",
          "original_form": "반찬중에서 그나마 괜찮은게 새우튀김나와요",
          "speaker_id": "1",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100042713.1.20",
          "form": "다른거는 무난해유",
          "original_form": "다른거는 무난해유",
          "speaker_id": "1",
          "time": "20210907 18:08"
        },
        {
          "id": "MDRW2100042713.1.21",
          "form": "오 맛있겠어요",
          "original_form": "오 맛있겠어요",
          "speaker_id": "2",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100042713.1.22",
          "form": "식당메뉴 잘 나오는편인가봐요",
          "original_form": "식당메뉴 잘 나오는편인가봐요",
          "speaker_id": "2",
          "time": "20210907 18:29"
        },
        {
          "id": "MDRW2100042713.1.23",
          "form": "저는 잘모르겠어요",
          "original_form": "저는 잘모르겠어요",
          "speaker_id": "1",
          "time": "20210907 18:42"
        },
        {
          "id": "MDRW2100042713.1.24",
          "form": "그날 나오는 메뉴보고",
          "original_form": "그날 나오는 메뉴보고",
          "speaker_id": "1",
          "time": "20210907 18:43"
        },
        {
          "id": "MDRW2100042713.1.25",
          "form": "그나마 괜찮은걸 기억해요",
          "original_form": "그나마 괜찮은걸 기억해요",
          "speaker_id": "1",
          "time": "20210907 18:43"
        }
      ]
    }
