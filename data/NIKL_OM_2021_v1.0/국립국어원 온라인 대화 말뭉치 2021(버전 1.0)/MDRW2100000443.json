      "utterance": [
        {
          "id": "MDRW2100000443.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210819 16:06"
        },
        {
          "id": "MDRW2100000443.1.2",
          "form": "안녕하세요! 오늘 점심 맛있게 드셨나요?",
          "original_form": "안녕하세요! 오늘 점심 맛있게 드셨나요?",
          "speaker_id": "2",
          "time": "20210819 16:08"
        },
        {
          "id": "MDRW2100000443.1.3",
          "form": "네! 간단히 먹으려고",
          "original_form": "네! 간단히 먹으려고",
          "speaker_id": "1",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000443.1.4",
          "form": "치킨 배달 시켜먹었네요",
          "original_form": "치킨 배달 시켜먹었네요",
          "speaker_id": "1",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000443.1.5",
          "form": "혹시 추천할만한 치킨집 있으실까요??",
          "original_form": "혹시 추천할만한 치킨집 있으실까요??",
          "speaker_id": "1",
          "time": "20210819 16:11"
        },
        {
          "id": "MDRW2100000443.1.6",
          "form": "저는 개인적으로 비비큐만 시켜먹어요ㅋㅋㅋ",
          "original_form": "저는 개인적으로 비비큐만 시켜먹어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:12"
        },
        {
          "id": "MDRW2100000443.1.7",
          "form": "잉 비비큐에서만 제가 좋아하는 부분을 팔아서요ㅋㅋ",
          "original_form": "잉 비비큐에서만 제가 좋아하는 부분을 팔아서요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:13"
        },
        {
          "id": "MDRW2100000443.1.8",
          "form": "아 혹시 광희나는갈릭치킨인가??",
          "original_form": "아 혹시 광희나는갈릭치킨인가??",
          "speaker_id": "1",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000443.1.9",
          "form": "그 엉치살 부분 말씀하시는건가요?!",
          "original_form": "그 엉치살 부분 말씀하시는건가요?!",
          "speaker_id": "1",
          "time": "20210819 16:14"
        },
        {
          "id": "MDRW2100000443.1.10",
          "form": "닭다리만 파는거요! ㅋㅋ",
          "original_form": "닭다리만 파는거요! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:15"
        },
        {
          "id": "MDRW2100000443.1.11",
          "form": "아 그렇군요 ㅋㅋㅋㅋㅋ",
          "original_form": "아 그렇군요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000443.1.12",
          "form": "나중엔 비비큐로 한번 먹어봐야겠어요",
          "original_form": "나중엔 비비큐로 한번 먹어봐야겠어요",
          "speaker_id": "1",
          "time": "20210819 16:18"
        },
        {
          "id": "MDRW2100000443.1.13",
          "form": "맛있어요 진짜ㅋㅋ제가 부드러운 치킨살을 좋아해서 저것만 시켜먹게 되더라구요",
          "original_form": "맛있어요 진짜ㅋㅋ제가 부드러운 치킨살을 좋아해서 저것만 시켜먹게 되더라구요",
          "speaker_id": "2",
          "time": "20210819 16:21"
        },
        {
          "id": "MDRW2100000443.1.14",
          "form": "특별히 좋아하시는 음식 있으세요?",
          "original_form": "특별히 좋아하시는 음식 있으세요?",
          "speaker_id": "2",
          "time": "20210819 16:22"
        },
        {
          "id": "MDRW2100000443.1.15",
          "form": "저는 부침개요!!!!",
          "original_form": "저는 부침개요!!!!",
          "speaker_id": "1",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000443.1.16",
          "form": "진짜 전 종류면 다 좋아해요",
          "original_form": "진짜 전 종류면 다 좋아해요",
          "speaker_id": "1",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000443.1.17",
          "form": "명절날이 부침개 먹으려고 기다려질 정도였어요 정말",
          "original_form": "명절날이 부침개 먹으려고 기다려질 정도였어요 정말",
          "speaker_id": "1",
          "time": "20210819 16:24"
        },
        {
          "id": "MDRW2100000443.1.18",
          "form": "헐!! 부침개!! ㅜㅠ",
          "original_form": "헐!! 부침개!! ㅜㅠ",
          "speaker_id": "2",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000443.1.19",
          "form": "저도 전 좋아해요!!!",
          "original_form": "저도 전 좋아해요!!!",
          "speaker_id": "2",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000443.1.20",
          "form": "특히 바삭바삭한 부추전!!",
          "original_form": "특히 바삭바삭한 부추전!!",
          "speaker_id": "2",
          "time": "20210819 16:27"
        },
        {
          "id": "MDRW2100000443.1.21",
          "form": "와..음식좀 즐길줄 아시는군요..??",
          "original_form": "와..음식좀 즐길줄 아시는군요..??",
          "speaker_id": "1",
          "time": "20210819 16:28"
        },
        {
          "id": "MDRW2100000443.1.22",
          "form": "부추전 맛있죠..",
          "original_form": "부추전 맛있죠..",
          "speaker_id": "1",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000443.1.23",
          "form": "ㅋㅋㅋㅋ맛집 찾아다니는 1인입니다ㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ맛집 찾아다니는 1인입니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000443.1.24",
          "form": "저녁메뉴로 삼고싶다..",
          "original_form": "저녁메뉴로 삼고싶다..",
          "speaker_id": "1",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000443.1.25",
          "form": "ㅋㅋㅋㅋㅋㅋ제일 좋아하시는 음식이",
          "original_form": "ㅋㅋㅋㅋㅋㅋ제일 좋아하시는 음식이",
          "speaker_id": "1",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000443.1.26",
          "form": "저녁메뉴는 부침개?!",
          "original_form": "저녁메뉴는 부침개?!",
          "speaker_id": "2",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000443.1.27",
          "form": "어떤거에요?!",
          "original_form": "어떤거에요?!",
          "speaker_id": "1",
          "time": "20210819 16:29"
        },
        {
          "id": "MDRW2100000443.1.28",
          "form": "전 다 좋아하긴하는데.. 음 곰탕?ㅋㅋㅋㅋ",
          "original_form": "전 다 좋아하긴하는데.. 음 곰탕?ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210819 16:30"
        },
        {
          "id": "MDRW2100000443.1.29",
          "form": "진한 국물음식 좋아해요 거기에 밥말아먹으면 크으으으",
          "original_form": "진한 국물음식 좋아해요 거기에 밥말아먹으면 크으으으",
