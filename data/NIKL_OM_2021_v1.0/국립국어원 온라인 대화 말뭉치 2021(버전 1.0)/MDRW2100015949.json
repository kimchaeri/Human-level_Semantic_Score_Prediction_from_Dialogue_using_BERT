      "utterance": [
        {
          "id": "MDRW2100015949.1.1",
          "form": "태권도 알바 할때 어땠어?",
          "original_form": "태권도 알바 할때 어땠어?",
          "speaker_id": "2",
          "time": "20210826 03:35"
        },
        {
          "id": "MDRW2100015949.1.2",
          "form": "엄청 힘들었어",
          "original_form": "엄청 힘들었어",
          "speaker_id": "1",
          "time": "20210826 03:35"
        },
        {
          "id": "MDRW2100015949.1.3",
          "form": "너가 했던 알바중에 제일 힘들었어?",
          "original_form": "너가 했던 알바중에 제일 힘들었어?",
          "speaker_id": "2",
          "time": "20210826 03:35"
        },
        {
          "id": "MDRW2100015949.1.4",
          "form": "응",
          "original_form": "응",
          "speaker_id": "1",
          "time": "20210826 03:35"
        },
        {
          "id": "MDRW2100015949.1.5",
          "form": "애들 보는게 엄청 힘들어",
          "original_form": "애들 보는게 엄청 힘들어",
          "speaker_id": "1",
          "time": "20210826 03:36"
        },
        {
          "id": "MDRW2100015949.1.6",
          "form": "그정도야?",
          "original_form": "그정도야?",
          "speaker_id": "2",
          "time": "20210826 03:36"
        },
        {
          "id": "MDRW2100015949.1.7",
          "form": "응 처음 2개월은 재미있었는데 애들이 영악하더라고",
          "original_form": "응 처음 2개월은 재미있었는데 애들이 영악하더라고",
          "speaker_id": "1",
          "time": "20210826 03:36"
        },
        {
          "id": "MDRW2100015949.1.8",
          "form": "애들이 몇살인데?",
          "original_form": "애들이 몇살인데?",
          "speaker_id": "2",
          "time": "20210826 03:36"
        },
        {
          "id": "MDRW2100015949.1.9",
          "form": "5살부터 10살정도까지 내가 봐",
          "original_form": "5살부터 10살정도까지 내가 봐",
          "speaker_id": "1",
          "time": "20210826 03:36"
        },
        {
          "id": "MDRW2100015949.1.10",
          "form": "그 애들이 영악하다고?",
          "original_form": "그 애들이 영악하다고?",
          "speaker_id": "2",
          "time": "20210826 03:36"
        },
        {
          "id": "MDRW2100015949.1.11",
          "form": "요즘 어린애들 엄청 영악하고 말도 안들어",
          "original_form": "요즘 어린애들 엄청 영악하고 말도 안들어",
          "speaker_id": "1",
          "time": "20210826 03:37"
        },
        {
          "id": "MDRW2100015949.1.12",
          "form": "같이 일하는 사람들은 어때?",
          "original_form": "같이 일하는 사람들은 어때?",
          "speaker_id": "2",
          "time": "20210826 03:37"
        },
        {
          "id": "MDRW2100015949.1.13",
          "form": "정말 별로였어",
          "original_form": "정말 별로였어",
          "speaker_id": "1",
          "time": "20210826 03:37"
        },
        {
          "id": "MDRW2100015949.1.14",
          "form": "그건 왜?",
          "original_form": "그건 왜?",
          "speaker_id": "2",
          "time": "20210826 03:37"
        },
        {
          "id": "MDRW2100015949.1.15",
          "form": "학부모님들이나 애들 뒷담을 엄청 많이 해",
          "original_form": "학부모님들이나 애들 뒷담을 엄청 많이 해",
          "speaker_id": "1",
          "time": "20210826 03:37"
        },
        {
          "id": "MDRW2100015949.1.16",
          "form": "모범은 못될망정 뒷담을 해?",
          "original_form": "모범은 못될망정 뒷담을 해?",
          "speaker_id": "2",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100015949.1.17",
          "form": "그러면서 앞에서는 애들 칭찬하는게 어이없어",
          "original_form": "그러면서 앞에서는 애들 칭찬하는게 어이없어",
          "speaker_id": "1",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100015949.1.18",
          "form": "인간이 덜 됐네",
          "original_form": "인간이 덜 됐네",
          "speaker_id": "2",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100015949.1.19",
          "form": "맞아 그런것같아",
          "original_form": "맞아 그런것같아",
          "speaker_id": "1",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100015949.1.20",
          "form": "그거말고는 괜찮아?",
          "original_form": "그거말고는 괜찮아?",
          "speaker_id": "2",
          "time": "20210826 03:38"
        },
        {
          "id": "MDRW2100015949.1.21",
          "form": "관장님이 너무 기분파야",
          "original_form": "관장님이 너무 기분파야",
          "speaker_id": "1",
          "time": "20210826 03:39"
        },
        {
          "id": "MDRW2100015949.1.22",
          "form": "왜?",
          "original_form": "왜?",
          "speaker_id": "2",
          "time": "20210826 03:39"
        },
        {
          "id": "MDRW2100015949.1.23",
          "form": "자기 기분 좋을 때는 잘해주는데",
          "original_form": "자기 기분 좋을 때는 잘해주는데",
          "speaker_id": "1",
          "time": "20210826 03:39"
        },
        {
          "id": "MDRW2100015949.1.24",
          "form": "나쁘면 얄짤도 없어",
          "original_form": "나쁘면 얄짤도 없어",
          "speaker_id": "1",
          "time": "20210826 03:39"
        },
        {
          "id": "MDRW2100015949.1.25",
          "form": "사회생활 미리 해본거지",
          "original_form": "사회생활 미리 해본거지",
          "speaker_id": "2",
          "time": "20210826 03:39"
        },
        {
          "id": "MDRW2100015949.1.26",
          "form": "그런사람들 많아",
          "original_form": "그런사람들 많아",
          "speaker_id": "2",
          "time": "20210826 03:39"
        },
        {
          "id": "MDRW2100015949.1.27",
          "form": "그런것같아서 너무 별로야",
          "original_form": "그런것같아서 너무 별로야",
          "speaker_id": "1",
          "time": "20210826 03:39"
        }
      ]
    }
  ]
