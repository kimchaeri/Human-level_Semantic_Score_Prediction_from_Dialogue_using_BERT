      "utterance": [
        {
          "id": "MDRW2100011562.1.1",
          "form": "티비 프로 자주보시나요?",
          "original_form": "티비 프로 자주보시나요?",
          "speaker_id": "1",
          "time": "20210824 14:05"
        },
        {
          "id": "MDRW2100011562.1.2",
          "form": "네 자주봐요.",
          "original_form": "네 자주봐요.",
          "speaker_id": "2",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100011562.1.3",
          "form": "뭐 자주 보세요?",
          "original_form": "뭐 자주 보세요?",
          "speaker_id": "2",
          "time": "20210824 14:10"
        },
        {
          "id": "MDRW2100011562.1.4",
          "form": "저 유퀴즈 좋아해요",
          "original_form": "저 유퀴즈 좋아해요",
          "speaker_id": "1",
          "time": "20210824 14:11"
        },
        {
          "id": "MDRW2100011562.1.5",
          "form": "저는 놀면뭐하니요ㅋ",
          "original_form": "저는 놀면뭐하니요ㅋ",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011562.1.6",
          "form": "둘 다 유재석 나오는거네요ㅋ",
          "original_form": "둘 다 유재석 나오는거네요ㅋ",
          "speaker_id": "2",
          "time": "20210824 14:18"
        },
        {
          "id": "MDRW2100011562.1.7",
          "form": "놀뭐 최근꺼 재밌더라구요ㅎㅎ",
          "original_form": "놀뭐 최근꺼 재밌더라구요ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011562.1.8",
          "form": "네 ㅋ 나혼자산다도 좋아해요ㅋ",
          "original_form": "네 ㅋ 나혼자산다도 좋아해요ㅋ",
          "speaker_id": "2",
          "time": "20210824 14:23"
        },
        {
          "id": "MDRW2100011562.1.9",
          "form": "나혼산도 재밌어요ㅎㅎㅎ",
          "original_form": "나혼산도 재밌어요ㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 14:25"
        },
        {
          "id": "MDRW2100011562.1.10",
          "form": "맞아요ㅋ즐겨봐요ㅋ",
          "original_form": "맞아요ㅋ즐겨봐요ㅋ",
          "speaker_id": "2",
          "time": "20210824 14:27"
        },
        {
          "id": "MDRW2100011562.1.11",
          "form": "어떤편 가장 재밌으셨나요",
          "original_form": "어떤편 가장 재밌으셨나요",
          "speaker_id": "1",
          "time": "20210824 17:33"
        },
        {
          "id": "MDRW2100011562.1.12",
          "form": "싹쓰리, 환불원정대, MSG워너비요.ㅋㅋ",
          "original_form": "싹쓰리, 환불원정대, MSG워너비요.ㅋㅋ",
          "speaker_id": "2",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011562.1.13",
          "form": "노래듣는걸 좋아해서요ㅋ",
          "original_form": "노래듣는걸 좋아해서요ㅋ",
          "speaker_id": "2",
          "time": "20210824 17:40"
        },
        {
          "id": "MDRW2100011562.1.14",
          "form": "어떤편 재미있게 보셨어요?",
          "original_form": "어떤편 재미있게 보셨어요?",
          "speaker_id": "2",
          "time": "20210824 17:41"
        },
        {
          "id": "MDRW2100011562.1.15",
          "form": "저는 최근에 신유빈 선수 나온편ㅎㅎ",
          "original_form": "저는 최근에 신유빈 선수 나온편ㅎㅎ",
          "speaker_id": "1",
          "time": "20210824 17:45"
        },
        {
          "id": "MDRW2100011562.1.16",
          "form": "아 맞아요.ㅋㅋ신유빈선수 나온것도 재미있었어요ㅋ",
          "original_form": "아 맞아요.ㅋㅋ신유빈선수 나온것도 재미있었어요ㅋ",
          "speaker_id": "2",
          "time": "20210824 17:49"
        },
        {
          "id": "MDRW2100011562.1.17",
          "form": "무도멤버들 나와서 좋았네요",
          "original_form": "무도멤버들 나와서 좋았네요",
