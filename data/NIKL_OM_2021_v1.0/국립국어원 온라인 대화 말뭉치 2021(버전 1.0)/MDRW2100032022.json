      "utterance": [
        {
          "id": "MDRW2100032022.1.1",
          "form": "한 가지이 게임에서도 이렇게 많이 한답니다",
          "original_form": "한 가지이 게임에서도 이렇게 많이 한답니다",
          "speaker_id": "1",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100032022.1.2",
          "form": "요즘 TV 보시나요",
          "original_form": "요즘 TV 보시나요",
          "speaker_id": "2",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100032022.1.3",
          "form": "넷플릭스를 자주 보는 편이에요",
          "original_form": "넷플릭스를 자주 보는 편이에요",
          "speaker_id": "1",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100032022.1.4",
          "form": "하지만 일요일에는 동물 농장은 꼭 챙겨 보는 편입니다",
          "original_form": "하지만 일요일에는 동물 농장은 꼭 챙겨 보는 편입니다",
          "speaker_id": "1",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100032022.1.5",
          "form": "TV를 자주 보십니까",
          "original_form": "TV를 자주 보십니까",
          "speaker_id": "1",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100032022.1.6",
          "form": "넷플릭스 뮈보셨어요 최근에",
          "original_form": "넷플릭스 뮈보셨어요 최근에",
          "speaker_id": "2",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100032022.1.7",
          "form": "그레이스 봤었어요",
          "original_form": "그레이스 봤었어요",
          "speaker_id": "1",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100032022.1.8",
          "form": "엄마 때문에 트로트를 매일 봐요",
          "original_form": "엄마 때문에 트로트를 매일 봐요",
          "speaker_id": "2",
          "time": "20210905 11:01"
        },
        {
          "id": "MDRW2100032022.1.9",
          "form": "트로트 좋아하는 가수 있으신가요",
          "original_form": "트로트 좋아하는 가수 있으신가요",
          "speaker_id": "1",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100032022.1.10",
          "form": "그레이스는 드라마인가요",
          "original_form": "그레이스는 드라마인가요",
          "speaker_id": "2",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100032022.1.11",
          "form": "요즘 트로트 선수들이 핫 하잖아요",
          "original_form": "요즘 트로트 선수들이 핫 하잖아요",
          "speaker_id": "1",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100032022.1.12",
          "form": "저는 별로인데 어머니는 찬원이를 좋아합니다",
          "original_form": "저는 별로인데 어머니는 찬원이를 좋아합니다",
          "speaker_id": "2",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100032022.1.13",
          "form": "네 캐나다 짧은 단편드라마입니다",
          "original_form": "네 캐나다 짧은 단편드라마입니다",
          "speaker_id": "1",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100032022.1.14",
          "form": "귀엽다고 하네요",
          "original_form": "귀엽다고 하네요",
          "speaker_id": "2",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100032022.1.15",
          "form": "주인공들이 매우 매력적입니다",
          "original_form": "주인공들이 매우 매력적입니다",
          "speaker_id": "1",
          "time": "20210905 11:02"
        },
        {
          "id": "MDRW2100032022.1.16",
          "form": "노래를 구수하게 부릅니다",
          "original_form": "노래를 구수하게 부릅니다",
          "speaker_id": "2",
