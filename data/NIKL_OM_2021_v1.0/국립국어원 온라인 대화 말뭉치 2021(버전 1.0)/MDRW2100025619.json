      "utterance": [
        {
          "id": "MDRW2100025619.1.1",
          "form": "name2님~ 공연 보는거 좋아하세요?",
          "original_form": "&name2&님~ 공연 보는거 좋아하세요?",
          "speaker_id": "1",
          "time": "20210901 14:26"
        },
        {
          "id": "MDRW2100025619.1.2",
          "form": "안녕하세요. 수도권이 아니라 공연이 많지않아요",
          "original_form": "안녕하세요. 수도권이 아니라 공연이 많지않아요",
          "speaker_id": "2",
          "time": "20210901 14:28"
        },
        {
          "id": "MDRW2100025619.1.3",
          "form": "그래서 접근성 쉬운 영화를 더 좋아해요",
          "original_form": "그래서 접근성 쉬운 영화를 더 좋아해요",
          "speaker_id": "2",
          "time": "20210901 14:29"
        },
        {
          "id": "MDRW2100025619.1.4",
          "form": "영화도 좋죠... 저는 마지막에 본 영화가 캣츠였어요.. name2님은요?",
          "original_form": "영화도 좋죠... 저는 마지막에 본 영화가 캣츠였어요.. &name2&님은요?",
          "speaker_id": "1",
          "time": "20210901 14:34"
        },
        {
          "id": "MDRW2100025619.1.5",
          "form": "영화관에서요?",
          "original_form": "영화관에서요?",
          "speaker_id": "2",
          "time": "20210901 14:36"
        },
        {
          "id": "MDRW2100025619.1.6",
          "form": "영화관은 안간지오래돼서",
          "original_form": "영화관은 안간지오래돼서",
          "speaker_id": "2",
          "time": "20210901 14:37"
        },
        {
          "id": "MDRW2100025619.1.7",
          "form": "창밖의여자 였나",
          "original_form": "창밖의여자 였나",
          "speaker_id": "2",
          "time": "20210901 14:37"
        },
        {
          "id": "MDRW2100025619.1.8",
          "form": "넷플 스릴러영화였어ㅛ",
          "original_form": "넷플 스릴러영화였어ㅛ",
          "speaker_id": "2",
          "time": "20210901 14:37"
        },
        {
          "id": "MDRW2100025619.1.9",
          "form": "아~ 넷플에서 보는군요",
          "original_form": "아~ 넷플에서 보는군요",
          "speaker_id": "1",
          "time": "20210901 14:47"
        },
        {
          "id": "MDRW2100025619.1.10",
          "form": "전 코로나이후로는 영화관 못갔네요..",
          "original_form": "전 코로나이후로는 영화관 못갔네요..",
          "speaker_id": "1",
          "time": "20210901 14:47"
        },
        {
          "id": "MDRW2100025619.1.11",
          "form": "네 그래서 저도 집에서 봐요",
          "original_form": "네 그래서 저도 집에서 봐요",
          "speaker_id": "2",
          "time": "20210901 14:48"
        },
        {
          "id": "MDRW2100025619.1.12",
          "form": "콘서트도 다 못간지 됐네요ㅜ",
          "original_form": "콘서트도 다 못간지 됐네요ㅜ",
          "speaker_id": "2",
          "time": "20210901 14:48"
        },
        {
          "id": "MDRW2100025619.1.13",
          "form": "맞아요.. 공연은 직관이 최고인데 말이죠",
          "original_form": "맞아요.. 공연은 직관이 최고인데 말이죠",
          "speaker_id": "1",
          "time": "20210901 14:49"
        },
        {
          "id": "MDRW2100025619.1.14",
          "form": "네. 공연본 때가 전생인건지ㅎ",
          "original_form": "네. 공연본 때가 전생인건지ㅎ",
          "speaker_id": "2",
          "time": "20210901 14:50"
        },
        {
          "id": "MDRW2100025619.1.15",
          "form": "온라인콘서트도 하던데 티비랑 다름이 무엇인지",
          "original_form": "온라인콘서트도 하던데 티비랑 다름이 무엇인지",
          "speaker_id": "2",
          "time": "20210901 14:51"
        },
        {
          "id": "MDRW2100025619.1.16",
          "form": "뭔가 사람들 모이는 건 다 작년부터 못하고 있어서요.. 까마득하네요",
          "original_form": "뭔가 사람들 모이는 건 다 작년부터 못하고 있어서요.. 까마득하네요",
          "speaker_id": "1",
          "time": "20210901 14:53"
        },
        {
          "id": "MDRW2100025619.1.17",
          "form": "곧 추석인데 예전같으면 가족영화 넘쳐났을거에요",
          "original_form": "곧 추석인데 예전같으면 가족영화 넘쳐났을거에요",
          "speaker_id": "2",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100025619.1.18",
          "form": "추석인데 어차피 사람들 못모일테니까 티비만 보게 될 거 같기도 하네요",
          "original_form": "추석인데 어차피 사람들 못모일테니까 티비만 보게 될 거 같기도 하네요",
          "speaker_id": "1",
          "time": "20210901 14:54"
        },
        {
          "id": "MDRW2100025619.1.19",
          "form": "네 재밌는 영화나 보여주면 좋겠어요ㅎ",
          "original_form": "네 재밌는 영화나 보여주면 좋겠어요ㅎ",
          "speaker_id": "2",
          "time": "20210901 14:56"
        },
        {
          "id": "MDRW2100025619.1.20",
          "form": "그래도 전시회는 한번씩하더라구요",
          "original_form": "그래도 전시회는 한번씩하더라구요",
          "speaker_id": "2",
          "time": "20210901 14:57"
        },
        {
          "id": "MDRW2100025619.1.21",
          "form": "이중섭미술관에서 이건희컬렉션전 하고있어요",
          "original_form": "이중섭미술관에서 이건희컬렉션전 하고있어요",
          "speaker_id": "2",
          "time": "20210901 14:58"
        },
        {
          "id": "MDRW2100025619.1.22",
          "form": "전시회 좋아하는데.. 요즘엔 일단 사람들 있는 곳을 안가다보니까요",
          "original_form": "전시회 좋아하는데.. 요즘엔 일단 사람들 있는 곳을 안가다보니까요",
          "speaker_id": "1",
          "time": "20210901 15:01"
        }
      ]
