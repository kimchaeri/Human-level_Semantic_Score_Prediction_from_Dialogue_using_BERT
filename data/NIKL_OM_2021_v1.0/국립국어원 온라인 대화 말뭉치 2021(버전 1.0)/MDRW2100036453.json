      "utterance": [
        {
          "id": "MDRW2100036453.1.1",
          "form": "뭘 마셨어? 궁금하다",
          "original_form": "뭘 마셨어? 궁금하다",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100036453.1.2",
          "form": "커피를 마셨어. 아아. 어디 걸 마셨는지 맞혀봐.",
          "original_form": "커피를 마셨어. 아아. 어디 걸 마셨는지 맞혀봐.",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100036453.1.3",
          "form": "더 벤티 아아를 마셨는데 정말 맛없더라.",
          "original_form": "더 벤티 아아를 마셨는데 정말 맛없더라.",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100036453.1.4",
          "form": "흠 빽다방은 아닐 것 같아",
          "original_form": "흠 빽다방은 아닐 것 같아",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100036453.1.5",
          "form": "탄맛만 가득했어",
          "original_form": "탄맛만 가득했어",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100036453.1.6",
          "form": "스타벅스일까?",
          "original_form": "스타벅스일까?",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100036453.1.7",
          "form": "나 빽다방도 나름 자주가는데.. 오늘은 아니었어",
          "original_form": "나 빽다방도 나름 자주가는데.. 오늘은 아니었어",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100036453.1.8",
          "form": "더벤티에 갔다구.",
          "original_form": "더벤티에 갔다구.",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100036453.1.9",
          "form": "더벤티는 너네 집 근처에있니??",
          "original_form": "더벤티는 너네 집 근처에있니??",
          "speaker_id": "2",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100036453.1.10",
          "form": "음 아니 조금 거리가 있어",
          "original_form": "음 아니 조금 거리가 있어",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100036453.1.11",
          "form": "근데ㅜ너무 맛없었어",
          "original_form": "근데ㅜ너무 맛없었어",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100036453.1.12",
          "form": "조심해!",
          "original_form": "조심해!",
          "speaker_id": "1",
          "time": "20210830 23:16"
        },
        {
          "id": "MDRW2100036453.1.13",
          "form": "그럼 걸어서 간게 아니겠구나",
          "original_form": "그럼 걸어서 간게 아니겠구나",
          "speaker_id": "2",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100036453.1.14",
          "form": "가격만 싼 곳이야.",
          "original_form": "가격만 싼 곳이야.",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100036453.1.15",
          "form": "걸어서 갈 수 있을 정도야 근데.",
          "original_form": "걸어서 갈 수 있을 정도야 근데.",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100036453.1.16",
          "form": "싸서 갔다온거야?",
          "original_form": "싸서 갔다온거야?",
          "speaker_id": "2",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100036453.1.17",
          "form": "아니 그냥 있길래 가봤어.",
          "original_form": "아니 그냥 있길래 가봤어.",
          "speaker_id": "1",
          "time": "20210830 23:17"
        },
        {
          "id": "MDRW2100036453.1.18",
          "form": "흠 가끔 새로운걸 먹어보고싶지",
          "original_form": "흠 가끔 새로운걸 먹어보고싶지",
          "speaker_id": "2",
          "time": "20210830 23:17"
        }
