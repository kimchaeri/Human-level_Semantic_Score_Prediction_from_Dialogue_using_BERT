      "utterance": [
        {
          "id": "MDRW2100014538.1.1",
          "form": "요새 날씨가 진짜 안좋은거같아요",
          "original_form": "요새 날씨가 진짜 안좋은거같아요",
          "speaker_id": "1",
          "time": "20210825 19:44"
        },
        {
          "id": "MDRW2100014538.1.2",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014538.1.3",
          "form": "비가 하루종일와요",
          "original_form": "비가 하루종일와요",
          "speaker_id": "2",
          "time": "20210825 19:45"
        },
        {
          "id": "MDRW2100014538.1.4",
          "form": "날씨가 더운것도 힘든데 습하기 까지하니까",
          "original_form": "날씨가 더운것도 힘든데 습하기 까지하니까",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014538.1.5",
          "form": "이번 여름이 더 힘들어요",
          "original_form": "이번 여름이 더 힘들어요",
          "speaker_id": "1",
          "time": "20210825 19:46"
        },
        {
          "id": "MDRW2100014538.1.6",
          "form": "제습기 산 사람들이 진정한 승리자 같아요",
          "original_form": "제습기 산 사람들이 진정한 승리자 같아요",
          "speaker_id": "1",
          "time": "20210825 19:47"
        },
        {
          "id": "MDRW2100014538.1.7",
          "form": "ㅋㅋㅋ ㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋ ㅋㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014538.1.8",
          "form": "긍데.제습기는",
          "original_form": "긍데.제습기는",
          "speaker_id": "2",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014538.1.9",
          "form": "좀 더운게 문제에요",
          "original_form": "좀 더운게 문제에요",
          "speaker_id": "2",
          "time": "20210825 19:53"
        },
        {
          "id": "MDRW2100014538.1.10",
          "form": "아 제습기를 틀면 더워지나요",
          "original_form": "아 제습기를 틀면 더워지나요",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014538.1.11",
          "form": "이런",
          "original_form": "이런",
          "speaker_id": "1",
          "time": "20210825 19:55"
        },
        {
          "id": "MDRW2100014538.1.12",
          "form": "좀 뜨거운 공기가 나와요 !!",
          "original_form": "좀 뜨거운 공기가 나와요 !!",
          "speaker_id": "2",
          "time": "20210825 19:56"
        },
        {
          "id": "MDRW2100014538.1.13",
          "form": "에어컨이나 선풍기를 틀고 쓰는게 좋겠네요",
          "original_form": "에어컨이나 선풍기를 틀고 쓰는게 좋겠네요",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014538.1.14",
          "form": "요즘 같은 날씨는 한가지만 하면좋은데",
          "original_form": "요즘 같은 날씨는 한가지만 하면좋은데",
          "speaker_id": "1",
          "time": "20210825 20:01"
        },
        {
          "id": "MDRW2100014538.1.15",
          "form": "습하기만 한게 아니니",
          "original_form": "습하기만 한게 아니니",
          "speaker_id": "1",
          "time": "20210825 20:02"
        },
        {
          "id": "MDRW2100014538.1.16",
          "form": "ㅋㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋㅋ 맞아요",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014538.1.17",
          "form": "요새는 에어컨에 제습이랑도 다 있으닠가요",
          "original_form": "요새는 에어컨에 제습이랑도 다 있으닠가요",
          "speaker_id": "2",
          "time": "20210825 20:22"
        },
        {
          "id": "MDRW2100014538.1.18",
          "form": "에어컨이 좋긴하죠",
          "original_form": "에어컨이 좋긴하죠",
          "speaker_id": "1",
          "time": "20210825 20:28"
        },
        {
          "id": "MDRW2100014538.1.19",
          "form": "4계절중에서 어떤 계절을 제일 좋아하세요?",
          "original_form": "4계절중에서 어떤 계절을 제일 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014538.1.20",
          "form": "전 여름이 제일 좋은데 이번 여름은 좀 힘든거같아요",
          "original_form": "전 여름이 제일 좋은데 이번 여름은 좀 힘든거같아요",
          "speaker_id": "1",
          "time": "20210825 20:29"
        },
        {
          "id": "MDRW2100014538.1.21",
          "form": "헐 여름을 가장 좋아하세요?",
          "original_form": "헐 여름을 가장 좋아하세요?",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100014538.1.22",
          "form": "전 가을이요",
          "original_form": "전 가을이요",
          "speaker_id": "2",
          "time": "20210825 22:08"
        },
        {
          "id": "MDRW2100014538.1.23",
          "form": "가을 좋아하는 사람이 많은거같아요",
          "original_form": "가을 좋아하는 사람이 많은거같아요",
          "speaker_id": "1",
          "time": "20210825 22:21"
        },
        {
          "id": "MDRW2100014538.1.24",
          "form": "날씨가 선선하고 옷입기도 좋은",
          "original_form": "날씨가 선선하고 옷입기도 좋은",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100014538.1.25",
          "form": "여름은 제가 맑은 하늘을 너무 좋아해서ㅋㅋㅋㅋ초여름 한정으로 좋아합니다",
          "original_form": "여름은 제가 맑은 하늘을 너무 좋아해서ㅋㅋㅋㅋ초여름 한정으로 좋아합니다",
          "speaker_id": "1",
          "time": "20210825 22:22"
        },
        {
          "id": "MDRW2100014538.1.26",
          "form": "하늘은 예쁘지만 자외선 너무 쎄서요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "하늘은 예쁘지만 자외선 너무 쎄서요ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100014538.1.27",
          "form": "초여름이면 그나마 괜찮죠",
          "original_form": "초여름이면 그나마 괜찮죠",
          "speaker_id": "2",
          "time": "20210825 22:38"
        },
        {
          "id": "MDRW2100014538.1.28",
          "form": "후반부의 여름은 싫어요 특히 비오는 날의 여름은...",
          "original_form": "후반부의 여름은 싫어요 특히 비오는 날의 여름은...",
          "speaker_id": "1",
          "time": "20210825 23:12"
        },
        {
          "id": "MDRW2100014538.1.29",
          "form": "비올꺼면 가을이나 이런 날씨가 분위기라도 좋은거같아요",
          "original_form": "비올꺼면 가을이나 이런 날씨가 분위기라도 좋은거같아요",
          "speaker_id": "1",
          "time": "20210825 23:13"
        },
        {
          "id": "MDRW2100014538.1.30",
          "form": "네.. 맞아요",
          "original_form": "네.. 맞아요",
          "speaker_id": "2",
          "time": "20210826 08:26"
        },
        {
          "id": "MDRW2100014538.1.31",
          "form": "하지만 비는 절대적으로 반대요 ㅠㅠ",
          "original_form": "하지만 비는 절대적으로 반대요 ㅠㅠ",
          "speaker_id": "2",
