      "utterance": [
        {
          "id": "MDRW2100040350.1.1",
          "form": "여기서도 악역인줄알고 초반 몇편은",
          "original_form": "여기서도 악역인줄알고 초반 몇편은",
          "speaker_id": "2",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100040350.1.2",
          "form": "마자요ㅋㅋㅋㅋ 저거저거",
          "original_form": "마자요ㅋㅋㅋㅋ 저거저거",
          "speaker_id": "1",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100040350.1.3",
          "form": "굉장히 의심하면서 봤습니다 ㅋㅋㅋㅋ",
          "original_form": "굉장히 의심하면서 봤습니다 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100040350.1.4",
          "form": "배신때리겠지",
          "original_form": "배신때리겠지",
          "speaker_id": "1",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100040350.1.5",
          "form": "그니까요 ㅋㅋㅋㅋㅋ 진짜 조마조마 ㅋㅋㅋㅋ",
          "original_form": "그니까요 ㅋㅋㅋㅋㅋ 진짜 조마조마 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100040350.1.6",
          "form": "꿍꿍이가있겠지",
          "original_form": "꿍꿍이가있겠지",
          "speaker_id": "1",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100040350.1.7",
          "form": "이래서 필모가 중요한가봐요ㅋㅋㅋㅋㅋ",
          "original_form": "이래서 필모가 중요한가봐요ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100040350.1.8",
          "form": "그런이미지가 강해서 몇번이나 의심했다구요 ㅋㅋㅋ",
          "original_form": "그런이미지가 강해서 몇번이나 의심했다구요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100040350.1.9",
          "form": "투제니 초반에 장기하노래 나오잖아요",
          "original_form": "투제니 초반에 장기하노래 나오잖아요",
          "speaker_id": "1",
          "time": "20210904 16:51"
        },
        {
          "id": "MDRW2100040350.1.10",
          "form": "네네 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "네네 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:52"
        },
        {
          "id": "MDRW2100040350.1.11",
          "form": "김성철이 부르는 키읔",
          "original_form": "김성철이 부르는 키읔",
          "speaker_id": "1",
          "time": "20210904 16:52"
        },
        {
          "id": "MDRW2100040350.1.12",
          "form": "노래 많이하잖아요 ㅋㅋㅋㅋ",
          "original_form": "노래 많이하잖아요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:52"
        },
        {
          "id": "MDRW2100040350.1.13",
          "form": "저그거 진짜 너무스며들어서 어제새벽부터",
          "original_form": "저그거 진짜 너무스며들어서 어제새벽부터",
          "speaker_id": "1",
          "time": "20210904 16:52"
        },
        {
          "id": "MDRW2100040350.1.14",
          "form": "투제니 다보셨어요?",
          "original_form": "투제니 다보셨어요?",
          "speaker_id": "2",
          "time": "20210904 16:52"
        },
        {
          "id": "MDRW2100040350.1.15",
          "form": "오늘 아침에도 계속 돌려보고 하필 그부분은 유튜브에없더라고요...ㅡ",
          "original_form": "오늘 아침에도 계속 돌려보고 하필 그부분은 유튜브에없더라고요...ㅡ",
          "speaker_id": "1",
          "time": "20210904 16:52"
        },
        {
          "id": "MDRW2100040350.1.16",
          "form": "웨이브 계속 돌려보고ㅋㅋㅋㅋㅋ",
          "original_form": "웨이브 계속 돌려보고ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:52"
        },
        {
          "id": "MDRW2100040350.1.17",
          "form": "전 그거 보고 한동안 티라미수 케이크 계속 들었어요 ㅋㅋㅋㅋ",
          "original_form": "전 그거 보고 한동안 티라미수 케이크 계속 들었어요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:52"
        },
        {
          "id": "MDRW2100040350.1.18",
          "form": "네! 티라미수케잌~~~~티티티라미수~~~",
          "original_form": "네! 티라미수케잌~~~~티티티라미수~~~",
          "speaker_id": "1",
          "time": "20210904 16:52"
        },
        {
          "id": "MDRW2100040350.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:52"
        },
        {
          "id": "MDRW2100040350.1.20",
          "form": "진짜 중독적이에요",
          "original_form": "진짜 중독적이에요",
          "speaker_id": "2",
          "time": "20210904 16:52"
        },
        {
          "id": "MDRW2100040350.1.21",
