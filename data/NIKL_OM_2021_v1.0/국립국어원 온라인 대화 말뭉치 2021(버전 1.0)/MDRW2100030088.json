      "utterance": [
        {
          "id": "MDRW2100030088.1.1",
          "form": "건강을 위해 먹는 영양제 있나요?",
          "original_form": "건강을 위해 먹는 영양제 있나요?",
          "speaker_id": "1",
          "time": "20210903 21:18"
        },
        {
          "id": "MDRW2100030088.1.2",
          "form": "루테인 먹어요...",
          "original_form": "루테인 먹어요...",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100030088.1.3",
          "form": "하루종일 컴퓨터하니 눈이 아파서...",
          "original_form": "하루종일 컴퓨터하니 눈이 아파서...",
          "speaker_id": "2",
          "time": "20210903 21:38"
        },
        {
          "id": "MDRW2100030088.1.4",
          "form": "컴퓨터를 주로 하는 일이군요..",
          "original_form": "컴퓨터를 주로 하는 일이군요..",
          "speaker_id": "1",
          "time": "20210904 05:16"
        },
        {
          "id": "MDRW2100030088.1.5",
          "form": "루테인 많이들 드시더라구요",
          "original_form": "루테인 많이들 드시더라구요",
          "speaker_id": "1",
          "time": "20210904 05:16"
        },
        {
          "id": "MDRW2100030088.1.6",
          "form": "전 감기 걸리지 말라고 비타민씨 먹어요",
          "original_form": "전 감기 걸리지 말라고 비타민씨 먹어요",
          "speaker_id": "1",
          "time": "20210904 05:16"
        },
        {
          "id": "MDRW2100030088.1.7",
          "form": "요새 감기 걸리면 엄청 눈치 보이잖아요",
          "original_form": "요새 감기 걸리면 엄청 눈치 보이잖아요",
          "speaker_id": "1",
          "time": "20210904 05:16"
        },
        {
          "id": "MDRW2100030088.1.8",
          "form": "아 그렇죠",
          "original_form": "아 그렇죠",
          "speaker_id": "2",
          "time": "20210904 07:01"
        },
        {
          "id": "MDRW2100030088.1.9",
          "form": "분명 평범한 감기인데",
          "original_form": "분명 평범한 감기인데",
          "speaker_id": "2",
          "time": "20210904 07:01"
        },
        {
          "id": "MDRW2100030088.1.10",
          "form": "콜록 하는 것으로도..",
          "original_form": "콜록 하는 것으로도..",
          "speaker_id": "2",
          "time": "20210904 07:01"
        },
        {
          "id": "MDRW2100030088.1.11",
          "form": "그러니까요.. 사래 걸려도 꼭 말하잖아요",
          "original_form": "그러니까요.. 사래 걸려도 꼭 말하잖아요",
          "speaker_id": "1",
          "time": "20210904 07:05"
        },
        {
          "id": "MDRW2100030088.1.12",
          "form": "사래걸렸다고 혼잣말처럼",
          "original_form": "사래걸렸다고 혼잣말처럼",
          "speaker_id": "1",
          "time": "20210904 07:05"
        },
        {
          "id": "MDRW2100030088.1.13",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 07:05"
        },
        {
          "id": "MDRW2100030088.1.14",
          "form": "민망하지만",
          "original_form": "민망하지만",
          "speaker_id": "2",
          "time": "20210904 07:05"
        },
        {
          "id": "MDRW2100030088.1.15",
          "form": "안 그러면 눈총을 받는지라...",
          "original_form": "안 그러면 눈총을 받는지라...",
          "speaker_id": "2",
          "time": "20210904 07:05"
        },
        {
          "id": "MDRW2100030088.1.16",
          "form": "참 코시국이 많은 걸 바꿔놨어요",
          "original_form": "참 코시국이 많은 걸 바꿔놨어요",
          "speaker_id": "1",
          "time": "20210904 07:06"
        },
        {
          "id": "MDRW2100030088.1.17",
          "form": "전 비타민디도 먹는데 한국인이 의외로 햇빛을 마니 안받는다고 하더라ㅜㄱ요",
          "original_form": "전 비타민디도 먹는데 한국인이 의외로 햇빛을 마니 안받는다고 하더라ㅜㄱ요",
          "speaker_id": "1",
          "time": "20210904 07:06"
        },
        {
          "id": "MDRW2100030088.1.18",
          "form": "아아",
          "original_form": "아아",
          "speaker_id": "2",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100030088.1.19",
          "form": "그러고보니",
          "original_form": "그러고보니",
          "speaker_id": "2",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100030088.1.20",
          "form": "비타민 씨랑 디 같이 들어있는",
          "original_form": "비타민 씨랑 디 같이 들어있는",
          "speaker_id": "2",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100030088.1.21",
          "form": "젤리도 잠깐 먹었었어요",
          "original_form": "젤리도 잠깐 먹었었어요",
          "speaker_id": "2",
          "time": "20210904 13:26"
        },
        {
          "id": "MDRW2100030088.1.22",
          "form": "영양제인데 그냥 맛있더라고요",
          "original_form": "영양제인데 그냥 맛있더라고요",
          "speaker_id": "2",
          "time": "20210904 13:27"
        },
        {
          "id": "MDRW2100030088.1.23",
          "form": "비타민씨는 사탕같죠..;)",
          "original_form": "비타민씨는 사탕같죠..;)",
          "speaker_id": "1",
          "time": "20210904 15:55"
        },
        {
          "id": "MDRW2100030088.1.24",
          "form": "그렇긴 하죠",
          "original_form": "그렇긴 하죠",
          "speaker_id": "2",
          "time": "20210904 18:02"
        },
        {
          "id": "MDRW2100030088.1.25",
          "form": "기본적으로 새콤해서",
          "original_form": "기본적으로 새콤해서",
          "speaker_id": "2",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100030088.1.26",
          "form": "달콤만 있으면 훌륭한 간식",
          "original_form": "달콤만 있으면 훌륭한 간식",
          "speaker_id": "2",
          "time": "20210904 18:03"
        },
        {
          "id": "MDRW2100030088.1.27",
          "form": "그래서 비타민은 약이란 느낌 안들고 먹기 좋아요",
          "original_form": "그래서 비타민은 약이란 느낌 안들고 먹기 좋아요",
          "speaker_id": "1",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100030088.1.28",
          "form": "유산균도 약간 우유맛나서 먹기 어렵지 않구요",
          "original_form": "유산균도 약간 우유맛나서 먹기 어렵지 않구요",
          "speaker_id": "1",
          "time": "20210904 20:39"
        },
        {
          "id": "MDRW2100030088.1.29",
          "form": "그렇죠",
          "original_form": "그렇죠",
          "speaker_id": "2",
          "time": "20210904 21:09"
        },
        {
          "id": "MDRW2100030088.1.30",
          "form": "아 유산균은",
          "original_form": "아 유산균은",
          "speaker_id": "2",
          "time": "20210904 21:09"
        },
        {
          "id": "MDRW2100030088.1.31",
          "form": "뭔가 요구르트처럼",
          "original_form": "뭔가 요구르트처럼",
          "speaker_id": "2",
          "time": "20210904 21:09"
        },
        {
          "id": "MDRW2100030088.1.32",
          "form": "맛있은 음료로 채울 수도 있구요",
          "original_form": "맛있은 음료로 채울 수도 있구요",
          "speaker_id": "2",
          "time": "20210904 21:09"
        },
        {
          "id": "MDRW2100030088.1.33",
          "form": "마자요",
          "original_form": "마자요",
          "speaker_id": "1",
          "time": "20210904 21:22"
        },
        {
          "id": "MDRW2100030088.1.34",
          "form": "영양제 말고도 건강을 위해 운동도 할 수 있는데",
          "original_form": "영양제 말고도 건강을 위해 운동도 할 수 있는데",
