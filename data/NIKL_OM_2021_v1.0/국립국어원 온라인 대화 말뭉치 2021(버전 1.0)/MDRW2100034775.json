      "utterance": [
        {
          "id": "MDRW2100034775.1.1",
          "form": "최근에 구매하신거 있으세요?!",
          "original_form": "최근에 구매하신거 있으세요?!",
          "speaker_id": "2",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100034775.1.2",
          "form": "저는 운동화 샀어용!ㅎㅎ",
          "original_form": "저는 운동화 샀어용!ㅎㅎ",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100034775.1.3",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210828 21:31"
        },
        {
          "id": "MDRW2100034775.1.4",
          "form": "오 ! 저는 가을 니트샀어요",
          "original_form": "오 ! 저는 가을 니트샀어요",
          "speaker_id": "2",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100034775.1.5",
          "form": "어디 운동화 사셨어요 ㅎㅎ ?",
          "original_form": "어디 운동화 사셨어요 ㅎㅎ ?",
          "speaker_id": "2",
          "time": "20210828 21:34"
        },
        {
          "id": "MDRW2100034775.1.6",
          "form": "가을하면 니트의 계절이죵!ㅋㅋㅋㅋ",
          "original_form": "가을하면 니트의 계절이죵!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100034775.1.7",
          "form": "저는 나이키 넘 좋아해서",
          "original_form": "저는 나이키 넘 좋아해서",
          "speaker_id": "1",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100034775.1.8",
          "form": "나이키샀어요ㅋㅋㅋㅋ",
          "original_form": "나이키샀어요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:35"
        },
        {
          "id": "MDRW2100034775.1.9",
          "form": "선호하는 의류나 신발 브랜드 있으세요?",
          "original_form": "선호하는 의류나 신발 브랜드 있으세요?",
          "speaker_id": "1",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034775.1.10",
          "form": "저도 나이키 좋아해요 !!",
          "original_form": "저도 나이키 좋아해요 !!",
          "speaker_id": "2",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034775.1.11",
          "form": "나이키 아디다스 좋아합니다 ㅎㅎ",
          "original_form": "나이키 아디다스 좋아합니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:36"
        },
        {
          "id": "MDRW2100034775.1.12",
          "form": "ㅎㅎ 저랑 비슷하시네용",
          "original_form": "ㅎㅎ 저랑 비슷하시네용",
          "speaker_id": "1",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100034775.1.13",
          "form": "나이키 코트로얄이라는 운동화있는데",
          "original_form": "나이키 코트로얄이라는 운동화있는데",
          "speaker_id": "1",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100034775.1.14",
          "form": "가격대도 저렴한편이고 예뻐요",
          "original_form": "가격대도 저렴한편이고 예뻐요",
          "speaker_id": "1",
          "time": "20210828 21:37"
        },
        {
          "id": "MDRW2100034775.1.15",
          "form": "오오 ! 검색해봤는데",
          "original_form": "오오 ! 검색해봤는데",
          "speaker_id": "2",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100034775.1.16",
          "form": "이런 기본 디자인 좋아요 ㅎㅎㅎ",
          "original_form": "이런 기본 디자인 좋아요 ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100034775.1.17",
          "form": "착용감도 편한가요?",
          "original_form": "착용감도 편한가요?",
          "speaker_id": "2",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100034775.1.18",
          "form": "그쵸ㅎㅎㅎ 가죽이라 비와도 물 안들어가요",
          "original_form": "그쵸ㅎㅎㅎ 가죽이라 비와도 물 안들어가요",
          "speaker_id": "1",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100034775.1.19",
          "form": "음 런닝화 위주로 신으셨으면",
          "original_form": "음 런닝화 위주로 신으셨으면",
          "speaker_id": "1",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100034775.1.20",
          "form": "조오금 무겁다고 느끼실수도 있는데",
          "original_form": "조오금 무겁다고 느끼실수도 있는데",
          "speaker_id": "1",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100034775.1.21",
          "form": "편하긴 편해요",
          "original_form": "편하긴 편해요",
          "speaker_id": "1",
          "time": "20210828 21:39"
        },
        {
          "id": "MDRW2100034775.1.22",
          "form": "오오 ㅎㅎ 담에 한번 매장가서 신어봐야겠어요 !",
          "original_form": "오오 ㅎㅎ 담에 한번 매장가서 신어봐야겠어요 !",
          "speaker_id": "2",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100034775.1.23",
          "form": "이번에 사신게 이 제품인가요?ㅎㅎ",
          "original_form": "이번에 사신게 이 제품인가요?ㅎㅎ",
          "speaker_id": "2",
          "time": "20210828 21:41"
        },
        {
          "id": "MDRW2100034775.1.24",
          "form": "네넹! 이거 떨어지면 또사고 또사고 그래요ㅋㅋㅋㅋ",
          "original_form": "네넹! 이거 떨어지면 또사고 또사고 그래요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210828 21:42"
        },
        {
          "id": "MDRW2100034775.1.25",
          "form": "오오ㅋㅋㅋ 색깔은 어떤걸로 사셨어요?",
          "original_form": "오오ㅋㅋㅋ 색깔은 어떤걸로 사셨어요?",
          "speaker_id": "2",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100034775.1.26",
          "form": "저는 늘 화이트로 사용!",
          "original_form": "저는 늘 화이트로 사용!",
          "speaker_id": "1",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100034775.1.27",
          "form": "name2님은 최근에 구매하시고",
          "original_form": "&name2&님은 최근에 구매하시고",
          "speaker_id": "1",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100034775.1.28",
          "form": "만족하신거 있으세요?",
          "original_form": "만족하신거 있으세요?",
          "speaker_id": "1",
          "time": "20210828 21:43"
        },
        {
          "id": "MDRW2100034775.1.29",
          "form": "전 최근에 아디다스 운동화 샀었어요 !!",
