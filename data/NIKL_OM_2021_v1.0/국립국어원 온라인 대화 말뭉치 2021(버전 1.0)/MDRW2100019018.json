      "utterance": [
        {
          "id": "MDRW2100019018.1.1",
          "form": "요즘 뾰루지가 걱정입니다",
          "original_form": "요즘 뾰루지가 걱정입니다",
          "speaker_id": "1",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019018.1.2",
          "form": "건강이 안좋나봐요",
          "original_form": "건강이 안좋나봐요",
          "speaker_id": "1",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019018.1.3",
          "form": "네 그럴땐 녹차성분이 좋아요",
          "original_form": "네 그럴땐 녹차성분이 좋아요",
          "speaker_id": "2",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019018.1.4",
          "form": "피부진정",
          "original_form": "피부진정",
          "speaker_id": "2",
          "time": "20210827 14:41"
        },
        {
          "id": "MDRW2100019018.1.5",
          "form": "음 녹차요?",
          "original_form": "음 녹차요?",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019018.1.6",
          "form": "참고하겠습니다.",
          "original_form": "참고하겠습니다.",
          "speaker_id": "1",
          "time": "20210827 14:47"
        },
        {
          "id": "MDRW2100019018.1.7",
          "form": "저도 기름진 음식 먹다보니 살도 찌고 그래요",
          "original_form": "저도 기름진 음식 먹다보니 살도 찌고 그래요",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019018.1.8",
          "form": "운동은 걷기 밖에 안해서요",
          "original_form": "운동은 걷기 밖에 안해서요",
          "speaker_id": "2",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019018.1.9",
          "form": "아하 글쿤용 저도 걷기는 종종해요",
          "original_form": "아하 글쿤용 저도 걷기는 종종해요",
          "speaker_id": "1",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100019018.1.10",
          "form": "저는 밀가루가 안맞아서 줄이려고 하고 있어요",
          "original_form": "저는 밀가루가 안맞아서 줄이려고 하고 있어요",
          "speaker_id": "2",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100019018.1.11",
          "form": "음 근데 밀가루가 맛있쥬",
          "original_form": "음 근데 밀가루가 맛있쥬",
          "speaker_id": "1",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100019018.1.12",
          "form": "넹 빵 면 모두 좋아하는데",
          "original_form": "넹 빵 면 모두 좋아하는데",
          "speaker_id": "2",
          "time": "20210827 15:05"
        },
        {
          "id": "MDRW2100019018.1.13",
          "form": "음 다이어트는 하세요?",
          "original_form": "음 다이어트는 하세요?",
          "speaker_id": "1",
          "time": "20210827 15:14"
        },
        {
          "id": "MDRW2100019018.1.14",
          "form": "아니요 그냥 자연스럽게 조절해요",
          "original_form": "아니요 그냥 자연스럽게 조절해요",
          "speaker_id": "2",
          "time": "20210827 15:16"
        },
        {
          "id": "MDRW2100019018.1.15",
          "form": "님은요",
          "original_form": "님은요",
          "speaker_id": "2",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019018.1.16",
          "form": "오호 .. 저는 항상 다이어트 중입니다ㅜㅜ",
          "original_form": "오호 .. 저는 항상 다이어트 중입니다ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019018.1.17",
          "form": "항상 신경쓰는게 좋죠",
          "original_form": "항상 신경쓰는게 좋죠",
          "speaker_id": "2",
          "time": "20210827 15:17"
        },
        {
          "id": "MDRW2100019018.1.18",
          "form": "ㅜㅜ 근데 전혀 안 빠집니다",
          "original_form": "ㅜㅜ 근데 전혀 안 빠집니다",
          "speaker_id": "1",
