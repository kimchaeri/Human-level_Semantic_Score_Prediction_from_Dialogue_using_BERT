      "utterance": [
        {
          "id": "MDRW2100017085.1.1",
          "form": "슬개골 수술 비싸다",
          "original_form": "슬개골 수술 비싸다",
          "speaker_id": "1",
          "time": "20210826 17:19"
        },
        {
          "id": "MDRW2100017085.1.2",
          "form": "병원비 비싸요",
          "original_form": "병원비 비싸요",
          "speaker_id": "2",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017085.1.3",
          "form": "의료보험 해줘라",
          "original_form": "의료보험 해줘라",
          "speaker_id": "1",
          "time": "20210826 17:22"
        },
        {
          "id": "MDRW2100017085.1.4",
          "form": "사료 뭐 먹이죠",
          "original_form": "사료 뭐 먹이죠",
          "speaker_id": "2",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017085.1.5",
          "form": "블랙우드 라지브리드 먹여요",
          "original_form": "블랙우드 라지브리드 먹여요",
          "speaker_id": "1",
          "time": "20210826 17:23"
        },
        {
          "id": "MDRW2100017085.1.6",
          "form": "잘먹나요?",
          "original_form": "잘먹나요?",
          "speaker_id": "2",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017085.1.7",
          "form": "잘먹죠 하루에 두끼나 먹어요",
          "original_form": "잘먹죠 하루에 두끼나 먹어요",
          "speaker_id": "1",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017085.1.8",
          "form": "당신의 개는 다리가 짧은가요",
          "original_form": "당신의 개는 다리가 짧은가요",
          "speaker_id": "2",
          "time": "20210826 17:24"
        },
        {
          "id": "MDRW2100017085.1.9",
          "form": "많이 짧아요",
          "original_form": "많이 짧아요",
          "speaker_id": "1",
          "time": "20210826 17:25"
        },
        {
          "id": "MDRW2100017085.1.10",
          "form": "슬프네요",
          "original_form": "슬프네요",
          "speaker_id": "2",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017085.1.11",
          "form": "웰시코기 귀엽죠",
          "original_form": "웰시코기 귀엽죠",
          "speaker_id": "2",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017085.1.12",
          "form": "하지만 잘 달려요",
          "original_form": "하지만 잘 달려요",
          "speaker_id": "1",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017085.1.13",
          "form": "네 엄청 귀엽다구요",
          "original_form": "네 엄청 귀엽다구요",
          "speaker_id": "1",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017085.1.14",
          "form": "짧으면 못달리는게아닌가요",
          "original_form": "짧으면 못달리는게아닌가요",
          "speaker_id": "2",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017085.1.15",
          "form": "알러지도 없어요",
          "original_form": "알러지도 없어요",
          "speaker_id": "1",
          "time": "20210826 17:27"
        },
        {
          "id": "MDRW2100017085.1.16",
          "form": "햐 좋군요",
          "original_form": "햐 좋군요",
          "speaker_id": "2",
          "time": "20210826 17:28"
        },
        {
          "id": "MDRW2100017085.1.17",
          "form": "코기는 좋은개예요",
          "original_form": "코기는 좋은개예요",
          "speaker_id": "2",
          "time": "20210826 17:29"
        }
