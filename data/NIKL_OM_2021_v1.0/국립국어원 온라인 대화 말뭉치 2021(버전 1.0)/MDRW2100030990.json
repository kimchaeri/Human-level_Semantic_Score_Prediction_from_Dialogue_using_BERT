      "utterance": [
        {
          "id": "MDRW2100030990.1.1",
          "form": "안녕하세요!",
          "original_form": "안녕하세요!",
          "speaker_id": "1",
          "time": "20210904 15:12"
        },
        {
          "id": "MDRW2100030990.1.2",
          "form": "반갑습니당",
          "original_form": "반갑습니당",
          "speaker_id": "2",
          "time": "20210904 15:13"
        },
        {
          "id": "MDRW2100030990.1.3",
          "form": "좋아하시는 운동있으신가요?",
          "original_form": "좋아하시는 운동있으신가요?",
          "speaker_id": "1",
          "time": "20210904 15:14"
        },
        {
          "id": "MDRW2100030990.1.4",
          "form": "운동은 숨쉬기 운동...과 산책만",
          "original_form": "운동은 숨쉬기 운동...과 산책만",
          "speaker_id": "2",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100030990.1.5",
          "form": "name1님은 어떠세요?",
          "original_form": "&name1&님은 어떠세요?",
          "speaker_id": "2",
          "time": "20210904 15:43"
        },
        {
          "id": "MDRW2100030990.1.6",
          "form": "앗 저도 산책 엄청 좋아해요",
          "original_form": "앗 저도 산책 엄청 좋아해요",
          "speaker_id": "1",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100030990.1.7",
          "form": "방금도 산책하고오는길입니당",
          "original_form": "방금도 산책하고오는길입니당",
          "speaker_id": "1",
          "time": "20210904 15:44"
        },
        {
          "id": "MDRW2100030990.1.8",
          "form": "오오, 집주변에 산책하기 좋은 곳 있나요",
          "original_form": "오오, 집주변에 산책하기 좋은 곳 있나요",
          "speaker_id": "2",
          "time": "20210904 16:07"
        },
        {
          "id": "MDRW2100030990.1.9",
          "form": "집 주변보다는 보통 어디나갔다오는길을 걸어오는걸 좋아해요!",
          "original_form": "집 주변보다는 보통 어디나갔다오는길을 걸어오는걸 좋아해요!",
          "speaker_id": "1",
          "time": "20210904 16:12"
        },
        {
          "id": "MDRW2100030990.1.10",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210904 16:12"
        },
        {
          "id": "MDRW2100030990.1.11",
          "form": "아하, 그러시군요",
          "original_form": "아하, 그러시군요",
          "speaker_id": "2",
          "time": "20210904 16:13"
        },
        {
          "id": "MDRW2100030990.1.12",
          "form": "저는 한두시간 걸리는 맛집 알아보고 거기까지 왔다갔다 걷는 거 가끔 해요ㅋㅋㅋ",
          "original_form": "저는 한두시간 걸리는 맛집 알아보고 거기까지 왔다갔다 걷는 거 가끔 해요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 16:13"
        },
        {
          "id": "MDRW2100030990.1.13",
          "form": "일종의 원정",
          "original_form": "일종의 원정",
          "speaker_id": "2",
          "time": "20210904 16:13"
        },
        {
          "id": "MDRW2100030990.1.14",
          "form": "산책이라면 산책인",
          "original_form": "산책이라면 산책인",
          "speaker_id": "2",
          "time": "20210904 16:13"
        },
        {
          "id": "MDRW2100030990.1.15",
          "form": "헉저도 그런거 좋아요",
          "original_form": "헉저도 그런거 좋아요",
          "speaker_id": "1",
          "time": "20210904 16:15"
        },
        {
          "id": "MDRW2100030990.1.16",
          "form": "그러면 왠지 맛집에서 잔뜩먹어도",
          "original_form": "그러면 왠지 맛집에서 잔뜩먹어도",
          "speaker_id": "1",
          "time": "20210904 16:15"
        },
        {
          "id": "MDRW2100030990.1.17",
          "form": "걸어온걸로 합리화할수있자나요히히",
          "original_form": "걸어온걸로 합리화할수있자나요히히",
          "speaker_id": "1",
          "time": "20210904 16:15"
        },
        {
          "id": "MDRW2100030990.1.18",
          "form": "맞아요ㅎㅎ 먹고 걷고 일석이조(?)",
          "original_form": "맞아요ㅎㅎ 먹고 걷고 일석이조(?)",
          "speaker_id": "2",
          "time": "20210904 16:16"
        },
        {
          "id": "MDRW2100030990.1.19",
          "form": "백신은 맞으셨어요?",
          "original_form": "백신은 맞으셨어요?",
          "speaker_id": "2",
          "time": "20210904 16:21"
        },
        {
          "id": "MDRW2100030990.1.20",
          "form": "저 다음주에 맞아요!",
          "original_form": "저 다음주에 맞아요!",
          "speaker_id": "1",
          "time": "20210904 16:26"
        },
        {
          "id": "MDRW2100030990.1.21",
          "form": "부작용이 좀 무서워서 다음주 일정은 비워놨는데",
          "original_form": "부작용이 좀 무서워서 다음주 일정은 비워놨는데",
          "speaker_id": "1",
          "time": "20210904 16:27"
        },
        {
          "id": "MDRW2100030990.1.22",
          "form": "name2님은 맞으셨나요?",
          "original_form": "&name2&님은 맞으셨나요?",
          "speaker_id": "1",
          "time": "20210904 16:27"
        },
        {
          "id": "MDRW2100030990.1.23",
          "form": "이미맞은제친구들 얘기들어보면 케바케가 큰것같더라구요",
          "original_form": "이미맞은제친구들 얘기들어보면 케바케가 큰것같더라구요",
          "speaker_id": "1",
          "time": "20210904 16:27"
        },
        {
          "id": "MDRW2100030990.1.24",
          "form": "저는 14일이요",
          "original_form": "저는 14일이요",
          "speaker_id": "2",
          "time": "20210904 16:36"
        },
        {
          "id": "MDRW2100030990.1.25",
          "form": "가족들이랑 친구들은 다 맞았는데",
          "original_form": "가족들이랑 친구들은 다 맞았는데",
          "speaker_id": "2",
          "time": "20210904 16:36"
        },
        {
          "id": "MDRW2100030990.1.26",
          "form": "진짜 케이스바이케이스 같아요",
          "original_form": "진짜 케이스바이케이스 같아요",
          "speaker_id": "2",
          "time": "20210904 16:36"
        },
        {
          "id": "MDRW2100030990.1.27",
          "form": "건강하면 부작용도 덜하려나요ㅋㅋㅋㅋ",
          "original_form": "건강하면 부작용도 덜하려나요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 16:41"
        },
        {
          "id": "MDRW2100030990.1.28",
          "form": "요즘 갑자기 건강이 절실해져서 운동도 하게되더라구요",
          "original_form": "요즘 갑자기 건강이 절실해져서 운동도 하게되더라구요",
          "speaker_id": "1",
          "time": "20210904 16:41"
        }
      ]
    }
