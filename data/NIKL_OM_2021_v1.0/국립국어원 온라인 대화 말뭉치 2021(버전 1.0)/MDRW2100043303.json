      "utterance": [
        {
          "id": "MDRW2100043303.1.1",
          "form": "연애중이세요?",
          "original_form": "연애중이세요?",
          "speaker_id": "1",
          "time": "20210907 17:10"
        },
        {
          "id": "MDRW2100043303.1.2",
          "form": "아니요~ 연애하고 싶으면서 안하고 싶어요ㅋㅋㅋ",
          "original_form": "아니요~ 연애하고 싶으면서 안하고 싶어요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 17:11"
        },
        {
          "id": "MDRW2100043303.1.3",
          "form": "연애하고 계시나요?",
          "original_form": "연애하고 계시나요?",
          "speaker_id": "2",
          "time": "20210907 17:11"
        },
        {
          "id": "MDRW2100043303.1.4",
          "form": "왜용",
          "original_form": "왜용",
          "speaker_id": "1",
          "time": "20210907 17:12"
        },
        {
          "id": "MDRW2100043303.1.5",
          "form": "왜 연애안하고 싶냐구요?",
          "original_form": "왜 연애안하고 싶냐구요?",
          "speaker_id": "2",
          "time": "20210907 17:13"
        },
        {
          "id": "MDRW2100043303.1.6",
          "form": "네넵",
          "original_form": "네넵",
          "speaker_id": "1",
          "time": "20210907 17:14"
        },
        {
          "id": "MDRW2100043303.1.7",
          "form": "전 혼자가 더 좋더라구요..아직 진짜 좋아하는 사람이랑 못만나봐서 그런지ㅎㅎ",
          "original_form": "전 혼자가 더 좋더라구요..아직 진짜 좋아하는 사람이랑 못만나봐서 그런지ㅎㅎ",
          "speaker_id": "2",
          "time": "20210907 17:15"
        },
        {
          "id": "MDRW2100043303.1.8",
          "form": "그럴수 있죠 저도 나29인데",
          "original_form": "그럴수 있죠 저도 나29인데",
          "speaker_id": "1",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100043303.1.9",
          "form": "나이",
          "original_form": "나이",
          "speaker_id": "1",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100043303.1.10",
          "form": "제 친구도 28부터 연애하더라고요 ㅋㅋㅋ",
          "original_form": "제 친구도 28부터 연애하더라고요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100043303.1.11",
          "form": "그전까지 관심이아예없드니",
          "original_form": "그전까지 관심이아예없드니",
          "speaker_id": "1",
          "time": "20210907 17:16"
        },
        {
          "id": "MDRW2100043303.1.12",
          "form": "혹시 나중에 결혼 하실건가요?",
          "original_form": "혹시 나중에 결혼 하실건가요?",
          "speaker_id": "2",
          "time": "20210907 17:19"
        },
        {
          "id": "MDRW2100043303.1.13",
          "form": "제 주위에는 비혼주의인 친구들이 더 많더라구요",
          "original_form": "제 주위에는 비혼주의인 친구들이 더 많더라구요",
          "speaker_id": "2",
          "time": "20210907 17:19"
        },
        {
          "id": "MDRW2100043303.1.14",
          "form": "아 저는 결혼했습니다 작년에요 ~",
          "original_form": "아 저는 결혼했습니다 작년에요 ~",
          "speaker_id": "1",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100043303.1.15",
          "form": "비혼도 좋은거같아요",
          "original_form": "비혼도 좋은거같아요",
          "speaker_id": "1",
          "time": "20210907 17:24"
        },
        {
          "id": "MDRW2100043303.1.16",
          "form": "아 진짜요? 늦었지만 축하드려요",
          "original_form": "아 진짜요? 늦었지만 축하드려요",
          "speaker_id": "2",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100043303.1.17",
          "form": "감사합니다 ㅋㅋ 이상형이 어떻게 되세요",
          "original_form": "감사합니다 ㅋㅋ 이상형이 어떻게 되세요",
          "speaker_id": "1",
          "time": "20210907 17:28"
        },
        {
          "id": "MDRW2100043303.1.18",
          "form": "전 일단 대화 잘 통하고 저랑 잘 맞는 사람이요~",
          "original_form": "전 일단 대화 잘 통하고 저랑 잘 맞는 사람이요~",
