        },
        {
          "id": "MDRW2100019521.1.2",
          "form": "name1님은 싸이월드 많이 하셨나요?",
          "original_form": "&name1&님은 싸이월드 많이 하셨나요?",
          "speaker_id": "2",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019521.1.3",
          "form": "저는 그다지 즐겨하진 않고",
          "original_form": "저는 그다지 즐겨하진 않고",
          "speaker_id": "2",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019521.1.4",
          "form": "전 싸이월드 유명할때 하지않았습니다\r\n지조있게...",
          "original_form": "전 싸이월드 유명할때 하지않았습니다\r\n지조있게...",
          "speaker_id": "1",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019521.1.5",
          "form": "제 주변 사람들이 많이 하는 것만 봤어ㅛ",
          "original_form": "제 주변 사람들이 많이 하는 것만 봤어ㅛ",
          "speaker_id": "2",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019521.1.6",
          "form": "그래서 흑역사가 남은게 없네요",
          "original_form": "그래서 흑역사가 남은게 없네요",
          "speaker_id": "1",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019521.1.7",
          "form": "요",
          "original_form": "요",
          "speaker_id": "2",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019521.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋ저도요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ저도요",
          "speaker_id": "2",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019521.1.9",
          "form": "싸이월드 부활로 제가 샀던 주식이 갑자기",
          "original_form": "싸이월드 부활로 제가 샀던 주식이 갑자기",
          "speaker_id": "1",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019521.1.10",
          "form": "지금 돌이켜보면 참 잘한 일이란 생각이 듭니다",
          "original_form": "지금 돌이켜보면 참 잘한 일이란 생각이 듭니다",
          "speaker_id": "2",
          "time": "20210827 20:03"
        },
        {
          "id": "MDRW2100019521.1.11",
          "form": "65프로 오른 기억은 있습니다",
          "original_form": "65프로 오른 기억은 있습니다",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019521.1.12",
          "form": "ㅋㅋㅌㅋㅋㅋㅋㅋㅋ축하드려요",
          "original_form": "ㅋㅋㅌㅋㅋㅋㅋㅋㅋ축하드려요",
          "speaker_id": "2",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019521.1.13",
          "form": "그만큼 사람들이 관심이",
          "original_form": "그만큼 사람들이 관심이",
          "speaker_id": "2",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019521.1.14",
          "form": "높았나봐요",
          "original_form": "높았나봐요",
          "speaker_id": "2",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019521.1.15",
          "form": "저는 지금도 노관심인데",
          "original_form": "저는 지금도 노관심인데",
          "speaker_id": "2",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019521.1.16",
          "form": "신기할 따름ㅎ",
          "original_form": "신기할 따름ㅎ",
          "speaker_id": "2",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019521.1.17",
          "form": "싸이월드 화폐 그 뭐더라",
          "original_form": "싸이월드 화폐 그 뭐더라",
          "speaker_id": "2",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019521.1.18",
          "form": "이번에 싸이월드가 부활하면",
          "original_form": "이번에 싸이월드가 부활하면",
          "speaker_id": "1",
          "time": "20210827 20:04"
        },
        {
          "id": "MDRW2100019521.1.19",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019521.1.20",
          "form": "데이터가 복구된다고 하더라구욧",
          "original_form": "데이터가 복구된다고 하더라구욧",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019521.1.21",
          "form": "화폐 도토리예요",
          "original_form": "화폐 도토리예요",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019521.1.22",
          "form": "아 그래서 다들 흑역사를",
          "original_form": "아 그래서 다들 흑역사를",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019521.1.23",
          "form": "직접 불태우러",
          "original_form": "직접 불태우러",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019521.1.24",
          "form": "갔나",
          "original_form": "갔나",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019521.1.25",
          "form": "도토리의 시세를 모르겠네요",
          "original_form": "도토리의 시세를 모르겠네요",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019521.1.26",
          "form": "도토리 환불이니 뭐니로",
          "original_form": "도토리 환불이니 뭐니로",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019521.1.27",
          "form": "옛날에 친구들이 현질한다고 눈에 핏발섰던데",
          "original_form": "옛날에 친구들이 현질한다고 눈에 핏발섰던데",
          "speaker_id": "1",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019521.1.28",
          "form": "좀 시끌시끌하지 않았나요",
          "original_form": "좀 시끌시끌하지 않았나요",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019521.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋ핏발이래",
          "original_form": "ㅋㅋㅋㅋㅋㅋ핏발이래",
          "speaker_id": "2",
          "time": "20210827 20:05"
        },
        {
          "id": "MDRW2100019521.1.30",
          "form": "환불도 참 옛날에 몇천원정도 아닐까 싶습니다",
          "original_form": "환불도 참 옛날에 몇천원정도 아닐까 싶습니다",
          "speaker_id": "1",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019521.1.31",
          "form": "그렇지않고서야 저렇게 당당하게 환불해준다고",
          "original_form": "그렇지않고서야 저렇게 당당하게 환불해준다고",
          "speaker_id": "1",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019521.1.32",
          "form": "말할리가 없어요",
          "original_form": "말할리가 없어요",
          "speaker_id": "1",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019521.1.33",
          "form": "그러게요 별 타격 없으니까",
          "original_form": "그러게요 별 타격 없으니까",
          "speaker_id": "2",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019521.1.34",
          "form": "환불해주는 거일 것 같아요",
          "original_form": "환불해주는 거일 것 같아요",
          "speaker_id": "2",
          "time": "20210827 20:06"
        },
        {
          "id": "MDRW2100019521.1.35",
          "form": "애들 코묻은 돈",
          "original_form": "애들 코묻은 돈",
          "speaker_id": "2",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019521.1.36",
          "form": "또 환불하러 들어갔다가 추억팔이로",
          "original_form": "또 환불하러 들어갔다가 추억팔이로",
          "speaker_id": "1",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019521.1.37",
          "form": "홍보효과를 누릴려고",
          "original_form": "홍보효과를 누릴려고",
          "speaker_id": "1",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019521.1.38",
          "form": "머릴쓴것같아요",
          "original_form": "머릴쓴것같아요",
          "speaker_id": "1",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019521.1.39",
          "form": "맞아요ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019521.1.40",
          "form": "괜히 들어갔다가",
          "original_form": "괜히 들어갔다가",
          "speaker_id": "2",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019521.1.41",
          "form": "옛 추억에 젖어서",
          "original_form": "옛 추억에 젖어서",
          "speaker_id": "2",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019521.1.42",
          "form": "그대로 눌러붙으면 싸이월드 입장에서야 땡큐니까",
          "original_form": "그대로 눌러붙으면 싸이월드 입장에서야 땡큐니까",
          "speaker_id": "2",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019521.1.43",
          "form": "싸이월드의 부활이 성공일지 실패일지 예상해보셨나요?",
          "original_form": "싸이월드의 부활이 성공일지 실패일지 예상해보셨나요?",
          "speaker_id": "1",
          "time": "20210827 20:07"
        },
        {
          "id": "MDRW2100019521.1.44",
          "form": "메타버스라는 개념이 생긴지금",
          "original_form": "메타버스라는 개념이 생긴지금",
          "speaker_id": "1",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100019521.1.45",
          "form": "싸이월드는 어떻게 부활할까요",
          "original_form": "싸이월드는 어떻게 부활할까요",
          "speaker_id": "1",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100019521.1.46",
          "form": "싸이월드가 이전이랑 다르게",
          "original_form": "싸이월드가 이전이랑 다르게",
          "speaker_id": "2",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100019521.1.47",
          "form": "최근 트렌드에 맞는 서비스를",
          "original_form": "최근 트렌드에 맞는 서비스를",
          "speaker_id": "2",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100019521.1.48",
          "form": "새로 개발한다면 숨을 이어갈 수는 있을 듯한데",
          "original_form": "새로 개발한다면 숨을 이어갈 수는 있을 듯한데",
          "speaker_id": "2",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100019521.1.49",
          "form": "만약 기존 미니홈피서비스에서",
          "original_form": "만약 기존 미니홈피서비스에서",
          "speaker_id": "2",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100019521.1.50",
          "form": "변하지 않는다면",
          "original_form": "변하지 않는다면",
          "speaker_id": "2",
          "time": "20210827 20:08"
        },
        {
          "id": "MDRW2100019521.1.51",
          "form": "딱히 사람들이 쓸 메리트가 없어보여요",
          "original_form": "딱히 사람들이 쓸 메리트가 없어보여요",
          "speaker_id": "2",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019521.1.52",
          "form": "미니홈피 꾸며봤자 할 수 있는 게 한계가 있어서",
          "original_form": "미니홈피 꾸며봤자 할 수 있는 게 한계가 있어서",
          "speaker_id": "2",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019521.1.53",
          "form": "차라리 블로그나 카페나 티스토리 갈 듯",
          "original_form": "차라리 블로그나 카페나 티스토리 갈 듯",
          "speaker_id": "2",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019521.1.54",
          "form": "...?",
          "original_form": "...?",
          "speaker_id": "1",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019521.1.55",
          "form": "아니면 아예 게임이라던가",
          "original_form": "아니면 아예 게임이라던가",
          "speaker_id": "2",
          "time": "20210827 20:09"
        },
        {
          "id": "MDRW2100019521.1.56",
          "form": "나쁜말이라고",
          "original_form": "나쁜말이라고",
          "speaker_id": "2",
          "time": "20210827 20:10"
        },
        {
          "id": "MDRW2100019521.1.57",
          "form": "아바타를 어떻게 꾸미느냐가 중요할것같아요",
