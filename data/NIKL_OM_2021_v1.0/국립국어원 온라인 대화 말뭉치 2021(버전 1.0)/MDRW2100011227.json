          "speaker_id": "2",
          "time": "20210824 08:51"
        },
        {
          "id": "MDRW2100011227.1.7",
          "form": "출근하기싫게",
          "original_form": "출근하기싫게",
          "speaker_id": "2",
          "time": "20210824 08:51"
        },
        {
          "id": "MDRW2100011227.1.8",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210824 08:52"
        },
        {
          "id": "MDRW2100011227.1.9",
