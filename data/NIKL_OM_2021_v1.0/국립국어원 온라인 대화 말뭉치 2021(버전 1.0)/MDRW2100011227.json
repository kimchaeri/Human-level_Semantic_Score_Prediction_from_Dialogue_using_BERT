          "time": "20210824 08:54"
        },
        {
          "id": "MDRW2100011227.1.15",
          "form": "코로나로 뭐 할것도 없고",
          "original_form": "코로나로 뭐 할것도 없고",
          "speaker_id": "2",
          "time": "20210824 08:54"
        },
        {
          "id": "MDRW2100011227.1.16",
          "form": "어닠ㅋㅋㅋ그래도,,",
          "original_form": "어닠ㅋㅋㅋ그래도,,",
          "speaker_id": "1",
          "time": "20210824 08:54"
        },
        {
          "id": "MDRW2100011227.1.17",
          "form": "아 연차수당 나오나봐요",
          "original_form": "아 연차수당 나오나봐요",
          "speaker_id": "1",
          "time": "20210824 08:54"
        },
        {
          "id": "MDRW2100011227.1.18",
          "form": "옛날엔 여행가려고 붙여쓰고 했는데",
