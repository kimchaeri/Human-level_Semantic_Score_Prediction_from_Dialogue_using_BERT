        },
        {
          "id": "MDRW2100001086.1.2",
          "form": "반려동물 키우세요??",
          "original_form": "반려동물 키우세요??",
          "speaker_id": "2",
          "time": "20210723 15:55"
        },
        {
          "id": "MDRW2100001086.1.3",
          "form": "네 전 강아지 키워요",
          "original_form": "네 전 강아지 키워요",
          "speaker_id": "1",
          "time": "20210723 15:56"
        },
        {
          "id": "MDRW2100001086.1.4",
          "form": "!",
          "original_form": "!",
          "speaker_id": "1",
          "time": "20210723 15:56"
        },
        {
          "id": "MDRW2100001086.1.5",
          "form": "우와 종이 뭐예요?? 저 강아지 사랑해요",
          "original_form": "우와 종이 뭐예요?? 저 강아지 사랑해요",
          "speaker_id": "2",
          "time": "20210723 15:57"
        },
        {
          "id": "MDRW2100001086.1.6",
          "form": "ㅋㅋㅋ저는 말티즈요",
          "original_form": "ㅋㅋㅋ저는 말티즈요",
          "speaker_id": "1",
          "time": "20210723 15:57"
        },
        {
          "id": "MDRW2100001086.1.7",
          "form": "name2님은요?",
          "original_form": "&name2&님은요?",
          "speaker_id": "1",
          "time": "20210723 15:57"
        },
        {
          "id": "MDRW2100001086.1.8",
          "form": "아 전.. 금붕어 키워요!!",
          "original_form": "아 전.. 금붕어 키워요!!",
          "speaker_id": "2",
          "time": "20210723 15:57"
        },
        {
          "id": "MDRW2100001086.1.9",
          "form": "우와",
          "original_form": "우와",
          "speaker_id": "1",
          "time": "20210723 15:57"
        },
        {
          "id": "MDRW2100001086.1.10",
          "form": "몇마리요?",
          "original_form": "몇마리요?",
          "speaker_id": "1",
          "time": "20210723 15:57"
        },
        {
          "id": "MDRW2100001086.1.11",
          "form": "잠시만요",
          "original_form": "잠시만요",
          "speaker_id": "2",
          "time": "20210723 15:58"
        },
        {
          "id": "MDRW2100001086.1.12",
          "form": "세볼게요",
          "original_form": "세볼게요",
          "speaker_id": "2",
          "time": "20210723 15:58"
        },
        {
          "id": "MDRW2100001086.1.13",
          "form": "8마리요!!",
          "original_form": "8마리요!!",
          "speaker_id": "2",
          "time": "20210723 15:58"
        },
        {
          "id": "MDRW2100001086.1.14",
          "form": "애정이 없는 게 아니라..",
          "original_form": "애정이 없는 게 아니라..",
          "speaker_id": "2",
          "time": "20210723 15:58"
        },
        {
          "id": "MDRW2100001086.1.15",
          "form": "까먹는 거예요..",
          "original_form": "까먹는 거예요..",
          "speaker_id": "2",
          "time": "20210723 15:58"
