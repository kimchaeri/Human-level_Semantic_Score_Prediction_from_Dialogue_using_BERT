      "utterance": [
        {
          "id": "MDRW2100013814.1.1",
          "form": "동물 좋아하세요?",
          "original_form": "동물 좋아하세요?",
          "speaker_id": "1",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013814.1.2",
          "form": "좋아해요",
          "original_form": "좋아해요",
          "speaker_id": "2",
          "time": "20210825 16:03"
        },
        {
          "id": "MDRW2100013814.1.3",
          "form": "키우는 동물이 있으세요?",
          "original_form": "키우는 동물이 있으세요?",
          "speaker_id": "2",
          "time": "20210825 16:04"
        },
        {
          "id": "MDRW2100013814.1.4",
          "form": "지금 키우는 동물은 없지만",
          "original_form": "지금 키우는 동물은 없지만",
          "speaker_id": "1",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013814.1.5",
          "form": "나중에 고양이나 강아지 기르고싶어요",
          "original_form": "나중에 고양이나 강아지 기르고싶어요",
          "speaker_id": "1",
          "time": "20210825 16:05"
        },
        {
          "id": "MDRW2100013814.1.6",
          "form": "혹시 키우는 반려동물있으세요?",
          "original_form": "혹시 키우는 반려동물있으세요?",
          "speaker_id": "1",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013814.1.7",
          "form": "저는 강아지를 기르고있어요",
          "original_form": "저는 강아지를 기르고있어요",
          "speaker_id": "2",
          "time": "20210825 16:06"
        },
        {
          "id": "MDRW2100013814.1.8",
          "form": "오 너무 귀엽겠다",
          "original_form": "오 너무 귀엽겠다",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013814.1.9",
          "form": "무슨 종 아가 인가요?",
          "original_form": "무슨 종 아가 인가요?",
          "speaker_id": "1",
          "time": "20210825 16:07"
        },
        {
          "id": "MDRW2100013814.1.10",
          "form": "작은 믹스견이예요!",
          "original_form": "작은 믹스견이예요!",
          "speaker_id": "2",
          "time": "20210825 16:09"
        },
        {
          "id": "MDRW2100013814.1.11",
          "form": "믹스견이 건강하고 이뻐서 좋죠",
          "original_form": "믹스견이 건강하고 이뻐서 좋죠",
          "speaker_id": "1",
          "time": "20210825 16:10"
        },
        {
          "id": "MDRW2100013814.1.12",
          "form": "예전에 말티푸 기르고 싶어했었어요",
          "original_form": "예전에 말티푸 기르고 싶어했었어요",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013814.1.13",
          "form": "애기 몇살이에요?",
          "original_form": "애기 몇살이에요?",
          "speaker_id": "1",
          "time": "20210825 16:11"
        },
        {
          "id": "MDRW2100013814.1.14",
          "form": "이제 다섯살 됐어요! 아직 아기같은데 시간이 참 빠르네요 ㅎㅎ",
          "original_form": "이제 다섯살 됐어요! 아직 아기같은데 시간이 참 빠르네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:12"
        },
        {
          "id": "MDRW2100013814.1.15",
          "form": "오늘 미용예약을 해서 조금 있다가 맡기고 오려구요",
          "original_form": "오늘 미용예약을 해서 조금 있다가 맡기고 오려구요",
          "speaker_id": "2",
          "time": "20210825 16:13"
        },
        {
          "id": "MDRW2100013814.1.16",
          "form": "5살이면 완전 애기죠",
          "original_form": "5살이면 완전 애기죠",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100013814.1.17",
          "form": "제 친구 강아지는  4살인데 아직도 애기애기하더라구요",
          "original_form": "제 친구 강아지는  4살인데 아직도 애기애기하더라구요",
          "speaker_id": "1",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013814.1.18",
          "form": "강아지는 몇 살이어도 아기인 것 같아요",
          "original_form": "강아지는 몇 살이어도 아기인 것 같아요",
          "speaker_id": "2",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100013814.1.19",
          "form": "정말 공감합니다",
          "original_form": "정말 공감합니다",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013814.1.20",
          "form": "어른들은 강아지가 장난감 꺼내서 어질러놓는거 보고 사람 아기랑 똑같다고 하시더라구요ㅎㅎ",
          "original_form": "어른들은 강아지가 장난감 꺼내서 어질러놓는거 보고 사람 아기랑 똑같다고 하시더라구요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100013814.1.21",
          "form": "가끔보면 그냥 사람 애기같아요",
          "original_form": "가끔보면 그냥 사람 애기같아요",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013814.1.22",
          "form": "눈치도 빨라서  사람말로 말걸것같기도 한 느낌이 들기도해요",
          "original_form": "눈치도 빨라서  사람말로 말걸것같기도 한 느낌이 들기도해요",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100013814.1.23",
          "form": "말은 못하지만 산책가자 침대가자는 귀신같이 알아듣더라구요 ㅎㅎ",
          "original_form": "말은 못하지만 산책가자 침대가자는 귀신같이 알아듣더라구요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 16:18"
        },
        {
          "id": "MDRW2100013814.1.24",
          "form": "ㅋㅋㅋㅋ너무귀여울것같아요",
          "original_form": "ㅋㅋㅋㅋ너무귀여울것같아요",
          "speaker_id": "1",
          "time": "20210825 16:19"
        }
      ]
    }
  ]
