      "utterance": [
        {
          "id": "MDRW2100044870.1.1",
          "form": "앗 아니오 안봤어요ㅠㅠ",
          "original_form": "앗 아니오 안봤어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044870.1.2",
          "form": "과일 좋아하시나요",
          "original_form": "과일 좋아하시나요",
          "speaker_id": "1",
          "time": "20210908 00:01"
        },
        {
          "id": "MDRW2100044870.1.3",
          "form": "과일 환장합니다",
          "original_form": "과일 환장합니다",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044870.1.4",
          "form": "좋아하시나요",
          "original_form": "좋아하시나요",
          "speaker_id": "2",
          "time": "20210908 00:03"
        },
        {
          "id": "MDRW2100044870.1.5",
          "form": "ㅋㅋㅋ네 저도 환장해요 ㅋㅋㅋ",
          "original_form": "ㅋㅋㅋ네 저도 환장해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044870.1.6",
          "form": "수박 제일좋아해요",
          "original_form": "수박 제일좋아해요",
          "speaker_id": "1",
          "time": "20210908 00:04"
        },
        {
          "id": "MDRW2100044870.1.7",
          "form": "아 수박 맛있더라구요",
          "original_form": "아 수박 맛있더라구요",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044870.1.8",
          "form": "잘라드시나요",
          "original_form": "잘라드시나요",
          "speaker_id": "2",
          "time": "20210908 00:06"
        },
        {
          "id": "MDRW2100044870.1.9",
          "form": "아 당연히 잘라드시겠네요ㅎㅎㅎ",
          "original_form": "아 당연히 잘라드시겠네요ㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044870.1.10",
          "form": "수박빙수 드셔보셨나요",
          "original_form": "수박빙수 드셔보셨나요",
          "speaker_id": "2",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044870.1.11",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ네 ㅋㅋㅋㅋㅋㅋ미리 통에 잘라 담아놨다가 먹어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ네 ㅋㅋㅋㅋㅋㅋ미리 통에 잘라 담아놨다가 먹어요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:07"
        },
        {
          "id": "MDRW2100044870.1.12",
          "form": "수박빙수는 안먹어봣어요!!!어디서파나요",
          "original_form": "수박빙수는 안먹어봣어요!!!어디서파나요",
          "speaker_id": "1",
          "time": "20210908 00:08"
        },
        {
          "id": "MDRW2100044870.1.13",
          "form": "개인카페에서 파는거 첨 먹어봤는데 괜찮더라구요",
          "original_form": "개인카페에서 파는거 첨 먹어봤는데 괜찮더라구요",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044870.1.14",
          "form": "낯설어서 걱정했는데ㅎㅎ",
          "original_form": "낯설어서 걱정했는데ㅎㅎ",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044870.1.15",
          "form": "과일빙수 좋아하시나요?",
          "original_form": "과일빙수 좋아하시나요?",
          "speaker_id": "2",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044870.1.16",
          "form": "오 신기해요 ㅋㅋㅋ",
          "original_form": "오 신기해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044870.1.17",
          "form": "음 빙수는 설빙좋아해요 ㅋㅋㅋ",
          "original_form": "음 빙수는 설빙좋아해요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044870.1.18",
          "form": "물베이스보다 우유베이스가 맛있더라구요 ㅋㅋㅋ",
          "original_form": "물베이스보다 우유베이스가 맛있더라구요 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:11"
        },
        {
          "id": "MDRW2100044870.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "2",
          "time": "20210908 00:12"
        },
        {
          "id": "MDRW2100044870.1.20",
          "form": "빙수 얼음이 물이면 맛없더라구요",
          "original_form": "빙수 얼음이 물이면 맛없더라구요",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044870.1.21",
          "form": "우유얼음이 최고예요",
          "original_form": "우유얼음이 최고예요",
          "speaker_id": "2",
          "time": "20210908 00:13"
        },
        {
          "id": "MDRW2100044870.1.22",
          "form": "ㅋㅋ여름이 끝나서 수박이 끝나가서 슬퍼요ㅠ",
          "original_form": "ㅋㅋ여름이 끝나서 수박이 끝나가서 슬퍼요ㅠ",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044870.1.23",
          "form": "그래도이제 배의 계절입니다 ㅋㅋㅋㅋ",
          "original_form": "그래도이제 배의 계절입니다 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 00:14"
        },
        {
          "id": "MDRW2100044870.1.24",
          "form": "오 이제 배 먹을때네요",
          "original_form": "오 이제 배 먹을때네요",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044870.1.25",
          "form": "복숭아는 드셨나요?",
          "original_form": "복숭아는 드셨나요?",
          "speaker_id": "2",
          "time": "20210908 00:15"
        },
        {
          "id": "MDRW2100044870.1.26",
          "form": "아니여ㅠ 올해는 이상하게 복숭아를 거의못먹었어요",
          "original_form": "아니여ㅠ 올해는 이상하게 복숭아를 거의못먹었어요",
          "speaker_id": "1",
          "time": "20210908 00:19"
        },
        {
          "id": "MDRW2100044870.1.27",
          "form": "아쉽네요ㅠㅠ",
          "original_form": "아쉽네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044870.1.28",
          "form": "복숭아 맛있더라구요",
          "original_form": "복숭아 맛있더라구요",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044870.1.29",
          "form": "딱복, 물복 뭐가 좋으세요?",
          "original_form": "딱복, 물복 뭐가 좋으세요?",
          "speaker_id": "2",
          "time": "20210908 00:21"
        },
        {
          "id": "MDRW2100044870.1.30",
          "form": "오 그랬나요?제가 너무 수박 외길인생을 걸었네여ㅠㅠ",
          "original_form": "오 그랬나요?제가 너무 수박 외길인생을 걸었네여ㅠㅠ",
          "speaker_id": "1",
          "time": "20210908 00:22"
        },
        {
          "id": "MDRW2100044870.1.31",
          "form": "근데 수박도 맛있어서ㅠㅠ",
          "original_form": "근데 수박도 맛있어서ㅠㅠ",
          "speaker_id": "2",
          "time": "20210908 00:23"
        },
        {
          "id": "MDRW2100044870.1.32",
          "form": "맞아요ㅠㅠ 너무좋아요ㅠㅠ",
