        {
          "id": "MDRW2100018539.1.1",
          "form": "여행 좋아하세요?",
          "original_form": "여행 좋아하세요?",
          "speaker_id": "2",
          "time": "20210827 08:00"
        },
        {
          "id": "MDRW2100018539.1.2",
          "form": "드라이브 좋아해요",
          "original_form": "드라이브 좋아해요",
          "speaker_id": "1",
          "time": "20210827 08:02"
        },
        {
          "id": "MDRW2100018539.1.3",
          "form": "코로나로 그리된거 같아요",
          "original_form": "코로나로 그리된거 같아요",
          "speaker_id": "1",
          "time": "20210827 08:02"
        },
        {
          "id": "MDRW2100018539.1.4",
          "form": "여행좋아요",
          "original_form": "여행좋아요",
          "speaker_id": "1",
          "time": "20210827 08:02"
        },
        {
          "id": "MDRW2100018539.1.5",
          "form": "젛아 하세요?",
          "original_form": "젛아 하세요?",
          "speaker_id": "1",
          "time": "20210827 08:02"
        },
        {
          "id": "MDRW2100018539.1.6",
          "form": "좋아",
          "original_form": "좋아",
          "speaker_id": "1",
          "time": "20210827 08:02"
        },
        {
          "id": "MDRW2100018539.1.7",
          "form": "그리 좋아하지 않았는데 못가니 가고싶네요ㅋㅋ",
          "original_form": "그리 좋아하지 않았는데 못가니 가고싶네요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 08:04"
        },
        {
          "id": "MDRW2100018539.1.8",
          "form": "원래 못하게 하면 막 하고싶어요",
          "original_form": "원래 못하게 하면 막 하고싶어요",
          "speaker_id": "1",
          "time": "20210827 08:05"
        },
        {
          "id": "MDRW2100018539.1.9",
          "form": "맞아요ㅋㅋ",
          "original_form": "맞아요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 08:06"
        },
        {
          "id": "MDRW2100018539.1.10",
          "form": "전 요새 산밑에 계곡 쓱 구경하고",
          "original_form": "전 요새 산밑에 계곡 쓱 구경하고",
          "speaker_id": "1",
          "time": "20210827 08:06"
        },
        {
          "id": "MDRW2100018539.1.11",
          "form": "언제든 갈 수 있었는데 못간다하니 더 가고싶어요",
          "original_form": "언제든 갈 수 있었는데 못간다하니 더 가고싶어요",
          "speaker_id": "2",
          "time": "20210827 08:06"
        },
        {
          "id": "MDRW2100018539.1.12",
          "form": "산채정식 먹고와요",
          "original_form": "산채정식 먹고와요",
          "speaker_id": "1",
          "time": "20210827 08:06"
        },
        {
          "id": "MDRW2100018539.1.13",
          "form": "오~~!!!",
          "original_form": "오~~!!!",
          "speaker_id": "2",
          "time": "20210827 08:06"
        },
        {
          "id": "MDRW2100018539.1.14",
          "form": "정식이라니 산타고 드시는건가요?",
          "original_form": "정식이라니 산타고 드시는건가요?",
          "speaker_id": "2",
          "time": "20210827 08:06"
        },
        {
          "id": "MDRW2100018539.1.15",
          "form": "그럼요 언제든 갈땐 소중한걸 몰라요 ㅜㅜ당연시해요",
          "original_form": "그럼요 언제든 갈땐 소중한걸 몰라요 ㅜㅜ당연시해요",
          "speaker_id": "1",
          "time": "20210827 08:06"
        },
        {
          "id": "MDRW2100018539.1.16",
          "form": "아뇨 ㅋㅋ",
          "original_form": "아뇨 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 08:06"
        },
        {
          "id": "MDRW2100018539.1.17",
          "form": "기냥 쫌 절둘러보고",
          "original_form": "기냥 쫌 절둘러보고",
          "speaker_id": "1",
          "time": "20210827 08:07"
        },
        {
          "id": "MDRW2100018539.1.18",
          "form": "밥먹고외요",
          "original_form": "밥먹고외요",
          "speaker_id": "1",
          "time": "20210827 08:07"
        },
        {
          "id": "MDRW2100018539.1.19",
          "form": "정식먹음 건강해 지는것 같아요 ㅎㅎ",
          "original_form": "정식먹음 건강해 지는것 같아요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 08:07"
        },
        {
          "id": "MDRW2100018539.1.20",
          "form": "맞아요ㅠ 없어야 소중함을 아는게 참..ㅠ",
          "original_form": "맞아요ㅠ 없어야 소중함을 아는게 참..ㅠ",
          "speaker_id": "2",
          "time": "20210827 08:08"
        },
        {
          "id": "MDRW2100018539.1.21",
          "form": "오오ㅎㅎ 맞아요 한끼 든든히 먹고 보답받는 느낌이에요",
          "original_form": "오오ㅎㅎ 맞아요 한끼 든든히 먹고 보답받는 느낌이에요",
          "speaker_id": "2",
          "time": "20210827 08:08"
        },
        {
          "id": "MDRW2100018539.1.22",
          "form": "네 나물들 건강해지고 속도좋고요 ㅎㅎ",
          "original_form": "네 나물들 건강해지고 속도좋고요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 08:08"
        },
        {
          "id": "MDRW2100018539.1.23",
          "form": "미식 여행이죠 ㅎㅎ",
          "original_form": "미식 여행이죠 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 08:08"
        },
        {
          "id": "MDRW2100018539.1.24",
          "form": "맞아요ㅎㅎ 여행중 최고는 식이죠!",
          "original_form": "맞아요ㅎㅎ 여행중 최고는 식이죠!",
          "speaker_id": "2",
          "time": "20210827 08:29"
        },
        {
          "id": "MDRW2100018539.1.25",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 08:29"
        },
        {
          "id": "MDRW2100018539.1.26",
          "form": "요새 다녀온곳 있으세요?",
          "original_form": "요새 다녀온곳 있으세요?",
          "speaker_id": "1",
          "time": "20210827 08:29"
        },
        {
          "id": "MDRW2100018539.1.27",
          "form": "최근에 동대문밖에 없네요ㅠㅠ",
          "original_form": "최근에 동대문밖에 없네요ㅠㅠ",
          "speaker_id": "2",
          "time": "20210827 08:29"
        },
        {
          "id": "MDRW2100018539.1.28",
          "form": "전 안가본지 백만년 됐네요 ㅜㅜ",
          "original_form": "전 안가본지 백만년 됐네요 ㅜㅜ",
          "speaker_id": "1",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100018539.1.29",
          "form": "좋아했는데 동대문",
          "original_form": "좋아했는데 동대문",
          "speaker_id": "1",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100018539.1.30",
          "form": "옷많이 사셨어요?",
          "original_form": "옷많이 사셨어요?",
          "speaker_id": "1",
          "time": "20210827 08:30"
        },
        {
          "id": "MDRW2100018539.1.31",
          "form": "동대문도 구경하기  좋은걸요 ㅎㅎ",
          "original_form": "동대문도 구경하기  좋은걸요 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 08:31"
        },
        {
          "id": "MDRW2100018539.1.32",
          "form": "사람이 너무 많아서 치이다 온 것 같아요ㅠㅠㅎㅎ",
          "original_form": "사람이 너무 많아서 치이다 온 것 같아요ㅠㅠㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 08:39"
        },
        {
          "id": "MDRW2100018539.1.33",
          "form": "맞아요 역시 먹을곳이 많고 좋았어요",
          "original_form": "맞아요 역시 먹을곳이 많고 좋았어요",
          "speaker_id": "2",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018539.1.34",
          "form": "아직도 많군요",
          "original_form": "아직도 많군요",
          "speaker_id": "1",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018539.1.35",
          "form": "사람구경이 반인곳이죠",
          "original_form": "사람구경이 반인곳이죠",
          "speaker_id": "1",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018539.1.36",
          "form": "먹거리도 많고 ㅎㅎ",
          "original_form": "먹거리도 많고 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210827 08:40"
        },
        {
          "id": "MDRW2100018539.1.37",
          "form": "맞아요ㅎㅎ",
          "original_form": "맞아요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210827 08:41"
        },
        {
          "id": "MDRW2100018539.1.38",
          "form": "그게 또 나름 재미긴 해요",
          "original_form": "그게 또 나름 재미긴 해요",
          "speaker_id": "2",
