        {
          "id": "MDRW2100013104.1.1",
          "form": "아뇨 학생이세요?",
          "original_form": "아뇨 학생이세요?",
          "speaker_id": "2",
          "time": "20210825 12:32"
        },
        {
          "id": "MDRW2100013104.1.2",
          "form": "아뇨 직장인입니당",
          "original_form": "아뇨 직장인입니당",
          "speaker_id": "1",
          "time": "20210825 12:33"
        },
        {
          "id": "MDRW2100013104.1.3",
          "form": "수강신청하던 생각 나네요 ㅎㅎ",
          "original_form": "수강신청하던 생각 나네요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 12:33"
        },
        {
          "id": "MDRW2100013104.1.4",
          "form": "클릭질 ㅋㅋ",
          "original_form": "클릭질 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:33"
        },
        {
          "id": "MDRW2100013104.1.5",
          "form": "그쵸 공강만들려고 ㅋㅋ",
          "original_form": "그쵸 공강만들려고 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 12:34"
        },
        {
          "id": "MDRW2100013104.1.6",
          "form": "그것도그렇고",
          "original_form": "그것도그렇고",
          "speaker_id": "2",
          "time": "20210825 12:34"
        },
        {
          "id": "MDRW2100013104.1.7",
          "form": "인기있는거 빨리넣으려구요 ㅋㅋ",
          "original_form": "인기있는거 빨리넣으려구요 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210825 12:34"
        },
        {
          "id": "MDRW2100013104.1.8",
          "form": "학교생활 재밌으셨나요",
          "original_form": "학교생활 재밌으셨나요",
          "speaker_id": "1",
          "time": "20210825 12:35"
        },
        {
          "id": "MDRW2100013104.1.9",
          "form": "아뇨 별로요 ㅎㅎ",
          "original_form": "아뇨 별로요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100013104.1.10",
          "form": "무슨 과 나오셨어요?",
          "original_form": "무슨 과 나오셨어요?",
          "speaker_id": "1",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100013104.1.11",
          "form": "법학과요",
          "original_form": "법학과요",
          "speaker_id": "2",
          "time": "20210825 12:37"
        },
        {
          "id": "MDRW2100013104.1.12",
          "form": "오 주변에 법학과친구들 있었는뎅 ㅎㅎ",
          "original_form": "오 주변에 법학과친구들 있었는뎅 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210825 12:40"
        },
        {
          "id": "MDRW2100013104.1.13",
          "form": "무슨과나오셨어요?",
          "original_form": "무슨과나오셨어요?",
          "speaker_id": "2",
          "time": "20210825 12:41"
        },
        {
          "id": "MDRW2100013104.1.14",
          "form": "저는 치위생과요",
          "original_form": "저는 치위생과요",
          "speaker_id": "1",
          "time": "20210825 13:12"
        },
        {
          "id": "MDRW2100013104.1.15",
          "form": "공부는 안어렵나요",
          "original_form": "공부는 안어렵나요",
