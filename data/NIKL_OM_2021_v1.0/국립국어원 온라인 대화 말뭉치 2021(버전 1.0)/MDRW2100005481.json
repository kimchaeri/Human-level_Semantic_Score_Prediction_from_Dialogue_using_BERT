      "utterance": [
        {
          "id": "MDRW2100005481.1.1",
          "form": "형제가 있나요?",
          "original_form": "형제가 있나요?",
          "speaker_id": "2",
          "time": "20210821 22:38"
        },
        {
          "id": "MDRW2100005481.1.2",
          "form": "저는 남동생 한명있어요ㅋㅋ 장녀입니다",
          "original_form": "저는 남동생 한명있어요ㅋㅋ 장녀입니다",
          "speaker_id": "1",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005481.1.3",
          "form": "어 저두 남동생 한명있어요ㅎㅎ",
          "original_form": "어 저두 남동생 한명있어요ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005481.1.4",
          "form": "남동생 어때요? 저는 어릴 때 진짜 박터지게 싸웠거든요",
          "original_form": "남동생 어때요? 저는 어릴 때 진짜 박터지게 싸웠거든요",
          "speaker_id": "2",
          "time": "20210821 22:42"
        },
        {
          "id": "MDRW2100005481.1.5",
          "form": "오 저는 생각보다 덜 싸웠어요ㅋㅋㅋ 둘다 얌전하고 조용한 성격이라 싸울땐 말싸움을 주로 했어요 ㅋㅋ",
          "original_form": "오 저는 생각보다 덜 싸웠어요ㅋㅋㅋ 둘다 얌전하고 조용한 성격이라 싸울땐 말싸움을 주로 했어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005481.1.6",
          "form": "헉 되게 점잖으시구나",
          "original_form": "헉 되게 점잖으시구나",
          "speaker_id": "2",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005481.1.7",
          "form": "저희는 진짜 둘 중 하나 사라질 때 까지 싸웠던 거 같아요ㅋㅋㅋ",
          "original_form": "저희는 진짜 둘 중 하나 사라질 때 까지 싸웠던 거 같아요ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 22:44"
        },
        {
          "id": "MDRW2100005481.1.8",
          "form": "집안인을 분담으로 하나요?",
          "original_form": "집안인을 분담으로 하나요?",
          "speaker_id": "2",
          "time": "20210821 22:45"
        },
        {
          "id": "MDRW2100005481.1.9",
          "form": "둘 중 하나 사라질때까짘ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "둘 중 하나 사라질때까짘ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:50"
        },
        {
          "id": "MDRW2100005481.1.10",
          "form": "집안일 남동생은 안해서 제가 개판쳤더니 다 같이 합니다 ㅎ\r\nㅋㅋㅋㅋㅋㅋ",
          "original_form": "집안일 남동생은 안해서 제가 개판쳤더니 다 같이 합니다 ㅎ\r\nㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005481.1.11",
          "form": "약간 그런게 있는거같아요",
          "original_form": "약간 그런게 있는거같아요",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005481.1.12",
          "form": "남동생들은 왜 그런지 모르겠는데 집안일은 자기가 해야한다고 생각을 안하더라구요",
          "original_form": "남동생들은 왜 그런지 모르겠는데 집안일은 자기가 해야한다고 생각을 안하더라구요",
          "speaker_id": "2",
          "time": "20210821 22:51"
        },
        {
          "id": "MDRW2100005481.1.13",
          "form": "저도 안하길래 줘패고 난리치니까 하는데 탐탁치는 않아하더라구요. 근데 어쩔거야 해야지 그쵸",
          "original_form": "저도 안하길래 줘패고 난리치니까 하는데 탐탁치는 않아하더라구요. 근데 어쩔거야 해야지 그쵸",
          "speaker_id": "2",
          "time": "20210821 22:52"
        },
        {
          "id": "MDRW2100005481.1.14",
          "form": "아휴 완전 공감되네요\r\n저희집 아부지가 집안일 잘 안하셔서 그런지 남동생도 똑같아요 으휴",
          "original_form": "아휴 완전 공감되네요\r\n저희집 아부지가 집안일 잘 안하셔서 그런지 남동생도 똑같아요 으휴",
          "speaker_id": "1",
          "time": "20210821 22:55"
        },
        {
          "id": "MDRW2100005481.1.15",
          "form": "저는 그래서 그 꼴 못보겠더라구요",
          "original_form": "저는 그래서 그 꼴 못보겠더라구요",
          "speaker_id": "2",
          "time": "20210821 22:56"
        },
        {
          "id": "MDRW2100005481.1.16",
          "form": "집에 살면 같이 해야지 어디서 지만 쏙 빠지려고",
          "original_form": "집에 살면 같이 해야지 어디서 지만 쏙 빠지려고",
          "speaker_id": "2",
          "time": "20210821 22:57"
        },
        {
          "id": "MDRW2100005481.1.17",
          "form": "진짜 얼마나 난리쳤는지 몰라요",
          "original_form": "진짜 얼마나 난리쳤는지 몰라요",
          "speaker_id": "1",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005481.1.18",
          "form": "답답한 것,,",
          "original_form": "답답한 것,,",
          "speaker_id": "1",
          "time": "20210821 23:02"
        },
        {
          "id": "MDRW2100005481.1.19",
          "form": "그 마음 오백만번 이해합니다",
          "original_form": "그 마음 오백만번 이해합니다",
          "speaker_id": "2",
          "time": "20210821 23:03"
        },
        {
          "id": "MDRW2100005481.1.20",
          "form": "도대체 남자형제들은 왜 그러는걸까요",
          "original_form": "도대체 남자형제들은 왜 그러는걸까요",
          "speaker_id": "2",
          "time": "20210821 23:03"
        },
        {
          "id": "MDRW2100005481.1.21",
          "form": "정말 이상한거같아요",
          "original_form": "정말 이상한거같아요",
          "speaker_id": "2",
          "time": "20210821 23:04"
        },
        {
          "id": "MDRW2100005481.1.22",
          "form": "가족들도 오냐오냐\r\n사회도 오냐오냐 해주니 어쩔 도리가 없네요",
          "original_form": "가족들도 오냐오냐\r\n사회도 오냐오냐 해주니 어쩔 도리가 없네요",
          "speaker_id": "1",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005481.1.23",
          "form": "그러게요",
          "original_form": "그러게요",
          "speaker_id": "2",
          "time": "20210821 23:49"
        },
        {
          "id": "MDRW2100005481.1.24",
          "form": "하지만 전 절대 못 참아서 버럭버럭 짜증내고 바꿨슴다 ㅎ",
          "original_form": "하지만 전 절대 못 참아서 버럭버럭 짜증내고 바꿨슴다 ㅎ",
          "speaker_id": "1",
          "time": "20210821 23:49"
        }
      ]
