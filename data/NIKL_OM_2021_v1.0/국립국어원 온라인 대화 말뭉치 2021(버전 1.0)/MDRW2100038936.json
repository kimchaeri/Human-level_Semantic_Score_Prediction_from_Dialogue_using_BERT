      "utterance": [
        {
          "id": "MDRW2100038936.1.1",
          "form": "지금 3단계일걸요",
          "original_form": "지금 3단계일걸요",
          "speaker_id": "2",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100038936.1.2",
          "form": "이제 4명 가능해요?",
          "original_form": "이제 4명 가능해요?",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100038936.1.3",
          "form": "오 대전은 내려갔어요? 좋겠다",
          "original_form": "오 대전은 내려갔어요? 좋겠다",
          "speaker_id": "1",
          "time": "20210902 22:59"
        },
        {
          "id": "MDRW2100038936.1.4",
          "form": "이번주부터 그런듯요",
          "original_form": "이번주부터 그런듯요",
          "speaker_id": "2",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100038936.1.5",
          "form": "저에겐 별 의미 없다능..",
          "original_form": "저에겐 별 의미 없다능..",
          "speaker_id": "2",
          "time": "20210902 23:01"
        },
        {
          "id": "MDRW2100038936.1.6",
          "form": "ㅋㅋㅋ요즘에는 연구실사람들이랑 저녁 안먹어요?",
          "original_form": "ㅋㅋㅋ요즘에는 연구실사람들이랑 저녁 안먹어요?",
          "speaker_id": "1",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100038936.1.7",
          "form": "네...다혼자먹어요",
          "original_form": "네...다혼자먹어요",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100038936.1.8",
          "form": "몇달째 ㅋㅋ",
          "original_form": "몇달째 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:06"
        },
        {
          "id": "MDRW2100038936.1.9",
          "form": "진짜 누구랑 밥먹는거 서울갈때빼고 없네용",
          "original_form": "진짜 누구랑 밥먹는거 서울갈때빼고 없네용",
          "speaker_id": "2",
          "time": "20210902 23:07"
        },
        {
          "id": "MDRW2100038936.1.10",
          "form": "저두요 ㅋㅋ 퇴근하고 맨날 혼자먹어요 ㅋㅋㅋㅋ",
          "original_form": "저두요 ㅋㅋ 퇴근하고 맨날 혼자먹어요 ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100038936.1.11",
          "form": "저도 주말에 친구만나거나 가족들이랑먹을때나 같이먹어요",
          "original_form": "저도 주말에 친구만나거나 가족들이랑먹을때나 같이먹어요",
          "speaker_id": "1",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100038936.1.12",
          "form": "ㅋㅋㅋ 혼밥 조아여 나만의시간 ~~~~",
          "original_form": "ㅋㅋㅋ 혼밥 조아여 나만의시간 ~~~~",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100038936.1.13",
          "form": "4단계도 의미없는 저의 식사생활",
          "original_form": "4단계도 의미없는 저의 식사생활",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100038936.1.14",
          "form": "ㅋㅋ맞아요 혼자먹으면 먹고싶은거 조용히 잘즐길수잇다구용",
          "original_form": "ㅋㅋ맞아요 혼자먹으면 먹고싶은거 조용히 잘즐길수잇다구용",
          "speaker_id": "1",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100038936.1.15",
          "form": "ㅋㅋㅋㅋ ㄹㅇ 의미없움",
          "original_form": "ㅋㅋㅋㅋ ㄹㅇ 의미없움",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100038936.1.16",
          "form": "걍 돈도아낄겸 집에서먹아유",
          "original_form": "걍 돈도아낄겸 집에서먹아유",
          "speaker_id": "2",
          "time": "20210902 23:16"
        },
        {
          "id": "MDRW2100038936.1.17",
          "form": "맞아요 그리고 집에서 먹는게.더 맛있다구용...",
          "original_form": "맞아요 그리고 집에서 먹는게.더 맛있다구용...",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100038936.1.18",
          "form": "아맞다 오늘 밀키트 배송왓어요 내일 해먹어보려구요!!",
          "original_form": "아맞다 오늘 밀키트 배송왓어요 내일 해먹어보려구요!!",
          "speaker_id": "1",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100038936.1.19",
          "form": "저두왓어요! ㅋㅋㅋ",
          "original_form": "저두왓어요! ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100038936.1.20",
          "form": "스테이크 무거야징 ~_~",
          "original_form": "스테이크 무거야징 ~_~",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100038936.1.21",
          "form": "저도 오늘 점심 스테이크 먹을거에요 ㅋㅋ",
          "original_form": "저도 오늘 점심 스테이크 먹을거에요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 08:58"
        },
        {
          "id": "MDRW2100038936.1.22",
          "form": "꺄하 ~ 누가굽나요",
          "original_form": "꺄하 ~ 누가굽나요",
          "speaker_id": "2",
          "time": "20210903 09:16"
        },
        {
          "id": "MDRW2100038936.1.23",
          "form": "ㅋㅋ제가 구워야되는데 점심시간이 한시간밖에 없어서 걱정이네용",
          "original_form": "ㅋㅋ제가 구워야되는데 점심시간이 한시간밖에 없어서 걱정이네용",
          "speaker_id": "1",
          "time": "20210903 09:17"
        },
        {
          "id": "MDRW2100038936.1.24",
          "form": "ㅋㅋㅋ농땡이치면 안대나유?",
          "original_form": "ㅋㅋㅋ농땡이치면 안대나유?",
          "speaker_id": "2",
          "time": "20210903 09:19"
        },
        {
          "id": "MDRW2100038936.1.25",
          "form": "오늘교육은 중요해서 집중해야해요 ㅋㅋ",
          "original_form": "오늘교육은 중요해서 집중해야해요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 09:19"
        },
        {
          "id": "MDRW2100038936.1.26",
          "form": "앗 무슨교육이요",
          "original_form": "앗 무슨교육이요",
          "speaker_id": "2",
          "time": "20210903 09:20"
        },
        {
