      "utterance": [
        {
          "id": "MDRW2100008675.1.1",
          "form": "어떤게 가장 어려웠어요?",
          "original_form": "어떤게 가장 어려웠어요?",
          "speaker_id": "1",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008675.1.2",
          "form": "통신사할인 그런거요?",
          "original_form": "통신사할인 그런거요?",
          "speaker_id": "1",
          "time": "20210822 17:22"
        },
        {
          "id": "MDRW2100008675.1.3",
          "form": "통신사할인도 진짜 많고 카드마다 할인혜택이 다 달라서",
          "original_form": "통신사할인도 진짜 많고 카드마다 할인혜택이 다 달라서",
          "speaker_id": "2",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008675.1.4",
          "form": "그리고 각 혜택별로 적용 키?가 달라서 힘들었어요",
          "original_form": "그리고 각 혜택별로 적용 키?가 달라서 힘들었어요",
          "speaker_id": "2",
          "time": "20210822 18:08"
        },
        {
          "id": "MDRW2100008675.1.5",
          "form": "헷갈리는 일이 많았겠어요",
          "original_form": "헷갈리는 일이 많았겠어요",
          "speaker_id": "1",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008675.1.6",
          "form": "처음엔 안 알려줬으니 매번 매니저님 불렀죠 ㅋㅋ",
          "original_form": "처음엔 안 알려줬으니 매번 매니저님 불렀죠 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008675.1.7",
          "form": "그냥 계산하는 것도 가끔 헷갈리는데",
          "original_form": "그냥 계산하는 것도 가끔 헷갈리는데",
          "speaker_id": "1",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008675.1.8",
          "form": "헐 처음에 알려줘야지 안알려주다니",
          "original_form": "헐 처음에 알려줘야지 안알려주다니",
          "speaker_id": "1",
          "time": "20210822 18:18"
        },
        {
          "id": "MDRW2100008675.1.9",
          "form": "불친절한 지점이었네요",
          "original_form": "불친절한 지점이었네요",
          "speaker_id": "1",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008675.1.10",
          "form": "본인들이 더 고생이죠 ㅎ 제대로 안 알려줬으니까요",
          "original_form": "본인들이 더 고생이죠 ㅎ 제대로 안 알려줬으니까요",
          "speaker_id": "2",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008675.1.11",
          "form": "알바할 때 이런 손님까지 있었다 하는 손님 있었나요",
          "original_form": "알바할 때 이런 손님까지 있었다 하는 손님 있었나요",
          "speaker_id": "1",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008675.1.12",
          "form": "반말하고 카드 던지는 건 기본이죠??",
          "original_form": "반말하고 카드 던지는 건 기본이죠??",
          "speaker_id": "2",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008675.1.13",
          "form": "세상에",
          "original_form": "세상에",
          "speaker_id": "1",
          "time": "20210822 18:19"
        },
        {
          "id": "MDRW2100008675.1.14",
          "form": "카드를 던지다니",
          "original_form": "카드를 던지다니",
          "speaker_id": "1",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008675.1.15",
          "form": "나쁜인간",
          "original_form": "나쁜인간",
          "speaker_id": "1",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008675.1.16",
          "form": "저도 반말은 해봤어요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저도 반말은 해봤어요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008675.1.17",
          "form": "눈눈이이를 해봤어요",
          "original_form": "눈눈이이를 해봤어요",
          "speaker_id": "2",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008675.1.18",
          "form": "어떤식으로요?",
          "original_form": "어떤식으로요?",
          "speaker_id": "1",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008675.1.19",
          "form": "영화 시간표 물어보길래 은근슬쩍 티 안 나게 반말했아요",
          "original_form": "영화 시간표 물어보길래 은근슬쩍 티 안 나게 반말했아요",
          "speaker_id": "2",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008675.1.20",
          "form": "3시 ㅇㅇ영화\r\n3시30분 ㅁㅁ영화",
          "original_form": "3시 ㅇㅇ영화\r\n3시30분 ㅁㅁ영화",
          "speaker_id": "2",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008675.1.21",
          "form": "이렇게요 ㅋㅋㅋㅋㅋ",
          "original_form": "이렇게요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 18:20"
        },
        {
          "id": "MDRW2100008675.1.22",
