      "utterance": [
        {
          "id": "MDRW2100043170.1.1",
          "form": "그럼 비혼이신가요?",
          "original_form": "그럼 비혼이신가요?",
          "speaker_id": "2",
          "time": "20210907 15:14"
        },
        {
          "id": "MDRW2100043170.1.2",
          "form": "네 ㅋㅋㅋㅋ 사회에 의한 비혼",
          "original_form": "네 ㅋㅋㅋㅋ 사회에 의한 비혼",
          "speaker_id": "1",
          "time": "20210907 15:15"
        },
        {
          "id": "MDRW2100043170.1.3",
          "form": "저 또한",
          "original_form": "저 또한",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043170.1.4",
          "form": "동지들이 많아지고 있어서 좋아요!!",
          "original_form": "동지들이 많아지고 있어서 좋아요!!",
          "speaker_id": "2",
          "time": "20210907 15:16"
        },
        {
          "id": "MDRW2100043170.1.5",
          "form": "ㅎㅎㅎㅎㅎ 반갑습니다",
          "original_form": "ㅎㅎㅎㅎㅎ 반갑습니다",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043170.1.6",
          "form": "혼자 잘살려구요",
          "original_form": "혼자 잘살려구요",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043170.1.7",
          "form": "사실 감정 시간 돈 타인한테 쏟을 여유도 없지않나유",
          "original_form": "사실 감정 시간 돈 타인한테 쏟을 여유도 없지않나유",
          "speaker_id": "1",
          "time": "20210907 21:00"
        },
        {
          "id": "MDRW2100043170.1.8",
          "form": "맞아요!!!! 아 진짜 쿨하게 혼자사는게 좋은데요ㅋㅋ",
          "original_form": "맞아요!!!! 아 진짜 쿨하게 혼자사는게 좋은데요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100043170.1.9",
          "form": "가끔 술한잔 커피한잔 디저트할 친구만 있음 되는데",
          "original_form": "가끔 술한잔 커피한잔 디저트할 친구만 있음 되는데",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100043170.1.10",
          "form": "다들 애인이나 남편이 베프라며... ㅋㅋ",
          "original_form": "다들 애인이나 남편이 베프라며... ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:16"
        },
        {
          "id": "MDRW2100043170.1.11",
          "form": "애인한테 올인하는 삶 너무 불안하지않나요?? 인생긴데",
          "original_form": "애인한테 올인하는 삶 너무 불안하지않나요?? 인생긴데",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100043170.1.12",
          "form": "남편도 이혼하면 남인디ㅋㅋ",
          "original_form": "남편도 이혼하면 남인디ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100043170.1.13",
          "form": "너무 믿고싶은것만 믿죠ㅋㅋ",
          "original_form": "너무 믿고싶은것만 믿죠ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
        {
          "id": "MDRW2100043170.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 진짜 법과 제도가 묶어주는 관계라는게 얼마나 허울뿐인지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 진짜 법과 제도가 묶어주는 관계라는게 얼마나 허울뿐인지",
          "speaker_id": "1",
          "time": "20210908 07:49"
        },
        {
          "id": "MDRW2100043170.1.15",
          "form": "ㅋㅋ그래도 비혼이 많이 늘고있어서 다행이에요",
          "original_form": "ㅋㅋ그래도 비혼이 많이 늘고있어서 다행이에요",
          "speaker_id": "2",
          "time": "20210908 07:56"
        },
        {
          "id": "MDRW2100043170.1.16",
          "form": "맞아요! 혹시 애는 낳고싶으신가요",
          "original_form": "맞아요! 혹시 애는 낳고싶으신가요",
          "speaker_id": "1",
          "time": "20210908 08:20"
        },
        {
          "id": "MDRW2100043170.1.17",
          "form": "비혼인데요ㅋㅋ",
          "original_form": "비혼인데요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:22"
        },
        {
          "id": "MDRW2100043170.1.18",
          "form": "놉 책임질 자신이 없어요ㅜㄱㅋㅋ",
          "original_form": "놉 책임질 자신이 없어요ㅜㄱㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 08:23"
        },
        {
          "id": "MDRW2100043170.1.19",
          "form": "ㅋㅋㅋㅋㅋ 저두요 \r\n제 친구중에 비혼인데 애는 낳아서 키우고싶다는 애가 있었어서",
          "original_form": "ㅋㅋㅋㅋㅋ 저두요 \r\n제 친구중에 비혼인데 애는 낳아서 키우고싶다는 애가 있었어서",
          "speaker_id": "1",
          "time": "20210908 11:15"
        },
        {
          "id": "MDRW2100043170.1.20",
          "form": "신기햇어요",
          "original_form": "신기햇어요",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100043170.1.21",
          "form": "아ㅋㅋ 진짜 애기 좋아하나봐요.. ㅋㅋ 애 입장도ㅋㅋ",
          "original_form": "아ㅋㅋ 진짜 애기 좋아하나봐요.. ㅋㅋ 애 입장도ㅋㅋ",
