        {
          "id": "MDRW2100022246.1.2",
          "form": "비와욤",
          "original_form": "비와욤",
          "speaker_id": "1",
          "time": "20210830 08:13"
        },
        {
          "id": "MDRW2100022246.1.3",
          "form": "우산챙겼나요?",
          "original_form": "우산챙겼나요?",
          "speaker_id": "2",
          "time": "20210830 08:13"
        },
        {
          "id": "MDRW2100022246.1.4",
          "form": "전화해보세요",
          "original_form": "전화해보세요",
          "speaker_id": "2",
          "time": "20210830 08:13"
        },
        {
          "id": "MDRW2100022246.1.5",
          "form": "데이트 하고싶은거",
          "original_form": "데이트 하고싶은거",
          "speaker_id": "1",
          "time": "20210830 08:40"
        },
        {
          "id": "MDRW2100022246.1.6",
          "form": "생각해노으세욥",
          "original_form": "생각해노으세욥",
          "speaker_id": "1",
          "time": "20210830 08:40"
        },
        {
          "id": "MDRW2100022246.1.7",
          "form": "주말에 데이트하시나요?",
          "original_form": "주말에 데이트하시나요?",
          "speaker_id": "1",
          "time": "20210901 16:48"
        },
        {
          "id": "MDRW2100022246.1.8",
          "form": "네 님은 데이트하세요?",
          "original_form": "네 님은 데이트하세요?",
          "speaker_id": "2",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100022246.1.9",
          "form": "하고싶은데",
          "original_form": "하고싶은데",
          "speaker_id": "1",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100022246.1.10",
          "form": "못해욤",
          "original_form": "못해욤",
          "speaker_id": "1",
          "time": "20210901 17:05"
        },
        {
          "id": "MDRW2100022246.1.11",
          "form": "데이트할사람이읍더요",
          "original_form": "데이트할사람이읍더요",
          "speaker_id": "1",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100022246.1.12",
          "form": "왜요?",
          "original_form": "왜요?",
          "speaker_id": "2",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100022246.1.13",
          "form": "남자친구있다면서요",
          "original_form": "남자친구있다면서요",
          "speaker_id": "2",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100022246.1.14",
          "form": "토욜에 약속잇데요",
          "original_form": "토욜에 약속잇데요",
          "speaker_id": "1",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100022246.1.15",
          "form": "버림받아떠욥",
          "original_form": "버림받아떠욥",
          "speaker_id": "1",
          "time": "20210901 17:06"
        },
        {
          "id": "MDRW2100022246.1.16",
          "form": "아닐거에요",
          "original_form": "아닐거에요",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100022246.1.17",
          "form": "희망을가져요",
          "original_form": "희망을가져요",
          "speaker_id": "2",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100022246.1.18",
          "form": "비오네요",
          "original_form": "비오네요",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100022246.1.19",
          "form": "씻고오세요",
          "original_form": "씻고오세요",
          "speaker_id": "1",
          "time": "20210901 17:07"
        },
        {
          "id": "MDRW2100022246.1.20",
          "form": "턱걸이할겁니다",
          "original_form": "턱걸이할겁니다",
          "speaker_id": "2",
          "time": "20210901 17:09"
        },
        {
          "id": "MDRW2100022246.1.21",
          "form": "담에뵈요",
          "original_form": "담에뵈요",
          "speaker_id": "2",
          "time": "20210901 17:10"
        },
        {
          "id": "MDRW2100022246.1.22",
          "form": "오오 굿 열운동하고오세유",
          "original_form": "오오 굿 열운동하고오세유",
          "speaker_id": "1",
          "time": "20210901 17:11"
        },
        {
          "id": "MDRW2100022246.1.23",
          "form": "이걸로",
          "original_form": "이걸로",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100022246.1.24",
          "form": "답해주세요",
          "original_form": "답해주세요",
          "speaker_id": "1",
          "time": "20210901 17:21"
        },
        {
          "id": "MDRW2100022246.1.25",
          "form": "머하세요",
