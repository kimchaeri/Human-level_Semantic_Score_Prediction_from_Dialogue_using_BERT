        },
        {
          "id": "MDRW2100036574.1.2",
          "form": "내일 점심 뭐 먹을까",
          "original_form": "내일 점심 뭐 먹을까",
          "speaker_id": "1",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100036574.1.3",
          "form": "돈가스나 먹자",
          "original_form": "돈가스나 먹자",
          "speaker_id": "2",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100036574.1.4",
          "form": "난 탕수육이 땡기는데",
          "original_form": "난 탕수육이 땡기는데",
          "speaker_id": "1",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100036574.1.5",
          "form": "깐풍기 너무 아쉽겠다",
          "original_form": "깐풍기 너무 아쉽겠다",
          "speaker_id": "2",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100036574.1.6",
          "form": "맛있겠다",
          "original_form": "맛있겠다",
          "speaker_id": "2",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100036574.1.7",
          "form": "그럼 중식으로?",
          "original_form": "그럼 중식으로?",
          "speaker_id": "1",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100036574.1.8",
          "form": "아 근데 우리 집에 이연복 탕수육 있어",
          "original_form": "아 근데 우리 집에 이연복 탕수육 있어",
          "speaker_id": "2",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100036574.1.9",
          "form": "그럼 우선 그거부터 먹자",
          "original_form": "그럼 우선 그거부터 먹자",
          "speaker_id": "1",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100036574.1.10",
          "form": "소스도 같이 주더라",
          "original_form": "소스도 같이 주더라",
          "speaker_id": "2",
          "time": "20210831 00:54"
        },
        {
          "id": "MDRW2100036574.1.11",
          "form": "이혼 보고 탕수육 한번 먹어보고 싶네",
          "original_form": "이혼 보고 탕수육 한번 먹어보고 싶네",
          "speaker_id": "1",
          "time": "20210831 00:55"
        },
        {
          "id": "MDRW2100036574.1.12",
          "form": "이연복",
          "original_form": "이연복",
          "speaker_id": "1",
          "time": "20210831 00:55"
        },
        {
          "id": "MDRW2100036574.1.13",
          "form": "홈쇼핑에서 싸게 팔더라",
          "original_form": "홈쇼핑에서 싸게 팔더라",
          "speaker_id": "2",
          "time": "20210831 00:55"
        },
        {
          "id": "MDRW2100036574.1.14",
          "form": "홈쇼핑에서 맛있는 거 많이 팔더라",
          "original_form": "홈쇼핑에서 맛있는 거 많이 팔더라",
          "speaker_id": "1",
          "time": "20210831 00:55"
        },
        {
          "id": "MDRW2100036574.1.15",
          "form": "내일 중식 먹을 생각에 행복해요",
          "original_form": "내일 중식 먹을 생각에 행복해요",
          "speaker_id": "2",
          "time": "20210831 00:55"
        },
        {
          "id": "MDRW2100036574.1.16",
          "form": "이연복 동파육도 맛있겠던데",
          "original_form": "이연복 동파육도 맛있겠던데",
