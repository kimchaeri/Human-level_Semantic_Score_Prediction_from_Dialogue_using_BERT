      "utterance": [
        {
          "id": "MDRW2100035105.1.1",
          "form": "반려동물 키우세용?",
          "original_form": "반려동물 키우세용?",
          "speaker_id": "1",
          "time": "20210829 02:45"
        },
        {
          "id": "MDRW2100035105.1.2",
          "form": "저는 비염때문에 못키우는데 대신 동물 영상 많이 봐요!",
          "original_form": "저는 비염때문에 못키우는데 대신 동물 영상 많이 봐요!",
          "speaker_id": "1",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100035105.1.3",
          "form": "ㅋ영화보시고 ㅋㅋㅋ욕하시면 안되요 ㅋㅋ제가 이상하게 스포한걸 ㅋㅋ",
          "original_form": "ㅋ영화보시고 ㅋㅋㅋ욕하시면 안되요 ㅋㅋ제가 이상하게 스포한걸 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100035105.1.4",
          "form": "아니에용!ㅋㅋㅋㅋ",
          "original_form": "아니에용!ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100035105.1.5",
          "form": "괜찮아요ㅋㅋㅋ",
          "original_form": "괜찮아요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100035105.1.6",
          "form": "전 부모님댁에 살땐 키웠어요 ㅋㅋㅋ",
          "original_form": "전 부모님댁에 살땐 키웠어요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100035105.1.7",
          "form": "강아지 ㅋㅋㅋ",
          "original_form": "강아지 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100035105.1.8",
          "form": "와아",
          "original_form": "와아",
          "speaker_id": "1",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100035105.1.9",
          "form": "종이 뭐에용?",
          "original_form": "종이 뭐에용?",
          "speaker_id": "1",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100035105.1.10",
          "form": "지금은 안키워요ㅠㅠ말티푸",
          "original_form": "지금은 안키워요ㅠㅠ말티푸",
          "speaker_id": "2",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100035105.1.11",
          "form": "아아ㅠㅠ",
          "original_form": "아아ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100035105.1.12",
          "form": "말티푸 완전 귀엽겠어요",
          "original_form": "말티푸 완전 귀엽겠어요",
          "speaker_id": "1",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100035105.1.13",
          "form": "이제 열살두넘었어요",
          "original_form": "이제 열살두넘었어요",
          "speaker_id": "2",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100035105.1.14",
          "form": "털 꼬불꼬불 ㅣㅋㅋㅋ",
          "original_form": "털 꼬불꼬불 ㅣㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:46"
        },
        {
          "id": "MDRW2100035105.1.15",
          "form": "오 더 오래살아서 장수했음 좋겠어요!",
          "original_form": "오 더 오래살아서 장수했음 좋겠어요!",
          "speaker_id": "1",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100035105.1.16",
          "form": "완전 말량쟁이에요 ㅋㅋㅋㅋ",
          "original_form": "완전 말량쟁이에요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100035105.1.17",
          "form": "네네 지금은 동생이 결혼해서 제부랑 키우는데 ㅋㅋ",
          "original_form": "네네 지금은 동생이 결혼해서 제부랑 키우는데 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100035105.1.18",
          "form": "와아ㅜㅜ 퇴근하고 집에오면 너무 행복할것같아요",
          "original_form": "와아ㅜㅜ 퇴근하고 집에오면 너무 행복할것같아요",
          "speaker_id": "1",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100035105.1.19",
          "form": "완전 더 젊어진거같아요 ㅋㅋㅋ",
          "original_form": "완전 더 젊어진거같아요 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100035105.1.20",
          "form": "애교가 장난아니에요 ㅠㅠ",
          "original_form": "애교가 장난아니에요 ㅠㅠ",
          "speaker_id": "2",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100035105.1.21",
          "form": "그 얼마전에 동물농장에서 완전 오래 산 강아지",
          "original_form": "그 얼마전에 동물농장에서 완전 오래 산 강아지",
          "speaker_id": "1",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100035105.1.22",
          "form": "있었는데",
          "original_form": "있었는데",
          "speaker_id": "1",
          "time": "20210829 02:47"
        },
        {
          "id": "MDRW2100035105.1.23",
          "form": "진짜 안고있으면 포근하고 좋아요 ㅎ",
          "original_form": "진짜 안고있으면 포근하고 좋아요 ㅎ",
          "speaker_id": "2",
          "time": "20210829 02:48"
        },
        {
          "id": "MDRW2100035105.1.24",
          "form": "코가 다 닳았더라구요ㅋㅋㅋ",
          "original_form": "코가 다 닳았더라구요ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210829 02:48"
        },
        {
          "id": "MDRW2100035105.1.25",
          "form": "넘 사랑스러울것같아요ㅠㅠ",
          "original_form": "넘 사랑스러울것같아요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210829 02:48"
        },
        {
          "id": "MDRW2100035105.1.26",
          "form": "에고 ㅋㅋㅋ 꺔장 촉촉해야하는데",
          "original_form": "에고 ㅋㅋㅋ 꺔장 촉촉해야하는데",
          "speaker_id": "2",
          "time": "20210829 02:48"
        },
        {
          "id": "MDRW2100035105.1.27",
          "form": "36살",
          "original_form": "36살",
          "speaker_id": "1",
          "time": "20210829 02:48"
        },
        {
          "id": "MDRW2100035105.1.28",
          "form": "강아지 쳐보시면 나올거에용!",
          "original_form": "강아지 쳐보시면 나올거에용!",
          "speaker_id": "1",
          "time": "20210829 02:48"
        },
        {
          "id": "MDRW2100035105.1.29",
          "form": "와 그렇게 살수가있나요",
          "original_form": "와 그렇게 살수가있나요",
          "speaker_id": "2",
          "time": "20210829 02:48"
        }
      ]
    }
