      "utterance": [
        {
          "id": "MDRW2100029892.1.1",
          "form": "약사의 혼잣말은 남주 설정이 좀 특이하다고 느꼈어",
          "original_form": "약사의 혼잣말은 남주 설정이 좀 특이하다고 느꼈어",
          "speaker_id": "2",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100029892.1.2",
          "form": "남자들도 반하는 미인??ㅋㅋㅋㅋㅋ",
          "original_form": "남자들도 반하는 미인??ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100029892.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 진시가 절세미인이지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ 진시가 절세미인이지",
          "speaker_id": "1",
          "time": "20210903 16:10"
        },
        {
          "id": "MDRW2100029892.1.4",
          "form": "호르몬제같은거 먹었다고했나",
          "original_form": "호르몬제같은거 먹었다고했나",
          "speaker_id": "2",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100029892.1.5",
          "form": "하여튼 특이한 남주같음",
          "original_form": "하여튼 특이한 남주같음",
          "speaker_id": "2",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100029892.1.6",
          "form": "성격은 뭐.... 많이있눈 계열이지만",
          "original_form": "성격은 뭐.... 많이있눈 계열이지만",
          "speaker_id": "2",
          "time": "20210903 16:11"
        },
        {
          "id": "MDRW2100029892.1.7",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100029892.1.8",
          "form": "안 서게",
          "original_form": "안 서게",
          "speaker_id": "1",
          "time": "20210903 16:12"
        },
        {
          "id": "MDRW2100029892.1.9",
          "form": "한다고 한 거 같음",
          "original_form": "한다고 한 거 같음",
          "speaker_id": "1",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100029892.1.10",
          "form": "그건 여주가",
          "original_form": "그건 여주가",
          "speaker_id": "1",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100029892.1.11",
          "form": "성격이 특이해서",
          "original_form": "성격이 특이해서",
          "speaker_id": "1",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100029892.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ그런거였군",
          "original_form": "ㅋㅋㅋㅋㅋㅋ그런거였군",
          "speaker_id": "2",
          "time": "20210903 16:13"
        },
        {
          "id": "MDRW2100029892.1.13",
          "form": "예쁜남주 좋아해서 만족스러워",
          "original_form": "예쁜남주 좋아해서 만족스러워",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100029892.1.14",
          "form": "얼굴이 ^ㅁ^",
          "original_form": "얼굴이 ^ㅁ^",
          "speaker_id": "2",
          "time": "20210903 16:14"
        },
        {
          "id": "MDRW2100029892.1.15",
          "form": "원래 절세미인이고",
          "original_form": "원래 절세미인이고",
          "speaker_id": "1",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100029892.1.16",
          "form": "거기에 환관이라서",
          "original_form": "거기에 환관이라서",
          "speaker_id": "1",
          "time": "20210903 16:32"
        },
        {
          "id": "MDRW2100029892.1.17",
          "form": "여주가 대충 그나마 다행이구먼~ 하는 거고ㅋㅋㅋㅋㅋ",
          "original_form": "여주가 대충 그나마 다행이구먼~ 하는 거고ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100029892.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100029892.1.19",
          "form": "나라가 멸망하는 미모니까ㅋㅋㅋㅋㅋ",
          "original_form": "나라가 멸망하는 미모니까ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100029892.1.20",
          "form": "그런 남주 더 보고싶다",
          "original_form": "그런 남주 더 보고싶다",
          "speaker_id": "2",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100029892.1.21",
          "form": "근데 여주 성격이",
          "original_form": "근데 여주 성격이",
          "speaker_id": "1",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100029892.1.22",
          "form": "써줘요",
          "original_form": "써줘요",
          "speaker_id": "2",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100029892.1.23",
          "form": "눈치가 좋은데",
          "original_form": "눈치가 좋은데",
          "speaker_id": "1",
          "time": "20210903 16:33"
        },
        {
          "id": "MDRW2100029892.1.24",
          "form": "귀찮아지니까 일부러 생각을 안하는 거라",
          "original_form": "귀찮아지니까 일부러 생각을 안하는 거라",
          "speaker_id": "1",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029892.1.25",
          "form": "남주 정체 알아차리려면 진즉에 알아차리긴 했는데",
          "original_form": "남주 정체 알아차리려면 진즉에 알아차리긴 했는데",
          "speaker_id": "1",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029892.1.26",
          "form": "생각하기 싫어서",
          "original_form": "생각하기 싫어서",
          "speaker_id": "1",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029892.1.27",
          "form": "ㅋㅋㅋㅋㅋㅋ응응",
          "original_form": "ㅋㅋㅋㅋㅋㅋ응응",
          "speaker_id": "2",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029892.1.28",
          "form": "걍 놔두는 거",
          "original_form": "걍 놔두는 거",
          "speaker_id": "1",
          "time": "20210903 16:34"
        },
        {
          "id": "MDRW2100029892.1.29",
          "form": "마오마오",
          "original_form": "마오마오",
          "speaker_id": "2",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029892.1.30",
          "form": "이게 좀 웃겼던게",
          "original_form": "이게 좀 웃겼던게",
          "speaker_id": "1",
          "time": "20210903 16:35"
        },
        {
          "id": "MDRW2100029892.1.31",
          "form": "선황이 헨타이였단 말임",
          "original_form": "선황이 헨타이였단 말임",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029892.1.32",
          "form": "변",
          "original_form": "변",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029892.1.33",
          "form": "태역시 안되는군",
          "original_form": "태역시 안되는군",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029892.1.34",
          "form": "선황취향이 어린애여서 현황제는 그걸 극혐함",
          "original_form": "선황취향이 어린애여서 현황제는 그걸 극혐함",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029892.1.35",
          "form": "먼진 알겠거든",
          "original_form": "먼진 알겠거든",
          "speaker_id": "1",
          "time": "20210903 16:36"
        },
        {
          "id": "MDRW2100029892.1.36",
          "form": "근데 현황제는 30대 중반이고 황비들은 10대 중후반이니까",
          "original_form": "근데 현황제는 30대 중반이고 황비들은 10대 중후반이니까",
          "speaker_id": "1",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100029892.1.37",
          "form": "인지부조화옴",
          "original_form": "인지부조화옴",
          "speaker_id": "1",
          "time": "20210903 16:37"
        },
        {
          "id": "MDRW2100029892.1.38",
          "form": "헉 더 늙은줄",
          "original_form": "헉 더 늙은줄",
          "speaker_id": "2",
          "time": "20210903 16:38"
        },
        {
          "id": "MDRW2100029892.1.39",
          "form": "그... 너무 애가가 있어서",
          "original_form": "그... 너무 애가가 있어서",
          "speaker_id": "2",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100029892.1.40",
          "form": "징그럽긴하더라",
          "original_form": "징그럽긴하더라",
          "speaker_id": "2",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100029892.1.41",
          "form": "걔는 그거야",
          "original_form": "걔는 그거야",
          "speaker_id": "1",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100029892.1.42",
          "form": "선황이 자기 후궁으로 들였는데",
          "original_form": "선황이 자기 후궁으로 들였는데",
          "speaker_id": "1",
          "time": "20210903 16:39"
        },
        {
          "id": "MDRW2100029892.1.43",
          "form": "너무 어려서 현황제랑 현재는 폐비가 된 전후궁이 갸를 키움",
          "original_form": "너무 어려서 현황제랑 현재는 폐비가 된 전후궁이 갸를 키움",
          "speaker_id": "1",
          "time": "20210903 16:40"
        },
        {
          "id": "MDRW2100029892.1.44",
          "form": "대충 의붓아들이랑 의붓며느리가 키운 거 ㅇㅇ",
          "original_form": "대충 의붓아들이랑 의붓며느리가 키운 거 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210903 16:40"
        },
        {
          "id": "MDRW2100029892.1.45",
          "form": "그래서 선황 죽고나서",
          "original_form": "그래서 선황 죽고나서",
          "speaker_id": "1",
          "time": "20210903 16:40"
        },
        {
          "id": "MDRW2100029892.1.46",
          "form": "얘가 돌아가면 집에서 어떤 대접을 받을 지 아니까",
          "original_form": "얘가 돌아가면 집에서 어떤 대접을 받을 지 아니까",
          "speaker_id": "1",
          "time": "20210903 16:40"
        },
        {
          "id": "MDRW2100029892.1.47",
          "form": "일단 자기 후궁으로 놔두고",
          "original_form": "일단 자기 후궁으로 놔두고",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029892.1.48",
          "form": "나이 먹으면 좋은 남자랑 혼례시켜주려고 데리고 있는 거 ㅇㅇ",
          "original_form": "나이 먹으면 좋은 남자랑 혼례시켜주려고 데리고 있는 거 ㅇㅇ",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029892.1.49",
          "form": "신하한테 하사하는 개념으로",
          "original_form": "신하한테 하사하는 개념으로",
          "speaker_id": "1",
          "time": "20210903 16:41"
        },
        {
          "id": "MDRW2100029892.1.50",
          "form": "것도 싫다",
          "original_form": "것도 싫다",
          "speaker_id": "2",
          "time": "20210903 16:43"
        },
        {
          "id": "MDRW2100029892.1.51",
          "form": "근데 어차피 그쪽은",
          "original_form": "근데 어차피 그쪽은",
          "speaker_id": "1",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100029892.1.52",
          "form": "연애결혼으로 흘러갈거라",
          "original_form": "연애결혼으로 흘러갈거라",
          "speaker_id": "1",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100029892.1.53",
          "form": "약간 우리나라 왕이 공주 시집보내는 느낌정도로 인식하는 중",
          "original_form": "약간 우리나라 왕이 공주 시집보내는 느낌정도로 인식하는 중",
          "speaker_id": "1",
          "time": "20210903 16:44"
        },
        {
          "id": "MDRW2100029892.1.54",
          "form": "시대상이 그렇지만 ㅋㅋㅋ",
          "original_form": "시대상이 그렇지만 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210903 16:45"
        },
        {
          "id": "MDRW2100029892.1.55",
          "form": "어차피 거기 계속 있으면 후궁으로 있는 거고 (안 건드림)",
          "original_form": "어차피 거기 계속 있으면 후궁으로 있는 거고 (안 건드림)",
          "speaker_id": "1",
          "time": "20210903 16:45"
        },
        {
          "id": "MDRW2100029892.1.56",
          "form": "나가겠다고 하면 내보내주고 연애하겠다면 연애시켜주고",
          "original_form": "나가겠다고 하면 내보내주고 연애하겠다면 연애시켜주고",
