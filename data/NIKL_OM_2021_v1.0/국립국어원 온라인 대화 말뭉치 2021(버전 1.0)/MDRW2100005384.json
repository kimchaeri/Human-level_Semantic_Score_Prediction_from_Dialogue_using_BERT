      "utterance": [
        {
          "id": "MDRW2100005384.1.1",
          "form": "배달먹어요?",
          "original_form": "배달먹어요?",
          "speaker_id": "1",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005384.1.2",
          "form": "네 외식 어려워서",
          "original_form": "네 외식 어려워서",
          "speaker_id": "2",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005384.1.3",
          "form": "가끔 시켜요",
          "original_form": "가끔 시켜요",
          "speaker_id": "2",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005384.1.4",
          "form": "피자나 치킨 같은거요",
          "original_form": "피자나 치킨 같은거요",
          "speaker_id": "2",
          "time": "20210821 22:09"
        },
        {
          "id": "MDRW2100005384.1.5",
          "form": "저도 식당가기가 두려워",
          "original_form": "저도 식당가기가 두려워",
          "speaker_id": "1",
          "time": "20210821 22:10"
        },
        {
          "id": "MDRW2100005384.1.6",
          "form": "주로 어떤 음식 시켜먹나요?",
          "original_form": "주로 어떤 음식 시켜먹나요?",
          "speaker_id": "2",
          "time": "20210821 22:13"
        },
        {
          "id": "MDRW2100005384.1.7",
          "form": "치킨이여",
          "original_form": "치킨이여",
          "speaker_id": "1",
          "time": "20210821 22:14"
        },
        {
          "id": "MDRW2100005384.1.8",
          "form": "어디 치킨집에서 시켜 먹나요? 저는 육십계나 푸라닭",
          "original_form": "어디 치킨집에서 시켜 먹나요? 저는 육십계나 푸라닭",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005384.1.9",
          "form": "네네요",
          "original_form": "네네요",
          "speaker_id": "1",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005384.1.10",
          "form": "주로 어떤 맛으로 시키나요?",
          "original_form": "주로 어떤 맛으로 시키나요?",
          "speaker_id": "2",
          "time": "20210821 22:16"
        },
        {
          "id": "MDRW2100005384.1.11",
          "form": "후라이드요",
          "original_form": "후라이드요",
          "speaker_id": "1",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005384.1.12",
          "form": "기본에 충실하네요",
          "original_form": "기본에 충실하네요",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005384.1.13",
          "form": "피자도 잘 시켜 먹나요?",
          "original_form": "피자도 잘 시켜 먹나요?",
          "speaker_id": "2",
          "time": "20210821 22:17"
        },
        {
          "id": "MDRW2100005384.1.14",
          "form": "네 좋아해요",
          "original_form": "네 좋아해요",
          "speaker_id": "1",
          "time": "20210821 22:21"
        },
        {
          "id": "MDRW2100005384.1.15",
          "form": "어디 브랜드 거 좋아하나요?",
          "original_form": "어디 브랜드 거 좋아하나요?",
          "speaker_id": "2",
          "time": "20210821 22:24"
        },
        {
          "id": "MDRW2100005384.1.16",
          "form": "도미노요",
          "original_form": "도미노요",
          "speaker_id": "1",
          "time": "20210821 22:25"
        }
