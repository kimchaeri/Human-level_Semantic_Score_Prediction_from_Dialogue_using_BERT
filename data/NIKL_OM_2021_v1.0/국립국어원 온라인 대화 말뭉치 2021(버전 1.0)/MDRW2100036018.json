      "utterance": [
        {
          "id": "MDRW2100036018.1.1",
          "form": "점심 드셨나요?",
          "original_form": "점심 드셨나요?",
          "speaker_id": "1",
          "time": "20210830 14:13"
        },
        {
          "id": "MDRW2100036018.1.2",
          "form": "네! 간단히 도시락 싸온거 먹었습니다 ㅎㅎ",
          "original_form": "네! 간단히 도시락 싸온거 먹었습니다 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 14:27"
        },
        {
          "id": "MDRW2100036018.1.3",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210830 14:27"
        },
        {
          "id": "MDRW2100036018.1.4",
          "form": "저는 돼지불고기 먹었어요!",
          "original_form": "저는 돼지불고기 먹었어요!",
          "speaker_id": "1",
          "time": "20210830 14:29"
        },
        {
          "id": "MDRW2100036018.1.5",
          "form": "우와 맛있었겠어요",
          "original_form": "우와 맛있었겠어요",
          "speaker_id": "2",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100036018.1.6",
          "form": "고추장 양념이었나요?",
          "original_form": "고추장 양념이었나요?",
          "speaker_id": "2",
          "time": "20210830 14:42"
        },
        {
          "id": "MDRW2100036018.1.7",
          "form": "아뇨! 간장양념이요'",
          "original_form": "아뇨! 간장양념이요'",
          "speaker_id": "1",
          "time": "20210830 16:10"
        },
        {
          "id": "MDRW2100036018.1.8",
          "form": "앗 그렇군요 ㅎㅎ",
          "original_form": "앗 그렇군요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 16:13"
        },
        {
          "id": "MDRW2100036018.1.9",
          "form": "식당에서 사서 드신건가요?",
          "original_form": "식당에서 사서 드신건가요?",
          "speaker_id": "2",
          "time": "20210830 16:13"
        },
        {
          "id": "MDRW2100036018.1.10",
          "form": "네네! 점심으로 나와서 먹었어요",
          "original_form": "네네! 점심으로 나와서 먹었어요",
          "speaker_id": "1",
          "time": "20210830 16:17"
        },
        {
          "id": "MDRW2100036018.1.11",
          "form": "좋으셨겠다~",
          "original_form": "좋으셨겠다~",
          "speaker_id": "2",
          "time": "20210830 16:19"
        },
        {
          "id": "MDRW2100036018.1.12",
          "form": "요즘은 일상이 단조로워서",
          "original_form": "요즘은 일상이 단조로워서",
          "speaker_id": "2",
          "time": "20210830 16:19"
        },
        {
          "id": "MDRW2100036018.1.13",
          "form": "특별식만 먹어도 엄청 큰 이벤트예요 ㅎㅎ",
          "original_form": "특별식만 먹어도 엄청 큰 이벤트예요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 16:20"
        },
        {
          "id": "MDRW2100036018.1.14",
          "form": "맞아요ㅋㅋㅋㅋ 일상이 무료하니까요ㅠ",
          "original_form": "맞아요ㅋㅋㅋㅋ 일상이 무료하니까요ㅠ",
          "speaker_id": "1",
          "time": "20210830 16:21"
        },
        {
          "id": "MDRW2100036018.1.15",
          "form": "그래서인지 요즘 디저트가 더 화려해지는 것 같지 않나요?!",
          "original_form": "그래서인지 요즘 디저트가 더 화려해지는 것 같지 않나요?!",
          "speaker_id": "2",
          "time": "20210830 16:44"
        },
        {
          "id": "MDRW2100036018.1.16",
          "form": "그래서일수도 있겠네요?? 확실히 단가도 올라가고 꾸며진 인스타용 디저트가 많으니까요",
          "original_form": "그래서일수도 있겠네요?? 확실히 단가도 올라가고 꾸며진 인스타용 디저트가 많으니까요",
          "speaker_id": "1",
          "time": "20210830 16:45"
        },
        {
          "id": "MDRW2100036018.1.17",
          "form": "맞아요 ",
          "original_form": "맞아요 ",
          "speaker_id": "2",
          "time": "20210830 17:02"
        },
        {
          "id": "MDRW2100036018.1.18",
          "form": "이제는 밥 한 끼보다 디저트가 더 비싼 것 같기도 해요 ㅎㅎ",
          "original_form": "이제는 밥 한 끼보다 디저트가 더 비싼 것 같기도 해요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210830 17:03"
        },
        {
          "id": "MDRW2100036018.1.19",
          "form": "음료랑 디저트만 먹어도 만 원이 훌쩍 넘죠..",
          "original_form": "음료랑 디저트만 먹어도 만 원이 훌쩍 넘죠..",
          "speaker_id": "1",
          "time": "20210830 17:18"
        },
        {
          "id": "MDRW2100036018.1.20",
          "form": "맞아요,,",
          "original_form": "맞아요,,",
          "speaker_id": "2",
          "time": "20210830 17:33"
        },
        {
          "id": "MDRW2100036018.1.21",
          "form": "요즘은 케이크도 너무 비싸요",
          "original_form": "요즘은 케이크도 너무 비싸요",
          "speaker_id": "2",
          "time": "20210830 17:33"
        },
        {
          "id": "MDRW2100036018.1.22",
          "form": "크기는 작은데 말이죠,,",
          "original_form": "크기는 작은데 말이죠,,",
          "speaker_id": "2",
          "time": "20210830 17:33"
        }
