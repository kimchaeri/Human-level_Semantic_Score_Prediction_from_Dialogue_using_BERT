      "utterance": [
        {
          "id": "MDRW2100019042.1.1",
          "form": "네~",
          "original_form": "네~",
          "speaker_id": "2",
          "time": "20210827 14:48"
        },
        {
          "id": "MDRW2100019042.1.2",
          "form": "구매를 고려하고 있는 책 있으신가요?",
          "original_form": "구매를 고려하고 있는 책 있으신가요?",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019042.1.3",
          "form": "저는 마이클 샌델의 신작 읽어보고 싶어서 장바구니에 담아뒀어요",
          "original_form": "저는 마이클 샌델의 신작 읽어보고 싶어서 장바구니에 담아뒀어요",
          "speaker_id": "2",
          "time": "20210827 14:50"
        },
        {
          "id": "MDRW2100019042.1.4",
          "form": "요즘 학교 도서관에서 대부분 빌려서 딱히 구매 예정인 책은 없어요!",
          "original_form": "요즘 학교 도서관에서 대부분 빌려서 딱히 구매 예정인 책은 없어요!",
          "speaker_id": "1",
          "time": "20210827 14:51"
        },
        {
          "id": "MDRW2100019042.1.5",
          "form": "책 제목이 뭔가요?",
          "original_form": "책 제목이 뭔가요?",
          "speaker_id": "1",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019042.1.6",
          "form": "공정하다는 착각 이요",
          "original_form": "공정하다는 착각 이요",
          "speaker_id": "2",
          "time": "20210827 14:52"
        },
        {
          "id": "MDRW2100019042.1.7",
          "form": "아하~",
          "original_form": "아하~",
          "speaker_id": "1",
          "time": "20210827 14:53"
        },
        {
          "id": "MDRW2100019042.1.8",
          "form": "사회학 쪽 도서인가 봐요",
          "original_form": "사회학 쪽 도서인가 봐요",
          "speaker_id": "1",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019042.1.9",
          "form": "네네 맞아요",
          "original_form": "네네 맞아요",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019042.1.10",
          "form": "저자가 정의란 무엇인가 썼던 교수예요",
          "original_form": "저자가 정의란 무엇인가 썼던 교수예요",
          "speaker_id": "2",
          "time": "20210827 14:54"
        },
        {
          "id": "MDRW2100019042.1.11",
          "form": "아 유명한 책이죠! 들어본 적 있어요",
          "original_form": "아 유명한 책이죠! 들어본 적 있어요",
          "speaker_id": "1",
          "time": "20210827 14:55"
        },
        {
          "id": "MDRW2100019042.1.12",
          "form": "책은 분야 안 가리고 골고루 읽는 편이신가요?",
          "original_form": "책은 분야 안 가리고 골고루 읽는 편이신가요?",
          "speaker_id": "1",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019042.1.13",
          "form": "저는 보통 실용서 위주로 보는거같아요",
          "original_form": "저는 보통 실용서 위주로 보는거같아요",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019042.1.14",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210827 14:56"
        },
        {
          "id": "MDRW2100019042.1.15",
          "form": "예전에는 소설 많이 읽었는데 요즘은 저도 분야 안 가리고 보는 거 같아요.",
          "original_form": "예전에는 소설 많이 읽었는데 요즘은 저도 분야 안 가리고 보는 거 같아요.",
          "speaker_id": "1",
          "time": "20210827 14:57"
        },
        {
          "id": "MDRW2100019042.1.16",
          "form": "또 도움이 됐거나 흥미롭게 읽은 책 있으세요?",
          "original_form": "또 도움이 됐거나 흥미롭게 읽은 책 있으세요?",
          "speaker_id": "1",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019042.1.17",
          "form": "문학책은 친구한테 추천받아서",
          "original_form": "문학책은 친구한테 추천받아서",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019042.1.18",
          "form": "우리가 빛의 속도로 갈 수 없다면? 사뒀어요",
          "original_form": "우리가 빛의 속도로 갈 수 없다면? 사뒀어요",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019042.1.19",
          "form": "아직 읽지는 않앗지만요 ㅋㅋㅋㅋ",
          "original_form": "아직 읽지는 않앗지만요 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 14:58"
        },
        {
          "id": "MDRW2100019042.1.20",
          "form": "책은 대부분 사서 읽으시나 봐요",
          "original_form": "책은 대부분 사서 읽으시나 봐요",
          "speaker_id": "1",
          "time": "20210827 15:02"
        },
        {
          "id": "MDRW2100019042.1.21",
          "form": "네네 학생때는 구매희망도서 자주 이용했었고",
          "original_form": "네네 학생때는 구매희망도서 자주 이용했었고",
          "speaker_id": "2",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100019042.1.22",
          "form": "지금은 회사에서 도서지원금이 나와서 그걸로 구매해요",
          "original_form": "지금은 회사에서 도서지원금이 나와서 그걸로 구매해요",
          "speaker_id": "2",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100019042.1.23",
          "form": "오 되게 좋네요!",
          "original_form": "오 되게 좋네요!",
          "speaker_id": "1",
          "time": "20210827 15:03"
        },
        {
          "id": "MDRW2100019042.1.24",
          "form": "얼마 정도 나오나요?",
          "original_form": "얼마 정도 나오나요?",
          "speaker_id": "1",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100019042.1.25",
          "form": "1년에 30만원정도요!",
          "original_form": "1년에 30만원정도요!",
          "speaker_id": "2",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100019042.1.26",
          "form": "요즘은 책값이 많이 올라서",
          "original_form": "요즘은 책값이 많이 올라서",
          "speaker_id": "2",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100019042.1.27",
          "form": "좀만 비싼 도서 사면 금방 나가긴 하는데",
          "original_form": "좀만 비싼 도서 사면 금방 나가긴 하는데",
          "speaker_id": "2",
          "time": "20210827 15:04"
        },
        {
          "id": "MDRW2100019042.1.28",
          "form": "그래도 추천도서들 정도는 구매할 수 있는 금액인거같아요",
          "original_form": "그래도 추천도서들 정도는 구매할 수 있는 금액인거같아요",
          "speaker_id": "2",
          "time": "20210827 15:04"
        }
      ]
    }
