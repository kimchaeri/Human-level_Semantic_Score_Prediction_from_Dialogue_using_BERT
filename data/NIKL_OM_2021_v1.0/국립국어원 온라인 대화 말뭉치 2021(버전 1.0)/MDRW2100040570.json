      "utterance": [
        {
          "id": "MDRW2100040570.1.1",
          "form": "술값은 사실 거기가 오히려 조금 싼 편인거같아요",
          "original_form": "술값은 사실 거기가 오히려 조금 싼 편인거같아요",
          "speaker_id": "2",
          "time": "20210904 22:46"
        },
        {
          "id": "MDRW2100040570.1.2",
          "form": "비슷~ 일이천원 저렴",
          "original_form": "비슷~ 일이천원 저렴",
          "speaker_id": "2",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040570.1.3",
          "form": "아하 그렇군요ㅠ 술은 괜찮은데",
          "original_form": "아하 그렇군요ㅠ 술은 괜찮은데",
          "speaker_id": "1",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040570.1.4",
          "form": "안주가 문제인거군요",
          "original_form": "안주가 문제인거군요",
          "speaker_id": "1",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040570.1.5",
          "form": "정도 라고 생각하시먼 될듯",
          "original_form": "정도 라고 생각하시먼 될듯",
          "speaker_id": "2",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040570.1.6",
          "form": "요즘은 술도..ㅎㅎ",
          "original_form": "요즘은 술도..ㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040570.1.7",
          "form": "그리고 여성들만 갈 수 있다는 장점...",
          "original_form": "그리고 여성들만 갈 수 있다는 장점...",
          "speaker_id": "1",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040570.1.8",
          "form": "으아아아ㅠ",
          "original_form": "으아아아ㅠ",
          "speaker_id": "1",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040570.1.9",
          "form": "맞아요 그래서 첨엔 완벽햇죠",
          "original_form": "맞아요 그래서 첨엔 완벽햇죠",
          "speaker_id": "2",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040570.1.10",
          "form": "그럼 이제 점점",
          "original_form": "그럼 이제 점점",
          "speaker_id": "1",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040570.1.11",
          "form": "마진을 남기는 방향으로 가는 걸까요",
          "original_form": "마진을 남기는 방향으로 가는 걸까요",
          "speaker_id": "1",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040570.1.12",
          "form": "술도 맛닜고 저렴하고 여성전용이라 돈쭐 안낼 이유가 없엇는데",
          "original_form": "술도 맛닜고 저렴하고 여성전용이라 돈쭐 안낼 이유가 없엇는데",
          "speaker_id": "2",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040570.1.13",
          "form": "네네ㅠ",
          "original_form": "네네ㅠ",
          "speaker_id": "1",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040570.1.14",
          "form": "술이 점점 연해지니까 잘 안가게돼요",
          "original_form": "술이 점점 연해지니까 잘 안가게돼요",
          "speaker_id": "2",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040570.1.15",
          "form": "코로나 때문에 힘들어져서 그런가...",
          "original_form": "코로나 때문에 힘들어져서 그런가...",
          "speaker_id": "1",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040570.1.16",
          "form": "어쩌면 사람들 니즈를 반영해서 그런거같기도..",
          "original_form": "어쩌면 사람들 니즈를 반영해서 그런거같기도..",
          "speaker_id": "2",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040570.1.17",
          "form": "사실 저는",
          "original_form": "사실 저는",
          "speaker_id": "1",
          "time": "20210904 22:47"
        },
        {
          "id": "MDRW2100040570.1.18",
          "form": "거기 옷도...",
          "original_form": "거기 옷도...",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040570.1.19",
          "form": "아ㅠ 약한 술이요?",
          "original_form": "아ㅠ 약한 술이요?",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040570.1.20",
          "form": "저는 센거 좋아해서 술맛나는걸 좋아하니까요",
          "original_form": "저는 센거 좋아해서 술맛나는걸 좋아하니까요",
          "speaker_id": "2",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040570.1.21",
          "form": "네네네",
          "original_form": "네네네",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040570.1.22",
          "form": "네네 아니면 사실 저는 그생각이",
          "original_form": "네네 아니면 사실 저는 그생각이",
          "speaker_id": "2",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040570.1.23",
          "form": "거기 좀 지인분들이 알바 많이하잖아요",
          "original_form": "거기 좀 지인분들이 알바 많이하잖아요",
          "speaker_id": "2",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040570.1.24",
          "form": "네네!!!",
          "original_form": "네네!!!",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040570.1.25",
          "form": "네 맞아요",
          "original_form": "네 맞아요",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040570.1.26",
          "form": "그러다보니까 만드는 사람이 매번 달라지니까",
          "original_form": "그러다보니까 만드는 사람이 매번 달라지니까",
          "speaker_id": "2",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040570.1.27",
          "form": "인맥으로 이루어진 곳",
          "original_form": "인맥으로 이루어진 곳",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040570.1.28",
          "form": "술맛이 일정하지 못한거같아요",
          "original_form": "술맛이 일정하지 못한거같아요",
          "speaker_id": "2",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040570.1.29",
          "form": "ㅇㅇ그니까요",
          "original_form": "ㅇㅇ그니까요",
          "speaker_id": "2",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040570.1.30",
          "form": "아~!~!",
          "original_form": "아~!~!",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040570.1.31",
          "form": "아 그럴수도 있겠다",
          "original_form": "아 그럴수도 있겠다",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040570.1.32",
          "form": "매니저님이랑",
          "original_form": "매니저님이랑",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040570.1.33",
          "form": "전문적인 사람이 없으니",
          "original_form": "전문적인 사람이 없으니",
          "speaker_id": "2",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040570.1.34",
          "form": "name3님이랑..",
          "original_form": "&name3&님이랑..",
          "speaker_id": "1",
          "time": "20210904 22:48"
        },
        {
          "id": "MDRW2100040570.1.35",
          "form": "그래서 요즘 드는 생각이 역시",
          "original_form": "그래서 요즘 드는 생각이 역시",
          "speaker_id": "1",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100040570.1.36",
          "form": "뭐든지 유명해지고 봐야한다,,,",
          "original_form": "뭐든지 유명해지고 봐야한다,,,",
          "speaker_id": "1",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100040570.1.37",
          "form": "맞아요..옷도 사실ㅋㅋㅋㅋㅋ",
          "original_form": "맞아요..옷도 사실ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100040570.1.38",
          "form": "저는 그 샌드위치 맛있긴 했는데 너무 비싸서",
          "original_form": "저는 그 샌드위치 맛있긴 했는데 너무 비싸서",
          "speaker_id": "1",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100040570.1.39",
          "form": "그쵸",
          "original_form": "그쵸",
          "speaker_id": "1",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100040570.1.40",
          "form": "진짜 가격 무슨일 ㅋㅋㅋㅋ",
          "original_form": "진짜 가격 무슨일 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100040570.1.41",
          "form": "아니진짜",
          "original_form": "아니진짜",
          "speaker_id": "1",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100040570.1.42",
          "form": "그니까요",
          "original_form": "그니까요",
          "speaker_id": "1",
          "time": "20210904 22:49"
        },
        {
          "id": "MDRW2100040570.1.43",
          "form": "넼ㅋㅋㅋㅋ제 친구도 그거 몇십만원어치 샀는데",
          "original_form": "넼ㅋㅋㅋㅋ제 친구도 그거 몇십만원어치 샀는데",
