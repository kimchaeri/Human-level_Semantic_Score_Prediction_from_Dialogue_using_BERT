        {
          "id": "MDRW2100045131.1.1",
          "form": "전 연 1회!!! ㅋㅋ",
          "original_form": "전 연 1회!!! ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 00:57"
        },
        {
          "id": "MDRW2100045131.1.2",
          "form": "저도요 그정도?",
          "original_form": "저도요 그정도?",
          "speaker_id": "1",
          "time": "20210908 00:59"
        },
        {
          "id": "MDRW2100045131.1.3",
          "form": "오!!!",
          "original_form": "오!!!",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045131.1.4",
          "form": "저같은 사람 많지않은뎆ㅋㅈㅋ",
          "original_form": "저같은 사람 많지않은뎆ㅋㅈㅋ",
          "speaker_id": "2",
          "time": "20210908 01:00"
        },
        {
          "id": "MDRW2100045131.1.5",
          "form": "머리가 긴편이신가요?",
          "original_form": "머리가 긴편이신가요?",
          "speaker_id": "1",
          "time": "20210908 01:01"
        },
        {
          "id": "MDRW2100045131.1.6",
          "form": "단발자르고 펌하고 기르다가 다시 단발",
          "original_form": "단발자르고 펌하고 기르다가 다시 단발",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045131.1.7",
          "form": "반복이요ㅋㅋ",
          "original_form": "반복이요ㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 01:02"
        },
        {
          "id": "MDRW2100045131.1.8",
          "form": "전 단발이 자꾸 뻗쳐서 어깨선으로 자르고 너무 길면 미용실 가서 다시 어깨선으로 자르고를 반복해요",
          "original_form": "전 단발이 자꾸 뻗쳐서 어깨선으로 자르고 너무 길면 미용실 가서 다시 어깨선으로 자르고를 반복해요",
          "speaker_id": "1",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045131.1.9",
          "form": "펌은 안하세요?",
          "original_form": "펌은 안하세요?",
          "speaker_id": "2",
          "time": "20210908 01:04"
        },
        {
          "id": "MDRW2100045131.1.10",
          "form": "저 에어랩 있어요 ㅎㅎㅎㅎㅎ",
          "original_form": "저 에어랩 있어요 ㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210908 01:05"
        },
        {
          "id": "MDRW2100045131.1.11",
          "form": "아!!!! 만능템!!!",
          "original_form": "아!!!! 만능템!!!",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045131.1.12",
          "form": "진짜 좋아요?",
          "original_form": "진짜 좋아요?",
          "speaker_id": "2",
          "time": "20210908 01:06"
        },
        {
          "id": "MDRW2100045131.1.13",
          "form": "네~ 아무나 잘할 수 있어요",
          "original_form": "네~ 아무나 잘할 수 있어요",
          "speaker_id": "1",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045131.1.14",
          "form": "ㅋㅋ 가격이..??",
          "original_form": "ㅋㅋ 가격이..??",
          "speaker_id": "2",
          "time": "20210908 01:07"
        },
        {
          "id": "MDRW2100045131.1.15",
          "form": "60정도입니다.  리퍼로 잡아보세요",
          "original_form": "60정도입니다.  리퍼로 잡아보세요",
          "speaker_id": "1",
          "time": "20210908 01:08"
        },
        {
          "id": "MDRW2100045131.1.16",
          "form": "ㅋㅋ 친구가 생선으로 남편한테 받아냈어요ㅋㅋ",
