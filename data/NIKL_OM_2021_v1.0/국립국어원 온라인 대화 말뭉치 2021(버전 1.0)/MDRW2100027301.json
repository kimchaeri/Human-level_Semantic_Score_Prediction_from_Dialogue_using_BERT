      "utterance": [
        {
          "id": "MDRW2100027301.1.1",
          "form": "아니 원데이투데이하냐고",
          "original_form": "아니 원데이투데이하냐고",
          "speaker_id": "1",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027301.1.2",
          "form": "수수료싼곳으로한거같은데",
          "original_form": "수수료싼곳으로한거같은데",
          "speaker_id": "2",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027301.1.3",
          "form": "엔터사업하면 온콘은 똑바로 해야지",
          "original_form": "엔터사업하면 온콘은 똑바로 해야지",
          "speaker_id": "1",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027301.1.4",
          "form": "싼건....싼이유가있는법.....",
          "original_form": "싼건....싼이유가있는법.....",
          "speaker_id": "2",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027301.1.5",
          "form": "가성비따지지마라....",
          "original_form": "가성비따지지마라....",
          "speaker_id": "1",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027301.1.6",
          "form": "브이앱이",
          "original_form": "브이앱이",
          "speaker_id": "2",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027301.1.7",
          "form": "수수료가크다던데",
          "original_form": "수수료가크다던데",
          "speaker_id": "2",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027301.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ브이앱으로 하지",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ브이앱으로 하지",
          "speaker_id": "1",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027301.1.9",
          "form": "담엔그냥브이앱해....",
          "original_form": "담엔그냥브이앱해....",
          "speaker_id": "2",
          "time": "20210902 11:19"
        },
        {
          "id": "MDRW2100027301.1.10",
          "form": "그래도 버는게얼만데",
          "original_form": "그래도 버는게얼만데",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027301.1.11",
          "form": "보는사람속터지게하지말고....",
          "original_form": "보는사람속터지게하지말고....",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027301.1.12",
          "form": "아니",
          "original_form": "아니",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027301.1.13",
          "form": "왜 다 대기업대기업하곘어",
          "original_form": "왜 다 대기업대기업하곘어",
          "speaker_id": "1",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027301.1.14",
          "form": "화질도터지고내속도터지고....",
          "original_form": "화질도터지고내속도터지고....",
          "speaker_id": "2",
          "time": "20210902 11:20"
        },
        {
          "id": "MDRW2100027301.1.15",
          "form": "이유가있으니까.....",
          "original_form": "이유가있으니까.....",
          "speaker_id": "1",
          "time": "20210902 11:21"
        },
        {
          "id": "MDRW2100027301.1.16",
          "form": "콘이얼나중요한데",
