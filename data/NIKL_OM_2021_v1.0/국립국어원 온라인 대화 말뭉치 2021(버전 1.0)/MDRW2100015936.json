      "utterance": [
        {
          "id": "MDRW2100015936.1.1",
          "form": "저 유튜브 중독자라!",
          "original_form": "저 유튜브 중독자라!",
          "speaker_id": "2",
          "time": "20210826 03:15"
        },
        {
          "id": "MDRW2100015936.1.2",
          "form": "엇 저두요!",
          "original_form": "엇 저두요!",
          "speaker_id": "1",
          "time": "20210826 03:15"
        },
        {
          "id": "MDRW2100015936.1.3",
          "form": "유튜브 프리미엄 가입했어용",
          "original_form": "유튜브 프리미엄 가입했어용",
          "speaker_id": "1",
          "time": "20210826 03:15"
        },
        {
          "id": "MDRW2100015936.1.4",
          "form": "저두요!!ㅋㅋㅋㅋ",
          "original_form": "저두요!!ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 03:15"
        },
        {
          "id": "MDRW2100015936.1.5",
          "form": "광고없이 볼수잇어서 넘 좋아요",
          "original_form": "광고없이 볼수잇어서 넘 좋아요",
          "speaker_id": "2",
          "time": "20210826 03:15"
        },
        {
          "id": "MDRW2100015936.1.6",
          "form": "광고 뭔 두개나 나오규 짜증나게 ㅋㅋㅋㅋ",
          "original_form": "광고 뭔 두개나 나오규 짜증나게 ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 03:15"
        },
        {
          "id": "MDRW2100015936.1.7",
          "form": "광고 너무 많아져가지고 아휴 졌다 졌다 하고",
          "original_form": "광고 너무 많아져가지고 아휴 졌다 졌다 하고",
          "speaker_id": "1",
          "time": "20210826 03:15"
        },
        {
          "id": "MDRW2100015936.1.8",
          "form": "가입했어욬ㅋㅋㅋ",
          "original_form": "가입했어욬ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 03:15"
        },
        {
          "id": "MDRW2100015936.1.9",
          "form": "유튜브 말고도 보시는거 있으세용?",
          "original_form": "유튜브 말고도 보시는거 있으세용?",
          "speaker_id": "1",
          "time": "20210826 03:16"
        },
        {
          "id": "MDRW2100015936.1.10",
          "form": "저는 넷플 왓챠 번갈아가면서 봐용",
          "original_form": "저는 넷플 왓챠 번갈아가면서 봐용",
          "speaker_id": "1",
          "time": "20210826 03:16"
        },
        {
          "id": "MDRW2100015936.1.11",
          "form": "마자요 ㅋㅋㅋ 광고가 넘 많아가지구ㅠ",
          "original_form": "마자요 ㅋㅋㅋ 광고가 넘 많아가지구ㅠ",
          "speaker_id": "2",
          "time": "20210826 03:18"
        },
        {
          "id": "MDRW2100015936.1.12",
          "form": "앗 저는 돈은 나가는데 딱히 뭐 보는건 없어요 ㅋㅋㅋㅋㅋ",
          "original_form": "앗 저는 돈은 나가는데 딱히 뭐 보는건 없어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210826 03:19"
        },
        {
          "id": "MDRW2100015936.1.13",
          "form": "아항ㅋㅋㅋ",
          "original_form": "아항ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210826 03:19"
        },
        {
          "id": "MDRW2100015936.1.14",
          "form": "볼만한거 추천 가능하신가용",
          "original_form": "볼만한거 추천 가능하신가용",
          "speaker_id": "2",
          "time": "20210826 03:19"
        },
        {
          "id": "MDRW2100015936.1.15",
          "form": "넷플릭스 킹덤 이거 진짜 재밌아요!",
          "original_form": "넷플릭스 킹덤 이거 진짜 재밌아요!",
          "speaker_id": "1",
          "time": "20210826 03:19"
        },
        {
          "id": "MDRW2100015936.1.16",
          "form": "그리고 내가 사랑한 모든 남자들에게 이것도!",
          "original_form": "그리고 내가 사랑한 모든 남자들에게 이것도!",
          "speaker_id": "1",
          "time": "20210826 03:19"
        },
        {
          "id": "MDRW2100015936.1.17",
          "form": "앗!! 킹덤은 안그래도 꼭 한번 보려고 했어요!!",
          "original_form": "앗!! 킹덤은 안그래도 꼭 한번 보려고 했어요!!",
          "speaker_id": "2",
          "time": "20210826 03:22"
        },
        {
          "id": "MDRW2100015936.1.18",
          "form": "아직 결말 안나온거 맞죠?",
          "original_form": "아직 결말 안나온거 맞죠?",
          "speaker_id": "2",
          "time": "20210826 03:22"
        },
        {
          "id": "MDRW2100015936.1.19",
          "form": "그런데 좀 잔인해요ㅜ",
          "original_form": "그런데 좀 잔인해요ㅜ",
          "speaker_id": "1",
          "time": "20210826 03:22"
        },
        {
          "id": "MDRW2100015936.1.20",
          "form": "넹넹",
          "original_form": "넹넹",
          "speaker_id": "1",
          "time": "20210826 03:22"
        },
        {
          "id": "MDRW2100015936.1.21",
          "form": "오 밑에건 로맨스 영화인가봐요?",
          "original_form": "오 밑에건 로맨스 영화인가봐요?",
          "speaker_id": "2",
          "time": "20210826 03:23"
        },
        {
          "id": "MDRW2100015936.1.22",
          "form": "찜해두고왔어요",
          "original_form": "찜해두고왔어요",
          "speaker_id": "2",
          "time": "20210826 03:23"
        },
        {
          "id": "MDRW2100015936.1.23",
          "form": "추천감사해용 ㅎㅎ",
          "original_form": "추천감사해용 ㅎㅎ",
          "speaker_id": "2",
