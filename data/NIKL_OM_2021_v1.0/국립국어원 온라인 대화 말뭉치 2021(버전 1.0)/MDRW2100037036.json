      "utterance": [
        {
          "id": "MDRW2100037036.1.1",
          "form": "학교생활하실때 왠지 동아리도 여러가지하셨을것같아요!",
          "original_form": "학교생활하실때 왠지 동아리도 여러가지하셨을것같아요!",
          "speaker_id": "1",
          "time": "20210831 17:46"
        },
        {
          "id": "MDRW2100037036.1.2",
          "form": "동아리는 하나!",
          "original_form": "동아리는 하나!",
          "speaker_id": "2",
          "time": "20210831 17:51"
        },
        {
          "id": "MDRW2100037036.1.3",
          "form": "영상동아리 활동했어요",
          "original_form": "영상동아리 활동했어요",
          "speaker_id": "2",
          "time": "20210831 17:51"
        },
        {
          "id": "MDRW2100037036.1.4",
          "form": "name1님은요?",
          "original_form": "&name1&님은요?",
          "speaker_id": "2",
          "time": "20210831 17:52"
        },
        {
          "id": "MDRW2100037036.1.5",
          "form": "오 영상동아리 궁금해요",
          "original_form": "오 영상동아리 궁금해요",
          "speaker_id": "1",
          "time": "20210831 17:52"
        },
        {
          "id": "MDRW2100037036.1.6",
          "form": "전 독서토론..재미없는곳이었죠",
          "original_form": "전 독서토론..재미없는곳이었죠",
          "speaker_id": "1",
          "time": "20210831 17:52"
        },
        {
          "id": "MDRW2100037036.1.7",
          "form": "진짜 토론해요?",
          "original_form": "진짜 토론해요?",
          "speaker_id": "2",
          "time": "20210831 17:58"
        },
        {
          "id": "MDRW2100037036.1.8",
          "form": "네..정말로 하더라구요..",
          "original_form": "네..정말로 하더라구요..",
          "speaker_id": "1",
          "time": "20210831 17:58"
        },
        {
          "id": "MDRW2100037036.1.9",
          "form": "한학기하고 뛰쳐나왔어요",
          "original_form": "한학기하고 뛰쳐나왔어요",
          "speaker_id": "1",
          "time": "20210831 17:58"
        },
        {
          "id": "MDRW2100037036.1.10",
          "form": "한학기씩이나 대단쓰",
          "original_form": "한학기씩이나 대단쓰",
          "speaker_id": "2",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100037036.1.11",
          "form": "막 기한까지 지정된 책읽고 주제놓고 토론하는 그거맞죠?",
          "original_form": "막 기한까지 지정된 책읽고 주제놓고 토론하는 그거맞죠?",
          "speaker_id": "2",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100037036.1.12",
          "form": "스카이캐슬에 나오는",
          "original_form": "스카이캐슬에 나오는",
          "speaker_id": "2",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100037036.1.13",
          "form": "네..맞아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "네..맞아요 ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100037036.1.14",
          "form": "그 지정된책들도 어찌나..",
          "original_form": "그 지정된책들도 어찌나..",
          "speaker_id": "1",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100037036.1.15",
          "form": "스캐같은지",
          "original_form": "스캐같은지",
          "speaker_id": "1",
          "time": "20210831 18:29"
        },
        {
          "id": "MDRW2100037036.1.16",
          "form": "앜 생각만해도 수면제",
          "original_form": "앜 생각만해도 수면제",
          "speaker_id": "2",
          "time": "20210831 18:31"
        },
        {
          "id": "MDRW2100037036.1.17",
          "form": "영상동아리는 어떤활동하는거에요?",
          "original_form": "영상동아리는 어떤활동하는거에요?",
          "speaker_id": "1",
          "time": "20210831 21:08"
        },
        {
          "id": "MDRW2100037036.1.18",
          "form": "스토리 구상해서 짧은 영상만들어서 출품해요",
          "original_form": "스토리 구상해서 짧은 영상만들어서 출품해요",
          "speaker_id": "2",
          "time": "20210831 21:38"
        },
        {
          "id": "MDRW2100037036.1.19",
          "form": "기본 놀려고있는 동아리지만요",
          "original_form": "기본 놀려고있는 동아리지만요",
          "speaker_id": "2",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100037036.1.20",
          "form": "오 지금세대에 너무잘맞네요",
          "original_form": "오 지금세대에 너무잘맞네요",
          "speaker_id": "1",
          "time": "20210831 21:39"
        },
        {
          "id": "MDRW2100037036.1.21",
          "form": "활동은 오래하셨었나요",
          "original_form": "활동은 오래하셨었나요",
          "speaker_id": "1",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100037036.1.22",
          "form": "저는 예~전에 활동했던거지만요",
          "original_form": "저는 예~전에 활동했던거지만요",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100037036.1.23",
          "form": "대학다니는 동안요",
          "original_form": "대학다니는 동안요",
          "speaker_id": "2",
          "time": "20210831 21:40"
        },
        {
          "id": "MDRW2100037036.1.24",
          "form": "ㅎㅎ오 한학기하고 빤스런했던저와는 다르시군여",
          "original_form": "ㅎㅎ오 한학기하고 빤스런했던저와는 다르시군여",
          "speaker_id": "1",
          "time": "20210831 21:40"
        }
      ]
