        },
        {
          "id": "MDRW2100018341.1.3",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:02"
        },
        {
          "id": "MDRW2100018341.1.4",
          "form": "님",
          "original_form": "님",
          "speaker_id": "1",
          "time": "20210827 00:02"
        },
        {
          "id": "MDRW2100018341.1.5",
          "form": "일대백절대나가시면",
          "original_form": "일대백절대나가시면",
          "speaker_id": "1",
          "time": "20210827 00:02"
        },
        {
          "id": "MDRW2100018341.1.6",
          "form": "안되겠어요",
          "original_form": "안되겠어요",
          "speaker_id": "1",
          "time": "20210827 00:02"
        },
        {
          "id": "MDRW2100018341.1.7",
          "form": "백으로 가셔야겠어요",
          "original_form": "백으로 가셔야겠어요",
          "speaker_id": "1",
          "time": "20210827 00:02"
        },
        {
          "id": "MDRW2100018341.1.8",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:03"
        },
        {
          "id": "MDRW2100018341.1.9",
          "form": "아니 여기서 틀리다니",
          "original_form": "아니 여기서 틀리다니",
          "speaker_id": "2",
          "time": "20210827 00:03"
        },
        {
          "id": "MDRW2100018341.1.10",
          "form": "대단하세요",
          "original_form": "대단하세요",
          "speaker_id": "1",
          "time": "20210827 00:03"
        },
        {
          "id": "MDRW2100018341.1.11",
          "form": "정답은 엄마상어인가요?",
          "original_form": "정답은 엄마상어인가요?",
          "speaker_id": "2",
          "time": "20210827 00:03"
        },
        {
          "id": "MDRW2100018341.1.12",
          "form": "그렇죠ㅋㅋㅋㅋ",
          "original_form": "그렇죠ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:03"
        },
        {
          "id": "MDRW2100018341.1.13",
          "form": "엄마상어세트를 먹었답니다",
          "original_form": "엄마상어세트를 먹었답니다",
          "speaker_id": "1",
          "time": "20210827 00:03"
        },
        {
          "id": "MDRW2100018341.1.14",
          "form": "아니 다시 생각해봐도",
          "original_form": "아니 다시 생각해봐도",
          "speaker_id": "2",
          "time": "20210827 00:04"
        },
        {
          "id": "MDRW2100018341.1.15",
          "form": "엄마 아빠 아기 상어가 따로있는게",
          "original_form": "엄마 아빠 아기 상어가 따로있는게",
          "speaker_id": "2",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100018341.1.16",
          "form": "너무 웃겨요ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "너무 웃겨요ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210827 00:05"
        },
        {
          "id": "MDRW2100018341.1.17",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:06"
        },
        {
          "id": "MDRW2100018341.1.18",
          "form": "사이즈가다르답니다",
          "original_form": "사이즈가다르답니다",
          "speaker_id": "1",
          "time": "20210827 00:06"
        },
        {
          "id": "MDRW2100018341.1.19",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:06"
        },
        {
          "id": "MDRW2100018341.1.20",
          "form": "아 맛이 다른게 아니라 사이즈가 다른거군요",
          "original_form": "아 맛이 다른게 아니라 사이즈가 다른거군요",
          "speaker_id": "2",
          "time": "20210827 00:12"
        },
        {
          "id": "MDRW2100018341.1.21",
          "form": "진짜 버거킹 아이디어 대단하다",
          "original_form": "진짜 버거킹 아이디어 대단하다",
          "speaker_id": "2",
          "time": "20210827 00:13"
        },
        {
          "id": "MDRW2100018341.1.22",
          "form": "통새우 와퍼랑 뭐가 더맛있어요?",
          "original_form": "통새우 와퍼랑 뭐가 더맛있어요?",
          "speaker_id": "2",
          "time": "20210827 00:13"
        },
        {
          "id": "MDRW2100018341.1.23",
          "form": "ㅋㅋㅋㅋ 맞아요",
          "original_form": "ㅋㅋㅋㅋ 맞아요",
          "speaker_id": "1",
          "time": "20210827 00:26"
        },
        {
          "id": "MDRW2100018341.1.24",
          "form": "맛도다르답니다ㅋㅋㅋㅋㅋㅋ",
          "original_form": "맛도다르답니다ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:26"
        },
        {
          "id": "MDRW2100018341.1.25",
          "form": "으음...",
          "original_form": "으음...",
          "speaker_id": "1",
          "time": "20210827 00:26"
        },
        {
          "id": "MDRW2100018341.1.26",
          "form": "전 통새우와퍼가 나은거같아요",
          "original_form": "전 통새우와퍼가 나은거같아요",
          "speaker_id": "1",
          "time": "20210827 00:26"
        },
        {
          "id": "MDRW2100018341.1.27",
          "form": "오 통새우 와퍼가  맛있군요",
          "original_form": "오 통새우 와퍼가  맛있군요",
          "speaker_id": "2",
          "time": "20210827 00:29"
        },
        {
          "id": "MDRW2100018341.1.28",
          "form": "통새우는 매콤한소슨데  패밀리상어는 안매운소슨가요?",
          "original_form": "통새우는 매콤한소슨데  패밀리상어는 안매운소슨가요?",
          "speaker_id": "2",
          "time": "20210827 00:30"
        },
        {
          "id": "MDRW2100018341.1.29",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:39"
        },
        {
          "id": "MDRW2100018341.1.30",
          "form": "님 맞추는데는",
          "original_form": "님 맞추는데는",
          "speaker_id": "1",
          "time": "20210827 00:39"
        },
        {
          "id": "MDRW2100018341.1.31",
          "form": "영 소실이없으신거같아요",
          "original_form": "영 소실이없으신거같아요",
          "speaker_id": "1",
          "time": "20210827 00:39"
        },
        {
          "id": "MDRW2100018341.1.32",
          "form": "패밀리도 매운소스에요?",
          "original_form": "패밀리도 매운소스에요?",
          "speaker_id": "2",
          "time": "20210827 00:48"
        },
        {
          "id": "MDRW2100018341.1.33",
          "form": "ㅋㅋㅋㅋㅋㅋ아니 오늘따라 촉이 구리네요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ아니 오늘따라 촉이 구리네요",
          "speaker_id": "2",
          "time": "20210827 00:48"
        },
        {
          "id": "MDRW2100018341.1.34",
          "form": "그럼 패티가 다른거군요 신기하네",
          "original_form": "그럼 패티가 다른거군요 신기하네",
          "speaker_id": "2",
          "time": "20210827 00:49"
        },
        {
          "id": "MDRW2100018341.1.35",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210827 00:51"
        },
        {
          "id": "MDRW2100018341.1.36",
          "form": "아기냐엄마냐",
          "original_form": "아기냐엄마냐",
          "speaker_id": "1",
          "time": "20210827 00:51"
        },
        {
          "id": "MDRW2100018341.1.37",
          "form": "아빠냐에따라",
          "original_form": "아빠냐에따라",
          "speaker_id": "1",
          "time": "20210827 00:51"
        },
        {
          "id": "MDRW2100018341.1.38",
