      "utterance": [
        {
          "id": "MDRW2100025365.1.1",
          "form": "집안일해요?",
          "original_form": "집안일해요?",
          "speaker_id": "1",
          "time": "20210901 10:13"
        },
        {
          "id": "MDRW2100025365.1.2",
          "form": "그럼요! 매일 가사노동을 합니다.",
          "original_form": "그럼요! 매일 가사노동을 합니다.",
          "speaker_id": "2",
          "time": "20210901 10:17"
        },
        {
          "id": "MDRW2100025365.1.3",
          "form": "전 청소담당이요",
          "original_form": "전 청소담당이요",
          "speaker_id": "1",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100025365.1.4",
          "form": "저는 요리해요. 청소는 적성에 안맞아서요",
          "original_form": "저는 요리해요. 청소는 적성에 안맞아서요",
          "speaker_id": "2",
          "time": "20210901 10:19"
        },
        {
          "id": "MDRW2100025365.1.5",
          "form": "화장실청소가 가장 힘들죠",
          "original_form": "화장실청소가 가장 힘들죠",
          "speaker_id": "1",
          "time": "20210901 10:20"
        },
        {
          "id": "MDRW2100025365.1.6",
          "form": "맞아요. 청소는 하면 티안나고 안하면 티나고요.",
          "original_form": "맞아요. 청소는 하면 티안나고 안하면 티나고요.",
          "speaker_id": "2",
          "time": "20210901 10:21"
        },
        {
          "id": "MDRW2100025365.1.7",
          "form": "락스냄새싫어요",
          "original_form": "락스냄새싫어요",
          "speaker_id": "1",
          "time": "20210901 10:22"
        },
        {
          "id": "MDRW2100025365.1.8",
          "form": "요즘은 청소도구가 다양해서 장비빨도 커요",
          "original_form": "요즘은 청소도구가 다양해서 장비빨도 커요",
          "speaker_id": "2",
          "time": "20210901 10:23"
        },
        {
          "id": "MDRW2100025365.1.9",
          "form": "편리하져",
          "original_form": "편리하져",
          "speaker_id": "1",
          "time": "20210901 10:24"
        },
        {
          "id": "MDRW2100025365.1.10",
          "form": "주방일도 장비가 중요해요. 식기세척기 사고싶어요.",
          "original_form": "주방일도 장비가 중요해요. 식기세척기 사고싶어요.",
          "speaker_id": "2",
          "time": "20210901 10:25"
        },
        {
          "id": "MDRW2100025365.1.11",
          "form": "것고 좋죠",
          "original_form": "것고 좋죠",
          "speaker_id": "1",
          "time": "20210901 10:26"
        },
        {
          "id": "MDRW2100025365.1.12",
          "form": "청소할때 뜨거운물 사용은 안좋다고합니다",
          "original_form": "청소할때 뜨거운물 사용은 안좋다고합니다",
          "speaker_id": "2",
          "time": "20210901 10:28"
        },
