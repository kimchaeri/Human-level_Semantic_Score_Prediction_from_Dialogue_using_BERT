      "utterance": [
        {
          "id": "MDRW2100038131.1.1",
          "form": "계속 앉아있으니까",
          "original_form": "계속 앉아있으니까",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100038131.1.2",
          "form": "근육도 다 빠지고",
          "original_form": "근육도 다 빠지고",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100038131.1.3",
          "form": "아이고 약대편입 너무힘드셨었겠다ㅜㅠ",
          "original_form": "아이고 약대편입 너무힘드셨었겠다ㅜㅠ",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100038131.1.4",
          "form": "배가 그렇게 나오더라구요",
          "original_form": "배가 그렇게 나오더라구요",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100038131.1.5",
          "form": "맞아요.. 결과는 안좋았지만 ... 껄껄",
          "original_form": "맞아요.. 결과는 안좋았지만 ... 껄껄",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100038131.1.6",
          "form": "맞아요ㅜㅜ목도 일자목에서 거북목을얻구여",
          "original_form": "맞아요ㅜㅜ목도 일자목에서 거북목을얻구여",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100038131.1.7",
          "form": "앗 저도일자목이에요!!!",
          "original_form": "앗 저도일자목이에요!!!",
          "speaker_id": "2",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100038131.1.8",
          "form": "예..전 거북목이 되어부렸어요",
          "original_form": "예..전 거북목이 되어부렸어요",
          "speaker_id": "1",
          "time": "20210901 23:31"
        },
        {
          "id": "MDRW2100038131.1.9",
          "form": "으악 진짜 name1님도",
          "original_form": "으악 진짜 &name1&님도",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100038131.1.10",
          "form": "고생 많이 하셨어요",
          "original_form": "고생 많이 하셨어요",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100038131.1.11",
          "form": "서있으면 목은나오고 허리는 구부정하고",
          "original_form": "서있으면 목은나오고 허리는 구부정하고",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100038131.1.12",
          "form": "도수치료 받아보셨나요???",
          "original_form": "도수치료 받아보셨나요???",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100038131.1.13",
          "form": "네에 근데 가기가 힘들고 그래서",
          "original_form": "네에 근데 가기가 힘들고 그래서",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100038131.1.14",
          "form": "몇번받고 안갔어요",
          "original_form": "몇번받고 안갔어요",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100038131.1.15",
          "form": "아 저도 두 번 다녀왔는데",
          "original_form": "아 저도 두 번 다녀왔는데",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100038131.1.16",
          "form": "한 10번 받아여",
          "original_form": "한 10번 받아여",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100038131.1.17",
          "form": "받아야",
          "original_form": "받아야",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100038131.1.18",
          "form": "효과 보일락 말락 한다더라구요",
          "original_form": "효과 보일락 말락 한다더라구요",
          "speaker_id": "2",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100038131.1.19",
          "form": "ㄱ마자요",
          "original_form": "ㄱ마자요",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100038131.1.20",
          "form": "저도 그정도까지안받아서그런가...",
          "original_form": "저도 그정도까지안받아서그런가...",
          "speaker_id": "1",
          "time": "20210901 23:32"
        },
        {
          "id": "MDRW2100038131.1.21",
          "form": "영..",
          "original_form": "영..",
          "speaker_id": "1",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100038131.1.22",
          "form": "다 돈이에요 진짜ㅠ",
          "original_form": "다 돈이에요 진짜ㅠ",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100038131.1.23",
          "form": "저는 2번 받았어요... 근데 벌써 귀찮아져서...",
          "original_form": "저는 2번 받았어요... 근데 벌써 귀찮아져서...",
          "speaker_id": "2",
          "time": "20210901 23:33"
        },
        {
          "id": "MDRW2100038131.1.24",
          "form": "운동을하자니 운동도 돈이구여ㅜ",
          "original_form": "운동을하자니 운동도 돈이구여ㅜ",
