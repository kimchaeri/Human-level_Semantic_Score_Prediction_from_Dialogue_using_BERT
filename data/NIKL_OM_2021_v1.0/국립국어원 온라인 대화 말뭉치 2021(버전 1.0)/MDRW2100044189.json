      "utterance": [
        {
          "id": "MDRW2100044189.1.1",
          "form": "저녁 머드셧나용",
          "original_form": "저녁 머드셧나용",
          "speaker_id": "1",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044189.1.2",
          "form": "어묵탕이요",
          "original_form": "어묵탕이요",
          "speaker_id": "2",
          "time": "20210907 21:49"
        },
        {
          "id": "MDRW2100044189.1.3",
          "form": "비도 오고 쓸쓸해서",
          "original_form": "비도 오고 쓸쓸해서",
          "speaker_id": "2",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044189.1.4",
          "form": "크~~# 술도한잔 하셧는지여",
          "original_form": "크~~# 술도한잔 하셧는지여",
          "speaker_id": "1",
          "time": "20210907 21:50"
        },
        {
          "id": "MDRW2100044189.1.5",
          "form": "아 반주는 곁들이지 않았어요",
          "original_form": "아 반주는 곁들이지 않았어요",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044189.1.6",
          "form": "백신 맞은지 얼마 안돼서",
          "original_form": "백신 맞은지 얼마 안돼서",
          "speaker_id": "2",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044189.1.7",
          "form": "ㅋㅋㅋ 오뎅탕 ㅠㅠ",
          "original_form": "ㅋㅋㅋ 오뎅탕 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044189.1.8",
          "form": "아항",
          "original_form": "아항",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044189.1.9",
          "form": "사드신건가요",
          "original_form": "사드신건가요",
          "speaker_id": "1",
          "time": "20210907 21:52"
        },
        {
          "id": "MDRW2100044189.1.10",
          "form": "간단히 집에서 해먹었어요",
          "original_form": "간단히 집에서 해먹었어요",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044189.1.11",
          "form": "어묵이 마침 집에 있어서",
          "original_form": "어묵이 마침 집에 있어서",
          "speaker_id": "2",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044189.1.12",
          "form": "아하 물넣고 끓임되나요?",
          "original_form": "아하 물넣고 끓임되나요?",
          "speaker_id": "1",
          "time": "20210907 21:53"
        },
        {
          "id": "MDRW2100044189.1.13",
          "form": "무랑 파 있음 넣고요",
          "original_form": "무랑 파 있음 넣고요",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044189.1.14",
          "form": "물 간장 넣어요",
          "original_form": "물 간장 넣어요",
          "speaker_id": "2",
          "time": "20210907 21:54"
        },
        {
          "id": "MDRW2100044189.1.15",
          "form": "오호 요리잘하시나요",
          "original_form": "오호 요리잘하시나요",
          "speaker_id": "1",
          "time": "20210907 21:55"
        },
        {
          "id": "MDRW2100044189.1.16",
          "form": "그냥 맛있게 먹는 걸 좋아해요",
          "original_form": "그냥 맛있게 먹는 걸 좋아해요",
          "speaker_id": "2",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044189.1.17",
          "form": "아항 그런분들이 잘하시더라구용 ㅋㅋ",
          "original_form": "아항 그런분들이 잘하시더라구용 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:56"
        },
        {
          "id": "MDRW2100044189.1.18",
          "form": "ㅋㅋ그냥 맛연구가 입니다",
          "original_form": "ㅋㅋ그냥 맛연구가 입니다",
          "speaker_id": "2",
          "time": "20210907 21:57"
        },
        {
          "id": "MDRW2100044189.1.19",
          "form": "ㅋㅋㅋㅋㅋ 오늘또 머드셧나요",
          "original_form": "ㅋㅋㅋㅋㅋ 오늘또 머드셧나요",
          "speaker_id": "1",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044189.1.20",
          "form": "아 점심은 직장  구내식당에서 먹어서",
          "original_form": "아 점심은 직장  구내식당에서 먹어서",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044189.1.21",
          "form": "아침엔 코울슬로에 식빵 먹었고요",
          "original_form": "아침엔 코울슬로에 식빵 먹었고요",
          "speaker_id": "2",
          "time": "20210907 21:58"
        },
        {
          "id": "MDRW2100044189.1.22",
          "form": "이번 코울슬로가 역대급으로 맛있게 돼서",
          "original_form": "이번 코울슬로가 역대급으로 맛있게 돼서",
          "speaker_id": "2",
          "time": "20210907 21:59"
        },
