      "utterance": [
        {
          "id": "MDRW2100028981.1.1",
          "form": "갑자기 퍼스널컬러요?",
          "original_form": "갑자기 퍼스널컬러요?",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028981.1.2",
          "form": "근데 제 톤 맨날 바뀜",
          "original_form": "근데 제 톤 맨날 바뀜",
          "speaker_id": "1",
          "time": "20210902 23:11"
        },
        {
          "id": "MDRW2100028981.1.3",
          "form": "아니 글구 화장도 다 돈이에요ㅜㅜ",
          "original_form": "아니 글구 화장도 다 돈이에요ㅜㅜ",
          "speaker_id": "1",
          "time": "20210902 23:13"
        },
        {
          "id": "MDRW2100028981.1.4",
          "form": "많이들  퍼스널컬러 알려고들 서비스?신청하더라고요",
          "original_form": "많이들  퍼스널컬러 알려고들 서비스?신청하더라고요",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028981.1.5",
          "form": "어떤사람은 메이크업코치 30만원에함",
          "original_form": "어떤사람은 메이크업코치 30만원에함",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028981.1.6",
          "form": "아주만족한다고 추천하는데ㅋㅋ",
          "original_form": "아주만족한다고 추천하는데ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028981.1.7",
          "form": "걍 그렇구나...ㅋㅋ",
          "original_form": "걍 그렇구나...ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 23:15"
        },
        {
          "id": "MDRW2100028981.1.8",
          "form": "헐 비싸다",
          "original_form": "헐 비싸다",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100028981.1.9",
          "form": "걍 저는 그전도까진...?? ㅋㅋㅋㅋ 어차피 맨날 하는 화장 똑같이 함ㅋㅋㅋㅋ",
          "original_form": "걍 저는 그전도까진...?? ㅋㅋㅋㅋ 어차피 맨날 하는 화장 똑같이 함ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100028981.1.10",
          "form": "화장도 기력이에요",
          "original_form": "화장도 기력이에요",
          "speaker_id": "1",
          "time": "20210902 23:23"
        },
        {
          "id": "MDRW2100028981.1.11",
          "form": "그쵸? 엄청 유명한사람인가봐여",
          "original_form": "그쵸? 엄청 유명한사람인가봐여",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100028981.1.12",
          "form": "진짜 태생이 이쁜거 아니고서야",
          "original_form": "진짜 태생이 이쁜거 아니고서야",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100028981.1.13",
          "form": "꾸미기나름인데ㅠ",
          "original_form": "꾸미기나름인데ㅠ",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100028981.1.14",
          "form": "귀차니즘이 많은걸....",
          "original_form": "귀차니즘이 많은걸....",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100028981.1.15",
          "form": "ㅋㅋㅋㅋㅋㅋ안하게함",
          "original_form": "ㅋㅋㅋㅋㅋㅋ안하게함",
          "speaker_id": "2",
          "time": "20210902 23:24"
        },
        {
          "id": "MDRW2100028981.1.16",
          "form": "맞아여 근데 예쁘면",
          "original_form": "맞아여 근데 예쁘면",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100028981.1.17",
          "form": "화장을 안 하겟죠",
          "original_form": "화장을 안 하겟죠",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100028981.1.18",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100028981.1.19",
          "form": "글고 뭐 누구 잘 보일 사람 있어야 화장도 하는거지",
          "original_form": "글고 뭐 누구 잘 보일 사람 있어야 화장도 하는거지",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100028981.1.20",
          "form": "자꾸 그래서 점점 안하데됨",
          "original_form": "자꾸 그래서 점점 안하데됨",
          "speaker_id": "1",
          "time": "20210902 23:33"
        },
        {
          "id": "MDRW2100028981.1.21",
          "form": "ㅋㅋㅋ맞아여",
          "original_form": "ㅋㅋㅋ맞아여",
          "speaker_id": "2",
          "time": "20210902 23:36"
        },
        {
          "id": "MDRW2100028981.1.22",
          "form": "결국자기만족이에요",
          "original_form": "결국자기만족이에요",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100028981.1.23",
          "form": "옷도 이쁜건 너무 비쌈",
          "original_form": "옷도 이쁜건 너무 비쌈",
          "speaker_id": "2",
          "time": "20210902 23:37"
        },
        {
          "id": "MDRW2100028981.1.24",
          "form": "마자 ㅜ",
          "original_form": "마자 ㅜ",
          "speaker_id": "1",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100028981.1.25",
          "form": "저 옷 산지도 꼬ㅑ 됐어요 ㅋㅋㅋㅋㅋ",
          "original_form": "저 옷 산지도 꼬ㅑ 됐어요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100028981.1.26",
          "form": "꽤",
          "original_form": "꽤",
          "speaker_id": "1",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100028981.1.27",
          "form": "유행도 안 맞지만 깨끗하면 되지 하고 ㅋㅋ 입고다님",
          "original_form": "유행도 안 맞지만 깨끗하면 되지 하고 ㅋㅋ 입고다님",
          "speaker_id": "1",
          "time": "20210903 10:04"
        },
        {
          "id": "MDRW2100028981.1.28",
          "form": "저두요ㅋㅋ막 사고싶지않음",
          "original_form": "저두요ㅋㅋ막 사고싶지않음",
          "speaker_id": "2",
          "time": "20210904 18:59"
        },
        {
          "id": "MDRW2100028981.1.29",
          "form": "근데 요즘은 원피스 입기 너무 편해서",
          "original_form": "근데 요즘은 원피스 입기 너무 편해서",
          "speaker_id": "2",
          "time": "20210904 18:59"
        },
        {
          "id": "MDRW2100028981.1.30",
          "form": "좀 사려고요",
          "original_form": "좀 사려고요",
          "speaker_id": "2",
          "time": "20210904 18:59"
        },
        {
          "id": "MDRW2100028981.1.31",
          "form": "원피스 짱이져",
          "original_form": "원피스 짱이져",
          "speaker_id": "1",
          "time": "20210904 19:07"
        },
        {
          "id": "MDRW2100028981.1.32",
          "form": "저 여름엔.원피스만 입음 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "저 여름엔.원피스만 입음 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 19:08"
        },
        {
          "id": "MDRW2100028981.1.33",
          "form": "저도 오늘 가을원피스 사려고 좀 뵛는데 맘에 드는 게 없어요ㅠ",
          "original_form": "저도 오늘 가을원피스 사려고 좀 뵛는데 맘에 드는 게 없어요ㅠ",
          "speaker_id": "1",
          "time": "20210904 19:08"
        },
        {
          "id": "MDRW2100028981.1.34",
          "form": "맞아여 긴원피스 이쁜거 찾아봐야죠ㅋㅋㅋ",
          "original_form": "맞아여 긴원피스 이쁜거 찾아봐야죠ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 20:54"
        },
        {
          "id": "MDRW2100028981.1.35",
          "form": "이젠 긴팔찾아야함",
          "original_form": "이젠 긴팔찾아야함",
          "speaker_id": "2",
          "time": "20210904 20:54"
        },
        {
          "id": "MDRW2100028981.1.36",
          "form": "추워여",
          "original_form": "추워여",
          "speaker_id": "2",
          "time": "20210904 20:54"
        },
        {
          "id": "MDRW2100028981.1.37",
          "form": "저 긴 팔 입구 다닌지 이미 모래됨ㅋㅋㅋㅋ",
          "original_form": "저 긴 팔 입구 다닌지 이미 모래됨ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 12:51"
        },
        {
          "id": "MDRW2100028981.1.38",
          "form": "지지난주부터 입었어여",
          "original_form": "지지난주부터 입었어여",
          "speaker_id": "1",
          "time": "20210905 12:51"
        },
        {
          "id": "MDRW2100028981.1.39",
          "form": "아 진짜여?ㅋㅋㅋ전이직 시작도 안함",
          "original_form": "아 진짜여?ㅋㅋㅋ전이직 시작도 안함",
          "speaker_id": "2",
          "time": "20210905 13:28"
        },
        {
          "id": "MDRW2100028981.1.40",
          "form": "네 ㅋㅋ 아침저녁엔 춥지않아요?",
          "original_form": "네 ㅋㅋ 아침저녁엔 춥지않아요?",
          "speaker_id": "1",
          "time": "20210905 13:36"
        },
        {
          "id": "MDRW2100028981.1.41",
          "form": "이젠그래요 이불없이 못살아여",
          "original_form": "이젠그래요 이불없이 못살아여",
          "speaker_id": "2",
          "time": "20210905 13:49"
        },
        {
          "id": "MDRW2100028981.1.42",
          "form": "그쵸 ㅋㅋㅋ",
          "original_form": "그쵸 ㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210905 14:26"
        },
        {
          "id": "MDRW2100028981.1.43",
          "form": "이불 봄여름용으로 바꿔야할듯",
          "original_form": "이불 봄여름용으로 바꿔야할듯",
