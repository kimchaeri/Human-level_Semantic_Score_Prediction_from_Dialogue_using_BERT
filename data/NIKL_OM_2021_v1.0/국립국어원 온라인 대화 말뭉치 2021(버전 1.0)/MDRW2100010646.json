      "utterance": [
        {
          "id": "MDRW2100010646.1.1",
          "form": "확실히 날이 갈수록 사람들이 직접 요리를 안 해서",
          "original_form": "확실히 날이 갈수록 사람들이 직접 요리를 안 해서",
          "speaker_id": "1",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010646.1.2",
          "form": "배달음식 주문하거나 밀키트로 간단하게 조리하는 것 같아요",
          "original_form": "배달음식 주문하거나 밀키트로 간단하게 조리하는 것 같아요",
          "speaker_id": "1",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010646.1.3",
          "form": "얼마전 신림백순대 사봤는데",
          "original_form": "얼마전 신림백순대 사봤는데",
          "speaker_id": "2",
          "time": "20210823 20:16"
        },
        {
          "id": "MDRW2100010646.1.4",
          "form": "신림에서 먹는거랑 똑같더라구요ㅎ",
          "original_form": "신림에서 먹는거랑 똑같더라구요ㅎ",
          "speaker_id": "2",
          "time": "20210823 20:17"
        },
        {
          "id": "MDRW2100010646.1.5",
          "form": "우와 신기하네요",
          "original_form": "우와 신기하네요",
          "speaker_id": "1",
          "time": "20210824 00:22"
        },
        {
          "id": "MDRW2100010646.1.6",
          "form": "순대ㅠ 맛있겠어요",
          "original_form": "순대ㅠ 맛있겠어요",
          "speaker_id": "1",
          "time": "20210824 00:22"
        },
        {
          "id": "MDRW2100010646.1.7",
          "form": "백순대 드셔보셨어요?",
          "original_form": "백순대 드셔보셨어요?",
          "speaker_id": "2",
          "time": "20210824 00:25"
        },
        {
          "id": "MDRW2100010646.1.8",
          "form": "옛날에 급식 먹을 때",
          "original_form": "옛날에 급식 먹을 때",
          "speaker_id": "1",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100010646.1.9",
          "form": "백순대 들어간 순댓국은 먹어봣어요",
          "original_form": "백순대 들어간 순댓국은 먹어봣어요",
          "speaker_id": "1",
          "time": "20210824 16:52"
        },
        {
          "id": "MDRW2100010646.1.10",
          "form": "순대볶음인데 양념을 하얗게한거거든요",
          "original_form": "순대볶음인데 양념을 하얗게한거거든요",
          "speaker_id": "2",
          "time": "20210824 16:54"
        },
        {
          "id": "MDRW2100010646.1.11",
          "form": "아 순대볶음이에요??",
          "original_form": "아 순대볶음이에요??",
          "speaker_id": "1",
          "time": "20210825 15:41"
        },
        {
          "id": "MDRW2100010646.1.12",
          "form": "네네 신림백순대 유명해요",
          "original_form": "네네 신림백순대 유명해요",
          "speaker_id": "2",
          "time": "20210825 15:51"
        },
        {
          "id": "MDRW2100010646.1.13",
          "form": "저는 그냥 색깔이 하얀 순대인 줄 알았어요",
          "original_form": "저는 그냥 색깔이 하얀 순대인 줄 알았어요",
          "speaker_id": "1",
          "time": "20210825 16:14"
        },
        {
          "id": "MDRW2100010646.1.14",
          "form": "아 ㅎ 백순대 검색해보심 아! 하실듯요",
          "original_form": "아 ㅎ 백순대 검색해보심 아! 하실듯요",
          "speaker_id": "2",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100010646.1.15",
          "form": "찍어먹는 장이 있어서 깻잎에 싸면",
          "original_form": "찍어먹는 장이 있어서 깻잎에 싸면",
          "speaker_id": "2",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100010646.1.16",
          "form": "맛나요ㅎ",
          "original_form": "맛나요ㅎ",
          "speaker_id": "2",
          "time": "20210825 16:15"
        },
        {
          "id": "MDRW2100010646.1.17",
          "form": "아ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "아ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210825 16:16"
        },
        {
          "id": "MDRW2100010646.1.18",
          "form": "방금 검색하고 왔는데",
          "original_form": "방금 검색하고 왔는데",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100010646.1.19",
          "form": "익숙학 비주얼이네요",
          "original_form": "익숙학 비주얼이네요",
          "speaker_id": "1",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100010646.1.20",
          "form": "네네 양념도 맛나지만 백순대도 나름 매력이ㅎ",
          "original_form": "네네 양념도 맛나지만 백순대도 나름 매력이ㅎ",
          "speaker_id": "2",
          "time": "20210825 16:17"
        },
        {
          "id": "MDRW2100010646.1.21",
          "form": "맛있겠네요 ㅎ",
          "original_form": "맛있겠네요 ㅎ",
          "speaker_id": "1",
          "time": "20210825 16:53"
        }
      ]
