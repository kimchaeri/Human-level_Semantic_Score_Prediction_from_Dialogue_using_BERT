      "utterance": [
        {
          "id": "MDRW2100015044.1.1",
          "form": "고양이 키우고싶다",
          "original_form": "고양이 키우고싶다",
          "speaker_id": "1",
          "time": "20210826 13:55"
        },
        {
          "id": "MDRW2100015044.1.2",
          "form": "너도 그러지",
          "original_form": "너도 그러지",
          "speaker_id": "1",
          "time": "20210826 13:56"
        },
        {
          "id": "MDRW2100015044.1.3",
          "form": "응 근데 관리 힘들어서 보는게 좋아",
          "original_form": "응 근데 관리 힘들어서 보는게 좋아",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100015044.1.4",
          "form": "난 넘 사랑스러워서 좋던데",
          "original_form": "난 넘 사랑스러워서 좋던데",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100015044.1.5",
          "form": "그정도 수고는 할 수 있다",
          "original_form": "그정도 수고는 할 수 있다",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100015044.1.6",
          "form": "그건그래 근데 난 못할듯",
          "original_form": "그건그래 근데 난 못할듯",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100015044.1.7",
          "form": "돈도 많이 들잖아",
          "original_form": "돈도 많이 들잖아",
          "speaker_id": "2",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100015044.1.8",
          "form": "넌 넘 삭막해ㅜㅜ",
          "original_form": "넌 넘 삭막해ㅜㅜ",
          "speaker_id": "1",
          "time": "20210826 13:57"
        },
        {
          "id": "MDRW2100015044.1.9",
          "form": "사랑으로 키우는 거지",
          "original_form": "사랑으로 키우는 거지",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100015044.1.10",
          "form": "사랑은 무슨 돈으로 키우는거지",
          "original_form": "사랑은 무슨 돈으로 키우는거지",
          "speaker_id": "2",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100015044.1.11",
          "form": "하지만 사랑도 필수품이야",
          "original_form": "하지만 사랑도 필수품이야",
          "speaker_id": "2",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100015044.1.12",
          "form": "너는 진짜 붕노유발자야",
          "original_form": "너는 진짜 붕노유발자야",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100015044.1.13",
          "form": "분노!분노!",
          "original_form": "분노!분노!",
          "speaker_id": "2",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100015044.1.14",
          "form": "아 짜증나 니",
          "original_form": "아 짜증나 니",
          "speaker_id": "1",
          "time": "20210826 13:58"
        },
        {
          "id": "MDRW2100015044.1.15",
          "form": "응 그치만 고양이는 귀여웡",
          "original_form": "응 그치만 고양이는 귀여웡",
          "speaker_id": "2",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100015044.1.16",
          "form": "내 친구 강아지들 졸귀더라",
          "original_form": "내 친구 강아지들 졸귀더라",
          "speaker_id": "1",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100015044.1.17",
          "form": "내친구 강아지는 무섭더라",
          "original_form": "내친구 강아지는 무섭더라",
          "speaker_id": "2",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100015044.1.18",
          "form": "포멘데 둘다 넘 귀여움",
          "original_form": "포멘데 둘다 넘 귀여움",
          "speaker_id": "1",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100015044.1.19",
          "form": "왜 어케 무서운데",
          "original_form": "왜 어케 무서운데",
          "speaker_id": "1",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100015044.1.20",
          "form": "걔는 말티쥬인데 애가 저돌적이야",
          "original_form": "걔는 말티쥬인데 애가 저돌적이야",
          "speaker_id": "2",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100015044.1.21",
          "form": "너무 다가와 퍼스널 스페이스 제로",
          "original_form": "너무 다가와 퍼스널 스페이스 제로",
          "speaker_id": "2",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100015044.1.22",
          "form": "말티즈는 좀 무섭긴해",
          "original_form": "말티즈는 좀 무섭긴해",
          "speaker_id": "1",
          "time": "20210826 13:59"
        },
        {
          "id": "MDRW2100015044.1.23",
          "form": "멍멍이한테 그런게 어딧냐",
          "original_form": "멍멍이한테 그런게 어딧냐",
          "speaker_id": "1",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100015044.1.24",
          "form": "왜 얌전하면 귀여워 포메도 무서워",
          "original_form": "왜 얌전하면 귀여워 포메도 무서워",
          "speaker_id": "2",
          "time": "20210826 14:00"
        },
        {
          "id": "MDRW2100015044.1.25",
          "form": "쪼그만데 싸나워",
          "original_form": "쪼그만데 싸나워",
          "speaker_id": "2",
          "time": "20210826 14:00"
        }
      ]
