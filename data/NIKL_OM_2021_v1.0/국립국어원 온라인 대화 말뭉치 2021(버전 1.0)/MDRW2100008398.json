        },
        {
          "id": "MDRW2100008398.1.2",
          "form": "너무 마른 사람들 천지예요",
          "original_form": "너무 마른 사람들 천지예요",
          "speaker_id": "2",
          "time": "20210823 23:28"
        },
        {
          "id": "MDRW2100008398.1.3",
          "form": "네 그래서 다욧 용품이 불티나요",
          "original_form": "네 그래서 다욧 용품이 불티나요",
          "speaker_id": "1",
          "time": "20210823 23:28"
        },
        {
          "id": "MDRW2100008398.1.4",
          "form": "저도 샀어요 ㅋㅋ",
          "original_form": "저도 샀어요 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100008398.1.5",
          "form": "주제가 쇼핑이였네요? ㅎ",
          "original_form": "주제가 쇼핑이였네요? ㅎ",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100008398.1.6",
          "form": "grn 맨날사서 먹어요",
          "original_form": "grn 맨날사서 먹어요",
          "speaker_id": "1",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100008398.1.7",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100008398.1.8",
          "form": "그거 효과있어요?",
          "original_form": "그거 효과있어요?",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100008398.1.9",
          "form": "다욧약 ㅋㅋ",
          "original_form": "다욧약 ㅋㅋ",
          "speaker_id": "1",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100008398.1.10",
          "form": "아뇨",
          "original_form": "아뇨",
          "speaker_id": "1",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100008398.1.11",
          "form": "자기 위안이죠",
          "original_form": "자기 위안이죠",
          "speaker_id": "1",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100008398.1.12",
          "form": "허걱 ㅎ",
          "original_form": "허걱 ㅎ",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100008398.1.13",
          "form": "먹기위해 사서드시는걸로? ㅎㅎ",
          "original_form": "먹기위해 사서드시는걸로? ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 23:29"
        },
        {
          "id": "MDRW2100008398.1.14",
          "form": "네 그렇죠",
          "original_form": "네 그렇죠",
          "speaker_id": "1",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100008398.1.15",
          "form": "유산균은 변비엔좋아요",
          "original_form": "유산균은 변비엔좋아요",
          "speaker_id": "1",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100008398.1.16",
          "form": "저는 예전에 다이어트 환사서먹었어요",
          "original_form": "저는 예전에 다이어트 환사서먹었어요",
          "speaker_id": "2",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100008398.1.17",
          "form": "없음 ㅜㅜ변비에 죽어요",
          "original_form": "없음 ㅜㅜ변비에 죽어요",
          "speaker_id": "1",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100008398.1.18",
          "form": "네",
          "original_form": "네",
          "speaker_id": "1",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100008398.1.19",
          "form": "진짜 입맛은 없어져요",
          "original_form": "진짜 입맛은 없어져요",
          "speaker_id": "2",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100008398.1.20",
          "form": "효과는요?",
          "original_form": "효과는요?",
          "speaker_id": "1",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100008398.1.21",
          "form": "아 효과 있네요.",
          "original_form": "아 효과 있네요.",
          "speaker_id": "1",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100008398.1.22",
          "form": "네 효과는 있더라구요",
          "original_form": "네 효과는 있더라구요",
          "speaker_id": "2",
          "time": "20210823 23:30"
        },
        {
          "id": "MDRW2100008398.1.23",
          "form": "디톡스환이라고 같이 주던데",
          "original_form": "디톡스환이라고 같이 주던데",
          "speaker_id": "2",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100008398.1.24",
          "form": "아 근데 끊으면 쪄요",
          "original_form": "아 근데 끊으면 쪄요",
          "speaker_id": "1",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100008398.1.25",
          "form": "그거먹고는 배가 장이 꼬이는듯이 아프더라구요 ㅜㅠ",
          "original_form": "그거먹고는 배가 장이 꼬이는듯이 아프더라구요 ㅜㅠ",
          "speaker_id": "2",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100008398.1.26",
          "form": "아 그럼 설사약이네요 아휴",
          "original_form": "아 그럼 설사약이네요 아휴",
          "speaker_id": "1",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100008398.1.27",
          "form": "신랑이 그거보고는 먹지마라고 ㅜㅠ 머라하더라구요",
          "original_form": "신랑이 그거보고는 먹지마라고 ㅜㅠ 머라하더라구요",
          "speaker_id": "2",
          "time": "20210823 23:31"
        },
        {
          "id": "MDRW2100008398.1.28",
          "form": "맞아요 끊으면 다시 돌아와요 ㅎㅎ",
          "original_form": "맞아요 끊으면 다시 돌아와요 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100008398.1.29",
          "form": "먹음 안돼요 클나요",
          "original_form": "먹음 안돼요 클나요",
          "speaker_id": "1",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100008398.1.30",
          "form": "소용없어요",
          "original_form": "소용없어요",
          "speaker_id": "1",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100008398.1.31",
          "form": "그냥 정기적으로 구매해요",
          "original_form": "그냥 정기적으로 구매해요",
          "speaker_id": "1",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100008398.1.32",
          "form": "변비땜시",
          "original_form": "변비땜시",
          "speaker_id": "1",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100008398.1.33",
          "form": "GRN이요?",
          "original_form": "GRN이요?",
          "speaker_id": "2",
          "time": "20210823 23:32"
        },
        {
          "id": "MDRW2100008398.1.34",
          "form": "저는 유산균 락토핏 먹고있어요",
          "original_form": "저는 유산균 락토핏 먹고있어요",
