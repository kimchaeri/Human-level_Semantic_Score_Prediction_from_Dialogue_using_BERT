      "utterance": [
        {
          "id": "MDRW2100046215.1.1",
          "form": "안녕하세요",
          "original_form": "안녕하세요",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046215.1.2",
          "form": "점심 뭐 드실 예정이신가요?",
          "original_form": "점심 뭐 드실 예정이신가요?",
          "speaker_id": "1",
          "time": "20210908 11:16"
        },
        {
          "id": "MDRW2100046215.1.3",
          "form": "제육볶음이요~",
          "original_form": "제육볶음이요~",
          "speaker_id": "2",
          "time": "20210908 11:17"
        },
        {
          "id": "MDRW2100046215.1.4",
          "form": "어제 반찬가게에서 산게 잇어서",
          "original_form": "어제 반찬가게에서 산게 잇어서",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046215.1.5",
          "form": "와 맛있겠어요",
          "original_form": "와 맛있겠어요",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046215.1.6",
          "form": "맛있어서",
          "original_form": "맛있어서",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046215.1.7",
          "form": "이틀연속 시켰어요",
          "original_form": "이틀연속 시켰어요",
          "speaker_id": "2",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046215.1.8",
          "form": "와",
          "original_form": "와",
          "speaker_id": "1",
          "time": "20210908 11:18"
        },
        {
          "id": "MDRW2100046215.1.9",
          "form": "반찬 주로 시켜드시나요?_?",
          "original_form": "반찬 주로 시켜드시나요?_?",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046215.1.10",
          "form": "네 귀찮기도하고",
          "original_form": "네 귀찮기도하고",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046215.1.11",
          "form": "비싸더라도 재료 남고 버리고 하능거보다",
          "original_form": "비싸더라도 재료 남고 버리고 하능거보다",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046215.1.12",
          "form": "낫더라거요",
          "original_form": "낫더라거요",
          "speaker_id": "2",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046215.1.13",
          "form": "아 ㅠㅠㅠㅠ 그건 그래요",
          "original_form": "아 ㅠㅠㅠㅠ 그건 그래요",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046215.1.14",
          "form": "반찬은 집근처 반찬가게에서 시켜드시는거예요??",
          "original_form": "반찬은 집근처 반찬가게에서 시켜드시는거예요??",
          "speaker_id": "1",
          "time": "20210908 11:19"
        },
        {
          "id": "MDRW2100046215.1.15",
          "form": "요즘 자주먹는곳은 좀 멀어요",
          "original_form": "요즘 자주먹는곳은 좀 멀어요",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046215.1.16",
          "form": "아하",
          "original_form": "아하",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046215.1.17",
          "form": "버스로 40분?",
          "original_form": "버스로 40분?",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046215.1.18",
          "form": "근데 걍 배달시켜요ㅋㅋㅋㅋ",
          "original_form": "근데 걍 배달시켜요ㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046215.1.19",
          "form": "오... 동네 반찬가게도 배달이 되는건 첨알았어요",
          "original_form": "오... 동네 반찬가게도 배달이 되는건 첨알았어요",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046215.1.20",
          "form": "배달 안되는게 없어요ㅋㅋㅋㅋㅋ",
          "original_form": "배달 안되는게 없어요ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046215.1.21",
          "form": "커피도 하자나요",
          "original_form": "커피도 하자나요",
          "speaker_id": "2",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046215.1.22",
          "form": "저도 인터넷에서 반찬사먹는건 괜찮은거 같아서 고려중이었는데",
          "original_form": "저도 인터넷에서 반찬사먹는건 괜찮은거 같아서 고려중이었는데",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046215.1.23",
          "form": "아",
          "original_form": "아",
          "speaker_id": "1",
          "time": "20210908 11:20"
        },
        {
          "id": "MDRW2100046215.1.24",
          "form": "그건 그렇네요 ㅋㅋㅋㅋㅋㅋ",
          "original_form": "그건 그렇네요 ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210908 11:21"
        }
      ]
