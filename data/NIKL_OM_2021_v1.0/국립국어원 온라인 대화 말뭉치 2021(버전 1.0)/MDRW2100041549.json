      "utterance": [
        {
          "id": "MDRW2100041549.1.1",
          "form": "간식드시나요?",
          "original_form": "간식드시나요?",
          "speaker_id": "2",
          "time": "20210906 10:27"
        },
        {
          "id": "MDRW2100041549.1.2",
          "form": "허니버터아몬드 와사비맛 사랑합니다ㅎ",
          "original_form": "허니버터아몬드 와사비맛 사랑합니다ㅎ",
          "speaker_id": "1",
          "time": "20210906 11:04"
        },
        {
          "id": "MDRW2100041549.1.3",
          "form": "님은요?",
          "original_form": "님은요?",
          "speaker_id": "1",
          "time": "20210906 11:04"
        },
        {
          "id": "MDRW2100041549.1.4",
          "form": "저 초코랑 견과류 미쳐요ㅜㅋㅋ",
          "original_form": "저 초코랑 견과류 미쳐요ㅜㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:05"
        },
        {
          "id": "MDRW2100041549.1.5",
          "form": "ㅎㅎ 하루어쩌구 하는거",
          "original_form": "ㅎㅎ 하루어쩌구 하는거",
          "speaker_id": "1",
          "time": "20210906 11:05"
        },
        {
          "id": "MDRW2100041549.1.6",
          "form": "하루에 하나만 먹기 어렵지 않나요ㅜㅎㅎ",
          "original_form": "하루에 하나만 먹기 어렵지 않나요ㅜㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 11:05"
        },
        {
          "id": "MDRW2100041549.1.7",
          "form": "ㅋㅋ 저는 견과류 키로로 사놔요! ㅋㅋ 캐슈넛 환장합니다ㅋㅋ",
          "original_form": "ㅋㅋ 저는 견과류 키로로 사놔요! ㅋㅋ 캐슈넛 환장합니다ㅋㅋ",
          "speaker_id": "2",
          "time": "20210906 11:30"
        },
        {
          "id": "MDRW2100041549.1.8",
          "form": "견과류 칼로리 알지못합니다",
          "original_form": "견과류 칼로리 알지못합니다",
          "speaker_id": "2",
          "time": "20210906 11:31"
        },
        {
          "id": "MDRW2100041549.1.9",
          "form": "캐슈넛 인터넷서 사시나요?",
          "original_form": "캐슈넛 인터넷서 사시나요?",
          "speaker_id": "1",
          "time": "20210906 11:35"
        },
        {
          "id": "MDRW2100041549.1.10",
          "form": "네! 달달하고 맛있어요!",
          "original_form": "네! 달달하고 맛있어요!",
          "speaker_id": "2",
          "time": "20210906 11:51"
        },
        {
          "id": "MDRW2100041549.1.11",
          "form": "하루견과는 안 좋아하는것도 껴 있는데",
          "original_form": "하루견과는 안 좋아하는것도 껴 있는데",
          "speaker_id": "1",
          "time": "20210906 11:52"
        },
        {
          "id": "MDRW2100041549.1.12",
          "form": "네이버 너츠브라더 이용하고있어요",
          "original_form": "네이버 너츠브라더 이용하고있어요",
          "speaker_id": "2",
          "time": "20210906 11:52"
        },
        {
          "id": "MDRW2100041549.1.13",
          "form": "저도 좋아하는것만 대용량 질러야겠네요ㅎ",
          "original_form": "저도 좋아하는것만 대용량 질러야겠네요ㅎ",
          "speaker_id": "1",
          "time": "20210906 11:52"
        },
        {
          "id": "MDRW2100041549.1.14",
          "form": "캐슈넛하고 호두 추천이요!",
          "original_form": "캐슈넛하고 호두 추천이요!",
          "speaker_id": "2",
          "time": "20210906 11:52"
        },
        {
          "id": "MDRW2100041549.1.15",
          "form": "오오ㄱㅅㄱㅅ",
          "original_form": "오오ㄱㅅㄱㅅ",
          "speaker_id": "1",
          "time": "20210906 11:52"
        },
        {
          "id": "MDRW2100041549.1.16",
          "form": "저장했어요!",
          "original_form": "저장했어요!",
          "speaker_id": "1",
          "time": "20210906 11:52"
        },
        {
          "id": "MDRW2100041549.1.17",
          "form": "둘다 좋아함ㅎㅎ",
          "original_form": "둘다 좋아함ㅎㅎ",
          "speaker_id": "1",
          "time": "20210906 11:52"
        },
        {
          "id": "MDRW2100041549.1.18",
          "form": "호두는 8000",
          "original_form": "호두는 8000",
          "speaker_id": "2",
          "time": "20210906 11:52"
        },
        {
          "id": "MDRW2100041549.1.19",
          "form": "캐슈넛은 10900",
          "original_form": "캐슈넛은 10900",
          "speaker_id": "2",
          "time": "20210906 11:53"
        },
        {
          "id": "MDRW2100041549.1.20",
          "form": "생각보다 가격이 착하네요ㅎㅎ",
          "original_form": "생각보다 가격이 착하네요ㅎㅎ",
