      "utterance": [
        {
          "id": "MDRW2100009305.1.1",
          "form": "뭐 좋아하세요?",
          "original_form": "뭐 좋아하세요?",
          "speaker_id": "1",
          "time": "20210822 22:38"
        },
        {
          "id": "MDRW2100009305.1.2",
          "form": "저는 피자라면 환장해요",
          "original_form": "저는 피자라면 환장해요",
          "speaker_id": "2",
          "time": "20210822 22:44"
        },
        {
          "id": "MDRW2100009305.1.3",
          "form": "와 피자 맛있겠어요",
          "original_form": "와 피자 맛있겠어요",
          "speaker_id": "1",
          "time": "20210822 22:59"
        },
        {
          "id": "MDRW2100009305.1.4",
          "form": "혼자서 한판도 먹어요 힘내면 ㅋㅋ",
          "original_form": "혼자서 한판도 먹어요 힘내면 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:00"
        },
        {
          "id": "MDRW2100009305.1.5",
          "form": "혼자 한판이요?ㅋㅋ",
          "original_form": "혼자 한판이요?ㅋㅋ",
          "speaker_id": "1",
          "time": "20210822 23:01"
        },
        {
          "id": "MDRW2100009305.1.6",
          "form": "작은거라면",
          "original_form": "작은거라면",
          "speaker_id": "2",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009305.1.7",
          "form": "근데이런거에 힘 안내도 ㅋㅋ 되긴하져 ㅋㅋㅋ",
          "original_form": "근데이런거에 힘 안내도 ㅋㅋ 되긴하져 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210822 23:02"
        },
        {
          "id": "MDRW2100009305.1.8",
          "form": "대식가시군요!",
          "original_form": "대식가시군요!",
          "speaker_id": "1",
          "time": "20210822 23:04"
        },
        {
          "id": "MDRW2100009305.1.9",
          "form": "빵순이라서요",
          "original_form": "빵순이라서요",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009305.1.10",
          "form": "어떤음식 좋아하세요",
          "original_form": "어떤음식 좋아하세요",
          "speaker_id": "2",
          "time": "20210822 23:05"
        },
        {
          "id": "MDRW2100009305.1.11",
          "form": "저는 회요!",
          "original_form": "저는 회요!",
          "speaker_id": "1",
          "time": "20210822 23:07"
        },
        {
          "id": "MDRW2100009305.1.12",
          "form": "오 흰살생선 붉은살생선 중 고른다면?",
          "original_form": "오 흰살생선 붉은살생선 중 고른다면?",
          "speaker_id": "2",
          "time": "20210822 23:12"
        },
        {
          "id": "MDRW2100009305.1.13",
          "form": "연어요!",
          "original_form": "연어요!",
          "speaker_id": "1",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009305.1.14",
          "form": "오 많이들좋아하시져",
          "original_form": "오 많이들좋아하시져",
          "speaker_id": "2",
          "time": "20210822 23:20"
        },
        {
          "id": "MDRW2100009305.1.15",
          "form": "저는 무한리필집 가보고싶더라고요",
          "original_form": "저는 무한리필집 가보고싶더라고요",
          "speaker_id": "2",
          "time": "20210822 23:21"
        },
        {
          "id": "MDRW2100009305.1.16",
          "form": "예전엔 자주 갔죠",
          "original_form": "예전엔 자주 갔죠",
          "speaker_id": "1",
