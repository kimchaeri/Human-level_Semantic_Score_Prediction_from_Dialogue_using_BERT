        {
          "id": "MDRW2100020111.1.1",
          "form": "동물키워요?",
          "original_form": "동물키워요?",
          "speaker_id": "1",
          "time": "20210827 23:44"
        },
        {
          "id": "MDRW2100020111.1.2",
          "form": "아니요. 키우시나요?",
          "original_form": "아니요. 키우시나요?",
          "speaker_id": "2",
          "time": "20210827 23:46"
        },
        {
          "id": "MDRW2100020111.1.3",
          "form": "저도 여건이",
          "original_form": "저도 여건이",
          "speaker_id": "1",
          "time": "20210828 07:35"
        },
        {
          "id": "MDRW2100020111.1.4",
          "form": "키우고 싶은 동물 있나요?",
          "original_form": "키우고 싶은 동물 있나요?",
          "speaker_id": "2",
          "time": "20210828 09:36"
        },
        {
          "id": "MDRW2100020111.1.5",
          "form": "강아지요",
          "original_form": "강아지요",
          "speaker_id": "1",
          "time": "20210828 15:26"
        },
        {
          "id": "MDRW2100020111.1.6",
          "form": "좋아하는 품종 있나요?",
          "original_form": "좋아하는 품종 있나요?",
          "speaker_id": "2",
          "time": "20210828 15:29"
        },
        {
          "id": "MDRW2100020111.1.7",
          "form": "종을 잘 몰라서",
          "original_form": "종을 잘 몰라서",
          "speaker_id": "1",
          "time": "20210828 15:34"
        },
        {
          "id": "MDRW2100020111.1.8",
          "form": "리트리버 귀엽더라고요",
          "original_form": "리트리버 귀엽더라고요",
          "speaker_id": "2",
          "time": "20210828 15:43"
        },
        {
          "id": "MDRW2100020111.1.9",
          "form": "큰거 아닌가?",
          "original_form": "큰거 아닌가?",
          "speaker_id": "1",
          "time": "20210828 15:52"
        },
        {
          "id": "MDRW2100020111.1.10",
          "form": "맞아요 대형견이에요",
          "original_form": "맞아요 대형견이에요",
          "speaker_id": "2",
          "time": "20210828 16:14"
        },
        {
          "id": "MDRW2100020111.1.11",
          "form": "관리 힘들지 않나오?",
          "original_form": "관리 힘들지 않나오?",
          "speaker_id": "1",
          "time": "20210828 16:22"
        },
        {
          "id": "MDRW2100020111.1.12",
          "form": "네. 털이 많이 빠진다고 하더라구요",
          "original_form": "네. 털이 많이 빠진다고 하더라구요",
          "speaker_id": "2",
          "time": "20210828 16:25"
        },
        {
          "id": "MDRW2100020111.1.13",
          "form": "털은 고양이가",
          "original_form": "털은 고양이가",
