        {
          "id": "MDRW2100028672.1.4",
          "form": "넹 ㅋㅋㅋ",
          "original_form": "넹 ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028672.1.5",
          "form": "요즘 유트브사",
          "original_form": "요즘 유트브사",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028672.1.6",
          "form": "짱입니다",
          "original_form": "짱입니다",
          "speaker_id": "2",
          "time": "20210902 22:33"
        },
        {
          "id": "MDRW2100028672.1.7",
          "form": "돼지고기 넣었나영?",
          "original_form": "돼지고기 넣었나영?",
          "speaker_id": "1",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028672.1.8",
          "form": "참치넣었어용",
          "original_form": "참치넣었어용",
          "speaker_id": "2",
          "time": "20210902 22:34"
        },
        {
          "id": "MDRW2100028672.1.9",
          "form": "ㅋㅋ",
          "original_form": "ㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028672.1.10",
          "form": "집에있는게앖어서",
          "original_form": "집에있는게앖어서",
          "speaker_id": "2",
          "time": "20210902 22:35"
        },
        {
          "id": "MDRW2100028672.1.11",
          "form": "두부는여? ㅋㅋ",
          "original_form": "두부는여? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:37"
        },
        {
          "id": "MDRW2100028672.1.12",
          "form": "두부도없어섴ㅋㅋㅋㅋㅋ",
          "original_form": "두부도없어섴ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100028672.1.13",
          "form": "그냥 김치랑 참치로만...",
          "original_form": "그냥 김치랑 참치로만...",
          "speaker_id": "2",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100028672.1.14",
          "form": "그래도 먹을만해서..ㅎ",
          "original_form": "그래도 먹을만해서..ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100028672.1.15",
          "form": "아..장보기귀찮다진짜 ㅠ",
          "original_form": "아..장보기귀찮다진짜 ㅠ",
          "speaker_id": "2",
          "time": "20210902 22:46"
        },
        {
          "id": "MDRW2100028672.1.16",
          "form": "요새 아빠 요리 안하세여? ㅋㅋ",
          "original_form": "요새 아빠 요리 안하세여? ㅋㅋ",
          "speaker_id": "1",
          "time": "20210902 22:48"
        },
        {
          "id": "MDRW2100028672.1.17",
          "form": "네  ..ㅋㅋㅋ",
          "original_form": "네  ..ㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100028672.1.18",
          "form": "아빠 사업하셔서..",
          "original_form": "아빠 사업하셔서..",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100028672.1.19",
          "form": "바쁘세염..ㅎ",
          "original_form": "바쁘세염..ㅎ",
          "speaker_id": "2",
          "time": "20210902 22:49"
        },
        {
          "id": "MDRW2100028672.1.20",
          "form": "그럼 같이 식사도 못해염? ㅠㅠ",
          "original_form": "그럼 같이 식사도 못해염? ㅠㅠ",
