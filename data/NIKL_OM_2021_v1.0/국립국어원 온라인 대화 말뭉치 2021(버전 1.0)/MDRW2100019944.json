        {
          "id": "MDRW2100019944.1.1",
          "form": "야식 이것만먹고 자야겠어요",
          "original_form": "야식 이것만먹고 자야겠어요",
          "speaker_id": "2",
          "time": "20210827 22:56"
        },
        {
          "id": "MDRW2100019944.1.2",
          "form": "ㅍ혼술 하시는거에요?",
          "original_form": "ㅍ혼술 하시는거에요?",
          "speaker_id": "1",
          "time": "20210827 22:57"
        },
        {
          "id": "MDRW2100019944.1.3",
          "form": "네 늘저녁에 맥주한캔",
          "original_form": "네 늘저녁에 맥주한캔",
          "speaker_id": "2",
          "time": "20210827 22:59"
        },
        {
          "id": "MDRW2100019944.1.4",
          "form": "혼자 마시면 맛없지 않아요?",
          "original_form": "혼자 마시면 맛없지 않아요?",
          "speaker_id": "1",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100019944.1.5",
          "form": "자유시간이니깐요 애들재우고",
          "original_form": "자유시간이니깐요 애들재우고",
          "speaker_id": "2",
          "time": "20210827 23:00"
        },
        {
          "id": "MDRW2100019944.1.6",
          "form": "자유시간은 혼자 오롯이~",
          "original_form": "자유시간은 혼자 오롯이~",
          "speaker_id": "1",
          "time": "20210827 23:01"
        },
        {
          "id": "MDRW2100019944.1.7",
          "form": "조용히 즐기는거죠",
          "original_form": "조용히 즐기는거죠",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019944.1.8",
          "form": "글츄 그것도 좋죠",
          "original_form": "글츄 그것도 좋죠",
          "speaker_id": "1",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019944.1.9",
          "form": "여유랄까 선물이랄까",
          "original_form": "여유랄까 선물이랄까",
          "speaker_id": "2",
          "time": "20210827 23:02"
        },
        {
          "id": "MDRW2100019944.1.10",
          "form": "ㅎㅎㅎㅎ 소중하시겠어요",
          "original_form": "ㅎㅎㅎㅎ 소중하시겠어요",
          "speaker_id": "1",
          "time": "20210827 23:03"
        },
        {
          "id": "MDRW2100019944.1.11",
          "form": "잠좀자고오께요ㅠ",
          "original_form": "잠좀자고오께요ㅠ",
          "speaker_id": "2",
          "time": "20210827 23:04"
        },
        {
          "id": "MDRW2100019944.1.12",
