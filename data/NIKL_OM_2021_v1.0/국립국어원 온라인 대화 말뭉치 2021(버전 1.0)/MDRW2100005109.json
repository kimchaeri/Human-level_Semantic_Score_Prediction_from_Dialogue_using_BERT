      "utterance": [
        {
          "id": "MDRW2100005109.1.1",
          "form": "어떤 매체를 보세요",
          "original_form": "어떤 매체를 보세요",
          "speaker_id": "2",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005109.1.2",
          "form": "전 요새 유투브 넷플릭스 왓챠요",
          "original_form": "전 요새 유투브 넷플릭스 왓챠요",
          "speaker_id": "2",
          "time": "20210821 20:47"
        },
        {
          "id": "MDRW2100005109.1.3",
          "form": "다양하게 보시네요.전 유튜브요ㅋ",
          "original_form": "다양하게 보시네요.전 유튜브요ㅋ",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005109.1.4",
          "form": "유튜브 맨날봐요",
          "original_form": "유튜브 맨날봐요",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005109.1.5",
          "form": "주식방송 내성절톱 시원하게 관라하는거 ㅋㅋ",
          "original_form": "주식방송 내성절톱 시원하게 관라하는거 ㅋㅋ",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005109.1.6",
          "form": "음악요",
          "original_form": "음악요",
          "speaker_id": "2",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005109.1.7",
          "form": "저도 티비 또는 유튜브 ㅋ",
          "original_form": "저도 티비 또는 유튜브 ㅋ",
          "speaker_id": "1",
          "time": "20210821 20:49"
        },
        {
          "id": "MDRW2100005109.1.8",
          "form": "네 뭐 보세요?",
          "original_form": "네 뭐 보세요?",
          "speaker_id": "2",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005109.1.9",
          "form": "사건관련이나 음악 ㅋ",
          "original_form": "사건관련이나 음악 ㅋ",
          "speaker_id": "1",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005109.1.10",
          "form": "네 음악 듣기 좋아요",
          "original_form": "네 음악 듣기 좋아요",
          "speaker_id": "2",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005109.1.11",
          "form": "청소 할때도 틀고",
          "original_form": "청소 할때도 틀고",
          "speaker_id": "2",
          "time": "20210821 20:50"
        },
        {
          "id": "MDRW2100005109.1.12",
          "form": "요즘 악뮤 노래도 듣기 괜찮드라구요",
          "original_form": "요즘 악뮤 노래도 듣기 괜찮드라구요",
          "speaker_id": "1",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005109.1.13",
          "form": "네 ㅎㅎ",
          "original_form": "네 ㅎㅎ",
          "speaker_id": "2",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005109.1.14",
          "form": "저도 좋아해요",
          "original_form": "저도 좋아해요",
          "speaker_id": "2",
          "time": "20210821 20:51"
        },
        {
          "id": "MDRW2100005109.1.15",
          "form": "어떻게 이별까지 오늘 계속 들었어요",
          "original_form": "어떻게 이별까지 오늘 계속 들었어요",
          "speaker_id": "2",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005109.1.16",
          "form": "그래요?  전 아이유랑 부른 낙하 좋드라구요",
          "original_form": "그래요?  전 아이유랑 부른 낙하 좋드라구요",
          "speaker_id": "1",
          "time": "20210821 20:52"
        },
        {
          "id": "MDRW2100005109.1.17",
          "form": "네 사실 다좋나요 전 사실 다이노소어 까지도 좋아해요 ㅋㅌ",
          "original_form": "네 사실 다좋나요 전 사실 다이노소어 까지도 좋아해요 ㅋㅌ",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005109.1.18",
          "form": "천재같아요",
          "original_form": "천재같아요",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005109.1.19",
          "form": "네 맞아요 천재예요. 저도 그노래 좋아해요",
          "original_form": "네 맞아요 천재예요. 저도 그노래 좋아해요",
          "speaker_id": "1",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005109.1.20",
          "form": "음악적 재능이 남달라요",
          "original_form": "음악적 재능이 남달라요",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005109.1.21",
          "form": "음색도 너무 좋죠",
          "original_form": "음색도 너무 좋죠",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005109.1.22",
          "form": "가끔 소름 돋아요",
          "original_form": "가끔 소름 돋아요",
          "speaker_id": "2",
          "time": "20210821 20:53"
        },
        {
          "id": "MDRW2100005109.1.23",
          "form": "남매가 노래도 만들고 부르기도 하고 대단해요",
          "original_form": "남매가 노래도 만들고 부르기도 하고 대단해요",
          "speaker_id": "1",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005109.1.24",
          "form": "네 아 못하는개 없어요",
          "original_form": "네 아 못하는개 없어요",
          "speaker_id": "2",
          "time": "20210821 20:54"
        },
        {
          "id": "MDRW2100005109.1.25",
          "form": "정말 재능이 부러워요",
          "original_form": "정말 재능이 부러워요",
          "speaker_id": "2",
          "time": "20210821 20:54"
        }
      ]
    }
  ]
