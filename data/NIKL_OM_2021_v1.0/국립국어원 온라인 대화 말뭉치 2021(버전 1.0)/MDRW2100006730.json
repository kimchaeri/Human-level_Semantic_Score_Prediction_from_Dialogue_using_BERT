      "utterance": [
        {
          "id": "MDRW2100006730.1.1",
          "form": "환경 중시해야지",
          "original_form": "환경 중시해야지",
          "speaker_id": "2",
          "time": "20210908 07:55"
        },
        {
          "id": "MDRW2100006730.1.2",
          "form": "맞아",
          "original_form": "맞아",
          "speaker_id": "1",
          "time": "20210908 07:55"
        },
        {
          "id": "MDRW2100006730.1.3",
          "form": "어떤 방인이 좋을까",
          "original_form": "어떤 방인이 좋을까",
          "speaker_id": "2",
          "time": "20210908 07:55"
        },
        {
          "id": "MDRW2100006730.1.4",
          "form": "일단 일회용품을 줄여야해",
          "original_form": "일단 일회용품을 줄여야해",
          "speaker_id": "1",
          "time": "20210908 07:56"
        },
        {
          "id": "MDRW2100006730.1.5",
          "form": "일회용품 줄이는거 은근 힘들어",
          "original_form": "일회용품 줄이는거 은근 힘들어",
          "speaker_id": "2",
          "time": "20210908 07:56"
        },
        {
          "id": "MDRW2100006730.1.6",
          "form": "맞아 이미 편한거에익숙해서",
          "original_form": "맞아 이미 편한거에익숙해서",
          "speaker_id": "1",
          "time": "20210908 07:56"
        },
        {
          "id": "MDRW2100006730.1.7",
          "form": "그래서 카페들에선 다양한 이벤트를 하더라",
          "original_form": "그래서 카페들에선 다양한 이벤트를 하더라",
          "speaker_id": "2",
          "time": "20210908 07:56"
        },
        {
          "id": "MDRW2100006730.1.8",
          "form": "다회용품 사용하면ㅁ가격할인해주는거?",
          "original_form": "다회용품 사용하면ㅁ가격할인해주는거?",
          "speaker_id": "1",
          "time": "20210908 07:56"
        },
        {
          "id": "MDRW2100006730.1.9",
          "form": "맞아 그거",
          "original_form": "맞아 그거",
          "speaker_id": "2",
          "time": "20210908 07:56"
        },
        {
          "id": "MDRW2100006730.1.10",
          "form": "너도 하니",
          "original_form": "너도 하니",
          "speaker_id": "1",
          "time": "20210908 07:56"
        },
        {
          "id": "MDRW2100006730.1.11",
          "form": "나도 할인 받으려 종종 텀블러 들고 가",
          "original_form": "나도 할인 받으려 종종 텀블러 들고 가",
          "speaker_id": "2",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100006730.1.12",
          "form": "좋은 자세네",
          "original_form": "좋은 자세네",
          "speaker_id": "1",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100006730.1.13",
          "form": "스타벅스의 경우 별을 적립할 수도 있어",
          "original_form": "스타벅스의 경우 별을 적립할 수도 있어",
          "speaker_id": "2",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100006730.1.14",
          "form": "12잔모으면 1잔이니 괜잖네",
          "original_form": "12잔모으면 1잔이니 괜잖네",
          "speaker_id": "1",
          "time": "20210908 07:57"
        },
        {
          "id": "MDRW2100006730.1.15",
          "form": "나는 그런 대기업들이 친환경에 동참하는게 좋아",
          "original_form": "나는 그런 대기업들이 친환경에 동참하는게 좋아",
