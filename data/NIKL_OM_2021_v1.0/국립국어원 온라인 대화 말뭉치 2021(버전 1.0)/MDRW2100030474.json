      "utterance": [
        {
          "id": "MDRW2100030474.1.1",
          "form": "안녕하세요~",
          "original_form": "안녕하세요~",
          "speaker_id": "1",
          "time": "20210904 01:44"
        },
        {
          "id": "MDRW2100030474.1.2",
          "form": "내일 날씨도 오늘 같았으면 좋겠어요",
          "original_form": "내일 날씨도 오늘 같았으면 좋겠어요",
          "speaker_id": "1",
          "time": "20210904 01:45"
        },
        {
          "id": "MDRW2100030474.1.3",
          "form": "name1님 안녕하세요!!",
          "original_form": "&name1&님 안녕하세요!!",
          "speaker_id": "2",
          "time": "20210904 01:45"
        },
        {
          "id": "MDRW2100030474.1.4",
          "form": "맞아요.... 오늘 하늘 정말",
          "original_form": "맞아요.... 오늘 하늘 정말",
          "speaker_id": "2",
          "time": "20210904 01:45"
        },
        {
          "id": "MDRW2100030474.1.5",
          "form": "가을하늘이었어요~! 맑고 선선하고!",
          "original_form": "가을하늘이었어요~! 맑고 선선하고!",
          "speaker_id": "2",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100030474.1.6",
          "form": "맞아요! 하늘도 깨끗하고 햇볕도 따듯했는데",
          "original_form": "맞아요! 하늘도 깨끗하고 햇볕도 따듯했는데",
          "speaker_id": "1",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100030474.1.7",
          "form": "바람이 선선하니까 너무 좋더라구요",
          "original_form": "바람이 선선하니까 너무 좋더라구요",
          "speaker_id": "1",
          "time": "20210904 01:46"
        },
        {
          "id": "MDRW2100030474.1.8",
          "form": "앗 맞아요 정말!!!!",
          "original_form": "앗 맞아요 정말!!!!",
          "speaker_id": "2",
          "time": "20210904 01:47"
        },
        {
          "id": "MDRW2100030474.1.9",
          "form": "앗 뭐가 나쁜말이라는 걸까요ㅠ",
          "original_form": "앗 뭐가 나쁜말이라는 걸까요ㅠ",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030474.1.10",
          "form": "슬슬 가을 옷을 입을 준비를 해야하나봐요!",
          "original_form": "슬슬 가을 옷을 입을 준비를 해야하나봐요!",
          "speaker_id": "1",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030474.1.11",
          "form": "오늘 때 마침 낮에 점심 먹고",
          "original_form": "오늘 때 마침 낮에 점심 먹고",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030474.1.12",
          "form": "네네!!! 맞아요!",
          "original_form": "네네!!! 맞아요!",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030474.1.13",
          "form": "산책하고 들어오는데",
          "original_form": "산책하고 들어오는데",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030474.1.14",
          "form": "네네",
          "original_form": "네네",
          "speaker_id": "1",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030474.1.15",
          "form": "날씨가 기가 막히더라구요.....",
          "original_form": "날씨가 기가 막히더라구요.....",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030474.1.16",
          "form": "봄이 올 때랑 가을이 올 때랑",
          "original_form": "봄이 올 때랑 가을이 올 때랑",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030474.1.17",
          "form": "좀 다른 느낌의 설렘이에요",
          "original_form": "좀 다른 느낌의 설렘이에요",
          "speaker_id": "2",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030474.1.18",
          "form": "꺄 진짜 산책하기 좋은 날씨였죠",
          "original_form": "꺄 진짜 산책하기 좋은 날씨였죠",
          "speaker_id": "1",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030474.1.19",
          "form": "맞아요",
          "original_form": "맞아요",
          "speaker_id": "1",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030474.1.20",
          "form": "일년이 끝나간다는게 아쉽긴한데",
          "original_form": "일년이 끝나간다는게 아쉽긴한데",
          "speaker_id": "1",
          "time": "20210904 01:48"
        },
        {
          "id": "MDRW2100030474.1.21",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100030474.1.22",
          "form": "여름에 고생하고 가을날씨로 위로받는 느낌",
          "original_form": "여름에 고생하고 가을날씨로 위로받는 느낌",
          "speaker_id": "1",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100030474.1.23",
          "form": "헉 정말 맞는 말이에요",
          "original_form": "헉 정말 맞는 말이에요",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100030474.1.24",
          "form": "이번 여름이 진짜",
          "original_form": "이번 여름이 진짜",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100030474.1.25",
          "form": "유난히도 더웠어서",
          "original_form": "유난히도 더웠어서",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100030474.1.26",
          "form": "사람이 더워서 죽을수도 있겠구나 느꼈어요",
          "original_form": "사람이 더워서 죽을수도 있겠구나 느꼈어요",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100030474.1.27",
          "form": "와맞아요ㅋㅋㅋㅋ",
          "original_form": "와맞아요ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100030474.1.28",
          "form": "하..",
          "original_form": "하..",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100030474.1.29",
          "form": "거기에",
          "original_form": "거기에",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100030474.1.30",
          "form": "마스크까지 썼으니...",
          "original_form": "마스크까지 썼으니...",
          "speaker_id": "2",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100030474.1.31",
          "form": "에어컨도 보통 여름보다 빨리튼것같아요",
          "original_form": "에어컨도 보통 여름보다 빨리튼것같아요",
          "speaker_id": "1",
          "time": "20210904 01:49"
        },
        {
          "id": "MDRW2100030474.1.32",
          "form": "전기세ㅜㅜ",
          "original_form": "전기세ㅜㅜ",
          "speaker_id": "1",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100030474.1.33",
          "form": "맞아요!!!",
          "original_form": "맞아요!!!",
          "speaker_id": "2",
          "time": "20210904 01:50"
        },
        {
          "id": "MDRW2100030474.1.34",
          "form": "맞네요마스크ㅜㅠ..,",
          "original_form": "맞네요마스크ㅜㅠ..,",
