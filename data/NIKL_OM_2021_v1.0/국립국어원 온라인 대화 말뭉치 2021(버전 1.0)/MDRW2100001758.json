      "utterance": [
        {
          "id": "MDRW2100001758.1.1",
          "form": "뮤지컬이나 콘서트 좋아해요?",
          "original_form": "뮤지컬이나 콘서트 좋아해요?",
          "speaker_id": "1",
          "time": "20210819 18:37"
        },
        {
          "id": "MDRW2100001758.1.2",
          "form": "코로나때문에 못가서 넘 힘들죠ㅠㅠ",
          "original_form": "코로나때문에 못가서 넘 힘들죠ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 18:37"
        },
        {
          "id": "MDRW2100001758.1.3",
          "form": "전 여건상 못가봤어요",
          "original_form": "전 여건상 못가봤어요",
          "speaker_id": "2",
          "time": "20210819 18:39"
        },
        {
          "id": "MDRW2100001758.1.4",
          "form": "다음에 상황 좋아지면 가보세요~",
          "original_form": "다음에 상황 좋아지면 가보세요~",
          "speaker_id": "1",
          "time": "20210819 18:42"
        },
        {
          "id": "MDRW2100001758.1.5",
          "form": "근데 예전처럼 사람들이 막 모여서 같이 볼 수 있을지 모르겠어요ㅠㅠ",
          "original_form": "근데 예전처럼 사람들이 막 모여서 같이 볼 수 있을지 모르겠어요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 18:42"
        },
        {
          "id": "MDRW2100001758.1.6",
          "form": "그쵸. 코로나가 끝나서 봐야 알겠네요~",
          "original_form": "그쵸. 코로나가 끝나서 봐야 알겠네요~",
          "speaker_id": "2",
          "time": "20210819 18:43"
        },
        {
          "id": "MDRW2100001758.1.7",
          "form": "그냥 영화관람 하는 것도 좋아하는데",
          "original_form": "그냥 영화관람 하는 것도 좋아하는데",
          "speaker_id": "1",
          "time": "20210819 18:44"
        },
        {
          "id": "MDRW2100001758.1.8",
          "form": "영화관에서 영화보기도 쉽지 않네요ㅠㅠ",
          "original_form": "영화관에서 영화보기도 쉽지 않네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 18:44"
        },
        {
          "id": "MDRW2100001758.1.9",
          "form": "맞아요. 보고싶어도 혹시나 하고 못가고 나중에  인터넷결제로 봐야되죠",
          "original_form": "맞아요. 보고싶어도 혹시나 하고 못가고 나중에  인터넷결제로 봐야되죠",
          "speaker_id": "2",
          "time": "20210819 18:46"
        },
        {
          "id": "MDRW2100001758.1.10",
          "form": "그쵸 답답하네요ㅠㅠ",
          "original_form": "그쵸 답답하네요ㅠㅠ",
          "speaker_id": "1",
          "time": "20210819 19:32"
        },
        {
          "id": "MDRW2100001758.1.11",
          "form": "최근에 어떤 영화 보셨어요?",
          "original_form": "최근에 어떤 영화 보셨어요?",
          "speaker_id": "1",
          "time": "20210819 19:32"
        },
        {
          "id": "MDRW2100001758.1.12",
          "form": "블랙위도우요~",
          "original_form": "블랙위도우요~",
          "speaker_id": "2",
          "time": "20210819 20:11"
        },
        {
          "id": "MDRW2100001758.1.13",
          "form": "오 재밌었어요?",
          "original_form": "오 재밌었어요?",
          "speaker_id": "1",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100001758.1.14",
          "form": "봐야지 해놓고 못봤는데",
          "original_form": "봐야지 해놓고 못봤는데",
          "speaker_id": "1",
          "time": "20210819 21:02"
        },
        {
          "id": "MDRW2100001758.1.15",
          "form": "네~ 시리즈보셨거나 좋아하는장르면 괜찮으실거에요ㅎ",
          "original_form": "네~ 시리즈보셨거나 좋아하는장르면 괜찮으실거에요ㅎ",
          "speaker_id": "2",
          "time": "20210819 21:17"
        },
        {
          "id": "MDRW2100001758.1.16",
          "form": "어벤져스 미리 보고나서 보는게 나은가요?",
          "original_form": "어벤져스 미리 보고나서 보는게 나은가요?",
          "speaker_id": "1",
          "time": "20210819 21:17"
        },
        {
          "id": "MDRW2100001758.1.17",
          "form": "저 아이언맨만 봤거든요!",
          "original_form": "저 아이언맨만 봤거든요!",
          "speaker_id": "1",
          "time": "20210819 21:17"
        },
        {
          "id": "MDRW2100001758.1.18",
          "form": "안봐도 대략이해가는데 보기전에 시빌워보시는것도 좋아요",
          "original_form": "안봐도 대략이해가는데 보기전에 시빌워보시는것도 좋아요",
          "speaker_id": "2",
          "time": "20210819 21:30"
        },
        {
          "id": "MDRW2100001758.1.19",
          "form": "오 감사합니다!",
          "original_form": "오 감사합니다!",
          "speaker_id": "1",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100001758.1.20",
          "form": "제 주변에서도 다들 블랙위도우 재밌게 봤다고 하더라구요",
          "original_form": "제 주변에서도 다들 블랙위도우 재밌게 봤다고 하더라구요",
          "speaker_id": "1",
          "time": "20210819 21:32"
        },
        {
          "id": "MDRW2100001758.1.21",
          "form": "네~ 제주변도 그렇다고 하더라고요~ㅎ",
          "original_form": "네~ 제주변도 그렇다고 하더라고요~ㅎ",
          "speaker_id": "2",
          "time": "20210819 21:55"
        }
      ]
    }
