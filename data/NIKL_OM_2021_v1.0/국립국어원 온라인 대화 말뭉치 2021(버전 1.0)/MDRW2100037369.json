      "utterance": [
        {
          "id": "MDRW2100037369.1.1",
          "form": "겨울에도 아이스 아메리카노 먹어요",
          "original_form": "겨울에도 아이스 아메리카노 먹어요",
          "speaker_id": "1",
          "time": "20210831 22:56"
        },
        {
          "id": "MDRW2100037369.1.2",
          "form": "그렇군욥!",
          "original_form": "그렇군욥!",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037369.1.3",
          "form": "요리는 자주 해서 드시나요?",
          "original_form": "요리는 자주 해서 드시나요?",
          "speaker_id": "2",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037369.1.4",
          "form": "아니 엄마가 주로 하니까 전 잘 안 해요",
          "original_form": "아니 엄마가 주로 하니까 전 잘 안 해요",
          "speaker_id": "1",
          "time": "20210831 22:59"
        },
        {
          "id": "MDRW2100037369.1.5",
          "form": "평소에 어떤 음식 많이드세요?",
          "original_form": "평소에 어떤 음식 많이드세요?",
          "speaker_id": "2",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100037369.1.6",
          "form": "집에서 나물 비빔밥 자주 먹어요",
          "original_form": "집에서 나물 비빔밥 자주 먹어요",
          "speaker_id": "1",
          "time": "20210831 23:05"
        },
        {
          "id": "MDRW2100037369.1.7",
          "form": "우와 비빔밥 최고네요",
          "original_form": "우와 비빔밥 최고네요",
          "speaker_id": "2",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100037369.1.8",
          "form": "요즘 비빔밥을 안먹은거같아 먹고싶어지네요",
          "original_form": "요즘 비빔밥을 안먹은거같아 먹고싶어지네요",
          "speaker_id": "2",
          "time": "20210831 23:33"
        },
        {
          "id": "MDRW2100037369.1.9",
          "form": "네 맛있긴 한데 너무 만드는 거 힘들어요",
          "original_form": "네 맛있긴 한데 너무 만드는 거 힘들어요",
          "speaker_id": "1",
          "time": "20210831 23:34"
        },
        {
          "id": "MDRW2100037369.1.10",
          "form": "그쵸ㅠ",
          "original_form": "그쵸ㅠ",
          "speaker_id": "2",
          "time": "20210831 23:37"
        },
        {
          "id": "MDRW2100037369.1.11",
          "form": "나물 어떤거 넣으세요?",
          "original_form": "나물 어떤거 넣으세요?",
          "speaker_id": "2",
          "time": "20210831 23:38"
        },
        {
          "id": "MDRW2100037369.1.12",
          "form": "무나물 고사리 나물 시금치 나물 취나물 등등 많이 먹어요",
          "original_form": "무나물 고사리 나물 시금치 나물 취나물 등등 많이 먹어요",
          "speaker_id": "1",
          "time": "20210831 23:39"
        },
        {
          "id": "MDRW2100037369.1.13",
          "form": "우와ㅋㅋ 제대로 드시는군욥",
          "original_form": "우와ㅋㅋ 제대로 드시는군욥",
          "speaker_id": "2",
          "time": "20210831 23:39"
        },
        {
          "id": "MDRW2100037369.1.14",
          "form": "나물 다 좋아하세요?",
          "original_form": "나물 다 좋아하세요?",
          "speaker_id": "2",
          "time": "20210831 23:39"
        },
        {
          "id": "MDRW2100037369.1.15",
          "form": "네 저는 너무 맛있더라고요",
          "original_form": "네 저는 너무 맛있더라고요",
          "speaker_id": "1",
          "time": "20210831 23:40"
        },
        {
          "id": "MDRW2100037369.1.16",
          "form": "아 근데 진짜 다 맛있는것들뿐이네요",
          "original_form": "아 근데 진짜 다 맛있는것들뿐이네요",
          "speaker_id": "2",
          "time": "20210831 23:40"
        },
        {
          "id": "MDRW2100037369.1.17",
          "form": "저는 고사리 진짜 좋아하는데 밖에서 사먹을때 조금줘서 늘 더 달라그하거든요",
          "original_form": "저는 고사리 진짜 좋아하는데 밖에서 사먹을때 조금줘서 늘 더 달라그하거든요",
          "speaker_id": "2",
          "time": "20210831 23:41"
        },
        {
          "id": "MDRW2100037369.1.18",
          "form": "고사리가 식당 입장에선 비싼 음식일 거 같아요",
          "original_form": "고사리가 식당 입장에선 비싼 음식일 거 같아요",
          "speaker_id": "1",
          "time": "20210831 23:41"
        },
        {
          "id": "MDRW2100037369.1.19",
          "form": "아 그래서 그런걸까요?ㅠ",
          "original_form": "아 그래서 그런걸까요?ㅠ",
          "speaker_id": "2",
          "time": "20210831 23:42"
        },
        {
          "id": "MDRW2100037369.1.20",
          "form": "고사리가 비싸군요",
          "original_form": "고사리가 비싸군요",
          "speaker_id": "2",
          "time": "20210831 23:42"
        }
      ]
    }
