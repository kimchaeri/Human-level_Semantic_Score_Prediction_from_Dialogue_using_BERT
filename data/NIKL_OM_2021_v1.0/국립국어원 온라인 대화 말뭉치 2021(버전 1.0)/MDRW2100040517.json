      "utterance": [
        {
          "id": "MDRW2100040517.1.1",
          "form": "어 화동은 다섯살 이하만 할수있대요",
          "original_form": "어 화동은 다섯살 이하만 할수있대요",
          "speaker_id": "2",
          "time": "20210904 22:10"
        },
        {
          "id": "MDRW2100040517.1.2",
          "form": "아 몰랐어요",
          "original_form": "아 몰랐어요",
          "speaker_id": "1",
          "time": "20210904 22:11"
        },
        {
          "id": "MDRW2100040517.1.3",
          "form": "오늘 들은 쓸데없는 정보랍니다",
          "original_form": "오늘 들은 쓸데없는 정보랍니다",
          "speaker_id": "2",
          "time": "20210904 22:11"
        },
        {
          "id": "MDRW2100040517.1.4",
          "form": "ㅋㅋㅋㅋㅋㅋ 네 저도 몰랐어요",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 네 저도 몰랐어요",
          "speaker_id": "2",
          "time": "20210904 22:11"
        },
        {
          "id": "MDRW2100040517.1.5",
          "form": "애기 이쁠때이긴 하겠어요",
          "original_form": "애기 이쁠때이긴 하겠어요",
          "speaker_id": "2",
          "time": "20210904 22:11"
        },
        {
          "id": "MDRW2100040517.1.6",
          "form": "연애할땐 뭐에씌여서 없으면 못살것 같았거든요",
          "original_form": "연애할땐 뭐에씌여서 없으면 못살것 같았거든요",
          "speaker_id": "1",
          "time": "20210904 22:11"
        },
        {
          "id": "MDRW2100040517.1.7",
          "form": "넹",
          "original_form": "넹",
          "speaker_id": "2",
          "time": "20210904 22:11"
        },
        {
          "id": "MDRW2100040517.1.8",
          "form": "사실 남친이 있었는데 현 남편에게 반해서 환승 연애? 할 정도였어요",
          "original_form": "사실 남친이 있었는데 현 남편에게 반해서 환승 연애? 할 정도였어요",
          "speaker_id": "1",
          "time": "20210904 22:12"
        },
        {
          "id": "MDRW2100040517.1.9",
          "form": "진짜 사랑하셨네요",
          "original_form": "진짜 사랑하셨네요",
          "speaker_id": "2",
          "time": "20210904 22:12"
        },
        {
          "id": "MDRW2100040517.1.10",
          "form": "그런데 지금은...?",
          "original_form": "그런데 지금은...?",
          "speaker_id": "1",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100040517.1.11",
          "form": "ㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100040517.1.12",
          "form": "ㅋㅋㅋㅋㅋㅋ 지금은 뒷부분이",
          "original_form": "ㅋㅋㅋㅋㅋㅋ 지금은 뒷부분이",
          "speaker_id": "2",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100040517.1.13",
          "form": "...이네여",
          "original_form": "...이네여",
          "speaker_id": "2",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100040517.1.14",
          "form": "ㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100040517.1.15",
          "form": "이렇게 사랑해도 힘든게 결혼이군요",
          "original_form": "이렇게 사랑해도 힘든게 결혼이군요",
          "speaker_id": "2",
          "time": "20210904 22:13"
        },
        {
          "id": "MDRW2100040517.1.16",
          "form": "가끔 이 새*야 라고 튀어나와요",
          "original_form": "가끔 이 새*야 라고 튀어나와요",
          "speaker_id": "1",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100040517.1.17",
          "form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100040517.1.18",
          "form": "같이 살면 어쩔수 없는 걸까요",
          "original_form": "같이 살면 어쩔수 없는 걸까요",
          "speaker_id": "2",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100040517.1.19",
          "form": "가족끼리는 어쩔수가 없나봐요 ㅋㅋㅋㅋㅋ",
          "original_form": "가족끼리는 어쩔수가 없나봐요 ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100040517.1.20",
          "form": "그쵸 못볼꼴 다 보는데요이미 출산때 뭐...동물이 된 기분이에요",
          "original_form": "그쵸 못볼꼴 다 보는데요이미 출산때 뭐...동물이 된 기분이에요",
          "speaker_id": "1",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100040517.1.21",
          "form": "그대신 동료애는 생기셨어요?",
          "original_form": "그대신 동료애는 생기셨어요?",
          "speaker_id": "2",
          "time": "20210904 22:14"
        },
        {
          "id": "MDRW2100040517.1.22",
          "form": "그쵸 생사 유무 확인하며",
          "original_form": "그쵸 생사 유무 확인하며",
          "speaker_id": "1",
          "time": "20210904 22:15"
        },
        {
          "id": "MDRW2100040517.1.23",
          "form": "ㅎㅎ 그것만으로도 사이좋은 부부십니다",
          "original_form": "ㅎㅎ 그것만으로도 사이좋은 부부십니다",
          "speaker_id": "2",
          "time": "20210904 22:15"
        },
        {
          "id": "MDRW2100040517.1.24",
          "form": "무탈히 지내자고 전우애가 생기죠",
          "original_form": "무탈히 지내자고 전우애가 생기죠",
          "speaker_id": "1",
          "time": "20210904 22:15"
        },
        {
          "id": "MDRW2100040517.1.25",
          "form": "제가 보기엔 그래욯ㅎㅎㅎㅎ",
          "original_form": "제가 보기엔 그래욯ㅎㅎㅎㅎ",
          "speaker_id": "2",
          "time": "20210904 22:15"
        },
        {
          "id": "MDRW2100040517.1.26",
          "form": "전우애가 없어지면 진짜 문젠뎈ㅋㅋ",
          "original_form": "전우애가 없어지면 진짜 문젠뎈ㅋㅋ",
          "speaker_id": "2",
          "time": "20210904 22:15"
        },
        {
          "id": "MDRW2100040517.1.27",
          "form": "그런 부부들이 은근히 있는거 같더라고요",
          "original_form": "그런 부부들이 은근히 있는거 같더라고요",
          "speaker_id": "2",
          "time": "20210904 22:15"
        },
        {
          "id": "MDRW2100040517.1.28",
          "form": "네 나이를 먹다보니 이혼, 바람, 폭력 많은 것들을 듣고 보면서 많은 생각이 들죠",
