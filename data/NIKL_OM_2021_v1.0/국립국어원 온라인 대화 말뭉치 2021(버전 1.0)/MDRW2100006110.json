      "utterance": [
        {
          "id": "MDRW2100006110.1.1",
          "form": "종교~ 종교있으세요?",
          "original_form": "종교~ 종교있으세요?",
          "speaker_id": "2",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006110.1.2",
          "form": "아녕 ㅎ",
          "original_form": "아녕 ㅎ",
          "speaker_id": "1",
          "time": "20210907 21:10"
        },
        {
          "id": "MDRW2100006110.1.3",
          "form": "ㅎㅎㅎ 저도 무교에요",
          "original_form": "ㅎㅎㅎ 저도 무교에요",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006110.1.4",
          "form": "음",
          "original_form": "음",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006110.1.5",
          "form": "차라이좋다하면 절이좋아요",
          "original_form": "차라이좋다하면 절이좋아요",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006110.1.6",
          "form": "한적하고",
          "original_form": "한적하고",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006110.1.7",
          "form": "아 저두요!!!",
          "original_form": "아 저두요!!!",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006110.1.8",
          "form": "풀많고나무많고",
          "original_form": "풀많고나무많고",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006110.1.9",
          "form": "그냥 건축물적으로 좋아요 절",
          "original_form": "그냥 건축물적으로 좋아요 절",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006110.1.10",
          "form": "가는 길이 행복한",
          "original_form": "가는 길이 행복한",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006110.1.11",
          "form": "ㅋㅋㅋㅋㅋ",
          "original_form": "ㅋㅋㅋㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006110.1.12",
          "form": "맞아요 건축물도좋아뇨",
          "original_form": "맞아요 건축물도좋아뇨",
          "speaker_id": "1",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006110.1.13",
          "form": "맞아요 한적하고",
          "original_form": "맞아요 한적하고",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006110.1.14",
          "form": "향냄새도 좋고",
          "original_form": "향냄새도 좋고",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006110.1.15",
          "form": "밥고맛있고ㅋㅋ",
          "original_form": "밥고맛있고ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:12"
        },
        {
          "id": "MDRW2100006110.1.16",
          "form": "밥도~",
          "original_form": "밥도~",
          "speaker_id": "2",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006110.1.17",
          "form": "맞아요 밥도~",
          "original_form": "맞아요 밥도~",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006110.1.18",
          "form": "은근 맛나요!",
          "original_form": "은근 맛나요!",
          "speaker_id": "1",
          "time": "20210907 21:13"
        },
        {
          "id": "MDRW2100006110.1.19",
          "form": "신을 믿는다기보다 누구나 부처가될수있다는것도 좋은거같아요",
          "original_form": "신을 믿는다기보다 누구나 부처가될수있다는것도 좋은거같아요",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100006110.1.20",
          "form": "저는못되지만ㅋㅋ",
          "original_form": "저는못되지만ㅋㅋ",
          "speaker_id": "2",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100006110.1.21",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ되고싶지도않은...ㅎㅎㅎㅎㅎㅎㅎㅎㅎㅎㅎㅎ",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ되고싶지도않은...ㅎㅎㅎㅎㅎㅎㅎㅎㅎㅎㅎㅎ",
          "speaker_id": "1",
          "time": "20210907 21:14"
        },
        {
          "id": "MDRW2100006110.1.22",
          "form": "고기안먹고살수없어욬ㅋㅋㅋㅋㅋ",
          "original_form": "고기안먹고살수없어욬ㅋㅋㅋㅋㅋ",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100006110.1.23",
          "form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "original_form": "ㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋㅋ맞아요",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100006110.1.24",
          "form": "하루가니 밥도맛나지...... 맨날살라면",
          "original_form": "하루가니 밥도맛나지...... 맨날살라면",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100006110.1.25",
          "form": "어우부처님전글렀습니다...",
          "original_form": "어우부처님전글렀습니다...",
          "speaker_id": "1",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100006110.1.26",
          "form": "그춐ㅋㅋㅋㅋ 저도 땡이네요",
          "original_form": "그춐ㅋㅋㅋㅋ 저도 땡이네요",
          "speaker_id": "2",
          "time": "20210907 21:15"
        },
        {
          "id": "MDRW2100006110.1.27",
          "form": "부처님오신날만 가는걸로ㅋㅋㅋ",
          "original_form": "부처님오신날만 가는걸로ㅋㅋㅋ",
