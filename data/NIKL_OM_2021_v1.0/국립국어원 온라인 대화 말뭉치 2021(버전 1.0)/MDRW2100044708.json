      "utterance": [
        {
          "id": "MDRW2100044708.1.1",
          "form": "아..진짜 킹정이요",
          "original_form": "아..진짜 킹정이요",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044708.1.2",
          "form": "그 커버영상 제 관짝까지 들고가야댐요",
          "original_form": "그 커버영상 제 관짝까지 들고가야댐요",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044708.1.3",
          "form": "ㅠ",
          "original_form": "ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044708.1.4",
          "form": "돌았어요,,",
          "original_form": "돌았어요,,",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044708.1.5",
          "form": "아니 저는 재현 젤 좋았던게",
          "original_form": "아니 저는 재현 젤 좋았던게",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044708.1.6",
          "form": "보스에서 그",
          "original_form": "보스에서 그",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044708.1.7",
          "form": "팔 일케 들고",
          "original_form": "팔 일케 들고",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044708.1.8",
          "form": "하는 안무 있잖아요",
          "original_form": "하는 안무 있잖아요",
          "speaker_id": "2",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044708.1.9",
          "form": "악 !!!!!!알아요",
          "original_form": "악 !!!!!!알아요",
          "speaker_id": "1",
          "time": "20210907 23:31"
        },
        {
          "id": "MDRW2100044708.1.10",
          "form": "ㅠㅠ 솔짇히ㅜ그게 젤 ㄹㅈㄷ라고 생각해여",
          "original_form": "ㅠㅠ 솔짇히ㅜ그게 젤 ㄹㅈㄷ라고 생각해여",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044708.1.11",
          "form": "저 보스 첨 나왓을때",
          "original_form": "저 보스 첨 나왓을때",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044708.1.12",
          "form": "아니 근데 보스야 말류 진짜 컴눈명아닌가요?",
          "original_form": "아니 근데 보스야 말류 진짜 컴눈명아닌가요?",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044708.1.13",
          "form": "넘 아까워요..",
          "original_form": "넘 아까워요..",
          "speaker_id": "2",
          "time": "20210907 23:32"
        },
        {
          "id": "MDRW2100044708.1.14",
          "form": "진짜로요.....",
          "original_form": "진짜로요.....",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044708.1.15",
          "form": "보: 보스\n스: 돌아와\n가 괜히 있는말이 아녜요 ㅠㅠ",
          "original_form": "보: 보스\n스: 돌아와\n가 괜히 있는말이 아녜요 ㅠㅠ",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044708.1.16",
          "form": "보스는 무대를 찢어..ㅠ",
          "original_form": "보스는 무대를 찢어..ㅠ",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044708.1.17",
          "form": "진짜 딴건 다 그랗다쳐도",
          "original_form": "진짜 딴건 다 그랗다쳐도",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044708.1.18",
          "form": "진짜 착장이랑 하나하나 다 완벽..",
          "original_form": "진짜 착장이랑 하나하나 다 완벽..",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044708.1.19",
          "form": "라고 말하면서 떠오르는 타이틀곡이 꽤ㅜ많지만",
          "original_form": "라고 말하면서 떠오르는 타이틀곡이 꽤ㅜ많지만",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044708.1.20",
          "form": "아무튼 보스가 제루아까워요",
          "original_form": "아무튼 보스가 제루아까워요",
          "speaker_id": "2",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044708.1.21",
          "form": "보스는 진짜.... 슴에 머 2022버전인동 먼동 해서 다시 내줘야해요",
          "original_form": "보스는 진짜.... 슴에 머 2022버전인동 먼동 해서 다시 내줘야해요",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044708.1.22",
          "form": "아까워요짘짜..",
          "original_form": "아까워요짘짜..",
          "speaker_id": "1",
          "time": "20210907 23:33"
        },
        {
          "id": "MDRW2100044708.1.23",
          "form": "그걸 2주활동했다니.. 할배진짜.. 짜증나여",
          "original_form": "그걸 2주활동했다니.. 할배진짜.. 짜증나여",
          "speaker_id": "1",
          "time": "20210907 23:34"
        },
        {
          "id": "MDRW2100044708.1.24",
