          "original_form": "응",
          "speaker_id": "1",
          "time": "20210819 18:06"
        },
        {
          "id": "MDRW2100001686.1.8",
          "form": "건강보조제 챙겨먹나?",
          "original_form": "건강보조제 챙겨먹나?",
          "speaker_id": "1",
          "time": "20210819 18:09"
        },
        {
          "id": "MDRW2100001686.1.9",
          "form": "나는 초유단백질이랑 비타민 챙겨먹는데",
          "original_form": "나는 초유단백질이랑 비타민 챙겨먹는데",
          "speaker_id": "1",
          "time": "20210819 18:10"
        },
        {
          "id": "MDRW2100001686.1.10",
          "form": "난 건강보조제는",
          "original_form": "난 건강보조제는",
          "speaker_id": "2",
          "time": "20210819 18:20"
        },
        {
          "id": "MDRW2100001686.1.11",
          "form": "비타민c",
          "original_form": "비타민c",
          "speaker_id": "2",
          "time": "20210819 18:20"
        },
        {
          "id": "MDRW2100001686.1.12",
          "form": "가루로된거 머거",
          "original_form": "가루로된거 머거",
          "speaker_id": "2",
          "time": "20210819 18:20"
        },
        {
          "id": "MDRW2100001686.1.13",
          "form": "나도나도",
          "original_form": "나도나도",
          "speaker_id": "1",
          "time": "20210819 18:20"
        },
        {
          "id": "MDRW2100001686.1.14",
          "form": "너는 미용하는거 있어?",
          "original_form": "너는 미용하는거 있어?",
          "speaker_id": "2",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001686.1.15",
          "form": "나는 마스크팩하는거?",
          "original_form": "나는 마스크팩하는거?",
          "speaker_id": "1",
          "time": "20210819 18:21"
        },
        {
          "id": "MDRW2100001686.1.16",
          "form": "맞아 나도 마스크팩하는거 하고있어",
          "original_form": "맞아 나도 마스크팩하는거 하고있어",
          "speaker_id": "2",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001686.1.17",
          "form": "운동은 해?",
          "original_form": "운동은 해?",
          "speaker_id": "1",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001686.1.18",
          "form": "운동은 걷기하고있어",
          "original_form": "운동은 걷기하고있어",
          "speaker_id": "2",
          "time": "20210819 18:22"
        },
        {
          "id": "MDRW2100001686.1.19",
          "form": "너는 운동하는거 있어?",
          "original_form": "너는 운동하는거 있어?",
          "speaker_id": "2",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001686.1.20",
          "form": "나는 걷기랑 자전거타는거",
          "original_form": "나는 걷기랑 자전거타는거",
          "speaker_id": "1",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001686.1.21",
          "form": "열심히 하고 있네",
          "original_form": "열심히 하고 있네",
          "speaker_id": "2",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001686.1.22",
          "form": "부럽다",
          "original_form": "부럽다",
          "speaker_id": "2",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001686.1.23",
          "form": "걷기운동만해?",
          "original_form": "걷기운동만해?",
          "speaker_id": "1",
          "time": "20210819 18:23"
        },
        {
          "id": "MDRW2100001686.1.24",
          "form": "응 지금은 계속 걷기만하고 있어",
          "original_form": "응 지금은 계속 걷기만하고 있어",
          "speaker_id": "2",
          "time": "20210819 18:25"
        },
        {
          "id": "MDRW2100001686.1.25",
          "form": "코로나끝나면",
          "original_form": "코로나끝나면",
          "speaker_id": "2",
          "time": "20210819 18:25"
        },
        {
          "id": "MDRW2100001686.1.26",
          "form": "운동열심히할려구",
          "original_form": "운동열심히할려구",
          "speaker_id": "2",
          "time": "20210819 18:25"
        },
        {
          "id": "MDRW2100001686.1.27",
          "form": "아 운동열심히하면좋지",
