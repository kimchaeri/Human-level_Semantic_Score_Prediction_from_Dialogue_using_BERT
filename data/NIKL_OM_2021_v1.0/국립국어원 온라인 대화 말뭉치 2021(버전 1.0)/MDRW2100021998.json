      },
      "utterance": [
        {
          "id": "MDRW2100021998.1.1",
          "form": "나도 너무 귀여울 거 같아",
          "original_form": "나도 너무 귀여울 거 같아",
          "speaker_id": "2",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021998.1.2",
          "form": "한마리 정도?",
          "original_form": "한마리 정도?",
          "speaker_id": "1",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021998.1.3",
          "form": "응 한마리가 좋은듯",
          "original_form": "응 한마리가 좋은듯",
          "speaker_id": "2",
          "time": "20210829 00:37"
        },
        {
          "id": "MDRW2100021998.1.4",
          "form": "그게 딱 적당한 거 같아",
          "original_form": "그게 딱 적당한 거 같아",
          "speaker_id": "1",
          "time": "20210829 00:38"
        },
        {
          "id": "MDRW2100021998.1.5",
          "form": "뽈뽈 돌아다니면 너무 귀여울듯",
          "original_form": "뽈뽈 돌아다니면 너무 귀여울듯",
          "speaker_id": "2",
          "time": "20210829 00:39"
        },
        {
          "id": "MDRW2100021998.1.6",
          "form": "그니까",
          "original_form": "그니까",
          "speaker_id": "1",
          "time": "20210829 00:39"
        },
        {
          "id": "MDRW2100021998.1.7",
          "form": "쓰담쓰담해보고싶어",
          "original_form": "쓰담쓰담해보고싶어",
          "speaker_id": "2",
          "time": "20210829 00:40"
        },
        {
          "id": "MDRW2100021998.1.8",
          "form": "나도 ㅎㅎ",
          "original_form": "나도 ㅎㅎ",
          "speaker_id": "1",
          "time": "20210829 00:40"
        },
        {
          "id": "MDRW2100021998.1.9",
          "form": "감촉이 너무 신기할 거 같아",
          "original_form": "감촉이 너무 신기할 거 같아",
          "speaker_id": "2",
          "time": "20210829 00:41"
        },
        {
          "id": "MDRW2100021998.1.10",
          "form": "되게 이불같을 듯",
          "original_form": "되게 이불같을 듯",
          "speaker_id": "1",
          "time": "20210829 00:41"
        },
        {
          "id": "MDRW2100021998.1.11",
          "form": "나는 그 털 느낌이 어떨지 너무 궁금해",
          "original_form": "나는 그 털 느낌이 어떨지 너무 궁금해",
          "speaker_id": "2",
          "time": "20210829 00:42"
        },
        {
          "id": "MDRW2100021998.1.12",
          "form": "ㅋㅋㅋ 비슷하지 않을까?",
          "original_form": "ㅋㅋㅋ 비슷하지 않을까?",
          "speaker_id": "1",
          "time": "20210829 00:43"
        },
        {
          "id": "MDRW2100021998.1.13",
          "form": "이불에는 털이 없잖아",
          "original_form": "이불에는 털이 없잖아",
          "speaker_id": "2",
          "time": "20210829 00:43"
        },
        {
          "id": "MDRW2100021998.1.14",
          "form": "털 있는 이불 같은거 있잖아",
          "original_form": "털 있는 이불 같은거 있잖아",
          "speaker_id": "1",
          "time": "20210829 00:43"
        },
        {
          "id": "MDRW2100021998.1.15",
          "form": "아 털 담요 같은 거?",
          "original_form": "아 털 담요 같은 거?",
          "speaker_id": "2",
          "time": "20210829 00:44"
        },
        {
          "id": "MDRW2100021998.1.16",
          "form": "응 맞아",
          "original_form": "응 맞아",
          "speaker_id": "1",
          "time": "20210829 00:44"
        }
      ]
    }
