      "utterance": [
        {
          "id": "MDRW2100003684.1.1",
          "form": "치아건강이 안좋으신가요?",
          "original_form": "치아건강이 안좋으신가요?",
          "speaker_id": "1",
          "time": "20210821 10:44"
        },
        {
          "id": "MDRW2100003684.1.2",
          "form": "네",
          "original_form": "네",
          "speaker_id": "2",
          "time": "20210821 10:46"
        },
        {
          "id": "MDRW2100003684.1.3",
          "form": "임플란트 했어요",
          "original_form": "임플란트 했어요",
          "speaker_id": "2",
          "time": "20210821 10:46"
        },
        {
          "id": "MDRW2100003684.1.4",
          "form": "저도 제가 이리 될줄 몰랐어요",
          "original_form": "저도 제가 이리 될줄 몰랐어요",
          "speaker_id": "2",
          "time": "20210821 10:46"
        },
        {
          "id": "MDRW2100003684.1.5",
          "form": "헙 몇개요?",
          "original_form": "헙 몇개요?",
          "speaker_id": "1",
          "time": "20210821 10:46"
        },
        {
          "id": "MDRW2100003684.1.6",
          "form": "한개요",
          "original_form": "한개요",
          "speaker_id": "2",
          "time": "20210821 10:47"
        },
        {
          "id": "MDRW2100003684.1.7",
          "form": "근데 딴데도 곧이라고해거",
          "original_form": "근데 딴데도 곧이라고해거",
          "speaker_id": "2",
          "time": "20210821 10:47"
        },
        {
          "id": "MDRW2100003684.1.8",
          "form": "서",
          "original_form": "서",
          "speaker_id": "2",
          "time": "20210821 10:47"
        },
        {
          "id": "MDRW2100003684.1.9",
          "form": "너무 겁나요",
          "original_form": "너무 겁나요",
          "speaker_id": "2",
          "time": "20210821 10:47"
        },
        {
          "id": "MDRW2100003684.1.10",
          "form": "지금도 감진가요 잇몸이 워낙 약해요",
          "original_form": "지금도 감진가요 잇몸이 워낙 약해요",
          "speaker_id": "2",
          "time": "20210821 10:47"
        },
        {
          "id": "MDRW2100003684.1.11",
          "form": "검진가요 혹시 몰라서 잇몸치료하고요",
          "original_form": "검진가요 혹시 몰라서 잇몸치료하고요",
          "speaker_id": "2",
          "time": "20210821 10:48"
        },
        {
          "id": "MDRW2100003684.1.12",
          "form": "저도 임플란트 2개 할뻔 했어요ㅠ 한달전에",
          "original_form": "저도 임플란트 2개 할뻔 했어요ㅠ 한달전에",
          "speaker_id": "1",
          "time": "20210821 10:48"
        },
        {
          "id": "MDRW2100003684.1.13",
          "form": "오지금은 어떠세요?",
          "original_form": "오지금은 어떠세요?",
          "speaker_id": "2",
          "time": "20210821 10:49"
        },
        {
          "id": "MDRW2100003684.1.14",
          "form": "안하시고 다행이네요",
          "original_form": "안하시고 다행이네요",
          "speaker_id": "2",
          "time": "20210821 10:49"
        },
        {
          "id": "MDRW2100003684.1.15",
          "form": "치과를 옮겼더니 거기서는 안해도 된다고 하더라고요!!!!!!",
          "original_form": "치과를 옮겼더니 거기서는 안해도 된다고 하더라고요!!!!!!",
          "speaker_id": "1",
          "time": "20210821 10:49"
        },
        {
          "id": "MDRW2100003684.1.16",
          "form": "ㅇ아휴 치과들이 사기가 심해요",
          "original_form": "ㅇ아휴 치과들이 사기가 심해요",
          "speaker_id": "2",
          "time": "20210821 10:50"
        },
        {
          "id": "MDRW2100003684.1.17",
          "form": "안해도 될걸 하라하고",
          "original_form": "안해도 될걸 하라하고",
          "speaker_id": "2",
          "time": "20210821 10:50"
        },
        {
          "id": "MDRW2100003684.1.18",
          "form": "잇몸치약도 쓰고 관리해도 이러네요",
          "original_form": "잇몸치약도 쓰고 관리해도 이러네요",
          "speaker_id": "2",
          "time": "20210821 10:50"
        },
        {
          "id": "MDRW2100003684.1.19",
          "form": "꼭 관리 잘해서 임플란트 하지마세요",
          "original_form": "꼭 관리 잘해서 임플란트 하지마세요",
          "speaker_id": "2",
          "time": "20210821 10:50"
        },
        {
          "id": "MDRW2100003684.1.20",
          "form": "네 맞아요ㅠㅠ 저도 잇몸건강이 안좋아요",
          "original_form": "네 맞아요ㅠㅠ 저도 잇몸건강이 안좋아요",
          "speaker_id": "1",
          "time": "20210821 10:50"
        },
        {
          "id": "MDRW2100003684.1.21",
          "form": "별로예요",
          "original_form": "별로예요",
          "speaker_id": "2",
          "time": "20210821 10:51"
        },
        {
          "id": "MDRW2100003684.1.22",
          "form": "피가 막나요",
          "original_form": "피가 막나요",
          "speaker_id": "2",
          "time": "20210821 10:51"
        },
        {
          "id": "MDRW2100003684.1.23",
          "form": "내력이에요 집안",
          "original_form": "내력이에요 집안",
          "speaker_id": "1",
          "time": "20210821 10:51"
        },
        {
          "id": "MDRW2100003684.1.24",
          "form": "붓고",
          "original_form": "붓고",
          "speaker_id": "2",
          "time": "20210821 10:51"
        },
        {
          "id": "MDRW2100003684.1.25",
          "form": "저도요 치실하면 꼭 피나요",
          "original_form": "저도요 치실하면 꼭 피나요",
          "speaker_id": "1",
          "time": "20210821 10:51"
        },
        {
          "id": "MDRW2100003684.1.26",
          "form": "저도 그래요 저희딸은 더 심각해요",
          "original_form": "저도 그래요 저희딸은 더 심각해요",
          "speaker_id": "2",
          "time": "20210821 10:51"
        },
        {
          "id": "MDRW2100003684.1.27",
          "form": "태어날때부터 치아가 녹아요",
          "original_form": "태어날때부터 치아가 녹아요",
          "speaker_id": "2",
          "time": "20210821 10:51"
        },
        {
          "id": "MDRW2100003684.1.28",
          "form": "아 그러시구나ㅠㅠ 동지네요",
          "original_form": "아 그러시구나ㅠㅠ 동지네요",
          "speaker_id": "1",
          "time": "20210821 10:51"
        }
      ]
    }
